  <ofc>*</ofc>
  <rad>radi</rad>/o
</kap>

<drv mrk="radi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="radi.0o.rektlinia">
    <dif>
      Rektlinia mallar&gcirc;a lumstrio:
      <ekz>
        hela <tld/>o de la suno
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua <tld/>o de la suno
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la herbo malanta&ubreve; la dikaj trunkoj brilis la oraj strioj de la sunaj <tld/>oj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> tonoj melankolie kunfandi&gcirc;adis kun la lunaj radioj, penetrantaj tra la fenestro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li eksentis subite kvaza&ubreve; <tld/>on de fulmo
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        veni sub la <tld/>ojn de la lampo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        popoloj iros al via lumo, kaj re&gcirc;oj al la brilo de viaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 60:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        levi&gcirc;os suno de justeco, kaj sanigo estos en &gcirc;iaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Mala&hcirc;i 4:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kis.0i">kisi</ref>.
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
    <trd lng="bg">&c_l;&c_malmol;&c_ch;</trd>
    <trd lng="de">Strahl</trd>
    <trd lng="es">rayo</trd>
    <trd lng="fr">rayon<klr tip="ind"> (p.ex. lumineux)</klr></trd>
    <trd lng="hu">sug&aacute;r</trd> 
    <trd lng="id">radiasi</trd>   
    <trd lng="nl">straal</trd>
    <trd lng="pl">promie&nacute;</trd>
    <trd lng="pt">raio</trd>
    <trd lng="ru">&c_l;&c_u;&c_ch;</trd>
  </snc>
  <snc mrk="radi.0o.energielsendo">
    <dif>
      &Ccirc;iu aparta elsenda&jcirc;o el energifonto:
      <ekz>
        <tld/>o de varmo, ultraviola lumo;
      </ekz>
      <ekz>
        la rapideco de la <tld/>o de lumo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        rentgen-, alfa-, beta- kaj gama-~oj;
      </ekz>
      <ekz>
        en fiziko, rentgeno (simbolo R) estas mezurunuo por joniga <tld/>ado (kiel ikso-<tld/>oj kaj gamaj <tld/>oj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=202480">Rentgeno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">RAD</uzo>
        <tld/>o-aparato
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oamatoro
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9312-bosnio.txt">Bosnio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi e&ccirc; ne kura&gcirc;is &scirc;alti la <tld/>oricevilon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007140.html">de Julian Modest</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;telefonoj <klr>[&#x2026;]</klr> estas malsanigaj pro la <tld/>oj uzataj por tiu &ccirc;i moderna kaj populara komunikilo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="radi.0o.radioaparato">
    <dif>
      <ref tip="vid" cel="radiof.0o">Radiofonio</ref>:
      <ekz>
        helpokrio (per <tld/>o)
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="staci.radi0o">radiostacio</ref>;
      </ekz>
      <ekz>
        varsovia solena&jcirc;o, kiun oni povis spekti en la televido kaj a&ubreve;skulti en la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9308-naciaj-radikoj.txt">&Ccirc;u reiri al naciaj radikoj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Pola <tld lit="R"/>o persatelite elsendas esperantlingvajn programojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9504-radio.txt">Radio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oprogramoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9504-radio.txt">Radio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_jo;</trd>
    <trd lng="bg">&c_r;&c_a;&c_d;&c_i;&c_o;</trd>
    <trdgrp lng="de">
      <trd>Radio</trd>,
      <trd>Radioapparat</trd>,
      <trd>Radioger&auml;t</trd>,
      <trd>Rundfunkger&auml;t</trd>,
      <trd>Rundfunkempf&auml;nger</trd>
    </trdgrp>
    <trd lng="es">radio</trd>
    <trd lng="fr">radio</trd>
    <trdgrp lng="hu">
      <trd>r&aacute;di&oacute;</trd>,
      <trd>r&aacute;di&oacute;z&aacute;s</trd>
    </trdgrp>
    <trd lng="id">radio</trd>
    <trd lng="nl">radio</trd>
    <trd lng="pl">radio</trd>
    <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;</trd>
  </snc>
  <snc mrk="radi.0o.discelanta">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io, kio iras de iu centra punkto, discelante kiel bran&ccirc;oj de
      ventumilo:
      <ekz>
        aksoj kaj aksingoj kaj rad<tld/>oj kaj radrondoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mezo de la granda salono batis alta fontano,
        &gcirc;iaj <tld/>oj sin levadis &gcirc;is la vitra kupolo de la plafono
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la deklivo fluetis fontoj en formo de dikaj <tld/>oj de akvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tofo kun <tld/>oj, blankaj kaj rozokoloraj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[lekanto]</klr> kun siaj brilante blankaj folietoj,
        sidantaj kiel <tld/>oj &ccirc;irka&ubreve; la interna flava suno
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la harpunon oni enpu&scirc;is al la rosmaro en la bruston tiel, ke <tld/>o da varma sango el&scirc;prucis trans la glacion kiel fontano
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_d;&c_y;&c_ju;&c_s;</trd>,
      <trd>&c_s;&c_mol;&c_p;&c_ib;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_l;&c_malmol;&c_ch;</trd>
    <trd lng="de">Strahl</trd>
    <trd lng="es">rayo</trd>
    <trd lng="hu">sug&aacute;r</trd>   
    <trdgrp lng="id">
      <trd>jari-jari</trd>,
      <trd>radius</trd>
    </trdgrp> 
    <trd lng="nl">spaak</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_u;&c_ch;</trd>,
      <trd>&c_r;&c_a;&c_d;&c_i;&c_u;&c_s;</trd>,
      <trd>&c_s;&c_p;&c_i;&c_c;&c_a;<klr tip="amb"> (&c_v; &c_k;&c_o;&c_l;&c_je;&c_s;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="radi.0o.brila">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Gcirc;ojbrila aspekto:
      <ekz>
        super la viza&gcirc;o de la patrino kuris <tld/>o da
        feli&ccirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nenia <tld/>o de amo, boneco, malsevereco
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o da espero</ind>;
        <trd lng="hu">rem&eacute;nysug&aacute;r</trd>
      </ekz>
      <ekz>
        <ind><tld/>luma</ind> estonto.
        <trd lng="es">radiante</trd>
        <trd lng="fr">radieux</trd>
        <trd lng="hu">ragyog&oacute;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
    <trd lng="bg">&c_l;&c_malmol;&c_ch;</trd>
    <trd lng="de">Strahlen</trd>
    <trd lng="fr">rayon<klr tip="ind"> (p.ex. de bonheur)</klr></trd>
    <trd lng="hu">sug&aacute;r <klr tip="ind">(&aacute;tv.)</klr></trd>    
    <trd lng="nl">straal</trd>
    <trd lng="pl">blask</trd>
    <trd lng="ru">&c_l;&c_u;&c_ch;</trd>
  </snc>
  <snc mrk="radi.0o.radiuso">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
    <dif>
      <ref tip="dif" cel="radius.0o.cirklo">Radiuso</ref>:
      <ekz>
        armeo estas nenie en almena&ubreve; trimejla <tld/>o
        <klr>(en cirklo kun trimejla <tld/>o)</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rigardo &ccirc;irka&ubreve;prenis en kelkmejla <tld/>o preska&ubreve; la tutan
        provincon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la sfero &ccirc;iela havanta <frm><k>I</k></frm> kiel centron, <frm><k>C sek D</k></frm> kiel <tld/>on, ni konsideru <klr>[&#x2026;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_ju;&c_s;</trd>
    <trd lng="bg">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s;</trd>
    <trd lng="de">Radius</trd>
    <trd lng="es">radio</trd>
    <trd lng="hu">sug&aacute;r <klr tip="ind">(f&eacute;l &aacute;tm&eacute;r&odblac;)</klr></trd>    
    <trd lng="nl">radius</trd>
    <trd lng="pl">promie&nacute;</trd>
    <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s;</trd>
  </snc>
  <snc mrk="radi.0o.duonrekto">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <ref tip="dif" cel="rekt.duon0o">duonrekto.</ref>
    <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
    <trd lng="bg">&c_l;&c_malmol;&c_ch;</trd>
    <trd lng="de">Halbgerade</trd>
    <trd lng="es">semirrecta</trd>
    <trd lng="hu">f&eacute;legyenes</trd>    
    <trd lng="nl">halfrechte</trd>
    <trd lng="pl">p&oacute;&lstroke;prosta</trd>
    <trd lng="ru">&c_l;&c_u;&c_ch;</trd>
  </snc>
  <trd lng="af">radio</trd>
  <trd lng="am">&#x122C;&#x12F2;&#x12EE;</trd>
  <trd lng="ar">&#x631;&#x627;&#x62F;&#x64A;&#x648;</trd>
  <trd lng="az">radio</trd>
  <trd lng="bn">&#x9B0;&#x9C7;&#x9A1;&#x9BF;&#x993;</trd>
  <trd lng="bo">&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;&#xF60;&#xF55;&#xFB2;&#xF72;&#xF53;&#xF0B;</trd>
  <trd lng="bs">radio</trd>
  <trd lng="co">radiu</trd>
  <trdgrp lng="cs">
    <trd>dosah</trd>,
    <trd>okruh</trd>,
    <trd>paprsek</trd>,
    <trd>polop&rcaron;&iacute;mka</trd>,
    <trd>r&aacute;dius</trd>
  </trdgrp>
  <trd lng="da">radio</trd>
  <trd lng="en">ray</trd>
  <trd lng="et">raadio</trd>
  <trd lng="eu">radio</trd>
  <trd lng="fy">radio</trd>
  <trd lng="gl">radio</trd>
  <trd lng="gu">&#xAB0;&#xAC7;&#xAA1;&#xABF;&#xAAF;&#xACB;</trd>
  <trd lng="ha">radio</trd>
  <trd lng="hi">&#x930;&#x947;&#x921;&#x93F;&#x92F;&#x94B;</trd>
  <trd lng="hr">radio</trd>
  <trd lng="hy">&#x57C;&#x561;&#x564;&#x56B;&#x578;</trd>
