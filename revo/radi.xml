<drv mrk="radi.0ado">
  <kap>
    <tld/>ado
  </kap>
  <snc>
    <dif>
      Elsendado de lumo, de energio:
      <ekz>
        elmetita al intensa suna <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ultraviola <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        elektromagneta <tld/>ado, do jen survojas fotonoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        denaskaj genetikaj difektoj, rezulte de <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php">La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        altenergiaj kosmaj <tld/>oj kaj alia joniganta <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        de <ref cel="radioaktiv.0eco">radioaktiveco</ref>
        estas tri &ccirc;efaj specoj: alfa-<tld/>ado, beta-<tld/>ado kaj gama-<tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        beta-<tld/>ado konsistas el rapidflugaj elektronoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alfa-<tld/>ado konsistas el rapidflugaj atomkernoj de heliumo
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tutkorpaj skaniloj, funkciantaj per rentgena <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/011032.php">Skanado &gcirc;ena kaj kancerogena</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
