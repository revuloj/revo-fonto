<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buldog.xml,v 1.26 2025/08/05 20:51:18 revo Exp $">
<kap>
  <rad>buldog</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>

<drv mrk="buldog.0o">
  <kap><tld/>o</kap>
  <snc mrk="buldog.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="hund.0o.dombesto">Hundo</ref> el raso
      havanta mallongan, lar&gcirc;an kapon kaj elstarajn makzelojn:
      <ekz>
        lia granda <tld/>o <klr>[&#x2026;]</klr> aspektis tre sova&gcirc;a kaj mordema
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grandaj <tld/>oj, kiuj aspektis kvaza&ubreve; &ccirc;iu el
        ili povus engluti homon, eksaltis alten, tamen sen bojado
        <fnt>
           <bib>Fab2</bib>,
           <lok>ne&gcirc;a re&gcirc;ino, fabelo en sep rakontoj,
           kvina rakonto, malgranda rabistinfano</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj grasaj viza&gcirc;muskoloj stre&ccirc;i&gcirc;is kaj malstre&ccirc;i&gcirc;is <tld/>e
        <fnt>
          <bib>ChV</bib>
          <lok>12</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela kiel <tld/>o
        <klr>(t.e. malbela, timiga)</klr>
        <fnt>
          <bib>ChV</bib>
          <lok>4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mastif.0o">mastifo</ref>,
      <ref cel="molos.0o">moloso</ref>
    </refgrp>
    <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_g;</trd>
    <trd lng="bg">&c_b;&c_u;&c_l;&c_d;&c_o;&c_g;</trd>
    <trd lng="de">Bulldogge</trd>
    <trd lng="en">French <ind>bulldog</ind></trd>
    <trd lng="es">bulldog</trd>
    <trd lng="fr">bouledogue</trd>
    <trd lng="hu">bulldog</trd>
    <trd lng="pl">buldog</trd>
    <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_g;</trd>
    <trd lng="sk">buldog</trd>
    <trdgrp lng="zh">
    <trd>&#x6597;&#x725B;&#x72D7; <pr>d&ograve;uni&uacute;g&#x1D2;u</pr></trd>,
    <trd>&#x725B;&#x5934;&#x72AC; <pr>ni&uacute;t&oacute;uqu&#x1CE;n</pr></trd>,
    <trd>&#x5DE6;&#x8F2A;&#x624B;&#x69CD; <pr>zu&#x1D2;l&uacute;nsh&#x1D2;uqi&amacron;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">buldok</trd>
  <trd lng="he">&bet;&#x5BC;&vav;&#x5BC;&lamed;&dalet;&vav;&#x5B9;&gimel;</trd>
  <trd lng="ja">&#x30D6;&#x30EB;&#x30C9;&#x30C3;&#x30B0;</trd>
  <trd lng="uk">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_gu;</trd>
</drv>
</art>
