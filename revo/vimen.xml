<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vimen.xml,v 1.18 2022/03/28 11:08:55 revo Exp $">
<kap>
  <rad>vimen</rad>/o
</kap>
<drv mrk="vimen.0o">
  <kap><tld/>o</kap>
   <snc mrk="vimen.0o.BOT">
   <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="salik.0o">saliko</ref>
      (<trd lng="la">Salix viminalis</trd>), kies tre longaj
      <ref tip="prt" cel="verg.0o.brancxeto">vergoj</ref>
      servas por fari korbojn ks.:
      <ekz>
        <tld/>a&jcirc;o estas objekto farita el plektitaj <tld/>o-bran&ccirc;oj, ekzemple korbo 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=14935">Vimena&jcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la manoj ili portas vergon el <tld/>o a&ubreve; kratago, kiu brilas per &scirc;an&gcirc;anta koloro 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=388382">Anjana</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_r;&c_b;&c_a; &c_n;&c_ib;&c_c;&c_a;&c_ja;</trd>, 
    <trd>&c_l;&c_a;&c_z;&c_a;</trd> 
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_k;&c_i;&c_t;&c_a;</trd>
  <trd lng="ca">vimetera</trd>
  <trd lng="cs">vrba ko&scaron;&iacute;k&aacute;&rcaron;sk&aacute;</trd>
  <trd lng="de">Korbweide</trd>
  <trdgrp lng="en">
    <trd>osier</trd>,
    <trd>willow</trd>,
    <trd>wicker</trd>
  </trdgrp>
  <trd lng="fr">osier</trd>
  <trdgrp lng="hu">
    <trd>kos&aacute;rk&ouml;t&odblac; <ind>f&udblac;z</ind></trd>,
    <trd>nemes <ind>f&udblac;z</ind></trd>,
    <trd>k&ouml;t&odblac;f&udblac;z</trd>,
    <trd>kenderf&udblac;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>teenwilg</trd>,
    <trd>katwilg</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_v;&c_a;
&c_p;&c_r;&c_u;&c_t;&c_o;&c_v;&c_i;&c_d;&c_n;&c_a;&c_ja;</trd>
  <trd lng="sk">ko&scaron;ik&aacute;rska v&racute;ba</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x435;&#x440;&#x431;&#x430; &#x43F;&#x440;&#x443;&#x442;&#x43E;&#x432;&#x438;&#x434;&#x43D;&#x430;</trd>,
    <trd>&#x43B;&#x43E;&#x437;&#x430;</trd>,
    <trd>&#x43B;&#x43E;&#x437;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x432;&#x435;&#x440;&#x431;&#x43E;&#x432;&#x438;&#x439; &#x43F;&#x430;&#x433;&#x456;&#x43D;</trd>,
    <trd>&#x43F;&#x440;&#x443;&#x442;</trd>
  </trdgrp>
</drv>
</art>
