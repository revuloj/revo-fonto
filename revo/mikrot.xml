<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mikrot</rad>/o
</kap>
<drv mrk="mikrot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="hamstr.0edoj">hamstredoj</ref>
      (<trd lng="la">Microtus</trd>), simila al muso, sed ne vivanta
      en domoj kaj kun malgrandegaj oreloj.
    </dif>
    <ref tip="sin" cel="mus.kampo0o">kampomuso</ref>
  </snc>
  <trd lng="de">W&uuml;hlmaus</trd>
</drv>
</art>
