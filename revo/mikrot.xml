<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mikrot</rad>/o
</kap>
<drv mrk="mikrot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio
      <ref tip="super" cel="hamstr1.0oj">hamstredoj</ref>
      (<trd lng="la">Microtus</trd>), simila al muso, sed ne vivanta
      en domoj kaj kun malgrandegaj oreloj.
    </dif>
    <ref tip="sin" cel="mus.kampo0o">kampomuso</ref>
  </snc>
  <trd lng="de">W&uuml;hlmaus</trd>
  <trd lng="en">vole</trd>
  <trd lng="nl">woelmuis</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_jo;&c_v;&c_k;&c_a;</trd>
</drv>
</art>
