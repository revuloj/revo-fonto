<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enfil.xml,v 1.17 2023/10/14 15:49:04 revo Exp $">
  <kap>
    <rad>enfil</rad>/i
  </kap>
  <drv mrk="enfil.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <uzo tip="fak">MIL</uzo>
    <snc>
      <dif>
      Pafi la&ubreve;longe:
      <ekz>per artilerio a&ubreve; ma&scirc;inpafiloj oni <tld/>as la
      malamikajn poziciojn.</ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib; &c_w;&c_z;&c_d;&c_o;&c_w;&c_zh;&c_n;&c_y; &c_a;&c_g;&c_o;&c_n;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_s;&c_t;&c_r;&c_e;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol; &c_u;&c_z;&c_d;&c_o;&c_w;&c_zh;&c_n;&c_y;&c_m; &c_a;&c_g;&c_n;&c_jo;&c_m;</trd>
    </trdgrp>
  <trd lng="cs">bo&#x10D;n&#x11B; ost&rcaron;elovat</trd>
  <trdgrp lng="de">
       <trd>mit Feuer bestreichen</trd>,
       <trd>der L&auml;nge nach beschie&szlig;en</trd>
    </trdgrp>
  <trd lng="es">enfilar</trd>
  <trdgrp lng="fr">
      <trd>enfiler<klr tip="ind"> (prendre en enfilade)</klr></trd>,
      <trd>prendre en <ind>enfilade</ind></trd>
    </trdgrp>
  <trdgrp lng="hu">
      <trd>oldalaz&oacute; <ind>t&udblac;z</ind> al&aacute; vesz</trd>,
      <trd>v&eacute;gigp&aacute;szt&aacute;z<klr tip="ind">
      (l&ouml;v&eacute;ssorozattal)</klr></trd>
    </trdgrp>
  <trd lng="it">infilare<klr> (di artiglieria o sim.)</klr></trd>
  <trd lng="ja">&#x7E26;&#x5C04;&#x3059;&#x308B;  <pr>&#x305F;&#x3066;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
      <trd>ostrzeliwa&cacute;</trd>,
      <trd>przykrywa&cacute; <ind>ogniem</ind><klr> (pot.)</klr></trd>
    </trdgrp>
  <trd lng="pt">enfiar <klr tip="amb">(mil.)</klr></trd>
  <trd lng="ru">&c_o;&c_b;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;
    &c_p;&c_r;&c_o;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;
    &c_o;&c_g;&c_n;&c_jo;&c_m;</trd>
  <trd lng="sk">bo&#x10D;ne ostre&#x13E;ova&tcaron;</trd>
  <trd lng="uk">&#x43E;&c_b;&c_s;&c_t;&c_r;&c_ib;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43E;&c_z;&c_d;&#x43E;&c_v;&c_zh;&#x43D;&c_ib;&#x43C;
  &c_v;&#x43E;&c_g;&#x43D;&c_je;&#x43C;</trd>
</drv>
</art>
