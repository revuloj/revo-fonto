<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>menonit</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="menonit.0o">
  <kap><tld/>o</kap>
  <snc mrk="menonit.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Ano de <ref tip="vid" cel="menonit.0ismo.KRI"><tld/>ismo</ref>:
      <ekz>
        tio estas glorkanto kiun ni ofte kantas en <tld/>a pre&gcirc;ejo &ccirc;e 
        la universitato kie mi studas
        <fnt>
          <aut>Ellen</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/37290/61471">
          Gvidu Miajn Piedojn</url></vrk>,
          <lok>2008-04-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mennonite<klr> (subst.)</klr></trd>
  <trd lng="sk">novokrstenec</trd>
</drv>

<drv mrk="menonit.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="menonit.0ismo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Doktrino kaj praktiko de
      <ref tip="vid" cel="menonit.0o.KRI"><tld/>oj</ref>;
      anabaptisma bran&ccirc;o de 
      <ref tip="malprt" cel="protes1.0ismo.KRI">protestantismo</ref>
      esti&gcirc;inta en Svisujo en la 16a jarcento.      
    </dif>
  </snc>
  <trd lng="fr">mennonitisme</trd>
</drv>
</art>

