<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kisx.xml,v 1.1 2024/10/20 16:31:57 afido Exp $">
    <kap>
      <rad>ki&scirc;</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="kisx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Nedol&ccirc;a <ref tip="super" cel="tort.0o">torto</ref> origina el Loreno, farita el murbo,
      kun surmetita ovo-lakto-miksa&jcirc;o kun lardo kaj aliaj aldonoj:
      <ekz>
        <ind>Lorena <tld/>o</ind>
        <fnt>
          <vrk><url ref="https://www.thschuetz.de/rezepte/auflauf_eo.html"
              >Gratena&jcirc;oj</url></vrk>
        </fnt>;
        <trd lng="de">Quiche lorraine</trd>
        <trd lng="en">Quiche lorraine</trd>
        <trd lng="fr">Quiche lorraine</trd>
      </ekz>
      <ekz>
        beta <tld/>o kun nuksoj kaj &scirc;affroma&gcirc;o
        <fnt>
          <vrk><url ref="https://apetito.ikso.net/recepto/beta-kisxo-kun-nuksoj-kaj-sxaffromagxo"
              >Receptoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Quiche</trd>,
      <trd>Tarte</trd>
    </trdgrp>
    <trd lng="en">quiche</trd>
    <trdgrp lng="fr">
      <trd>quiche</trd>,
      <trd>tarte</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

