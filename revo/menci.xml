<drv mrk="menci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Paroli pretere pri ne &ccirc;eftema, tamen dirinda afero:
      <ekz>
        neniam <tld/>u la nomon en mia &ccirc;eesto
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;APITRO 2</lok>
        </fnt>;
      </ekz>
      <ekz>
        por esti tute verema, mi devas <tld/>i unu escepton
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Stein</nom> plurfoje <tld/>as la gestojn kiel la praformon de kompreni&gcirc;o
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        juna in&gcirc;eniero okaze parolante kun li <tld/>is, ke lia edzino deziras junan
        knabinon por helpi &scirc;in en la hejmaj laboroj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiaj estos tiuj pa&scirc;oj, en la deklaro ne estas
        <tld/>ite
        <fnt>
          <aut>E. Georgiev</aut>, <vrk>&Ccirc;u venos fino de
          la impostaj paradizoj?</vrk>, <bib>Monato</bib>,
          <lok>2000:10, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cit.0i.mencii">citi<sncref/></ref>
    <refgrp tip="ant">
      <ref cel="silent.pri0i">prisilenti</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="indik.0i.montri">indiki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>menegi&ntilde;</trd>,
    <trd>ober <ind>meneg</ind> eus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dotknout se</trd>,
    <trd>podotknout</trd>,
    <trd>uv&eacute;st</trd>,
    <trd>zm&iacute;nit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erw&auml;hnen</trd>,
    <trd>nennen</trd>,
    <trd>anf&uuml;hren</trd>
  </trdgrp>
  <trd lng="en">mention</trd>
  <trd lng="es">mencionar</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>mention</ind> de</trd>,
    <trd>mentionner</trd>
  </trdgrp>
  <trd lng="hu">eml&iacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x8A00;&#x53CA;&#x3059;&#x308B;  <pr>&#x3052;&#x3093;&#x304D;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3061;&#x3087;&#x3063;&#x3068;&#x89E6;&#x308C;&#x308B;  <pr>&#x3061;&#x3087;&#x3063;&#x3068;&#x3075;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x540D;&#x3092;&#x6319;&#x3052;&#x308B;  <pr>&#x306A;&#x3092;&#x3042;&#x3052;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vermelden</trd>
  <trdgrp lng="pl">
    <trd>nadmienia&cacute;</trd>,
    <trd>napomkn&aogonek;&cacute;</trd>,
    <trd>wzmiankowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mencionar</trd>,
    <trd>citar</trd>,
    <trd>referir</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cita</trd>
    <trd>men&tcomma;ioneaz&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_o;&c_m;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_o;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podotkn&uacute;&tcaron;</trd>,
    <trd>spomen&uacute;&tcaron;</trd>,
    <trd>zmieni&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_g;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_k;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6807;&#x79F0; <pr>bi&amacron;och&emacron;ng</pr></trd>,
    <trd>&#x6A19;&#x7A31; <pr>bi&amacron;och&emacron;ng</pr></trd>,
    <trd>&#x63D0;&#x5230; <pr>t&iacute;d&agrave;o</pr></trd>,
    <trd>&#x63D0;&#x53CA; <pr>t&iacute;j&iacute;</pr></trd>,
    <trd>&#x8AAA;&#x8D77; <pr>shu&omacron;q&#x1D0;</pr></trd>,
    <trd>&#x8BF4;&#x8D77; <pr>shu&omacron;q&#x1D0;</pr></trd>,
    <trd>&#x8AC7;&#x8D77; <pr>t&aacute;nq&#x1D0;</pr></trd>,
    <trd>&#x8C08;&#x8D77; <pr>t&aacute;nq&#x1D0;</pr></trd>,
    <trd>&#x8AC7;&#x53CA; <pr>t&aacute;nj&iacute;</pr></trd>,
    <trd>&#x8C08;&#x53CA; <pr>t&aacute;nj&iacute;</pr></trd>,
    <trd>&#x63D0;&#x8AAA; <pr>t&iacute;shu&omacron;</pr></trd>,
    <trd>&#x63D0;&#x8BF4; <pr>t&iacute;shu&omacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="menci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kelkvorta&jcirc;o, per kiu oni atentigas pri io:
      <ekz>
        &ccirc;e la <tld/>o de la nomo <nom>Lemuel</nom> la rideto &ccirc;irka&ubreve; &scirc;ia bu&scirc;o transiris en
        malestiman kaj malplezuran esprimon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        el tiuj <klr>[&#x2026;]</klr> neniu faris ion eminentan, kio meritus <tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Infana babilado</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu epizodo estas la unua <tld/>o pri Moskvo en la skribita
        historio
        <fnt><aut>S. Pokrovskij</aut>, <vrk>Moskvo
        jubileas</vrk>, <bib>Monato</bib>, <lok>1997:9, p.
        22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_a;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">meneg</trd>
  <trdgrp lng="cs">
    <trd>zm&iacute;nka</trd>,
    <trd>zpr&aacute;va</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erw&auml;hnung</trd>,
    <trd>Nennung</trd>
  </trdgrp>
  <trd lng="es">menci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>mention</trd>
  </trdgrp>
  <trd lng="hu">eml&iacute;t&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x8A00;&#x53CA;  <pr>&#x3052;&#x3093;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x8A18;&#x8F09;  <pr>&#x304D;&#x3055;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wzmianka</trd>
    <trd>napomknienie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>adnotare</trd>
    <trd>aluzie</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">zmienka</trd>
  <trd lng="uk">&c_z;&c_g;&c_a;&c_d;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x63D0;&#x5230; <pr>t&iacute;d&agrave;o</pr></trd>,
    <trd>&#x63D0;&#x8D77; <pr>t&iacute;q&#x1D0;</pr></trd>,
    <trd>&#x53D9;&#x8FF0; <pr>x&ugrave;sh&ugrave;</pr></trd>,
    <trd>&#x6558;&#x8FF0; <pr>x&ugrave;sh&ugrave;</pr></trd>,
    <trd>&#x8AC7;&#x5230; <pr>t&aacute;nd&agrave;o</pr></trd>,
    <trd>&#x8C08;&#x5230; <pr>t&aacute;nd&agrave;o</pr></trd>,
    <trd>&#x8AD6;&#x53CA; <pr>l&ugrave;nj&iacute;</pr></trd>,
    <trd>&#x8BBA;&#x53CA; <pr>l&ugrave;nj&iacute;</pr></trd>,
    <trd>&#x8B1B;&#x5230; <pr>ji&#x1CE;ngd&agrave;o</pr></trd>,
    <trd>&#x8BB2;&#x5230; <pr>ji&#x1CE;ngd&agrave;o</pr></trd>,
    <trd>&#x8FF0;&#x8AAA; <pr>sh&ugrave;shu&omacron;</pr></trd>,
    <trd>&#x8FF0;&#x8BF4; <pr>sh&ugrave;shu&omacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="menci.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Sufi&ccirc;e <ref tip="super" cel="grav.0a">grava</ref>, por ke oni <tld/>u &gcirc;in:
      <ekz>
        kiel instruantoj de la parolarto estas <tld/>indaj la famaj retoroj Trasima&hcirc;o el
        Kalcedonio, Teodoro el Bizanco, Izeo kaj precipe Izokrato
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        aparte <tld/>inda estas la tre valora kontribuo de <klr>[&#x2026;]</klr>
        <fnt>
          <bib>EeP</bib>
          <lok>Nomaro De Kunlaborintoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas <tld/>inde, ke
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; <ind>&c_z;&c_g;&c_a;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  <trd lng="br">da veza&ntilde; <ind>meneget</ind></trd>
  <trd lng="cs">stoj&iacute;c&iacute; za zm&iacute;nku</trd>
  <trdgrp lng="de">
    <trd>erw&auml;hnenswert</trd>,
    <trd>nennenswert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>digne d'int&eacute;r&ecirc;t</trd>,
    <trd>digne d'&ecirc;tre mentionn&eacute;</trd>
  </trdgrp>
  <trd lng="hu">eml&iacute;t&eacute;sre m&eacute;lt&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x8A00;&#x53CA;&#x306B;&#x5024;&#x3059;&#x308B;  <pr>&#x3052;&#x3093;&#x304D;&#x3085;&#x3046;&#x306B;&#x3042;&#x305F;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7279;&#x7B46;&#x3059;&#x3079;&#x304D;  <pr>&#x3068;&#x304F;&#x3072;&#x3064;&#x3059;&#x3079;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pl">wart wymienienia</trd>
  <trd lng="ro">merit&abreve; men&tcomma;ionat</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j; &c_u;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_u;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">stoj&iacute; za zmienku</trd>
  <trd lng="uk">&c_ja;&c_k;&c_i;&c_j; &c_v;&c_a;&c_r;&c_t;&c_o; &c_z;&c_g;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x5024;&#x5F97;&#x63D0; <pr>zh&iacute;det&iacute;</pr></trd>,
    <trd>&#x503C;&#x5F97;&#x63D0; <pr>zh&iacute;det&iacute;</pr></trd>,
    <trd>&#x53EF;&#x5EFA;&#x8B70; <pr>k&ecaron;ji&agrave;ny&igrave;</pr></trd>,
    <trd>&#x53EF;&#x5EFA;&#x8BAE; <pr>k&ecaron;ji&agrave;ny&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="menci.0indi">
  <kap><tld/>indi</kap>
  <snc>
    <dif>
      Esti sufi&ccirc;e kurioza, grava, interesa por <tld/>o:
      <ekz>
        inter la prioritatoj por la venonta oficperiodo sendube unua <tld/>indas la
        diversigo de niaj monfontoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Rubriko de la prezidanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel plua kulmino en lia vivo <tld/>indas anka&ubreve; la unua post
        1912 sukcesa ekspedicio al la suda poluso
        <fnt><aut>E. Georgiev</aut>, <vrk>Sir Edmund Hillary</vrk>,
        <bib>Monato</bib>, <lok>2000:10, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_v;&c_a;&c_r;&c_t;&c_y;&c_m; <ind>&c_z;&c_g;&c_a;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  <trdgrp lng="de">
    <trd><ind>erw&auml;hnenswert</ind> sein</trd>,
    <trd><ind>nennenswert</ind> sein</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>digne d'int&eacute;r&ecirc;t</ind></trd>
  </trdgrp>
  <trd lng="hu">eml&iacute;t&eacute;st &eacute;rdemel</trd>
  <trdgrp lng="pl">
    <trd>wymienia&cacute;</trd>
    <trd>wzmiankowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>schimba</trd>
    <trd>men&tcomma;ioneaz&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_u;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="menci.supre0ita">
  <kap>supre<tld/>ita</kap>
  <snc>
    <dif>
      Sama, pli frue dum parolado a&ubreve; en teksto jam <tld/>ita:
      <ekz>
        la supre<tld/>ita internacia kongreso
        <fnt><aut>V. Lemelev</aut>: <vrk>Grava bazo por preventi
        aidoson</vrk>,
        <bib>Monato</bib>, <lok>2001:5, p. 15a</lok></fnt>;
      </ekz>
      <ekz>
        la supre<tld/>itaj lingvoj diversgrade kontribuis al la kreo de Esperanto
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Monato;/2003/008004.html"
              >La gepatra de Zamenhof</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_sh;&c_e;&c_j;&c_z;&c_g;&c_a;&c_d;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_sh;&c_e;&c_j;&c_p;&c_a;&c_m;&c_ja;&c_n;&c_jo;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_sh;&c_e;&c_j;&c_n;&c_a;&c_z;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">meneget uheloc'h</trd>
  <trd lng="cs">svrchu &rcaron;e&ccaron;en&yacute;</trd>
  <trdgrp lng="de">
    <trd>oben <ind>erw&auml;hnte</ind></trd>,
    <trd>oben <ind>genannte</ind></trd>,
    <trd>oben <ind>angef&uuml;hrte</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>susdit</trd>,
    <trd>susmentionn&eacute;</trd>,
    <trd>susnomm&eacute;</trd>
  </trdgrp>
  <trd lng="hu">fent eml&iacute;tett</trd>
  <trd lng="pl">wy&zdot;ej wymieniony</trd>
  <trd lng="ro">men&tcomma;ionat mai sus</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_sh;&c_je;&c_u;&c_p;&c_o;&c_m;&c_ja;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_sh;&c_je;&c_n;&c_a;&c_z;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hore menovan&yacute;</trd>,
    <trd>hore uveden&yacute;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E0A;&#x8FF0; <pr>sh&agrave;ngsh&ugrave;</pr></trd>
</drv>
