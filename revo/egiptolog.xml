<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: egiptolog.xml,v 1.12 2022/09/26 14:06:52 revo Exp $">
<kap>
  <rad>egiptolog</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="egiptolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="egiptolog.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="dif" cel="egiptologi.0isto.HIS">Egiptologiisto</ref>:
      <ekz>
        glora <tld/>o
        <fnt>
        <bib>FK</bib>,
        <lok>6, artikoloj pri esperanto, al la historio de la provoj de
        lingvoj tutmondaj de Leibnitz &gcirc;is la nuna tempo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kunlabore tradukas egiptajn tekstojn, diskutante ilin
        linion post linio
        <fnt>
          <aut>Klivo</aut>,
          <vrk>Re: diktaturoj kaj Esperanto</vrk>,
          <lok>soc.culture.esperanto, 2000-12-23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> tuj vidos la &hcirc;-an tiel nomatan placento-signon kaj aldone
        oficialan transskribmanieron
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2012/010409.php">Etimologia ekskurso</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la teksto estis priesplorita de la <tld/>o <nom>Brugsch</nom> kaj identigita kiel etruska
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010755.php">Notindaj rilatoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_g;&c_ib;&c_p;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">egipt&ograve;leg</trd>
  <trd lng="cs">egyptolog</trd>
  <trd lng="de">&Auml;gyptologe</trd>
  <trd lng="fr">&eacute;gyptologue</trd>
  <trd lng="pl">egiptolog</trd>
  <trd lng="pt">egipt&oacute;logo</trd>
  <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">egyptol&oacute;g</trd>
  <trd lng="uk">&c_jeu;&c_g;&c_i;&#x43F;&c_t;&#x43E;&#x43B;&#x43E;&c_g;</trd>
</drv>
</art>

