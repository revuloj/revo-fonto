<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>egiptolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="egiptolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="egiptolog.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <ref tip="dif" cel="egiptologi.0isto.HIS">egiptologiisto</ref>
      <ekz>
        glora <tld/>o
        <fnt>
        <bib>FK</bib>,
        <lok>6, artikoloj pri esperanto, al la historio de la provoj de
        lingvoj tutmondaj de Leibnitz &gcirc;is la nuna tempo</lok>
        </fnt>
      </ekz>
      <ekz>
        <tld/>oj kunlabore tradukas egiptajn tekstojn, diskutante ilin
        linion post linio
        <fnt>
        <aut>Klivo</aut>,
        <vrk>Re: diktaturoj kaj Esperanto</vrk>,
        <lok>soc.culture.esperanto, 2000-12-23</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="bg">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">egipt&ograve;leg</trd>
  <trd lng="de">&Auml;gyptologe</trd>
  <trd lng="fr">&eacute;gyptologue</trd>
  <trd lng="pl">egiptolog</trd>
  <trd lng="pt">egipt&oacute;logo</trd>
  <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>

