<drv mrk="ind.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Ne meritanta ion:
      <ekz>
        li estas vere mal<tld/>a je rekompenco;
      </ekz>
      <ekz>
        la ri&ccirc;a&jcirc;oj <klr>[&#x2026;]</klr> devas fine esti el&scirc;iritaj el la mal<tld/>aj manoj, kaj
        redonitaj al la fidelaj servistoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&eacute;rdemtelen</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Ne <ref tip="ant" cel="dign.0a.respektinda">digna<sncref/></ref>:
      <ekz>
        miaj mal<tld/>aj piedoj tu&scirc;is vian plankon, princo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;teloj, mortigoj, adultoj, avideco, mal<tld/>a&jcirc;oj <klr>[&#x2026;]</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Marko 7:22</lok>
        </fnt>;
      </ekz>
      <ekz>
       la silento <klr>[&#x2026;]</klr> kvaza&ubreve; purigis la internon de &ccirc;io malbona kaj
       mal<tld/>a
       <fnt><aut>Stellan Engholm</aut>: <vrk>Al Torento</vrk>,
       <lok>&ccirc;apitro IV, 1930</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a viro, konduto, ago.
      </ekz>
    </dif>
    <trd lng="hu">m&eacute;ltatlan</trd>
    <trd lng="ru">&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_v;&c_a;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ib;&c_k;&c_ch;&c_e;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ned&uring;stojn&yacute;</trd>,
    <trd>nevhodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unwert</trd>,
    <trd>unw&uuml;rdig</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>indigno</trd>,
    <trd>indigno (de)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>indigne</trd>,
    <trd>indigne (de)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5024;&#x6253;&#x3061;&#x306E;&#x306A;&#x3044;  <pr>&#x306D;&#x3046;&#x3061;&#x306E;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x3068;&#x308A;&#x3048;&#x306E;&#x306A;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">onwaardig</trd>
  <trd lng="pt">indigno<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="sk">nehodn&yacute;</trd>
