  <trd lng="mi">raina</trd>
  <trd lng="mk">&c_l;&c_i;&c_n;&c_i;&#x458;&c_a;</trd>
  <trd lng="ml">&#xD35;&#xD30;</trd>
  <trd lng="mn">&c_m;&#x4E9;&c_r; &c_n;&c_mol;</trd>
  <trd lng="mr">&#x913;&#x933;</trd>
  <trd lng="ms">talian</trd>
  <trd lng="my">&#x101C;&#x102D;&#x102F;&#x1004;&#x103A;&#x1038;</trd>
  <trd lng="ne">&#x928;&#x92F;&#x93E;&#x901;</trd>
  <trd lng="nl">lijn</trd>
  <trd lng="ny">mzere</trd>
  <trd lng="pa">&#xA32;&#xA3E;&#xA08;&#xA28;</trd>
  <trd lng="ps">&a_kh;&a_T;</trd>
