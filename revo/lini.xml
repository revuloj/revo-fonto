<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lini</rad>/o
</kap>
<drv mrk="lini.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lini.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
    <dif>
      Unudimensia <ref tip="super" cel="figur.geometria0o">geometria
      figuro</ref>:
      <ekz>
        rekta, kurba, rompita <tld/>o;
      </ekz>
      <ekz>
        <tld/>o estas la bildaro de kontinua
        <ref cel="bild.0igo.MAT">bildigo</ref> de la aro de reeloj al la
        koncerna spaco <klr>(tian bildigon oni nomas parametra prezento de la
        <tld/>o)</klr>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kurb.0o.MAT">kurbo.</ref>
    <trd lng="de">Linie</trd>
    <trdgrp lng="fr">
      <trd>courbe<klr tip="ind"> (g&eacute;om&eacute;trique)</klr></trd>,
      <trd>ligne<klr tip="ind"> (g&eacute;om&eacute;trique)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Kontinua streko montranta difinitan direkton:
      <ekz>
        belforma <tld/>o de la bu&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oj de papero, desegno, de la viza&gcirc;o, de la mano;
      </ekz>
      <ekz>
        konduki, streki, desegni, tiri <tld/>on;
      </ekz>
      <ekz>
        super<tld/>a signeto<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trajt.0o">trajto</ref>,
      <ref cel="siluet.0o">silueto</ref>,
      <ref cel="kontur.0o">konturo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Difinita direkto seninterrompe trairota:
      <ekz>
        marvetura, trafika, fervoja <tld/>o;
      </ekz>
      <ekz>
        organizi, aran&gcirc;i <tld/>ojn de a&ubreve;tobusoj, aeroplanoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="voj.0o">vojo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Vico da objekton a&ubreve; personoj ordigitaj la&ubreve; difinita direkto:
      <ekz>
        &ccirc;iu pa&gcirc;o enhavas tridek <tld/>ojn;
      </ekz>
      <ekz>
        duobla <tld/>o da arboj;
      </ekz>
      <ekz>
        armeo ordigita la&ubreve; 3 <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">line</trd>
  <trd lng="es">l&iacute;nea</trd>
  <trd lng="fr">ligne</trd>
  <trd lng="nl">lijn</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="lini.0a">
  <kap><tld/>a</kap>
  <snc mrk="lini.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a aran&gcirc;o, desegno.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lini.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="dif" cel="linear.0a">Lineara</ref>:
      <ekz>
        <tld/>a ekvacio.
      </ekz>
    </dif>
    <rim>
      Kvankam ar&hcirc;ai&gcirc;inta, tiu senco restas logika, radik&scirc;para,
      internacia kaj promociinda. Riskoj de ambigueco apena&ubreve; ekzistas.
    </rim>
    <trd lng="de">linear</trd>
    <trd lng="en">linear</trd>
  </snc>
  <trd lng="fr">lin&eacute;aire</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="lini.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a ordo.
    </dif>
  </snc>
  <trd lng="es">linealmente</trd>
  <trd lng="fr">en <ind>ligne</ind></trd>
  <trd lng="nl">rechtlijnig</trd>
</drv>

<drv mrk="lini.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn, marki <tld/>ojn sur io:
      <ekz>
        <tld/>ita papero.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">line</trd>
  <trd lng="fr">ligner</trd>
  <trd lng="nl">lini&euml;ren</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="lini.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo por streki <tld/>ojn.
    </dif>
  </snc>
  <trd lng="fr">r&egrave;gle<klr tip="ind"> (pour tracer des traits)</klr></trd>
  <trd lng="nl">liniaal</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_ch;&c_je;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
</drv>

<drv mrk="lini.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Aro da kvin horizontalaj <tld/>oj, sur kiuj estas lokitaj la
      diversaj notoj, la&ubreve; ilia valoro.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>staff</trd>,
    <trd>stave</trd>
  </trdgrp>
  <trd lng="es">pentagrama</trd>
  <trd lng="fr">port&eacute;e<klr tip="ind"> (mus.)</klr></trd>
  <trd lng="nl">notebalk</trd>
  <trd lng="ru">&c_n;&c_o;&c_t;&c_n;&c_y;&c_j; &c_s;&c_t;&c_a;&c_n;</trd>
</drv>

<drv mrk="lini.laux0igi">
  <kap>la&ubreve;<tld/>igi</kap>
  <snc>
    <dif>
      Ordigi la&ubreve; <tld/>oj:
      <ekz>
        la&ubreve;<tld/>igi monerojn sur la tablo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>align</trd>,
    <trd>line up</trd>
  </trdgrp>
  <trd lng="es">alinear</trd>
  <trd lng="fr">aligner</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol; <klr>&c_v; &c_l;&c_i;&c_n;&c_i;&c_ju;</klr></trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; <klr>&c_v; &c_l;&c_i;&c_n;&c_i;&c_ju;</klr></trd>
  </trdgrp>
</drv>
</art>
