<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grop.xml,v 1.16 2023/11/11 11:36:37 revo Exp $">
<kap>
  <rad>grop</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="grop.0o">
  <kap><tld/>o</kap>
  <snc mrk="grop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malanta&ubreve;a parto de la <ref tip="malprt" cel="dors.0o.ZOO">dorso</ref> de kvarpiedulo:
      <ekz>
        dusako, <klr>[&#x2026;]</klr> kiu ku&scirc;is sur la <tld/>o de la
        &ccirc;evalo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 3a, p.
        40a</lok></fnt>.
      </ekz>
      <ekz>
        <tld/>aj makuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48893">Altnaza antilopo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ARKI</uzo>
        <tld/>tegmento (kvarflanka)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=86596">Groptegmento</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lumb.0o">lumbo</ref>
    </refgrp>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="krop.0o">kropo</ref> (gor&gcirc;a po&scirc;o a&ubreve; tumoro).
  </rim>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh; 
  <klr tip="amb">(&c_u; &c_k;&c_a;&c_n;&c_ja;)</klr></trd>
  <trd lng="ca">gropa</trd>
  <trdgrp lng="cs">
    <trd>ko&ncaron;sk&yacute; zadek</trd>,
    <trd>pta&ccaron;&iacute; zadek</trd>,
    <trd>zv&iacute;&rcaron;ec&iacute; zadek</trd>
  </trdgrp>
  <trd lng="de">Kruppe</trd>
  <trdgrp lng="en">
    <trd>rump (quadruped)</trd>,
    <trd>haunch (quadruped)</trd>,
    <trd>croup</trd>
  </trdgrp>
  <trd lng="fr">croupe</trd>
  <trd lng="hu">far</trd>
  <trd lng="nl">kruis<klr> (v. e. paard)</klr></trd>
  <trd lng="pl">zad</trd>
  <trd lng="pt">garupa</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_p;</trd>
  <trd lng="sk">k&ocirc;&ncaron; <klr>(&scaron;portov&eacute; n&aacute;radie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_K;&c_r;&c_u;&c_p;</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a; &c_d;&c_a;&c_h;&c_u; &c_n;&c_a;&c_d; &c_f;&c_r;&c_o;&c_n;&c_t;&c_o;&c_n;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="zh">&#x99AC;&#x5C41;&#x80A1; <pr>m&#x1CE;p&#xEC;g&#x1D4;</pr></trd>
</drv>
</art>
