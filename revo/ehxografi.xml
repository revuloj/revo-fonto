    <ekz>
      <tld/>oj sciigas anta&ubreve; nasko al kiu sekso apartenas la
      naskot(in)o
      <fnt>
        <aut>C. Sarandrea</aut>,
        <vrk>Kial &ccirc;inaj patrinoj estas pli bonaj</vrk>,
        <lok><url
        ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
        soc.culture.esperanto</url>, 2011-01-09</lok>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="ehx.0o.sono">e&hcirc;o<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="ca">ecografia</trd>
  <trd lng="en">echography</trd>
  <trd lng="fr">&eacute;chographie</trd>
</drv>
