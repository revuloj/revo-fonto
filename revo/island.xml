<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: island.xml,v 1.16 2023/11/19 10:22:14 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Island</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="island.I0o">
  <kap>  <ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Islando">Islando</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">IS</mlg>
    <dif>
      <ref tip="lst" cel="land.0o" lst="voko:&scirc;tatoj">Lando</ref> 
      sur la samnoma, vulkana 
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">insulo</ref> en
      Norda <ref tip="malprt" cel="atlant.A0o">Atlantiko</ref>:
      <ekz>preter <tld/>o neniu lando estas trovebla, sed nur dezerta
      maro sen fino
        <fnt>
          <aut>Frans G. Bengtsson, trad. Bertil Nilsson</aut>
          <vrk>Orm la Ru&gcirc;a</vrk>
          <lok>parto 1a, &ccirc;apitro 4a</lok>
        </fnt>;
      </ekz>
      <ekz>la kvadrata &scirc;tona turo de la pre&gcirc;ejo aspektis norda e&ccirc; &gcirc;is
      paganeco, kvaza&ubreve; &gcirc;i estus barbara turo inter la marrokoj de
      <tld/>o
        <fnt>
          <aut>G. K. Chesterton, trad. Cecil Bean</aut>
          <vrk>La naiveco de pastro Brown</vrk>
          <lok>La signo de la rompita glavo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_s;&c_mol;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_I;&c_s;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="de">Island</trd>
  <trd lng="en">Iceland</trd>
  <trd lng="fr">Islande</trd>
  <trd lng="id">Islandia</trd>
  <trd lng="ja">&#x30A2;&#x30A4;&#x30B9;&#x30E9;&#x30F3;&#x30C9;</trd>
  <trd lng="nl">IJsland</trd>
  <trd lng="no">Island</trd>
  <trd lng="pl">Islandia</trd>
  <trd lng="pt">Isl&acirc;ndia</trd>
  <trd lng="uk">&#x406;&#x441;&#x43B;&#x430;&#x43D;&#x434;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x51B0;&#x5C9B; <pr>B&#x12B;ngd&#x1CE;o</pr></trd>
</drv>

<drv mrk="island.0ano">
  <kap><tld lit="i"/>ano</kap>
  <snc>
    <dif>
      &Scirc;tatano de Islando
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_s;&c_mol;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_je;&c_c;</trd>
  <trdgrp lng="fr"> 
    <trd>Islandais<klr> (subst.)</klr></trd>
    <trd>Islandaise<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="island.0a">
  <kap><tld lit="i"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Islando
    </dif>
    <trd lng="be">&c_ib;&c_s;&c_mol;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_ib;</trd>
  <trdgrp lng="fr"> 
    <trd>islandais<klr> (adj.)</klr></trd>
    <trd>islandaise<klr> (adj.)</klr></trd>
  </trdgrp>
  </snc>
  <trd lng="zh">&#x51B0;&#x5C9B;&#x8BED; <pr>b&#x12B;ngd&#x1CE;oy&#x1D4;</pr></trd>
</drv>

<drv mrk="island.la0a">
  <kap>la <tld lit="i"/>a
  <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Islanda lingvo">Islanda
  lingvo</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
    "la islanda" (lingvo) = la nacia lingvo de &scirc;tato Islando
    </dif>
    <trd lng="en">Icelandic<klr> (language)</klr></trd>
    <trd lng="fr">islandais<klr> (langue)</klr></trd>
  </snc>
  <trd lng="be">&c_ib;&c_s;&c_mol;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
</drv>
</art>

