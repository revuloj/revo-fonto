    <trd lng="en">gross</trd>
    <trd lng="fr">brut</trd>
    <trd lng="pl">brutto</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_b;&c_r;&c_u;&c_t;&c_a;</trd>,
  <trd>&c_v;&c_a;&c_l;&c_a;&c_v;&c_y;</trd>
</trdgrp>
</drv>
