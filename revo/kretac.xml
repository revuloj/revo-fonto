<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kretac.xml,v 1.21 2019/06/30 18:21:03 revo Exp $">
<kap>
  <rad>kretace</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kretac.0o">
  <kap><tld/>o</kap>
  <snc mrk="kretac.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref tip="lst" cel="period.0o.dauxro"
      lst="voko:geologiaj_periodoj" val="-145500000">periodo</ref> fine
      de <ref tip="malprt" cel="mezozoik.0o">mezozoiko</ref>:
      <ekz>
        la molaso en suda Germanio, anta&ubreve; jarmilionoj &scirc;uti&gcirc;inta de la alpoj,
        konservis multajn interesajn bestojn kaj plantojn, inter ili trovi&gcirc;as 
        fi&scirc;oj, akvolilioj, skeleto de mezosa&ubreve;ro, flugsa&ubreve;ro, kraboj, terciara
        hipokampo, skorpio el la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009438.php">Fosilioj en suda Germanio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        amonito estis kapopiedulo, kiu vivis de malfrua Devonio &gcirc;is malfrua <tld lit="K"/>o kaj formortis
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36146">Amonito</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_d;&c_a;&c_v;&c_y; &c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</trd>
  <trd lng="ca">cretaci</trd>
  <trd lng="cs">k&rcaron;&iacute;da</trd>
  <trdgrp lng="de">
    <trd>Kreidezeit</trd>,
    <trd>Kreideformation</trd>
  </trdgrp>
  <trd lng="en">Cretaceous</trd>
  <trd lng="es">cret&aacute;cico</trd>
  <trd lng="fr">Cr&eacute;tac&eacute;</trd>
  <trd lng="hu">kr&eacute;takor</trd>
  <trdgrp lng="nl">
    <trd>krijt<klr tip="ind"> (periode)</klr></trd>,
    <trd>krijtperiode</trd>
  </trdgrp>
  <trd lng="pl">kreda <klr>(okres)</klr></trd>
  <trd lng="pt">Cret&aacute;ceo</trd>
  <trd lng="ro">cretacic</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_v;&c_o;&c_j; &c_p;&c_je;&c_r;&c_i;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>krieda</trd>,
    <trd>kriedov&yacute; &uacute;tvar</trd>
  </trdgrp>
</drv>
</art>
