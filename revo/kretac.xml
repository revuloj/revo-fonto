<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kretace</rad>/o
</kap>
<drv mrk="kretac.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      La plej supra formacio de la sekundara epoko.
    </dif>
    <refgrp tip="vid">
      <ref cel="jxuras.0o">&jcirc;uraso</ref>,
      <ref cel="lias.0o">liaso</ref>,
      <ref cel="trias.0o">triaso</ref>,
      <ref cel="dias.0o">diaso</ref>,
      <ref cel="devon.0o">devono</ref>,
      <ref cel="silur.0o">siluro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_d;&c_a;&c_v;&c_y; &c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</trd>
  <trd lng="ca">cretaci</trd>
  <trdgrp lng="de">
     <trd>Kreidezeit</trd>,
     <trd>Kreideformation</trd>
  </trdgrp>
  <trd lng="en">Cretaceous</trd>
  <trd lng="es">cret&aacute;cico</trd>
  <trd lng="hu">kr&eacute;takor</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_v;&c_o;&c_j; &c_p;&c_je;&c_r;&c_i;&c_o;&c_d;</trd>
</drv>
</art>
