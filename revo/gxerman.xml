<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&gcirc;erman</rad>/oj
</kap>
<drv mrk="gxerman.0oj">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Vasta grupo de antikvaj triboj, apartenintaj al
      la <ref tip="vid" cel="hind.0euxropa">hinde&ubreve;ropa</ref>
      lingvofamilio, kiuj en la 1a jarcento a.K. okupis teritorion
      inter <ref tip="vid" cel="rejn.0o">Rejno</ref>
      kaj Vistulo, <ref tip="vid" cel="danub.0o">Danubo</ref>
      kaj Norda Maro kaj sudan Skandinavion.
    </dif>
    <ref tip="vid" cel="german1.0o">germano</ref>
  </snc>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_m;&c_a;&c_n;&c_c;&c_y;</trd>
</drv>

<drv mrk="gxerman.0a">
  <kap>
    <tld/>aj lingvoj
  </kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Grupo de parencaj
      <ref tip="super" cel="hind.0euxropa">hinde&ubreve;ropaj</ref>
      lingvoj, devenintaj el lingvoj de la antikvaj <tld/>oj:
      la <ref tip="sub" cel="german1.0a">germana</ref>,
      la <ref tip="sub" cel="nederl.0a">nederlanda</ref>,
      la <ref tip="sub" cel="angl.0a">angla</ref>,
      la dana, la sveda,
      <ref tip="sub" cel="jid.0o">Jido</ref> kaj aliaj.
    </dif>
  </snc>
  <trd lng="en">Germanic languages</trd>
  <trd lng="de">germanische Sprachen</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je; &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>