<vortaro>
 <art mrk="$Id$">
  <kap>
   <rad>babord</rad>/o
   <fnt>Z</fnt>
  </kap>
  <drv mrk="babord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
   <dif>
    Maldekstra flanko de &scirc;ipo rigardante al la pruo: <ekz>la <tld/>o 
    estas signita per ru&gcirc;a fajro.
    </ekz>
    </dif>
   <refgrp tip="ant">
   <ref cel="tribor.0o">tribordo</ref>.
   </refgrp>
  </snc>
  <trd lng="de">Backbord</trd>
  <trd lng="en">port</trd>
  <trd lng="es">babor</trd>
  <trd lng="fr">babord</trd>
  <trd lng="hu">haj&oacute; <ind>baloldala</ind></trd>
  <trd lng="nl">bakboord</trd>
  <trd lng="pt">bombordo</trd>
  <trdgrp lng="ru">
   <trd>&c_b;&c_a;&c_k;&c_b;&c_o;&c_r;&c_t;</trd>,
   <trd>&c_l;&c_je;&c_v;&c_y;&c_j; &c_b;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="sv">babord</trd>
  </drv>
 </art>
