 <art mrk="$Id: babord.xml,v 1.24 2023/11/08 09:26:28 revo Exp $">
  <kap>
   <rad>babord</rad>/o
   <fnt>Z</fnt>
  </kap>
  <drv mrk="babord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
   <dif>
    Maldekstra flanko de &scirc;ipo rigardante al la <ref tip="vid" cel="pru.0o">pruo</ref>:
    <ekz>
      la <tld/>o estas signita per ru&gcirc;a fajro.
    </ekz>
    </dif>
   <refgrp tip="ant">
     <ref cel="tribor.0o">tribordo</ref>.
   </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_k;&c_b;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_v;&c_y; <ind>&c_b;&c_o;&c_r;&c_t;</ind> <klr tip="amb">(&c_v;&c_a;&c_d;&c_a;&c_p;&c_l;&c_a;&c_v;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_t;</trd>
  <trd lng="br">babourzh</trd>
  <trd lng="ca">babord</trd>
  <trd lng="cs">lev&#xE1; strana lodi</trd>
  <trd lng="de">Backbord</trd>
  <trd lng="en">port</trd>
  <trd lng="es">babor</trd>
  <trd lng="fr">babord</trd>
  <trd lng="hu">haj&oacute; <ind>baloldala</ind></trd>
  <trd lng="it">babordo</trd>
  <trd lng="ja">&#x5DE6;&#33335;  <pr>&#12373;&#12370;&#12435;</pr></trd>
  <trd lng="nl">bakboord</trd>
  <trdgrp lng="pl">
    <trd>bakburta</trd>,
    <trd>lewa <ind>burta</ind></trd>
  </trdgrp>
  <trd lng="pt">bombordo</trd>
  <trdgrp lng="ru">
   <trd>&c_b;&c_a;&c_k;&c_b;&c_o;&c_r;&c_t;</trd>,
   <trd>&c_l;&c_je;&c_v;&c_y;&c_j; &c_b;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">&#x13E;av&#xE1; strana lode</trd>
  <trd lng="sv">babord</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&#x43A;&c_b;&#x43E;&c_r;&c_t;</trd>,
    <trd>&#x43B;&c_ib;&c_v;&c_i;&c_j; &c_b;&#x43E;&c_r;&c_t;</trd>
  </trdgrp>
</drv>
