<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afgan</rad>/o
</kap>

<drv mrk="afgan.0ujo">
  <kap>
    <tld lit="A"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="afgan.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AF</mlg>
    <dif>
      &Scirc;tato en la sudcentra Azio kun &ccirc;efurbo Kabulo.
      Oficiala nomo: <em>Islama Respubliko de Afganujo</em>.
      &Gcirc;i limas kun <ref tip="vid"
      cel="cxin.Cx0ujo">&Ccirc;inujo</ref>,
      <ref tip="vid" cel="iran.0o.GEOG">Irano</ref>,
      <ref tip="vid" cel="pakistan.0o.GEOG">Pakistano</ref>,
      <ref tip="vid" cel="tagxik.T0ujo.GEO">Ta&gcirc;ikujo</ref>,
      <ref tip="vid" cel="turkmen.0ujo.GEOG">Turkmenujo</ref>
      kaj <ref tip="vid" cel="uzbek.0ujo">Uzbekujo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Afganistan</trd>,
      <trd>Islamska Republika <ind>Afganistanu</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="afgan.0io">
  <kap>
    <tld lit="A"/>io
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="afgan.0io.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AF</mlg>
    <dif>
      <ref tip="dif" cel="afgan.0ujo.GEOG">Afganujo</ref>
    </dif>
    <trdgrp lng="pl">
      <trd>Afganistan</trd>,
      <trd>Islamska Republika <ind>Afganistanu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

