<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemora.xml,v 1.20 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>hemoragi</rad>/o
</kap>
<drv mrk="hemora.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Sangelfluo, sangado:
      <ekz>
        unu el la plej dan&gcirc;eraj virusaj malsanoj, kiu ofti&gcirc;is pro plivarmi&gcirc;o, estas la
        konga-krimea <tld/>a febro <klr>[&#x2026;]</klr>, por la malsano estas tipaj
        multnombraj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/009243.php">Epidemiaj sekvoj en E&ubreve;ropo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_m;&c_a;&c_r;&c_a;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_v;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_c;&c_mol;&c_c;&c_jo;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_r;&c_malmol;&c_v;&c_o;&c_i;&c_z;&c_l;&c_i;&c_v;</trd>,
    <trd>&c_h;&c_je;&c_m;&c_o;&c_r;&c_a;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">hemorr&agrave;gia</trd>
  <trdgrp lng="de">
    <trd>H&auml;morrhagie</trd>,
    <trd>Gewebeblutung</trd>
  </trdgrp>
  <trd lng="el">&alfa;&jota;&my;&omikron;&rho;&rho;&alfa;&gamma;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>bleeding</trd>,
    <trd>hemorrhage</trd>
  </trdgrp>
  <trd lng="es">hemorragia</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;morragie</trd>,
    <trd>saignement</trd></trdgrp>
  <trd lng="it">emorragia</trd>
  <trd lng="ja">&#x51FA;&#x8840;  <pr>&#x3057;&#x3085;&#x3063;&#x3051;&#x3064;</pr></trd>
  <trd lng="nl">bloeding</trd>
  <trd lng="pl">krwotok</trd>
  <trd lng="pt">hemorragia</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_a;&c_g;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_v;&c_o;&c_i;&c_z;&c_l;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x435;&#x43C;&#x43E;&#x440;&#x430;&#x433;&#x456;&#x44F;</trd>,
    <trd>&#x43A;&#x440;&#x43E;&#x432;&#x43E;&#x432;&#x438;&#x43B;&#x438;&#x432;</trd>
  </trdgrp>
  <trd lng="zh">&#x51FA;&#x8840; <pr>ch&#x16B;xu&#xE8;</pr></trd>
</drv>

<drv mrk="hemora.0a">
  <kap>
    <tld/>a
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        el la virusaj <tld/>aj febroj la plej dan&gcirc;era estas la Ebola-malsano
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html">Damokla glavo super la homaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_r;&c_a;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_h;&c_je;&c_m;&c_o;&c_r;&c_a;&c_g;&c_i;&c_ch;&c_n;&c_a;</trd>
  <trd lng="ca">hemorr&agrave;gic</trd>
  <trdgrp lng="de">
    <trd>h&auml;morrhagisch</trd>,
    <trd>gewebsblutend</trd>
  </trdgrp>
  <trd lng="en">hemorrhagic</trd>
  <trd lng="es">hemorr&aacute;gico</trd>
  <trd lng="fr">h&eacute;morragique</trd>
  <trd lng="it">emorragico</trd>
  <trd lng="ja">&#x51FA;&#x8840;&#x6027;&#x306E;  <pr>&#x3057;&#x3085;&#x3063;&#x3051;&#x3064;&#x305B;&#x3044;&#x306E;</pr></trd>
  <trd lng="pt">hemorr&aacute;gico</trd>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_a;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="zh">&#x51FA;&#x8840;&#x6027; <pr>ch&#x16B;xu&#xE8;x&#xEC;ng</pr></trd>
</drv>
</art>
