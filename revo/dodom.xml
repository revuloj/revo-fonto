<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: dodom.xml,v 1.6 2023/11/04 18:50:51 afido Exp $">
    <kap>
      <rad>Dodoma</rad>/o, 
      <var><kap><rad var="m">Dodom</rad>/o</kap></var>
    </kap>

<drv mrk="dodom.0o">
  <kap><tld/>o, <var><kap><tld var="m"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="dodom.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="tanzani.T0o">Tanzanio</ref>:
      <ekz>
        <tld/>o estas la nova &ccirc;efurbo, kie la federacia parlamento situas
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Tanzanio">Tanzanio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efurbo <tld var="m"/>o trovi&gcirc;as &ccirc;e la koro de la lando, sed la &ccirc;efa ekonomia centro
        estas la iama &ccirc;efurbo Daresalamo &ccirc;e la marbordo
        <fnt>
          <vrk><url ref="https://esperanto-afriko.org/Kongresa%20libro.pdf">Kajero de la 6-a Afrika Kongreso de Esperanto de UEA</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_a;&c_d;&c_o;&c_m;&c_a; <klr>(&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a; &c_T;&c_a;&c_n;&c_z;&c_a;&c_n;&c_ib;&c_ib;)</klr></trd>
    <trd lng="de">Dodoma</trd>
    <trd lng="en">Dodoma</trd>
    <trd lng="fr">Dodoma</trd>
    <trd lng="zh">&#x591A;&#x591A;&#x99AC; <pr>du&#x14D;du&#x14D;m&#x1CE;</pr></trd>
</snc>
  <trd lng="ja">&#x30C9;&#x30C9;&#x30DE;</trd>
</drv>

  </art>

