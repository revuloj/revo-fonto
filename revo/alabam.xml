<art mrk="$Id: alabam.xml,v 1.8 2023/10/28 08:33:04 revo Exp $">
<kap>
  <rad>Alabam</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="alabam.0o">
  <kap><tld/>o</kap>
  <snc mrk="alabam.0o.GEOG_rivero">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en Usono.
    </dif>
  </snc>
  <snc mrk="alabam.0o.GEOG_sxtato">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la suda parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        Johano lo&gcirc;as en <em>Mobile</em>, <tld/>o
        <fnt>
          <aut>J. Bentley</aut>,
          <vrk><url ref="http://bulteno.esperanto-usa.org/2013/6/50-vojagxe-tra-usono.html">
          Lumineska akvo en Alabamo</url></vrk>,
          <lok>Usona Esperantisto, 2013:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_l;&c_ja;&c_b;&c_a;&c_m;&c_a;</trd>
  <trd lng="cs">Alabama</trd>
  <trd lng="en">Alabama</trd>
  <trd lng="fr">Alabama</trd>
  <trd lng="ja">&#x30A2;&#x30E9;&#x30D0;&#x30DE;&#x5DDE;  <pr>&#x3042;&#x3089;&#x3070;&#x307E;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="sk">Alabama</trd>
  <trd lng="uk">&c_A;&#x43B;&c_a;&c_b;&c_a;&#x43C;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E9A;&#x62C9;&#x5DF4;&#x9A6C; <pr>y&#xE0;l&#x101;b&#x101;m&#x1CE;</pr></trd>,
    <trd>&#x4E9E;&#x62C9;&#x5DF4;&#x99AC; <pr>y&#xE0;l&#x101;b&#x101;m&#x1CE;</pr></trd>,
    <trd>&#x4E9A;&#x62C9;&#x5DF4;&#x9A6C;&#x5DDE; <pr>y&#xE0;l&#x101;b&#x101;m&#x1CE;zh&#x14D;u</pr></trd>,
    <trd>&#x4E9E;&#x62C9;&#x5DF4;&#x99AC;&#x5DDE; <pr>y&#xE0;l&#x101;b&#x101;m&#x1CE;zh&#x14D;u</pr></trd>,
    <trd>&#x4E9A;&#x62C9;&#x5DF4;&#x5417; <pr>y&#xE0;l&#x101;b&#x101;ma</pr></trd>,
    <trd>&#x4E9E;&#x62C9;&#x5DF4;&#x55CE; <pr>y&#xE0;l&#x101;b&#x101;ma</pr></trd>,
    <trd>&#x963F;&#x62C9;&#x5DF4;&#x99AC;&#x5DDE; <pr>&#x101;l&#x101;b&#x101;m&#x1CE;zh&#x14D;u</pr></trd>,
    <trd>&#x963F;&#x62C9;&#x5DF4;&#x9A6C;&#x5DDE; <pr>&#x101;l&#x101;b&#x101;m&#x1CE;zh&#x14D;u</pr></trd>
