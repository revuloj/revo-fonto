<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elizabet.xml,v 1.11 2022/03/17 13:47:18 revo Exp $">
<kap>
   <ofc>*</ofc><ofc>7</ofc>
    <rad>Elizabet</rad>/o <fnt><bib>NT</bib></fnt>
</kap>
<drv mrk="elizabet.0o">
  <kap>
  <ofc>*</ofc>
  <tld/>o</kap>
  <snc mrk="elizabet.0o.virino">
   <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
      hebredevena, i.a. la nomo de la patrino de Johano la Baptisto:    
      <ekz>
        Petro, Anno kaj <tld/>o estas miaj gefratoj
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>;
        via pre&gcirc;o estas a&ubreve;dita kaj via edzino <tld/>o naskos al vi 
        filon kaj vi donos al li la nomon Johano
        <fnt><bib>NT</bib>, <lok>Luko 1:13</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>Al&zcaron;b&ecaron;ta</trd>,
    <trd>Elizabeta</trd>,
    <trd>Eli&scaron;ka</trd>
  </trdgrp>
  <trd lng="de">Elisabeth</trd>
  <trd lng="en">Elizabeth</trd>
  <trdgrp lng="fr">
    <trd>&Eacute;lisabeth</trd>,
    <trd>&Eacute;lisa</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>El&zdot;bieta</trd>,
    <trd>Ela <klr>(zdrob.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Elisabete</trd>
  <trdgrp lng="sk">
    <trd>Eli&scaron;ka</trd>,
    <trd>Izabela</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x404;&#x43B;&#x438;&#x441;&#x430;&#x432;&#x435;&#x442;&#x430;</trd>,
    <trd>&#x404;&#x43B;&#x438;&#x437;&#x430;&#x432;&#x435;&#x442;&#x430;</trd>
  </trdgrp>
</drv>
</art>

