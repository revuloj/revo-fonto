<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elizabet.xml,v 1.14 2023/11/07 20:34:10 revo Exp $">
<kap>
   <ofc>*</ofc><ofc>7</ofc>
    <rad>Elizabet</rad>/o <fnt><bib>NT</bib></fnt>
</kap>
<drv mrk="elizabet.0o">
  <kap>
  <ofc>*</ofc>
  <tld/>o</kap>
  <snc mrk="elizabet.0o.virino">
   <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
      hebredevena, i.a. la nomo de la patrino de Johano la Baptisto:    
      <ekz>
        Petro, Anno kaj <tld/>o estas miaj gefratoj
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>;
        via pre&gcirc;o estas a&ubreve;dita kaj via edzino <tld/>o naskos al vi 
        filon kaj vi donos al li la nomon Johano
        <fnt><bib>NT</bib>, <lok>Luko 1:13</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_l;&c_mol;&c_zh;&c_b;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_L;&c_ib;&c_z;&c_a;&c_v;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_E;&c_l;&c_ib;&c_z;&c_a;&c_b;&c_e;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>Al&zcaron;b&ecaron;ta</trd>,
    <trd>Elizabeta</trd>,
    <trd>Eli&scaron;ka</trd>
  </trdgrp>
  <trd lng="de">Elisabeth</trd>
  <trd lng="en">Elizabeth</trd>
  <trdgrp lng="fr">
    <trd>&Eacute;lisabeth</trd>,
    <trd>&Eacute;lisa</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30EA;&#x30B6;&#x30D9;&#x30B9;</trd>,
    <trd>&#x30A8;&#x30EA;&#x30B6;&#x30D9;&#x30FC;&#x30C8;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>El&zdot;bieta</trd>,
    <trd>Ela <klr>(zdrob.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Elisabete</trd>
  <trdgrp lng="sk">
    <trd>Eli&scaron;ka</trd>,
    <trd>Izabela</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_Jeu;&#x43B;&c_i;&c_s;&c_a;&c_v;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_Jeu;&#x43B;&c_i;&c_z;&c_a;&c_v;&c_je;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F0A;&#x4E3D;&#x838E;&#x767D; <pr>y&#x12B;l&#xEC;sh&#x101;b&#xE1;i</pr></trd>,
    <trd>&#x4F0A;&#x8389;&#x838E;&#x767D; <pr>y&#x12B;l&#xEC;sh&#x101;b&#xE1;i</pr></trd>
  </trdgrp>
</drv>
</art>

