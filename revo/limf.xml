<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>limf</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="limf.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Flaveta a&ubreve; senkolora likva&jcirc;o konsistanta el likva substanco kaj el
      le&ubreve;kocitoj de la sango, kaj kiu cirkulas en angioj kaj nutras la
      histojn.
    </dif>
    <trd lng="en">lymph</trd>
    <trd lng="fr">lymphe</trd>
    <trd lng="nl">lymfevocht</trd>
    <trd lng="ru">&c_l;&c_i;&c_m;&c_f;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      La akveca suko, cirkulanta en plantoj.
    </dif>
  </snc>
</drv>

<drv mrk="limf.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o a&ubreve; enhavanta <tld/>on.
    </dif>
    <trd lng="en">lymphatic</trd>
    <trd lng="fr">lymphatique</trd>
  </snc>
</drv>

<drv mrk="limf.0a.angio">
  <kap>
    <tld/>a angio, vazo
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Angio, vazo, en kiu cirkulas <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">vaisseau lymphatique</trd>
  <trd lng="nl">lymfevaten</trd>
</drv>
</art>
