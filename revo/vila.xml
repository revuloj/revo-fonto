<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vila</rad>/o
</kap>
<drv mrk="vila.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Somerdomo, kampodomo:
      <ekz>
        ni vidis ke multaj vila&gcirc;anoj konstruis jam belajn <tld/>ojn,
        meblitajn kaj provizitajn per akvo kaj elektra energio, kiel en
        la
        urbo<fnt><aut>Bardhyl Selimi</aut>: <vrk>Voja&gcirc;impresoj, somero
        2000</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Villa</trd>
  <trd lng="el">&beta;&jota_ton;&lambda;&lambda;&alfa;</trd>
  <trdgrp lng="en">
    <trd>villa</trd>,
    <trd>cottage</trd>,
    <trd>country cottage</trd>,
    <trd>seaside cottage</trd>,
    <trd>summer residence</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;sidence secondaire</trd>,
    <trd>villa</trd>
  </trdgrp>
  <trd lng="hu">villa <klr tip="ind">(h&aacute;z)</klr></trd>
  <trdgrp lng="nl">
    <trd>villa</trd>,
    <trd>landhuis</trd>
  </trdgrp>
  <trd lng="pt">vila</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_l;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
</drv>
</art>
