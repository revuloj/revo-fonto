  <trd lng="es">Ram&oacute;n</trd>
  <trd lng="fr">Raymond</trd>
  <trd lng="it">Raimondo</trd>
  <trd lng="lat">Raymundus</trd>
  <trd lng="lt">Rajmundas</trd>
  <trd lng="nl">Raymond</trd>
  <trd lng="pl">Rajmund</trd>
  <trd lng="pt">Raimundo</trd>
  <trd lng="ro">Raymond</trd>
  <trd lng="zh">&#x96F7;&#x8499;&#x5FB7; <pr>L&#xE9;im&#xE9;ngd&#xE9;</pr></trd>
