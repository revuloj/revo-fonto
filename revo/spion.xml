<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spion.xml,v 1.25 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>spion</rad>/o
</kap>
<drv mrk="spion.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="spion.0o.MIL">
    <dif>
      Homo komisiita de la a&ubreve;toritatoj por ka&scirc;e observi la militajn kaj
      aliajn agojn de malamiko:
      <ekz>
        vi estas <tld/>oj: vi venis, por vidi la malfortajn lokojn de la lando
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 42:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa generalo ekspedis <tld/>ojn al la proksimi&gcirc;antaj Libianoj, por ekscii
        ilian nombron kaj ordon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas al mi kvaza&ubreve; &ccirc;iam <tld/>o el la infero akompanus &ccirc;iun mian pa&scirc;on
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne turisto li estas, sed <tld/>o
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 1a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avanga.0o.MIL">avangardo<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_p;&c_ib;&c_jo;&c_n;</trd>,
      <trd>&c_v;&c_ib;&c_zh;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&eacute;m</trd>,
      <trd>felder&iacute;t&odblac;</trd>,
      <trd>spion</trd>
    </trdgrp>
    <trd lng="it">spia</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_d;&c_ch;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="spion.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo sekrete observanta ies farojn.
    </dif>
    <ref tip="vid" cel="observ.0anto">observanto</ref>
    <trd lng="be">&c_sh;&c_p;&c_ib;&c_jo;&c_n;</trd>
    <trd lng="hu">spion</trd>
    <trdgrp lng="it">
      <trd>spia</trd>,
      <trd>spione</trd>
      <trd>infame<klr> (gerg.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="ca">espia</trd>
  <trdgrp lng="cs">
    <trd>vyzv&ecaron;da&ccaron;</trd>,
    <trd>zv&ecaron;d</trd>,
    <trd>&scaron;peh</trd>,
    <trd>&scaron;pion</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spion</trd>,
    <trd>Sp&auml;her</trd>
  </trdgrp>
  <trd lng="en">spy</trd>
  <trd lng="es">esp&iacute;a</trd>
  <trd lng="fr">espion</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30D1;&#x30A4;  <pr>&#12377;&#12401;&#12356;</pr></trd>,
    <trd>&#38291;&#x8ADC;  <pr>&#x304B;&#12435;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#x8ADC;&#22577;&#x90E8;&#x54E1;  <pr>&#12385;&#12423;&#12358;&#x307B;&#12358;&#12406;&#12356;&#12435;</pr></trd>,
    <trd>&#x5BC6;&#20597;  <pr>&#x307F;&#12387;&#12390;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">spion</trd>
  <trdgrp lng="pl">
    <trd>szpieg</trd>,
    <trd>szpicel</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>vyzveda&ccaron;</trd>,
    <trd>&scaron;peh</trd>,
    <trd>&scaron;pi&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_p;&c_i;&c_g;&c_u;&c_n;</trd>,
    <trd>&c_t;&c_a;&c_jeu;&c_m;&c_n;&c_i;&c_j; &c_a;&c_gu;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="spion.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="observ.kasx0i">Ka&scirc;observi</ref> la agadon de
      malamiko:
      <ekz>
        <tld/>u lin, observu &ccirc;iun lian pa&scirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        el &gcirc;ardeno elrigardis iu homo kaj <tld/>is la voja&gcirc;anton
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ruze envenis, por <tld/>i nian liberecon
        <fnt>
          <bib>NT</bib>
          <lok>Galatoj 2:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i iradis en la familiojn, <klr>[&#8230;]</klr> por el<tld/>i la malkorekta&jcirc;ojn de la
        proksimuloj
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ennesti&gcirc;as en kafejoj, bordeloj, drinkejoj, vi <tld/>as, sondas, kiu
        <klr>[&#8230;]</klr> plej multe insultas la registaron
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rigardis &ccirc;irka&ubreve; &scirc;i, &ccirc;u neniu lin <tld/>as
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref tip="vid" cel="esplor.0i">esplori</ref>,
      <ref cel="sond.0i.FIG">sondi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_ib;&c_jo;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">espiar</trd>
  <trdgrp lng="cs">
    <trd>prov&aacute;d&ecaron;t &scaron;pion&aacute;&zcaron;</trd>,
    <trd>&scaron;pehovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spionieren</trd>,
    <trd>sp&auml;hen</trd>
  </trdgrp>
  <trd lng="es">espiar</trd>
  <trd lng="fr">espionner</trd>
  <trd lng="hu">k&eacute;mkedik</trd>
  <trd lng="it">spiare</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30D1;&#x30A4;&#12377;&#x308B;  <pr>&#12377;&#12401;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#12402;&#x305D;&#x304B;&#x306B;&#x63A2;&#x308B; 
    <pr>&#12402;&#x305D;&#x304B;&#x306B;&#12373;&#12368;&#x308B;</pr></trd>,
    <trd>&#x76E3;&#35222;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#12375;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bespioneren</trd>
  <trdgrp lng="pl">
    <trd>szpiegowa&cacute;</trd>,
    <trd>dyba&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">prev&aacute;dza&tcaron; &scaron;pion&aacute;&zcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_p;&c_i;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_sh;&c_p;&c_i;&c_g;&c_u;&c_n;&c_s;&c_t;&c_v;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
</art>
