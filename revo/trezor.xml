<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trezor</rad>/o
</kap>
<drv mrk="trezor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Amaso da ka&scirc;itaj moneroj a&ubreve; multvaloraj objektoj:
      <ekz>
        trovi <tld/>on en ruinoj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io tre &scirc;atinda:
      <ekz>
        tiu libro estas vera <tld/>o de scienco;
      </ekz>
      <ekz>
        arkitektura, lingva <tld/>o;
      </ekz>
      <ekz>
        la verko de Z estas <tld/>o por la homaro.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karesa nomo pp iu tre kara:
      <ekz>
        mia an&gcirc;elo, mia <tld/>o<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esper.0o">espero 3</ref>,
      <ref cel="gxoj.0o">&gcirc;ojo 2</ref>,
      <ref cel="glor.0o">gloro 2</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">tresor</trd>
  <trd lng="de">Schatz</trd>
  <trd lng="en">treasure</trd>
  <trd lng="fr">tr&eacute;sor</trd>
  <trd lng="lat">thesaurus, i</trd>
  <trd lng="nl">schat</trd>
  <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
</drv>

<drv mrk="trezor.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie oni gardas <tld/>on a&ubreve; multvaloran monon.
    </dif>
  </snc>
  <trd lng="lat">aerarium, ii</trd>
  <trd lng="nl">schatkamer</trd>
  <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
