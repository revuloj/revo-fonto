<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trezor.xml,v 1.34 2024/02/08 21:21:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trezor</rad>/o
</kap>

<drv mrk="trezor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trezor.0o.EKON">
    <dif>
      Amaso da multvaloraj objektoj &ndash; oro, ar&gcirc;ento, juveloj:
    </dif>
    <subsnc mrk="trezor.0o.juveloj">
      <uzo tip="fak">EKON</uzo>
      <dif>
        Rezerve tenata:
        <ekz>
          promesita <tld/>o estas sen valoro
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          ne timu <klr>[&#8230;]</klr> via patro donis al vi <tld/>on en viajn sakojn
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 43:23</lok>
          </fnt>;
        </ekz>
        <ekz>
          pli bona estas malmulto kun timo anta&ubreve; la Eternulo,
          ol granda <tld/>o kun maltrankvileco &ccirc;e &gcirc;i
          <fnt><bib>MT</bib>, <lok>&Sen; 15:16</lok></fnt>;
        </ekz>
        <ekz>
          amasigi <tld/>ojn en keloj
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro X</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <ind>&scirc;tata <tld/>o</ind>
          <klr>(anka&ubreve; <ctl>&scirc;tatkaso</ctl>)</klr>.
          <trd lng="hu">&aacute;llamkincst&aacute;r</trd>
          <trd lng="pl">skarb pa&nacute;stwa</trd>
          <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_a;</trd>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="trezor.0o.kasxita">
      <dif>
        Enterigita a&ubreve; iel ka&scirc;ita, precipe tia, ke la
        posedanto ne plu vivas kaj la situo ne estas sciata:
        <ekz>
          <tld/>oj, ka&scirc;itaj en la sablo
          <fnt>
            <bib>MT</bib>
            <lok>Readmono 33:19</lok>
          </fnt>;
        </ekz>
        <ekz>
          la regno de la &ccirc;ielo similas al ka&scirc;ita en kampo
          <tld/>o, kiun viro trovis kaj ka&scirc;is, kaj pro &gcirc;ojo li
          iras kaj vendas &ccirc;ion, kion li posedas, kaj a&ccirc;etas tiun kampon
          <fnt><bib>NT</bib>, <lok>&Mat; 13:44</lok></fnt>;
        </ekz><ekz>
          trovi <tld/>on en ruinoj<fnt>B</fnt>.
        </ekz>
      </dif>
      <trd lng="ru">&c_k;&c_l;&c_a;&c_d;</trd>
    </subsnc>
  </snc>
  <snc mrk="trezor.0o.aprezindajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io tre valora, tre &scirc;atinda:
      <ekz>
        tiu libro estas vera <tld/>o de scienco;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>amiko fidela estas <tld/>o plej bela</ind>
        <trd lng="hu">a j&oacute; bar&aacute;t dr&aacute;g&aacute;bb az
          aranyn&aacute;l</trd>
        <trd lng="pl">wierny przyjaciel to najpi&eogonek;kniejszy skarb</trd>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="ru">&c_n;&c_je; &c_i;&c_m;&c_je;&c_j;
          <ind>&c_s;&c_t;&c_o; &c_r;&c_u;&c_b;&c_l;&c_je;&c_j;</ind>, &c_a; &c_i;&c_m;&c_je;&c_j; &c_s;&c_t;&c_o; &c_d;&c_r;&c_u;&c_z;&c_je;&c_j;</trd>
      </ekz>
    </dif>
    <subsnc mrk="trezor.0o.karulo">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Karesa vorto pri io a&ubreve; iu tre kara al oni:
        <ekz>
          mia kara <tld/>o, vintro sin trovos sur vangoj
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          mia an&gcirc;elo, mia <tld/>o<fnt>B</fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="gxoj.0o">&gcirc;ojo</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="trezor.0o.verkoj">
      <dif>
        Kolekto de valoraj verkoj:
        <ekz>
          arkitektura, lingva <tld/>o;
        </ekz><ekz>
          la verko de <nom>Zamenhof</nom> estas <tld/>o por la homaro;
        </ekz>
        <ekz>
          malgranda biblioteko, kiu <klr>[&#8230;]</klr> enhavas la plej elektitajn <tld/>ojn de
          la angla literaturo
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          la <klr>[bo]</klr>patrino <klr>[&#8230;]</klr> transdonas al li la kartojn kaj
          tagolibrojn de sia mortinta edzo <klr>[&#8230;]</klr> &dash; amaso da flavi&gcirc;intaj
          paperoj <klr>[&#8230;]</klr>, por li tio &ccirc;i estas tuta trezoro, kaj li tutajn
          tagojn sidas super ili
          <fnt>
            <bib>FK</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
<!-- tio estas subsnc de trezoro FIG, sed mankas veraj cita&jcirc;oj:
  <snc mrk="trezor.0o.LIN">
    <dif>
      Kolekto da valoraj lingvaj ekzemploj a&ubreve; tekstoj, precipe:
    </dif>
    <subsnc mrk="trezor.0o.korpuso">
      <ref tip="dif" cel="korpus.0o.LIN">korpuso <sncref/></ref>
      <ekz>
        la <tld lit="T"/>o de la anglaj poemoj
        <fnt><bib>PIV2</bib></fnt>.
      </ekz>
    </subsnc>
    <subsnc mrk="trezor.0o.vortoprovizo">
      <ref tip="dif" cel="vort.0oprovizo">vortoprovizo</ref>
      <ekz>
        la <tld lit="T"/>o de la greka lingvo
        <fnt><bib>PIV2</bib></fnt>.
      </ekz>
    </subsnc>
  </snc>
-->
  <trdgrp lng="ja">
    <trd>&#x5B9D;  <pr>&#x305F;&#x304B;&#12425;</pr></trd>,
    <trd>&#x5B9D;&#29289;  <pr>&#x305F;&#x304B;&#12425;&#12418;&#x306E;</pr></trd>,
    <trd>&#x79D8;&#x5B9D;  <pr>&#12402;&#x307B;&#12358;</pr></trd>,
    <trd>&#x5BCC;  <pr>&#12392;&#x307F;</pr></trd>,
    <trd>&#x8CA1;&#x5B9D;  <pr>&#12374;&#12356;&#x307B;&#12358;</pr></trd>,
    <trd>&#x8CB4;&#x91CD;&#x306A;&#29289;  <pr>&#x304D;&#12385;&#12423;&#12358;&#x306A;&#12406;&#12388;</pr></trd>,
    <trd>&#22823;&#x4E8B;&#x306A;&#x4EBA;  <pr>&#12384;&#12356;&#12376;&#x306A;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="lat">thesaurus, i</trd>
  <trd lng="be">&c_s;&c_k;&c_a;&c_r;&c_b;</trd>
  <trd lng="ca">tresor</trd>
  <trd lng="cs">poklad</trd>
  <trd lng="de">Schatz</trd>
  <trd lng="en">treasure</trd>
  <trd lng="es">tesoro</trd>
  <trd lng="fr">tr&eacute;sor</trd>
  <trdgrp lng="hu">
    <trd>kincs</trd>,
    <trd>dr&aacute;gas&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">schat</trd>
  <trdgrp lng="pl">
    <trd>skarb</trd>,
    <trd>bogactwo</trd>
  </trdgrp>
  <trd lng="pt">tesouro</trd>
  <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
  <trd lng="sk">poklad</trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&c_r;&c_b;</trd>
</drv>

<drv mrk="trezor.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="trezor.0ejo.loko">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Loko, kie oni konservas <tld/>on a&ubreve; grandajn monsumojn:
      <ekz>
        &scirc;telisto <klr>[&#8230;]</klr> &scirc;telis juvelojn el la <tld/>ejo
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
            bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        la faraona <tld/>ejo estis tiel malplena <klr>[&#8230;]</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bona homo el la bona <tld/>ejo de sia koro liveras bonon
        <fnt>
          <bib>NT</bib>
          <lok>Luko 6:45</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Schatzkammer</trd>,
      <trd>Tresorraum</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_n;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc mrk="trezor.0ejo.servo">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Oficejo, kie oni administras <tld/>on, precipe la &scirc;tatan:
      <ekz>
        la vitrofarejo estis grava fonto de enspezoj de la faraona
        <tld/>ejo
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17a</lok></fnt>;
      </ekz><ekz>
        &scirc;tatsekretario pri la Trezorejo <fnt><bib>PIV1</bib></fnt>
        <klr>(tiel oni nomas la financministron en Usono)</klr>.
      </ekz>
    </dif>
    <ref tip="sub" cel="fisk.0o.kaso">fisko<sncref/></ref>
    <trd lng="de">Schatzamt</trd>
    <trdgrp lng="de">
      <trd>Fiskus</trd>,
      <trd>Staatskasse</trd>,
      <trd>Schatzamt</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_z;&c_n;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5B9D;&#x5EAB;  <pr>&#x307B;&#12358;&#12371;</pr></trd>,
    <trd>&#x5B9D;&#29289;&#x5EAB;  <pr>&#x307B;&#12358;&#12418;&#12388;&#12371;</pr></trd>,
    <trd>&#x56FD;&#x5EAB;  <pr>&#12371;&#12387;&#12371;</pr></trd>
  </trdgrp>
  <trd lng="lat">aerarium, ii</trd>
  <trd lng="be">&c_s;&c_k;&c_a;&c_r;&c_b;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="ca">erari</trd>
  <trdgrp lng="cs">
    <trd>klenotnice</trd>,
    <trd>pokladnice</trd>,
    <trd>trezor</trd>
  </trdgrp>
  <trd lng="en">treasury</trd>
  <trd lng="es">c&aacute;mara del tesoro</trd>
  <trd lng="hu">kincst&aacute;r</trd>
  <trd lng="nl">schatkamer</trd>
  <trd lng="pl">skarbiec</trd>
  <trdgrp lng="sk">
    <trd>pokladnica</trd>,
    <trd>trezor</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#x43A;&c_a;&c_r;&c_b;&#x43D;&c_i;&c_c;&#x44F;</trd>
</drv>

<drv mrk="trezor.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="trezor.0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Oficisto rajtigita enspezi kaj elspezi monrimedojn de &scirc;tato
      a&ubreve; organiza&jcirc;o:
      <ekz>
        la impostoj alfluas tre malrapide &ndash; diris la <tld/>isto
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 10a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      <tld lit="T"/>isto estas rajtigita mem decidi pri
      spezoj; <ref cel="kas.0isto">kasisto</ref> faras tion
      la&ubreve; ordono (interalie, de <tld/>isto) a&ubreve; la&ubreve;
      rutina regulo.
    </rim>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_r;&c_b;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">tresorer</trd>
  <trd lng="cs">pokladn&iacute;k</trd>
  <trd lng="de">Schatzmeister</trd>
  <trd lng="es">tesorero</trd>
  <trd lng="fr">tr&eacute;sorier</trd>
  <trdgrp lng="hu">
    <trd>kincst&aacute;rnok</trd>,
    <trd>kincst&aacute;ros</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CA1;&#x52D9;&#x5B98;  <pr>&#12374;&#12356;&#12416;&#x304B;&#12435;</pr></trd>,
    <trd>&#x4F1A;&#x8A08;&#x5B98;  <pr>&#x304B;&#12356;&#12369;&#12356;&#x304B;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">skarbnik</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_j;</trd>
  <trd lng="sk">pokladn&iacute;k</trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&c_r;&c_b;&#x43D;&c_i;&#x43A;</trd>
</drv>
