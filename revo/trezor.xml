<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
    <ofc>*</ofc>
    <rad>trezor</rad>/o
    </kap>

    <drv mrk="trezor.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="trezor.0o.juveloj">
      <uzo tip="fak">EKON</uzo>
	<dif>
          Amaso da multvaloraj objektoj &dash; oro, ar&gcirc;ento, juveloj,
          rezerve tenataj:
          <ekz>
            pli bona estas malmulto kun timo anta&ubreve; la Eternulo,
            Ol granda <tld/>o kun maltrankvileco &ccirc;e &gcirc;i 
            <fnt><bib>MT</bib>, <lok>&Sen; 15:16.</lok></fnt>;
          </ekz><ekz>
            la <ind>&scirc;tata <tld/>o</ind> <klr>(anka&ubreve; <ctl>&scirc;tatkaso</ctl>)</klr>.
            <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_a;</trd>
          </ekz>
        </dif>
      </snc>
      <snc mrk="trezor.0o.kasxita">
	<dif>
          <tld lit="T"/>o<sncref ref="trezor.0o.juveloj"/> enterigita
          a&ubreve; alimaniere ka&scirc;ita, precipe tia, ke &gcirc;ia posedanto ne plu
          vivas kaj &gcirc;ia situo ne estas sciata:
          <ekz>
            la regno de la &ccirc;ielo similas al ka&scirc;ita en kampo <tld/>o, kiun
            viro trovis kaj ka&scirc;is, kaj pro &gcirc;ojo li iras kaj vendas &ccirc;ion, kion
            li posedas, kaj a&ccirc;etas tiun kampon
            <fnt><bib>NT</bib>, <lok>&Mat; 13:44.</lok></fnt>;
          </ekz><ekz>
            trovi <tld/>on en ruinoj<fnt>B</fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_k;&c_l;&c_a;&c_d;</trd>
      </snc>
      <snc mrk="trezor.0o.aprezindajxo">
	<uzo tip="stl">FIG</uzo>
        <dif>
          Io tre &scirc;atinda:
	  <ekz>
  	    tiu libro estas vera <tld/>o de scienco;
          </ekz><ekz>
	    arkitektura, lingva <tld/>o;
          </ekz><ekz>
	    la verko de Z estas <tld/>o por la homaro;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>amiko fidela estas <tld/>o plej bela</ind>
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="ru">&c_n;&c_je; &c_i;&c_m;&c_je;&c_j; <ind>&c_s;&c_t;&c_o; &c_r;&c_u;&c_b;&c_l;&c_je;&c_j;</ind>, &c_a; &c_i;&c_m;&c_je;&c_j; &c_s;&c_t;&c_o; &c_d;&c_r;&c_u;&c_z;&c_je;&c_j;</trd>
          </ekz>
        </dif>
      </snc>
      <snc mrk="trezor.0o.karulo">
	<uzo tip="stl">FIG</uzo>
	<dif>
          Karesa nomo pp iu tre kara:
          <ekz>
	    mia an&gcirc;elo, mia <tld/>o<fnt>B</fnt>
          </ekz>
        </dif>
	<refgrp tip="vid">
          <ref cel="gxoj.0o">&gcirc;ojo</ref>
        </refgrp>
      </snc>
      <snc mrk="trezor.0o.korpuso">
	<ref tip="dif" cel="korpus.0o.LIN">Korpuso<sncref/>:</ref>
	<ekz>
          la <tld lit="T"/>o de la anglaj poemoj
          <fnt><bib>PIV2</bib></fnt>.
        </ekz>
      </snc>
      <snc mrk="trezor.0o.vortoprovizo">
	<ref tip="dif" cel="vort.0oprovizo">Vortoprovizo:</ref>
	<ekz>
          la <tld lit="T"/>o de la greka lingvo
          <fnt><bib>PIV2</bib></fnt>.
        </ekz>
      </snc>
      <trd lng="ca">tresor</trd>
      <trd lng="de">Schatz</trd>
      <trd lng="en">treasure</trd>
      <trd lng="fr">tr&eacute;sor</trd>
      <trd lng="lat">thesaurus, i</trd>
      <trd lng="nl">schat</trd>
      <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
    </drv>

    <drv mrk="trezor.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="fak">EKON</uzo>
      <snc mrk="trezor.0ejo.loko">
	<dif>
          Loko, kie oni konservas <tld/>on a&ubreve; grandajn monsumojn.  
        </dif>
        <trd lng="de">Tresorraum</trd>
        <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_n;&c_i;&c_c;&c_a;</trd>
      </snc>
      <snc mrk="trezor.0ejo.servo">
	<dif>
          Oficejo, kie oni administras <tld/>on (precipe la &scirc;tatan)
        </dif>
        <trd lng="de">Schatzamt</trd>
        <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
      </snc>
      <trd lng="ca">erari</trd>
      <trd lng="lat">aerarium, ii</trd>
      <trd lng="nl">schatkamer</trd>
    </drv>

    <drv mrk="trezor.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">EKON</uzo>
      <snc>
	<dif>
          Oficisto rajtigita enspezi kaj elspezi monrimedojn de &scirc;tato
          a&ubreve; organiza&jcirc;o.
        </dif>
	<rim>
          <tld lit="T"/>isto estas rajtigita mem decidi pri
          transspezoj; <ref cel="kas.0isto">kasisto</ref> faras tion
          la&ubreve; ordono (i.a., de <tld/>isto) a&ubreve; la&ubreve; rutina regulo.
        </rim>
      </snc>
      <trd lng="ca">tresorer</trd>
      <trd lng="de">Schatzmeister</trd>
      <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_j;</trd>
    </drv>

  </art>
