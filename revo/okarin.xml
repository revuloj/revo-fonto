<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>okarin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="okarin.0o">
  <kap><tld/>o</kap>
  <snc mrk="okarin.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika
      <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>
      simila al fluto sed kun globa formo:
      <ekz>
        la najbaro Totoro <klr>...</klr>
        blovas <tld/>on en la lunplena nokto
        <fnt>
          <aut>trad. P. Oudejans</aut>,
          <vrk><url ref="http://filmoj.net/mia-najbaro-totoro">
          Mia najbaro Totoro</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ocarina</trd>
</drv>
</art>

