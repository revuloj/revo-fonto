<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>miel</rad>/o
</kap>
<drv mrk="miel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sukera substanco ellaborata de la
      <ref tip="vid" cel="abel.0o">abeloj</ref> el suko de floroj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_jo;&c_d;</trd>
  <trd lng="bg">&c_m;&c_je;&c_d;</trd>   
  <trd lng="br">mel</trd>
  <trd lng="de">Honig</trd>
  <trd lng="en">honey</trd>
  <trd lng="es">miel</trd>
  <trd lng="fr">miel</trd>
  <trd lng="hu">m&eacute;z</trd>  
  <trdgrp lng="nl">
    <trd>honing</trd>,
    <trd>honig</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_jo;&c_d;</trd>
</drv>

<drv mrk="miel.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, el <tld/>o:
      <ekz>
        <tld/>a gusto, <tld/>a kuko.
      </ekz>
    </dif>
    <trd lng="de">aus <ind>Honig</ind></trd>
    <trdgrp lng="nl">
      <trd>honing-</trd>,
      <trd>honig-</trd>
    </trdgrp>
    <trd lng="en">honey</trd>
    <trdgrp lng="hu">
      <trd>m&eacute;z-</trd>,
      <trd>m&eacute;zes</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Afekte nesincere dol&ccirc;a:
      <ekz>
        <tld/>a vo&ccirc;o;
      </ekz>
      <ekz>
        flati per <tld/>aj vortoj;
      </ekz>
      <ekz>lango <tld/>a, sed koro kruela
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="log">allogi</ref>,
      <ref cel="vinagr.0o">vinagro</ref>.
    </refgrp>
    <trd lng="de">honigs&uuml;&szlig;</trd>
    <trd lng="en">sweet</trd>
    <trd lng="hu">m&eacute;zes</trd>    
    <trdgrp lng="nl">
      <trd>honingzoet</trd>,
      <trd>honigzoet</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_d;&c_o;&c_v;&c_y;</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
