<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ebri</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ebri.0a">
  <kap><tld/>a</kap>
  <snc mrk="ebri.0a.droge">
    <dif>
      Konfuzita de alkoholo:
      <ekz>
        duonmorte <tld/>a;
      </ekz>
      <ekz>
        kiel longe vi restos <tld/>a? forigu vian vinon el vi
        <fnt><bib>MT</bib>, <lok>&Sa1; 1:14</lok></fnt>;
      </ekz>
      <ekz>
        vi trinkos sangon &gcirc;is<tld/>e de Mia bu&ccirc;ofero
        <fnt><bib>MT</bib>, <lok>&Jeh; 39:19</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sobr">malsobra</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;'&c_ja;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_ib;&c_w;&c_sh;&c_y;&c_s;&c_ja;</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_s;&a_t;</trd>
    <trdgrp lng="pl">
      <trd>pijany</trd>,
      <trd>nietrze&zacute;wy</trd>,
      <trd>upojony<klr> alkoholem</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>b&ecirc;bedo</trd>,
      <trd>b&ecirc;bado</trd>,
      <trd>&eacute;brio</trd>,
      <trd>embriagado</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>berusad</trd>,
      <trd>full</trd>
    </trdgrp>
  </snc>
  <snc mrk="ebri.0a.spontanee">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konfuzita de sento, pasio, emocio:
      <ekz>
        <tld/>aj de l' &gcirc;ojo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a pro la venko;
      </ekz>
      <ekz>
        <ind>dorm<tld/>a</ind> pro subita veki&gcirc;o
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        24a</lok></fnt>.
        <trd lng="hu">&aacute;lomittas</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_je;&c_w;&c_sh;&c_y;</trd>
    <trd lng="fa">&a_s;&a_r;&a_m;&a_s;&a_t;</trd>
    <trdgrp lng="pl">
      <trd>pijany</trd>,
      <trd>upojony</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">opilý</trd>
  <trdgrp lng="de">
    <trd>betrunken</trd>,
    <trd>besoffen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>drunk</trd>,
    <trd>inebriated</trd>,
    <trd>intoxicated</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ebrio</trd>,
    <trd>borracho</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ivre</trd>,
    <trd>saoul</trd>,
    <trd>so&ucirc;l</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&eacute;szeg</trd>,
    <trd>ittas</trd>
  </trdgrp>
  <trd lng="id">mabuk</trd>
  <trd lng="nl">dronken</trd>
  <trdgrp lng="pt">
    <trd>b&ecirc;bedo</trd>,
    <trd>b&ecirc;bado</trd>,
    <trd>&eacute;brio</trd>,
    <trd>embriagado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_mol;&c_ja;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">opitý</trd>
  <trd lng="sv">berusad</trd>
  <trd lng="tp">nasa</trd>
</drv>

<drv mrk="ebri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ebri.0i.PSI">
    <dif>
      Senti konfuzon pro alkoholo, similefika drogo a&ubreve; sento:
      <ekz>
        la maato <tld/>is kaj ne sobri&gcirc;is anta&ubreve; Bulonjo
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fa">&a_m;&a_s;&a_t; &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>saoul</ind></trd>,
    <trd>&ecirc;tre <ind>so&ucirc;l</ind></trd>
  </trdgrp>
  <trd lng="hu">r&eacute;szeg</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>pijanym</ind></trd>,
    <trd>by&cacute; <ind>nietrze&zacute;wym</ind></trd>,
    <trd>by&cacute; <ind>upojonym</ind><klr> (zw&lstroke;aszcza
    alkoholem)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol;
  <ind>&c_p;&c_mol;&c_ja;&c_n;&c_y;&c_m;</ind></trd>
</drv>

<drv mrk="ebri.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="ebri.0o.PSI_alkohole">
    <dif>
      Animstato de <tld/>a homo:
      <ekz>
        vera opinio montri&gcirc;as en <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de amo, de entuziasmo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ebri.0o.PSI_gxenerale">
    <dif>
      &Ccirc;ia animstato simila al alkoholinfluo,
      malfortiganta la memregon kaj
      <ref tip="vid" cel="ekzalt.0o.PSI">ekzaltanta</ref>
      la sentojn:
      <ekz>
        la &ccirc;evaloj &scirc;ajnis kaptitaj de rapid-<tld/>eco
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li 
        venis trakosme?</vrk>, <lok>1980</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="delir.0o">deliro</ref>,
    <ref cel="frenez">frenezo</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;
  <klr>(&c_s;&c_t;&c_a;&c_n;)</klr></trd>
  <trd lng="de">Trunkenheit</trd>
  <trdgrp lng="en">
    <trd>drunkenness</trd>,
    <trd>intoxication</trd>
  </trdgrp>
  <trd lng="es">embriaguez</trd>
  <trd lng="fa">&a_m;&a_s;&a_t;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;bri&eacute;t&eacute;</trd>,
    <trd>ivresse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&eacute;szegs&eacute;g</trd>,
    <trd>ittass&aacute;g</trd>
  </trdgrp>
  <trd lng="id">[<ind>mabuk</ind>] kemabukan</trd>
  <trd lng="nl">dronkenschap</trd>
  <trdgrp lng="pl">
    <trd>bycie <ind>pijanym</ind></trd>,
    <trd>bycie <ind>nietrze&zacute;wym</ind></trd>,
    <trd>nietrze&zacute;wo&sacute;&cacute;</trd>,
    <trd>bycie <ind>upojonym</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>opilstvo</trd>,
    <trd>opojenie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>opilost</trd>,
    <trd>opilství</trd>,
    <trd>opojení</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="ebri.0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta mensan konfuzon de <tld/>o a&ubreve; simila al
      <tld/>o:
      <ekz>
        la vino estas blasfemanto, <tld/>iga trinka&jcirc;o estas
        sova&gcirc;a
        <fnt><bib>MT</bib>, <lok>&Sen; 20:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_ja;&c_ju;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>omamující</trd>,
    <trd>opojný</trd>
  </trdgrp>
  <trd lng="en">intoxicating</trd>
  <trd lng="fa">&a_m;&a_s;&a_t;&f_y;&zwnj;&a_A_madda;&a_w;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>alcoolis&eacute;</trd>,
    <trd>enivrant</trd>
  </trdgrp>
  <trd lng="hu">r&eacute;szeg&iacute;t&odblac;</trd>
  <trd lng="id">[<ind>mabuk</ind>] memabukkan</trd>
  <trd lng="nl">benevelend</trd>
  <trd lng="pl">upajaj&aogonek;cy</trd>
  <trd lng="ru">&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>omamujúci</trd>,
    <trd>opojný</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0igajxo">
  <kap><tld/>iga&jcirc;o<fnt>Z</fnt></kap>
  <snc mrk="ebri.0igajxo.KOMUNE">
    <dif>
      <tld lit="E"/>iga substanco.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_n;&c_a;&c_p;&c_o;&c_j;, &c_p;&c_ib;&c_t;&c_v;&c_o;</trd>
  <trd lng="de">Ges&ouml;ff</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_w;&a_sh;&a_A;&a_b;&a_h;&a_hamza_sure; &a_A;&a_l;&f_k;&a_l;&f_y;</trd>,
    <trd>&a_m;&a_sh;&a_r;&a_w;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">boisson<klr tip="ind"> (alcoolis&eacute;e)</klr></trd>
  <trd lng="hu">r&eacute;szeg&iacute;t&odblac; ital</trd>
  <trdgrp lng="id">
    <trd><ind>minum</ind>an beralkohol</trd>,
    <trd><ind>minum</ind>an keras</trd>
  </trdgrp>
  <trd lng="nl">alcoholische drank</trd>
  <trdgrp lng="pl">
    <trd>nap&oacute;j alkoholowy</trd>,
    <trd>nap&oacute;j wyskokowy</trd>,
    <trd>woda ognista</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_n;&c_a;&c_p;&c_i;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="ebri.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ebri.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        ni <tld/>igu nian patron per vino, kaj ni ku&scirc;u kun li,
        por ke ni aperigu de nia patro idaron
        <fnt><bib>MT</bib>, <lok>&Gen; 19:32</lok></fnt>;
      </ekz>
      <ekz>
        lin <tld/>igis la sukceso<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>omámit</trd>,
    <trd>opojit</trd>,
    <trd>opíjet</trd>,
    <trd>opít</trd>
  </trdgrp>
  <trd lng="en">intoxicate</trd>
  <trd lng="es">emborrachar<klr tip="amb"> (a otro)</klr></trd>
  <trd lng="fa">&a_m;&a_s;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>enivrer</trd>,
    <trd>griser</trd>,
    <trd>saouler</trd>,
    <trd>souler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ler&eacute;szeg&iacute;t</trd>,
    <trd>r&eacute;szegg&eacute; tesz</trd>
  </trdgrp>
  <trd lng="id">[<ind>mabuk</ind>] memabukkan</trd>
  <trdgrp lng="pl">
    <trd>upija&cacute;</trd>,
    <trd>upaja&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>omámiť</trd>,
    <trd>opiť</trd>,
    <trd>opojiť</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ebri.0igxi.KOMUNE">
    <dif>
      Atingi tian konfuzon, kian donas konsumo de alkoholo:
      <ekz>
        <klr>[Noa]</klr> trinkis el la vino kaj <tld/>i&gcirc;is,
        kaj nudi&gcirc;is en sia tendo
        <fnt><bib>MT</bib>, <lok>&Gen; 9:21</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drink.0i">drinki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;'&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">betrunken werden</trd>
  <trd lng="en">get drunk</trd>
  <trd lng="es">emborrachar se<klr tip="amb"> (uno mismo)</klr></trd>
  <trd lng="fa">&a_m;&a_s;&a_t; &a_sh;&a_d;&a_n;</trd>
  <trd lng="fr">s'<ind>enivrer</ind></trd>
  <trdgrp lng="hu">
    <trd>ler&eacute;szegedik</trd>,
    <trd>megittasul</trd>
  </trdgrp>
  <trd lng="nl">dronken worden</trd>
  <trdgrp lng="pl">
    <trd>upija&cacute; si&eogonek;</trd>,
    <trd>upaja&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="ebri.0ulo.KOMUNE">
    <dif>
      Homo en <tld/>a stato:
      <ekz>
        por <tld/>ulo ne ekzistas dan&gcirc;ero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;'&c_ja;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_p;'&c_ja;&c_n;&c_mol;</trd>,
    <trd>&c_p;'&c_ja;&c_n;&c_ch;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_o;&c_j;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_ib;&c_v;&c_a;&c_h;&c_a;</trd>,
    <trd>&c_zh;&c_l;&c_u;&c_k;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">opilec</trd>
  <trd lng="de">Betrunkener</trd>
  <trd lng="en">drunk person</trd>
  <trd lng="fr">ivrogne<klr tip="ind"> (personne saoule)</klr></trd>
  <trd lng="hu">r&eacute;szeg<klr tip="ind"> (ember)</klr></trd>
  <trd lng="id">[<ind>mabuk</ind>] pemabuk</trd>
  <trd lng="nl">dronkelap</trd>
  <trdgrp lng="pl">
    <trd><klr>kto&sacute; </klr>nietrze&zacute;wy</trd>,
    <trd><klr>kto&sacute; </klr>pijany</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_mol;&c_ja;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_mol;&c_ja;&c_n;&c_y;&c_j;<klr tip="amb">
    (&c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">opilec</trd>
</drv>

<drv mrk="ebri.mal0igxi">
  <kap>mal<tld/>i&gcirc;i, <var><kap>el<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ebri.mal0igxi.KOMUNE">
    <dif>
      Ne plu <tld/>i, senkonfuzi&gcirc;i,
      <ref tip="vid" cel="sobr.0igxi.KOMUNE">sobri&gcirc;i</ref>:
      <ekz>
        jam du sitelojn da akvo oni elver&scirc;is sur lin, sed
        &gcirc;is nun li ne mal<tld/>i&gcirc;is
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 4a</lok></fnt>;
      </ekz>
      <ekz>
        estas nur nun, ke mi kvaza&ubreve; el<tld/>i&gcirc;as
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">n&uuml;chtern werden</trd>
  <trd lng="en">sober up</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;griser</ind></trd>,
    <trd>dessaouler</trd>,
    <trd>dessouler</trd>
  </trdgrp>
  <trd lng="hu">kij&oacute;zanodik</trd>
  <trdgrp lng="id">
    <trd>sadar</trd>,
    <trd>siuman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trze&zacute;wie&cacute;</trd>,
    <trd>wytrze&zacute;wie&cacute;</trd>,
    <trd>otrze&zacute;wie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_v;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_r;&c_je;&c_z;&c_v;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="ebri.post0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kapdoloro, okazanta post <tld/>o:
      <ekz>
        &ccirc;e tagi&gcirc;o ni estis jam satdormintaj kaj
        iom post<tld/>aj
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro
        22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kocovina</trd>
  <trd lng="de">Kater (Kopfschmerzen)</trd>
  <trd lng="en">hangover</trd>
  <trd lng="fr">gueule de bois</trd>
  <trd lng="hu">m&aacute;snaposs&aacute;g</trd>
  <trd lng="pl">kac</trd>
  <trd lng="ru">&c_p;&c_o;&c_h;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
