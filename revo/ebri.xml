<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ebri.xml,v 1.49 2023/11/07 19:21:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ebri</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ebri.0a">
  <kap><tld/>a</kap>
  <snc mrk="ebri.0a.droge">
    <dif>
      Konfuzita de alkoholo:
      <ekz>
        duonmorte <tld/>a;
      </ekz>
      <ekz>
        kiel longe vi restos <tld/>a? forigu vian vinon el vi
        <fnt><bib>MT</bib>, <lok>&Sa1; 1:14</lok></fnt>;
      </ekz>
      <ekz>
        vi trinkos sangon &gcirc;is<tld/>e de Mia bu&ccirc;ofero
        <fnt><bib>MT</bib>, <lok>&Jeh; 39:19</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sobr.mal0a">malsobra</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;'&c_ja;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_ib;&c_w;&c_sh;&c_y;&c_s;&c_ja;</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_s;&a_t;</trd>
    <trdgrp lng="pl">
      <trd>pijany</trd>,
      <trd>nietrze&zacute;wy</trd>,
      <trd>upojony<klr> alkoholem</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>b&ecirc;bedo</trd>,
      <trd>b&ecirc;bado</trd>,
      <trd>&eacute;brio</trd>,
      <trd>embriagado</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>berusad</trd>,
      <trd>full</trd>
    </trdgrp>
  </snc>
  <snc mrk="ebri.0a.spontanee">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konfuzita de sento, pasio, emocio:
      <ekz>
        <tld/>aj de l' &gcirc;ojo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a pro la venko;
      </ekz>
      <ekz>
        <ind>dorm<tld/>a</ind> pro subita veki&gcirc;o
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        24a</lok></fnt>.
        <trd lng="hu">&aacute;lomittas</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_je;&c_w;&c_sh;&c_y;</trd>
    <trd lng="fa">&a_s;&a_r;&a_m;&a_s;&a_t;</trd>
    <trdgrp lng="pl">
      <trd>pijany</trd>,
      <trd>upojony</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_ra;&t_tsheg;&t_ba;&t_za;&t_gigu;&t_tsheg;</trd>
  <trd lng="cs">opil&yacute;</trd>
  <trdgrp lng="de">
    <trd>betrunken</trd>,
    <trd>besoffen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>drunk</trd>,
    <trd>inebriated</trd>,
    <trd>intoxicated</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ebrio</trd>,
    <trd>borracho</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ivre</trd>,
    <trd>saoul</trd>,
    <trd>so&ucirc;l</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&eacute;szeg</trd>,
    <trd>ittas</trd>
  </trdgrp>
  <trd lng="id">mabuk</trd>
  <trdgrp lng="ja">
    <trd>&#x9154;&#x3063;&#x305F;  <pr>&#x3088;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x9676;&#x9154;&#x3057;&#x305F;  <pr>&#x3068;&#x3046;&#x3059;&#x3044;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">dronken</trd>
  <trdgrp lng="pt">
    <trd>b&ecirc;bedo</trd>,
    <trd>b&ecirc;bado</trd>,
    <trd>&eacute;brio</trd>,
    <trd>embriagado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_mol;&c_ja;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">opit&yacute;</trd>
  <trd lng="sv">berusad</trd>
  <trd lng="tok">nasa</trd>
  <trd lng="uk">&c_p;&#x2019;&c_ja;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x9163; <pr>h&#x101;n</pr></trd>,
    <trd>&#x9152;&#x9189; <pr>ji&#x1D4;zu&#xEC;</pr></trd>,
    <trd>&#x9189;&#x4E86; <pr>zu&#xEC;le</pr></trd>,
    <trd>&#x6709;&#x6C41; <pr>y&#x1D2;uzh&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ebri.0i.PSI">
    <dif>
      Senti konfuzon pro alkoholo, similefika drogo a&ubreve; sento:
      <ekz>
        la maato <tld/>is kaj ne sobri&gcirc;is anta&ubreve; Bulonjo
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&#x2019;&c_ja;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_n;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_ju;)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_s;&a_t; &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>saoul</ind></trd>,
    <trd>&ecirc;tre <ind>so&ucirc;l</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;&shin;&tav;&kaf;&resh;</trd>,
    <trd>&lamed;&shin;&tav;&vav;&tav; &lamed;&shin;&vav;&kaf;&resh;&he;</trd>
  </trdgrp>
  <trd lng="hu">r&eacute;szeg</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>pijanym</ind></trd>,
    <trd>by&cacute; <ind>nietrze&zacute;wym</ind></trd>,
    <trd>by&cacute; <ind>upojonym</ind><klr> (zw&lstroke;aszcza
    alkoholem)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol;
  <ind>&c_p;&c_mol;&c_ja;&c_n;&c_y;&c_m;</ind></trd>
</drv>

<drv mrk="ebri.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="ebri.0o.PSI_alkohole">
    <dif>
      Animstato de <tld/>a homo:
      <ekz>
        vera opinio montri&gcirc;as en <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de amo, de entuziasmo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ebri.0o.PSI_gxenerale">
    <dif>
      &Ccirc;ia animstato simila al alkoholinfluo,
      malfortiganta la memregon kaj
      <ref tip="vid" cel="ekzalt.0o.PSI">ekzaltanta</ref>
      la sentojn:
      <ekz>
        la &ccirc;evaloj &scirc;ajnis kaptitaj de rapid-<tld/>eco
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li 
        venis trakosme?</vrk>, <lok>1980</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="delir.0o">deliro</ref>,
    <ref cel="frenez">frenezo</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;
  <klr>(&c_s;&c_t;&c_a;&c_n;)</klr></trd>
  <trd lng="de">Trunkenheit</trd>
  <trdgrp lng="en">
    <trd>Trunkenheit</trd>,
    <trd>Betrunkenheit</trd>,
    <trd>Berauschtheit</trd>,
    <trd>Betrunkensein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>drunkenness</trd>,
    <trd>intoxication</trd>
  </trdgrp>
  <trd lng="es">embriaguez</trd>
  <trd lng="fa">&a_m;&a_s;&a_t;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;bri&eacute;t&eacute;</trd>,
    <trd>ivresse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&eacute;szegs&eacute;g</trd>,
    <trd>ittass&aacute;g</trd>
  </trdgrp>
  <trd lng="id">ke<ind>mabuk</ind>an</trd>
  <trdgrp lng="ja">
    <trd>&#x9154;&#x3044;  <pr>&#x3088;&#x3044;</pr></trd>,
    <trd>&#x9676;&#x9154;  <pr>&#x3068;&#x3046;&#x3059;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">dronkenschap</trd>
  <trdgrp lng="pl">
    <trd>bycie <ind>pijanym</ind></trd>,
    <trd>bycie <ind>nietrze&zacute;wym</ind></trd>,
    <trd>nietrze&zacute;wo&sacute;&cacute;</trd>,
    <trd>bycie <ind>upojonym</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>opilstvo</trd>,
    <trd>opojenie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>opilost</trd>,
    <trd>opilstv&iacute;</trd>,
    <trd>opojen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_p;&#x2019;&c_ja;&c_n;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_p;&#x2019;&c_ja;&c_n;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9676;&#x9189; <pr>t&#xE1;ozu&#xEC;</pr></trd>,
    <trd>&#x9189;&#x6001; <pr>zu&#xEC;t&#xE0;i</pr></trd>,
    <trd>&#x9189;&#x5012; <pr>zu&#xEC;d&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="ebri.0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta mensan konfuzon de <tld/>o a&ubreve; simila al
      <tld/>o:
      <ekz>
        la vino estas blasfemanto, <tld/>iga trinka&jcirc;o estas
        sova&gcirc;a
        <fnt><bib>MT</bib>, <lok>&Sen; 20:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_ja;&c_ju;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>omamuj&iacute;c&iacute;</trd>,
    <trd>opojn&yacute;</trd>
  </trdgrp>
  <trd lng="en">intoxicating</trd>
  <trd lng="fa">&a_m;&a_s;&a_t;&f_y;&zwnj;&a_A_madda;&a_w;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>alcoolis&eacute;</trd>,
    <trd>enivrant</trd>
  </trdgrp>
  <trd lng="he">&mem;&#x5B0;&shin;&#x5C1;&#x5B7;&kaf;&#x5BC;&#x5B5;&resh;</trd>
  <trd lng="hu">r&eacute;szeg&iacute;t&odblac;</trd>
  <trd lng="id">me<ind>mabuk</ind>kan</trd>
  <trd lng="ja">&#x9154;&#x308F;&#x305B;&#x308B;  <pr>&#x3088;&#x308F;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">benevelend</trd>
  <trd lng="pl">upajaj&aogonek;cy</trd>
  <trd lng="ru">&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>omamuj&uacute;ci</trd>,
    <trd>opojn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&#x2019;&c_ja;&c_n;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&#x2019;&c_ja;&c_n;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <snc mrk="ebri.0igajxo.KOMUNE">
    <dif>
      <tld lit="E"/>iga substanco:
      <ekz>
        <tld/>iga&jcirc;on ne trinku, nek vi, nek viaj filoj kun vi
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 10:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne trinkos vinon nek <tld/>iga&jcirc;on kaj li estos plena de la Sankta Spirito
        <fnt>
          <bib>NT</bib>
          <lok>Luko 1:15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="alkoho.0ajxo">alkohola&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_n;&c_a;&c_p;&c_o;&c_j;, &c_p;&c_ib;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="de">
    <trd>Alkohol<klr>(isches Getr&auml;nk)</klr></trd>,
    <trd>Ges&ouml;ff</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_w;&a_sh;&a_A;&a_b;&a_h;&a_hamza_sure; &a_A;&a_l;&f_k;&a_l;&f_y;</trd>,
    <trd>&a_m;&a_sh;&a_r;&a_w;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">boisson<klr tip="ind"> (alcoolis&eacute;e)</klr></trd>
  <trd lng="he">&mem;&#x5B0;&shin;&#x5C1;&#x5B7;&kaf;&#x5BC;&#x5B5;&resh;</trd>
  <trd lng="hu">r&eacute;szeg&iacute;t&odblac; ital</trd>
  <trdgrp lng="id">
    <trd><ind>minum</ind>an beralkohol</trd>,
    <trd><ind>minum</ind>an keras</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9152;  <pr>&#x3055;&#x3051;</pr></trd>,
    <trd>&#x30A2;&#x30EB;&#x30B3;&#x30FC;&#x30EB;  <pr>&#x3042;&#x308B;&#x3053;&#x30FC;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">alcoholische drank</trd>
  <trdgrp lng="pl">
    <trd>nap&oacute;j alkoholowy</trd>,
    <trd>nap&oacute;j wyskokowy</trd>,
    <trd>woda ognista</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_n;&c_a;&c_p;&c_i;&c_t;&c_o;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x9152;&#x7CBE;&#x98F2;&#x6599; <pr>ji&#x1D4;j&#x12B;ngy&#x1D0;nli&#xE0;o</pr></trd>
</drv>

<drv mrk="ebri.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ebri.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        ni <tld/>igu nian patron per vino, kaj ni ku&scirc;u kun li,
        por ke ni aperigu de nia patro idaron
        <fnt><bib>MT</bib>, <lok>&Gen; 19:32</lok></fnt>;
      </ekz>
      <ekz>
        lin <tld/>igis la sukceso<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;'&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>om&aacute;mit</trd>,
    <trd>opojit</trd>,
    <trd>op&iacute;jet</trd>,
    <trd>op&iacute;t</trd>
  </trdgrp>
  <trd lng="en">intoxicate</trd>
  <trd lng="es">emborrachar<klr tip="amb"> (a otro)</klr></trd>
  <trd lng="fa">&a_m;&a_s;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>enivrer</trd>,
    <trd>griser</trd>,
    <trd>saouler</trd>,
    <trd>souler</trd>
  </trdgrp>
  <trd lng="he">&lamed;&shin;&kaf;&resh;</trd>
  <trdgrp lng="hu">
    <trd>ler&eacute;szeg&iacute;t</trd>,
    <trd>r&eacute;szegg&eacute; tesz</trd>
  </trdgrp>
  <trd lng="id">me<ind>mabuk</ind>kan</trd>
  <trd lng="ja">&#x9154;&#x308F;&#x305B;&#x308B;  <pr>&#x3088;&#x308F;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>upija&cacute;</trd>,
    <trd>upaja&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_mol;&c_ja;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>om&aacute;mi&tcaron;</trd>,
    <trd>opi&tcaron;</trd>,
    <trd>opoji&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_o;&c_ji;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_p;&#x2019;&c_ja;&c_n;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="ebri.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ebri.0igxi.KOMUNE">
    <dif>
      Atingi tian konfuzon, kian donas konsumo de alkoholo:
      <ekz>
        <klr>[Noa]</klr> trinkis el la vino kaj <tld/>i&gcirc;is,
        kaj nudi&gcirc;is en sia tendo
        <fnt><bib>MT</bib>, <lok>&Gen; 9:21</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drink.0i">drinki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;'&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de"><ind>betrunken</ind> werden</trd>
  <trd lng="en">get drunk</trd>
  <trd lng="es">emborrachar se<klr tip="amb"> (uno mismo)</klr></trd>
  <trd lng="fa">&a_m;&a_s;&a_t; &a_sh;&a_d;&a_n;</trd>
  <trd lng="fr">s'<ind>enivrer</ind></trd>
  <trd lng="he">&lamed;&he;&shin;&tav;&kaf;&resh;</trd>
  <trdgrp lng="hu">
    <trd>ler&eacute;szegedik</trd>,
    <trd>megittasul</trd>
  </trdgrp>
  <trd lng="ja">&#x9154;&#x3046;  <pr>&#x3088;&#x3046;</pr></trd>
  <trd lng="nl">dronken worden</trd>
  <trdgrp lng="pl">
    <trd>upija&cacute; si&eogonek;</trd>,
    <trd>upaja&cacute; si&eogonek;</trd>
  </trdgrp>
