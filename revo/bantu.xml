<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: bantu.xml,v 1.7 2023/10/14 08:39:48 revo Exp $">
    <kap>
      <rad>bantu</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="bantu.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Iu el mult<ref tip="lst" lst="voko:popolanoj" cel="etn.0o">etn</ref>a grupo 
      de suda parto de <ref tip="malprt" cel="afrik.A0o.mondparto">Afriko</ref>,
      karakterizata de parenca lingvaro:
      <ekz>
        la <tld/>-lingvoj
        <fnt>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="www.esperanto-muenchen.de/prelegoj/prelego1_wf.pps">Lingvofamilioj,
          migradoj de popoloj  kaj genetikaj
              parencecoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> burunda lingvo <klr>[&#x2026;]</klr> estas <tld/>a
        lingvo parolata de
        &ccirc;irka&ubreve; 6 milionoj da homoj, &ccirc;efe en Burundo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28716">Burunda lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="zulu.0o">zuluo</ref>
    
  <trd lng="be">&c_b;&c_a;&c_n;&c_t;&c_u;</trd>   
  <trd lng="de">Bantu</trd>
  <trd lng="fr">Bantou</trd>
  <trd lng="pl">Bantu</trd>
  <trd lng="ro">Bantu</trd>
  <trd lng="uk">&c_b;&c_a;&#x43D;&c_t;&c_u;</trd>
  </snc>  
  <trd lng="ja">&#x30D0;&#x30F3;&#x30C8;&#x30A5;&#x30FC;&#x4EBA;  <pr>&#x30D0;&#x30F3;&#x30C8;&#x30A5;&#x30FC;&#x3058;&#x3093;</pr></trd>
</drv>

  </art>

