<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ran</rad>/o
</kap>
<drv mrk="ran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Iu el <tld/>edoj.
    </dif>
    <ref tip="vid" cel="buf.0o">bufo</ref>
  </snc>
  <snc num="2">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="ran.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Rana</trd>).
    </dif>
  </snc>
  <bld lok="../bld/ran2.jpg">
    akvo<tld/>o (<trd lng="la">Rana esculenta</trd>)
  </bld>
  <trd lng="de">Frosch</trd>
</drv>
<drv mrk="ran.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de amfibioj (<trd lng="la">Ranidae</trd>),
      kun senveruka ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="de">Fr&ouml;sche</trd>
</drv>
</art>
