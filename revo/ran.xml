<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ran</rad>/o
</kap>
<drv mrk="ran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el <tld/>edoj.
    </dif>
    <ref tip="vid" cel="buf.0o">bufo</ref>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="raned.0oj"><tld/>edoj</ref>
      (<trd lng="la">Rana</trd>).
    </dif>
  </snc>
  <bld lok="../bld/ran2.jpg">
    <ind>akvo<tld/>o</ind>
    <trd lng="la">Rana esculenta</trd>
  </bld>
  <trd lng="de">Frosch</trd>
  <trdgrp lng="nl">
    <trd>kikker</trd>,
    <trd>kikvors</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ja;&c_g;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sv">groda</trd>
</drv>
</art>
