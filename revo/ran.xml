<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ran.xml,v 1.41 2025/05/02 02:06:36 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ran</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="ran.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="ran.0o.komune">
     <uzo tip="stl">KOMUNE</uzo>
     <dif>
       Iu el <ref tip="malprt" cel="raned.0oj">ranedoj</ref>;
       <ref tip="lst" cel="amfibi.0o" lst="voko:amfibioj">amfibio</ref>,
       senvostulo kun horizontalaj pupiloj kaj dentoj:
      <ekz>
        kie ajn <tld/>o iras, &gcirc;i &ccirc;iam mar&ccirc;on sopiras
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksvarmos la rivero per <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 8:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o kvakis tiel timeme, kvaza&ubreve; &gcirc;i volus elpetegi de &scirc;i kompaton
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="vid" cel="buf.0o">bufo</ref>
   <trd lng="en">frog</trd>
   </snc>
   <snc mrk="ran.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="raned.0oj"><tld/>edoj</ref>
       (<trd lng="la">Rana</trd>):
      <ekz>
        la e&ubreve;ropa hilo estas la plej rara <tld/>o en Litovio
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010875.php">Pereon de specioj malhelpas homoj</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="en">Rana</trd>
     <trd lng="pl">&zdot;aba</trd>
   </snc>
   <bld lok="&WCU;/thumb/7/75/Rana_esculenta_on_Nymphaea_edit.JPG/640px-Rana_esculenta_on_Nymphaea_edit.JPG" prm="CC BY 3.0"><ind>akvo<tld/>o</ind> (Rana esculenta)
       <fnt>
         <aut>Helge Busch-Paulick (Grand-Duc @ Wikipedia), prilaborita de Niabot</aut>
         <lok><url ref="&WCW;/File:Rana_esculenta_on_Nymphaea_edit.JPG">&WCW;/File:Rana_esculenta_on_Nymphaea_edit.JPG</url></lok>
       </fnt>
     <trd lng="de">Wasserfrosch</trd>
     <trd lng="hu">kecskeb&eacute;ka</trd>
     <trd lng="la">Rana esculenta</trd>
     <trd lng="es">Rana de agua</trd>
   </bld>
   <trd lng="be">&c_zh;&c_a;&c_b;&c_a;</trd>
   <trd lng="bg">&c_zh;&c_a;&c_b;&c_a;</trd>
   <trd lng="bo">&t_sa;&t_bata;&t_la;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="de">Frosch</trd>
   <trd lng="en">frog</trd>
   <trd lng="es">rana</trd>
   <trd lng="fr">grenouille</trd>
   <trd lng="he">&tsadi;&pe;&resh;&dalet;&ayin;</trd>
   <trd lng="hu">b&eacute;ka</trd>
   <trd lng="id">katak</trd>
   <trdgrp lng="ja">
    <trd>&#12488;&#12494;&#12469;&#12510;&#12460;&#12456;&#12523;</trd>,
    <trd>&#12459;&#12456;&#12523;  <pr>&#12363;&#12360;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>kikker</trd>,
     <trd>kikvors</trd>
   </trdgrp>
   <trd lng="pt">r&atilde;</trd>
   <trd lng="ru">&c_l;&c_ja;&c_g;&c_u;&c_sh;&c_k;&c_a;</trd>
   <trd lng="sv">groda</trd>
  <trd lng="sk">&zcaron;aba</trd>
  <trdgrp lng="cs">
    <trd>skokan</trd>,
    <trd>&zcaron;&aacute;ba</trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_a;&c_b;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#30000;&#38622; <pr>ti&aacute;nj&imacron;</pr></trd>,
    <trd>&#30000;&#40481; <pr>ti&aacute;nj&imacron;</pr></trd>,
    <trd>&#34532; <pr>h&aacute;</pr></trd>,
    <trd>&#38738;&#34521; <pr>q&imacron;ngw&amacron;</pr></trd>,
    <trd>&#34452; <pr>q&iacute;</pr></trd>,
    <trd>&#34521; <pr>w&amacron;</pr></trd>,
    <trd>&#34532;&#34758; <pr>h&aacute;ma</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ran.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a ha&ubreve;to
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i konforme al sia <tld/>a naturo subakvi&gcirc;adis kaj poste levi&gcirc;adis returne supren
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_b;&c_ib;&c_n;&c_y;</trd>
  <trd lng="en">frog</trd>
  <trd lng="uk">&c_zh;&c_a;&c_b;&#8217;&c_ja;&c_ch;&c_i;&c_j;</trd>
</drv>

</art>
