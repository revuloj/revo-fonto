<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>velk</rad>/i
</kap>
<drv mrk="velk.0i">
 <kap><ofc>*</ofc><tld/>i</kap>
 <gra><vspec>ntr</vspec></gra>
 <snc mrk="velk.0i.malfresxigxi">
   <dif>
     Perdi la vivfre&scirc;econ; sensuki&gcirc;i:
     <ekz>
       <tld/>inta rozo, pomo, viza&gcirc;o, lipo<fnt>Z</fnt>,
     </ekz>
     <ekz>
       koloro;
     </ekz>
     <ekz>
       la varmega suno <tld/>igas florojn;
     </ekz>
     <ekz>
       flamo <tld/>sekigos liajn bran&ccirc;ojn<fnt>Z</fnt>.
     </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="putr.0i">putri</ref>,
     <ref cel="sxrump.0i">&scirc;rumpi</ref>.
   </refgrp>
   <trd lng="nl">verwelken</trd>
 </snc>
 <snc mrk="velk.0i.FIG">
   <uzo tip="stl">FIG</uzo>
   <dif>
     Senforti&gcirc;i, kaduki&gcirc;i, konsumi&gcirc;i:
     <ekz>
       li <tld/>as en karcero;
     </ekz>
     <ekz>
       <tld/>as la flamoj, steloj, gloro;
     </ekz>
     <ekz>
       senforti&gcirc;as kaj <tld/>as la lando<fnt>Z</fnt>;
     </ekz>
     <ekz>
       kun subi&gcirc;o a&ubreve; malapero de &gcirc;ia lingvo <tld/>as 
la propraj kutimoj,
       moroj de nacio<fnt>Z</fnt>;
     </ekz>
     <ekz>
       miaj oreloj <tld/>as de viaj vortoj<fnt>B</fnt>;
     </ekz>
     <ekz>
       &ccirc;iuj iam ekzistantaj ebloj jam ek<tld/>is
       <klr>(fari&gcirc;is malpli efektivigeblaj)</klr>;
     </ekz>
     <ekz>
       ek<tld/>inta fervoro.
     </ekz>
   </dif>
   <trd lng="nl">kwijnen</trd>
 </snc>
  <trdgrp lng="be">
   <trd>&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_c;&c_mol;</trd>,
   <trd>&c_p;&c_a;&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_a;&c_b;&c_v;&c_ja;&c_c;&c_mol;</trd>,
   <trd>&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_p;&c_a;&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_m;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>,
   <trd>&c_z;&c_m;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>,
   <trd>&c_s;&c_mol;&c_m;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol; <klr 
tip="amb">(pro varmego)</klr></trd>
 </trdgrp>
  <trd lng="ca">pansir-se</trd>
  <trdgrp lng="cs">
    <trd>blednout</trd>,
    <trd>chřadnout</trd>,
    <trd>vadnout</trd>,
    <trd>vyblednout</trd>,
    <trd>zeslábnout</trd>
  </trdgrp>
  <trd lng="de">welken</trd>
  <trd lng="en">wither</trd>
  <trd lng="es">marchitarse</trd>
  <trdgrp lng="fr">
   <trd>se <ind>faner</ind></trd>
 </trdgrp>
  <trd lng="hu">hervad</trd>
  <trdgrp lng="pl">
   <trd>przekwita&cacute;</trd>,
   <trd>wi&eogonek;dn&aogonek;&cacute;</trd>,
 </trdgrp>
  <trdgrp lng="pt">
   <trd>murchar</trd>,
   <trd>definhar</trd>
 </trdgrp>
  <trd lng="ro">a se ofili</trd>
  <trdgrp lng="ru">
   <trd>&c_v;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
   <trd>&c_u;&c_v;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
   <trd>&c_z;&c_a;&c_v;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
   <trd>&c_u;&c_v;&c_ja;&c_d;&c_a;&c_t;&c_mol;</trd>,
   <trd>&c_u;&c_v;&c_ja;&c_s;&c_t;&c_mol;</trd>
 </trdgrp>
  <trd lng="sk">vädnúť</trd>
</drv>

<drv mrk="velk.0a">
 <kap><tld/>a</kap>
 <snc>
   <dif>
     Senfre&scirc;a, senvigla:
     <ekz>
       ne pensu, ke mi kreita estas el tia <tld/>a &scirc;tofo, ke mi
       lasos...<fnt>Z</fnt>;
     </ekz>
     <ekz>
       <tld/>a animo;
     </ekz>
     <ekz>
       &scirc;iaj manoj pendis <tld/>e<fnt>Z</fnt>
       <klr>(senforte, peze)</klr>.
     </ekz>
   </dif>
 </snc>
  <trdgrp lng="be">
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_l;&c_y;</trd>,
   <trd>&c_b;&c_l;&c_ja;&c_k;&c_l;&c_y;</trd>,
   <trd>&c_p;&c_a;&c_b;&c_l;&c_ja;&c_k;&c_l;&c_y;</trd>
 </trdgrp>
  <trd lng="ca">pansida</trd>
  <trdgrp lng="cs">
    <trd>schlíplý</trd>,
    <trd>zdeptaný</trd>
  </trdgrp>
  <trd lng="de">welk</trd>
  <trd lng="es">marchito</trd>
  <trdgrp lng="fr">
   <trd>d&eacute;ss&eacute;ch&eacute;e</trd>,
   <trd>fan&eacute;e</trd>,
   <trd>morte<klr> (feuille, plante)</klr></trd>
 </trdgrp>
  <trdgrp lng="hu">
   <trd>hervatag</trd>,
   <trd>b&aacute;gyadt</trd>
 </trdgrp>
  <trd lng="nl">verlept</trd>
  <trdgrp lng="pl">
   <trd>uschni&eogonek;ty</trd>,
   <trd>zwi&eogonek;dni&eogonek;ty</trd>,
 </trdgrp>
  <trdgrp lng="pt">
   <trd>murcho</trd>,
   <trd>definhado</trd>
 </trdgrp>
  <trdgrp lng="ru">
   <trd>&c_u;&c_v;&c_ja;&c_d;&c_sh;&c_i;&c_j;</trd>,
   <trd>&c_p;&c_o;&c_b;&c_l;&c_jo;&c_k;&c_sh;&c_i;&c_j;</trd>,
   <trd>&c_p;&c_o;&c_n;&c_i;&c_k;&c_sh;&c_i;&c_j;</trd>
 </trdgrp>
  <trdgrp lng="sk">
    <trd>ochabnutý</trd>,
    <trd>zoslabnutý</trd>,
    <trd>zvädnutý</trd>
  </trdgrp>
</drv>

<drv mrk="velk.for0i">
 <kap>for<tld/>i</kap>
 <gra><vspec>ntr</vspec></gra>
 <snc>
   <dif>
     Nenii&gcirc;i <tld/>ante:
     <ekz>
       kun vero severa komerca&jcirc;o for<tld/>os
       <fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
       <uzo tip="stl">FIG</uzo>
       pli bone estas tio, ol for<tld/>i kiel edzin' honesta en la budo
       de &scirc;uisto malpura.
     </ekz>
   </dif>
 </snc>
  <trdgrp lng="be">
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_c;&c_mol;</trd>,
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_p;&c_a;&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
 </trdgrp>
  <trdgrp lng="cs">
    <trd>uvadnout</trd>,
    <trd>zvadnout</trd>
  </trdgrp>
  <trd lng="de">verwelken</trd>
  <trdgrp lng="es">
   <trd>consumirse</trd>,
   <trd>agostarse</trd>
 </trdgrp>
  <trdgrp lng="fr">
   <trd>se <ind>fl&eacute;trir</ind></trd>
 </trdgrp>
  <trd lng="hu">elhervad</trd>
  <trd lng="nl">wegkwijnen</trd>
  <trd lng="pl">zwi&eogonek;dn&aogonek;&cacute;</trd>
  <trd lng="ru">&c_u;&c_v;&c_ja;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">zvädnúť</trd>
</drv>
</art>
