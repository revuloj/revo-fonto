<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: velk.xml,v 1.30 2023/09/05 09:43:54 revo Exp $">
<kap>
 <ofc>*</ofc>
 <rad>velk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="velk.0i">
 <kap><ofc>*</ofc><tld/>i</kap>
 <gra><vspec>ntr</vspec></gra>
 <snc mrk="velk.0i.malfresxigxi">
   <dif>
     Perdi la vivfre&scirc;econ; sensuki&gcirc;i:
      <ekz>
        mi volis doni al vi kelke da violoj, sed ili &ccirc;iuj <tld/>is
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la foliarboj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo preska&ubreve; sesdekjara, kun <tld/>inta viza&gcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel floro li elkreskas kaj <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 14:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        flavi&gcirc;intaj, <tld/>intaj, kavi&gcirc;intaj vangoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ekvidis <klr>[&#x2026;]</klr> &scirc;iajn palajn, <tld/>intajn lipojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
       flamo <tld/>sekigos liajn bran&ccirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 15:30</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="putr.0i">putri</ref>,
      <ref cel="sxrump.0i">&scirc;rumpi</ref>
    </refgrp>
    <trd lng="nl">verwelken</trd>
  </snc>
  <snc mrk="velk.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senforti&gcirc;i, <ref cel="kaduk.0igxi">kaduki&gcirc;i</ref>, <ref cel="konsum.0igxi">konsumi&gcirc;i</ref>:
      <ekz>
        nun nia animo <tld/>as, estas nenio krom &ccirc;i tiu manao anta&ubreve; niaj okuloj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 11:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Anna</nom> <klr>[&#x2026;]</klr> &scirc;ajnas, iom paci&gcirc;is kun la penso <tld/>i en la provinca
        angulo
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj sentoj ankora&ubreve; ne <tld/>is, &ccirc;ar vi eble estas tre bona homo
        <fnt>
          <bib>Tor</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eksentis, ke lia koro <tld/>as
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam &ccirc;io alia komencis malforti&gcirc;i kaj <tld/>i, tiam la industrio de Montan
        ekfloris kiel neniam anta&ubreve;e
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Movado dum mallonga tempo floris, sed same rapide <tld/>is
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam li multe mankis al mi en la komenco, lia memoro balda&ubreve; <tld/>is, kvaza&ubreve;
        li neniam ekzistis
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as intereso en Japanio pri scienco
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009689.php">Premioj kaj premoj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        senforti&gcirc;as kaj <tld/>as la mondo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 24:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esting.0igxi">estingi&gcirc;i</ref>,
      <ref cel="pal.0igxi.FIG">pali&gcirc;i<sncref/></ref>
    </refgrp>
    <trd lng="nl">kwijnen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_m;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol; <klr tip="amb">(pro varmego)</klr></trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_shabchu;&t_la;&t_tsheg;</trd>
  <trd lng="ca">pansir-se</trd>
  <trdgrp lng="cs">
    <trd>blednout</trd>,
    <trd>ch&rcaron;adnout</trd>,
    <trd>vadnout</trd>,
    <trd>vyblednout</trd>,
    <trd>zesl&aacute;bnout</trd>
  </trdgrp>
  <trd lng="de">welken</trd>
  <trd lng="en">wither</trd>
  <trd lng="es">marchitarse</trd>
  <trdgrp lng="fr">
    <trd>se <ind>faner</ind></trd>
  </trdgrp>
  <trd lng="hu">hervad</trd>
  <trdgrp lng="ja">
    <trd>&#x67AF;&#x308C;&#x308B;  [<ind>&#x304B;&#x308C;&#x308B;</ind>]</trd>,
    <trd>&#x3057;&#x304A;&#x308C;&#x308B;</trd>,
    <trd>&#x3057;&#x306A;&#x3073;&#x308B;</trd>,
    <trd>&#x8870;&#x3048;&#x308B;  [<ind>&#x304A;&#x3068;&#x308D;&#x3048;&#x308B;</ind>]</trd>,
    <trd>&#x3042;&#x305B;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przekwita&cacute;</trd>,
    <trd>wi&eogonek;dn&aogonek;&cacute;</trd>,
  </trdgrp>
  <trdgrp lng="pt">
    <trd>murchar</trd>,
    <trd>definhar</trd>
  </trdgrp>
  <trd lng="ro">a se ofili</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_ja;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_ja;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">v&auml;dn&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#x2019;&c_ja;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_sh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="velk.0a">
 <kap><tld/>a</kap>
 <snc mrk="velk.0a.KOMUNE">
   <dif>
      Senfre&scirc;a, senvigla:
      <ekz>
        regulaj, kvankam jam iom <tld/>aj trajtoj de viza&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>        
        sur sia lito de morto li vane etendos siajn <tld/>ajn manojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ne pensu vi, ke mi kreita estas &parsl; El tia <tld/>a &scirc;tofo, ke mi lasos
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj manoj <klr>[&#x2026;]</klr> kvankam ne perdinte sian forton, pendis tamen <tld/>e
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kolorri&ccirc;aj florbedoj <klr>[&#x2026;]</klr> estis fari&gcirc;intaj grizaj kaj <tld/>aj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Flugo al la Ebura Turo</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu virino estis &scirc;ire preska&ubreve; senvestigita de la alia, kaj unu <tld/>a mamo
        elpendis
        <fnt>
          <bib>KrM</bib>
          <lok>31. Ricevis Bandito La&ubreve; Sia Merito.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="febl.0a">febla</ref>,
      <ref cel="fort.sen0a">senforta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_l;&c_y;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_k;&c_l;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_b;&c_l;&c_ja;&c_k;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">pansida</trd>
  <trdgrp lng="cs">
    <trd>schl&iacute;pl&yacute;</trd>,
    <trd>zdeptan&yacute;</trd>
  </trdgrp>
  <trd lng="de">welk</trd>
  <trdgrp lng="en">
    <trd>wilted</trd>,
    <trd>withered</trd>, <trd>limp</trd>
  </trdgrp>
  <trd lng="es">marchito</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;ss&eacute;ch&eacute;e</trd>,
    <trd>fan&eacute;e</trd>,
    <trd>morte<klr> (feuille, plante)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hervatag</trd>,
    <trd>b&aacute;gyadt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x67AF;&#x308C;&#x305F;  [<ind>&#x304B;&#x308C;&#x305F;</ind>]</trd>,
    <trd>&#x3057;&#x304A;&#x308C;&#x305F;</trd>
  </trdgrp>
  <trd lng="nl">verlept</trd>
  <trdgrp lng="pl">
    <trd>uschni&eogonek;ty</trd>,
    <trd>zwi&eogonek;dni&eogonek;ty</trd>,
  </trdgrp>
  <trdgrp lng="pt">
    <trd>murcho</trd>,
    <trd>definhado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_ja;&c_d;&c_sh;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_l;&c_jo;&c_k;&c_sh;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_i;&c_k;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochabnut&yacute;</trd>,
    <trd>zoslabnut&yacute;</trd>,
    <trd>zv&auml;dnut&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ib;&c_v;&#x2019;&c_ja;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_l;&c_ja;&c_k;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_b;&c_l;&c_ja;&c_k;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_i;&c_k;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="velk.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="velk.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on de...:
      <ekz>
        la varmega suno <tld/>igas florojn;
      </ekz>
      <ekz>
        Mi, la Eternulo, <klr>[&#x2026;]</klr> <tld/>igas arbon sukoplenan kaj verdigas arbon
        <tld/>intan
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 17:24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><ind>welken</ind> lassen</trd>
  <trd lng="fr">faire <ind>faner</ind></trd>
</drv>

<drv mrk="velk.for0i">
 <kap>for<tld/>i</kap>
 <gra><vspec>ntr</vspec></gra>
 <snc mrk="velk.for0i.KOMUNE">
   <dif>
     Nenii&gcirc;i <tld/>ante:
      <ekz>
        vermo <klr>[&#x2026;]</klr> difektis la hederon, kaj &gcirc;i for<tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Jona 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la arbeto for<tld/>is, mi prenis &gcirc;ian lastan fre&scirc;an bran&ccirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post nelonga tempo, la bovino komencis for<tld/>i
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabineto devas havi patrinlakton, alie &scirc;i for<tld/>as
        <fnt>
          <bib>VRA</bib>
          <lok>Helmer Grundstr&ouml;m</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun vero severa komerca&jcirc;o for<tld/>os
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esting.0igxi">estingi&gcirc;i</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="pas.0i.forpasi">pasi<sncref/></ref>,
      <ref cel="pere.0i">perei</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_c;&c_mol;</trd>,
   <trd>&c_z;&c_mol;&c_v;&c_ja;&c_n;&c_u;&c_c;&c_mol;</trd>,
   <trd>&c_p;&c_a;&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>uvadnout</trd>,
    <trd>zvadnout</trd>
  </trdgrp>
  <trd lng="de">verwelken</trd>
  <trd lng="en">wither away</trd>
  <trdgrp lng="es">
    <trd>consumirse</trd>,
    <trd>agostarse</trd>
  </trdgrp>
  <trd lng="fr">se <ind>fl&eacute;trir</ind></trd>
  <trd lng="hu">elhervad</trd>
  <trdgrp lng="ja">
    <trd>&#x67AF;&#x308C;&#x3066;&#x3057;&#x307E;&#x3046;  [<ind>&#x304B;&#x308C;&#x3066;&#x3057;&#x307E;&#x3046;</ind>]</trd>,
    <trd>&#x8870;&#x3048;&#x306F;&#x3066;&#x308B;  [<ind>&#x304A;&#x3068;&#x308D;&#x3048;&#x306F;&#x3066;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">wegkwijnen</trd>
  <trd lng="pl">zwi&eogonek;dn&aogonek;&cacute;</trd>
  <trd lng="ru">&c_u;&c_v;&c_ja;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">zv&auml;dn&uacute;&tcaron;</trd>
  <trd lng="uk">&c_z;&c_o;&c_v;&c_s;&c_ib;&c_m; &c_z;&c_ib;&c_v;&#x2019;&c_ja;&c_n;&c_u;&c_t;&c_i;</trd>
</drv>
