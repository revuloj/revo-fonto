<drv mrk="heteroseksual.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="heteroseksual.0a.MOR">
    <dif>
      <ref tip="dif" cel="seks.ali0ema">Aliseksema</ref>:
      <ekz>
        etnalingvaj tradukoj, e&ccirc; modernaj, malofte senambigue
        prezentas kelkajn el la temoj pritraktataj de Platono
        en &ccirc;i tiu libro, precipe la seksajn, homoseksualajn
        kiel anka&ubreve; <tld/>ajn
        <fnt>
          <aut>D. Broadribb</aut>,
          <vrk>Anta&ubreve;parolo de la Tradukinto</vrk>,
          <lok>en: La Respubliko, 1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_t;&c_e;&c_r;&c_a;&c_s;&c_e;&c_k;&c_s;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="fr">h&eacute;t&eacute;rosexuel<klr> (adj.)</klr></trd>
</drv>
