<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fregat</rad>/o
</kap>
<drv mrk="fregat.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Trimasta milita <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>
      kun kvadrataj veloj.
    </dif>
    <trd lng="de">Fregatte</trd>
    <trd lng="es">fragata</trd>
    <trd lng="fr">fr&eacute;gate<klr> (navire)</klr></trd>
    <trd lng="hu">fregatt</trd>
    <trd lng="nl">fregat</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara palmopieda birdo, kies flugo estas tre rapida
      (<trd lng="la">tachypedes</trd>).
    </dif>
    <trd lng="de">Pracht-Fregattvogel</trd>
    <trd lng="fr">fr&eacute;gate<klr> (oiseau)</klr></trd>
    <trdgrp lng="hu">
      <trd>fregattmad&aacute;r</trd>,
      <trd>haj&oacute;smad&aacute;r</trd></trdgrp>
  </snc>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;</trd>
</drv>
</art>
