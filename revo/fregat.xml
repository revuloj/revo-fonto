<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fregat.xml,v 1.29 2024/04/18 06:29:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fregat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fregat.0o">
  <kap><tld/>o</kap>
  <snc mrk="fregat.0o.MIL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Trimasta milita <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>
      kun kvadrataj veloj, mezgranda inter
      <ref cel="korvet.0o">korveto</ref> kaj
      <ref cel="vesel.0o">veselo</ref>:
      <ekz>
        sur la <tld/>o Danujo, sur kiu mi servis en la &scirc;ipta&ccirc;mento <klr>[&#8230;]</klr>, unu
        viro batalis flanke de mi
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la usona skadro, kiu venis por negoci pri komerca traktato kun la
        ermita lando kaj &jcirc;etis ankron fronte de la golfo de Tokio
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006909.php">Novjara kla&ccirc;o super konjako</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 25</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="fregat1.0o.ZOO">Fregato</ref>
    anka&ubreve; estas la nomo de mara birdo.
  </rim>
  <trd lng="be">&c_f;&c_r;&c_e;&c_g;&c_a;&c_t;</trd>
  <trd lng="bg">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;&c_a;</trd>
  <trd lng="de">Fregatte</trd>
  <trd lng="en">frigate</trd>
  <trd lng="es">fragata</trd>
  <trd lng="fr">fr&eacute;gate<klr> (navire)</klr></trd>
  <trd lng="hu">fregatt</trd>
  <trd lng="it">fregata</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30EA;&#x30B2;&#x30FC;&#x30C8;&#33382; 
    <pr>&#x30D5;&#x30EA;&#x30B2;&#x30FC;&#x30C8;&#x304B;&#12435;</pr></trd>,
    <trd>&#x30B0;&#x30F3;&#x30AB;&#x30F3;&#x30C9;&#x30EA;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fregat</trd>,
    <trd>fregatschip</trd>
  </trdgrp>
  <trd lng="pl">fregata</trd>
  <trd lng="pt">fragata</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;</trd>
  <trd lng="sr">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;&c_a;</trd>
  <trd lng="uk">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;</trd>
  <trd lng="sk">fregata vzne&scaron;en&aacute;</trd>
  <trd lng="cs">fregatka z&aacute;padoindick&aacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E09;&#x5E06;&#x5FEB;&#x901F;&#25112;&#33328;
    <pr>s&amacron;nf&#xE1;nku&#xE0;is&#xF9;zh&#xE0;nji&#xE0;n</pr></trd>,
    <trd>&#x62A4;&#x536B;&#33328; <pr>h&#xF9;w&#xE8;iji&#xE0;n</pr></trd>,
    <trd>&#x5DE1;&#38450;&#33328; <pr>x&#xFA;nf&#xE1;ngji&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
