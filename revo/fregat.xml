<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>fregat</rad>/o</kap>
<drv mrk="fregat.0o">
  <kap><tld/>o</kap>
  <snc mrk="fregat.0o.MIL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Trimasta milita <ref tip="super"
      cel="sxip.vel0o">vel&scirc;ipo</ref>
      kun kvadrataj veloj, mezgranda inter
      <ref cel="korvet.0o">korveto</ref> kaj
      <ref cel="vesel.0o">veselo</ref>.
    </dif>
    <trd lng="de">Fregatte</trd>
    <trd lng="en">frigate</trd>
    <trd lng="es">fragata</trd>
    <trd lng="fr">fr&eacute;gate<klr tip="ind"> (navire)</klr></trd>
    <trd lng="hu">fregatt</trd>
    <trd lng="nl">fregatschip</trd>
    <trd lng="pl">fregata</trd>
    <trd lng="pt">fragata</trd>
  </snc>
  <snc mrk="fregat.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara palmopieda birdo, kies flugo estas tre rapida
      (<trd lng="la">tachypedes</trd>).
    </dif>
      <rim>
      Mi nenie trovis latinan nomon "tachypedes", nur "fregata" (Fregata
      magnificens, Fregata minor, Fregata aquila, Fregata ariel, 
      Fregata andrewsi).
      <aut>SC</aut>
      </rim>
    <trd lng="be">&c_f;&c_r;&c_e;&c_g;&c_a;&c_t;</trd>
    <trd lng="de">Pracht-Fregattvogel</trd>
    <trdgrp lng="en">
      <trd>frigate bird</trd>,
      <trd>frigatebird</trd>
    </trdgrp>
    <trd lng="fr">fr&eacute;gate<klr tip="ind"> (oiseau)</klr></trd>
    <trd lng="nl">fregatvogel</trd>
    <trdgrp lng="hu">
      <trd>fregattmad&aacute;r</trd>,
      <trd>haj&oacute;smad&aacute;r</trd>
    </trdgrp>
    <trd lng="la">fregata</trd>
    <trd lng="pl">fregata</trd>
    <trdgrp lng="pt">
      <trd>fragata <klr tip="amb">(zool.)</klr></trd>,
      <trd>tesour&atilde;o <klr tip="amb">(zool.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;</trd>
  </snc>
  <trd lng="nl">fregat</trd>
</drv>
</art>
