<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>marok</rad>/o
</kap>

<drv mrk="marok.0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="marok.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la 
      nordokcidenta Afriko kun &ccirc;efurbo Rabato.
      Teritorio de <em>Regno Maroko</em>.
      ISO-3166-1 kodo: MA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.0o.GEOG">Al&gcirc;erio</ref>,
      Okcidenta Saharo (asertas &gcirc;in kiel propra&jcirc;o),
      <ref tip="vid" cel="mediterane.0o.GEOG">Mediteraneo</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trdgrp lng="ar">
      <trd>&a_A;&a_l;&a_m;&a_gh;&a_r;&a_b;</trd>
      <trd>&a_A;&a_l;&a_m;&a_m;&a_l;&a_k;&a_t_marbuta; &a_A;&a_l;&a_m;&a_gh;&a_r;&a_b;&a_y;&a_t_marbuta;&lrm;</trd>
    </trdgrp>
    <trd lng="bg">&c_M;&c_a;&c_r;&c_o;&c_k;&c_o;</trd>
    <trd lng="de">Marokko</trd>
    <trd lng="en">Morocco</trd>
    <trd lng="it">Marocco</trd>
    <trd lng="nl">Marokko</trd>
    <trdgrp lng="pl">
      <trd>Maroko</trd>,
      <trd>Kr&oacute;lestwo <ind>Maroka</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

