<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: marok.xml,v 1.22 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Marok</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="marok.M0o">
  <kap>
    <ofc>8</ofc><tld lit="M"/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Maroko">Maroko</url></lok></fnt>
  </kap>
  <mlg kod="ISO-3166">MA</mlg>
  <snc mrk="marok.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&Scirc;tato</ref> en
      la nordokcidenta Afriko kun &ccirc;efurbo Rabato. Teritorio de <em>Regno
      <tld/>o</em>. &Gcirc;i limas kun <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>, Okcidenta Saharo (asertas
      &gcirc;in kiel propra&jcirc;on), <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteraneo</ref> kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>:
      <ekz>
        la onklino el <tld/>o
        <fnt><vrk><url>http://kantaro.ikso.net/onklino_el_maroko</url></vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ar">
      <trd>&a_A;&a_l;&a_m;&a_gh;&a_r;&a_b;</trd>
      <trd>&a_A;&a_l;&a_m;&a_m;&a_l;&a_k;&a_t_marbuta; &a_A;&a_l;&a_m;&a_gh;&a_r;&a_b;&a_y;&a_t_marbuta;&lrm;</trd>
    </trdgrp>
    <trd lng="be">&c_M;&c_a;&c_r;&c_o;&c_k;&c_a;</trd>
    <trd lng="bg">&c_M;&c_a;&c_r;&c_o;&c_k;&c_o;</trd>
    <trdgrp lng="ca">
      <trd>el <ind>Marroc</ind></trd>,
      <trd>Regne del <ind>Marroc</ind></trd>
    </trdgrp>
    <trd lng="de">Marokko</trd>
    <trd lng="en">Morocco</trd>
    <trd lng="fr">Maroc</trd>
    <trd lng="fa">&a_m;&a_r;&a_A;&f_k;&a_sh;</trd>
    <trd lng="fy">Marokko</trd>
    <trd lng="id">Maroko</trd>
    <trd lng="it">Marocco</trd>
    <trd lng="nl">Marokko</trd>
    <trdgrp lng="pl">
      <trd>Maroko</trd>,
      <trd>Kr&oacute;lestwo <ind>Maroka</ind></trd>
    </trdgrp>
    <trd lng="vo">Marok&auml;n</trd>
    <trd lng="zh">&#x6469;&#x6D1B;&#x54E5; <pr>M&#xF3;lu&#xF2;g&#x113;</pr></trd>
</snc>
  <trd lng="ja">&#x30E2;&#x30ED;&#x30C3;&#x30B3;</trd>
  <trd lng="uk">&#x41C;&#x430;&#x440;&#x43E;&#x43A;&#x43A;&#x43E;</trd>
</drv> 

<drv mrk="marok.0ano">
  <kap><tld lit="m"/>ano</kap>
  <snc>
    <dif>
      Civitano de <tld/>o:
      <ekz>
        la <tld lit="m"/>anoj havas nur malprecizan ideon pri la maniero
        la&ubreve; kiu politiko ellabori&gcirc;as &ccirc;e la &scirc;tato-pinto kaj pri la homoj,
        kiuj respondecas pri &gcirc;i
        <fnt>
          <aut>H. Alaoui, trad. J.-M. Cash</aut>,
          <vrk><url ref="&Mondediplo;article2427.html">
          Magrebo inter a&ubreve;toritatismo kaj demokratia espero</url></vrk>,
          
          <lok>le Monde diplomatique, 2016-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="en">Moroccan</trd>
  <trdgrp lng="fr"> 
    <trd>Marocain</trd>
    <trd>Marocaine<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="fy">Marokkaan</trd>
  <trd lng="nl">Marokkaan</trd>
  <trd lng="vo">Marok&auml;nan</trd>
</drv>

<drv mrk="marok.0a">
  <kap><tld lit="m"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld/>o:
      <ekz>
        kelkaj <tld lit="m"/>aj amasmedioj taksis &ccirc;ion &ccirc;i kiel
        entrudi&gcirc;on en internaj aferoj
        <fnt>
          <aut>W. Kristianasen, trad. J.-M. Cash</aut>,
          <vrk><url ref="&Mondediplo;article1261.html">
          La marokaj islamistoj volas imiti la turkan
          modelon</url></vrk>, 
          <lok>le Monde diplomatique, 2007-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_k;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="en">Moroccan</trd>
  <trdgrp lng="fr"> 
    <trd>marocain</trd>
    <trd>marocaine<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="fy">Marokkaansk</trd>
  <trd lng="nl">Marokkaans</trd>
  <trd lng="vo">Marok&auml;nik</trd>
</drv>

</art>

