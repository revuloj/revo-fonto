<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>enoter</rad>/o
</kap>
<drv mrk="enoter.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="enoter1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Oenothera</trd>), kiu malfermas siajn florojn
      precipe
      nokte a&ubreve; &ccirc;e malhela vetero.
    </dif>
  </snc>
  <trd lng="de">Nachtkerze</trd>
  <trd lng="en">evening primrose</trd>
  <trd lng="pt">enotera</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_n;&c_o;&c_t;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_i;&c_n;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

</art>
