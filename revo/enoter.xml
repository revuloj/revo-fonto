<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>enoter</rad>/o
</kap>
<drv mrk="enoter.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="enoter.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Oenothera</trd>), kiu malfermas siajn florojn precipe
      nokte a&ubreve; &ccirc;e malhela vetero.
    </dif>
  </snc>
  <trd lng="de">Nachtkerze</trd>
</drv>
<drv mrk="enoter.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Oenotheraceae</trd>.
    </dif>
  </snc>
  <trd lng="de">Nachtkerzengew&auml;chse</trd>
</drv>
</art>
