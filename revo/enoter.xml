<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enoter.xml,v 1.26 2025/03/03 06:17:27 revo Exp $">
<kap>
<rad>enoter</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="enoter.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="enoter1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Oenothera</trd>), kiu malfermas siajn florojn
      precipe nokte a&ubreve; &ccirc;e malhela vetero:
      <ekz>
        Hugo de Vries <klr>[&#x2026;]</klr> priskribis pli ol 20 plantspeciojn,
        multaj el la
        genro <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=271970">Hugo de Vries</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas rekoni <tld/>on pro la ikso-forma stigmo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20159">Enotero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ab/Oenothera_stricta_(d.j.b.)_02.jpg/320px-Oenothera_stricta_(d.j.b.)_02.jpg" prm="CC BY-SA 4.0">floro de <tld/>o (Oenothera stricta)
        <fnt>
          <aut>Dominicus Johannes Bergsma, propra verko</aut>
          <lok><url ref="&WCW;/File:Oenothera_stricta_(d.j.b.)_02.jpg">&WCW;/File:Oenothera_stricta_(d.j.b.)_02.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_e;&c_n;&c_a;&c_t;&c_e;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_s;&c_mol;&c_v;&c_je;&c_ch;&c_k;&c_a;</ind> <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="cs">pupalka</trd>
  <trd lng="de">Nachtkerze</trd>
  <trd lng="en">evening primrose</trd>
  <trd lng="fr">oenoth&egrave;re</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30C4;&#x30E8;&#x30A4;&#x30B0;&#x30B5;</trd>,
    <trd>&#x6708;&#x898B;&#x8349;  <pr>&#x3064;&#x304D;&#x307F;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x5BB5;&#x5F85;&#x8349;  <pr>&#x3088;&#x3044;&#x307E;&#x3061;&#x3050;&#x3055;</pr></trd>
  </trdgrp>
  <trd lng="pl">wiesio&lstroke;ek</trd>
  <trd lng="pt">enotera</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_n;&c_o;&c_t;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_i;&c_n;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">pupalka</trd>
  <trd lng="uk">&c_je;&#x43D;&#x43E;&c_t;&c_je;&c_r;&c_a;</trd>
  <trd lng="zh">&#x6708;&#x898B;&#x8349; <pr>yu&#xE8;ji&#xE0;nc&#x1CE;o</pr></trd>
</drv>

</art>
