<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lupe</rad>/o
</kap>
<drv mrk="lupe.0o">
  <kap>
  <tld/>o
  </kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Konveksa <ref tip="super" cel="lens.0o">lenso</ref>, kiu montras tre
      proksimajn objektojn, grandigante
      &gcirc;is &ccirc;irka&ubreve; dekfoje.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="vitr.grandiga0o">grandiga vitro</ref>
  </refgrp>
  <trd lng="es">lupa</trd>
  <trdgrp lng="fr">
    <trd>loupe</trd>,
    <trd>verre grossissant</trd>
  </trdgrp>
  <trd lng="he">&zayin;&kaf;&vav;&kaf;&yod;&tav;
  &mem;&gimel;&dalet;&lamed;&tav;</trd>
  <trdgrp lng="nl">
    <trd>loep</trd>,
    <trd>vergrootglas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_u;&c_p;&c_a;</trd>,
    <trd>&c_u;&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_s;&c_t;&c_je;&c_k;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>
