<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gnom.xml,v 1.23 2019/10/06 16:10:21 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>gnom</rad>/o
</kap>
<drv mrk="gnom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc mrk="gnom.0o.koboldo">
    <dif>
      <ref tip="dif" cel="kobold.0o">Koboldo</ref>:
      <ekz>
        maljuna <tld/>o el Norvegujo, kiu lo&gcirc;as en la malnova roko de Dovro kaj posedas
        multe da rokaj kasteloj kaj orminejon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj rigardis de alta&jcirc;oj kaj el sulkoj, &ccirc;ie aperis ru&gcirc;aj, bluaj kaj verdaj
        lumoj, kiujn ili portis en la manoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        flanke de homoj, rolas elfoj, <tld/>oj
        <fnt><aut>Familio Macgill</aut><vrk>June, nejune, pretertempe 
        (J. R. R. Tolkien: La kunularo de l' ringo)</vrk>
        <bib>Monato</bib><lok>jaro 1996a, numero 4a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_o;&c_m;</trd>
  <trd lng="bg">&c_g;&c_n;&c_o;&c_m;</trd>
  <trd lng="ca">gnom</trd>
  <trdgrp lng="cs">
    <trd>gn&oacute;m</trd>,
    <trd>podzemn&iacute; duch</trd>,
    <trd>sk&rcaron;&iacute;tek</trd>
  </trdgrp>
  <trd lng="de">Gnom</trd>
  <trd lng="en">gnome</trd>
  <trd lng="es">gnomo</trd>
  <trdgrp lng="fr">
    <trd>gnome</trd>,
    <trd>kobold</trd>,
    <trd>lutin</trd>,
    <trd>nain<klr> (myth.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;rpe</trd>,
    <trd>gn&oacute;m</trd>
  </trdgrp>
  <trd lng="it">gnomo</trd>
  <trdgrp lng="nl">
    <trd>gnoom</trd>,
    <trd>berggeest</trd>,
    <trd>aardgeest</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gnom</trd>,
    <trd>krasnolud</trd>
  </trdgrp>
  <trd lng="pt">gnomo</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>trpasl&iacute;k</trd>,
    <trd>&scaron;kriatok</trd>
  </trdgrp>
  <trd lng="tr">c&uuml;ce</trd>
</drv>

<drv mrk="gnom.0a">
  <kap><tld/>a</kap>
  <snc mrk="gnom.0a.rilata">
    <dif>
      Rilata al aspekto, moroj, eteco de <tld/>oj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>a &scirc;tato &jcirc;us fari&gcirc;is 189a membro 
        de Unui&gcirc;intaj Nacioj
        <fnt><aut>Stefan Maul</aut><vrk>Ri&ccirc;a per .tv</vrk>
        <bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">gn&ograve;mic</trd>
  <trdgrp lng="de">
    <trd>gnomenhaft</trd>,
    <trd>winzig <klr>(wie ein Gnom)</klr></trd>
  </trdgrp>
  <trd lng="en">gnomish</trd>
  <trdgrp lng="fr">
    <trd>nain<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gnomesco</trd>,
    <trd>nano<klr> (agg.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mikry</trd>,
    <trd>maciupki</trd>
  </trdgrp>
</drv>
</art>
