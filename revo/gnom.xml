<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>2</ofc>
<rad>gnom</rad>/o</kap>
<drv mrk="gnom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      Fabela malgranda esta&jcirc;o, gardanta trezorojn en la interno de la tero.
    </dif>
    <refgrp tip="vid">
      <ref cel="kobold.0o">koboldo</ref>,
      <ref cel="fe.0o">feo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Gnom</trd>
  <trd lng="fr">gnome</trd>
  <trdgrp lng="nl">
    <trd>gnoom</trd>,
    <trd>berggeest</trd>, 
    <trd>aardgeest</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_m;</trd>
</drv>
</art>
