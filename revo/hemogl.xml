<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemogl.xml,v 1.17 2023/08/31 15:46:16 revo Exp $">
<kap>
  <rad>hemoglobin</rad>/o
</kap>
<drv mrk="hemogl.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">FIZL</uzo>
  <snc mrk="hemogl.0o.FIZL">
    <dif>
      Proteino en la <ref tip="malprt" cel="cxel.rugxasango0o">ru&gcirc;aj sango&ccirc;eloj</ref>,
      kiu kolorigas ilin kaj portas oksigenon al korpaj &ccirc;eloj:
      <ekz>
        oksidigo de la <tld/>o en la pulmoj regeneras la sangon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_g;&c_l;&c_ja;&c_b;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
    <trd>H&auml;moglobin</trd>,
    <trd>Blutfarbstoff</trd>
  </trdgrp>
  <trd lng="el">&alfa;&jota;&my;&omikron;&gamma;&lambda;&omikron;&beta;&jota_ton;&ny;&eta;</trd>
  <trd lng="en">hemoglobin</trd>
  <trd lng="es">hemoglobina</trd>
  <trd lng="fr">h&eacute;moglobine</trd>
  <trdgrp lng="hu">
    <trd>v&ouml;r&ouml;s v&eacute;rfest&eacute;k</trd>,
    <trd>hemoglobin</trd>
  </trdgrp>
  <trd lng="it">emoglobina</trd>
  <trd lng="ja">&#x30D8;&#x30E2;&#x30B0;&#x30ED;&#x30D3;&#x30F3;</trd>
  <trd lng="nl">hemoglobine</trd>
  <trd lng="pl">hemoglobina</trd>
  <trd lng="pt">hemoglobina</trd>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_g;&c_l;&c_o;&c_b;&c_i;&c_n;</trd>
  <trd lng="uk">&#x433;&#x435;&#x43C;&#x43E;&#x433;&#x43B;&#x43E;&#x431;&#x456;&#x43D;</trd>
</drv>
</art>
