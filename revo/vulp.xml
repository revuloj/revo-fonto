<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vulp.xml,v 1.30 2018/05/30 20:10:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vulp</rad>/o
</kap>

<drv mrk="vulp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">Rabobesto</ref>
      kun longa longhara vosto kaj ru&gcirc;eta pelto;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el la familio <ref
      tip="malprt"
      cel="hunded.0oj">hundedoj</ref>
      (<trd lng="la">Vulpes</trd>):
      <ekz>
        viza&gcirc;o sen kulpo, sed koro de <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne ekzistas naiva <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        se <tld/>o pentofaras, gardu la kokidojn
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ref cel="ruz.0a">ruza</ref> kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_s;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">louarn</trd>
  <trd lng="ca">guineu</trd>
  <trd lng="cs">li&scaron;ka</trd>
  <trd lng="de">Fuchs</trd>
  <trd lng="el">&alfa;&lambda;&epsilon;&pi;&omikron;&ypsilon_ton;</trd>
  <trd lng="en">fox</trd>
  <trd lng="es">zorro</trd>
  <trd lng="fa">&a_r;&a_w;&a_b;&a_A;&a_h;</trd>
  <trd lng="fr">renard</trd>
  <trd lng="he">&shin;&vav;&ayin;&lamed;</trd>
  <trd lng="hu">r&oacute;ka</trd>
  <trd lng="id">rubah</trd>
  <trd lng="nl">vos</trd>
  <trd lng="pl">lis</trd>
  <trd lng="pt">raposa</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">l&iacute;&scaron;ka</trd>
  <trd lng="sv">r&auml;v</trd>
</drv>

<drv mrk="vulp.V0ino">
  <kap><tld lit="V"/>ino
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Vul</mlg>
  <snc mrk="vulp.V0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Vulpecula</trd>).
    </dif>
  </snc>
  <trd lng="en">Vulpecula</trd>
  <trd lng="fr">Petit Renard<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="nl">Vosje</trd>
</drv>

<drv mrk="vulp.vir0o">
  <kap>vir<tld/>o</kap>
  <snc>
    <dif>Vira vulpo</dif>
  </snc>
  <trdgrp lng="nl">
    <trd>moervos</trd>,
    <trd>wijfjesvos</trd>,
    <trd>vrouwtjesvos</trd>
  </trdgrp>
</drv>

<drv mrk="vulp.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>Ina vulpo</dif>
  </snc>
  <trdgrp lng="nl">
    <trd>rekel</trd>,
    <trd>mannetjesvos</trd>
  </trdgrp>
</drv>
</art>
