<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vulp.xml,v 1.38 2024/12/23 18:33:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vulp</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="vulp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="vulp.0o.rabobesto">
    <dif>
      <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">Rabobesto</ref>
      kun longa longhara vosto kaj ru&gcirc;eta pelto;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el la familio <ref tip="malprt" cel="hunded.0oj">hundedoj</ref>
      (<trd lng="la">Vulpes</trd>):
      <ekz>
        ruza kiel <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;a filo <klr>[&#8230;]</klr> pafis, sed maltrafis la <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        andaj <tld/>oj longavostaj
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php"
              >Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        ampleksa programo por vakcinadi <tld/>ojn kontra&ubreve; rabio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009614.php"
              >Historia histerio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;o sen kulpo, sed koro de <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne ekzistas naiva <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        se <tld/>o pentofaras, gardu la kokidojn
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="vulp.0o.ruzulo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ruzulo:
      <ekz>
        knabo mia, fabelu al iu ajn, sed ne al sperta <tld/>o kiel mi
        <fnt>
          <bib>ChV</bib>
          <lok>8</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sagac.0ulo">sagaculo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_s;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">louarn</trd>
  <trd lng="ca">guineu</trd>
  <trd lng="cs">li&scaron;ka</trd>
  <trd lng="de">Fuchs</trd>
  <trd lng="el">&alfa;&lambda;&epsilon;&pi;&omikron;&ypsilon_ton;</trd>
  <trd lng="en">fox</trd>
  <trd lng="es">zorro</trd>
  <trd lng="fa">&a_r;&a_w;&a_b;&a_A;&a_h;</trd>
  <trd lng="fr">renard</trd>
  <trd lng="fy">foks</trd>
  <trd lng="he">&shin;&vav;&ayin;&lamed;</trd>
  <trd lng="hu">r&oacute;ka</trd>
  <trd lng="id">rubah</trd>
  <trd lng="io">foxo</trd>
  <trd lng="ja">&#12461;&#12484;&#12493;  <pr>&#12365;&#12388;&#12397;</pr></trd>
  <trd lng="lfn">volpe</trd>
  <trd lng="nl">vos</trd>
  <trd lng="pl">lis</trd>
  <trd lng="pt">raposa</trd>
  <trd lng="ro">vulpe</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">l&iacute;&scaron;ka</trd>
  <trd lng="sv">r&auml;v</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_s;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_i;&c_c;&c_ja;</trd>
  </trdgrp>
  <trd lng="vo">renar</trd>
  <trd lng="zh">&#29392;&#29432; <pr>h&uacute;li</pr></trd>
</drv>

<drv mrk="vulp.V0o">
  <kap>
    <tld lit="V"/>o,
    <var><kap>
      <tld lit="V"/>ino
      <fnt>
        <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
        <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
        universo</vrk>.
        <lok>FEL, Antverpeno, 2005</lok>
      </fnt>
    </kap></var>
  </kap>
  <mlg>Vul</mlg>
  <snc mrk="vulp.V0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Vulpecula</trd>):
      <ekz>
        NGC 6800 estas malfermita stelamaso en konstelacio <tld lit="V"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=120109"
              >NGC 6800</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_ib;&c_s;&c_ib;&c_ch;&c_k;&c_a; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;e)</klr></trd>
  <trd lng="de">Fuchs <klr>(Sternbild)</klr></trd>
  <trd lng="en">Vulpecula</trd>
  <trd lng="fr">Petit Renard<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="lfn">Volpe</trd>
  <trd lng="nl">Vosje</trd>
  <trd lng="pl">Lisek  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Vulpea <klr>(constela&tcomma;ie)</klr></trd>
  <trd lng="zh">&#29392;&#29432;&#24231; <pr>H&uacute;lizu&ograve;</pr></trd>
</drv>

<drv mrk="vulp.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Ina <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu &ccirc;i <tld/>ino ka&ubreve;zis la tutan katastrofon, &scirc;i, kiu rapidis publikigi lian
        leteron, tiun malbenitan leteron
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_s;&c_ib;&c_c;&c_a; <klr>(&c_s;&c_a;&c_m;&c_k;&c_a;)</klr></trd>
  <trd lng="de">F&uuml;chsin</trd>
  <trd lng="en">vixen</trd>
  <trd lng="fr">renarde</trd>
  <trd lng="io">foxino</trd>
  <trdgrp lng="nl">
    <trd>moervos</trd>,
    <trd>vossin</trd>,
    <trd>wijfjesvos</trd>,
    <trd>vrouwtjesvos</trd>
  </trdgrp>
  <trd lng="pl">lisica</trd>
  <trd lng="ro">c&abreve;&tcomma;ea</trd>
  <trd lng="vo">jirenar</trd>
</drv>

