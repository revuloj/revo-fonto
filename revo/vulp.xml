<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vulp</rad>/o
</kap>
<drv mrk="vulp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="hunded.0oj">hundedoj</ref>
      (<trd lng="la">Vulpes</trd>), kun longa longhara vosto
      kaj ru&gcirc;eta pelto:
      <ekz>
        viza&gcirc;o sen kulpo, sed koro de <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne ekzistas naiva <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se <tld/>o pentofaras, gardu la kokidojn
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ruza kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_s;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Fuchs</trd>
  <trd lng="cs">li&scaron;ka</trd>
  <trd lng="el">&alfa;&lambda;&epsilon;&pi;&omikron;&ypsilon_ton;</trd>
  <trd lng="en">fox</trd>
  <trd lng="es">zorro</trd>
  <trd lng="fr">renard</trd>
  <trd lng="he">&shin;&vav;&ayin;&lamed;</trd>
  <trd lng="hu">r&oacute;ka</trd>
  <trd lng="nl">vos</trd>
  <trd lng="pl">lis</trd>
  <trd lng="pt">raposa</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
