<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: poci.xml,v 1.3 2023/11/28 09:27:01 afido Exp $">
    <kap>
      <rad>poci</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="poci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kurac.0ilo">Kuracilo</ref> likva, glutenda:
      <ekz>
        <klr>[vi]</klr> enliti&gcirc;os kaj englutos la <tld/>on, kiun post momento mi sendos
        al vi
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        penu <klr>[&#8230;]</klr> en&scirc;ovi inter liaj dentoj kelkajn gutojn de tiu <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u anta&ubreve; la pendigo oni donis al ili la <tld/>on
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>?
      </ekz>
      <ekz>
        oni <klr>[bezonus]</klr> magian <tld/>on por kontra&ubreve;stari
        <fnt>
          <bib>Monato</bib>
          <aut>&Jcirc;anin Ratu&scirc;njak</aut>
          <vrk><url ref="&Monato;/2010/009980.php"
              >Senpaga transporto danke al lo&gcirc;antarkonscii&gcirc;o</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        libero estas la plej granda iluzio <klr>[&#8230;]</klr>, dormiga <tld/>o altrudita al
        la homaro de post prahistoriaj tempoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jo Haazen</aut>
          <vrk><url ref="&Monato;/2013/010773.php"
              >Klareco kaj simpleco, fundamento por nova epoko</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekokt.0ajxo">dekokta&jcirc;o</ref>,
      <ref cel="drog.0o">drogo</ref>,
      <ref cel="infuz.0ajxo">infuza&jcirc;o</ref>,
      <ref cel="tinktu.0o.MED">tinkturo<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k; <klr tip="amb">(&c_l;&c_je;&c_k;&c_a;&c_v;&c_y;)</klr></trd>,
      <trd>&c_z;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Heiltrank</trd>,
      <trd>Zaubertrank</trd>
    </trdgrp>
    <trd lng="en">potion</trd>
    <trd lng="fr">potion</trd>
    <trd lng="ru">&c_z;&c_je;&c_l;&c_mol;&c_je;</trd>
  </snc>
</drv>

  </art>

