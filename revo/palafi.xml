<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: palafi.xml,v 1.19 2024/01/31 18:21:03 revo Exp $">
<kap>
  <rad>palafit</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="palafi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc mrk="palafi.0o.domo">
    <dif>
      Surfosta <ref tip="super" cel="dom.0o.konstruajxo">domo</ref>, konstruata kutime super akvo a&ubreve; mar&ccirc;oj:
      <ekz>
        la senvandaj <tld/>oj de la vila&gcirc;eto situis inter forhakitaj
        bran&ccirc;oj en subakvigita mar&ccirc;o<fnt><aut>Werner Schad</aut>:
        <vrk>Orinoko-delto: Indianoj, mar&ccirc;oj, arbarego</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
        20a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>oj konstruitaj la&ubreve;longe de la rivero <nom>Tuojiang</nom> kiu kviete fluas sub ili
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/2015-07/13/content_36045906.htm"
              >La antikva urbo Fenikso pompe lumigita</url></vrk>
          <lok>Esperanto China, 2015-07-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvas bildoj de la lago, la <tld/>oj hejmaj
        <fnt>
          <vrk><url ref="https://books.google.de/books?id=uLMPQ_bhy0kC&amp;lpg=PA115&amp;ots=Z88dMGGbSp&amp;pg=PA115"
              >Belartaj konkursoj 2009</url></vrk>
          <lok>Beletra Alamaniko 6, p. 115</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ja;&c_f;&c_ib;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_v;&c_y; &c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&uring;m na k&uring;lech</trd>,
    <trd>kolov&yacute; d&uring;m</trd>
  </trdgrp>
  <trd lng="de">Pfahlbau</trd>
  <trd lng="en">stilt house</trd>
  <trd lng="es">palafito</trd>
  <trdgrp lng="fr">
    <trd>maison sur pilotis</trd>,
    <trd>palafitte</trd>
  </trdgrp>
  <trd lng="hu">c&ouml;l&ouml;ph&aacute;z</trd>
  <trdgrp lng="ja">
    <trd>&#28246;&#19978;&#23478;&#23627;  <pr>&#12371;&#12376;&#12423;&#12358;&#12363;&#12362;&#12367;</pr></trd>,
    <trd>&#26477;&#19978;&#20303;&#23621;  <pr>&#12367;&#12356;&#12376;&#12423;&#12358;&#12376;&#12421;&#12358;&#12365;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="nl">paalwoning</trd>
  <trd lng="pl">palafit</trd>
  <trd lng="pt">palafita</trd>
  <trd lng="ro">palatul</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_m; &c_n;&c_a; &c_v;&c_y;&c_s;&c_o;&c_k;&c_i;&c_h;
    &c_s;&c_v;&c_a;&c_ja;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dom na koloch</trd>,
    <trd>kolov&yacute; dom</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_l;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_d;&c_ib;&c_v;&c_l;&c_ja; &c_n;&c_a; &c_p;&c_a;&c_l;&c_ja;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_f;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>
</art>
