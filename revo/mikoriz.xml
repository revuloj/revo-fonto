<vortaro>
  <art mrk="$Id: mikoriz.xml,v 1.2 2026/02/21 02:48:27 afido Exp $">
    <kap>
      <rad>mikoriz</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="mikoriz.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikoriz.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="simbio.0o.BOT">Simbiozo</ref> inter fungo kaj la radikoj de planto:
      <ekz>
        la teamo de la universitato <klr>[&#8230;]</klr> sukcesis pruvi ke arboj kreskas des
        pli rapide, ju pli da <tld/>o-fungoj ili havas &ccirc;irka&ubreve; siaj radikoj
        <fnt>
          <vrk><url
          ref="https://esperantaretradio.blogspot.com/2020/10/grundaj-fungoj-subtenas-kreskadon-de.html"
              >Grundaj fungoj subtenas kreskadon de arboj</url></vrk>
          <lok>Esperanta Retradio, 2020-10-31</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mykorrhiza</trd>
    <trd lng="en">mycorrhiza</trd>
    <trd lng="fr">mycorhize</trd>
    <trd lng="pl">mikoryza</trd>
    <trd lng="ru">&c_m;&c_i;&c_k;&c_o;&c_r;&c_i;&c_z;&c_a;</trd>
  </snc>
 </drv>
</art>

