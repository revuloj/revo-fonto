<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: simon.xml,v 1.14 2023/10/10 20:23:30 revo Exp $">
<kap>
  <rad>Simon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="simon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        ili kaptis <tld/>on, Kirenanon, venantan de la kamparo, kaj
        metis sur lin la krucon
        <fnt><bib>NT</bib>, <lok>&Luk; 23:26</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_y;&c_m;&c_o;&c_n;</trd>
  <trd lng="bg">&c_S;&c_i;&c_m;&c_o;&c_n;</trd>
  <trd lng="br">Simon</trd>
  <trd lng="de">Simon</trd>
  <trd lng="fr">Simon</trd>
  <trd lng="hu">Simon</trd>
  <trd lng="it">Simone</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E2;&#x30F3;</trd>,
    <trd>&#x30B5;&#x30A4;&#x30E2;&#x30F3;</trd>,
    <trd>&#x30B7;&#x30E2;&#x30FC;&#x30CC;</trd>
  </trdgrp>
  <trd lng="nl">Simon</trd>
  <trd lng="pl">Szymon</trd>
  <trdgrp lng="ru">
    <trd>&c_S;&c_je;&c_m;&c_jo;&c_n;</trd>,
    <trd>&c_S;&c_i;&c_m;&c_o;&c_n;<klr> (&c_b;&c_i;&c_b;&c_l;., &c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_S;&c_i;&c_m;&c_je;&c_o;&c_n;<klr> (&c_u;&c_s;&c_t;&c_a;&c_r;.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_S;&c_i;&#x43C;&#x43E;&#x43D;</trd>
</drv>
</art>

