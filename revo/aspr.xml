<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aspr.xml,v 1.14 2021/03/04 17:56:54 revo Exp $">
<kap>
  <rad>aspr</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aspr.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="aspr.0a.sento">
    <dif>
      <ref tip="dif" cel="glat.mal0a">Malglata</ref>:
      <ekz>
        kvar <tld/>aj tabuloj sur du stabloj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 16a, p.
        111a</lok></fnt>;
      </ekz>
      <ekz>
        la germana estas &leftquot;aspra&rightquot;, &leftquot;bruta&rightquot; kaj &leftquot;malina&rightquot; &dash; plejparte mi a&ubreve;dis tiajn
        eldira&jcirc;ojn de homoj kiuj parolas nek la francan nek la germanan
        <fnt>
          <vrk><url ref="https://lernu.net/ca/forumo/temo/3360/1">&Ccirc;u &leftquot;Molaj&rightquot; lingvoj? / Sobre altres lleng&uuml;es</url></vrk>
          <lok>Forumo lernu.net</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rauxk.0a">ra&ubreve;ka</ref>
    <trdgrp lng="be">
      <trd>&c_sh;&c_u;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_n;&c_ja;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_ja;&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>rough</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>rau</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>r&ecirc;che</trd>,
      <trd>rugueux</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>aspro<klr> (al tatto)</klr></trd>,
      <trd>scabro</trd>,
      <trd>ruvido</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szorstki</trd>,
      <trd>nier&oacute;wny</trd>,
      <trd>surowy</trd>
    </trdgrp>
    <trd lng="pt">&aacute;spero</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_sh;&c_je;&c_r;&c_o;&c_h;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aspr.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malkomforta, peniga, ka&ubreve;zanta malfacilojn.
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="fr">&acirc;pre<klr> (fig.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_t;&c_je;&c_r;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">aspre</trd>
  <trd lng="cs">drsn&yacute; <klr>(povrch)</klr></trd>
  <trd lng="es">&aacute;spero</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D7;&#x5D5;&#x5BC;&#x5E1;&#x5E4;&#x5BC;&#x5B8;&#x5E1;</trd>
  <trd lng="nl">ruw</trd>
  <trdgrp lng="sk">
    <trd>drap&lcaron;av&yacute;</trd>,
    <trd>drsn&yacute;</trd>
  </trdgrp>
</drv>
</art>

