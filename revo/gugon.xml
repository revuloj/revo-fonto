<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gugon.xml,v 1.2 2013/10/11 09:10:11 revo Exp $">
<kap>
    <rad>gugon</rad>/o
</kap>
<drv mrk="gugon.0o">
  <kap><tld/>o</kap>
  <snc mrk="gugon.0o.fisxo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="gobi.0o">Gobio:</ref>
      <ekz>
        &scirc;i iris al la rivereto, kiu fluis tra la &gcirc;ardeno, kaj ordonis
        kapti sitelon da gugonoj
        <fnt><bib>ElektFab</bib>, <lok>fabelo pri iu, kiu migris por
        ekkoni timon</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_ch;&c_k;&c_u;&c_r;</trd>
  <trd lng="ca">gobi</trd>
  <trd lng="de">Gr&uuml;ndling</trd>
  <trd lng="en">gudgeon</trd>
  <trd lng="fr">goujon</trd>
  <trd lng="hu">g&eacute;b</trd>
  <trd lng="it">ghizzo</trd>
  <trdgrp lng="nl">
    <trd>grondel</trd>,
    <trd>grondeling</trd>
  </trdgrp>
  <trd lng="pl">kielb</trd>
  <trdgrp lng="pt">
   <trd>g&oacute;bio</trd>, 
   <trd>cadoz</trd>, 
   <trd>gobi&atilde;o</trd> 
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_k;&c_a;&c_r;&c_mol;</trd>
</drv>
</art>

