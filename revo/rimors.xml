   <rad>rimors</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rimors.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       Da&ubreve;ra, dolora sento pri propra kulpo:
       <ekz>
         ru&gcirc;is la viza&gcirc;o de Caiyun, kiun kaj honto kaj <tld/>o
         ekron&gcirc;is
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 61a, p. 326a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="bedaux.0o">beda&ubreve;ro</ref>,
       <ref cel="pent.0i">penti</ref>
     </refgrp>
   </snc>
  <trd lng="be">&c_z;&c_g;&c_r;&c_y;&c_z;&c_o;&c_t;&c_a;</trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_k;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>l&#xED;tost</trd>,
    <trd>v&#xFD;&#x10D;itka sv&#x11B;dom&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Schuldgef&uuml;hl</trd>
   </trdgrp>
  <trd lng="en">remorse</trd>
  <trdgrp lng="es">
     <trd>remordimiento</trd>
   </trdgrp>
  <trdgrp lng="fa">
     <trd>&f_p;&a_sh;&f_y;&a_m;&a_A;&a_n;&f_y;</trd>,
     <trd>&a_n;&a_d;&a_A;&a_m;&a_t;</trd>,
     <trd>&a_ayn;&a_dh;&a_A;&a_b; &a_w;&a_j;&a_d;&a_A;&a_n;</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>remords</trd>
   </trdgrp>
  <trd lng="hu">lelkiismeret-furdal&aacute;s</trd>
  <trd lng="it">rimorso</trd>
  <trd lng="pl">wyrzuty sumienia  {pl}</trd>
  <trd lng="ro">remu&#x219;care</trd>
  <trd lng="ru">&c_u;&c_g;&c_r;&c_y;&c_z;&c_je;&c_n;&c_i;&c_ja; &c_s;&c_o;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  <trd lng="sk">v&#xFD;&#x10D;itka svedomia</trd>
  <trd lng="uk">&c_d;&#x43E;&#x43A;&c_ib;&c_r; &c_s;&c_u;&#x43C;&#x43B;&c_ib;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x759A; <pr>ji&#xF9;</pr></trd>,
    <trd>&#x5167;&#x759A; <pr>n&#xE8;iji&#xF9;</pr></trd>,
    <trd>&#x5185;&#x759A; <pr>n&#xE8;iji&#xF9;</pr></trd>,
    <trd>&#x6094;&#x6068; <pr>hu&#x1D0;h&#xE8;n</pr></trd>,
    <trd>&#x540E;&#x6094; <pr>h&#xF2;uhu&#x1D0;</pr></trd>,
    <trd>&#x5F8C;&#x6094; <pr>h&#xF2;uhu&#x1D0;</pr></trd>,
    <trd>&#x6094;&#x610F; <pr>hu&#x1D0;y&#xEC;</pr></trd>
  </trdgrp>
</drv>
