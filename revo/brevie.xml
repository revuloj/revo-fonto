<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brevie.xml,v 1.24 2023/10/30 06:30:15 revo Exp $">
<kap>
  <rad>brevier</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="brevie.0o">
  <kap><tld/>o</kap>
  <snc mrk="brevie.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Latina <ref tip="vid" cel="pregx.0o">pre&gcirc;o</ref>libro por katolikaj pastroj, enhavanta la
      devigajn pre&gcirc;ojn difinitajn por &ccirc;iu tago de la
      jaro:
      <ekz>
        gajmiena, dika pastro <klr>[&#8230;]</klr> trankvile legas sian <tld/>on
        <fnt>
          <bib>KrB</bib>
          <lok>Virino &ccirc;e landlimo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Liszt</nom> asertis sin hungaro, <klr>[&#8230;]</klr> en Romo <klr>[&#8230;]</klr> li devis
        kompreni la italan lingvon, kaj, ricevinte la subajn pastrajn benojn, li
        legis la <tld/>on latine
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk>Geniulo mondcivitana</vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la binditaj kolektoj de la malnova LOdE estas nia <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="epitom.0o">epitomo</ref>
    </refgrp>
    <trd lng="be">&c_b;&c_r;&c_e;&c_v;&c_ib;&c_ja;&c_r;&c_y;&c_j;</trd>
    <trd lng="bg">&c_t;&c_r;&c_je;&c_b;&c_n;&c_i;&c_k;</trd>
    <trd lng="en">breviary</trd>
    <trd lng="pl">brewiarz</trd>
    <trdgrp lng="ru">
      <trd><klr>(&c_k;&c_r;&c_a;&c_t;&c_k;&c_i;&c_j;)
      </klr>&c_m;&c_o;&c_l;&c_i;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_t;&c_r;&c_je;&c_b;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sk">brevi&aacute;r</trd>
    <trd lng="zh">&#31934;&#21326;&#24405; <pr>j&#299;nghu&aacute;l&ugrave;</pr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>brevi&aacute;&rcaron;</trd>,
    <trd>brev&iacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brevier</trd>,
    <trd>Stundenbuch</trd>
  </trdgrp>
  <trd lng="fr">br&eacute;viaire</trd>
  <trd lng="he">breviero</trd>
  <trd lng="hu">brevi&aacute;rium</trd>
  <trd lng="it">breviario</trd>
  <trdgrp lng="ja">
    <trd>&#32854;&#21209;&#26085;&#35506;&#26360;  <pr>&#12402;&#12376;&#12426;&#12388;&#12392;&#12416;&#12395;&#12387;&#12363;&#12375;&#12423;</pr></trd>,
    <trd>&#24859;&#35501;&#26360;  <pr>&#12354;&#12356;&#12393;&#12367;&#12375;&#12423;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>(&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j;) &c_m;&c_o;&c_l;&c_i;&c_t;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_b;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
