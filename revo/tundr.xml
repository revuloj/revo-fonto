<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tundr</rad>/o
    </kap>

    <drv mrk="tundr.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="tundr.0o.norda">
	<dif>
	  Senarba ebena&jcirc;o inter la Arkta oceano kaj
	  <ref cel="tajg.0o">tajgo</ref>, kun konstante glacii&gcirc;inta
	  subgrundo, kie kreskas precipe muskoj, likenoj, herboj kaj
	  arbustoj:
          <ekz>
            ili scias, ke tie, sur la limo mem de la du <tld
            lit="T"/>oj, de nia Tundro por la vivantoj kaj de la alia
            <tld lit="T"/>o por la mortintoj, ku&scirc;as terura eterna lago,
            senlime lar&gcirc;ega, glacie malvarma, senfunde profunda; kaj
            tiun &ccirc;i lagon gardante, staras &ccirc;e &gcirc;i senkompateme severega
            maljunulino, pli praa&gcirc;a ol la <tld lit="T"/>o mem
            <fnt>
              <aut>Ero&scirc;enko</aut>: <vrk>La destinita vivi.</vrk>
              <lok>El vivo de &ccirc;uk&ccirc;oj
                (Esperanta Ligilo, n-roj 5&ndash;8, 1933).</lok>
            </fnt>.
          </ekz>
	</dif>
	<trd lng="nl">toendra</trd>
      </snc>
      <snc mrk="tundr.0o.suda">
	<dif>
	  Simila regiono en la suda duonglobo apud Antarkto.
	</dif>
      </snc>
      <snc mrk="tundr.0o.monta">
	<dif>
	  Simila senarba regiono en montaro, super la arbara zono.
	</dif>
      </snc>
      <trd lng="be">&c_t;&c_u;&c_n;&c_d;&c_r;&c_a;</trd>
      <trd lng="de">Tundra</trd>
      <trd lng="en">tundra</trd>
      <trd lng="hu">tundra</trd>
      <trd lng="pt">tundra</trd>
      <trd lng="ru">&c_t;&c_u;&c_n;&c_d;&c_r;&c_a;</trd>
    </drv>

    <drv mrk="tundr.0ano">
      <kap><tld/>ano</kap>
      <snc>
	<dif>
          Lo&gcirc;anto de <tld/>o:
          <ekz>
            la &ccirc;uk&ccirc;oj bonege konas siajn sam<tld/>anojn, multe pli
            bone ol ni konas niajn samvila&gcirc;anojn
            <fnt>
              <aut>Ero&scirc;enko</aut>: <vrk>Blindaj &ccirc;uk&ccirc;oj.</vrk>
              <lok>El vivo de &ccirc;uk&ccirc;oj
                (Esperanta Ligilo, n-roj 5&ndash;8, 1933).</lok>
            </fnt>.            
          </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_zh;&c_y;&c_h;&c_a;&c_r; &c_t;&c_u;&c_n;&c_d;&c_r;&c_y;</trd>
      <trd lng="hu">tundralak&oacute;</trd>
      <trd lng="ru">&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol; &c_t;&c_u;&c_n;&c_d;&c_r;&c_y;</trd>
    </drv>

  </art>
