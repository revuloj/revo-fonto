<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tundr.xml,v 1.19 2023/09/04 19:01:55 revo Exp $">
<kap>
  <rad>tundr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="tundr.0o">
  <kap><tld/>o</kap>
  <snc mrk="tundr.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Senarba ebena&jcirc;o kun konstante glacii&gcirc;inta
      subgrundo, kie kreskas precipe muskoj, likenoj, herboj kaj
      arbustoj.
    </dif>
    <subsnc mrk="tundr.0o.norda">
      <dif>
        Tia regiono en la norda globduono inter la Arkta oceano kaj
        <ref cel="tajg.0o">tajgo</ref>:
        <ekz>
          tiun &ccirc;i lagon gardante, staras &ccirc;e
          &gcirc;i senkompateme severega
          maljunulino, pli praa&gcirc;a ol la <tld lit="T"/>o mem
            <fnt>
              <aut>V. Ero&scirc;enko</aut>,
              <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>
              <lok>Esperanta Ligilo, 1933:5&ndash;8</lok>
            </fnt>.
        </ekz>
      </dif>
      <trd lng="nl">toendra</trd>
    </subsnc>
    <subsnc mrk="tundr.0o.suda">
      <dif>
        Tia regiono en la suda globduono apud Antarkto.
      </dif>
    </subsnc>
    <subsnc mrk="tundr.0o.monta">
      <dif>
        Tia senarba regiono en montaro, super la arbara zono.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_n;&c_d;&c_r;&c_a;</trd>
  <trd lng="bg">&c_t;&c_u;&c_n;&c_d;&c_r;&c_a;</trd>
  <trd lng="ca">tundra</trd>
  <trd lng="cs">tundra</trd>
  <trd lng="de">Tundra</trd>
  <trd lng="en">tundra</trd>
  <trd lng="fr">toundra</trd>
  <trd lng="hu">tundra</trd>
  <trd lng="ja">&#x30C4;&#x30F3;&#x30C9;&#x30E9;</trd>
  <trd lng="pl">tundra</trd>
  <trd lng="pt">tundra</trd>
  <trd lng="ru">&c_t;&c_u;&c_n;&c_d;&c_r;&c_a;</trd>
  <trd lng="sk">tundra</trd>
  <trd lng="uk">&#x442;&#x443;&#x43D;&#x434;&#x440;&#x430;</trd>
</drv>

<drv mrk="tundr.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="tundr.0ano.GEOG">
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        la &ccirc;uk&ccirc;oj bonege konas siajn sam<tld/>anojn, multe
        pli bone ol ni konas niajn samvila&gcirc;anojn
        <fnt>
          <aut>V. Ero&scirc;enko</aut>,
          <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>
          <lok>Esperanta Ligilo, 1933:5&ndash;8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_h;&c_a;&c_r; &c_t;&c_u;&c_n;&c_d;&c_r;&c_y;</trd>
  <trd lng="ca">habitant de la <ind>tundra</ind></trd>
  <trd lng="fr">habitant de la <ind>toundra</ind></trd>
  <trd lng="hu">tundralak&oacute;</trd>
  <trd lng="pl">mieszkaniec <ind>tundry</ind></trd>
  <trd lng="ru">&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol; &c_t;&c_u;&c_n;&c_d;&c_r;&c_y;</trd>
</drv>

</art>
