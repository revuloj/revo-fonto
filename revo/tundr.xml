<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tundr</rad>/o
    </kap>
    <drv mrk="tundr.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="tundr.0o.norda">
	<dif>
	  Senarba ebena&jcirc;o inter la Arkta oceano kaj
	  <ref cel="tajg.0o">tajgo</ref>, kun konstante glacii&gcirc;inta
	  subgrundo, kie kreskas precipe muskoj, likenoj, herboj kaj
	  arbustoj.  
	</dif>
	<trd lng="nl">toendra</trd>
      </snc>
      <snc mrk="tundr.0o.suda">
	<dif>
	  Simila regiono en la suda duonglobo apud Antarkto.
	</dif>
      </snc>
      <snc mrk="tundr.0o.monta">
	<dif>
	  Simila senarba regiono en montaro, super la arbara zono.
	</dif>
      </snc>
      <trd lng="de">Tundra</trd>
      <trd lng="en">tundra</trd>
      <trd lng="ru">&c_t;&c_u;&c_n;&c_d;&c_r;&c_a;</trd>
    </drv>
  </art>
