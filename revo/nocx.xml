<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nocx.xml,v 1.12 2025/03/07 16:21:52 revo Exp $">
<kap>
  <rad>no&ccirc;</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="nocx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nocx.0i.KOMUNE">
    <dif>
      En<ref tip="super" cel="trancx.0i.KOMUNE">tran&ccirc;i</ref>
      V-forman fendeton (en bastono, e&gcirc;o):
      <ekz>
        la rando <klr>(de folietoj)</klr> estas malforte denta a&ubreve;
        <tld/>ita
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Aegle_marmelos">
          Aegle marmelos</url>, 2014-11-25</lok>
        </fnt>;
      </ekz>
      <ekz>
        sketantoj <klr>[&#x2026;]</klr> no&ccirc;as glacion, kiu iom poste denove fermfrosti&gcirc;as
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fold.0i">foldi</ref>,
      <ref cel="kanel.0i">kaneli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_ib;</ind></trd>,
    <trd>&c_k;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einkerben</trd>,
    <trd>nuten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cocher</trd>,
    <trd>faire une <ind>encoche</ind></trd>
  </trdgrp>
  <trd lng="it">&#xAD;intaccare</trd>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x308C;&#x8FBC;&#x307F;&#x3092;&#x5165;&#x308C;&#x308B;  <pr>&#x304D;&#x308C;&#x3053;&#x307F;&#x3092;&#x3044;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x523B;&#x307F;&#x76EE;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x304D;&#x3056;&#x307F;&#x3081;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>karbowa&cacute;</trd>,
    <trd>nacina&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>grava</trd>,
    <trd>t&abreve;ia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_k;&c_a;&c_r;&c_b;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_u;&c_b;&c_k;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ib;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="nocx.0o">
  <kap><tld/>o</kap>
  <snc mrk="nocx.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="mark.0o.signo">Marko</ref>
      entran&ccirc;ita en ligno a&ubreve; simila materialo:
      <ekz>
        bastono kun unu <tld/>o
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili eltran&ccirc;as <tld/>on en la lignan randon de la boato
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2011/010256.php">Tunelo anstata&ubreve; &hcirc;aoso a&ubreve; &ccirc;u nur &hcirc;aoso?</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="trancx.en0o">entran&ccirc;o</ref>
    <refgrp tip="vid">
      <ref cel="brecx.0eto">bre&ccirc;eto</ref>,
      <ref cel="fold.0o">foldo</ref>,
      <ref cel="kanel.0o.TEK">kanelo<sncref/></ref>,
      <ref cel="sulk.0o.KOMUNE">sulko<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_b;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kerbe</trd>,
    <trd>Nut</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coche<klr> (encoche)</klr></trd>,
    <trd>encoche</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x308C;&#x8FBC;&#x307F;  <pr>&#x304D;&#x308C;&#x3053;&#x307F;</pr></trd>,
    <trd>&#x5207;&#x6B20;&#x304D;  <pr>&#x305B;&#x3064;&#x304B;&#x304D;</pr></trd>,
    <trd>&#x523B;&#x307F;&#x76EE;  <pr>&#x304D;&#x3056;&#x307F;&#x3081;</pr></trd>,
    <trd>&#x30CE;&#x30C3;&#x30C1;</trd>
  </trdgrp>
  <trd lng="pl">karb</trd>
  <trd lng="ro">crest&abreve;tur&abreve;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_r;&c_b;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ib;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_ib;&c_z;</trd>,
    <trd>&c_z;&c_a;&c_z;&c_u;&c_b;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_u;&c_b; (&c_n;&c_a; &c_k;&c_o;&c_l;&c_je;&c_s;&c_ib;)</trd>
  </trdgrp>
  <trd lng="zh">&#x523B;&#x75D5; <pr>k&egrave;h&eacute;n</pr></trd>
</drv>
</art>

