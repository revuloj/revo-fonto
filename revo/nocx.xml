<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nocx.xml,v 1.4 2021/04/15 21:53:54 revo Exp $">
<kap>
  <rad>no&ccirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="nocx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nocx.0i.KOMUNE">
    <dif>
      En<ref tip="super" cel="trancx.0i.KOMUNE">tran&ccirc;i</ref>
      la&ubreve; V-forma fendo:
      <ekz>
        la rando <klr>(de folietoj)</klr> estas malforte denta a&ubreve; 
        <tld/>ita
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Aegle_marmelos">
          Aegle marmelos</url>, 2014-11-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cocher</trd>,
    <trd>faire une <ind>encoche</ind></trd>
  </trdgrp>
  <trd lng="it">&#173;intaccare</trd>
  <trdgrp lng="pl">
    <trd>karbowa&cacute;</trd>,
    <trd>nacina&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>grava</trd>,
    <trd>t&abreve;ia</trd>
  </trdgrp>
</drv>

<drv mrk="nocx.0o">
  <kap><tld/>o</kap>
  <snc mrk="nocx.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="mark.0o.signo">Marko</ref>
      entran&ccirc;ita en ligno a&ubreve; simila materialo:
      <ekz>
        bastono kun unu <tld/>o
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>coche<klr> (encoche)</klr></trd>,
    <trd>encoche</trd>
  </trdgrp>
  <trd lng="pl">karb</trd>
  <trd lng="ro">crest&abreve;tur&abreve;</trd>
</drv>
</art>

