<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jxur.xml,v 1.29 2022/02/13 09:27:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&jcirc;ur</rad>/i
</kap>
<drv mrk="jxur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="promes.0i.certigi">Promesi</ref> 
      a&ubreve; <ref tip="super" cel="atest.0i">atesti</ref> ion je la 
      nomo de iu a&ubreve; io sankta:
      <ekz>
        <tld/>u do al mi per Dio, ke vi ne agos malfidele kun mi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 21:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as per la vivo de Faraono, ke vi foriros el &ccirc;i tie nur tiam, kiam venos
        &ccirc;i tien via plej juna frato
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 42:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        ke mi &ccirc;ion &ccirc;i tion faros kaj plenumos, mi <tld/>as per &ccirc;iuj belaj okuloj de &ccirc;iuj
        miaj diinoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        diru kion vi volas, <tld/>u kiel vi volas, mi tamen ne kredos al vi
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li tradibo&ccirc;as &ccirc;ion, &gcirc;is la lasta &ccirc;emizo, tiel ke sur li restas nur surtuteto kaj
        palteto, vere, mi <tld/>as al vi&jcirc;uras
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi <tld/>as per mia honoro &mdash; mi faros uzon el tiuj rajtoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la morto mi <tld/>as! <klr>[&#8230;]</klr> ni tion devas fari
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
      <ekz>
        per tiu &ccirc;i vira mano mi <tld/>as al vi &ccirc;i tie, ke fidele kaj kura&gcirc;e mi restos
        via hetmano la morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi <tld/>as per mia animo! mi neniam vin forlasos
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>!
      </ekz>
      <ekz>
        apud la mortlito de sia amiko li <tld/>is, ke li dedi&ccirc;os sian tutan vivadon al
        la filo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri nenio oni povas <tld/>i <klr>(esti certa)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i per la barbo de l' profeto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kaj re<tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="invok.0i">invoki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_ja;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_k;&c_l;&c_je;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trd lng="bo">&t_ma;&t_na;&t_-a;&t_tsheg;&t_sa;&t_kata;&t_yata;&t_drengbu;&t_la;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>schw&ouml;ren</trd>,
    <trd>beeiden</trd>
  </trdgrp>
  <trd lng="el">
&omikron;&rho;&kappa;&jota_ton;&zeta;&omikron;&my;&alfa;&jota;</trd>
  <trd lng="en">swear</trd>
  <trd lng="es">jurar</trd>
  <trdgrp lng="fr">
    <trd>jurer<klr tip="ind"> (faire un serment)</klr></trd>,
    <trd>donner sa <ind>parole</ind></trd>,
    <trd>faire <ind>serment</ind></trd>,
    <trd>pr&ecirc;ter <ind>serment</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>esk&uuml;szik</trd>,
    <trd>megesk&uuml;szik</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ber<ind>sumpah</ind></trd>,
    <trd>ber<ind>ikrar</ind></trd>
  </trdgrp>
  <trd lng="it">giurare</trd>
  <trd lng="nl">zweren</trd>
  <trd lng="pt">jurar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_ja;&c_s;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_ja;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="jxur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Atesto a&ubreve; promeso, pri kiu oni <tld/>as:
      <ekz>
        kie <tld/>on vi a&ubreve;das, malbonon suspektu
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tion li rakontis, postulante <tld/>on pri diskreteco;
      </ekz>
      <ekz>
        se la sango bolas, tiam lango la <tld/>ojn ne
        avaras<fnt>Z</fnt>;
      </ekz>
      <ekz>
        rompi sian <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_ja;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_ja;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_t;&c_v;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_tata;&t_shabchu;&t_la;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Schwur</trd>,
    <trd>Eid</trd>
  </trdgrp>
  <trd lng="el">&omikron_ton;&rho;&kappa;&omikron;&sigma_fina;</trd>
  <trd lng="en">oath</trd>
  <trd lng="es">juramento</trd>
  <trdgrp lng="fr">
    <trd>serment</trd>,
    <trd>parole<klr tip="ind"> (serment)</klr></trd>,
    <trd>v&oelig;u<klr tip="ind"> (serment)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>esk&uuml;</trd>,
    <trd>esk&uuml;v&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ikrar</trd>,
    <trd>sumpah</trd>
  </trdgrp>
  <trd lng="it">giuramento</trd>
  <trd lng="nl">eed</trd>
  <trdgrp lng="pt">
    <trd>juramento</trd>,
    <trd>jura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_ja;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_ja;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="jxur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konfirmata per <tld/>o:
      <ekz>
        <tld/>a interkonsento;
      </ekz>
      <ekz>
        <tld/>a promeso.
      </ekz>
    </dif>
  </snc>
  <adm>
    PIV1 donas plian sencon: "promesinta per jxuro, ke li agos juste".
    Por cxi tiu senco PV uzis "jxurinta" (cf ekz "jxurinta oficisto").
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;</trd>
  <trd lng="de">beschworen</trd>
  <trd lng="en">sworn</trd>
  <trd lng="hu">esk&uuml;-</trd>
  <trd lng="id">ter<ind>sumpah</ind></trd>
  <trd lng="nl">be&euml;digd</trd>
  <trd lng="ru">&c_k;&c_l;&c_ja;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="it">giurato<klr> (agg.)</klr></trd>
</drv>

<drv mrk="jxur.je0ikun0i">
  <kap>je<tld/>i, <var><kap>kun<tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Konspiri:
      <ekz>
        je<tld/>i kontra&ubreve; iu<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol; 
<ind>&c_z;&c_m;&c_o;&c_v;&c_u;</ind></trd>
  <trd lng="en">conspire</trd>
  <trd lng="es">conjurar</trd>
  <trd lng="fr">se <ind>conjurer</ind></trd>
  <trd lng="hu">&ouml;sszeesk&uuml;szik</trd>
  <trdgrp lng="id">
    <trd>ber<ind>komplot</ind></trd>,
    <trd>ber<ind>sekongkol</ind></trd>
  </trdgrp>
  <trd lng="nl">samenzweren</trd>
  <trd lng="it">congiurare</trd>
</drv>

<drv mrk="jxur.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ligi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; 
&c_d;&c_a; <ind>&c_p;&c_r;&c_y;&c_s;&c_ja;&c_g;&c_ib;</ind></trd>
  <trdgrp lng="de">
    <trd>vereidigen</trd>,
    <trd>einen <ind>Eid</ind> abnehmen</trd>
  </trdgrp>
  <trd lng="en">swear (in)<klr> (transitiva)</klr></trd>
  <trdgrp lng="fr">
    <trd>faire <ind>jurer</ind></trd>,
    <trd>faire pr&ecirc;ter <ind>serment</ind></trd>,
    <trd>asserment&eacute;</trd>
  </trdgrp>
  <trd lng="hu">megesket</trd>
  <trdgrp lng="id">
    <trd><baz>sumpah</baz> menyumpah</trd>,
    <trd><baz>sumpah</baz> menyumpahi</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_k; 
&c_p;&c_r;&c_i;&c_s;&c_ja;&c_g;&c_je;</trd>
  <trd lng="it">far <ind>giurare</ind></trd>
</drv>

<drv mrk="jxur.0peti">
  <kap><tld/>peti<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Peti per <tld/>oj:
      <ekz>
        &scirc;i <tld/>petis, ke mi lin forpelu.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">petition</trd>
  <trd lng="es">suplicar</trd>
  <trd lng="fr">supplier</trd>
  <trd lng="id"><baz>kemis</baz> mengemis</trd>
  <trd lng="hu">k&ouml;ny&ouml;r&ouml;g</trd>
  <trd lng="nl">bezweren</trd>
  <trdgrp lng="it">
    <trd>supplicare<klr> (scongiurare, chiedere)</klr></trd>,
    <trd>scongiurare<klr> (pregare, chiedere)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="jxur.0rompi">
  <kap><tld/>rompi</kap>
  <snc>
    <dif>
      Ne plenumi sian <tld/>on a&ubreve; fari falsan <tld/>on.
    </dif>
    <trdgrp lng="es">
      <trd>cometer <ind>perjurio</ind></trd>,
      <trd>perjurar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_u;&c_sh;&c_a;&c_c;&c_mol; 
<ind>&c_k;&c_l;&c_ja;&c_t;&c_v;&c_u;</ind></trd>
  <trdgrp lng="de">
    <trd>Schwur brechen</trd>,
    <trd>Eid brechen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>commit <ind>perjury</ind></trd>,
    <trd>perjure<klr> (one's self)</klr></trd>
  </trdgrp>
  <trd lng="fr">se <ind>parjurer</ind></trd>
  <trd lng="hu">esk&uuml;t szeg</trd>
  <trd lng="it">spergiurare</trd>
  <trd lng="nl">meineed plegen</trd>
  <trd lng="pt">perjurar</trd>
</drv>
</art>
