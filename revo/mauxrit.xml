<vortaro>
<art mrk="$Id: mauxrit.xml,v 1.13 2020/06/17 23:10:32 revo Exp $">
<kap>
  <rad>ma&ubreve;ritani</rad>/o
</kap>

<drv mrk="mauxrit.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="mauxrit.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&scirc;tato</ref>
      en la nordokcidenta <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Nuak&scirc;oto;
      teritorio de <em>Islama Respubliko Ma&ubreve;ritanio</em>,
      limanta kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref>,
      Okcidenta Saharo,
      <ref tip="vid" cel="senegal.S0o.GEOG">Senegalo</ref>,
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>:
      <ekz>
        plago de migrantaj akridoj neniigis 4 milionojn da hektaroj en
        <tld lit="M"/>o, Senegalo, Malio, Ni&gcirc;ero kaj &Ccirc;ado
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008814.php">E&ubreve;ropo plialtigas
          sian drataron</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni finance adoptis blindajn infanojn en Kongo, Maroko, <tld
        lit="M"/>o kaj Rumanio
        <fnt>
          <bib>Monato</bib>
          <aut>Jacques Tuinder</aut>
          <vrk><url ref="&Monato;/2011/010356.php">Fonda&jcirc;o E-vid-E-nt-E
          jam 45 jarojn aktivadas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&a_m;&a_w;&a_r;&a_y;&a_t;&a_A;&a_n;&a_y;&a_A;</trd>
    <trd lng="be">&c_M;&c_a;&c_w;&c_r;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_M;&c_a;&c_v;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Maurit&agrave;nia</trd>,
      <trd>Rep&uacute;blica Isl&agrave;mica de <ind>Maurit&agrave;nia</ind></trd>
    </trdgrp>
    <trd lng="de">Mauretanien</trd>
    <trd lng="fa">&a_m;&a_w;&a_r;&f_y;&a_t;&a_A;&a_n;&f_y;</trd>
    <trdgrp lng="fr">
      <trd>Mauritanie</trd>,
      <trd>R&eacute;publique islamique de Mauritanie</trd>
    </trdgrp>
    <trd lng="hu">Maurit&aacute;nia</trd>
    <trd lng="id">Mauritania</trd>
    <trdgrp lng="pl">
      <trd>Mauretania</trd>,
      <trd>Islamska Republika Mauretra&nacute;ska</trd>
    </trdgrp>
    <trd lng="pt">Maurit&acirc;nia</trd>
  </snc>
</drv>

</art>

