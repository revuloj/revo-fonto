<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ma&ubreve;ritani</rad>/o
</kap>

<drv mrk="mauxrit.0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="mauxrit.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordokcidenta Afriko kun &ccirc;efurbo Nuak&scirc;oto.
      Teritorio de <em>Islama Respubliko Ma&ubreve;ritanio</em>.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.0o.GEOG">Al&gcirc;erio</ref>
      <ref tip="vid" cel="mali.0o.GEOG">Malio</ref>,
      Okcidenta Saharo,
      <ref tip="vid" cel="senegal.0o.GEOG">Senegalo</ref>,
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="ar">&mim;&waw;&ra;&ya;&ta;&alif;&nun1;&ya;&alif;</trd>
    <trd lng="bg">&c_M;&c_a;&c_v;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
    <trd lng="de">Mauretanien</trd>
    <trdgrp lng="pl">
      <trd>Mauretania</trd>,
      <trd>Islamska Republika Mauretra&nacute;ska</trd>
    </trdgrp>
  </snc>
</drv>

</art>

