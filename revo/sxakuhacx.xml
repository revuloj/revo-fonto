<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;akuha&ccirc;</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sxakuhacx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Orientazia <ref tip="super"
      cel="instru2.blov0o">blovinstrumento</ref>
      kun milda, densa sono,
      farita el 7-noda, proksimume 55 cm longa bambupeco,
      kun 4 truoj fronte kaj unu dorse,
      rekte tenata, kun beko tegita per bufala korno:
      <ekz>centojn da trupoj mi rigardis, tamen neniam vidis, ke oni akompanas sole per <tld/>o
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 54a, p. 211a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="de">Shakuhachi-Fl&ouml;te</trd>
  <trdgrp lng="fr">
    <trd>shakuhachi</trd>
  </trdgrp>
  <trd lng="hu">sakuhacsi (fuvola)</trd>
  <trd lng="pt">shakuhachi</trd>
</drv>
</art>
