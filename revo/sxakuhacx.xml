<kap>
  <rad>&scirc;akuha&ccirc;</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sxakuhacx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Orientazia <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumento</ref>
      kun milda, densa sono,
      farita el 7-noda, proksimume 55 cm longa bambupeco,
      kun 4 truoj fronte kaj unu dorse,
      rekte tenata, kun beko tegita per bufala korno:
      <ekz>centojn da trupoj mi rigardis, tamen neniam vidis, ke oni
      akompanas
sole per <tld/>o
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 54a, p. 211a</lok></fnt>.
</ekz>
    </dif>
  </snc>
  <trd lng="ca">shakuhachi</trd>
  <trd lng="de">Shakuhachi-Fl&ouml;te</trd>
  <trdgrp lng="fr">
    <trd>shakuhachi</trd>
  </trdgrp>
  <trd lng="hu">sakuhacsi (fuvola)</trd>
  <trd lng="it">shakuhachi</trd>
  <trd lng="pl">shakuhachi</trd>
  <trd lng="pt">shakuhachi</trd>
  <trd lng="uk">&c_s;&#x44F;&#x43A;&c_u;&c_h;&c_a;&c_c;&c_ib; (&#x44F;&#x43F;&#x43E;&#x43D;&c_s;&#x44C;&#x43A;&c_a;
  &c_s;&#x43E;&#x43F;&c_ib;&#x43B;&#x43A;&c_a; &c_z; &c_b;&c_a;&#x43C;&c_b;&c_u;&#x43A;&c_a;)</trd>
  <trd lng="zh">&#x5C3A;&#x516B; <pr>ch&#x1D0;b&#x101;</pr></trd>
</drv>
</art>
