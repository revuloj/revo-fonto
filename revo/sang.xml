<drv mrk="sang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sang.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Likva <ref tip="super" cel="hist.0o">histo</ref> fluanta
      en la <ref cel="angi.0o">angioj</ref>
      (arterioj, vejnoj) de animaloj, ru&gcirc;a &ccirc;e la
      vertebruloj, portanta la nutrajn substancojn
      en la tutan korpon:
      <ekz>
        <tld/>aj gutoj falis sur lian manikon, li forvi&scirc;is ilin, haltigis la <tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu iom el la <tld/>o de la bovido kaj <klr>[&#x2026;]</klr> elver&scirc;u &ccirc;e la bazo de la
        altaro
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; bruas la <tld/>o &ccirc;irka&ubreve; mia orelo, sur kiun vi donis al mi vangfrapon
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        glavo <tld/>on soifanta
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Espero</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la popolo soifas <tld/>on
        <klr>(ribelas)</klr>
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        virina lango bu&ccirc;as sen <tld/>o
        <klr>(sen korpe vundi)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sang.0o.temperamento">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="temper.0o">Temperamento</ref>, <ref tip="dif" cel="pasi.0o">pasio</ref>,
      <ref cel="san.0o">saneco</ref>:
      <ekz>
        ekboloj de senbrida juna <tld/>o
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis juna, sova&gcirc;a kaj varmeg<tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        hispana <tld/>o rulas en &scirc;iaj vejnoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o bolas, juneco petolas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        maldol&ccirc;a por la lango, sed saniga por la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sang.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Familia, hereda <ref cel="ven.de0o">deveno</ref>, <ref cel="parenc.0eco">parenceco</ref>:
      <ekz>
        &scirc;ia infano, &scirc;ia propra karno kaj <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabo prosperis, la nobela <tld/>o elmontri&gcirc;is malgra&ubreve; la malluksa nutrado
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Don Isaak Abarbanel</nom>, nevo de la granda rabeno, devenas el la plej bona sango de
        <nom>Izrael</nom>
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej malgranda guto da montana <tld/>o en la vejnoj de homo rekondukas lin &ccirc;iam
        al la hejma regiono
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        sukcesaj firmaoj ne blinde akceptas siajn stultajn filojn sed kapablajn
        aliulojn, alivorte, enfluas fre&scirc;a, ekstera <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/010932.php"
              >Adapti, persisti, akcepti</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia sam<tld/>a <klr>(vera, samfamilia)</klr> frato
        <fnt>
          <bib>ElektFab</bib>
          <lok>La senditoj de morto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o komuna reciproke sin altiras</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">megsz&oacute;lal a <ind>v&eacute;r</ind> szava</trd>
      </ekz>
    </dif>
  </snc>
