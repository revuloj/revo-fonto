<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sang</rad>/o
</kap>
<drv mrk="sang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sang.0o.ANA" num="1">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Ru&gcirc;a flua&jcirc;o fluanta en la arterioj kaj vejnoj de la vertebruloj kaj
      portanta la nutrajn substancojn en la tutan korpon.
    </dif>
  </snc>
  <snc mrk="sang.0o.FIG" num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Idaro, raso; deveno:
      <ekz>
        la <tld/>o de Davido;
      </ekz>
      <ekz>
        <tld/>o komuna reciproke sin altiras
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sam<tld/>a
        <klr>(samfamilia, samrasa)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">krev</trd>
  <trd lng="de">Blut</trd>
  <trd lng="en">blood</trd>
  <trd lng="es">sangre</trd>
  <trd lng="fr">sang</trd>
  <trd lng="it">sangue</trd>
  <trd lng="nl">bloed</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_v;&c_mol;</trd>
  <trd lng="sv">blod</trd>
</drv>

<drv mrk="sang.0i">
  <kap><tld/>(ad)i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi <tld/>on:
      <ekz>
        <tld/>adi el la nazo.
      </ekz>
    </dif>
    <trd lng="de">bluten</trd>
    <trd lng="fr">saigner<klr> (intr.)</klr></trd>
    <trd lng="sv">bl&ouml;da</trd>
  </snc>
  <snc>
    <dif>
      Esti ru&gcirc;a kiel <tld/>o:
      <ekz>
        &ccirc;e la horizonto la suno
        <tld/>adis sur la maro.
      </ekz>
    </dif>
    <trd lng="fr">rougoyer</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kruele suferi kvaza&ubreve; pro <tld/>adanta vundo:
      <ekz>
        pro tio <tld/>as mia koro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">saigner<klr> (intr., f.)</klr></trd>
  <trd lng="nl">bloeden</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_v;&c_o;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol; <ind>&c_k;&c_r;&c_o;&c_v;&c_mol;&c_ju;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sang.0eltiri">
  <kap><tld/>eltiri</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Kuraci per eltiro de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">saigner<klr> (tr., med.)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_k;&c_r;&c_o;&c_v;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="sang.malvarm0eco">
  <kap>malvarm<tld/>o, trankvil<tld/>(ec)o</kap>
  <snc>
    <dif>
      Malkonsterneco kaj memregeco.
    </dif>
    <refgrp tip="sin">
      <ref cel="flegm.0o">flegmo</ref>,
      <ref cel="emoci">malemocio</ref>,
      <ref cel="ekvili.0o">ekvilibro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>flegme</trd>,
    <trd>self-control</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_l;&c_a;&c_d;&c_n;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_je;</trd>
</drv>

<drv mrk="sang.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      Igi sen<tld/>a.
    </dif>
    <trd lng="fr">saigner<klr> (tr.)</klr></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute senfortigi:
      <ekz>
        la milito sen<tld/>igis la popolon.
      </ekz>
    </dif>
    <trd lng="fr">saigner &agrave; blanc</trd>
  </snc>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_s;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
