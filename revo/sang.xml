<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sang</rad>/o
</kap>
<drv mrk="sang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sang.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ru&gcirc;a flua&jcirc;o fluanta en la arterioj kaj vejnoj de la vertebruloj kaj
      portanta la nutrajn substancojn en la tutan korpon.
    </dif>
  </snc>
  <snc mrk="sang.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Idaro, rasa deveno:
      <ekz>
        la <tld/>o de Davido;
      </ekz>
      <ekz>
        <tld/>o komuna reciproke sin altiras
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sam<tld/>a
        <klr>(samfamilia, samrasa)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_w;</trd>
  <trd lng="cs">krev</trd>
  <trd lng="de">Blut</trd>
  <trd lng="en">blood</trd>
  <trd lng="es">sangre</trd>
  <trd lng="fr">sang</trd>
  <trd lng="hu">v&eacute;r</trd>
  <trd lng="it">sangue</trd>
  <trd lng="nl">bloed</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_v;&c_mol;</trd>
  <trd lng="sk">krv</trd>
  <trd lng="sv">blod</trd>
</drv>

<drv mrk="sang.0adi">
  <kap><tld/>adi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sang.0adi.FIZL">
    <dif>
      Ellasi <tld/>on:
      <ekz>
        <tld/>adi el la nazo.
      </ekz>
    </dif>
    <trd lng="de">bluten</trd>
    <trd lng="fr">saigner<klr> (intr.)</klr></trd>
    <trd lng="sv">bl&ouml;da</trd>
  </snc>
  <snc mrk="sang.0adi.rugxi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti ru&gcirc;a kiel <tld/>o:
      <ekz>
        &ccirc;e la horizonto la suno <tld/>adis sur la maro.
      </ekz>
    </dif>
    <trd lng="fr">rougeoyer</trd>
  </snc>
  <snc mrk="sang.0adi.suferi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kruele <ref tip="super" cel="sufer.0i">suferi</ref> kvaza&ubreve; pro
      <tld/>adanta vundo:
      <ekz>
        pro tio <tld/>as mia koro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">saigner<klr> (intr., fig.)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_o;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trd lng="hu">v&eacute;rzik</trd>
  <trd lng="nl">bloeden</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_v;&c_o;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol; <ind>&c_k;&c_r;&c_o;&c_v;&c_mol;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="sk">krv&aacute;ca&tcaron;</trd>
</drv>

<drv mrk="sang.0eltiri">
  <kap><tld/>eltiri</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kuraci per eltiro de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_k;&c_r;&c_y;&c_v;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  <trdgrp lng="fr">
    <trd>faire une <ind>saign&eacute;e</ind></trd>,
    <trd>saigner<klr> (faire une saign&eacute;e)</klr></trd>
  </trdgrp>
  <trd lng="hu">v&eacute;rt vesz</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_k;&c_r;&c_o;&c_v;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="sang.malvarm0eco">
  <kap>malvarm<tld/>o, trankvil<tld/>eco</kap>
  <snc>
    <dif>
      <ref tip="super" cel="trankv.0o.anima">Trankvilo</ref> de iu
      senkonsterna kaj memrega.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">sang-froid</trd>
  <trd lng="hu">hidegv&eacute;r</trd>
  <trd lng="ru">&c_h;&c_l;&c_a;&c_d;&c_n;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_je;</trd>
  <trd lng="sk">chladnokrvnos&tcaron;</trd>
</drv>

<drv mrk="sang.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="sang.sen0igi.FIZL">
    <dif>
      Igi sen<tld/>a.
    </dif>
  </snc>
  <snc mrk="sang.sen0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute senfortigi:
      <ekz>
        la milito sen<tld/>igis la popolon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_ja;&c_s;&c_k;&c_r;&c_o;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="fr">saigner &agrave; blanc</trd>
  <trd lng="hu">kiv&eacute;reztet</trd>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_s;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
