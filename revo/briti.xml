<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: briti.xml,v 1.26 2023/10/29 19:11:03 revo Exp $">
<kap><rad>Briti</rad>/o</kap>
<drv mrk="briti.B0o">
  <kap><tld/>o<fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Britio">Britio</url></lok></fnt></kap>
  <snc mrk="briti.B0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="brit.B0ujo">Britujo</ref>:
      <ekz>
        por la Esperanto-movado en <tld/>o mi komencis
        fari sekretarian laboron
        <fnt><aut>Will GREEN</aut>:
        <vrk>Telefona labirinto</vrk>, <bib>Monato</bib>,
        <lok>n-ro 01/1994</lok></fnt>;
      </ekz>
      <ekz>
        en <tld/>o nun oni apena&ubreve; povas viziti butikan
        kvartalon sen renkonti&gcirc;o kun tiuj prabestoj
        <fnt><aut>Marjorie BOULTON</aut>:
        <vrk>Dinomanio</vrk>, <bib>Monato</bib>,
        <lok>n-ro 02/1994</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_b;&c_r;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;</trd>,
      <trd>&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja;
      <ind>&c_B;&c_r;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;</ind></trd>
    </trdgrp>
    <trd lng="bg">&c_V;&c_je;&c_l;&c_i;&c_k;&c_o;&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
    <trd lng="de">Gro&szlig;britannien<klr tip="ind">
    (Staat)</klr></trd>
    <trd lng="id">Britania Raya</trd>
    <trd lng="nl">Groot-Brittanni&euml;</trd>
    <trd lng="pl">Wielka Brytania</trd>
    <trdgrp lng="zh">
    <trd>&#x5927;&#x4E0D;&#x5217;&#x985A; <pr>d&#xE0;b&#xF9;li&#xE8;di&#x101;n</pr></trd>,
    <trd>&#x82F1; <pr>y&#x12B;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="briti.B0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda <ref tip="lst" cel="insul.0o" lst="voko:insuloj">insulo</ref> de la
      <ref tip="malprt" cel="brit.B0aInsularo">Brita Insularo</ref>:
      <ekz>
        la kornvala kaj la kimra lingvoj komencis disevolui
        &ccirc;irka&ubreve; 600,
        post kiam la angloj, movi&gcirc;ante okcidenten, kaptis
        la urbon Gloucester kaj tiel dividis la keltlingvanojn de
        sudokcidenta <tld/>o amba&ubreve;flanke de la Bristola Golfo.
        <fnt>
        <aut>Edmund Grimley EVANS</aut>:
        <vrk>La kornvala</vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 01/1998 p. 23</lok>
        </fnt>
      </ekz>
    </dif>
    <url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/">AdE:
         Listo de Rekomendataj Landnomoj
    </url>
    <trd lng="be">&c_B;&c_r;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;<klr tip="amb"> (&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;)</klr></trd>
    <trd lng="bg">&c_V;&c_je;&c_l;&c_i;&c_k;&c_o;&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
    <trd lng="de">Gro&szlig;britannien<klr tip="ind">
    (Insel)</klr></trd>
    <trd lng="nl">Brittanni&euml;</trd>
    <trd lng="pl">Wielka Brytania</trd>
  </snc>
  <trd lng="cs">Brit&aacute;nie <klr>(Velk&aacute;)</klr></trd>
  <trd lng="es">Gran Breta&ntilde;a</trd>
  <trd lng="fr">Grande Bretagne</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D9;&#x5D8;&#x5E0;&#x5D9;&#x5D4; &#x5D4;&#x5D2;&#x5D3;&#x5D5;&#x5DC;&#x5D4;</trd>
  <trd lng="id"><ind>Britania</ind> Raya</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30AE;&#x30EA;&#x30B9;  <pr>&#x3044;&#x304E;&#x308A;&#x3059;</pr></trd>,
    <trd>&#x82F1;&#x56FD;  <pr>&#x3048;&#x3044;&#x3053;&#x304F;</pr></trd>,
    <trd>&#x30D6;&#x30EA;&#x30C6;&#x30F3;&#x5CF6;  <pr>&#x30D6;&#x30EA;&#x30C6;&#x30F3;&#x3057;&#x307E;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_V;&c_je;&c_l;&c_i;&c_k;&c_o;&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>Ve&lcaron;k&aacute; <ind>Brit&aacute;nia</ind></trd>,
    <trd>Ve&lcaron;k&aacute; Brit&aacute;nia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x411;&#x440;&#x438;&#x442;&#x430;&#x43D;&#x456;&#x44F;</trd>,
    <trd>&#x412;&#x435;&#x43B;&#x438;&#x43A;&#x430; &#x411;&#x440;&#x438;&#x442;&#x430;&#x43D;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
