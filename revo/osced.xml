<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: osced.xml,v 1.31 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>osced</rad>/i
</kap>
<drv mrk="osced.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malfermi la bu&scirc;on per spasma movo de ekspiro, sekvita de longa
      elspiro, ka&ubreve;zita de <ref cel="dorm.0ema">dormemo</ref>, 
      malsato, <ref cel="lac.0igxi">laci&gcirc;o</ref> a&ubreve; <ref cel="enu.0o">enuo</ref>:
      <ekz>
        la kato <klr>[&#x2026;]</klr> veki&gcirc;is kaj <tld/>is post la dol&ccirc;a dormo
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo <tld/>is kaj &ccirc;irka&ubreve;rigardante per enuaj okuloj, li diris al si
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la dezerto <tld/>is la hienoj kaj &scirc;akaloj, kaj la dormetanta leono stre&ccirc;is
        siajn potencajn membrojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> eklegis dek liniojn kaj lace jam <tld/>is
        <fnt>
          <bib>LSB</bib>
          <lok>Sibilla Orakolas</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj enuigaj deklinacioj kaj konjugacioj, super kiuj 
        <klr>[&#x2026;]</klr> &ccirc;iutage <tld/>as miloj da infanoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="osced.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Montri vastan malferma&jcirc;on, malplenon; 
      <ref tip="sin" cel="fauxk.0i">fa&ubreve;ki</ref>:
      <ekz>
        la granda stacio <tld/>is pro la malpleno
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mizeraj garbejoj <klr>[&#x2026;]</klr> kun <tld/>antaj
        <klr>(malfermitaj)</klr>
        pordegoj
        <fnt>
          <bib>PatrojFiloj</bib> <lok>&ccirc;apitro 3a, p.
          14a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>anta abismo</ind> inter havuloj kaj senhavuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011468.php">Unu regulo por ri&ccirc;uloj, dua regulo por malri&ccirc;uloj</url></vrk>
          <lok>2015</lok>
        </fnt>.
        <trd lng="de">g&auml;hnender Abgrund</trd>
        <trd lng="hu">t&aacute;tong&oacute; m&eacute;lys&eacute;g</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_ja;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_je;&c_w;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_z;&c_ja;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trd lng="br">bazailhat</trd>
  <trd lng="ca">badallar</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;en. zet <klr>(d&iacute;ra)</klr></trd>,
    <trd>z&iacute;vat</trd>
  </trdgrp>
  <trd lng="de">g&auml;hnen</trd>
  <trd lng="el">&chi;&alfa;&sigma;&my;&omikron;&ypsilon;&rho;&jota;&epsilon_ton;&my;&alfa;&jota;</trd>
  <trd lng="en">yawn</trd>
  <trd lng="es">bostezar</trd>
  <trd lng="fa">&a_kh;&a_m;&f_y;&a_A;&a_z;&a_h; &f_k;&a_sh;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">b&acirc;iller</trd>
  <trd lng="hu">&aacute;s&iacute;t</trd>
  <trd lng="id"><baz>kuap</baz>menguap<ind> (mengantuk)</ind></trd>
  <trd lng="ja">&#x3042;&#x304F;&#x3073;&#x3092;&#x3059;&#x308B;</trd>
  <trd lng="nl">geeuwen</trd>
  <trd lng="pl">ziewa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>bocejar</trd>,
    <trd>oscitar</trd>
    <trd>abrir a <klr tip="amb">boca</klr></trd>    
  </trdgrp>
  <trd lng="ro">c&abreve;sca</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>;
    <trd>&c_z;&c_i;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">z&iacute;va&tcaron;</trd>
  <trd lng="sv">g&auml;spa</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_z;&c_ib;&c_h;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_ja;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F20;&#x53E3; <pr>zh&#x101;ngk&#x1D2;u</pr></trd>,
    <trd>&#x5F35;&#x53E3; <pr>zh&#x101;ngk&#x1D2;u</pr></trd>,
    <trd>&#x6253;&#x54C8;&#x6B20; <pr>d&#x1CE;h&#x101;qian</pr></trd>,
    <trd>&#x547F; <pr>q&#x16B;</pr></trd>,
    <trd>&#x6253;&#x5475;&#x6B20; <pr>d&#x1CE;h&#x113;qi&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
