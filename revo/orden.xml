<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orden.xml,v 1.34 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>orden</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="orden.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="orden.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      <ref tip="super" cel="societ.0o.institucio">Societo</ref> el kavaliroj 
      a&ubreve; religiuloj, kiuj vivas kune la&ubreve; apartaj
      reguloj, liginte sin per votoj:
      <ekz>
        la <ind>jezuita <tld/>o</ind>
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
        <trd lng="de">Jesuitenorden</trd>
      </ekz>
      <ekz>
        la <ind><tld/>o de Kruckavaliroj</ind>
        <fnt>
          <bib>SrT</bib>
          <lok>Anta&ubreve;parolo.</lok>
        </fnt>;
        <trd lng="de">Kreuzritterorden</trd>
      </ekz>
      <ekz>
        misiistoj de la <tld/>o <nom>Sacr&eacute; C&oelig;ur</nom>
        <fnt>
          <aut>Rudiger Hauger</aut>
            <vrk><url ref="&Monato;/2001/007315.php">De&ccirc;ifrado de la paskinsula skribo</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 23</lok>
          </fnt>;
      </ekz>
      <ekz>
        la konstitucio de la <tld/>o de la maltaj kavaliroj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj kapablas tuj montri Malton surmape kaj diri ion pri la <tld lit="O"/>o de
        Sankta Johano
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Sankta Se&gcirc;o, krome, diplomatie rilatas kun <klr>[&#x2026;]</klr> la Suverena Milita
        <ind><tld lit="O"/>o de Malto</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2009/009739.php">Vatikano diplomatie rilatas kun 177 landoj</url></vrk>
          <lok>2009</lok>
        </fnt>;
        <trd lng="de">Malteserorden</trd>
      </ekz>
      <ekz>
        mona&hcirc;ejo de la Nudpieda <tld lit="O"/>o de sankta <nom>Feardia Anticoncupiensis</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2013/010838.php">Bernardo la Simpligisto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kavaliro de la <tld lit="O"/>o de <nom>Oranje Nassau</nom>
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kongre.0o.ordeno">kongregacio<sncref/></ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>congr&eacute;gation religieuse</trd>,
      <trd>ordre religieux</trd>
    </trdgrp>
    <trd lng="hu">szerzetesrend</trd>
    <trdgrp lng="nl">
      <trd>orde</trd>,
      <trd>kloosterorde</trd>
    </trdgrp>
  </snc>
  <snc mrk="orden.0o.honora">
    <dif>
      Honoriga socieco
      starigita de suvereno por rekompenci meriton:
      <ekz>
        <tld/>o de Nigra Aglo;
      </ekz>
      <ekz>
        <tld lit="O"/>o de la &Gcirc;artero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=201509">Ordeno de la &Gcirc;artero</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> haltis por miope kaj esplore rigardi kelkajn tre ebriajn
        komandorojn de la Vasa <tld/>o
        <fnt>
          <bib>Kiso</bib>
          <lok>Poetoj Kaj Homoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Stiftung</trd>
    <trdgrp lng="fr">
      <trd>ordre<klr> (civil, militaire)</klr></trd>
    </trdgrp>
    <trd lng="hu">rend<klr tip="ind"> (pl. becs&uuml;let-rend)</klr></trd>
    <trdgrp lng="nl">
      <trd>orde</trd>,
      <trd>ridderorde</trd>
    </trdgrp>
  </snc>
  <snc mrk="orden.0o.insigno">
    <dif>
      Distinga <ref tip="super" cel="insign.0o.signo">insigno</ref> 
      &scirc;tata, militista a&ubreve; religia, kutime kun formo de stelo a&ubreve; kruco pendanta de rubando;
      <ref tip="sin" cel="dekora.0o.ordeninsigno">dekoracio<sncref/></ref>:
      <ekz>
        juna soldato kun kelkaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna konsilano, kiu pro persista silentado ricevis tri <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Konkurso de saltado</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne brilas luksaj vestoj kaj multego da imponantaj <tld/>oj en nia salono
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi estas rekomendita por la <tld/>o de Vladimiro de kvara grado
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz> 
      <ekz>
        kian sencon havus la <tld/>oj, se sur &ccirc;iuj brustoj pendus la samaj
        <fnt>
          <bib>VaK</bib>
          <lok>Kvina &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blua rubando de la Krurzona <tld lit="O"/>o
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>badge</trd>,
      <trd>decoration</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;coration<klr> (insigne)</klr></trd>,
      <trd>insigne<klr> (d&eacute;coration)</klr></trd>
    </trdgrp>
    <trd lng="hu">rendjel</trd>
    <trdgrp lng="zh">
    <trd>&#x52CB;&#x7AE0; <pr>x&#x16B;nzh&#x101;ng</pr></trd>,
    <trd>&#x52DB;&#x7AE0; <pr>x&#x16B;nzh&#x101;ng</pr></trd>,
    <trd>&#x5FBD; <pr>hu&#x12B;</pr></trd>,
    <trd>&#x5956;&#x7AE0; <pr>ji&#x1CE;ngzh&#x101;ng</pr></trd>,
    <trd>&#x734E;&#x7AE0; <pr>ji&#x1CE;ngzh&#x101;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_o;&c_r;&c_d;&c_e;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;bo&zcaron;. &rcaron;&aacute;d</trd>,
    <trd>&rcaron;ehole</trd>
  </trdgrp>
  <trd lng="de">Orden</trd>
  <trdgrp lng="en">
    <trd>order</trd>,
    <trd>fraternity</trd>
  </trdgrp>
  <trd lng="es">orden militar o religiosa<klr> (grupo de personas)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x52F2;&#x7AE0;  <pr>&#x304F;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x52F2;&#x4F4D;  <pr>&#x304F;&#x3093;&#x3044;</pr></trd>,
    <trd>&#x9A0E;&#x58EB;&#x56E3;  <pr>&#x304D;&#x3057;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x4FEE;&#x9053;&#x4F1A;  <pr>&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x304B;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zakon</trd>,
    <trd>bractwo</trd>,
    <trd>emblemat</trd>,
    <trd>order</trd>
  </trdgrp> 
  <trd lng="pt">ordem</trd>
  <trdgrp lng="ro">
    <trd>ordin</trd>,
    <trd>fraternitate</trd>,
    <trd>emblem&abreve;</trd>
  </trdgrp> 
  <trd lng="ru">&c_o;&c_r;&c_d;&c_je;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>rad</trd>,
    <trd>vyznamenanie</trd>
  </trdgrp>
  <trd lng="sv">orden</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_r;&c_d;&c_je;&c_n;</trd>,
    <trd>&c_o;&c_r;&c_d;&c_je;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5144;&#x5F1F;&#x4F1A; <pr>xi&#x14D;ngd&#xEC;hu&#xEC;</pr></trd>,
    <trd>&#x5144;&#x5F1F;&#x6703; <pr>xi&#x14D;ngd&#xEC;hu&#xEC;</pr></trd>,
    <trd>&#x7AE0; <pr>zh&#x101;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orden.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="orden.0i.atribui">
    <dif>
      Atribui <tld/>on, <ref tip="super" cel="honor.0i.igi">honori</ref> iun per <tld/>o:
      <ekz>
        dum la kongreso &scirc;tata prezidento <tld/>is medalojn de amikeco
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 10 (60)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la simplaj kaj sufi&ccirc;e senpretendaj verkoj <klr>[ne]</klr> permesas sincere
        <tld/>i lin per tiu honora titolo
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/008063.html">50-jara stabileco en verkado - &ccirc;u klasika?</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distin.0igi">distingigi</ref>,
      <ref cel="premi.0i">premii</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <ind>&c_o;&c_r;&c_d;&c_e;&c_n;&c_a;&c_m;</ind>
  </trd>
  <trdgrp lng="cs">
    <trd>ud&ecaron;lit &rcaron;&aacute;d</trd>,
    <trd>vyznamenat &rcaron;&aacute;dem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>(jmdm.) einen </klr>Orden <ind>verleihen</ind></trd>,
    <trd>auszeichnen</trd>
  </trdgrp>
  <trd lng="en">decorate</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;corer<klr> (remettre une d&eacute;coration)</klr></trd>,
    <trd>m&eacute;dailler</trd>
  </trdgrp>
  <trd lng="hu">kit&uuml;ntet<klr> (rendjellel)</klr></trd>
  <trd lng="ja">&#x53D9;&#x52F2;&#x3059;&#x308B;  <pr>&#x3058;&#x3087;&#x304F;&#x3093;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">ridderen</trd>
  <trdgrp lng="pl">
    <trd>odznaczy&cacute; orderem</trd>,
    <trd>dekorowa&cacute; orderem</trd>,
    <trd>nadawa&cacute; order</trd>
  </trdgrp> 
  <trd lng="pt">ordenar</trd>
  <trdgrp lng="ro">
    <trd>decorat cu o comand&abreve;</trd>,
    <trd>s&abreve; acorde o comand&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol; &c_o;&c_r;&c_d;&c_je;&c_n;&c_o;&c_m;</trd>
  <trd lng="sk">vyznamena&tcaron; radom</trd>
  <trd lng="uk">&c_n;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_t;&c_i; &c_o;&c_r;&c_d;&c_je;&c_n;&c_o;&c_m;</trd>
</drv>
</art>
