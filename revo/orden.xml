<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>orden</rad>/o
</kap>
<drv mrk="orden.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="orden.0o.honora">
    <dif>
      Honoriga <ref tip="super" cel="societ.0o.institucio">societo</ref>
      starigita de regnestro por rekompenci meriton de homoj elstare
      helpintaj al la regno:
      <ekz>
        <tld/>o de Nigra Aglo, de &Scirc;trumpligilo, de Kristo, de Lenino
        ktp.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>ordre<klr> (civil, militaire)</klr></trd>
    </trdgrp>
    <trd lng="hu">rend<klr tip="ind"> (pl. becs&uuml;let-rend)</klr></trd>
    <trdgrp lng="nl">
      <trd>orde</trd>,
      <trd>ridderorde</trd>
    </trdgrp>
  </snc>
  <snc mrk="orden.0o.KAT">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Societo el inoj a&ubreve; viroj, kiuj vivas kune la&ubreve; apartaj
      reguloj, liginte sin per votoj:
      <ekz>
        <tld/>o de Benediktanoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kongre.0o">kongregacio</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>congr&eacute;gation religieuse</trd>,
      <trd>ordre religieux</trd>
    </trdgrp>
    <trd lng="hu">szerzetesrend</trd>
    <trdgrp lng="nl">
      <trd>orde</trd>,
      <trd>kloosterorde</trd>
    </trdgrp>
  </snc>
  <snc mrk="orden.0o.insigno">
    <dif>
      Ordena <ref tip="super" cel="insign.0o.signo">insigno</ref> kutime
      kun formo de stelo a&ubreve; kruco pendanta de rubando.
    </dif>
    <trdgrp lng="en">
      <trd>badge</trd>,
      <trd>decoration</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;coration<klr> (insigne)</klr></trd>,
      <trd>insigne<klr> (d&eacute;coration)</klr></trd>
    </trdgrp>
    <trd lng="hu">rendjel</trd>
    <refgrp tip="sin">
      <ref cel="dekora.0o.ordeninsigno">dekoracio <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_o;&c_r;&c_d;&c_e;&c_n;</trd>
  <trd lng="de">Orden</trd>
  <trdgrp lng="en">
    <trd>order</trd>,
    <trd>fraternity</trd>
  </trdgrp>
  <trd lng="es">orden militar o religiosa<klr> (grupo de
  personas)</klr></trd>
  <trd lng="pt">ordem</trd>
  <trd lng="ru">&c_o;&c_r;&c_d;&c_je;&c_n;</trd>
  <trd lng="sv">orden</trd>
</drv>

<drv mrk="orden.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Honori iun per <tld/>o:
      <ekz>
        la Franca ministro pri la publika instruado <tld/>is nian
        majstron
        per la Honora Legio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;
    <ind>&c_o;&c_r;&c_d;&c_e;&c_n;&c_a;&c_m;</ind>
  </trd>
  <trd lng="de"><klr>(jmdm.) einen </klr>Orden
  <ind>verleihen</ind></trd>
  <trd lng="en">decorate</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;corer<klr> (remettre une d&eacute;coration)</klr></trd>,
    <trd>m&eacute;dailler</trd>
  </trdgrp>
  <trd lng="hu">kit&uuml;ntet<klr> (rendjellel)</klr></trd>
  <trd lng="nl">ridderen</trd>
  <trd lng="pt">ordenar</trd>
  <trd lng="ru">&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol;
&c_o;&c_r;&c_d;&c_je;&c_n;&c_o;&c_m;</trd>
</drv>
</art>
