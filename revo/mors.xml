<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Mors</rad>/o
</kap>
<drv mrk="mors.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Anglalingve: Samuel <trd lng="en">Morse</trd>, 1791-1872.
      Usona artisto kaj inventisto.
    </dif>
  </snc>
  <trd lng="fr">Morse</trd>
  <trd lng="ru">&c_M;&c_o;&c_r;&c_z;&c_je;</trd>
</drv>

<drv mrk="mors.m0a">
  <kap><tld lit="m"/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ref cel="kod.morsa0o"><tld lit="m"/>a kodo</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">de <ind>Morse</ind></trd>
</drv>

<drv mrk="mors.m0i">
  <kap><tld lit="m"/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sendi per <ref cel="kod.morsa0o"><tld lit="m"/>a kodo</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>envoyer en <ind>morse</ind></trd>,
    <trd>manipuler<klr tip="ind"> (envoyer du morse)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_t;&c_u;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol; <ind>&c_m;&c_o;&c_r;&c_z;&c_ja;&c_n;&c_k;&c_o;&c_j;</ind></trd>
</drv>

</art>

