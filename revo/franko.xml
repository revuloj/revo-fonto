<kap><rad>frankolin</rad>/o</kap>
<drv mrk="franko.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Francolinus</trd>), simila al la perdriko:
      <ekz>
        <tld/>oj estas surteremaj (kvankam ne neflugaj) birdoj kiuj man&gcirc;as insektojn,
        vegetalan materialon kaj semojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17699">Frankolino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/88/Francolinus_leucoscepus1.jpg/640px-Francolinus_leucoscepus1.jpg"
    prm="PD"><tld/>oj (Francolinus leucoscepus)
        <fnt>
          <aut>Gary M. Stolz</aut>
          <lok><url
          ref="&WCW;/File:Francolinus_leucoscepus1.jpg">&WCW;/File:Francolinus_leucoscepus1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_r;&c_a;&c_n;&c_k;&c_a;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_t;&c_u;&c_r;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">francol&iacute;</trd>
  <trdgrp lng="cs">
    <trd>frankolin</trd>,
    <trd>frankol&iacute;n</trd>
  </trdgrp>
  <trd lng="de">Frankolin</trd>
  <trd lng="en">francolin</trd>
  <trd lng="es">francol&iacute;n</trd>
  <trd lng="fr">francolin</trd>
  <trd lng="hu">frankolin</trd>
  <trd lng="ja">&#x30B7;&#x30E3;&#x30B3;</trd>
  <trdgrp lng="nl">
    <trd>frankolijn</trd>,
    <trd>frankolien</trd>
  </trdgrp>
  <trd lng="pl">frankolin</trd>
  <trdgrp lng="pt">
    <trd>francolino</trd>,
    <trd>francolim</trd>,
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_a;&c_ch;</trd>
  <trd lng="sk">frankol&iacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_a;&#x43D;&#x43A;&#x43E;&#x43B;&c_ib;&#x43D;</trd>,
    <trd>&c_t;&c_u;&c_r;&c_a;&c_ch;</trd>
  </trdgrp>
</drv>
</art>
