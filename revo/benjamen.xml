<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>    
  <rad>Benjamen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="benjamen.0o">
  <kap><tld/>o, <var><kap><tld/></kap></var></kap>
  <snc mrk="benjamen.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo hebredevena:
      <ekz>
        la surskribo sur la bareloj estis farita de armilisto <tld/>o 
        Ellian en Pondi&ccirc;ero
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url
          ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="pl">Beniamin</trd>
  </snc>
  <snc mrk="benjamen.0o.PERS">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Plej juna filo de <ref tip="vid" cel="jakob.0o.BIB">Jakobo</ref>:
      <ekz>
        li &jcirc;etis sin sur la kolon de sia frato <tld/> kaj ploris,
        kaj 
        <tld/> ploris sur lia kolo
        <fnt><bib>MT</bib>, <lok>&amp;Gen 45:14</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="pl">Beniamin</trd>
  </snc>
  <trd lng="de">Benjamin</trd>
  <trd lng="fr">Benjamin</trd>
  <trd lng="it">Beniamino</trd>
</drv>
</art>

