<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: benjamen.xml,v 1.11 2023/10/29 19:11:01 revo Exp $">
<kap>    
  <rad>Benjamen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="benjamen.0o">
  <kap><tld/>o, <var><kap><tld/></kap></var></kap>
  <snc mrk="benjamen.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        la surskribo sur la bareloj estis farita de armilisto <tld/>o 
        Ellian en Pondi&ccirc;ero
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="pl">Beniamin</trd>
    <trd lng="zh">&#x4FBF;&#x96C5;&#x60AF; <pr>bi&#xE0;ny&#x1CE;m&#x1D0;n</pr></trd>
</snc>
  <snc mrk="benjamen.0o.PERS">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Plej juna filo de <ref tip="vid" cel="jakob.0o.BIB">Jakobo</ref>:
      <ekz>
        li &jcirc;etis sin sur la kolon de sia frato <tld/> kaj ploris,
        kaj 
        <tld/> ploris sur lia kolo
        <fnt><bib>MT</bib>, <lok>&Gen; 45:14</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="pl">Beniamin</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_e;&c_n;&c_mol;&c_ja;&c_m;&c_ib;&c_n;</trd>,
    <trd>&c_B;&c_e;&c_n;&c_d;&c_zh;&c_a;&c_m;&c_ib;&c_n;</trd>,
    <trd>&c_V;&c_e;&c_n;&c_ib;&c_ja;&c_m;&c_ib;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Benjamin</trd>
  <trd lng="es">Benjam&iacute;n</trd>
  <trd lng="fr">Benjamin</trd>
  <trd lng="it">Beniamino</trd>
  <trd lng="ca">Benjam&iacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_V;&c_je;&#x43D;&c_ib;&c_a;&#x43C;&c_ib;&#x43D;</trd>,
    <trd>&c_V;&c_je;&#x43D;&c_ib;&#x44F;&#x43C;&c_i;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

