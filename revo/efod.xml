<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efod.xml,v 1.8 2023/08/31 11:49:26 revo Exp $">
<kap>
  <rad>efod</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="efod.0o">
  <kap><tld/>o</kap>
  <snc mrk="efod.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o.TEKS">Vesto</ref> de antikvaj judaj
      &ccirc;efpastroj:
      <ekz>
        metu sur Aaronon la <ref tip="prt" cel="hxiton.0o">&hcirc;itono</ref>n kaj 
        la <ref tip="prt" cel="tunik.0o">tuniko</ref>n de la <tld/>o kaj la <tld/>on kaj la
        surbrusta&jcirc;on, kaj zonu lin per la zono de la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> anka&ubreve; havis sur si linan <tld/>on
        <fnt><bib>MT</bib>, <lok>&Kr1; 15:27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_f;&c_o;&c_d;</trd>
  <trd lng="bg">&c_je;&c_f;&c_o;&c_d;</trd>
  <trd lng="cs">efod</trd>
  <trd lng="de">Efod</trd>
  <trd lng="en">ephod</trd>
  <trd lng="es">efod</trd>
  <trd lng="fr">&eacute;phod</trd>
  <trd lng="ja">&#x30A8;&#x30DD;&#x30C7;</trd>
  <trd lng="nl">efod</trd>
  <trd lng="pl">efod</trd>
  <trd lng="sk">ef&oacute;d</trd>
  <trd lng="uk">&c_je;&c_f;&#x43E;&c_d;</trd>
</drv>
</art>

