<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kugl.xml,v 1.27 2023/12/03 09:59:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kugl</rad>/o
</kap>
<drv mrk="kugl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kugl.0o.ARM">
    <dif>
      Malgranda peco de malmola metalo kun proksimume konusa formo, kiun
      oni el&jcirc;etas per &ccirc;iaj porteblaj <ref cel="paf.0ilo.fusilo">pafilo</ref>j:
      <ekz>
        pafu, mi ne timas viajn <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; la <tld/>oj zorge lin evitas
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi petas rigardi, kiajn <tld/>ojn li fandas
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        la soldatoj dum kelke da minutoj super&scirc;utis ilin per plumbaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> siblis &ccirc;irka&ubreve; mi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o frakasis al li la dekstran manon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        frapita en &scirc;ultro per malgrandkalibra <tld/>o, kredeble per <tld/>o de revolvero
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o trafis lin en la kruron
        <fnt><aut>Wim M. A. De Smet</aut>:
        <vrk>Anta&ubreve; cent jaroj</vrk>, <bib>Monato</bib><lok>jaro 1998a,
        numero 5a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="kartoc.0o.MIL">karto&ccirc;o</ref>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>koule</trd>,
    <trd>kule</trd>,
    <trd>kulka</trd>,
    <trd>st&rcaron;ela</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kugel</trd>,
    <trd>Gewehrkugel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bullet</trd>,
    <trd>shot</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bala<klr> (de arma de fuego)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balle<klr> (d'arme &agrave; feu)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&ouml;ved&eacute;k</trd>,
    <trd>goly&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>pelor</trd>,
    <trd>peluru</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F3E;&#x4E38;  <pr>&#x3060;&#x3093;&#x304C;&#x3093;</pr></trd>,
    <trd>&#x305F;&#x307E;</trd>,
    <trd>&#x5C0F;&#x9283;&#x5F3E;  <pr>&#x3057;&#x3087;&#x3046;&#x3058;&#x3085;&#x3046;&#x3060;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">kogel</trd>
  <trdgrp lng="pl">
    <trd>kula</trd>,
    <trd>pocisk</trd>
  </trdgrp>
  <trd lng="pt">bala <klr tip="amb">(de arma de fogo)</klr></trd>
  <trd lng="ro">glon&#x21B; </trd>
  <trd lng="ru">&c_p;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>gu&lcaron;ka</trd>,
    <trd>strela</trd>
  </trdgrp>
  <trd lng="sv"><klr>(gev&auml;rs)</klr><ind>kula</ind></trd>
  <trd lng="uk">&c_k;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x5F39; <pr>d&#xE0;n</pr></trd>,
    <trd>&#x5F48; <pr>d&#xE0;n</pr></trd>,
    <trd>&#x67AA;&#x5F39; <pr>qi&#x101;ngd&#xE0;n</pr></trd>,
    <trd>&#x69CD;&#x5F48; <pr>qi&#x101;ngd&#xE0;n</pr></trd>,
    <trd>&#x5B50;&#x5F39; <pr>z&#x1D0;d&#xE0;n</pr></trd>,
    <trd>&#x5B50;&#x5F48; <pr>z&#x1D0;d&#xE0;n</pr></trd>,
    <trd>&#x67AA;&#x5B50; <pr>qi&#x101;ngz&#x1D0;</pr></trd>,
    <trd>&#x69CD;&#x5B50; <pr>qi&#x101;ngz&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kugl.0ego">
  <kap>
    <tld/>ego
  </kap>
  <snc>
    <dif>
      Kanon<tld/>o:
      <ekz>
        por detrui tion, necesa estus pafilega <tld/>ego
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        &Ccirc;e pordego, en herbo, &gcirc;is nun ku&scirc;as granda, &#x2AFD; Kiel kapo, <tld/>ego, el kanon&#x2019;
        svedlanda
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia frato estas konsumita de &ccirc;agreno, kaj trenas kiel <tld/>egon ekzistadon de
        nun sencelan
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_d;&c_r;&c_o; <klr tip="amb">(&c_g;&c_a;&c_r;&c_m;&c_a;&c_t;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="de">Kanonenkugel</trd>
  <trd lng="ja">&#x7832;&#x5F3E;  <pr>&#x307B;&#x3046;&#x3060;&#x3093;</pr></trd>
</drv>

<drv mrk="kugl.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc>
    <dif>
      <ref tip="malprt" cel="sxrot.0o.MIL">&Scirc;rot</ref>ero:
      <ekz>
        en la kanaro zumis la <tld/>eta&jcirc;o <klr>(<ref cel="sxrot.0o.MIL">&scirc;roto<sncref/></ref>)</klr>
        kaj krakis pafo post pafo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malfeli&ccirc;a gardisto, ricevinte en la &scirc;ultron tutan &scirc;argon de <tld/>etoj, falis
        flanken
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tintis per prenitaj el po&scirc;o <tld/>etoj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VII.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_r;&c_o;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_r;&c_a;&c_c;&c_ib;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_b;&c_ib;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Schrotkugel</trd>
  <trd lng="en">shot <klr>(pellet)</klr></trd>
  <trd lng="fr">grenaille de plomb</trd>
  <trd lng="ja">&#x6563;&#x5F3E;  <pr>&#x3055;&#x3093;&#x3060;&#x3093;</pr></trd>
  <trd lng="ru">&c_o;&c_r;&c_u;&c_zh;&c_je;&c_j;&c_n;&c_a;&c_ja; <ind>&c_d;&c_r;&c_o;&c_b;&c_mol;</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x836F;&#x4E38; <pr>xi&#x1CE;oy&#xE0;ow&#xE1;n</pr></trd>,
    <trd>&#x5C0F;&#x85E5;&#x4E38; <pr>xi&#x1CE;oy&#xE0;ow&#xE1;n</pr></trd>,
    <trd>&#x4E38; <pr>w&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kugl.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="kugl.0ingo.ARM">
    <dif>
      Parto de <ref tip="malprt" cel="kartoc.0o.MIL">karto&ccirc;o</ref>
      konsistanta el cilindra metala ujeto, kiu entenas
      <ref cel="pulv.0o">pulvo</ref>n kaj en kiu al&gcirc;usti&gcirc;as la <tld/>o:
      <ekz>
        sur la planko saltetis eluzitaj <tld/>ingoj, frakasi&gcirc;is fenestrovitroj
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_l;&c_mol;&c_z;&c_a;</trd>
  <trd lng="cs">n&aacute;bojnice</trd>
  <trd lng="de">Patronenh&uuml;lse</trd>
  <trdgrp lng="en">
    <trd>case</trd>,
    <trd>casing</trd>
  </trdgrp>
  <trd lng="fr">douille</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;vely<klr tip="ind"> (l&ouml;ved&eacute;k&eacute;)</klr></trd>,
    <trd>patron<klr tip="ind"> (l&ouml;ved&eacute;k&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="id">selongsong</trd>
  <trd lng="ja">&#x85AC;&#x304D;&#x3087;&#x3046;  <pr>&#x3084;&#x3063;&#x304D;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">patroon<klr tip="ind"> (van kogel)</klr></trd>
  <trd lng="pl">nab&oacute;j</trd>
  <trd lng="ro">cartu&#x219;</trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_z;&c_a;</trd>
  <trd lng="sk">n&aacute;bojnica</trd>
  <trd lng="uk">&c_g;&c_ib;&c_l;&c_mol;&c_z;&c_a;</trd>
</drv>
</art>
