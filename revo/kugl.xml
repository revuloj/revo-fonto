<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kugl.xml,v 1.21 2018/10/06 20:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kugl</rad>/o
</kap>
<drv mrk="kugl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kugl.0o.ARM">
    <dif>
      Malgranda peco de malmola metalo kun proksimume konusa formo, kiun
      oni el&jcirc;etas per &ccirc;iaj porteblaj pafiloj:
      <ekz>
        <tld/>o trafis lin en la kruron<fnt><aut>Wim M. A. De
        Smet</aut>:
        <vrk>Anta&ubreve; cent jaroj</vrk>, <bib>Monato</bib><lok>jaro 1998a,
        numero 5a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="kartoc.0o.MIL">karto&ccirc;o</ref>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>koule</trd>,
    <trd>kule</trd>,
    <trd>kulka</trd>,
    <trd>st&rcaron;ela</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kugel</trd>,
    <trd>Gewehrkugel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bullet</trd>,
    <trd>shot</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bala<klr> (de arma de fuego)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balle<klr> (d'arme &agrave; feu)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&ouml;ved&eacute;k</trd>,
    <trd>goly&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>pelor</trd>,
    <trd>peluru</trd>
  </trdgrp>
  <trd lng="nl">kogel</trd>
  <trdgrp lng="pl">
    <trd>kula</trd>,
    <trd>pocisk</trd>
  </trdgrp>  
  <trd lng="pt">bala <klr tip="amb">(de arma de fogo)</klr></trd>
  <trd lng="ro">glon&#539; </trd>
  <trd lng="ru">&c_p;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>gu&lcaron;ka</trd>,
    <trd>strela</trd>
  </trdgrp>
  <trd lng="sv"><klr>(gev&auml;rs)</klr><ind>kula</ind></trd>
</drv>

<drv mrk="kugl.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="kugl.0ingo.ARM">
    <dif>
      Parto de <ref tip="malprt" cel="kartoc.0o.MIL">karto&ccirc;o</ref>
      konsistanta el cilindra metala ujeto, kiu entenas
      pulvon kaj en kiu al&gcirc;usti&gcirc;as la <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_l;&c_mol;&c_z;&c_a;</trd>
  <trd lng="cs">n&aacute;bojnice</trd>
  <trd lng="de">Patrone</trd>
  <trdgrp lng="en">
    <trd>case</trd>,
    <trd>casing</trd>
  </trdgrp>
  <trd lng="fr">douille</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;vely<klr tip="ind"> (l&ouml;ved&eacute;k&eacute;)</klr></trd>,
    <trd>patron<klr tip="ind"> (l&ouml;ved&eacute;k&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="id">selongsong</trd>
  <trd lng="nl">patroon<klr tip="ind"> (van kogel)</klr></trd>
  <trd lng="pl">nab&oacute;j</trd>     
  <trd lng="ro">cartu&scedil;</trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_z;&c_a;</trd>
  <trd lng="sk">n&aacute;bojnica</trd>
</drv>
</art>
