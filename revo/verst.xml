<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>verst</rad>/o
</kap>
<drv mrk="verst.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rusa <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      vojlongo
      valoranta 1067 metrojn:
      <ekz>
        la kale&scirc;o veturis ne malpli ol duonon de <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 3a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_s;&c_t;&c_a;</trd>
  <trd lng="de">Werst</trd>
  <trd lng="en">verst</trd>
  <trdgrp lng="fr">
    <trd>verste</trd>
  </trdgrp>
  <trd lng="hu">verszta</trd>
  <trd lng="nl">werst</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_s;&c_t;&c_a;</trd>
</drv>
</art>
