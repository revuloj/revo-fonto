<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bolet</rad>/o
</kap>
<drv mrk="bolet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fung.tub0o">Tubfungo</ref>.
    </dif>
    <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j; &c_g;&c_r;&c_i;&c_b;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="boleta.0oj"><tld/>acoj</ref>
      (<trd lng="la">Boletus</trd>).
    </dif>
    <trd lng="fr">bolet</trd>
    <trd lng="ru">&c_b;&c_o;&c_l;&c_je;&c_t;&c_u;&c_s;</trd>
  </snc>
</drv>
</art>
