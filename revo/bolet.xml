<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bolet</rad>/o
</kap>
<drv mrk="bolet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fung.tub0o">Tubfungo</ref>.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="bolet.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Boletus</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="bolet.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="fung.klab0oj">klabfungoj</ref>
      (<trd lng="la">Boletaceae</trd>), kies ombrelforma
      fruktokorpo de sube prezentas tubformajn truojn.
    </dif>
  </snc>
  <ref tip="prt" cel="fung.tub0o">tubfungo</ref>
</drv>
</art>
