<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bolet.xml,v 1.25 2023/12/20 07:21:43 revo Exp $">
<kap>
  <rad>bolet</rad>/o
</kap>
<drv mrk="bolet.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="fung.tub0o">Tubfungo</ref>:
      <ekz>
        melongeno <klr>[&#8230;]</klr> havas ri&ccirc;an kaj kompleksan bonguston,
        iel similan al tiu de kelkaj fungoj (tiel, ke oni ofte falsas
        sekigitajn <tld/>ojn miksante ilin kun tran&ccirc;oj de melongenoj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20032">Melongeno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la satan<tld/>o (Satanus boletus) estas neman&gcirc;ebla, venena, mikoriza fungo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=125483">Satanboleto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y; <ind>&c_g;&c_r;&c_y;&c_b;</ind></trd>
    <trd lng="de">R&ouml;hrling</trd>
      <trd lng="en">bolete</trd>
    <trd lng="hu">cs&odblac;gomba</trd>
    <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;
&c_g;&c_r;&c_i;&c_b;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="boleta.0oj"><tld/>acoj</ref>
      (<trd lng="la">Boletus</trd>):
      <ekz>
        betulacaj arboj <klr>[&#8230;]</klr> vivas simbioze kun fungoj kiel amanitoj, trikolomoj,
        <tld/>oj, laktarioj, rusuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18703">Betulacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_k;</trd>,
      <trd>&c_sh;&c_ch;&c_y;&c_r;&c_y; <ind>&c_g;&c_r;&c_y;&c_b;</ind></trd>
    </trdgrp>
    <trd lng="bg">&c_m;&c_a;&c_n;&c_a;&c_t;&c_a;&c_r;&c_k;&c_a;</trd>
    <trd lng="de">R&ouml;hrenpilze</trd>
    <trd lng="en">boletus</trd>
    <trd lng="fr">bolet</trd>
    <trd lng="hu">tin&oacute;ru(gomba)</trd>
    <trd lng="nl">boletus</trd>
    <trd lng="pl">borowik</trd>
    <trd lng="ru">&c_b;&c_o;&c_l;&c_je;&c_t;&c_u;&c_s;</trd>
    <trd lng="sv">boletus</trd>
  </snc>
  <trd lng="ca">bolet</trd>
  <trd lng="cs">boleta</trd>
  <trd lng="ja">&#x30A4;&#x30B0;&#x30C1;</trd>
  <trd lng="sk">hr&iacute;b</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_i;&c_j; &c_g;&c_r;&c_i;&c_b;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&c_i;&c_j; &c_g;&c_r;&c_i;&c_b;</trd>,
    <trd>&c_b;&#x43E;&c_r;&#x43E;&c_v;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
