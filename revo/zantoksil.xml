<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zantoksil.xml,v 1.6 2023/09/05 09:43:57 revo Exp $">
<kap>
  <rad>zantoksil</rad>/o
</kap>
<drv mrk="zantoksil.0o">
  <kap><tld/>o</kap>
  <snc mrk="zantoksil.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo a&ubreve; arbeto el varmaj landoj,
      kun ofte por homoj venenaj floroj, folioj a&ubreve; &scirc;eloj; 
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio <ref tip="malprt" cel="rutac.0oj">rutacoj</ref>
      (<trd lng="la">Zanthoxylum</trd>):
      <ekz>
        en la montaro kreskas <tld/>oj,
        evonimoj kaj <esc>pli-ol-10-specaj</esc> aceroj, kaj ili malavare
        donas la ru&gcirc;an koloron al la naturo
        <fnt>
          <aut>Wang Lihua</aut>,
          <vrk><url ref="&CRI;2521/2017/12/15/176s196485.htm">
          Aparte superbas &ccirc;i tie la pejza&gcirc;o en frua a&ubreve;tuno</url></vrk>,
          <lok>El Popola &Ccirc;inio, 2016-09-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">Zanthoxylum</trd>
  </snc>
  <snc mrk="zantoksil.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:spicoj" cel="spic.0o.KUI">Spico</ref>
      el la &scirc;elo de beroj de kelkaj specioj de <tld/>aj arboj,
      anka&ubreve; nomata Si&ccirc;uan-pipro:
      <ekz>
        enmetu <tld/>ojn kaj stelanizon kiam la kuiroleo varmi&gcirc;as
        <fnt>
          <aut>Shanshan</aut>,
          <vrk><url ref="&CRI;2521/2017/12/15/176s196485.htm">
          Koka&jcirc;o kun papriko kaj terpomo</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2017-12-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/3e/Zanthoxylum_piperitum.jpg/440px-Zanthoxylum_piperitum.jpg" prm="CC     BY-SA 4.0">Si&ccirc;uan-pipro (Zanthoxylum piperitum) &#x2010; fruktoj kaj folioj
        <fnt>
          <aut>Didier Descouens, propra verko</aut>
          <lok><url ref="&WCW;/File:Zanthoxylum_piperitum.jpg">&WCW;/File:Zanthoxylum_piperitum.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de"> Szechuanpfeffer</trd>
    <trdgrp lng="en">
      <trd>Sichuan pepper</trd>,
      <trd>Szechuan pepper</trd>,
      <trd>Szechwan pepper</trd>,
      <trd>Chinese prickly ash</trd>,
      <trd>Chinese pepper</trd>,
      <trd>Timut pepper</trd>,
      <trd>mala pepper</trd>
    </trdgrp>
    <trd lng="fr">poivre du Sichuan</trd>
  </snc>
  <trd lng="ja">&#x30B5;&#x30F3;&#x30B7;&#x30E7;&#x30A6;</trd>
</drv>
</art>

