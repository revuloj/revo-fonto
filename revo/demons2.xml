<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: demons2.xml,v 1.24 2023/12/10 10:30:20 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>demonstrativ</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="demons2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Montra <ref tip="super" cel="pronom.0o">pronomo</ref> a&ubreve; adjektivo:
      <ekz>
        &leftquot;<nac>esta</nac>&rightquot; kaj &leftquot;<nac>&eacute;sta</nac>&rightquot; prononci&gcirc;as same,
        do &scirc;ajne la korno de la dua vorto superfluas,
        tamen &gcirc;i montras ke la dua vorto rolas kiel pronomo (la unua estas <tld/>a
        adjektivo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1586">Hispana lingvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a adjektivo estas &leftquot;<nac>lu</nac>&rightquot; (tiu, tia) kaj
        la demonstraj pronomoj estas &leftquot;<nac>el</nac>&rightquot; a&ubreve;
        &leftquot;<nac>lo</nac>&rightquot; (tio, tiu viro), &leftquot;<nac>ella</nac>&rightquot;
        a&ubreve; &leftquot;<nac>la</nac>&rightquot; (tio, tiu virino)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37892">Spokil</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_y;
&c_z;&c_a;&c_j;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>demonstrativum</trd>,
    <trd>p&rcaron;&iacute;slovce</trd>,
    <trd>ukazovac&iacute; z&aacute;jmeno</trd>,
    <trd>&ccaron;&iacute;slovka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Demonstrativum</trd>,
    <trd>Demonstrativpronomen</trd>,
    <trd>hinweisendes F&uuml;rwort</trd>
  </trdgrp>
  <trd lng="en">demonstrative</trd>
  <trd lng="es">demostrativo</trd>
  <trdgrp lng="fa">
    <trd>&a_D;&a_m;&f_y;&a_r; &a_A;&a_sh;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_S;&a_f;&a_t; &a_A;&a_sh;&a_A;&a_r;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;monstratif<klr tip="ind"> (grammaire)</klr></trd>
  <trd lng="he">&#x5DB;&#x5D9;&#x5E0;&#x5D5;&#x5D9; &#x5E8;&#x5D5;&#x5DE;&#x5D6;</trd>
  <trd lng="hu">mutat&oacute; <ind>n&eacute;vm&aacute;s</ind></trd>
  <trd lng="ja">&#25351;&#x793A;&#x8A5E;  <pr>&#12375;&#12376;&#12375;</pr></trd>
  <trd lng="pl">zaimek wskazuj&aogonek;cy</trd>
  <trd lng="pt">demonstrativo</trd>
  <trd lng="ro">pronume demonstrativ</trd>
  <trd lng="ru">&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">ukazovacie z&aacute;meno</trd>
  <trd lng="uk">&c_v;&#x43A;&c_a;&c_z;&c_ib;&c_v;&#x43D;&c_i;&c_j; &c_z;&c_a;&c_j;&#x43C;&c_je;&#x43D;&#x43D;&c_i;&#x43A;</trd>
  <trd lng="zh">&#25351;&#x793A;&#x4EE3;&#x8A5E; <pr>zh&#x1D0;sh&#xEC;d&#xE0;ic&#xED;</pr></trd>
</drv>
</art>
