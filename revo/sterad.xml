<art mrk="$Id: sterad.xml,v 1.15 2024/03/25 07:28:36 revo Exp $">
<kap>
  <rad>steradian</rad>/o
</kap>
<drv mrk="sterad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <mlg kod="SI">sr</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="angul.solida0o">solida angulo</ref> en la Internacia
      Sistemo de unuoj, egala al solida angulo, kiu 
      <ref cel="trancx.de0i.MAT">detran&ccirc;as</ref> 
      sur <ref cel="sfer.0o.surfaco">sfero</ref> surfacon kun areo egala
      al la kvadrato de ties radiuso:
      <ekz>
        la mezuro de orta triedro estas <frm am="pi//2 &quot; sr&quot;">&pi;/2
        sr</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_a;&c_d;&c_y;&c_ja;&c_n;</trd>
  <trd lng="bg">&c_s;&c_t;&c_je;&c_r;&c_a;&c_d;&c_i;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>jednotka prostorov&eacute;ho &uacute;hlu</trd>,
    <trd>steradi&aacute;n</trd>
  </trdgrp>
  <trd lng="da">steradian</trd>
  <trd lng="de">Steradiant</trd>
  <trd lng="en">steradian</trd>
  <trd lng="fr">st&eacute;radian<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="hu">szteradi&aacute;n</trd>
  <trd lng="id">steradian</trd>
  <trd lng="nl">steradiaal</trd>
  <trd lng="pl">steradian</trd>
  <trd lng="pt">esterradiano</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_a;&c_d;&c_i;&c_a;&c_n;</trd>
  <trd lng="sk">steradi&aacute;n</trd>
  <trd lng="sv">steradian</trd>
  <trd lng="uk">&#x441;&#x442;&#x435;&#x440;&#x430;&#x434;&#x456;&#x430;&#x43D;</trd>
  <trd lng="zh">&#x7403;&#x9762;&#x5EA6; <pr>qi&#xFA;mi&#xE0;nd&#xF9;</pr></trd>
</drv>
</art>

