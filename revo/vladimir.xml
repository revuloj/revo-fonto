<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Vladimir</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vladimir.0o">
  <kap><tld/>o</kap>
  <snc mrk="vladimir.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo slavdevena:
      <ekz>
        mi estas rekomendita por la ordeno de <tld/>o de kvara grado
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_U;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_U;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_ib;&c_r;</trd>
  </trdgrp>
  <trd lng="fr">Vladimir</trd>
  <trd lng="hu">Vlagyimir</trd>
  <trd lng="ru">&c_V;&c_l;&c_a;&c_d;&c_i;&c_m;&c_i;&c_r;</trd>
</drv>
</art>

