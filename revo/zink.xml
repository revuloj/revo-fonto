<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>zink</rad>/o
</kap>
<drv mrk="zink.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun atomnumero 30, atompezo 65,37, simbolo Zn. Bluegriza <ref tip="super" cel="metal.0o">metalo</ref>, uzata por kovri tegmentojn, formi baterielementojn ktp.
    </dif>
  </snc>
  <trd lng="de">Zink</trd>
  <trd lng="en">zinc</trd>
  <trdgrp lng="es">
    <trd>cinc</trd>,
    <trd>zinc</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>zinc</trd>
  </trdgrp>
  <trd lng="nl">zink</trd>
  <trd lng="ru">&c_c;&c_i;&c_n;&c_k;</trd>
  <trd lng="sv">zink</trd>
</drv>
</art>
