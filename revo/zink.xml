<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>zink</rad>/o
</kap>
<drv mrk="zink.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 30, atompezo 65,37, simbolo Zn. Bluegriza <ref tip="super"
      cel="metal.0o">metalo</ref>, uzata por kovri tegmentojn, formi
      baterielementojn ktp:
      <ekz>
        Kosovo estas anka&ubreve; ri&ccirc;a je plumbo, <tld/>o, ar&gcirc;ento, kadmio,
        oro<fnt><aut>Bardhyl Mezini</aut>: <vrk><url
        ref="&Monato;/006488.html">La vera ka&ubreve;zo de la milito</url></vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 5a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_n;&c_k;</trd>
  <trd lng="br">zink</trd>
  <trd lng="de">Zink</trd>
  <trdgrp lng="el">
    <trd>&psi;&epsilon;&ypsilon;&delta;&alfa_ton;&rho;&gamma;&ypsilon;&rho;&omikron;&sigma_fina;</trd>,
    <trd>&tau;&sigma;&jota_ton;&gamma;&kappa;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">zinc</trd>
  <trdgrp lng="es">
    <trd>cinc</trd>,
    <trd>zinc</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>zinc</trd>
  </trdgrp>
  <trd lng="he">&alef;&bet;&ftsadi;</trd>
  <trd lng="hu">cink</trd>
  <trd lng="it">zinco</trd>
  <trd lng="nl">zink</trd>
  <trd lng="pt">zinco</trd>
  <trd lng="ru">&c_c;&c_i;&c_n;&c_k;</trd>
  <trd lng="sv">zink</trd>
  <trd lng="sk">zinok</trd>
</drv>
</art>
