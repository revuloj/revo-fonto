<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>delt</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="delt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Triangula insulo formita de disbran&ccirc;i&gcirc;o de rivero &ccirc;e
&gcirc;ia enfluo en la maron:
      <ekz>
        <tld/>o de Nilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Delta</trd>,
     <trd>Flussdelta</trd>
  </trdgrp>
  <trd lng="en">delta</trd>
  <trd lng="es">delta</trd>
  <trd lng="fr">delta</trd>
  <trd lng="hu">delta (torkolat)</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_mol;&c_t;&c_a;</trd>
</drv>
</art>
