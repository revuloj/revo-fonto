<art mrk="$Id: ilustr.xml,v 1.23 2025/11/25 20:36:46 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ilustr</rad>/i
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="ilustr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Ornami tekston per bildoj a&ubreve; desegna&jcirc;oj rilataj al &gcirc;i:
      <ekz><tld/>i libron;</ekz>
      <ekz><tld/>ita revuo.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Ornami per bildoj: <ekz><tld/>ita po&scirc;tkarto.</ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klarigi ideon per komparoj a&ubreve; ekzemploj:
      <ekz><tld/>i regulon per multaj ekzemploj;</ekz>
      <ekz><tld/>i sian penson per metaforoj kaj figuroj.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>ilustrovat</trd>,
    <trd>zobrazit</trd>,
    <trd>zobrazovat</trd>
  </trdgrp>
  <trd lng="de">illustrieren</trd>
  <trd lng="en">illustrate</trd>
  <trd lng="es">ilustrar</trd>
  <trd lng="fr">illustrer</trd>
  <trd lng="hu">illusztr&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x633F;&#x7D75;&#12434;&#20837;&#x308C;&#x308B;  <pr>&#12373;&#12375;&#12360;&#12434;&#12356;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x56F3;&#x89E3;&#12377;&#x308B;  <pr>&#x305A;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x8AAC;&#x660E;&#12377;&#x308B;  <pr>&#x305B;&#12388;&#12417;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">illustreren</trd>
  <trdgrp lng="pl">
    <trd>ilustrowa&cacute;</trd>,
    <trd>ozdabia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">ilustrar</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ilustrova&tcaron;</trd>,
    <trd>zobrazi&tcaron;</trd>
  </trdgrp>
  <trd lng="zh">&#x56FE;&#x89E3; <pr>t&#xFA;ji&#x11B;</pr></trd>
</drv>

<drv mrk="ilustr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc num="1">
    <dif>
      Desegno, kiu <tld/>as.
    </dif>
    <trd lng="nl">illustratie</trd>
  </snc>
  <snc num="2">
    <dif>
      Ekzemplo, kiu klarigas.
    </dif>
    <trd lng="de">Illustration</trd>
    <trd lng="en">illustration</trd>
    <trd lng="es">ilustraci&oacute;n</trd>
    <trd lng="nl">voorbeeld</trd>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="fr">illustration<klr tip="ind"> (ce qui illustre)</klr></trd>
  <trd lng="hu">illusztr&aacute;ci&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x633F;&#x7D75;  <pr>&#12373;&#12375;&#12360;</pr></trd>,
    <trd>&#x56F3;  <pr>&#x305A;</pr></trd>,
    <trd>&#x30A4;&#x30E9;&#x30B9;&#x30C8;  <pr>&#12356;&#12425;&#12377;&#12392;</pr></trd>,
    <trd>&#x4F8B;&#x8A3C;  <pr>&#x308C;&#12356;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x4F8B;&#x793A;  <pr>&#x308C;&#12356;&#12376;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ilustracja</trd>,
    <trd>rycina</trd>
  </trdgrp>
  <trd lng="pt">ilustra&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_ib;&#x43B;&#x44E;&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x63D2;&#x753B; <pr>ch&amacron;hu&#xE0;</pr></trd>,
    <trd>&#x63D2;&#x56FE; <pr>ch&amacron;t&#xFA;</pr></trd>
  </trdgrp>
</drv>
