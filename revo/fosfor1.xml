<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fosfor1.xml,v 1.21 2022/03/20 13:07:54 revo Exp $">
<kap>
  <rad>fosforesk</rad>/i
</kap>
<drv mrk="fosfor1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lum.0i.FIZ">Lumi</ref> en mallumo, &ccirc;u proprece, &ccirc;u post prilumigo, dum relative
      longa tempo:
      <ekz>
        la profesoro funkciigis la aparaton, la tubo komencis <tld/>i
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        fantoma lumo en la kajuto <tld/>as
        <fnt>
          <bib>KFl</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bran&ccirc;o estis dika kiel barelo kaj <tld/>is ru&gcirc;e de interne
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Perelin', la Noktarbaro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="lumine.0i">lumineski</ref>
    <refgrp tip="vid">
      <ref cel="fluore.0i">fluoreski</ref>,
      <ref cel="reflek.0i">reflekti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_r;&c_e;&c_s;&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">fosforeskovat</trd>
  <trd lng="de">phosphoreszieren</trd>
  <trd lng="en">phosphoresce</trd>
  <trd lng="fr">&ecirc;tre <ind>phosphorescent</ind></trd>
  <trdgrp lng="hu">
    <trd>foszforeszk&aacute;l</trd>,
    <trd>vill&oacute;dzik</trd>
  </trdgrp>
  <trd lng="pl">fosforyzowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>fosforescer</trd>,
    <trd>fosforear</trd>,
    <trd>fosforejar</trd>,
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>fosforeskova&tcaron;</trd>,
    <trd>svetielkova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x456;&#x437;. &#x444;&#x43E;&#x441;&#x444;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x444;&#x43E;&#x441;&#x444;&#x43E;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x444;&#x43E;&#x441;&#x444;&#x43E;&#x440;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="fosfor1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta ecojn de <tld/>o:
      <ekz>
        &scirc;prucvernisita per buntaj <tld/>aj farboj
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna po&scirc;to</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni e&ccirc; malkovris rarajn <tld/>ajn &scirc;tonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010474.php">Ri&ccirc;a tero, malri&ccirc;a popolo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni nepre vizitu la akvarion, kiu unua en la mondo prezentas <tld/>ajn koralojn
        <fnt>
          <bib>Monato</bib>
          <aut>G&eacute;rard Mansel</aut>
          <vrk><url ref="&Monato;/2014/011366.php">&Ccirc;arma insulo en Pacifiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_r;&c_e;&c_s;&c_c;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>
  <trd lng="cs">sv&ecaron;t&eacute;lkuj&iacute;c&iacute;</trd>
  <trd lng="de">phosphoreszierend</trd>
  <trd lng="en">phosphorescent</trd>
  <trd lng="es">fosforescente</trd>
  <trd lng="fr">phosphorescent</trd>
  <trd lng="hu">foszforeszk&aacute;l&oacute;</trd>
  <trd lng="pl">fosforyzuj&aogonek;cy</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">svetielkuj&uacute;ci</trd>
</drv>

<drv mrk="fosfor1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fenomeno <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_r;&c_e;&c_s;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Phosphoreszenz</trd>
  <trd lng="en">phosphorescence</trd>
  <trd lng="es">fosforescencia</trd>
  <trd lng="fr">phosphorescence</trd>
  <trd lng="hu">foszforeszk&aacute;l&aacute;s</trd>
  <trdgrp lng="pl">
    <trd>fosforyzacja</trd>,
    <trd>fosforescencja</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x444;&#x43E;&#x441;&#x444;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>
</drv>
</art>
