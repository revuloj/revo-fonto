<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fosforesk</rad>/i
</kap>
<drv mrk="fosfor1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Lumi en mallumo, &ccirc;u proprece, &ccirc;u post prilumigo. 
    </dif>
    <refgrp tip="vid">
      <ref cel="fluore.0i">fluoreski</ref>,
      <ref cel="reflek.0i">reflekti</ref>.
    </refgrp>
  </snc>
  <trd lng="de">phosphoreszieren</trd>
  <trd lng="fr">&ecirc;tre <ind>phosphorescent</ind></trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fosfor1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta ecojn de <tld/>o. 
    </dif>
  </snc>
  <trd lng="de">phosphoreszierend</trd>
  <trd lng="fr">phosphorescent</trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
</drv>
<drv mrk="fosfor1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fenomeno <tld/>i. 
    </dif>
  </snc>
  <trd lng="de">Phosphoreszenz</trd>
  <trd lng="fr">phosphorescence</trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
