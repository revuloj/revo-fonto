<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>akciz</rad>/o
</kap>
<drv mrk="akciz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Imposto, kiun oni devas pagi por man&gcirc;a&jcirc;oj, trinka&jcirc;oj,
      materialoj, a&ubreve; por diversaj &scirc;tataj servoj.
    </dif>
  </snc>
  <trd lng="de">Verbrauchssteuer</trd>
  <trd lng="en">exise tax</trd>
  <trdgrp lng="fr">
    <trd>accise</trd>,
    <trd>taxe</trd>
  </trdgrp>
  <trd lng="nl">accijns</trd>
  <trd lng="pt">imposto de consumo</trd>
  <trd lng="ru">&c_a;&c_k;&c_c;&c_i;&c_z;</trd>
  <trd lng="sk">spotrebn&aacute; da&ncaron;</trd>
  <trdgrp lng="sv">
    <trd>accis</trd>,
    <trd>konsumtionsskatt</trd>
  </trdgrp>
</drv>

<drv mrk="akciz.urba0o">
  <kap>urba <tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Imposto pri diversaj varoj, kiam ili eniras en urbon.
    </dif>
    <refgrp tip="sin">
      <ref cel="dogan.0o">dogano</ref>.
    </refgrp>
  </snc>
  <trd lng="de">kommunale Verbrauchssteuer</trd>
  <trd lng="en">local tariff</trd>
  <trd lng="fr">octroi</trd>
</drv>

<drv mrk="akciz.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Imposti varojn per akcizo.
    </dif>
  </snc>
  <trd lng="de">Verbrauchssteuern erheben</trd>
  <trd lng="en">levy an exise tax</trd>
</drv>

</art>
