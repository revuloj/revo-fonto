<kap>
  <rad>male</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="male.0o">
  <kap><tld/>o</kap>
  <snc mrk="male.0o.ilo">
    <dif>
      <ref tip="super" cel="martel.0o">Martelo</ref> kun kapo
      amba&ubreve;flanke
      samforma kaj samuza, ofte ligna:
      <ekz>
        <tld/>o enbatas la ringojn de barelo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 7a, p.
        432a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Anna Ritam&auml;ki</nom> stafete reprenis de <nom>Jukka Laaksonen</nom> ne nur la
        prezidantan
        <tld/>on, sed anka&ubreve; la grandan respondecon
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="klab.0o">klabo</ref>
  </snc>
  <ref tip="hom" cel="male1.0o">Maleo (&ccirc;efurbo)</ref>
  <trd lng="be">&c_k;&c_ib;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">ma&ccedil;a</trd>
  <trdgrp lng="cs">
    <trd>d&rcaron;ev&ecaron;n&aacute; pali&ccaron;ka</trd>,
    <trd>tlouk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kn&uuml;pfel</trd>,
    <trd>Klipfel</trd>,
    <trd>Kl&uuml;pfel</trd>,
    <trd>Kl&ouml;pfel</trd>,
    <trd>Schlegel</trd>,
    <trd>Holzhammer</trd>
  </trdgrp>
  <trd lng="es">mazo</trd>
  <trd lng="fr">maillet</trd>
  <trdgrp lng="hu">
    <trd>fakalap&aacute;cs</trd>,
    <trd>k&eacute;tfej&udblac; <ind>kalap&aacute;cs</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6728;&#x69CC; <pr>&#x304D;&#x3065;&#x3061;</pr></trd>,
    <trd>&#x6253;&#x7403;&#x69CC; <pr>&#x3060;&#x304D;&#x3085;&#x3046;&#x3064;&#x3061;</pr></trd>,
    <trd>&#x69CC;&#x9AA8; <pr>&#x3064;&#x3061;&#x3053;&#x3064;</pr></trd>,
    <trd>&#x30B7;&#x30E5;&#x30E2;&#x30AF;&#x30AC;&#x30A4;</trd>
  </trdgrp>
  <trd lng="nl">houten <ind>hamer</ind></trd>
  <trdgrp lng="pl">
    <trd>m&lstroke;otek drewniany</trd>,
    <trd>pobijak</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ma&ccedil;o</trd>,
    <trd>malho</trd>
  </trdgrp>
  <trd lng="ro">ciocan de lemn</trd>
  <trd lng="ru">&c_k;&c_i;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dvojhlav&eacute; dreven&eacute; kladivo</trd>,
    <trd>pobij&aacute;k</trd>,
    <trd>p&aacute;lka <klr>( &scaron;port.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_i;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&#x2019;&c_ja;&c_n;&c_i;&c_j; &c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_k;&c_je;&c_t;&c_n;&c_i;&c_j; &c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_ch;&c_o;&c_k; (&c_s;&c_je;&c_r;&c_je;&c_d;&c_n;&c_mol;&c_o;&c_g;&c_o; &c_v;&c_u;&c_h;&c_a;)</trd>,
    <trd>&c_s;&c_a;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x69CC; <pr>chu&#xED;</pr></trd>,
    <trd>&#x96DE;&#x5927;&#x817F; <pr>j&#x12B;d&#xE0;tu&#x1D0;</pr></trd>,
    <trd>&#x9E21;&#x5927;&#x817F; <pr>j&#x12B;d&#xE0;tu&#x1D0;</pr></trd>
  </trdgrp>
</drv>
