<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>perpleks</rad>/a
</kap>
<drv mrk="perple.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      &Scirc;anceli&gcirc;a pro senkonsileco, hezitanta inter pluraj kontra&ubreve;aj decidoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="dub.0i">dubi</ref>,
      <ref cel="konfuz.0i">konfuzi</ref>,
      <ref cel="embara.0i">embarasi</ref>,
      <ref cel="konste1.0i">konsterni</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_u;&c_b;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_mol;&c_b;&c_ja;&c_n;&c_t;&c_e;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_sh;&c_a;&c_l;&c_o;&c_m;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>perplex</trd>,
    <trd>best&uuml;rzt</trd>,
    <trd>verdutzt</trd>,
    <trd>unschl&uuml;ssig</trd>,
    <trd>ratlos</trd>,
    <trd>fassungslos</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;sorient&eacute;</trd>,
    <trd>ind&eacute;cis</trd>,
    <trd>perplexe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zavarodott</trd>,
    <trd>hat&aacute;rozatlan</trd>,
    <trd>bizonytalankod&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>perplex</trd>,
    <trd>verbluft</trd>,
    <trd>besluiteloos</trd>
  </trdgrp>
  <trd lng="pt">perplexo</trd>
  <trd lng="es">perplejo, -a</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_je;&c_r;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_o;&c_u;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_sh;&c_je;&c_l;&c_o;&c_m;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="perple.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Embaraso anta&ubreve; neatendita situacio, malfacile solvebla afero:
      <ekz>
        la palataligitaj <ctl>&ccirc;ji, &scirc;ji</ctl> estas skribataj <ctl>qi,
        xi</ctl>, kiuj ka&ubreve;zas senfinan <tld/>on al ne&ccirc;inoj
        <fnt><aut>Yamasaki Seik&ocirc;</aut>: <vrk>Transskribo de la &ccirc;ina</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 4a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_u;&c_b;&c_l;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ja;&c_sh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;sarroi</trd>,
    <trd>irr&eacute;solution</trd>,
    <trd>perplexit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zavar</trd>,
    <trd>hat&aacute;rozatlans&aacute;g</trd>,
    <trd>bizonytalankod&aacute;s</trd>
  </trdgrp>  
  <trd lng="es">perplejidad</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_je;&c_r;&c_ja;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_o;&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
