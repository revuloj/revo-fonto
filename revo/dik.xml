<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dik</rad>/a
</kap>
<drv mrk="dik.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Granda la&ubreve; la tria dimensio, kiu estas nek lar&gcirc;eco, nek
      longeco:
      <ekz>
        <tld/>a homo, libro, krajono, &scirc;nuro, mantelo, ventro;
      </ekz>
      <ekz>
        mal<tld/>a folio;
      </ekz>
      <ekz>
        <tld/>aj gutoj de &scirc;vito
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Ne&gcirc;a re&gcirc;ino</vrk>)</fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de tiu tabulo estas ducentimetra <klr>(f)</klr>;
      </ekz>
      <ekz>
        mal<tld/>a vo&ccirc;o;
      </ekz>
      <ekz>
        <tld/>e skribita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        greno <tld/>e muelita;
      </ekz>
      <ekz>
        kie mal<tld/>e, tie rompi&gcirc;as
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>trunka viro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viando<tld/>a mano<fnt>Z</fnt>.
      </ekz>  
    </dif>
    <refgrp tip="sin">
      <ref cel="gras.0a">grasa</ref>.
    </refgrp>
  </snc>
  <trd lng="de">dick</trd>
  <trd lng="en">thick</trd>
  <trd lng="nl">dik</trd>
  <trd lng="pl">gruby</trd>
</drv>
<drv mrk="dik.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
     I&gcirc;i <tld/>a 
    </dif>
    <refgrp tip="sin">
      <ref cel="barel">bareli&gcirc;i</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>.
    </refgrp>
  </snc>
</drv>
</art>
