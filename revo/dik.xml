<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dik</rad>/a
</kap>
<drv mrk="dik.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Granda la&ubreve; la dimensio <tld/>o; 
      havanta difinitan grandon la&ubreve; tiu dimensio:
      <ekz>
        <tld/>a homo, libro, krajono, &scirc;nuro, mantelo, ventro;
      </ekz>
      <ekz>
        <ind>mal<tld/>a</ind> folio;
        <trdgrp lng="fr">
          <trd>fin<klr tip="ind"> (peu &eacute;pais)</klr></trd>,
          <trd>fluet</trd>,
          <trd>menu<klr tip="ind"> (fluet)</klr></trd>,
          <trd>mince</trd>
        </trdgrp>
      <trdgrp lng="nl">
       <trd>mager</trd>,
       <trd>dun</trd>
  </trdgrp>
</ekz>
      <ekz>
        folio duonmilimetron <tld/>a;
      </ekz>
      <ekz>
        <tld/>aj gutoj de &scirc;vito
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal<tld/>a vo&ccirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e skribita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        greno <tld/>e muelita;
      </ekz>
      <ekz>kie mal<tld/>e, tie rompi&gcirc;as
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        <tld/>trunka viro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viando<tld/>a mano<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gras.0a">grasa</ref>.
    </refgrp>
  </snc>
  <trd lng="de">dick</trd>
  <trd lng="en">thick</trd>
  <trdgrp lng="fr">
    <trd>corpulent</trd>,
    <trd>&eacute;pais</trd>,
    <trd>gras<klr tip="ind"> (&eacute;pais)</klr></trd>,
    <trd>gros<klr tip="ind"> (&eacute;pais, corpulent)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dik</trd>,
    <trd>corpulent</trd>
  </trdgrp>
  <trd lng="pl">gruby</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_l;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j; <klr>(&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j; <klr>(&c_o; &c_sh;&c_r;&c_i;&c_f;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sv">tjock</trd>
  <trd lng="vo">bigik</trd>
</drv>
<drv mrk="dik.0o">
  <kap><tld/>o</kap>
  <snc mrk="dik.0o.dimensio">
    <dif>
      La tria, plej malgranda
      <ref tip="super" cel="dimens.0o.ampleksa">dimensio</ref> de io (t.e. 
      nek &gcirc;ia <ref cel="long.0o.dimensio">longo</ref>,
      nek &gcirc;ia <ref cel="largx.0o.dimensio">lar&gcirc;o</ref>:
      <ekz>
        &gcirc;i havis la <tld/>on de manlar&gcirc;o
        <fnt><bib>M</bib>, <lok>&Re1; 7:26</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de tiu tabulo estas du centimetroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tiefe</trd>
  <trd lng="fr">&eacute;paisseur<klr tip="ind"> (dimension)</klr></trd>
  <trd lng="nl">dikte</trd>
</drv>
<drv mrk="dik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        la &ccirc;ambrego estis granda, sed &scirc;ajnis malvasta pro la 
        <tld/>eco de la kolonoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de liaj plandumoj kompensis lian malaltan
        staturon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>embonpoint</trd>,
      <trd>&eacute;paisseur</trd>,
      <trd>grosseur<klr tip="ind"> (embonpoint, &eacute;paisseur)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="dik.0o.dimensio"><tld lit="D"/>o
      <sncref ref="dik.0o.dimensio"/></ref>:
      <ekz>
        la <tld/>eco de tiu tabulo estas du centimetroj
        <fnt><bib>PV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dicke</trd>
<trd lng="nl">dikte</trd>
</drv>
<drv mrk="dik.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>se lipo <tld/>i&gcirc;as, ventro mal<tld/>i&gcirc;as
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        la deziro de kelkaj knabinoj <ind>mal<tld/>i&gcirc;i</ind> iras iafoje
        &gcirc;is ne&ubreve;rozo.
        <trdgrp lng="fr">
          <trd>mincir</trd>,
          <trd>maigrir</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>d&uuml;nn werden</trd>,
          <trd>abmagern</trd>
        </trdgrp>
      <trdgrp lng="nl">
    <trd>mager worden</trd>,
    <trd>vermageren</trd>
  </trdgrp>
  </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barel">bareli&gcirc;i</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>.
    </refgrp>
  </snc>
  <trd lng="de">dick werden</trd>
  <trdgrp lng="fr">
    <trd>grossir<klr tip="ind"> (devenir gros)</klr></trd>,
    <trd>&eacute;paissir<klr tip="ind"> (devenir &eacute;pais)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dik worden</trd>,
    <trd>verdikken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_l;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
