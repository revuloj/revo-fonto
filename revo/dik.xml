<drv mrk="dik.harmal0a">
  <kap>harmal<tld/>a</kap>
  <snc mrk="dik.harmal0a.KOMUNE">
    <dif>
      Treege mal<tld/>a, kiel haro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sen eraro e&ccirc; harmal<tld/>a
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_u;&c_d;&c_y; &c_ja;&c_k; &c_sh;&c_ch;&c_e;&c_p;&c_k;&c_a;</trd>
  <trdgrp lng="de">
