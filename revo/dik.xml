<drv mrk="dik.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <snc mrk="dik.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i mal<tld/>a:
      <ekz>
        mi ja bezonas man&gcirc;i, alie mi povas tute mal<tld/>i&gcirc;i
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>i&gcirc;is la Jarlibro, UEA klarigis, ke tio okazis pro neceso &scirc;pari
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        se lipo <tld/>i&gcirc;as, ventro mal<tld/>i&gcirc;as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_a;&c_n;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_u;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
