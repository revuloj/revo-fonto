<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dik</rad>/a
</kap>
<drv mrk="dik.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Granda la&ubreve; la dimensio <tld/>o;
      havanta difinitan grandon la&ubreve; tiu dimensio:
      <ekz>
        <tld/>a homo, libro, krajono, &scirc;nuro, mantelo, ventro;
      </ekz>
      <ekz>
        folio duonmilimetron <tld/>a;
      </ekz>
      <ekz>
        <tld/>aj gutoj de &scirc;vito
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e skribita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        greno <tld/>e muelita;
      </ekz>
      <ekz>
        <tld/>trunka viro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viando<tld/>a mano<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gras.0a">grasa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_o;&c_w;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">dick</trd>
  <trd lng="en">thick</trd>
  <trdgrp lng="fr">
    <trd>corpulent</trd>,
    <trd>&eacute;pais</trd>,
    <trd>gras<klr tip="ind"> (&eacute;pais)</klr></trd>,
    <trd>gros<klr tip="ind"> (&eacute;pais, corpulent)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dik</trd>,
    <trd>corpulent</trd>
  </trdgrp>
  <trd lng="pl">gruby</trd>
  <trdgrp lng="pt">
    <trd>grosso</trd>,
    <trd>espesso</trd>,
    <trd>encorpado</trd>,
    <trd>volumoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_l;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j; <klr tip="amb">(&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j; <klr tip="amb">(&c_o; &c_sh;&c_r;&c_i;&c_f;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sv">tjock</trd>
  <trd lng="vo">bigik</trd>
</drv>

<drv mrk="dik.0o">
  <kap><tld/>o</kap>
  <snc mrk="dik.0o.dimensio">
    <dif>
      La tria, plej malgranda
      <ref tip="super" cel="dimens.0o.ampleksa">dimensio</ref> de io (t.e.
      nek &gcirc;ia <ref cel="long.0o.dimensio">longo</ref>,
      nek &gcirc;ia <ref cel="largx.0o.dimensio">lar&gcirc;o</ref>):
      <ekz>
        &gcirc;i havis la <tld/>on de manlar&gcirc;o
        <fnt><bib>MT</bib>, <lok>&Re1; 7:26</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de tiu tabulo estas du centimetroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_w;&c_sh;&c_ch;&c_y;&c_n;&c_ja;</trd>
  <trd lng="de">Tiefe</trd>
  <trd lng="fr">&eacute;paisseur<klr tip="ind"> (dimension)</klr></trd>
  <trd lng="nl">dikte</trd>
  <trd lng="pt">espessura<klr tip="ind"> (dimens&atilde;o)</klr></trd>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_shch;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="dik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        la &ccirc;ambrego estis granda, sed &scirc;ajnis malvasta pro la
        <tld/>eco de la kolonoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de liaj plandumoj kompensis lian malaltan
        staturon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>embonpoint</trd>,
      <trd>&eacute;paisseur</trd>,
      <trd>grosseur<klr tip="ind"> (embonpoint, &eacute;paisseur)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>espessura</trd>,
      <trd>grossura<klr tip="ind"> (qualidade)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="dik.0o.dimensio"><tld lit="D"/>o
      <sncref ref="dik.0o.dimensio"/></ref>:
      <ekz>
        la <tld/>eco de tiu tabulo estas du centimetroj
        <fnt><bib>PV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dicke</trd>
  <trd lng="nl">dikte</trd>
  <trd lng="pt">espessura<klr tip="ind"> (dimens&atilde;o)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_l;&c_shch;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="dik.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        se lipo <tld/>i&gcirc;as, ventro mal<tld/>i&gcirc;as
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barel">bareli&gcirc;i</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_w;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_zh;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">dick werden</trd>
  <trdgrp lng="fr">
    <trd>grossir<klr tip="ind"> (devenir gros)</klr></trd>,
    <trd>&eacute;paissir<klr tip="ind"> (devenir &eacute;pais)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dik worden</trd>,
    <trd>verdikken</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tornar-se <ind>grosso</ind></trd>,
    <trd>tornar-se <ind>espesso</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_l;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="dik.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Malgranda la&ubreve; la dimensio <tld/>o;
      havanta difinitan malgrandon la&ubreve; tiu dimensio:
      <ekz>
        mal<tld/>a folio, homo, libro, krajono, &scirc;nuro, mantelo, ventro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
	mal<tld/>a vo&ccirc;o;
      </ekz>
      <ekz>
        kie mal<tld/>e, tie rompi&gcirc;as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gras.mal0a">malgrasa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_u;&c_d;&c_y;</trd>,
    <trd>&c_t;&c_a;&c_n;&c_k;&c_l;&c_ja;&c_v;&c_y;</trd>,
    <trd>&c_t;&c_o;&c_n;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fin<klr tip="ind"> (peu &eacute;pais)</klr></trd>,
    <trd>fluet</trd>,
    <trd>menu<klr tip="ind"> (fluet)</klr></trd>,
    <trd>mince</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mager</trd>,
    <trd>dun</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fino</trd>,
    <trd>delicado</trd>,
    <trd>delgado</trd>
  </trdgrp>
</drv>  

<drv mrk="dik.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i mal<tld/>a:
      <ekz>
        se lipo <tld/>i&gcirc;as, ventro mal<tld/>i&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la deziro de kelkaj knabinoj mal<tld/>i&gcirc;i iras iafoje
        &gcirc;is ne&ubreve;rozo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_a;&c_n;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_u;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_h;&c_u;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mincir</trd>,
    <trd>maigrir</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>d&uuml;nn werden</trd>,
    <trd>abmagern</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mager worden</trd>,
    <trd>vermageren</trd>
  </trdgrp>
  <trd lng="pt">afinar-se</trd>
</drv>	
</art>
