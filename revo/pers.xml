<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>pers</rad>/o
    <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="pers.0o">
  <kap><tld/>o</kap>
  <snc mrk="pers.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de la &ccirc;efa gento de <tld lit="P"/>ujo.
    </dif>
  </snc>
  <trd lng="de">Perser</trd>
  <trd lng="nl">Pers</trd>
  <trd lng="pl">Persja</trd>
  <trd lng="sv">en <ind>persisk</ind> person</trd>
</drv>

<drv mrk="pers.0a">
  <kap><tld/>a</kap>
  <snc mrk="pers.0a.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Rilata al <tld/>o a&ubreve; al <tld lit="P"/>ujo:
      <ekz>
        la <tld/>a lingvo,
      </ekz>
      <ekz>
        la <tld/>a kato,
      </ekz>
      <ekz>
        elprenu tapi&scirc;on la plej bonan, la <tld/>an,
      </ekz>
      <ekz>
        de kiu vi de&jcirc;etadis la <tld/>an satrapon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>persische Sprache</trd>,
    <trd>persisch</trd>
  </trdgrp>
  <trd lng="en">Persian</trd>
  <trd lng="nl">Perzisch</trd>
  <trd lng="pl">perski</trd>
  <trd lng="sv">persisk</trd>
</drv>

<drv mrk="pers.P0aGolfo">
  <kap><tld lit="P"/>a Golfo</kap>
  <snc mrk="pers.P0aGolfo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vasta golfo inter Irano kaj Arabio.
    </dif>
  </snc>
  <trd lng="de">Persischer Golf</trd>
  <trd lng="en">Persian Gulf</trd>
  <trd lng="nl">Perzische Golf</trd>
  <trd lng="pl">Zatoka Perska</trd>
  <trd lng="sv">Persiska viken</trd>
</drv>

<drv mrk="pers.P0ujo">
  <kap><tld lit="P"/>ujo</kap>
  <snc mrk="pers.P0ujo.HIS">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Eksa nomo de Irano.
      <ekz>
        en la unua jaro de Ciro, re&gcirc;o de <tld lit="P"/>ujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Persien</trd>
  <trd lng="en">Persia</trd>
  <trd lng="nl">Perzi&euml;</trd>
  <trd lng="pl">Persja</trd>
  <trd lng="sv">Persien</trd>
</drv>

<drv mrk="pers.P0io">
  <kap><tld lit="P"/>io</kap>
  <snc mrk="pers.P0io.HIS">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <ref tip="dif" cel="pers.P0ujo">Persujo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Perserreich</trd>,
    <trd>Persien</trd>
  </trdgrp>
  <trd lng="en">Persia</trd>
  <trd lng="nl">Perzi&euml;</trd>
  <trd lng="pl">Persja</trd>
  <trd lng="sv">Persien</trd>
</drv>
</art>

