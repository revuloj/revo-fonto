<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>veft</rad>/o
</kap>
<drv mrk="veft.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Fadenoj, kiujn oni pasigas inter la
      <ref tip="vid" cel="varp.0ero">varperojn</ref>.
    </dif>
    <ref tip="malprt" cel="teks.0ajxo">teksa&jcirc;o</ref>
  </snc>
  <trd lng="en">weft</trd>
  <trd lng="nl">inslag</trd>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_k;<klr tip="ind"> (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
</drv>

<drv mrk="veft.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      &Ccirc;iu unuopa fadeno, kiu interpasante la
      <ref tip="vid" cel="varp.0o">varpon</ref>
      formas la <tld/>on.
    </dif>
  </snc>
  <trd lng="nl">inslagdraad</trd>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_ch;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
