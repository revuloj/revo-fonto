<art mrk="$Id: senegal.xml,v 1.16 2024/03/24 20:00:01 revo Exp $">
<kap>
  <rad>Senegal</rad>/o
   <fnt>
     <bib>FK</bib>
   </fnt>
</kap>

<drv mrk="senegal.S0o">
  <kap>
    <tld lit="S"/>o
    <fnt> <bib>RLN</bib> </fnt>
  </kap>
  <mlg kod="ISO-3166">SN</mlg>
  <snc mrk="senegal.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la orienta <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Dakaro;
      teritorio de Respubliko <tld/>o, limanta kun
      <ref tip="vid" cel="gambi.G0o.GEOG">Gambio</ref>,
      <ref tip="vid" cel="gvine.G0o.GEOG">Gvineo</ref>,
      <ref tip="vid" cel="gvine.G0oBisauxo.GEOG">Gvineo-Bisa&ubreve;o</ref>,
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref>,
      <ref tip="vid" cel="mauxrit.M0o.GEOG">Ma&ubreve;ritanio</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>:
      <ekz>
        ses centojn da negroj mi &ccirc;e <esc>Senegal</esc> akiris je prezo profita
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        pozitivaj evoluoj okazis <klr>[&#x2026;]</klr> en Butano, Grekio, Samoo, <tld/>o kaj Turkio, kiuj
        abolis la mortpunon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008644.php">Ekzekutado ekzekuciata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; unu jaro plago de migrantaj akridoj neniigis 4 milionojn da hektaroj en Ma&ubreve;ritanio,
        <tld/>o, Malio, Ni&gcirc;ero kaj &Ccirc;ado
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008814.php">E&ubreve;ropo plialtigas sian drataron</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_e;&c_n;&c_e;&c_g;&c_a;&c_l;</trd>
    <trd lng="bg">&c_S;&c_je;&c_n;&c_je;&c_g;&c_a;&c_l;</trd>
    <trd lng="de">Senegal</trd>
    <trd lng="en">Senegal</trd>
    <trd lng="es">Senegal</trd>
    <trd lng="fr">S&eacute;n&eacute;gal</trd>
    <trd lng="hu">Szeneg&aacute;l</trd>
    <trd lng="id">Senegal</trd>
    <trd lng="it">Senegal</trd>
    <trdgrp lng="pl">
      <trd>Senegal</trd>,
      <trd>Republika <ind>Senegalu</ind></trd>
    </trdgrp>
    <trd lng="pt">Senegal</trd>
    <trdgrp lng="zh">
    <trd>&#x585E;&#x5167;&#x52A0;&#x723E; <pr>S&#xE0;in&#xE8;iji&#x101;&#x11B;r</pr></trd>,
    <trd>&#x585E;&#x5185;&#x52A0;&#x5C14; <pr>S&#xE0;in&#xE8;iji&#x101;&#x11B;r</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30BB;&#x30CD;&#x30AC;&#x30EB;&#x5DDD;  <pr>&#x30BB;&#x30CD;&#x30AC;&#x30EB;&#x304B;&#x308F;</pr></trd>
  <trd lng="uk">&#x421;&#x435;&#x43D;&#x435;&#x491;&#x430;&#x43B;</trd>
</drv>

</art>

