<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>1</ofc>
    <rad>bro&ccirc;</rad>/o
  </kap>
  <drv mrk="brocx.0o">
    <kap><tld/>o</kap>
    <snc num="1">
      <dif>
        Pinglo refaldita tiele, ke &gcirc;ia pinto kro&ccirc;i&gcirc;as en hoketo
        a&ubreve; &scirc;irma ingeto. 
      </dif>
    </snc>
    <snc num="2">
      <dif>
        Ornama juvelo provizita per samspeca pinglo. 
      </dif>
    </snc>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="buk.0o">buko</ref>.
    </refgrp>
    <trd lng="be">&c_b;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
    <trd lng="es">broche</trd>
    <trd lng="fr">broche <klr tip="ind">(&eacute;pingle, bijou)</klr></trd>
    <trd lng="hu">bross(t&udblac;)</trd>
    <trd lng="pt">broche</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_sh;&c_mol;</trd>
    <trd lng="sk">bro&scaron;&ncaron;a</trd>
  </drv>
</art>
