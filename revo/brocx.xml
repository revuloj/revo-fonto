<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brocx.xml,v 1.19 2021/03/05 17:02:56 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bro&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brocx.0o">
  <kap><tld/>o</kap>
  <snc mrk="brocx.0o.JUV">
    <dif>
      <ref tip="super" cel="ornam.0ajxo">Ornama&jcirc;o</ref>
      por la brusto, fiksebla &ccirc;e la vesta&jcirc;o per pinglo a&ubreve;
      <ref cel="pingl.sendangxera0o">sendan&gcirc;era pinglo</ref>:
      <ekz>
        unu desegnis modelojn de &ccirc;enetoj, braceletoj, <tld/>oj, ringoj, kovriloj por
        po&scirc;horlo&gcirc;oj kaj aliaj similaj multekostaj objektoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la verda stelo estas la plej simpla Esperanto-simbolo komune uzata ekzemple kiel
        <tld/>o a&ubreve; gluebla a&ubreve;to&scirc;ildo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9020">Verda stelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la imperiestro sendis al <nom>Sarah Bernhardt</nom> <tld/>on kun &scirc;iaj inicialoj skribitaj
        en diamantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=13316">Sarah Bernhardt</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="buk.0o">buko</ref>,
      <ref cel="fibul.0o.HIS">fibulo</ref>,
      <ref cel="insign.0o">insigno</ref>,
      <ref cel="sxild.nom0o">nom&scirc;ildo</ref>,
      <ref cel="agraf.0o.krocxilo">agrafo<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Brosche</trd>,
      <trd>Anstecknadel</trd>
    </trdgrp>
    <trd lng="en">brooch</trd>
    <trdgrp lng="pl">
      <trd>brosza</trd>,
      <trd>broszka</trd>
    </trdgrp>
  </snc>
  <snc mrk="brocx.0o.pinglo">
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="pingl.sendangxera0o.HEJM">sendan&gcirc;era
    pinglo</ref>
    <trd lng="en">safety pin</trd>
    <trdgrp lng="pl">
      <trd>agrafka</trd>,
      <trd>zapinka</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="es">broche</trd>
  <trd lng="fr">broche <klr tip="ind">(&eacute;pingle,
  bijou)</klr></trd>
  <trd lng="he">&#x5E1;&#x5D9;&#x5DB;&#x5EA; &#x5E7;&#x5D9;&#x5E9;&#x5D5;&#x5D8;</trd>
  <trd lng="hu">bross(t&udblac;)</trd>
  <trd lng="id">bros</trd>
  <trd lng="pt">broche</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_sh;&c_mol;</trd>
  <trd lng="sk">bro&scaron;&ncaron;a</trd>
</drv>
</art>
