<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>1</ofc>
    <rad>bro&ccirc;</rad>/o
  </kap>
  <drv mrk="brocx.0o">
    <kap><tld/>o</kap>
    <snc num="1" mrk="brocx.0o.pinglo">
      <dif>
        Pinglo refaldita tiele, ke &gcirc;ia pinto kro&ccirc;i&gcirc;as
        en hoketo
        a&ubreve; &scirc;irma ingeto. 
      </dif>
      <refgrp tip="sin">
        <ref cel="fibul.0o.pinglo">fibulo</ref>,
        <ref cel="pingl.sendangxera0o">sendan&gcirc;era pinglo</ref>
      </refgrp>
      
      <trdgrp lng="pl">
        <trd>agrafka</trd>,
        <trd>zapinka</trd>
      </trdgrp>
    </snc>
    <snc num="2" mrk="brocx.0o.juvelo">
      <dif>
        Ornama juvelo provizita per samspeca pinglo. 
      </dif>
      <ref tip="sub" cel="fibul.0o.HIS">fibulo</ref>
      <trdgrp lng="pl">
        <trd>brosza</trd>,
        <trd>broszka</trd>
      </trdgrp>
    </snc>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="buk.0o">buko</ref>.
    </refgrp>
    <trd lng="be">&c_b;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
    <trd lng="es">broche</trd>
    <trd lng="fr">broche <klr tip="ind">(&eacute;pingle,
    bijou)</klr></trd>
    <trd lng="hu">bross(t&udblac;)</trd>
    <trd lng="pt">broche</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_sh;&c_mol;</trd>
    <trd lng="sk">bro&scaron;&ncaron;a</trd>
  </drv>
</art>
