<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jurisprudenc.xml,v 1.14 2023/12/03 07:09:19 revo Exp $">
<kap>
  <rad>jurisprudenc</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jurisprudenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="jurisprudenc.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Decidoj de altaj ju&gcirc;ejoj,
      uzataj en interpreto kaj apliko de le&gcirc;oj:
      <ekz>
        kian juran forton havos la <tld/>o de la Mondaj Kortumoj 
        elnomitaj &ccirc;i-supre
        <fnt>
          <aut>J.-M. Breton, trad. J.-F. Clet</aut>,
          <vrk>
            <url ref="http://perso.orange.fr/mondanisto/mondjuro.htm">Ekapero de la monda
            Juro</url></vrk>,
          <lok>1997, &ccirc;apitro 11a</lok>
        </fnt>?
      </ekz>
      <ekz>
        tio estas konsekvenco de &gcirc;enerala agado pere de nova interpreto de <tld/>o en la
        lastaj 15 jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2010/010065.php">Maljusta justico</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_r;&a_w;&f_y;&a_h;&a_hamza_sure; &a_q;&a_D;&a_A;&f_y;&f_y;</trd>
  </snc>
  <snc mrk="jurisprudenc.0o.SCI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="jur.0oscienco.SCI">Juroscienco</ref>:
      <ekz>
        e&ccirc; tiom seka scienco kiel <tld/>o ne povas ekzisti sen 
        ekzemploj
        <fnt>
        <aut>J. Mura&scirc;kovskij, I. Mura&scirc;kovska</aut>,
        <vrk>Kien fluas roj&#x2019; Castalie?</vrk>,
        <lok>1984-1987</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>judikatura</trd>,
    <trd>jurisprudence</trd>,
    <trd>pr&aacute;vn&iacute; v&ecaron;da</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Rechtswissenschaft</trd>,
      <trd>Jurisprudenz</trd>
  </trdgrp>
  <trd lng="fr">jurisprudence</trd>
  <trd lng="it">giurisprudenza</trd>
  <trdgrp lng="ja">
    <trd>&#x5224;&#x4F8B;  <pr>&#x306F;&#x3093;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x6CD5;&#x89E3;&#x91C8;  <pr>&#x307B;&#x3046;&#x304B;&#x3044;&#x3057;&#x3083;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pt">jurisprud&ecirc;ncia</trd>
  <trdgrp lng="sk">
    <trd>1.pr&aacute;vna veda</trd>,
    <trd>2.s&uacute;dny v&yacute;klad pr&aacute;va</trd>,
    <trd>3.s&uacute;hrn s&uacute;dnych rozhodnut&iacute;</trd>,
    <trd>znalos&tcaron; pr&aacute;va</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x44E;&c_r;&c_i;&c_s;&#x43F;&c_r;&c_u;&c_d;&c_je;&#x43D;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_a;&c_v;&#x43E;&c_z;&#x43D;&c_a;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x44E;&c_r;&c_i;&c_d;&c_i;&c_ch;&#x43D;&c_ib; &#x43D;&c_a;&c_u;&#x43A;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x6CD5;&#x7406;&#x5B66; <pr>f&#x1CE;l&#x1D0;xu&#xE9;</pr></trd>
</drv>
