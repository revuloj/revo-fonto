<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>suveren</rad>/o
      <fnt>Z</fnt>
    </kap>
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <drv mrk="suvere.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Tio kion la juro rigardas la fonto kaj portanto de la supera
          kaj sendependa potenco: <ref tip="vid" cel="di.0o">dio</ref>
          en teokratio, la <ref tip="sub" cel="monarh">monarko</ref>
          en monarkio, la <ref tip="vid"
          cel="popol.0o.regataro">popolo</ref> en respubliko.
        </dif>
      </snc>
      <trdgrp lng="de">
          <trd>Machthaber</trd>,
          <trd>Herrscher</trd>,
          <trd>Souver&auml;n</trd>,
          <trd>Landesherr</trd>,
      </trdgrp>
      <trd lng="en">sovereign</trd>
      <trd lng="fr">souverain</trd>
      <trd lng="hu">egyeduralkod&oacute;</trd>
      <trd lng="nl">soeverein</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_u;&c_v;&c_je;&c_r;&c_je;&c_n;</trd>,
        <trd>&c_v;&c_y;&c_s;&c_sh;&c_a;&c_ja; &c_v;&c_l;&c_a;&c_s;&c_t;&c_mol;</trd></trdgrp>
   </drv>
    <drv mrk="suvere.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Plejpotenca, sendependa kaj senkontrola:
          <ekz>
            <tld/>a decido;
          </ekz><ekz>
            <tld/>a &scirc;tato.
          </ekz>
        </dif>
        <trd lng="ru">&c_s;&c_u;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      </snc>
      <trd lng="de">souver&auml;n</trd>
      <trdgrp lng="hu">
        <trd>szuver&eacute;n</trd>,
        <trd>f&uuml;ggetlen</trd>
      </trdgrp>
    <trdgrp lng="nl">
    <trd>soeverein</trd>,
    <trd>almachtig</trd>,
    <trd>oppermachtig</trd>
  </trdgrp>
</drv>
  </art>
