<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karit(at)</rad>/o
</kap>
<drv mrk="karit.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Supernatura virto per kiu oni amas Dion super &ccirc;io kaj la
      proksimulon kiel sin mem pro amo al Dio.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>christliche <ind>N&auml;chstenliebe</ind></trd>,
     <trd>Barmherzigkeit</trd>,
     <trd>Karitas</trd>,
     <trd>Caritas</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>benevolence</trd>,
    <trd>benignance</trd>,
    <trd>goodwill</trd>,
    <trd>Christian <ind>love</ind> of fellow men</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>naastenliefde</trd>,
    <trd>caritas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_mol; &c_k; &c_b;&c_l;&c_i;&c_zh;&c_n;&c_je;&c_m;&c_u;</trd>
  </trdgrp>
</drv>
</art>
