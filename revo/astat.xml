<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: astat.xml,v 1.9 2019/01/04 17:10:14 revo Exp $">
<kap>
  <rad>astat</rad>/o <fnt><bib>PIV2</bib></fnt>,
  <var><kap><rad>astaten</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="astat.0o">
  <kap><tld/>o,
    <var><kap>astateno</kap></var>
  </kap>
  <uzo tip="fak">KEM</uzo> 
  <mlg kod="IUPAC">At</mlg>  
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="85">elemento</ref> kun
      atomnumero 85, atompezo 209,9871, 
      kies &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref>
      estas radioaktivaj:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> okazas nature el la disfalado de uranio kaj torio, &gcirc;i
        estas la plej peza de &ccirc;iuj <ref tip="super" cel="haloge.0o">halogenoj</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27902"
              >Astato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_a;&c_t;</trd>
  <trd lng="bg">&c_a;&c_s;&c_t;&c_a;&c_t;&c_i;&c_j;</trd>
  <trd lng="br">astat</trd>
  <trd lng="ca">&agrave;stat</trd>
  <trd lng="cs">astat</trd>
  <trd lng="de">Astat</trd>
  <trd lng="en">astatine</trd>
  <trd lng="es">&aacute;stato</trd>
  <trd lng="fr">astate</trd>
  <trd lng="hu">aszt&aacute;cium</trd>
  <trd lng="id">astatin</trd>
  <trd lng="it">astato</trd>
  <trd lng="la">Astatinum</trd>
  <trd lng="nl">astaat</trd>
  <trd lng="pl">astat</trd>
  <trd lng="pt">astatino</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_s;&c_t;&c_a;&c_t;&c_i;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">ast&aacute;t</trd>
  <trd lng="sv">astat</trd>
</drv>
</art>

