<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>botel</rad>/o
</kap>
<drv mrk="botel.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc>
  <dif>
   Vazo plej ofte vitra, kun mallar&gcirc;a kolo. 
  </dif>
  <refgrp tip="sin">
   <ref cel="karaf.0o">karafo</ref>,
   <ref cel="flakon.0o">flakono</ref>.
  </refgrp>
 </snc>
 <trd lng="de">Flasche</trd>
 <trd lng="en">bottle</trd>
 <trd lng="nl">fles</trd>
 <trd lng="pl">butelka</trd>
 <trdgrp lng="ru">
   <trd>&c_b;&c_u;&c_t;&c_y;&c_l;&c_mol;</trd>,
   <trd>&c_b;&c_u;&c_t;&c_y;&c_l;&c_k;&c_a;</trd>
 </trdgrp>
 <trd lng="tr">&scedil;i&scedil;e</trd>
</drv>
<drv mrk="botel.trans0igi">
 <kap>trans<tld/>igi</kap>
 <snc>
  <dif>
   Elver&scirc;i en alian <tld/>on.  
  </dif>
 </snc>
 <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
