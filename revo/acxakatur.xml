<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: acxakatur.xml,v 1.15 2021/02/28 09:26:17 revo Exp $">
<kap>
  <rad>a&ccirc;akatur</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="acxakatur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko, kiu anta&ubreve;metas anta&ubreve; la tono
      tre mallongan komencan tonon duon- a&ubreve; plendute diferencan,
      a&ubreve;datan kiel preska&ubreve; samtempan:
      <ekz>
        la <tld/>on oni eble plej bone rigardu pli mallongan, malpli melodie gravan, varia&jcirc;on de la apo&gcirc;aturo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32272">Ornamo (muziko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apogxa.0o">apo&gcirc;aturo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_ch;&c_a;&c_k;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="bg">&c_a;&c_ch;&c_a;&c_k;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>acciaccatura</trd>,
    <trd>appoggiatura breu</trd>
  </trdgrp>
  <trd lng="de">kurzer <ind>Vorschlag</ind> <klr>(Musik)</klr></trd>
  <trdgrp lng="en">
    <trd>acciaccatura</trd>,
    <trd>crushed <ind>note</ind></trd>,
    <trd>short <ind>grace note</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acciaccatura</trd>
  </trdgrp>
  <trd lng="he">&#x5E2;&#x5E5; &#x5D4;&#x5E9;&#x5D9;&#x5D8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>fel&uuml;t&eacute;s</trd>,
    <trd>acciaccatura</trd>
  </trdgrp> 
  <trdgrp lng="it">
    <trd>appoggiatura breve</trd>,
    <trd>acciaccatura<klr> (mus.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>acciaccatura</trd>,
    <trd>pinc&eacute;</trd>
  </trdgrp> 
  <trd lng="ru">&c_a;&c_ch;&c_ch;&c_a;&c_k;&c_k;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>
</art>

