<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ccirc;akatur</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="acxakatur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu anta&ubreve;metas anta&ubreve; la tono
      tre mallongan komencan tonon duon- a&ubreve; plendute diferencan,
      a&ubreve;datan kiel preska&ubreve; samtempan.
    </dif>
    <refgrp tip="vid">
      <ref cel="apogxa.0o">apo&gcirc;aturo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Akkordbrechung</trd>
  <trdgrp lng="en">
    <trd>acciaccatura</trd>,
    <trd>crushed note</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acciaccatura</trd>
  </trdgrp>
</drv>
</art>

