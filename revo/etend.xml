  <trd lng="ka">&#x10D2;&#x10D0;&#x10D5;&#x10E0;&#x10EA;&#x10D4;&#x10DA;&#x10D4;&#x10D1;&#x10D0;</trd>
  <trd lng="km">&#x1780;&#x17B6;&#x179A;&#x179A;&#x17B8;&#x1780;&#x179A;&#x17B6;&#x179B;&#x178A;&#x17B6;&#x179B;</trd>
  <trd lng="ko">&#xD655;&#xC0B0;</trd>
  <trd lng="lo">&#xE81;&#xEB2;&#xE99;&#xEC1;&#xE9C;&#xEC8;&#xE81;&#xEB0;&#xE88;&#xEB2;&#xE8D;</trd>
  <trd lng="ml">&#xD35;&#xD3F;&#xD30;&#xD3F;&#xD15;&#xD4D;&#xD15;&#xD41;&#xD15;</trd>
  <trd lng="mr">&#x92A;&#x94D;&#x930;&#x938;&#x93E;&#x930;</trd>
  <trd lng="my">&#x1015;&#x103B;&#x1036;&#x1037;&#x1014;&#x103E;&#x1036;&#x1037;</trd>
  <trd lng="ne">&#x92B;&#x948;&#x932;&#x93F;&#x92F;&#x94B;</trd>
  <trd lng="ny">inafikira</trd>
  <trd lng="pa">&#xA2B;&#xA48;&#xA32;&#xA23;</trd>
  <trd lng="ps">&#x62F; &#x62E;&#x67E;&#x631;&#x6CC;&#x62F;&#x648;</trd>
  <trdgrp lng="pt">
    <trd>estender</trd>,
    <trd>desdobrar</trd>,
    <trd>estirar</trd>,
    <trd>espraiar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sd">&#x648;&#x687;&#x627;&#x626;&#x64A;</trd>
