  <ofc>*</ofc>
  <rad>etend</rad>/i
</kap>
<drv mrk="etend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="etend.0i.laulonge">
    <dif>
      <ref tip="sin" cel="volv.mal0i">Malvolvi</ref> la&ubreve; la longo, longigante en unu direkto:
      <ekz>
        la re&gcirc;ido <ind><tld/>is siajn brakojn al la &ccirc;ielo</ind>
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
        <trd lng="hu">az &eacute;g fel&eacute; ny&uacute;jtja a karj&aacute;t</trd>
        <trd lng="pl">wznosi&cacute; r&eogonek;ce ku niebu</trd>
      </ekz>
      <ekz>
        li <tld/>is la brakojn, kvaza&ubreve; vokante la &ccirc;ielon ateste al sia amo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[el]</klr> la korto <klr>[&#x2026;]</klr> estis <tld/>itaj fortikaj metalfadenoj
        al &ccirc;iuj eta&gcirc;oj de la domo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la manon kontra&ubreve; siajn malbondezirantojn
        <fnt>
          <bib>MT</bib>
          <lok>Ester 9:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        per kora movo <ind><tld/>i la manon</ind> por manpremo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
        <trd lng="de">die Hand ausstrecken</trd>
        <trd lng="fr">tendre la main</trd>
        <trd lng="hu">kiny&uacute;jtja a kez&eacute;t</trd>
        <trd lng="pl">wyci&aogonek;ga&cacute; r&eogonek;k&eogonek;</trd>
        <trd lng="es">extender la mano</trd>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;
        &c_r;&c_u;&c_k;&c_u;</trd>
      </ekz>
      <ekz>
        Lia ekscelenco <ind><tld/>i</ind>s <klr>(<ref cel="prezen1.0i.proponi">prezenti</ref>s)</klr>
        la ringon al <nom>Eunana</nom> kaj la &ccirc;eestantoj la&ubreve;te aklamis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
        <trd lng="de">darreichen</trd>
        <trd lng="hu">odany&uacute;jt</trd><trdgrp lng="pl">
          <trd>wyci&aogonek;ga&cacute;</trd>,
          <trd>wystawia&cacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>
      </ekz>
      <ekz>
        la cervido <klr>[&#x2026;]</klr> el<tld/>is sian gracian kolon kaj metis la kapon
        konfideme sur la &scirc;ultron de <nom>Umka</nom>
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dilat.0i">dilati</ref>,
      <ref cel="strecx.0i">stre&ccirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>strecken</trd>,
      <trd>ausstrecken</trd>
    </trdgrp><trdgrp lng="hu">
      <trd>kiegyenes&iacute;t</trd>,
      <trd>kiny&uacute;jt</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozk&lstroke;ada&cacute;</trd>,
      <trd>wyci&aogonek;ga&cacute;</trd>,
      <trd>rozprostowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="etend.0i.plurdirekte">
    <dif>
      <ref tip="sin" cel="volv.mal0i">Malvolvi</ref> plurdirekten,
      kovrante pli ampleksan spacon:
      <ekz>
        la birdo <tld/>is siajn belajn flugilojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen tiu vinbertrunko <klr>[&#x2026;]</klr> <tld/>is al &gcirc;i siajn bran&ccirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 17:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        arbo, plantita &ccirc;e akvo kaj <tld/>anta siajn radikojn al torento
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 17:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la figuro <tld/>is &ccirc;irka&ubreve; &scirc;i sian nigran mantelon
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is tendon super la tabernaklo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 40:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ka&scirc;is kaptilon por mi kaj &scirc;nurojn, ili <tld/>is reton &ccirc;e la vojo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 140:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la suno sin levis, <nom>Ali</nom> <tld/>is la tapi&scirc;on de pre&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stern.0i">sterni</ref>,
      <ref cel="sxvel.0igi">&scirc;veligi</ref>,
      <ref cel="vast.0igi.grandigi">vastigi<sncref/></ref>
    </refgrp>
    <trd lng="de">ausbreiten</trd>
    <trdgrp lng="hu">
      <trd>kiter&iacute;t</trd>,
      <trd>sz&eacute;tter&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozpo&sacute;ciera&cacute;</trd>,
      <trd>rozwiesza&cacute;</trd>,
      <trd>roz&sacute;ciela&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="etend.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Plilongigi a&ubreve; pliampleksigi metalon, malplidikigante
      &gcirc;in:
      <ekz>
        <tld/>i oron per martelado, per fadenigilo;
      </ekz>
      <ekz>
        kaj ili <ind>bat<tld/>i</ind>s foliojn da oro kaj tran&ccirc;is
        ilin en formo de fadenoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 39:3</lok>
        </fnt>.
        <trdgrp lng="fr">
          <trd>battre<klr tip="ind"> (un m&eacute;tal)</klr></trd>,
          <trd>marteler<klr tip="ind"> (un m&eacute;tal)</klr></trd>
        </trdgrp>
        <trd lng="pl">rozklepa&cacute;</trd>
        <trd lng="hu">kikalap&aacute;l</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>strecken</trd>,
      <trd>flie&szlig;pressen</trd>,
      <trd>kaltziehen</trd>
    </trdgrp>
    <trd lng="pl">rozklepa&cacute;</trd>
    <trd lng="hu">kiny&uacute;jt</trd>
  </snc>
  <snc mrk="etend.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disvastigi &ccirc;iudirekten:
      <ekz>
        Li <tld/>as sur ilin Sian lumon
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 36:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maro ku&scirc;is <tld/>ita anta&ubreve; la knabino
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        la blanka lotusa floro <klr>[&#x2026;]</klr> <tld/>is sur la arbaraj lagoj siajn
        grandajn verdajn foliojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la tuta longo de la valo sin <tld/>is, unu post alia, dek mar&ccirc;aj lagoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la lingvo-sciojn de la infanoj per la instruado
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kore beda&ubreve;ras, ke ni ne povas <tld/>i al vi la avanta&gcirc;on, kiun &gcirc;uis aliaj
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ausbreiten</trd>,
      <trd>verbreiten</trd>
    </trdgrp><trd lng="hu">kiterjeszt</trd>
    <trdgrp lng="pl">
      <trd>rozpo&sacute;ciera&cacute;</trd>,
      <trd>rozci&aogonek;ga&cacute;</trd>,
      <trd>rozprzestrzenia&cacute;</trd>,
      <trd>rozszerza&cacute;</trd>,
      <trd>si&eogonek;ga&cacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x626C; <pr>y&#xE1;ng</pr></trd>,
    <trd>&#x92EA; <pr>p&#x16B;</pr></trd>,
    <trd>&#x6313; <pr>zh&#x101;</pr></trd>,
    <trd>&#x589E; <pr>z&#x113;ng</pr></trd>,
    <trd>&#x6063;&#x8086; <pr>z&#xEC;s&#xEC;</pr></trd>,
    <trd>&#x83F6; <pr>b&#x11B;ng</pr></trd>,
    <trd>&#x58EE;&#x5927; <pr>zhu&#xE0;ngd&#xE0;</pr></trd>,
    <trd>&#x62D3; <pr>tu&#xF2;</pr></trd>
  </trdgrp>
</snc>
