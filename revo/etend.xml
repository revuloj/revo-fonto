<drv mrk="etend.sin0i">
  <kap>sin <tld/>i</kap>
  <snc>
    <dif>
      <tld lit="E"/>i siajn membrojn.
    </dif>
    <trd lng="ca">estirar-se</trd>
    <trd lng="de">sich <ind>ausbreiten</ind></trd>
    <trd lng="hu">ny&uacute;jt&oacute;zkodik</trd>
    <trd lng="pl">przeci&aogonek;ga&cacute; si&eogonek;</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="E"/>i&gcirc;i.
    </dif>
    <trd lng="ca">estendre's</trd>
    <trd lng="de">sich <ind>erstrecken</ind></trd>
    <trdgrp lng="hu">
      <trd>kiny&uacute;lik</trd>,
      <trd>elter&uuml;l</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nach&aacute;zet se</trd>,
    <trd>nat&aacute;hnout se</trd>,
    <trd>rozl&eacute;hat se</trd>,
    <trd>rozprost&iacute;rat se</trd>,
    <trd>rozt&aacute;hnout se</trd>,
    <trd>vytahovat se do d&eacute;lky</trd>
  </trdgrp>
  <trd lng="es">extenderse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tirer</ind></trd>,
    <trd>s'<ind>&eacute;tendre</ind></trd>
  </trdgrp>
  <trd lng="pl">rozci&aogonek;ga&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozprestiera&tcaron; sa</trd>,
    <trd>rozp&iacute;na&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="etend.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Malvolvi&gcirc;i la&ubreve; la longo:
      <ekz>
        sur amba&ubreve; bordoj
        <tld/>i&gcirc;is seninterrompa vico da dometoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 8</lok></fnt>.
      </ekz>
      <trdgrp lng="hu">
        <trd>kiny&uacute;lik</trd>,
        <trd>kiterjed</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="stern">sterni&gcirc;i</ref>,
      <ref cel="kusx.0i">ku&scirc;i</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Malvolvi&gcirc;i &ccirc;iudirekten:
      <ekz>
        la bran&ccirc;oj
        <tld/>i&gcirc;is &ccirc;irka&ubreve; la tuta frontono
        <fnt><bib>Fab2</bib>, <lok>Sonorilo</lok></fnt>;
      </ekz>
      <ekz>
        longa ombro <tld/>i&gcirc;is sur la plafonon
        <fnt><bib>Fab2</bib>, <lok>Dano Holger</lok></fnt>;
      </ekz>
      <ekz>
        nebulo <tld/>i&gcirc;is super la valo;
      </ekz>
      <ekz>
        dol&ccirc;a parfumo <tld/>i&gcirc;is tra la aero;
      </ekz>
      <ekz>
        bonodoro dis<tld/>i&gcirc;is<fnt>Z</fnt>.
      </ekz>
      <trdgrp lng="hu">
        <trd>elter&uuml;l</trd>,
        <trd>sz&eacute;tter&uuml;l</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="vast">disvasti&gcirc;i</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Okupi vastan spacon a&ubreve; tempon:
      <ekz>
        la grenkampoj <tld/>i&gcirc;as &gcirc;is la horizonto;
      </ekz>
      <ekz>
        la nemezurebla maro <tld/>i&gcirc;is anta&ubreve; li
        <fnt><bib>Fab2</bib>, <lok>Sonorilo</lok></fnt>;
      </ekz>
      <ekz>
        la Pireneoj <tld/>i&gcirc;as inter Francujo kaj Hispanujo;
      </ekz>
      <ekz>
        tiu periodo <tld/>i&gcirc;as de 1884 &gcirc;is 1900.
      </ekz>
      <trdgrp lng="hu">
        <trd>terjed <ind>vmeddig</ind></trd>,
        <trd>tart <ind>vmeddig</ind></trd>
      </trdgrp>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disvasti&gcirc;i &ccirc;iudirekten:
      <ekz>
        la scii&gcirc;o <tld/>i&gcirc;is rapide;
      </ekz>
      <ekz>
        la interesoj, la konkurenco nun <tld/>i&gcirc;as al la tuta
        mondo.
      </ekz>
      <trd lng="hu">terjed</trd>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">estendre's</trd>
  <trdgrp lng="es">
    <trd>extenderse</trd>,
    <trd>propagarse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tendre</ind></trd>,
