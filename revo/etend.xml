<drv mrk="etend.sin0i">
  <kap>sin <tld/>i</kap>
  <snc mrk="etend.sin0i.korpe">
    <dif>
      <tld lit="E"/>i siajn membrojn:
      <ekz>
        tro mallonga estos la lito, por sin <tld/>i
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        petege sin <tld/>is ies manoj
        <fnt>
          <bib>Lasta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam venis la tempo por dormi, la gasto sin <tld/>is sur la benko kaj sub la
        kapon metis la sakon kiel kusenon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        la urso <klr>[&#x2026;]</klr> sin <tld/>is &ccirc;e la fajro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">estirar-se</trd>
    <trd lng="de">sich <ind>ausbreiten</ind></trd>
    <trd lng="hu">ny&uacute;jt&oacute;zkodik</trd>
    <trd lng="pl">przeci&aogonek;ga&cacute; si&eogonek;</trd>
  </snc>
  <snc mrk="etend.sin0i.disvastigxi">
    <dif>
      <ref tip="dif" cel="etend.0igxi"><tld lit="E"/>i&gcirc;i</ref>:
      <ekz>
        nur nuda griza sabla tero sin <tld/>is &gcirc;is la akvoturnejo
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        dekstre kaj maldekstre de la <klr>[&#x2026;]</klr> pilonoj, sin <tld/>is muroj a&ubreve; pli
        &gcirc;uste kelketa&gcirc;aj konstrua&jcirc;oj, kun mallar&gcirc;aj fenestroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la pre&gcirc;ejo sin <tld/>is en du vicoj la domoj de la vila&gcirc;o
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">estendre's</trd>
    <trd lng="de">sich <ind>erstrecken</ind></trd>
    <trdgrp lng="hu">
      <trd>kiny&uacute;lik</trd>,
      <trd>elter&uuml;l</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7DBF;&#x5EF6; <pr>mi&#xE1;ny&#xE1;n</pr></trd>,
    <trd>&#x4F38;&#x5411; <pr>sh&#x113;nxi&#xE0;ng</pr></trd>,
    <trd>&#x8FBD;&#x9614; <pr>li&#xE1;oku&#xF2;</pr></trd>,
    <trd>&#x5EF6;&#x4F38; <pr>y&#xE1;nsh&#x113;n</pr></trd>,
    <trd>&#x5347;&#x61D2;&#x8170; <pr>sh&#x113;ngl&#x1CE;ny&#x101;o</pr></trd>,
    <trd>&#x6A2A;&#x8DE8; <pr>h&#xE9;ngku&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nach&aacute;zet se</trd>,
    <trd>nat&aacute;hnout se</trd>,
    <trd>rozl&eacute;hat se</trd>,
    <trd>rozprost&iacute;rat se</trd>,
    <trd>rozt&aacute;hnout se</trd>,
    <trd>vytahovat se do d&eacute;lky</trd>
  </trdgrp>
  <trd lng="es">extenderse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tirer</ind></trd>,
    <trd>s'<ind>&eacute;tendre</ind></trd>
  </trdgrp>
  <trd lng="pl">rozci&aogonek;ga&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozprestiera&tcaron; sa</trd>,
    <trd>rozp&iacute;na&tcaron; sa</trd>
  </trdgrp>
</drv>

