<drv mrk="etend.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="etend.0igxi.laulonge">
    <dif>
      Malvolvi&gcirc;i la&ubreve; la longo:
      <ekz>
        svene li <tld/>i&gcirc;is sur la tero
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mano kun brilanta fingringo <tld/>i&gcirc;as al la peco, <klr>[&#x2026;]</klr> la laboro
        progresas
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni suprengrimpis la rokon, kiu <tld/>i&gcirc;is super la maron
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur amba&ubreve; bordoj
        <tld/>i&gcirc;is seninterrompa vico da dometoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 8</lok></fnt>.
      </ekz>
      <trdgrp lng="hu">
        <trd>kiny&uacute;lik</trd>,
        <trd>kiterjed</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="stern">sterni&gcirc;i</ref>,
      <ref cel="kusx.0i">ku&scirc;i</ref>
    </refgrp>
  <trd lng="de">sich <ind>strecken</ind></trd>
  </snc>
  <snc mrk="etend.0igxi.plurdirekte">
    <dif>
      Malvolvi&gcirc;i plurdirekten:
      <ekz>
        la bran&ccirc;oj
        <tld/>i&gcirc;is &ccirc;irka&ubreve; la tuta frontono
        <fnt><bib>Fab2</bib>, <lok>Sonorilo</lok></fnt>;
      </ekz>
      <ekz>
        longa ombro <tld/>i&gcirc;is sur la plafonon
        <fnt><bib>Fab2</bib>, <lok>Dano Holger</lok></fnt>;
      </ekz>
      <ekz>
        ilia vosto povas dis<tld/>i&gcirc;i en la formo de granda rado kaj &scirc;an&gcirc;brilas en &ccirc;iuj
        koloroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>.
      </ekz>
      <trdgrp lng="hu">
        <trd>elter&uuml;l</trd>,
        <trd>sz&eacute;tter&uuml;l</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxirka.0i">&ccirc;irka&ubreve;i</ref>,
      <ref cel="kresk.cxirkaux0i">&ccirc;irka&ubreve;kreski</ref>,
      <ref cel="vast">disvasti&gcirc;i</ref>,
<ref cel="fald.dis0igxi">disfaldi&gcirc;i</ref>
    </refgrp>
  <trd lng="de">sich <ind>ausbreiten</ind></trd>
  </snc>
  <snc mrk="etend.0igxi.okupi">
    <dif>
      <ref tip="super" cel="okup.0i">Okupi</ref> vastan spacon a&ubreve; tempon:
      <ekz>
        la nemezurebla maro <tld/>i&gcirc;is anta&ubreve; li
        <fnt><bib>Fab2</bib>, <lok>Sonorilo</lok></fnt>;
      </ekz>
      <ekz>
        verdaj ebena&jcirc;oj <klr>[&#x2026;]</klr> onde <tld/>i&gcirc;as &gcirc;is kien la okulo atingas
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie <tld/>i&gcirc;is senlima vasteco, malluma kaj timiga profunda&jcirc;o
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
      <trdgrp lng="hu">
        <trd>terjed <ind>vmeddig</ind></trd>,
        <trd>tart <ind>vmeddig</ind></trd>
      </trdgrp>
    </dif>
  <trdgrp lng="de">
    <trd>sich <ind>ausdehen</ind></trd>,
    <trd>sich <ind>erstrecken</ind></trd>
  </trdgrp></snc>
  <snc mrk="etend.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="vast.dis0igxi">Disvasti&gcirc;i</ref> &ccirc;iudirekten:
      <ekz>
        la grekaj lingvoj <tld/>i&gcirc;is super fremda substrato kaj tio grandparte eksplikas
        diversajn nova&jcirc;ojn en la gramatika strukturo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la scii&gcirc;o <tld/>i&gcirc;is rapide;
      </ekz>
      <ekz>
        la interesoj, la konkurenco nun <tld/>i&gcirc;as al la tuta
        mondo.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>verbreiten</ind></trd>
    <trd lng="hu">terjed</trd>
    <trdgrp lng="zh">
    <trd>&#x6D41;&#x5E03; <pr>li&#xFA;b&#xF9;</pr></trd>,
    <trd>&#x6563;&#x53D1; <pr>s&#xE0;nf&#x101;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">estendre's</trd>
  <trdgrp lng="es">
    <trd>extenderse</trd>,
    <trd>propagarse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tendre</ind></trd>,
    <trd>se <ind>propager</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4F38;&#x3073;&#x308B;  <pr>&#x306E;&#x3073;&#x308B;</pr></trd>,
    <trd>&#x5E83;&#x304C;&#x308B;  <pr>&#x3072;&#x308D;&#x304C;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozci&aogonek;ga&cacute; si&eogonek;</trd>,
    <trd>rozpo&sacute;ciera&cacute; si&eogonek;</trd>,
    <trd>roz&sacute;ciela&cacute; si&eogonek;</trd>
  </trdgrp>
