<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekinoderm</rad>/oj
</kap>
<drv mrk="ekinod.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Echinodermata</trd>),
      vivantaj en maro, kun plej ofte kvinradia simetrio kaj
      kovritaj de pikiloj.
    </dif>
    <refgrp tip="sub">
      <ref cel="ekin.0o">ekino</ref>,
      <ref cel="asteri.0o">asterio</ref>
    </refgrp>
  </snc>
  <trd lng="de">Stachelh&auml;uter</trd>
  <trd lng="ru">&c_i;&c_g;&c_l;&c_o;&c_k;&c_o;&c_zh;&c_i;&c_je;</trd>
</drv>
</art>
