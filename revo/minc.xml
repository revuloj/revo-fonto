<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: minc.xml,v 1.18 2024/02/04 09:20:02 revo Exp $">
<kap>
  <rad>minc</rad>/a
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="minc.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="minc.0a.maldika">
    <dif>
      <ke><ref tip="dif" cel="dik.mal0a">Maldika</ref></ke>, <ke><ref tip="dif" cel="largx.mal0a">mallar&gcirc;a</ref></ke>:
      <ekz>
        la olda servistino nigravesta pa&ubreve;tis, kun la <tld/>aj lipoj
        kunfermitaj
        <fnt><aut>Jon Mirande, trad. Joxemari Sarasua</aut>
        <vrk>La Baptofilino</vrk><lok>Fonto, jaro 2002a, p. 15a</lok></fnt>.
      </ekz>
      <ekz>
        Rafo iom post iom korinklinis al tiu <tld/>a kaj rigida &ccirc;arma junulino
         <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la felisaj okuletoj <klr>[li]</klr> vidi&gcirc;is <tld/>a kaj senbarba, preska&ubreve;
        alabastra
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni ne plu bezonas misuzi nian <tld/>an inteligentecon por provi komprenon pri
        praaj okazinta&jcirc;oj
        <fnt>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200111.pdf#page=5">Ovo kaj kokino</url></vrk>
          <bib>Monato</bib>
          <lok>2001/11, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="magr.0a.maldika">magra<sncref/></ref>,
       <ref cel="svelt.0a">svelta</ref>
    </refgrp>
    <trd lng="be">&c_h;&c_u;&c_d;&c_y;</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>prim</trd>,
    <trd>minso</trd>
  </trdgrp>
  <trd lng="cs">tenk&yacute; <klr>(lit.)</klr></trd>
  <trdgrp lng="de">
    <trd>mager</trd>,
    <trd>d&uuml;nn</trd>,
    <trd>d&uuml;rr</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_r;&f_y;&f_k;</trd>,
    <trd>&a_l;&a_A;&a_gh;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fin<klr tip="ind"> (peu &eacute;pais)</klr></trd>,
    <trd>fluet</trd>,
    <trd>menu<klr tip="ind"> (fluet)</klr></trd>,
    <trd>mince</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&eacute;kony</trd>,
    <trd>sov&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mager</trd>,
    <trd>dun</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fino</trd>,
    <trd>delicado</trd>,
    <trd>delgado</trd>
  </trdgrp>
  <trd lng="sk">tenk&yacute;</trd>
  <trd lng="uk">&c_t;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#32048; <pr>x&igrave;</pr></trd>,
    <trd>&#32454; <pr>x&igrave;</pr></trd>,
    <trd>&#30246; <pr>sh&ograve;u</pr></trd>
  </trdgrp>
</drv>
</art>

