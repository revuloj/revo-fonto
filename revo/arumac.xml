<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: arumac.xml,v 1.3 2024/12/03 14:28:33 afido Exp $">
    <kap>
      <rad>arumac</rad>/oj
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="arumac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref> de
      <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref> plantoj (<trd
      lng="la">Araceae</trd>)
      de plej multe tropikaj specioj:
      <ekz>
        kunvivas kaktoj, orkidacoj, <tld/>oj kaj filikoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2018/012073.php"
              >Mirinda&jcirc;oj en montaro, La kampadejo</url></vrk>
          <lok>2018, numero 11, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Aronstabgew&auml;chse</trd>
    <trd lng="en">Araceae</trd>
    <trd lng="fr">Araceae</trd>
    <trd lng="pl">obrazkowate</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_a;&c_r;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_y;&c_ja;</trd>,
  <trd>&c_a;&c_r;&c_o;&c_ib;&c_d;&c_n;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
</trdgrp>
</drv>
  </art>

