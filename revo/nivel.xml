<drv mrk="nivel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nivel.0i.GEOD">
    <uzo tip="fak">GEOD</uzo>
    <dif>
      Mezuri diferencon inter <tld/>oj, alton rilate al <tld/>o
    </dif>
    <trdgrp lng="de">
       <trd>einebnen</trd>,
       <trd>nivellieren</trd>
    </trdgrp>
    <trd lng="en">to <ind>level</ind></trd>
    <trdgrp lng="hu">
      <trd>szintez</trd>,
      <trd>szintk&uuml;l&ouml;nbs&eacute;get m&eacute;r</trd></trdgrp>
    <trd lng="nl">peilen</trd>
    <trd lng="ru">&c_n;&c_i;&c_v;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </snc>
  <snc mrk="nivel.0i.horizontaligi">
    <dif>
      Fari ion unu<tld/>a, horizontale ebena:
      <ekz>
        <tld/>i vojon, &gcirc;ardenon;
      </ekz>
      <ekz>
        oni instruis la junulojn fari planojn de l' kampoj
        <klr>[&#8230;]</klr> kaj <tld/>i kun helpo de du tabuletoj kaj ortangulilo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i</ind> <klr>(egaligi, samrangigi)</klr> la klasojn sociajn, la kondi&ccirc;ojn.
        <trd lng="de">angleichen</trd>
        <trd lng="nl">gelijkmaken</trd>
        <trdgrp lng="hu">
          <trd>nivell&aacute;l</trd>,
          <trd>kiegyenl&iacute;t</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_u;&c_r;&c_a;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_r;&c_a;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="hu">egyenget</trd>
    <trd lng="nl">nivelleren</trd>
    <trdgrp lng="pl">
      <trd>niwelowa&cacute;</trd>,
      <trd>poziomowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>nivelarea</trd>,
      <trd>nivela</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_p;&c_l;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_k;&c_u; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>aplanar</trd>,
      <trd>nivelar</trd>
    </trdgrp>
  </snc>
  <trd lng="es">nivelar</trd>
  <trd lng="fr">niveler</trd>
  <trdgrp lng="sk">
    <trd>nivelova&tcaron;</trd>,
    <trd>rovna&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_ib;&c_v;&c_je;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_r;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_ib;&c_v;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_r;&c_ib;&c_v;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="nivel.0ilo">
  <kap><tld/>ilo</kap>
