<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>nivel</rad>/o
    </kap>
    
    <drv mrk="nivel.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="nivel.0o.ebeno">
        <dif>
          Horizontala ebeno:
          <ekz>
            mil metrojn alta super la <tld/>o de la oceano
            <fnt><aut>&B;</aut>: <lok>PIV1, p. 739.</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="de">
           <trd>Niveau</trd>,
           <trd>Ebene</trd>,
           <trd>Spiegel</trd>,
           <trd>H&ouml;he</trd>
        </trdgrp>
      </snc>
      <snc mrk="nivel.0o.alto">
        <dif>
          Alto rilate al baza horizontala ebeno:
          <ekz>
            la inundo atingis la <tld/>on de la unua eta&gcirc;o.
          </ekz>
        </dif>
        <subsnc mrk="nivel.0o.FIG">
          <uzo tip="stl">FIG</uzo><dif>
            Grado de io kompare al alia afero:
            <ekz>
              atingi altan <tld/>on;
            </ekz><ekz>
              altigi la <tld/>on de la studoj, de la ekzamenoj;
            </ekz><ekz>
              la morala, ekonomia <tld/>o malalti&gcirc;is;
            </ekz><ekz>
              tiu ekspozicio permesas ekkoni la <tld/>on de la belga
              arto;
            </ekz><ekz>
              la uzado de internacia lingvo metas &ccirc;iujn oratorojn
              sur la <tld/>on de plej kompleta egaleco.
            </ekz>
          </dif>
        </subsnc>
        <trdgrp lng="de">
           <trd>Niveau</trd>,
           <trd>Stand</trd>,
           <trd>H&ouml;he</trd>
        </trdgrp>
      </snc>
      <trd lng="en">level</trd>
      <trdgrp lng="hu">
        <trd>szint</trd>,
        <trd>n&iacute;v&oacute;</trd></trdgrp>
      <trdgrp lng="nl">
        <trd>niveau</trd>,
        <trd>peil</trd>
      </trdgrp>
      <trd lng="pt">n&iacute;vel</trd>
      <trd lng="ru">&c_u;&c_r;&c_o;&c_v;&c_je;&c_n;&c_mol;</trd>
    </drv>
    
    <drv mrk="nivel.0i">
      <kap><tld/>i<fnt>Z</fnt></kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="nivel.0i.GEOD">
        <uzo tip="fak">GEOD</uzo>
        <dif>
          Mezuri diferencon inter <tld/>oj, alton rilate al <tld/>o
        </dif>
        <trdgrp lng="de">
           <trd>einebnen</trd>,
           <trd>nivellieren</trd>
        </trdgrp>
        <trd lng="en">to <ind>level</ind></trd>
        <trdgrp lng="hu">
          <trd>szintez</trd>,
          <trd>szintk&uuml;l&ouml;nbs&eacute;get m&eacute;r</trd></trdgrp>
        <trd lng="nl">peilen</trd>
        <trd lng="ru">&c_n;&c_i;&c_v;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        </snc>
      <snc mrk="nivel.0i.horizontaligi">
        <dif>
          Fari ion unu<tld/>a, horizontale ebena:
          <ekz>
            <tld/>i vojon, &gcirc;ardenon.
          </ekz> 
        </dif>
        <trd lng="hu">egyenget</trd>
        <trd lng="nl">nivelleren</trd>
          <trdgrp lng="ru">
          <trd>&c_s;&c_p;&c_l;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_k;&c_u; &c_i;
          &c_t;.&c_p;.)</klr></trd>,
          <trd>&c_v;&c_y;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd></trdgrp>
        <subsnc mrk="nivel.0i.egaligi">
          <uzo tip="stl">FIG</uzo><dif>
            Egaligi, samrangigi:
            <ekz>
              <tld/>i la klasojn sociajn, la kondi&ccirc;ojn.
            </ekz>
          </dif>
          <trd lng="nl">gelijkmaken</trd>
          <trdgrp lng="hu">
            <trd>nivell&aacute;l</trd>,
            <trd>kiegyenl&iacute;t</trd></trdgrp>
          <trdgrp lng="ru">
            <trd>&c_u;&c_r;&c_a;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>,
            <trd>&c_v;&c_y;&c_r;&c_a;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd></trdgrp>
        </subsnc>
        <trdgrp lng="pt">
            <trd>aplanar</trd>,
            <trd>nivelar</trd>
        </trdgrp>
      </snc>
    </drv>
    
    <drv mrk="nivel.0ilo">
      <kap><tld/>ilo</kap>
      <snc>
        <dif>
          Instrumento por kontroli horizontalecon de io. 
        </dif>
      </snc>
      <trd lng="de">Wasserwaage</trd>
      <trdgrp lng="hu">
        <trd>szintez&odblac;</trd>,
        <trd>v&iacute;zm&eacute;rt&eacute;k</trd></trdgrp>
      <trd lng="nl">waterpas</trd>
      <trdgrp lng="ru">
        <trd>&c_u;&c_r;&c_o;&c_v;&c_je;&c_n;&c_mol;</trd>,
        <trd>&c_v;&c_a;&c_t;&c_je;&c_r;&c_p;&c_a;&c_s;</trd></trdgrp>
    </drv>
    
    <drv mrk="nivel.sam0a">
      <kap>sam<tld/>a</kap>
      <snc mrk="nivel.sam0a.samalta">
        <dif>
          Estanta sur sama <tld/>o. 
        </dif>
        <trd lng="hu">azonos <ind>szint&udblac;</ind></trd>
        <trd lng="nl">op dezelfde <ind>hoogte</ind></trd>
      </snc>
      <snc mrk="nivel.sam0a.samranga">
        <uzo tip="stl">FIG</uzo><dif>
          Estanta &ccirc;e sama rango.  
        </dif>
        <trd lng="hu">azonos <ind>rang&uacute;</ind></trd>
        <trd lng="nl">van gelijke <ind>rang</ind></trd>
      </snc>
        <trd lng="pt">do mesmo <ind>n&iacute;vel</ind></trd>
      <trd lng="ru">&c_t;&c_o;&c_g;&c_o; &c_zh;&c_je; <ind>&c_u;&c_r;&c_o;&c_v;&c_n;&c_ja;</ind></trd>
    </drv>
    
    <drv mrk="nivel.al0igi">
      <kap>al<tld/>igi<fnt>Z</fnt></kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Movi, transformi ion al koncerna <tld/>o:
          <ekz>
            Dio detruis la fortika&jcirc;ojn de Jerusalem, al<tld/>igis
            ilin al la tero<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="hu">egyenl&odblac;v&eacute; tesz<klr tip="ind"> (pl. a
        f&ouml;lddel)</klr></trd>
        <trd lng="nl">met de grond <ind>gelijkmaken</ind></trd>
        <trd lng="pt">nivelar</trd>
        <trd lng="ru">&c_s;&c_r;&c_a;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
      </snc>
    </drv>
    
    <drv mrk="nivel.ocean0o">
      <kap>ocean<tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          La <tld/>o de la oceana surfaco, uzata kiel bazo por indiki
          alton.
        </dif>
        <rim num="1">
          PV, la&ubreve; la nacilingvaj kutimoj, parolas pri &leftquot;mar<tld/>o&rightquot;;
          tio estas konfuza, &ccirc;ar iujn internajn lagojn oni tradicie
          nomas <ctl>maro</ctl>:
          <ekz>
            en 1986 Kaspia maro altis 27,9 metrojn sub la ocean<tld/>o;
          </ekz><ekz>
            en tiu epoko Bakuo malpli altis super la mar<tld/>o.
          </ekz>
        </rim>
        <rim num="2">
          Preciza difino iom varias la&ubreve;lande; &gcirc;enerale temas pri
          mezumo, atentanta la altan kaj malatan tajdojn kaj multjaran
          variadon.
        </rim>
      </snc>
      <trd lng="de">Meeresspiegel</trd>
      <trd lng="en">sea level</trd>
      <trd lng="hu">tengerszint</trd>
      <trd lng="nl">zeespiegel</trd>
      <trd lng="pt">n&iacute;vel do mar</trd>
      <trdgrp lng="ru">
        <trd>&c_u;&c_r;&c_o;&c_v;&c_je;&c_n;&c_mol; &c_m;&c_o;&c_r;&c_ja;</trd>,
        <trd>&c_u;&c_r;&c_o;&c_v;&c_je;&c_n;&c_mol; &c_o;&c_k;&c_je;&c_a;&c_n;&c_a;</trd></trdgrp>
    </drv>
  </art>
