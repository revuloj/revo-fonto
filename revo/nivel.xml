    <dif>
      Instrumento por kontroli horizontalecon de io:
      <ekz>
        se la <tld/>ilo estas en &gcirc;uste vera situo la bobelo estas en centro de la surfaco inter
        specialaj streketoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51558">Nivelilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>libela</trd>,
    <trd>niveliza&ccaron;n&iacute; p&rcaron;&iacute;stroj</trd>,
    <trd>vodov&aacute;ha</trd>
  </trdgrp>
  <trd lng="de">Wasserwaage</trd>
  <trd lng="es">nivel<klr tip="amb"> (instrumento para
  nivelar)</klr></trd>
  <trd lng="fr">niveau<klr> (instrument)</klr></trd>
  <trdgrp lng="hu">
    <trd>szintez&odblac;</trd>,
    <trd>v&iacute;zm&eacute;rt&eacute;k</trd></trdgrp>
  <trd lng="nl">waterpas</trd>
  <trdgrp lng="pl">
    <trd>poziomnica</trd>,
    <trd>niwelator</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>calul-dracului</trd>,
    <trd>libelul&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_r;&c_o;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_t;&c_je;&c_r;&c_p;&c_a;&c_s;</trd></trdgrp>
  <trdgrp lng="sk">
    <trd>nivela&ccaron;n&yacute; pr&iacute;stroj</trd>,
