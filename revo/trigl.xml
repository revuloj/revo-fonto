<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trigl</rad>/o
</kap>
<drv mrk="trigl.0o">
  <kap><tld/>o</kap>
 
  <snc mrk="trigl.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio 
      <ref tip="malprt"
      cel="trigle.0oj"><tld/>edoj</ref>,
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">marfi&scirc;o</ref>
      kun dornoj sur la brankokovriloj
      kaj ru&gcirc;a korpo, povanta murmuri.
    </dif>
  </snc>
  <snc mrk="trigl.0o.genro">   
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="trigle.0oj"><tld/>edoj</ref>
      (<trd lng="la">Trigla</trd>). 
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_r;&c_i;&c_g;&c_l;&c_a;</trd>
  <trd lng="de">Knurrhahn</trd>
  <trdgrp lng="en">
    <trd>gurnard</trd>,
    <trd>sea robin</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grondin</trd>,
    <trd>trigle</trd>
  </trdgrp>
  <trd lng="es">rubios</trd>
  <trd lng="hu">morg&oacute;hal</trd>
  <trd lng="it">pesce capone</trd>
  <trdgrp lng="nl">
    <trd>poon</trd>,
    <trd>knorhaan</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cabrinha</trd>,
    <trd>cabra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_i;&c_g;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_p;&c_je;&c_t;&c_u;&c_h;</trd>
  </trdgrp>
</drv>
</art>
