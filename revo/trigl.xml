<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>trigl</rad>/o
</kap>
<drv mrk="trigl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="trigl.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Trigla</trd>), kun dornoj sur la brankokovriloj
      kaj ru&gcirc;a korpo, povanta murmuri.
    </dif>
  </snc>
  <trd lng="de">Knurrhahn</trd>
  <trdgrp lng="en">
    <trd>gurnard</trd>,
    <trd>sea robin</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grondin</trd>,
    <trd>trigle</trd>
  </trdgrp>
  <trd lng="es">rubios</trd>
  <trd lng="it">pesce capone</trd>
  <trdgrp lng="nl">
    <trd>poon</trd>,
    <trd>knorhaan</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cabrinha</trd>,
    <trd>cabra</trd>
  </trdgrp>
</drv>

</art>

