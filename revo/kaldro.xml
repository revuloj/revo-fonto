<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>kaldron</rad>/o
    </kap>

    <drv mrk="kaldro.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="kaldro.0o.boligilo">
	<dif>
	  Granda metalvazo por kuiri, boligi ktp:
	  <ekz>
	    <uzo tip="stl">FRAZ</uzo>
	    <ind><tld/>o ridas pro poto, kaj mem estas kota</ind>
	    <fnt><bib>PrV</bib></fnt>;
	    <trd lng="ru">&c_n;&c_a; &c_s;&c_je;&c_b;&c_ja; &c_p;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_i;<klr> (&c_p;&c_o;&c_s;&c_l;.)</klr></trd>
	  </ekz><ekz>
	    <uzo tip="stl">FRAZ</uzo>
	    ne batalu pot' el tero kontra&ubreve; <tld/>o el fero
	    <klr>(ne ataku pli fortajn ol vi)</klr>
	    <fnt><bib>PrV</bib></fnt>.
	  </ekz> 
	</dif>
	<trd lng="en">cauldron</trd>
	<trdgrp lng="hu">
	  <trd>&uuml;st</trd>,
	  <trd>bogr&aacute;cs</trd>,
	  <trd>kond&eacute;r</trd>
	</trdgrp>
	<trd lng="nl">ketel</trd>
	<trdgrp lng="ru">
	  <trd>&c_k;&c_o;&c_t;&c_jo;&c_l;</trd>,
	  <trd>&c_b;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_k;&c_i;&c_p;&c_ja;&c_ch;&c_je;&c_n;&c_i;&c_ja;, &c_v;&c_a;&c_r;&c_k;&c_i;)</klr></trd>
	</trdgrp>
	<trd lng="sv">kittel</trd>
      </snc>
      <snc mrk="kaldro.0o.GEOG">
	<uzo tip="fak">GEOG</uzo>
	<dif>
	  Rondvalo. 
	</dif>
	<trd lng="en">hollow</trd>
	<trd lng="hu">v&ouml;lgykatlan</trd>
	<trd lng="nl">keteldal</trd>
      </snc>
      <trd lng="de">Kessel</trd>
    </drv>

    <drv mrk="kaldro.0ego">
      <kap><tld/>ego</kap>
      <snc>
	<uzo tip="fak">TEK</uzo>
	<dif>
	  Granda ujo, kie oni vaporigas akvon, por funkciigi vaporma&scirc;inojn.  
	</dif>
	<rim>
	  Tiu vortformo estas malkonvena por ne tro antikvaj
	  aran&gcirc;a&jcirc;oj, kiuj balda&ubreve; perdis duonsferan formon kaj i&gcirc;is
	  tuba&jcirc;oj (kiel en varmocentraloj).  Tial la formo de la angla
	  termino estas pli rekomendinda kaj ta&ubreve;ga:
	  <ref tip="super" cel="bol.0igilo">boligilo</ref>.
	  <aut>Sergio Pokrovskij</aut>
	</rim>
      </snc>
      <trd lng="en">(steam) <ind>boiler</ind></trd>
      <trd lng="hu">kaz&aacute;n</trd>
      <trd lng="nl">stoomketel</trd>
      <trd lng="ru">&c_k;&c_o;&c_t;&c_jo;&c_l; <klr>(&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
    </drv>
  </art>
