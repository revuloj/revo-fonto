<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaldro.xml,v 1.42 2023/12/02 21:20:04 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kaldron</rad>/o
</kap>

<drv mrk="kaldro.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="kaldro.0o.boligilo">
     <uzo tip="fak">KUI</uzo>
     <dif>
       Granda metala <ref tip="super" cel="vaz.0o">vazo</ref> por kuiri,
       boligi ktp:
       <ekz>
         el la <tld/>o, en kiu sin trovas bolanta akvo, eliras vaporo
         <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
       </ekz>
      <ekz>
        te<tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
       <ekz>
         <uzo tip="stl">FRAZ</uzo>
         <ind><tld/>o ridas pro poto, kaj mem estas kota</ind>
         <trd lng="ru">&c_n;&c_a; &c_s;&c_je;&c_b;&c_ja; 
&c_p;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_i;<klr> 
(&c_p;&c_o;&c_s;&c_l;.)</klr></trd>
         <trd lng="fi">pata kattilaa soimaa<klr> (sananl.)</klr></trd>
         <trd lng="nl">de <ind>pot</ind> verwijt de ketel dat hij zwart
         ziet</trd>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FRAZ</uzo>
         ne batalu pot' el tero kontra&ubreve; <tld/>o el fero
         <klr>(ne ataku pli fortajn ol vi)</klr>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <ctl>La granda <tld/>o</ctl>
         <klr>(romano de John Francis)</klr>
         <fnt><aut>Francis John</aut>, 
            <vrk>La granda kaldrono. &#x2010;
            Antverpeno, La Laguna: Stafeto, 1978</vrk>
         </fnt>;
       </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Ka&ubreve;kazio estis granda <tld/>o, en kiu fandi&gcirc;is popoloj de
        diversaj lingvoj, vivmanieroj kaj historio
        <fnt>
          <bib>Monato</bib>
          <aut>Vja&ccirc;eslav (Slavik) Ivanov</aut>
          <vrk><url ref="&Monato;/2003/007865.html">La oseta
          lingvo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
     </dif>
       <subsnc mrk="kaldro.0o.vaporkaldrono">
         <uzo tip="fak">TEK</uzo>
         <dif>
           Akvujo de vaporma&scirc;ino, <ref tip="dif" cel="kaldro.0ego">kaldronego</ref>:
            <ekz>
              la <tld/>oj de la &scirc;ipo kun mu&gcirc;o kaj bruo ellasas vaporon
              <fnt>
                <bib>FK</bib>
              </fnt>;
            </ekz>
            <ekz>
              vaporma&scirc;ina <tld/>o
              <fnt>
                <bib>Fab3</bib>
                <lok>Kolumo</lok>
              </fnt>;
            </ekz>
         </dif>
         <trd lng="de">Dampfkessel</trd>
       </subsnc>
       <subsnc mrk="kaldro.0o.fermentujo">
         <uzo tip="fak">TEK</uzo>
         <dif>
           Granda metala ujo por varmigi miksa&jcirc;on de akvo, malto kaj
           lupolo
           anta&ubreve; la fermentado al biero:
            <ekz>
              bierfara <tld/>o
              <fnt>
                <bib>Fab2</bib>
                <lok>Ne&gcirc;a re&gcirc;ino</lok>
              </fnt>
              <fnt>
                <bib>Fab2</bib>
                <lok>Abio</lok>
              </fnt>.
            </ekz>
         </dif>
         <trdgrp lng="de">
           <trd>Bierpfanne</trd>,
           <trd>Sudkessel</trd>,
           <trd>Bierkessel</trd>
         </trdgrp>
       </subsnc>
     <trdgrp lng="be">
       <trd>&c_k;&c_a;&c_c;&c_jo;&c_l;</trd>,
       <trd>&c_b;&c_a;&c_k; <klr>(&c_n;&c_a;&c_p;&c_r;., 
&c_d;&c_l;&c_ja; 
&c_k;&c_ib;&c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
     </trdgrp>
     <trd lng="br">kaoter<klr tip="ind"> (kegin)</klr></trd>
     <trdgrp lng="ca">
       <trd>cassola</trd>,
       <trd>olla</trd>
     </trdgrp>
     <trd lng="de">Kessel</trd>
     <trd lng="en">cauldron</trd>
     <trd lng="es">caldero</trd>
     <trdgrp lng="fi">
       <trd>pata</trd>,
       <trd><klr tip="amb">(iso)</klr> kattila</trd>
     </trdgrp>
     <trd lng="fr">chaudron</trd>
     <trdgrp lng="hu">
       <trd>&uuml;st</trd>,
       <trd>bogr&aacute;cs</trd>,
       <trd>kond&eacute;r</trd>
     </trdgrp>
     <trd lng="nl">ketel</trd>
     <trd lng="pt">caldeir&atilde;o</trd>
     <trdgrp lng="ru">
       <trd>&c_k;&c_o;&c_t;&c_jo;&c_l;</trd>,
       <trd>&c_b;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; 
&c_k;&c_i;&c_p;&c_ja;&c_ch;&c_je;&c_n;&c_i;&c_ja;, 
&c_v;&c_a;&c_r;&c_k;&c_i;)</klr></trd>
     </trdgrp>
     <trd lng="sv">kittel</trd>
     <trd lng="th">&#xE2B;&#xE21;&#xE49;&#xE2D;</trd>
   </snc>
   <snc mrk="kaldro.0o.GEOG">
     <uzo tip="fak">GEOG</uzo>
     <dif>
       Ronda valo:
      <ekz>
        la itinero de la unuaj surgrimpintoj de Everesto pasis
        <klr>[&#x2026;]</klr> 
        tra okcident-<tld/>valo, tra la Lhotse-muro &gcirc;is la 
        suda selo kaj de tie tra la sudsupro &gcirc;is la &ccirc;efsupro
        <fnt><aut>E. Georgiev</aut>,
        <vrk><url ref="&Monato;/2000/007116.html">Sir Edmund Hillary: Vivo
        dedi&ccirc;e al
        la &scirc;erpoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/10, p. 18-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        imponaj pejza&gcirc;eroj kiel la monto Fu&jcirc;i kaj <tld/>aj lagoj estas
        kreitaj de vulkanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011447.php">Kiam
          tremetoj fari&gcirc;as tremegoj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_ib;&c_z;&c_ib;&c_n;&c_a;</trd>,
       <trd>&c_l;&c_a;&c_g;&c_ch;&c_y;&c_n;&c_a; 
<klr>(&c_k;&c_r;&c_u;&c_g;&c_l;&c_a;&c_ja;)</klr></trd>
     </trdgrp>
     <trd lng="br">toull<klr tip="ind"> (torosennadur)</klr></trd>
     <trdgrp lng="ca">
       <trd>circ</trd>,
       <trd>caldera</trd>,
       <trd>depressi&oacute;</trd>
     </trdgrp>
     <trd lng="de">Talkessel</trd>
     <trd lng="en">hollow</trd>
     <trd lng="fi">kattilalaakso</trd>
     <trd lng="fr">cuvette<klr tip="ind"> (g&eacute;og.)</klr></trd>
     <trd lng="hu">v&ouml;lgykatlan</trd>
     <trd lng="nl">keteldal</trd>
     <trd lng="ru">&c_k;&c_o;&c_t;&c_l;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>kotel</trd>,
    <trd>kotl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Kessel</trd>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x307E;</trd>,
    <trd>&#x5927;&#x306A;&#x3079;  <pr>&#x3060;&#x3044;&#x306A;&#x3079;</pr></trd>,
    <trd>&#x30AB;&#x30EB;&#x30C7;&#x30E9;</trd>,
    <trd>&#x91DC;&#x72B6;&#x9665;&#x6CA1;  <pr>&#x304B;&#x307E;&#x3058;&#x3087;&#x3046;&#x304B;&#x3093;&#x307C;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bojler</trd>,
    <trd>kad&zacute;</trd>,
    <trd>kocio&lstroke;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>boiler</trd>,
    <trd>cuv&abreve;</trd>
  </trdgrp> 
  <trd lng="sk">kotlina</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_z;&c_a;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x70E7;&#x6C34;&#x58F6; <pr>sh&#x101;oshu&#x1D0;h&#xFA;</pr></trd>,
    <trd>&#x934B; <pr>gu&#x14D;</pr></trd>,
    <trd>&#x944A; <pr>hu&#xF2;</pr></trd>,
    <trd>&#x9B35; <pr>qi&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kaldro.0ego">
   <kap><tld/>ego</kap>
   <uzo tip="fak">TEK</uzo>
   <snc mrk="kaldro.0ego.TEK">
     <fnt><bib>PV</bib></fnt>
     <dif>
       Granda ujo, kie oni vaporigas akvon, por funkciigi 
vaporma&scirc;inojn.
     </dif>
     <rim>
       Tiu vortformo estas malkonvena por ne tro antikvaj
       aran&gcirc;a&jcirc;oj, kiuj balda&ubreve; perdis duonsferan 
formon kaj i&gcirc;is
       tuba&jcirc;oj (kiel en varmocentraloj). Tial la formo de la angla
       termino estas pli rekomendinda kaj ta&ubreve;ga:
       <ref tip="super" cel="bol.0igilo">boligilo</ref>.
       <aut>Sergio Pokrovskij</aut>
     </rim>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_jo;&c_l; 
<klr>(&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;)</klr></trd>
  <trd lng="br">kaoter<klr tip="ind"> (tommerezh)</klr></trd>
  <trd lng="ca">caldera</trd>
  <trd lng="cs">velk&yacute; kotel</trd>
  <trd lng="de">Kessel <klr>(grosser)</klr></trd>
  <trd lng="en">(steam) <ind>boiler</ind></trd>
  <trd lng="es">caldera</trd>
  <trd lng="fi">(h&ouml;yrykoneen) <ind>kattila</ind></trd>
  <trd lng="fr">chaudi&egrave;re</trd>
  <trd lng="hu">kaz&aacute;n</trd>
  <trd lng="ja">&#x30DC;&#x30A4;&#x30E9;&#x30FC;</trd>
  <trd lng="nl">stoomketel</trd>
  <trdgrp lng="pl">
    <trd>bojler</trd>,
    <trd>kad&zacute;</trd>,
    <trd>kocio&lstroke;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>cazan</trd>,
    <trd>c&abreve;ldare</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_jo;&c_l; 
<klr>(&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; 
&c_m;&c_a;&c_sh;&c_i;&c_n;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sk">ve&lcaron;k&yacute; kotol</trd>
  <trd lng="uk">(&c_p;&c_a;&c_r;&c_o;&c_v;&c_i;&c_j;) &c_k;&c_o;&c_t;&c_je;&c_l;</trd>
  <trdgrp lng="zh">
    <trd>&#x934B;&#x7210; <pr>gu&#x14D;l&#xFA;</pr></trd>,
    <trd>&#x91DC; <pr>f&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kaldro.vapor0o">
   <kap>vapor<tld/>o</kap>
   <uzo tip="fak">TEK</uzo>
   <snc>
     <fnt><bib>PIV1</bib></fnt>
     <dif>
       <ref tip="dif" cel="kaldro.0ego"><tld lit="K"/>ego</ref>:
       <ekz>
         en la <ind>vapor<tld/>ejo</ind> <klr>[de la
         elektrostacio]</klr>
         oni klarigas al mi, ke dudek vagonoj da &ccirc;iutage
         venanta karbo estas mal&scirc;ar&gcirc;ataj &ccirc;i
         tie de unu homo dum du kaj duono minutoj
         <fnt><bib>Metrop</bib><lok>&ccirc;ap. 1</lok></fnt>.
         <trd lng="br">kambr ar gaoter</trd>
         <trd lng="fr">chaufferie</trd>
       </ekz>
     </dif>
   <trd lng="en">steam generator</trd>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_jo;&c_l; 
<klr>(&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;)</klr></trd>
  <trd lng="br">kaoter<klr tip="ind"> (tommerezh)</klr></trd>
  <trd lng="ca">caldera</trd>
  <trd lng="cs">parn&iacute; kotel</trd>
  <trd lng="de">Dampfkessel</trd>
  <trd lng="en">(steam) <ind>boiler</ind></trd>
  <trd lng="fi">h&ouml;yrykattila<klr> (tekn.)</klr></trd>
  <trd lng="fr">chaudi&egrave;re</trd>
  <trd lng="ja">&#x84B8;&#x6C17;&#x30DC;&#x30A4;&#x30E9;&#x30FC;  <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x307C;&#x3044;&#x3089;&#x30FC;</pr></trd>
  <trd lng="nl">stoomketel</trd>
  <trd lng="pl">kocio&lstroke; parowy</trd>     
  <trd lng="ro">generator de abur</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; 
<ind>&c_k;&c_o;&c_t;&c_jo;&c_l;</ind></trd>
  <trd lng="sk">parn&yacute; kotol</trd>
  <trd lng="uk">&c_p;&c_a;&c_r;&c_o;&c_v;&c_i;&c_j; &c_k;&c_o;&c_t;&c_je;&c_l;</trd>
  <trd lng="zh">&#x934B;&#x7210; <pr>gu&#x14D;l&#xFA;</pr></trd>
</drv>
</art>
