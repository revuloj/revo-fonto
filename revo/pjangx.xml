<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Pjan&gcirc;</rad>/o
    </kap>

    <drv mrk="pjangx.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Rivero en la
	  <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
	  dislimanta Ta&gcirc;ikujon kaj Afganujon, la maldekstra
	  komponanto de <ref cel="amudarj.0o">Amudarjo</ref>.
	</dif>
      </snc>
      <trd lng="en">Pandj</trd>
      <trd lng="de">Pjandsch</trd>
      <trd lng="fr">Piandj</trd>
      <trd lng="hu">Pjandzs</trd>
      <trd lng="ru">&c_P;&c_ja;&c_n;&c_d;&c_zh;</trd>
    </drv>

  </art>

