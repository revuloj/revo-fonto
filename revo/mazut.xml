<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>mazut</rad>/o</kap>
<drv mrk="mazut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="petrol.0o">Petrola</ref> frakcio, restanta
      post fordistilado de benzino kaj keroseno; uzata por hejtado,
      por pr&c_o;dukti
      <refgrp>
        <ref cel="bitum.0o">bitumojn</ref>,
        <ref cel="koaks.0o">koakson</ref>,
        <ref cel="lubrik.0ajxo">lubrika&jcirc;ojn</ref>
      </refgrp>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_u;&c_t;</trd>
  <trd lng="de">Masut</trd>
  <trd lng="fr">mazout</trd>
  <trdgrp lng="hu">
    <trd>pakura</trd>,
    <trd>mazut</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stookolie</trd>,
    <trd>mazout</trd>
  </trdgrp>
  <trd lng="pt">mazurca</trd>
  <trd lng="ru">&c_m;&c_a;&c_z;&c_u;&c_t;</trd>
</drv>
</art>
