<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kram(p)f</rad>/o
</kap>
<drv mrk="kramf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super">spasmo</ref> kun subita
      kaj dolora kuntiri&gcirc;o de muskoloj.
    </dif>
    <refgrp tip="sin">
      <ref cel="konvul.0o">konvulsio</ref>,
      <ref cel="spasm.0o">spasmo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Krampf</trd>
  <trd lng="en">cramp</trd>
  <trd lng="ru">&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
