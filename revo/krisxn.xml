  <trd lng="be">&c_K;&c_r;&c_y;&c_sh;&c_n;&c_a;</trd>
  <trd lng="fr">Krishna</trd>
  <trdgrp lng="it">
    <trd>K&#x1E5B;&#x1E63;&#x1E47;a</trd>,
    <trd>Krishna</trd>
  </trdgrp>
  <trd lng="nl">Krisjna</trd>
  <trd lng="pl">Kryszna</trd>     
  <trd lng="ro">Krishna</trd>
  <trd lng="uk">&#x41A;&#x440;&#x438;&#x448;&#x43D;&#x430;</trd>
</drv>
