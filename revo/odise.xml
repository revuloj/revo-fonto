<vortaro>
<art mrk="$Id: odise.xml,v 1.23 2024/01/30 16:09:13 revo Exp $">
<kap><rad>Odise</rad>/o <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="odise.0o">
  <kap><tld/>o</kap>
  <snc mrk="odise.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena.
    </dif>
  <trd lng="pt">Ulisses</trd>
  <trd lng="it">Ulisse</trd>
  </snc>
  <snc mrk="odise.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Rolulo de <ref tip="vid" cel="iliad.0o">Iliado</ref>,
      &ccirc;efrolulo de <ref tip="vid" cel="odise.0ado.BELE"><tld/>ado</ref>, helena princo
      lertparola kaj ruzema.
    </dif>
    <trdgrp lng="it">
      <trd>Odisseo</trd>,
      <trd>Ulisse</trd>
    </trdgrp>
    <trd lng="pt">Ulisses</trd>
  </snc>
  <trd lng="be">&c_A;&c_d;&c_y;&c_s;&c_e;&c_j;</trd>
  <trd lng="bg">&c_O;&c_d;&c_i;&c_s;&c_je;&c_j;</trd>
  <trd lng="cs">Odysseus</trd>
  <trd lng="de">Odysseus</trd>
  <trdgrp lng="en">
    <trd>Odysseus</trd>,
    <trd>Ulysses</trd>
  </trdgrp>
  <trd lng="es">Ulises</trd>
  <trd lng="fr">Ulysse</trd>
  <trd lng="hu">Od&uuml;sszeusz</trd>
  <trd lng="ja">&#x30AA;&#x30C7;&#x30E5;&#x30C3;&#x30BB;&#x30A6;&#x30B9;</trd>
  <trd lng="nl">Odysseus</trd>
  <trd lng="pl">Odyseusz</trd>
  <trd lng="ru">&c_O;&c_d;&c_i;&c_s;&c_s;&c_je;&c_j;</trd>
  <trd lng="sk">Odyseus</trd>
  <trd lng="uk">&#x41E;&c_d;&c_ib;&c_s;&c_s;&c_je;&c_j; (&c_c;&c_a;&c_r;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5965;&#x5FB7;&#x4FEE;&#x65AF; <pr>&#xC0;od&#xE9;xi&#x16B;s&#x12B;</pr></trd>,
    <trd>&#x5967;&#x5FB7;&#x4FEE;&#x65AF; <pr>&#xC0;od&#xE9;xi&#x16B;s&#x12B;</pr></trd>,
    <trd>&#x5965;&#x8FEA;&#x4FEE;&#x65AF; <pr>&#xC0;od&#xED;xi&#x16B;s&#x12B;</pr></trd>,
    <trd>&#x5967;&#x8FEA;&#x4FEE;&#x65AF; <pr>&#xC0;od&#xED;xi&#x16B;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="odise.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="odise.0ado.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Antikva greklingva epopeo, sekvo de <ref tip="vid" cel="iliad.0o">Iliado</ref>, rakontanta la
      aventuran
      hejmen&scirc;ipadon de <tld/>o post la milito kontra&ubreve; Trojo,
      atribuita al
      <ref tip="vid" cel="homer.0o">Homero</ref>.
    </dif>
  </snc>
  <snc mrk="odise.0ado.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Longa, aventurplena voja&gcirc;ado:
      <ekz>
        lia kvara kaj lasta libro en la serio Spac-<tld/>ado &jcirc;us
        aperis:
        <ctl>3001, La lasta <tld/>ado</ctl>
        <fnt>
        <aut>Anne Tilmont</aut> <vrk><url ref="&Monato;/1998/005932.php">Kosmopolita civitano kun
        okulo al la universo</url></vrk>
        <bib>Monato</bib>
        <lok>1998/04, p. 9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_y;&c_s;&c_e;&c_ja;</trd>
  <trd lng="bg">&c_O;&c_d;&c_i;&c_s;&c_je;&c_ja;</trd>
  <trd lng="cs">Ilias a Odyssea</trd>
  <trd lng="de">Odyssee</trd>
  <trd lng="en">Odyssey</trd>
  <trd lng="es">Odisea</trd>
  <trd lng="fr">Odyss&eacute;e</trd>
  <trd lng="hu">Od&uuml;sszeia</trd>
  <trd lng="it">Odissea</trd>
  <trd lng="ja">&#x300E;<ind>&#x30AA;&#x30C7;&#x30E5;&#x30C3;&#x30BB;&#x30A4;&#x30A2;</ind>&#x300F;</trd>
  <trd lng="nl">Odysseia</trd>
  <trd lng="pl">Odyseja</trd>
  <trd lng="pt">Odiss&eacute;ia</trd>
  <trd lng="ru">&c_o;&c_d;&c_i;&c_s;&c_s;&c_je;&c_ja;</trd>
  <trd lng="sk">Ilias a Odyssea</trd>
  <trd lng="uk">&#x41E;&c_d;&c_ib;&c_s;&c_s;&c_je;&#x44F; (&#x43F;&#x43E;&c_je;&#x43C;&c_a; &c_G;&#x43E;&#x43C;&c_je;&c_r;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5965;&#x5FB7;&#x8D5B; <pr>&#xE0;od&#xE9;s&#xE0;i</pr></trd>,
    <trd>&#x5967;&#x5FB7;&#x8CFD; <pr>&#xE0;od&#xE9;s&#xE0;i</pr></trd>,
    <trd>&#x5965;&#x5FB7;&#x8D5B; <pr>&#xC0;od&#xE9;s&#xE0;i</pr></trd>,
    <trd>&#x5967;&#x5FB7;&#x8CFD; <pr>&#xC0;od&#xE9;s&#xE0;i</pr></trd>,
    <trd>&#x9577;&#x9014;&#x6F02;&#x6CCA; <pr>ch&#xE1;ngt&#xFA;pi&#x101;ob&#xF3;</pr></trd>,
    <trd>&#x957F;&#x9014;&#x6F02;&#x6CCA; <pr>ch&#xE1;ngt&#xFA;pi&#x101;ob&#xF3;</pr></trd>
  </trdgrp>
</drv>
</art>
