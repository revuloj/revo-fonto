<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: toks.xml,v 1.13 2024/02/24 21:19:38 revo Exp $">
    <kap>
      <rad>toks</rad>/a
    </kap>

<drv mrk="toks.0a">
  <kap><tld/>a</kap>
  <snc mrk="toks.0a.MED">
    <uzo tip="fak">MED</uzo><uzo tip="fak">BIO</uzo>
    <dif>
      <ref tip="dif" cel="venen.0a.MED">Venena</ref>:
      <ekz>
        konteneroj senditaj el Japanio enhavis <klr>[&#8230;]</klr> <tld/>ajn
        kaj aliajn
        dan&gcirc;erajn rubojn
        <fnt>
       <aut>HORI Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2000/006893.php">Rubo-ruzo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj dan&gcirc;eraj <tld/>aj materialoj dan&gcirc;erigas la la&ubreve;bordajn
        regionojn
        <fnt><aut>Katalin Smid&eacute;liusz</aut>:
          <vrk><url ref="&Monato;/2000/006912.php">Cinike cianide -- &ccirc;u respondecas
          Okcidento?</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/04, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        multe da <tld/>aj gasoj povas deflui la&ubreve; la flanko de la vulkano
        <fnt><aut>Evgeni Georgiev</aut>:
          <vrk><url ref="&Monato;/2001/007234.html">Vulkanoj</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/02, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="toksolog.0o">toksologo</ref>,
       <ref cel="toksologi.0o">toksologio</ref>
    </refgrp>
    <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trd lng="de">toxisch</trd>
    <trd lng="en">toxic</trd>
    <trd lng="fr">toxique</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x6BD2;&#x306E;  <pr>&#12393;&#x304F;&#x306E;</pr></trd>,
    <trd>&#26377;&#x6BD2;&#x306A;  <pr>&#12422;&#12358;&#12393;&#x304F;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>toksyczny</trd>,
    <trd>truj&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&#x43A;&c_s;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43E;&c_t;&c_r;&c_u;&c_j;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="toks.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="toks.0ajxo.MED">
  <uzo tip="fak">MED</uzo><uzo tip="fak">BIO</uzo>
  <dif>
      <tld lit="T"/>a substanco, <ref tip="sin" cel="venen.0o">veneno</ref>:
      <ekz>
        en la konteneroj trovi&gcirc;as diversaj ruboj, inkluzive de
        <tld/>a&jcirc;oj
        <fnt>
       <aut>HORI Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2000/006893.php">Rubo-ruzo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la&ubreve;dire li]</klr> posedas ne nur gasojn kaj aliajn
        <tld/>a&jcirc;ojn sed anka&ubreve;
        armilojn por amasdetruado
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2003/008109.html">Taglibro de l'
          milito ... el la lando de &Scirc;ekspiro (3)</url></vrk>
        </fnt>.
      </ekz>
      </dif>
  </snc>
  <rim>
      Ne konfuzu kun <ref cel="toksin.0o">toksino</ref>, t.e. biologie
      sintezita veneno,
      kaj evitu uzi la vorton &leftquot;<tld/>o&rightquot; tiusence:
      <ekz>
        algoj anka&ubreve; eligas sulfuran hidrogenon kaj aliajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2008/009281.php">Algoj
          atakas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li kuiris kaj man&gcirc;is ru&gcirc;an mu&scirc;fungon, nenio malbona okazis, tial
        li konkludis,
        ke rakontoj pri <tld/>oj mortigaj estas nuraj elpensa&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/010185.php">Malbonan
          apetiton</url></vrk>
        </fnt>.
      </ekz>
  </rim>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
  <trd lng="de">Giftstoff</trd>
  <trd lng="fr">toxine</trd>
  <trd lng="ja">&#x6BD2;&#29289;  <pr>&#12393;&#x304F;&#12406;&#12388;</pr></trd>
  <trdgrp lng="pl">
    <trd>trucizna</trd>,
    <trd>narkotyk</trd>
  </trdgrp>
</drv>

<drv mrk="toks.sen0igo">
  <kap>
    mal<tld/>igo, <var><kap>sen<tld/>igo</kap></var>
  </kap>
  <snc mrk="toks.sen0igo.MED">
  <uzo tip="fak">MED</uzo><uzo tip="fak">BIO</uzo>
    <dif>
      Malvenenigo:
      <ekz>
        mal<tld/>igo de la organismo
        <fnt>
          <vrk><url
          ref="http://aztekium.pl/word.py?site=wiki&amp;xlang=eo&amp;szukaj=hepato&amp;lang=de">Wiki
          Wort hepato</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la graveco de tiu planto estas precipe en la preventado kaj
        <klr>[&#8230;]</klr> en la sen<tld/>igo de la organismo
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010553.php">Topinamburo &ccirc;u
          herba&ccirc;o?</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_t;&c_a;&c_k;&c_s;&c_y;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Entgiftung</trd>,
    <trd>Detoxifikation</trd>,
    <trd>Entzug</trd>
  </trdgrp>
  <trd lng="en">detox</trd>
  <trd lng="fr">d&eacute;sintoxication</trd>
  <trdgrp lng="pl">
    <trd>odtruwanie</trd>,
    <trd>detoksykacja</trd>
  </trdgrp>
</drv>
  </art>

