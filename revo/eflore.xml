<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eflore.xml,v 1.23 2023/11/07 19:22:22 revo Exp $">
<kap>
  <rad>efloresk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="eflore.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eflore.0i.MIN">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Pulvori&gcirc;i, <ref cel="krista.0igxi">kristali&gcirc;i</ref> sur la supra&jcirc;o de substanco:
      <ekz>
        kun tempopaso <tld/>is sablo&scirc;tona tavolo
        <fnt>
          <aut>Pang Naiying</aut>,
          <vrk><url ref="http://www.espero.com.cn/2014-07/11/content_32923801.htm">
          Plej okcidentaj pasejoj</url></vrk>,
          <lok>2014-07-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld/>inta &scirc;tonviza&gcirc;o <klr>[&#x2026;]</klr> strange malgranda situis super la
        potencaj &scirc;ultroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erozi.0igxi">erozii&gcirc;i, erodi&gcirc;i</ref>,
      <ref cel="korod.0i.KEM">korodi&gcirc;i</ref>,
      <ref cel="sedime.0igxi">sedimenti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">zv&ecaron;tr&aacute;vat <klr>(o hornin&ecaron;)</klr></trd>
  <trdgrp lng="de">
    <trd>effloreszieren</trd>,
    <trd>ausbl&uuml;hen</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&f_k;&a_f;&a_t;&a_n;<klr tip="ind"> (&a_sh;&f_y;&a_m;&f_y;)</klr></trd>,
    <trd>&a_sh;&a_w;&a_r;&a_h; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">effleurir</trd>
  <trd lng="hu">kivir&aacute;gzik<klr tip="ind"> (pl. sal&eacute;trom)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x98A8;&#x89E3;&#x3059;&#x308B;  <pr>&#x304B;&#x305C;&#x304B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x98A8;&#x5316;&#x3059;&#x308B;  <pr>&#x3075;&#x3046;&#x304B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verweren</trd>
  <trd lng="pt">eflorescer</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">zvetr&aacute;va&tcaron; <klr>(na povrchu)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_t;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o; &c_p;&c_o;&c_r;&c_o;&c_d;&c_u;)</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_a;&c_d;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o; &c_s;&c_o;&c_l;&c_ib;)</trd>
  </trdgrp>
</drv>

<drv mrk="eflore.0o">
  <kap><tld/>o</kap>
  <snc mrk="eflore.0o.MIN">
    <dif>
      Fenomeno <tld/>i:
      <ekz>
        pro longtempa senuzo kaj <tld/>o, &ccirc;e la konstrua&jcirc;oj de la kastelo <klr>[&#x2026;]</klr>
        muroj krevis dum la pasintaj dekoj da jaroj
        <fnt>
          <aut>Xie Ruifeng</aut>
          <vrk><url ref="http://www.espero.com.cn/2015-10/15/content_36819444.htm">Finita rebonigo de la kastelo &ccirc;e la pasejo Jiayu en Gansu, &Ccirc;inio</url></vrk>
          <lok>El Popola &Ccirc;inio,  2015-10-15 </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>vyr&aacute;&zcaron;ka</trd>,
    <trd>zarudnut&iacute; k&uring;&zcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Effloreszieren</trd>,
    <trd>Ausbl&uuml;hen</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&f_k;&a_f;&a_t;<klr tip="ind"> (&a_sh;&f_y;&a_m;&f_y;)</klr></trd>
  <trd lng="fr">efflorescence</trd>
  <trdgrp lng="hu">
    <trd>kivir&aacute;gz&aacute;s<klr tip="ind"> (vegyi)</klr></trd>,
    <trd>effloreszcencia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x98A8;&#x89E3;  <pr>&#x3075;&#x3046;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x98A8;&#x5316;  <pr>&#x3075;&#x3046;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verwering</trd>
  <trd lng="pt">efloresc&ecirc;ncia</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">zvetr&aacute;vanie</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_t;&c_r;&c_ju;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_je;&c_f;&c_l;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x5168;&#x76DB;&#x671F; <pr>qu&#xE1;nch&#xE9;ngq&#x12B;</pr></trd>
</drv>

<drv mrk="eflore.0a">
  <kap><tld/>a</kap>
  <snc mrk="eflore.0a.MIN">
    <dif>
      Rilata al <tld/>ado:
      <ekz>
        la pozitiva <frm>Ce</frm>-anomalio estas perceptebla nur en la supra parto de la <tld/>a
        profilo
        <fnt>
          <aut>V&aacute;clav Proch&aacute;zka</aut>
          <vrk><url ref="http://www.eventoj.hu/steb/geologio/etr/etr-en-marsedimentoj.htm">ETR en mar-sedimentoj </url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>
  <trd lng="cs">zv&ecaron;tral&yacute;</trd>
  <trdgrp lng="de">
    <trd>effloreszierend</trd>,
    <trd>ausbl&uuml;hend</trd>
  </trdgrp>
  <trd lng="fr">efflorescent</trd>
  <trdgrp lng="ja">
    <trd>&#x98A8;&#x89E3;&#x306E;  <pr>&#x304B;&#x305C;&#x304B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x98A8;&#x89E3;&#x3057;&#x305F;  <pr>&#x304B;&#x305C;&#x304B;&#x3044;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x98A8;&#x5316;&#x306E;  <pr>&#x3075;&#x3046;&#x304B;&#x306E;</pr></trd>,
    <trd>&#x98A8;&#x5316;&#x3057;&#x305F;  <pr>&#x3075;&#x3046;&#x304B;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwerend</trd>,
    <trd>uitslaand</trd>
  </trdgrp>
  <trd lng="pt">eflorescente</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  <trd lng="sk">zvetran&yacute;</trd>
  <trd lng="uk">&c_v;&c_i;&c_v;&c_ib;&c_t;&c_r;&c_je;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
