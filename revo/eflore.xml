<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eflore.xml,v 1.18 2022/03/16 15:54:27 revo Exp $">
<kap>
  <rad>efloresk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="eflore.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eflore.0i.MIN">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Pulvori&gcirc;i sur la supra&jcirc;o de substanco:
      <ekz>
        kun tempopaso <tld/>is sablo&scirc;tona tavolo
        <fnt>
          <aut>Pang Naiying</aut>,
          <vrk><url ref="http://www.espero.com.cn/2014-07/11/content_32923801.htm">
          Plej okcidentaj pasejoj</url></vrk>,
          <lok>2014-07-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">zv&ecaron;tr&aacute;vat <klr>(o hornin&ecaron;)</klr></trd>
  <trdgrp lng="de">
    <trd>effloreszieren</trd>,
    <trd>ausbl&uuml;hen</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&f_k;&a_f;&a_t;&a_n;<klr tip="ind"> (&a_sh;&f_y;&a_m;&f_y;)</klr></trd>,
    <trd>&a_sh;&a_w;&a_r;&a_h; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">effleurir</trd>
  <trd lng="hu">kivir&aacute;gzik<klr tip="ind"> (pl. sal&eacute;trom)</klr></trd>
  <trd lng="nl">verweren</trd>
  <trd lng="pt">eflorescer</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">zvetr&aacute;va&tcaron; <klr>(na povrchu)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x432;&#x456;&#x442;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F; (&#x43F;&#x440;&#x43E; &#x43F;&#x43E;&#x440;&#x43E;&#x434;&#x443;)</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x430;&#x434;&#x430;&#x442;&#x438;&#x441;&#x44F; (&#x43F;&#x440;&#x43E; &#x441;&#x43E;&#x43B;&#x456;)</trd>
  </trdgrp>
</drv>

<drv mrk="eflore.0o">
  <kap><tld/>o</kap>
  <snc mrk="eflore.0o.MIN">
    <dif>
      Fenomeno <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>vyr&aacute;&zcaron;ka</trd>,
    <trd>zarudnut&iacute; k&uring;&zcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Effloreszieren</trd>,
    <trd>Ausbl&uuml;hen</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&f_k;&a_f;&a_t;<klr tip="ind"> (&a_sh;&f_y;&a_m;&f_y;)</klr></trd>
  <trd lng="fr">efflorescence</trd>
  <trdgrp lng="hu">
    <trd>kivir&aacute;gz&aacute;s<klr tip="ind"> (vegyi)</klr></trd>,
    <trd>effloreszcencia</trd>
  </trdgrp>
  <trd lng="nl">verwering</trd>
  <trd lng="pt">efloresc&ecirc;ncia</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">zvetr&aacute;vanie</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x432;&#x456;&#x442;&#x440;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x430;&#x434;</trd>,
    <trd>&#x435;&#x444;&#x43B;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="eflore.0a">
  <kap><tld/>a</kap>
  <snc mrk="eflore.0a.MIN">
    <dif>
      <tld lit="E"/>anta.
    </dif>
  </snc>
  <trd lng="cs">zv&ecaron;tral&yacute;</trd>
  <trdgrp lng="de">
    <trd>effloreszierend</trd>,
    <trd>ausbl&uuml;hend</trd>
  </trdgrp>
  <trd lng="fr">efflorescent</trd>
  <trdgrp lng="nl">
    <trd>verwerend</trd>,
    <trd>uitslaand</trd>
  </trdgrp>
  <trd lng="pt">eflorescente</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  <trd lng="sk">zvetran&yacute;</trd>
  <trd lng="uk">&#x432;&#x438;&#x432;&#x456;&#x442;&#x440;&#x435;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
