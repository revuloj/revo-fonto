<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>glan</rad>/o
</kap>
<drv mrk="glan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="glan.0o.BOT">
    <dif>
      La <ref tip="super" cel="nuks.0o.BOT">nukso</ref>
      de la <ref tip="malprt" cel="kverk.0o">kverko</ref>.
    </dif>
  </snc>
  <snc mrk="glnn.0o.ANA">
    <dif>
      La supra parto de la 
      <ref tip="malprt" cel="penis.0o">peniso</ref>.
    </dif>
  </snc>
  <trd lng="de">Eichel</trd>
  <trd lng="fr">gland</trd>
</drv>
<drv mrk="glan.0ingo">
  <kap><tld/>ingo</kap>
  <snc>
    <dif>
      La kupuleto, en kiu sidas la <ref tip="malprt"
      cel="glan.0o.BOT">glano</ref>.
    </dif>
  </snc>
</drv>
</art>

