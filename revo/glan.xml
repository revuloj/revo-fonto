<drv mrk="glan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="glan.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="malprt" cel="kverk.0o">Kverko</ref><ref tip="super" cel="nuks.0o.BOT">nukso</ref>:
      <ekz>
        &ccirc;iujare mi <klr>[&#8230;]</klr> plantas semojn, <tld/>ojn ktp
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006878.html">Dommastrumo: &Gcirc;ardenumi</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la kolombo per la sama gor&gcirc;o, kiu tiel &leftquot;delikate kantas&rightquot;,
        povas engluti e&ccirc; plej grandan pecon da <tld/>o
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">Slavonia
          Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_zh;&c_o;&c_l;&c_u;&c_d;</trd>
    <trdgrp lng="ca">
      <trd>gla</trd>,
      <trd>bellota</trd>
    </trdgrp>
    <trd lng="en">acorn</trd>
    <trd lng="hu">makk</trd>
    <trd lng="it">ghianda</trd>
    <trd lng="ja">&#x56E3;&#26647; [<ind>&#12393;&#12435;&#12368;&#x308A;</ind>]</trd>
    <trd lng="pl">&zdot;o&lstroke;&aogonek;d&zacute;</trd>
    <trdgrp lng="pt">
      <trd>glande</trd>,
      <trd>bolota</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_jo;&c_l;&c_u;&c_d;&c_mol;</trd>
  </snc>
  <snc mrk="glan.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Vaskula korpparto, kiu formas la finan parton de la <ref tip="malprt"
      cel="seks.0organo">seksorgano</ref>, per kiu oni &ccirc;efe
      sentas <ref tip="vid" cel="seks.0a.FIZL">seksan<sncref/></ref>
      <ref tip="vid" cel="plezur.0o">plezuron</ref>.
    </dif>
    <subsnc mrk="glan.0o.penisa">
      <dif>
        Sentema fina parto de la <ref tip="malprt" cel="penis.0o">peniso</ref>, kovrita de la <ref
        cel="prepuc.0o.penisa">prepucio<sncref/></ref>.
      </dif>
      <refgrp tip="sin">
        <ref cel="balan.0o.ANA">balano</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_zh;&c_o;&c_l;&c_u;&c_d;</trd>,
        <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a; <klr tip="amb">(&c_p;&c_e;&c_n;&c_ib;&c_s;&c_a;)</klr></trd>
      </trdgrp>
      <trd lng="de">Peniseichel</trd>
      <trd lng="en">glans penis</trd>
      <trd lng="fy">n&uacute;t</trd>
      <trd lng="hu">h&iacute;mvessz&odblac;makk</trd>
      <trd lng="iu">&#5125;&#x14D5;&#x155D;&#5465;&#5509;</trd>
      <trd lng="ja">&#38512;&#x830E;&#x4E80;&#x982D; [<ind>&#12356;&#12435;&#12369;&#12356;&#x304D;&#12392;&#12358;</ind>]</trd>
      <trd lng="lt">varpos galvut&#279;</trd>
      <trd lng="pl">&zdot;o&lstroke;&aogonek;d&zacute;</trd>
      <trd lng="sk">&zcaron;alu&dcaron; penisu</trd>
      <trd lng="tr">penis ba&scedil;&inodot;</trd>
    </subsnc>
    <subsnc mrk="glan.0o.klitora">
      <dif>
        Sentema fina parto de la <ref tip="malprt" cel="klitor.0o">klitoro</ref>, kovrita de la <ref
        cel="prepuc.0o.klitora">prepucio<sncref/></ref>.
      </dif>
      <trd lng="de">Klitoriseichel</trd>
      <trdgrp lng="en">
        <trd>clitoral glans</trd>,
        <trd>glans clitoridis</trd>
      </trdgrp>
      <trd lng="ja">&#38512;&#26680;&#x4E80;&#x982D; [<ind>&#12356;&#12435;&#x304B;&#x304F;&#x304D;&#12392;&#12358;</ind>]</trd>
      <trd lng="tr">klitoris ba&scedil;&inodot;</trd>
    </subsnc>
    <trd lng="ca">gland</trd>
    <trd lng="en">glans</trd>
    <trd lng="es">glande</trd>
    <trd lng="fi">p&auml;&auml;</trd>
    <trd lng="it">glande</trd>
    <trd lng="ja">&#x4E80;&#x982D; [<ind>&#x304D;&#12392;&#12358;</ind>]</trd>
    <trd lng="lt">varput&#279;s galvut&#279;</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
    <trd lng="tr">ba&scedil;&inodot;</trd>
  </snc>
  <trd lng="de">Eichel</trd>
  <trd lng="fr">gland<klr tip="ind"> (t.s.)</klr></trd>
  <trd lng="io">glano</trd>
  <trd lng="lt">galvut&#279;</trd>
  <trd lng="nl">eikel</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&#x43E;&#x43B;&c_u;&c_d;&#x44C;</trd>,
    <trd>&c_g;&#x43E;&#x43B;&#x43E;&c_v;&#x43A;&c_a; &c_s;&c_t;&c_a;&c_t;&c_je;&c_v;&#x43E;&c_g;&#x43E; &c_ch;&#x43B;&c_je;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="glan.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="glan.0ingo.BOT">
    <dif>
      Kupuleto, en kiu sidas la <ref tip="malprt" cel="glan.0o.BOT">glano<sncref/></ref> de kverko.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_s;&c_k;&c_a; <klr>(&c_zh;&c_o;&c_l;&c_u;&c_d;&c_a;)</klr></trd>
  <trd lng="ca">c&uacute;pula</trd>
  <trd lng="en">acorn cup</trd>
  <trd lng="fr">cupule du gland</trd>
  <trdgrp lng="hu">
    <trd>kupacs</trd>,
    <trd>makkcs&eacute;sze</trd>
  </trdgrp>
  <trd lng="it">cupola<klr> della ghianda</klr></trd>
  <trd lng="nl">eikeldop</trd>
  <trd lng="ru">&c_ch;&c_a;&c_sh;&c_je;&c_ch;&c_k;&c_a; &c_zh;&c_jo;&c_l;&c_u;&c_d;&c_ja;</trd>
  <trd lng="uk">&c_ch;&c_a;&c_sh;&c_je;&c_ch;&#x43A;&c_a; &c_zh;&#x43E;&#x43B;&c_u;&c_d;&#x44F;</trd>
</drv>

<drv mrk="glan.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="glan.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="balan1.0o">balano</ref>
  </snc>
  <trd lng="cs">balanus</trd>
  <trd lng="en">Balanus</trd>
  <trd lng="fr">balane</trd>
  <trd lng="nl">Balanus</trd>
  <trd lng="pl">p&aogonek;kle</trd>
  <trd lng="sk">morsk&yacute; &zcaron;alu&dcaron;</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j; &c_zh;&#x43E;&#x43B;&c_u;&c_d;&#x44C;</trd>
</drv>
</art>
