<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="glan.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="malprt" cel="kverk.0o">Kverko</ref><ref tip="super"
      cel="nuks.0o.BOT">nukso</ref>:
      <ekz>
        la kolombo per la sama gor&gcirc;o, kiu tiel &leftquot;delikate kantas&rightquot;,
        povas engluti e&ccirc; plej grandan pecon da <tld/>o
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url
          ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">Slavonia
          Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_zh;&c_o;&c_l;&c_u;&c_d;</trd>
    <trdgrp lng="ca">
      <trd>gla</trd>,
      <trd>bellota</trd>
    </trdgrp>
    <trd lng="en">acorn</trd>
    <trd lng="hu">makk</trd>
    <trd lng="it">ghianda</trd>
    <trd lng="pl">&zdot;o&lstroke;&aogonek;d&zacute;</trd>
    <trdgrp lng="pt">
      <trd>glande</trd>,
      <trd>bolota</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_jo;&c_l;&c_u;&c_d;&c_mol;</trd>
  </snc>
  <snc mrk="glan.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Sentema fina parto de la <ref tip="malprt"
      cel="penis.0o">peniso</ref>, kovrita de la <ref
      cel="prepuc.0o">prepucio</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="balan.0o.ANA">balano</ref>
    </refgrp>
    <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a; <klr tip="amb">(&c_p;&c_e;&c_n;&c_ib;&c_s;&c_a;)</klr></trd>
    <trd lng="ca">gland</trd>
    <trd lng="en">glans</trd>
    <trd lng="es">glande</trd>
    <trd lng="fy">n&uacute;t</trd>
    <trd lng="hu">h&iacute;mvessz&odblac;makk</trd>
    <trd lng="it">glande</trd>
    <trd lng="iu">&#5125;&#5333;&#5469;&#5465;&#5509;</trd>
    <trd lng="pl">&zdot;o&lstroke;&aogonek;d&zacute;</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>balanus</trd>,
    <trd>&zcaron;alud <klr>(penisu)</klr></trd>
  </trdgrp>
  <trd lng="de">Eichel</trd>
  <trd lng="fr">gland<klr tip="ind"> (t.s.)</klr></trd>
  <trd lng="io">glano</trd>
  <trd lng="nl">eikel</trd>
  <trd lng="sk">&zcaron;alu&dcaron; penisu</trd>
</drv>

<drv mrk="glan.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="glan.0ingo.BOT">
    <dif>
      Kupuleto, en kiu sidas la <ref tip="malprt"
      cel="glan.0o.BOT">glano</ref> de kverko.
    </dif>
  </snc>
  <trd lng="ca">c&uacute;pula</trd>
  <trd lng="en">acorn cup</trd>
  <trd lng="fr">cupule du gland</trd>
  <trdgrp lng="hu">
    <trd>kupacs</trd>,
    <trd>makkcs&eacute;sze</trd>
  </trdgrp>
  <trd lng="it">cupola<klr> della ghianda</klr></trd>
  <trd lng="nl">eikeldop</trd>
  <trd lng="ru">&c_ch;&c_a;&c_sh;&c_je;&c_ch;&c_k;&c_a; &c_zh;&c_jo;&c_l;&c_u;&c_d;&c_ja;</trd>
</drv>

<drv mrk="glan.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="glan.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="balan.0o.ZOO">balano</ref>
  </snc>
  <trd lng="cs">balanus</trd>
  <trd lng="en">Balanus</trd>
  <trd lng="fr">balane</trd>
  <trd lng="nl">Balanus</trd>
  <trd lng="pl">p&aogonek;kle</trd>
  <trd lng="sk">morsk&yacute; &zcaron;alu&dcaron;</trd>
</drv>
</art>
