<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dorn</rad>/o
</kap>
<drv mrk="dorn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Natura pikilo de vegeta&jcirc;oj:
      <ekz>
        neniu rozo sen <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Pikilo simila al <tld/>o:
      <ekz>
        buko<tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Malhelpa&jcirc;o, malplezuro. 
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Dorn</trd>, <trd>Stachel</trd></trdgrp>
</drv>
<drv mrk="dorn.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Plena je <tld/>oj:
      <ekz>
        kiel <tld/>a kano en la mano de ebriulo, tiel
        estas sentenco en la bu&scirc;o de malsa&gcirc;uloj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 26, 9</lok>)</fnt>
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Plena je malhelpa&jcirc;oj k malplezuroj. 
    </dif>
  </snc>
  <trd lng="de">dornig</trd>
</drv>
<drv mrk="dorn.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <tld/>arbetaro.  
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>