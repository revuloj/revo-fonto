<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dorn.xml,v 1.28 2024/12/20 16:33:06 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>dorn</rad>/o
</kap>
<drv mrk="dorn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dorn.0o.natura">
    <dif>
      Natura <ref tip="super" cel="pik.0ilo.ZOO">pikilo</ref> de vegeta&jcirc;oj:
      <ekz>
        la malfeli&ccirc;ulo &scirc;ajnis sidi sur <tld/>oj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabino intence pikis siajn fingrojn kaj disgratis la manon per <tld/>o por
        makuli la bobenon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La maljunulino Holle</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu per mallerta mano volis kapti rozon, tiu ne havas la rajton plendi, ke la
        <tld/>oj lin pikis
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>!
      </ekz>
      <ekz>
        <klr>[li]</klr> vidas la <tld/>on en la karno
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ri&ccirc;ulo havas kornojn, malri&ccirc;ulo <tld/>ojn
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="split.0o">splito</ref>
    <trd lng="es">espina</trd>
    <trdgrp lng="fr">
      <trd>&eacute;pine</trd>,
      <trd>piquant<klr tip="ind"> (&eacute;pine)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kolec</trd>,
      <trd>cier&nacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_i;&c_p;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dorn.0o.plantoj">
    <uzo tip="klr">(plurale)</uzo>
    <dif>
      <tld/>havaj plantoj:
      <ekz>
        kiel rozo inter la <tld/>oj, tiel estas mia amatino inter la knabinoj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        simila al la kraketado de <tld/>oj sub poto estas la ridado de la malsa&gcirc;uloj
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 7:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi dra&scirc;os viajn korpojn per <tld/>oj de dezerto kaj per kardoj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 8:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas kovrita de <tld/>oj, kaj la &scirc;tona muro estas detruita
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 24:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ekkuris inter pintaj &scirc;tonoj kaj <tld/>oj, kaj la sova&gcirc;aj bestoj saltis preter
        &scirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni baraktis anta&ubreve;en preter <tld/>oj kaj urtikoj
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vepr.0o">vepro</ref>
    <trd lng="de">Dornengestr&uuml;pp</trd>
  </snc>
  <snc mrk="dorn.0o.objekto">
    <dif>
      <ref tip="super" cel="pik.0ilo.TEK">Pikilo</ref> simila al <tld/>o:
      <ekz>
        <ind>buko<tld/>o</ind>.
        <trd lng="fr">ardillon</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="najl.0o">najlo</ref>,
      <ref cel="pingl.0o.tajlora">pinglo</ref>,
      <ref cel="pivot.0o">pivoto</ref>,
<ref cel="stift.0o">stifto</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>aiguillon</trd>,
      <trd>pointe<klr tip="ind"> (aiguillon)</klr></trd>
    </trdgrp>
    <trd lng="pl">szpilka</trd>
    <trd lng="ru">&c_sh;&c_p;&c_je;&c_n;&c_jo;&c_k;</trd>
  </snc>
  <snc mrk="dorn.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="gxen.0ajxo">&Gcirc;ena&jcirc;o</ref>, malplezuro:
      <ekz>
        <tld/>o en la flanko de la lo&gcirc;antoj estis Pigra Pego, kiu <klr>[&#8230;]</klr> insiste
        brua&ccirc;is &ccirc;iun nokton
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la la&ubreve;roj, venis anka&ubreve; <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011569.php">Revuon redakti ... kaj ranoj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>cactus<klr tip="ind"> (difficult&eacute;)</klr></trd>,
      <trd>os<klr tip="ind"> (difficult&eacute;)</klr></trd>,
      <trd>&eacute;cueil<klr tip="ind"> (difficult&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="pl">zagwozdka</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_n;&c_o;&c_z;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_v;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_p;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ju;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_mol;&c_p;&c_ib;&c_ch;&c_a;&c_k;</trd>,
    <trd>&c_ch;&c_e;&c_p;</trd>,
    <trd>&c_sh;&c_p;&c_ja;&c_n;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">espina</trd>
  <trdgrp lng="cs">
    <trd>osten</trd>,
    <trd>trn</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dorn</trd>,
    <trd>Stachel</trd>
  </trdgrp>
  <trd lng="en">thorn</trd>
  <trdgrp lng="ja">
    <trd>&#12392;&#12370;</trd>,
    <trd>&#12356;&#12400;&#12425;</trd>
  </trdgrp>
  <trd lng="nl">doorn</trd>
  <trdgrp lng="pt">
    <trd>espinho</trd>,
    <trd>estrepe</trd>,
    <trd>pico</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oste&ncaron;</trd>,
    <trd>pichlia&ccaron;</trd>,
    <trd>t&racute;&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_i;&c_p;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_i;&c_ch;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_je;&c_n;&c_i;&c_k; (&c_u; &c_p;&c_r;&c_ja;&c_zh;&c_c;&c_ib;)</trd>,
    <trd>&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#x82AF;&#22836; <pr>x&#x12B;nt&#xF3;u</pr></trd>
</drv>
<drv mrk="dorn.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Plena je <tld/>oj:
      <ekz>
        <tld/>aj arbetoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> plektinte <tld/>an kronon, surmetis &gcirc;in al li
        <fnt>
          <bib>NT</bib>
          <lok>Marko 15:17</lok>
        </fnt>;
      </ekz>      <ekz>
        kiel <tld/>a kano en la mano de ebriulo, tiel
        estas sentenco en la bu&scirc;o de malsa&gcirc;uloj
        <fnt><bib>MT</bib>, <lok>&Sen; 26:9</lok></fnt>;
      </ekz>
      <ekz>
        grand<tld/>aj rampa&jcirc;oj
        <fnt>
          <bib>MkM</bib>
          <lok>16</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="pik.0a.pinta">pika<sncref/></ref>
    <trdgrp lng="fr">
      <trd>&eacute;pineux<klr tip="ind"> (piquant)</klr></trd>,
      <trd>piquant<klr tip="ind"> (&eacute;pineux)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>doornig</trd>,
      <trd>stekelig</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ciernisty</trd>,
      <trd>kolczasty</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_i;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena je &gcirc;ena&jcirc;oj, malhelpa&jcirc;oj kaj malplezuroj:
      <ekz>
        la <tld/>a vojo de la honoro
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="embara.0i">embarasi</ref>,
      <ref cel="implik.0i">impliki</ref>,
      <ref cel="help.mal0i">malhelpi</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;licat<klr tip="ind"> (&eacute;pineux)</klr></trd>,
      <trd>embarrassant<klr tip="ind"> (&eacute;pineux)</klr></trd>,
      <trd>&eacute;pineux<klr tip="ind"> (difficile)</klr></trd>
    </trdgrp>
    <trd lng="nl">stekelig</trd>
    <trd lng="pl">naje&zdot;ony <klr tip="amb">trudno&sacute;ciami</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_c;&c_je;&c_r;&c_n;&c_je;&c_v;&c_y;</trd>,
    <trd>&c_c;&c_ja;&c_r;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_sh;&c_y;&c_p;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">espin&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>ostnat&yacute;</trd>,
    <trd>ostnit&yacute;</trd>,
    <trd>pichlav&yacute; s trny</trd>
  </trdgrp>
  <trd lng="de">dornig</trd>
  <trd lng="en">thorny</trd>
  <trd lng="es">espinoso</trd>
  <trd lng="he">&qof;&vav;&tsadi;&nun;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#12392;&#12370;&#x306E;&#12354;&#x308B;</trd>,
    <trd>&#12356;&#12400;&#12425;&#x306E;</trd>,
    <trd>&#x82E6;&#x96E3;&#x306E;  <pr>&#x304F;&#x306A;&#12435;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="sk">t&racute;nit&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x591A;&#x523A; <pr>du&#x14D;c&#xEC;</pr></trd>,
    <trd>&#x68D8;&#x624B; <pr>j&#xED;sh&#x1D2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dorn.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <tld lit="D"/>arbetaro.
    </dif>
    <refgrp tip="sin">
      <ref cel="vepr.0o">vepro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_je;&c_r;&c_n;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>esbarzer</trd>,
    <trd>bardissa</trd>,
    <trd>romeguera</trd>
  </trdgrp>
  <trd lng="cs">trnisko</trd>
  <trd lng="de">Dornengestr&uuml;pp</trd>
  <trdgrp lng="en">
    <trd>thorn grove</trd>,
    <trd>briar patch</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pinaie</trd>
  <trd lng="ja">&#12356;&#12400;&#12425;&#x306E;&#33538;&#x307F;  <pr>&#12356;&#12400;&#12425;&#x306E;&#12375;&#12370;&#x307F;</pr></trd>
  <trdgrp lng="nl">
    <trd>doornbos</trd>,
    <trd>braambos</trd>
  </trdgrp>
  <trd lng="sk">t&racute;nit&eacute; krovie</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&c_n;&c_i;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0B;&#x5C42;&#x4E1B;&#26519; <pr>xi&#xE0;c&#xE9;ngc&#xF3;ngl&#xED;n</pr></trd>,
    <trd>&#x699B;&#x699B; <pr>zh&emacron;nzh&emacron;n</pr></trd>
  </trdgrp>
</drv>
</art>
