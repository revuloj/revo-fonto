<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dorn</rad>/o
</kap>
<drv mrk="dorn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Natura pikilo de vegeta&jcirc;oj:
      <ekz>
        neniu rozo sen <tld/>o.
      </ekz>
    </dif>
    <trd lng="es">espina</trd>
    <trdgrp lng="fr">
      <trd>&eacute;pine</trd>,
      <trd>piquant<klr tip="ind"> (&eacute;pine)</klr></trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_sh;&c_i;&c_p;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Pikilo simila al <tld/>o:
      <ekz>
        <ind>buko<tld/>o</ind>.
        <trd lng="fr">ardillon</trd>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>aiguillon</trd>,
      <trd>pointe<klr tip="ind"> (aiguillon)</klr></trd></trdgrp> 
    <trd lng="ru">&c_sh;&c_p;&c_je;&c_n;&c_jo;&c_k;</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhelpa&jcirc;o, malplezuro. 
    </dif>
    <trdgrp lng="fr">
      <trd>cactus<klr tip="ind"> (difficult&eacute;)</klr></trd>,
      <trd>os<klr tip="ind"> (difficult&eacute;)</klr></trd>,
      <trd>&eacute;cueil<klr tip="ind"> (difficult&eacute;)</klr></trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_n;&c_o;&c_z;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_v;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Dorn</trd>,
    <trd>Stachel</trd>
  </trdgrp>
</drv>
<drv mrk="dorn.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Plena je <tld/>oj:
      <ekz>
        kiel <tld/>a kano en la mano de ebriulo, tiel
        estas sentenco en la bu&scirc;o de malsa&gcirc;uloj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 26, 9</lok>)</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;pineux<klr tip="ind"> (piquant)</klr></trd>,
      <trd>piquant<klr tip="ind"> (&eacute;pineux)</klr></trd></trdgrp> 
    <trd lng="ru">&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_i;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena je malhelpa&jcirc;oj k malplezuroj. 
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;licat<klr tip="ind"> (&eacute;pineux)</klr></trd>,
      <trd>embarrassant<klr tip="ind"> (&eacute;pineux)</klr></trd>,
      <trd>&eacute;pineux<klr tip="ind"> (difficile)</klr></trd></trdgrp> 
  </snc>
  <trd lng="de">dornig</trd>
  <trd lng="es">espinozo</trd>
</drv>
<drv mrk="dorn.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <tld lit="D"/>arbetaro.  
    </dif>
  </snc>
  <trd lng="fr">&eacute;pinaie</trd>
</drv>
</art>
