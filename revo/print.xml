<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>print</rad>/i
    </kap>

    <drv mrk="print.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="print.0i.FOT">
        <uzo tip="fak">FOT</uzo>
        <dif>
          Per fotografia procezo fari <ref cel="poziti.0a.FOT">pozitivan</ref>
          bildon sur speciala papero a&ubreve; alia surfaco, kopiante filman
          <ref cel="negati.0o.FOT">negativon</ref> a&ubreve; diapozitivon.
        </dif>
        <trd lng="de">Abzug machen</trd>
        <trd lng="hu">lenyomatot k&eacute;sz&iacute;t</trd>
      </snc>
      <snc mrk="print.0i.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Eligi rezultojn de komputado sur datumportilon paperan a&ubreve; filman,
          en formo home legebla,
          per rimedoj elektrome&hcirc;anikaj, elektrostatikaj, termikaj ktp:
          <ekz>
            <tld/>u kaj sendu &gcirc;in <klr>(vian ali&gcirc;ilon)</klr> al ni
            po&scirc;te
            <fnt>
              <lok><url ref=" http://www.geocities.com/Athens/Delphi/2742/"
                  >Brazila Esperantista Junulara Organizo</url>.
              </lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="de">drucken</trd>
        <trd lng="hu">nyomtat</trd>  
        <trd lng="pt">imprimir</trd>      
        <ref tip="sin" cel="pres.0i.KOMP">presi</ref>
      </snc>
      <trd lng="be">&c_d;&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z; &c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y; &c_p;&c_r;&c_a;&c_c;&c_e;&c_s; &c_a;&c_b;&c_o; &c_z; &c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_g;&c_a;&c_j; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_a;)</klr></trd>
      <trd lng="en">print</trd>
      <trd lng="nl">afdrukken</trd>
      <trd lng="ru">&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="print.0ilo">
      <kap><tld/>ilo</kap>
      <snc mrk="print.0ilo.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="super" cel="el.0igilo">Eligilo</ref> por <tld/>i:
          <ekz>
            nadlara, ink&jcirc;eta, lasera <tld/>o.
          </ekz> 
        </dif>
        <ref tip="vid" cel="tajp.0ilo">tajpilo</ref>
        <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_t;&c_je;&c_r;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_r;&c_y;&c_n;&c_t;&c_a;&c_r;</trd>,
        <trd>&c_d;&c_r;&c_u;&c_k;&c_a;&c_r;&c_k;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Drucker</trd>
      <trd lng="en">printer</trd>
      <trdgrp lng="hu">
        <trd>nyomtat&oacute;</trd>,
        <trd>printer</trd>
      </trdgrp>
      <trd lng="nl">printer</trd>
      <trd lng="pl">drukarka</trd>
</drv>

    <drv mrk="print.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Datumo eligita el komputilo en formo de papera a&ubreve; filma teksto a&ubreve;
          grafika&jcirc;o.
        </dif>
        <trd lng="de">Ausdruck</trd>
        <trdgrp lng="en">
          <trd>hard copy</trd>,
          <trd>listing</trd>,
          <trd>printout</trd>
        </trdgrp>
        <trd lng="nl">afdruk</trd>
        <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
      </snc>
    </drv>

  </art>

