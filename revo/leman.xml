<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: leman.xml,v 1.6 2024/01/15 16:14:09 afido Exp $">
    <kap>
      <rad>Leman</rad>/o
    </kap>

<drv mrk="leman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Plej granda <ref tip="super" cel="lag.0o">lago</ref>
      de Francujo kaj Svislando &ccirc;e la
      limo inter amba&ubreve; &scirc;tatoj (anka&ubreve; nomata &Gcirc;eneva Lago):
      <ekz>
        li estis rusa admiralo kaj venkis turkojn &ccirc;e <tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        Rodano <klr>[&#x2026;]</klr> fontas &ccirc;e la Rodan-Gla&ccirc;ero en
        Svislando, trafluas Valezon,
        trairas la lagon <tld/>o, trafluas la sudorientan Francion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30545">Rodano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_L;&c_je;&c_m;&c_a;&c_n;</trd>,
      <trd>&c_Zh;&c_e;&c_n;&c_e;&c_w;&c_s;&c_k;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Genfersee</trd>
    <trd lng="en">Lake Geneva</trd>
    <trd lng="fr">L&eacute;man</trd>
    <trd lng="nl">Meer van Gen&egrave;ve</trd>
  </snc>
  <trd lng="ja">&#x30EC;&#x30DE;&#x30F3;&#x6E56;  <pr>&#x30EC;&#x30DE;&#x30F3;&#x3053;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x65E5;&#x5167;&#x74E6;&#x6E56; <pr>r&#xEC;n&#xE8;iw&#x1CE;h&#xFA;</pr></trd>,
    <trd>&#x65E5;&#x5185;&#x74E6;&#x6E56; <pr>r&#xEC;n&#xE8;iw&#x1CE;h&#xFA;</pr></trd>
  </trdgrp>
</drv>

  </art>

