<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: leman.xml,v 1.1 2023/07/15 18:30:10 afido Exp $">
    <kap>
      <rad>Leman</rad>/o
    </kap>

<drv mrk="leman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Plej granda <ref tip="super" cel="lag.0o">lago</ref>
      de Francujo kaj Svislando &ccirc;e la
      limo inter amba&ubreve; &scirc;tatoj (anka&ubreve; nomata &Gcirc;eneva Lago):
      <ekz>
        li estis rusa admiralo kaj venkis turkojn &ccirc;e <tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        Rodano <klr>[&#8230;]</klr> fontas &ccirc;e la Rodan-Gla&ccirc;ero en
        Svislando, trafluas Valezon,
        trairas la lagon <tld/>o, trafluas la sudorientan Francion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30545"
              >Rodano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Genfersee</trd>
    <trd lng="en">Lake Geneva</trd>
    <trd lng="fr">L&eacute;man</trd>
  </snc>
</drv>

  </art>

