<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;lak</rad>/o  <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>
</kap>
<drv mrk="sxlak.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxlak.0o.TEK">
    <ref tip="dif" cel="skori.0o">skorio</ref>
    <ekz>
      el cent kilogramoj da <tld/>o
      ili gajnis &ccirc;irka&ubreve; dek kilogramojn da pura oro
      <fnt>
        <aut>K. R. Eginhard von Barfus, trad. M. Lennartz</aut>,
        <vrk><url ref="http://www.ipernity.com/blog/41683/200402">
        La orser&ccirc;istoj</url></vrk>,
        <lok>2009</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_a;&c_k;</trd>
  <trd lng="bg">&c_sh;&c_l;&c_a;&c_k;&c_a;</trd>
  <trd lng="de">Schlacke</trd>
  <trdgrp lng="en">
    <trd>dross</trd>,
    <trd>scalia</trd>,
    <trd>slag</trd>
  </trdgrp>
  <trd lng="fr">scorie</trd>
  <trd lng="hu">salak</trd>
  <trd lng="it">scoria<klr> (metallurgica)</klr></trd>
  <trd lng="nl">metaalschuim</trd>
  <trdgrp lng="pl">
    <trd>&zdot;u&zdot;el</trd>,
    <trd>szlaka</trd>
  </trdgrp>
  <trd lng="pt">esc&oacute;ria</trd>
  <trd lng="ru">&c_sh;&c_l;&c_a;&c_k;</trd>
</drv>
</art>
