<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Lebeg</rad>/o
</kap>
<drv mrk="lebeg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve:
      Henri <trd lng="fr">Lebesgue</trd>, 1875-1941. Franca
      matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_je;&c_b;&c_e;&c_g;</trd>
  <trd lng="de">Lebesgue</trd>
  <trd lng="en">Lebesgue</trd>
  <trd lng="hu">Lebesgue</trd>
  <trd lng="it">Lebesgue</trd>
  <trd lng="nl">Henri <ind>Lebesgue</ind></trd>
  <trd lng="pl">Lebesgue</trd>
  <trd lng="pt">Lebesgue</trd>
  <trd lng="ru">&c_L;&c_je;&c_b;&c_je;&c_g;</trd>
</drv>

<drv mrk="lebeg.l0a">
  <kap><tld lit="l"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="integr2.lebega0o"><tld lit="l"/>a integralo</ref>,
      <ref cel="mezur.lebega0o"><tld lit="l"/>a mezuro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_L;&c_je;&c_b;&c_e;&c_g;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_b;&c_e;&c_g;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="de">Lebesguesch</trd>
  <trd lng="en">Lebesgue['s]</trd>
  <trd lng="fr">de <ind>Lebesgue</ind></trd>
  <trd lng="hu">Lebesgue-</trd>
  <trd lng="it">di <ind>Lebesgue</ind></trd>
  <trd lng="nl">van <ind>Lebesgue</ind></trd>
  <trd lng="pl">Lebesgue'a</trd>
  <trdgrp lng="ru">
    <trd>&c_L;&c_je;&c_b;&c_je;&c_g;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_b;&c_je;&c_g;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
