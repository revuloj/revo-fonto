<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>instituci</rad>/o
</kap>
<drv mrk="instit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Da&ubreve;ra publika a&ubreve; privata organizo, fonda&jcirc;o:
      <ekz>la publikaj <tld/>oj de lando, urbo;</ekz>
      <ekz>la juraj <tld/>oj;</ekz>
      <ekz>la hospitaloj, la lernejoj estas plej utilaj <tld/>oj;</ekz>
      <ekz>la Londona komerca &ccirc;ambro estas tutmonde konata <tld/>o.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="instit1.0o">instituto</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">instituce</trd>
  <trd lng="de">Institution</trd>
  <trdgrp lng="nl">
    <trd>instituut</trd>,
    <trd>inrichting</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t; <klr>(&c_s;&c_o;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  </trdgrp>
</drv>
</art>
