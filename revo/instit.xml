<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>instituci</rad>/o
</kap>
<drv mrk="instit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Da&ubreve;ra publika a&ubreve; privata organizo, fonda&jcirc;o:
      <ekz>
        la publikaj <tld/>oj de lando, urbo;
      </ekz>
      <ekz>
        la juraj <tld/>oj;
      </ekz>
      <ekz>
        la hospitaloj, la lernejoj estas plej utilaj <tld/>oj;
      </ekz>
      <ekz>
        la Londona komerca &ccirc;ambro estas tutmonde konata <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="instit1.0o">instituto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t; <klr>(&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_t;&c_v;&c_a;)</klr></trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">instituce</trd>
  <trdgrp lng="de">
    <trd>Institution</trd>,
    <trd>Einrichtung</trd>
  </trdgrp>
  <trd lng="es">instutuci&oacute;n</trd>
  <trd lng="fr">institution</trd>
  <trd lng="hu">int&eacute;zm&eacute;ny</trd>
  <trdgrp lng="nl">
    <trd>instituut</trd>,
    <trd>inrichting</trd>
  </trdgrp>
  <trd lng="pt">institui&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;
    <klr>(&c_s;&c_o;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  </trdgrp>
</drv>
</art>
