<art mrk="$Id: instit.xml,v 1.22 2022/03/21 15:27:49 revo Exp $">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>instituci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="instit.0o">
  <kap><ofc>*</ofc> <tld/>o</kap>
  <snc mrk="instit.0o.POL">
    <dif>
      Da&ubreve;ra publika a&ubreve; privata organizo, fonda&jcirc;o:
      <ekz>
&Gcirc;is la tempo, kiam ia por &ccirc;iuj a&ubreve;toritata kaj nedisputebla institucio
decidos alie... <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
	la publikaj <tld/>oj de lando, urbo;
      </ekz>
      <ekz>
	la juraj <tld/>oj;
      </ekz>
      <ekz>
	la hospitaloj, la lernejoj estas plej utilaj <tld/>oj;
      </ekz>
      <ekz>
	la Londona komerca &ccirc;ambro estas tutmonde konata <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="instan.0o.JUR">instanco</ref>,
      <ref cel="instit1.0o">instituto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t; <klr>
    (&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_t;&c_v;&c_a;)</klr></trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>instituce</trd>,
    <trd>organizace</trd>,
    <trd>org&aacute;n</trd>,
    <trd>za&rcaron;&iacute;zen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Institution</trd>,
    <trd>Einrichtung</trd>
  </trdgrp>
  <trd lng="en">institution</trd>
  <trd lng="es">instutuci&oacute;n</trd>
  <trd lng="fr">institution</trd>
  <trd lng="hu">int&eacute;zm&eacute;ny</trd>
  <trdgrp lng="nl">
    <trd>instituut</trd>,
    <trd>inrichting</trd>
  </trdgrp>
  <trd lng="pl">instytucja</trd>
  <trd lng="pt">institui&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;
    <klr>(&c_s;&c_o;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>in&scaron;tit&uacute;t</trd>,
    <trd>&uacute;stav</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x443;&#x441;&#x442;&#x430;&#x43D;&#x43E;&#x432;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x43A;&#x43B;&#x430;&#x434;</trd>,
    <trd>&#x456;&#x43D;&#x441;&#x442;&#x438;&#x442;&#x443;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
