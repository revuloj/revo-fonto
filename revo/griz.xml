<drv mrk="griz.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="griz.0igxi.kolore">
    <dif>
      I&gcirc;i <tld/>a<sncref ref="griz.0a.koloro"/>:
      <ekz>
         de&scirc;iri&gcirc;as la lastaj folioj de l'arboj, flavi&gcirc;as la lasta
         verdeta
        herbo kaj <tld/>i&gcirc;as la karavanoj de malhelaj nubetoj
        <fnt>
          <aut>N. Hohlov</aut>
          <vrk>&Ccirc;e miaj plumvestitaj amikoj</vrk>
          <lok>Literatura Mondo, 1922-11</lok>
        </fnt>.
      </ekz>
    </dif>
