<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>griz</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="griz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="griz.0a.koloro">
    <dif>
      Havanta koloron mezan inter <ref cel="blank.0a">blanko</ref> kaj
      <ref cel="nigr.0a">nigro</ref>; <ref
      cel="cindr.0o">cindro</ref>kolora,
      <ref tip="super" cel="kolor.sen0a">senkolora</ref>:
      <ekz>
        <tld/>a nubo, nebulo;
      </ekz><ekz>
        la prudentaj <tld/>aj okuloj de la libristo
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        la polvo <ind><tld/>igi</ind>s la foliojn;
        <trd lng="ca">agrisar</trd>
      </ekz><ekz>
        <ind><tld/>eflava</ind> <klr>(lankolora)</klr>;
        <trd lng="ca">beix</trd>
        <trd lng="es">beige</trd>
        <trdgrp lng="de">
          <trd>graugelb</trd>,
          <trd>beige</trd>
        </trdgrp>
        <trd lng="fr">beige</trd>
        <trdgrp lng="hu">
          <trd>sz&uuml;rk&eacute;ss&aacute;rga</trd>,
          <trd>homoksz&iacute;n&udblac;</trd>,
          <trd>b&eacute;zs</trd>
        </trdgrp>
        <trd lng="nl">beige</trd>
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j;</trd>
  </snc>

  <snc mrk="griz.0a.haroj">
    <dif>
      (Pri haroj:) Senkolori&gcirc;inta pro la a&gcirc;o; do blanka, hele
      griza, ar&gcirc;entkolora:
      <ekz>
        <tld/>a barbo sa&gcirc;on ne atestas;
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        en tiu &ccirc;i minuto unu maljuna, <tld/>a sinjoro enkuris tute
        senspire
        <fnt>
          <aut>Charles Dickens, tr. &Z;</aut>:
          <vrk>La batalo de l' vivo</vrk>. <lok>1891</lok>
        </fnt>;
      </ekz><ekz>
        gloro por junuloj estas ilia forto,
        kaj ornamo por maljunuloj estas <tld/>eco
        <fnt><bib>MT</bib>, <lok>&Sen; 20:29</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_ib;&c_v;&c_y;</trd>
    <trd lng="ru">&c_s;&c_je;&c_d;&c_o;&c_j;</trd>
  </snc>

  <snc mrk="griz.0a.sengaja">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senplezura, enua, sen&gcirc;oja, malbrila:
      <ekz>
        la malvarma, <tld/>a, kvaza&ubreve; plumba tago nin premas al la tero
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pal.0a">pala</ref>.
    </refgrp>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j;</trd>
  </snc>

  <trd lng="be">&c_sh;&c_e;&c_r;&c_y;</trd>
  <trd lng="bg">&c_s;&c_i;&c_v;</trd>
  <trd lng="ca">gris</trd>
  <trd lng="en">gray</trd>
  <trd lng="de">grau</trd>
  <trd lng="es">gris</trd>
  <trd lng="fr">gris<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&uuml;rke</trd>,
    <trd>&odblac;sz</trd>
  </trdgrp>
  <trd lng="id">abu-abu</trd>
  <trd lng="nl">grijs<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="pl">szary</trd>
  <trdgrp lng="pt">
   <trd>cinzento</trd>, 
   <trd>gris</trd> 
  </trdgrp>
  <trd lng="sv">gr&aring;</trd>
  <trdgrp lng="tr">
    <trd>boz</trd>, <trd>gri</trd>
  </trdgrp>
  <trd lng="vo">gelik</trd>
</drv>

<drv mrk="griz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_i;&c_v;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">color gris</trd>
  <trd lng="de">Grau</trd>
  <trd lng="es">gris</trd>
  <trd lng="fr">gris<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">grijs<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="pt">cor <ind>cinzenta</ind></trd>
</drv>

<drv mrk="griz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a.
    </dif>
  </snc>
  <trd lng="de">etwas <ind>graues</ind></trd>
  <trd lng="nl">iets <ind>grijs</ind></trd>
</drv>

<drv mrk="griz.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Impresi, vidi&gcirc;i kiel <tld/>a<sncref ref="griz.0a.koloro"/>:
      <ekz>
        En mezo <tld/>is &scirc;tono
        <fnt>
          <aut>Mickiewicz, tr. Grabowski</aut>:
          <vrk>Sinjoro Tadeo</vrk>
          <lok>Libro 3&#170;, v. 300</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">show grey</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="griz.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="griz.0igxi.kolore">
    <dif>
      I&gcirc;i <tld/>a<sncref ref="griz.0a.koloro"/>:
      <ekz>
         de&scirc;iri&gcirc;as la lastaj folioj de l'arboj, flavi&gcirc;as la lasta
         verdeta
        herbo kaj <tld/>i&gcirc;as la karavanoj de malhelaj nubetoj
        <fnt>
          <aut>N. Hohlov</aut>:
          <vrk>&Ccirc;e miaj plumvestitaj amikoj</vrk>.
          <lok>Literatura Mondo, Nov. 1922</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="griz.0igxi.haroj">
    <dif>
      I&gcirc;i <tld/>a<sncref ref="griz.0a.haroj"/>:
      <ekz>
        kapo malsa&gcirc;a ne <tld/>i&gcirc;as nek kalvi&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        li rapide <tld/>i&gcirc;as.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_je;&c_d;&c_je;&c_t;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="griz.0hara">
  <kap><tld/>hara</kap>
  <snc>
    <ref tip="dif" cel="griz.0a.haroj"><tld/>a<sncref/></ref>
    <ekz>
      <tld/>hara maljunulo;
    </ekz><ekz>
      <tld/>hara Saturn' ekparolas <klr>...</klr>
      <fnt><aut>Herder, tr. V. Devjatnin</aut>:
        <vrk>Infano de zorgo</vrk>. <bib>FK</bib>
      </fnt>.
    </ekz>
    <trd lng="be">&c_s;&c_ib;&c_v;&c_y;</trd>
    <trd lng="ca">de cabells cendrosos</trd>
    <trd lng="de">grauharig</trd>
    <trd lng="es">canoso</trd>
    <trd lng="fr">aux cheveux <ind>gris</ind></trd>
    <trd lng="hu">&odblac;sz haj&uacute;</trd>
    <trd lng="nl">grijsharig</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_je;&c_d;&c_o;&c_j;</trd>,
      <trd>&c_s;&c_je;&c_d;&c_o;&c_v;&c_l;&c_a;&c_s;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="griz.0nivelo">
  <kap><tld/>nivelo</kap>
  <snc mrk="griz.0nivelo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Denso de rastrumeroj, en iu loko de tipografia kli&scirc;o, kiu povas
      impresi kiel grado de <tld/>o (de blanko &gcirc;is nigro).
    </dif>
  </snc>
  <snc mrk="griz.0nivelo.FOT">
    <uzo tip="fak">FOT</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En cifereca bildo, &ccirc;iu el la eblaj valoroj por intenso de
      rastrumeroj, impresanta kiel grado de <tld/>o:
      <ekz>
        ekrano kun 16 <tld/>niveloj
        <fnt>
          <aut>Edmund GRIMLEY EVANS</aut>:
              <vrk><url
ref="http://www.esperanto.be/fel/2011/010280.php"
                  >La nova Kindlo de Amazon</url></vrk>.
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">niveau de gris</trd>
  <trd lng="en">grayscale</trd>
  <trd lng="ru">&c_o;&c_t;&c_t;&c_je;&c_n;&c_o;&c_k; &c_s;&c_je;&c_r;&c_o;&c_g;&c_o;</trd>
</drv>

</art>
