    <dif>
      Io <tld/>a:
      <ekz>
        la abismo aperas kiel <tld/>a&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>&Ijo; 41:32</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">etwas <ind>Graues</ind></trd>
  <trd lng="nl">iets <ind>grijs</ind></trd>
  <trd lng="uk">&#x441;&#x438;&#x432;&#x438;&#x43D;&#x430;</trd>
