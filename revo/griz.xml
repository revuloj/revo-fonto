<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>griz</rad>/a
</kap>
<drv mrk="griz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta koloron mezan inter blanko kaj nigro; cindrokolora, senkolora:
      <ekz>
        <tld/>a barbo, vetero, nubo, nebulo;
      </ekz>
      <ekz>
        <ind><tld/>hara</ind> maljunulo;
        <trd lng="be">&c_s;&c_ib;&c_v;&c_y;</trd>
        <trd lng="es">canoso</trd>
        <trd lng="fr">aux cheveux <ind>gris</ind></trd>
        <trd lng="hu">&odblac;sz haj&uacute;</trd>
        <trd lng="nl">grijsharig</trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_je;&c_d;&c_o;&c_j;</trd>,
          <trd>&c_s;&c_je;&c_d;&c_o;&c_v;&c_l;&c_a;&c_s;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la polvo <tld/>igis la foliojn;
      </ekz>
      <ekz>
        li rapide <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <ind><tld/>eflava</ind> <klr>(lankolora)</klr>;
        <trd lng="es">beige</trd>
        <trd lng="fr">beige</trd>
        <trdgrp lng="hu">
          <trd>sz&uuml;rk&eacute;ss&aacute;rga</trd>,
          <trd>homoksz&iacute;n&udblac;</trd>,
          <trd>b&eacute;zs</trd>
        </trdgrp>
        <trd lng="nl">beige</trd>
      </ekz>
      <ekz>
        gloro por junuloj estas ilia forto,
        kaj ornamo por maljunuloj estas <tld/>eco
        <fnt><bib>MT</bib>, <lok>&Sen; 20:29</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
         <tld/>aj <klr>(senplezuraj, enuaj)</klr> tagoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pal.0a">pala</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_r;&c_y;</trd>
  <trd lng="en">gray</trd>
  <trd lng="de">grau</trd>
  <trd lng="es">gris</trd>
  <trd lng="fr">gris<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&uuml;rke</trd>,
    <trd>&odblac;sz</trd>
  </trdgrp>
  <trd lng="nl">grijs<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="pl">szary</trd>
  <trdgrp lng="pt">
   <trd>cinzento</trd>, 
   <trd>gris</trd> 
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j;</trd>
  <trd lng="sv">gr&aring;</trd>
  <trdgrp lng="tr">
    <trd>boz</trd>, <trd>gri</trd>
  </trdgrp>
  <trd lng="vo">gelik</trd>
</drv>

<drv mrk="griz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a koloro.
    </dif>
  </snc>
  <trd lng="es">gris</trd>
  <trd lng="fr">gris<klr tip="ind"> (couleur)</klr></trd>
  <trd lng="nl">grijs<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="pt">cor <ind>cinzenta</ind></trd>
</drv>

<drv mrk="griz.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Io <tld/>a.
    </dif>
  </snc>
  <trd lng="fr">gris<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">iets <ind>grijs</ind></trd>
</drv>
</art>
