<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>griz</rad>/a
</kap>
<drv mrk="griz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta koloron mezan inter blanko kaj nigro; cindrokolora, senkolora:
      <ekz>
        <tld/>a barbo, vetero, nubo, nebulo;
      </ekz>
      <ekz>
        <tld/>hara maljunulo;
      </ekz>
      <ekz>
        la polvo <tld/>igis la foliojn;
      </ekz>
      <ekz>
        li rapide <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <tld/>eflava <klr>(lankolora)</klr>;
      </ekz>
      <ekz>
        gloro por junuloj estas ilia forto,
        kaj ornamo por maljunuloj estas <tld/>eco
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 20, 29</lok>)</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
         <tld/>aj <klr>(senplezuraj, enuaj)</klr> tagoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pal.0a">pala</ref>.
    </refgrp>
  </snc>
  <trd lng="en">gray</trd>
  <trd lng="de">grau</trd>
  <trd lng="fr">gris</trd>
  <trd lng="nl">grijs</trd>
  <trd lng="pl">szary</trd>
  <trdgrp lng="tr">
    <trd>boz</trd>, <trd>gri</trd>
  </trdgrp>
</drv>
<drv mrk="griz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld/>a koloro.
    </dif>
  </snc>
</drv>
<drv mrk="griz.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Io <tld/>a.
    </dif>
  </snc>
</drv>
</art>
