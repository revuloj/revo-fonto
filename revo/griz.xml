<drv mrk="griz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="griz.0i.KOL">
    <dif>
      Impresi, vidi&gcirc;i kiel <tld/>a<sncref ref="griz.0a.koloro"/>:
      <ekz>
        En mezo <tld/>is &scirc;tono
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>
          <vrk>Sinjoro Tadeo</vrk>
          <lok>1918</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_sh;&c_e;&c_r;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="cs">b&yacute;t &scaron;ediv&yacute;</trd>
  <trd lng="en">show grey</trd>
  <trd lng="id"><baz>abu</baz>mengabu</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">by&tcaron; &scaron;ediv&yacute;</trd>
</drv>

