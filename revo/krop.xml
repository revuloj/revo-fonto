<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krop</rad>/o
</kap>
<drv mrk="krop.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Membrana po&scirc;o, kiun la birdoj havas sub gor&gcirc;o, kaj en kiu
      restadas
      la nutra&jcirc;oj anta&ubreve; ol penetri en stomakon.
    </dif>
    <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_l;&c_jo;</trd>
    <trd lng="de">Kropf</trd>
    <trdgrp lng="en">
      <trd>craw</trd>,
      <trd>crop</trd>,
      <trd>gullet</trd>,
      <trd>pouch</trd>
    </trdgrp>
    <trd lng="es">papo</trd> 
    <trd lng="fa">&f_ch;&f_y;&a_n;&a_h;&zwnj;&a_d;&a_A;&a_n;</trd>
    <trd lng="fr">jabot</trd>
    <trd lng="hu">begy</trd>
    <trd lng="nl">krop</trd>
    <trd lng="pl">wole</trd>
    <trd lng="pt">papo <klr tip="amb">(zool.)</klr></trd>
    <trd lng="ru">&c_z;&c_o;&c_b; <klr>(&c_u; &c_p;&c_t;&c_i;&c_c;)</klr></trd>
  </snc>
  <snc mrk="krop.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nedolora tumoro de la homa tiroida glando formanta pendantan sakon
      sur la kolo.
    </dif>
    <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_l;&c_ja;&c_k; <klr>(&c_p;&c_u;&c_h;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
    <trdgrp lng="de">
      <trd>Kropf</trd>,
      <trd>Schilddr&uuml;senschwellung</trd>
    </trdgrp>
    <trd lng="en">goiter</trd>
    <trdgrp lng="es">
      <trd>papo</trd>,
      <trd>bocio</trd>
    </trdgrp>
    <trd lng="fa">&f_g;&a_w;&a_A;&a_t;&a_r;</trd>
    <trd lng="fr">goitre</trd>
    <trd lng="hu">golyva</trd>
    <trdgrp lng="nl">
      <trd>kropziekte</trd>,
      <trd>kropgezwel</trd>,
      <trd>krop<klr tip="ind"> (ziekte</klr></trd>,
      <trd>struma</trd>
    </trdgrp>
    <trd lng="pl">wole</trd>
    <trd lng="pt">papo <klr tip="amb">(med.)</klr></trd>
    <trd lng="ru">&c_z;&c_o;&c_b; <klr>(&c_u; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_a;)</klr></trd>
  </snc>
</drv>

<drv mrk="krop.post0o">
  <kap>post<tld/>o</kap>
  <snc mrk="krop.post0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Dua stomaka po&scirc;o de la birdoj, kiu venas post la <tld/>o kaj en
      kiu
      la nutra&jcirc;oj estas pistitaj kaj pastigitaj; ma&ccirc;stomako.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_u;&c_g;&c_ib; <ind>&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;</ind> <klr>(&c_u;
  &c_p;&c_t;&c_u;&c_sh;&c_a;&c_k;)</klr></trd>
  <trd lng="de">Muskelmagen</trd>
  <trd lng="en">gizzard</trd>
  <trd lng="fa">&a_s;&a_n;&f_g;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">g&eacute;sier</trd>
  <trd lng="nl">spiermaag</trd>
  <trd lng="hu">z&uacute;za</trd>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_o;&c_j; &c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_k; <klr>(&c_u;
  &c_p;&c_t;&c_i;&c_c;)</klr></trd>
</drv>
</art>
