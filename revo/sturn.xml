<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>sturn</rad>/o
</kap>
<drv mrk="sturn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="sturn.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Sturnus</trd>), birdo imitanta aliajn birdojn.
    </dif>
  </snc>
  <trd lng="cs">&scaron;pa&ccaron;ek</trd>
  <trd lng="de">Star</trd>
  <trd lng="en">starling</trd>
  <trd lng="hu">sereg&eacute;ly</trd>
  <trd lng="pl">szpak</trd>
  <trd lng="ru">&c_s;&c_k;&c_v;&c_o;&c_r;&c_je;&c_c;</trd>
</drv>
<drv mrk="sturn.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Sturnidae</trd>).      
    </dif>
  </snc>
  <trd lng="de">Stare <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
