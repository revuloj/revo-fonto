<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>9</ofc><rad>ku</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ku.0o">
  <kap><tld/>o</kap>
  <snc mrk="ku.0o.SNC">
    <uzo tip="fak">TIP</uzo>
    <dif>
    Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref> (q,
    Q) uzata i.a. kiel:
    </dif>
    <trdgrp lng="fr">
    <trd>q</trd>,
    <trd>Q</trd>
    </trdgrp>
  <subsnc>
  <uzo tip="fak">MAT</uzo>
  <dif>
  (maj.) simbolo por la aro de &ccirc;iuj racionalaj nombroj:
  <ekz><em>Q</em></ekz>;
  </dif>
  <trd lng="fr">Q<klr> (ensemble des nombres rationnels)</klr></trd>
  </subsnc>  
  </snc>
  </drv>
</art>

