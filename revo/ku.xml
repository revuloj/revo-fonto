<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ku.xml,v 1.14 2023/09/01 15:46:46 revo Exp $">
<kap>
    <ofc>9</ofc><rad>ku</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ku.0o">
  <kap><tld/>o</kap>
  <snc mrk="ku.0o.SNC">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref>
      (q, Q):
      <ekz>
        <tld/>o, estas la 17-a litero de la latina alfabeto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22121">Q</url></lok>
        </fnt>;
      </ekz> uzata i.a. kiel
    </dif>
    <subsnc>
      <uzo tip="fak">MAT</uzo>
      <dif>
        (maj. kaj duoble a&ubreve; grase) simbolo por la aro de &ccirc;iuj racionalaj        
        nombroj: <ekz><frm am="QQ"><g>Q</g></frm></ekz>
      </dif>
      <trd lng="fr">Q<klr> (ensemble des nombres rationnels)</klr></trd>
      <trd lng="pl">Q <klr> (symbol liczb wymiernych)</klr></trd>
    </subsnc>  
    <trdgrp lng="fr">
    <trd>q</trd>,
    <trd>Q</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>Q <klr>(neesperantsk&eacute; p&iacute;smeno a souhl&aacute;ska)</klr></trd>,
    <trd>q</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Q</trd>,
    <trd>q</trd>
  </trdgrp>
  <trd lng="ja">&#x30AD;&#x30E5;&#x30FC;</trd>
  <trd lng="pl">q Q <klr>(ku)</klr></trd>
  <trd lng="ro">q Q <klr>(ciu)</klr></trd>
  <trdgrp lng="sk">
    <trd>Q</trd>,
    <trd>q</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u; <klr>(&#x43B;&c_a;&c_t;&c_i;&#x43D;&c_s;&#x44C;&#x43A;&c_a; &#x43B;&c_ib;&c_t;&c_je;&c_r;&c_a; Q, q)</klr></trd>
  </trdgrp>
</drv>
</art>

