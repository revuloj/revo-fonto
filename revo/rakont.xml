<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rakont</rad>/i
</kap>
<drv mrk="rakont.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Detale konigi per paroloj a&ubreve; skriboj okazinta&jcirc;on kun &gcirc;iaj
      cirkonstancoj:
      <ekz>
        <tld/>i belan historion<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sian tutan aventuron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la akcidenton, sian voja&gcirc;on, siajn impresojn;
      </ekz>
      <ekz>
        la legendo <tld/>as pri.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vypr&aacute;v&ecaron;t</trd>
  <trd lng="de">erz&auml;hlen</trd>
  <trd lng="en">relate</trd>
  <trd lng="nl">vertellen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="rakont.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>as.
    </dif>
  </snc>
  <trd lng="cs">pov&iacute;dka</trd>
  <trd lng="de">Erz&auml;hlung</trd>
  <trd lng="nl">verhaal</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_k;&c_a;&c_z;</trd>
</drv>

<drv mrk="rakont.popol0oj">
  <kap>popol<tld/>oj</kap>
  <snc>
    <dif>
      Folkloro, legendoj.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
