<drv mrk="rakont.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rakont.0i.KOMUNE">
    <dif>
      Detale konigi per paroloj a&ubreve; skriboj okazinta&jcirc;on kun
      &gcirc;iaj cirkonstancoj:
      <ekz>
        <tld/>u al mia juna amiko belan historion
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is al li sian tutan aventuron
        <fnt><bib>F</bib>, <lok>&FE; 23</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i la akcidenton, sian voja&gcirc;on, siajn impresojn;
      </ekz>
      <ekz>
        en tiu &ccirc;i libro la a&ubreve;toro <tld/>as sian vivon de la
        infaneco &gcirc;is la nuntempo
        <fnt>
        <aut>D. Cibulevskij</aut>,
        <vrk>Danio pli proksima</vrk>,
        <bib>Monato</bib>,
        <lok>1997:8a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">narrate</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_v;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>contar</trd>,
    <trd>relatar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vypr&aacute;v&ecaron;t</trd>,
    <trd>vypr&aacute;v&ecaron;t</trd>
  </trdgrp>
  <trd lng="de">erz&auml;hlen</trd>
  <trdgrp lng="en">
    <trd>relate</trd>,
    <trd>tell</trd>,
    <trd>recount</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contar</trd>,
    <trd>relatar</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_q;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_H;&f_k;&a_A;&f_y;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raconter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elbesz&eacute;l</trd>,
    <trd>elmes&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ber<ind>cerita</ind></trd>,
    <trd>men<ind>cerita</ind>kan</trd>
  </trdgrp>
  <trd lng="nl">vertellen</trd>
  <trdgrp lng="pt">
    <trd>contar</trd>,
    <trd>narrar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>porozpr&aacute;va&tcaron;</trd>,
    <trd>rozpoveda&tcaron;</trd>,
    <trd>rozpr&aacute;va&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_p;&c_o;&c_v;&c_ib;&c_d;&c_a;&c_t;&c_i;</trd>
</drv>

