<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>propagand</rad>/o
</kap>
<drv mrk="propag.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Klopodado por disvastigi ideon:
       <ekz>
         <tld/>o por Esperanto;
       </ekz>
       <ekz>
         libreto difinita speciale por la <tld/>o de nia
         afero<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>a verko, parolado.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="akcel">akcelo</ref>,
       <ref cel="pusx">pu&scirc;ado</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;</trd>
   <trdgrp lng="de">
     <trd>Werbung</trd>,
     <trd>Propaganda</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>propaganda</trd>,
     <trd>n&eacute;pszer&udblac;s&iacute;t&eacute;s</trd>,
     <trd>h&iacute;rver&eacute;s</trd>
   </trdgrp>
   <trd lng="nl">propaganda</trd>
   <trdgrp lng="pt">
     <trd>propaganda</trd>,
     <trd>divulga&ccedil;&atilde;o</trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;</trd>
   <trd lng="sv">propaganda</trd>
   <trd lng="fr">propagande</trd>
</drv>

<drv mrk="propag.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Fari <tld/>on por:
       <ekz>
         <tld/>i Esperanton.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="sem.0i">semi</ref>,
       <ref cel="fam">disfamigi</ref>,
       <ref cel="trumpe.dis0i">distrumpeti</ref>,
       <ref cel="anonc">disanonci</ref>,
       <ref cel="varb.0i">varbi</ref>,
       <ref cel="rekrut">rekrutigi</ref>,
       <ref cel="konver3.0i">konverti</ref>,
       <ref cel="adept.0igi">adeptigi</ref>.
     </refgrp>
   </snc>
   <trd 
lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trdgrp lng="de">
     <trd>werben</trd>,
     <trd>propagieren</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>propag&aacute;l</trd>,
     <trd>n&eacute;pszer&udblac;s&iacute;t</trd>
   </trdgrp>
   <trd lng="nl">propaganda maken</trd>
   <trd lng="fr">faire de la <ind>propagande</ind></trd>
   <trd 
lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="propag.0isto">
   <kap><tld/>isto</kap>
   <snc>
     <dif>
       Homo, kiu da&ubreve;re <tld/>as
     </dif>
     <refgrp tip="sin">
       <ref cel="pionir.0o">pioniro</ref>,
       <ref cel="aposto.0o">apostolo</ref>.
     </refgrp>
   </snc>
   <trd 
lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_y;&c_s;&c_t;</trd>
   <trd lng="de">Propagandist</trd>
   <trd lng="hu">propagandista</trd>
   <trd lng="nl">propagandist</trd>
   <trd lng="fr">propagandiste</trd>
   <trd 
lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
