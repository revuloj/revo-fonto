<kap>
  <ofc>1</ofc>
  <rad>propagand</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="propag.0o">
  <kap><tld/>o</kap>
  <snc mrk="propag.0o.KOMUNE">
    <dif>
      Klopodado por disvastigi ideon:
      <ekz>
        <tld/>o por Esperanto;
      </ekz>
      <ekz>
        ni kunvenas por fari manifestacion kaj sekve <tld/>on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        por la celo de <tld/>o mi publikigas nun mian kredon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        revolucia <tld/>o
        <fnt>
          <bib>ChC</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o per glavo
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcel">akcelo</ref>,
      <ref cel="reklam.0o">reklamo</ref>,
      <ref cel="misi.0o.KRI">misio<sncref/></ref>,
      <ref cel="pusx">pu&scirc;ado</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>propagace</trd>,
    <trd>propaganda</trd>,
    <trd>&scaron;&iacute;&rcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Werbung</trd>,
    <trd>Propaganda</trd>
  </trdgrp>
  <trd lng="es">propaganda</trd>
  <trd lng="fr">propagande</trd>
  <trdgrp lng="hu">
    <trd>propaganda</trd>,
    <trd>n&eacute;pszer&udblac;s&iacute;t&eacute;s</trd>,
    <trd>h&iacute;rver&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BA3;&#x4F1D;  <pr>&#x305B;&#x3093;&#x3067;&#x3093;</pr></trd>,
    <trd>&#x666E;&#x53CA;  <pr>&#x3075;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x30D7;&#x30ED;&#x30D1;&#x30AC;&#x30F3;&#x30C0;</trd>,
    <trd>&#x4F1D;&#x9053;  <pr>&#x3067;&#x3093;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x5E03;&#x6559;  <pr>&#x3075;&#x304D;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">propaganda</trd>
  <trdgrp lng="pl">
    <trd>propaganda</trd>,
    <trd>rozpowszechnianie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>propaganda</trd>,
    <trd>divulga&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="sk">propaganda</trd>
  <trd lng="sv">propaganda</trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_p;&c_a;&c_gu;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BA3;&#x4F20; <pr>xu&amacron;nchu&aacute;n</pr></trd>,
    <trd>&#x5BA3;&#x50B3; <pr>xu&amacron;nchu&aacute;n</pr></trd>,
    <trd>&#x653F;&#x6CBB;&#x5BA3;&#x4F20; <pr>zh&egrave;ngzh&igrave;xu&amacron;nchu&aacute;n</pr></trd>,
    <trd>&#x653F;&#x6CBB;&#x5BA3;&#x50B3; <pr>zh&egrave;ngzh&igrave;xu&amacron;nchu&aacute;n</pr></trd>,
    <trd>&#x5BA3;&#x4F20;&#x673A;&#x6784; <pr>xu&amacron;nchu&aacute;nj&imacron;g&ograve;u</pr></trd>,
    <trd>&#x5BA3;&#x50B3;&#x6A5F;&#x69CB; <pr>xu&amacron;nchu&aacute;nj&imacron;g&ograve;u</pr></trd>,
    <trd>&#x5BA3;&#x4F20;&#x6D3B;&#x52A8; <pr>xu&amacron;nchu&aacute;nhu&oacute;d&ograve;ng</pr></trd>,
    <trd>&#x5BA3;&#x50B3;&#x6D3B;&#x52D5; <pr>xu&amacron;nchu&aacute;nhu&oacute;d&ograve;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="propag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="propag.0i.KOMUNE">
    <dif>
      Fari <tld/>on por; strebi disvastigi ideon ks:
      <ekz>
        <tld/>i nian ideon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i universalan kunfratecon
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj <tld/>is la ideon de la lifto, e&ccirc; <klr>[&#x2026;]</klr> skribis verkon por
        reklami &gcirc;in
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i favore al la partio
        <fnt>
          <bib>ChV</bib>
          <lok>21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="adept.0igi">adeptigi</ref>,
      <ref cel="fam.dis0igi">disfamigi</ref>,
      <ref cel="anonc">disanonci</ref>,
      <ref cel="evangeliz.0i">evangelizi</ref>,
      <ref cel="reklam.0i.EKON">reklami</ref>,
      <ref cel="rekrut">rekrutigi</ref>,
      <ref cel="sem.0i.FIG">semi<sncref/></ref>,
      <ref cel="varb.0i">varbi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>propagovat</trd>,
    <trd>&scaron;&iacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>werben</trd>,
    <trd>propagieren</trd>
  </trdgrp>
  <trd lng="es">hacer propaganda</trd>
  <trd lng="fr">faire de la <ind>propagande</ind></trd>
  <trdgrp lng="hu">
    <trd>propag&aacute;l</trd>,
    <trd>n&eacute;pszer&udblac;s&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BA3;&#x4F1D;&#x3059;&#x308B;  <pr>&#x305B;&#x3093;&#x3067;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x666E;&#x53CA;&#x3055;&#x305B;&#x308B;  <pr>&#x3075;&#x304D;&#x3085;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5E03;&#x6559;&#x3059;&#x308B;  <pr>&#x3075;&#x304D;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">propaganda maken</trd>
  <trdgrp lng="pl">
    <trd>propagowa&cacute;</trd>,
    <trd>og&lstroke;asza&cacute;</trd>,
    <trd>rozpowszechnia&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">propagova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_gu;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_p;&c_r;&c_o;&c_p;&c_a;&c_gu;&c_a;&c_n;&c_d;&c_o;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BA3;&#x4F20; <pr>xu&amacron;nchu&aacute;n</pr></trd>,
    <trd>&#x5BA3;&#x50B3; <pr>xu&amacron;nchu&aacute;n</pr></trd>,
    <trd>&#x505A;&#x5E7F;&#x544A; <pr>zu&ograve;gu&#x1CE;ngg&agrave;o</pr></trd>,
    <trd>&#x505A;&#x5EE3;&#x544A; <pr>zu&ograve;gu&#x1CE;ngg&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="propag.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="propag.0ilo.KOMUNE">
    <dif>
      &Ccirc;ia dokumento, objekto ktp, helpanta en <tld/>o:
      <ekz>
        tie &ccirc;i estas a&ccirc;eteblaj lernolibroj kaj <tld/>iloj de la lingvo
        internacia
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 40a</lok></fnt>;
      </ekz>
      <ekz>
        la plej grava <tld/>ilo de tiu tempo estis la granda Esperanto-ekspozicio
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis <klr>[&#x2026;]</klr> aktoro, promenanta <tld/>ilo
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia presti&gcirc;a revuo estas anka&ubreve; bona <tld/>ilo
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url ref="&Monato;/2004/008228.html"
              >Retrorigarde kaj perspektive</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_r;&c_o;&c_d;&c_a;&c_k; &c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_y;&c_s;&c_c;&c_k;&c_ib; &c_m;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ja;&c_l;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_y;&c_s;&c_c;&c_k;&c_ib; &c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>let&aacute;k</trd>,
    <trd>prost&rcaron;edek propagace</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Propagandamaterial</trd>,
    <trd>Werbung <klr>(Material)</klr></trd>
  </trdgrp>
  <trd lng="es">material de propaganda (folletos, carteles&#x2026;)</trd>
  <trdgrp lng="fr">
    <trd>objet promotionnel</trd>,
    <trd>mat&eacute;riel de <ind>propagande</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x5BA3;&#x4F1D;&#x6587;&#x66F8;  <pr>&#x305B;&#x3093;&#x3067;&#x3093;&#x3076;&#x3093;&#x3057;&#x3087;</pr></trd>
  <trd lng="pl">narz&eogonek;dzie  <ind>propagandy</ind></trd>
  <trd lng="sk">let&aacute;k</trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_p;&c_a;&c_gu;&c_a;&c_n;&c_d;&c_n;&c_i;&c_j; &c_m;&c_a;&c_t;&c_je;&c_r;&c_ib;&c_a;&c_l;</trd>
  <trd lng="zh">&#x6587;&#x5BA3; <pr>w&eacute;nxu&amacron;n</pr></trd>
</drv>

<drv mrk="propag.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="propag.0isto.KOMUNE">
    <dif>
      Homo, kiu da&ubreve;re <tld/>as:
      <ekz>
        Leopoldo <nom>Einstein</nom>, la unua energia <tld/>isto<tld/> de nia afero
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        inter ili staris la markizo Louis de Beaufront, unua <tld/>isto en Francujo
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej fanatikaj <tld/>istoj de la I&scirc;tar-kulto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="misi.0isto">misiisto</ref>,
      <ref cel="reklam.0isto">reklamisto</ref>,
      <ref cel="varb.0isto">varbisto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="cs">propag&aacute;tor</trd>
  <trd lng="de">Propagandist</trd>
  <trd lng="es">propagandista</trd>
  <trd lng="fr">propagandiste</trd>
  <trd lng="hu">propagandista</trd>
  <trdgrp lng="ja">
    <trd>&#x5BA3;&#x4F1D;&#x5BB6;  <pr>&#x305B;&#x3093;&#x3067;&#x3093;&#x304B;</pr></trd>,
    <trd>&#x4F1D;&#x9053;&#x8005;  <pr>&#x3067;&#x3093;&#x3069;&#x3046;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x5E03;&#x6559;&#x5E2B;  <pr>&#x3075;&#x304D;&#x3087;&#x3046;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="nl">propagandist</trd>
  <trd lng="pl">propagandysta</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">propag&aacute;tor</trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_p;&c_a;&c_gu;&c_a;&c_n;&c_d;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BA3;&#x4F20;&#x5458; <pr>xu&amacron;nchu&aacute;nyu&aacute;n</pr></trd>,
    <trd>&#x5BA3;&#x50B3;&#x54E1; <pr>xu&amacron;nchu&aacute;nyu&aacute;n</pr></trd>,
    <trd>&#x5BA3;&#x4F20;&#x8005; <pr>xu&amacron;nchu&aacute;nzh&ecaron;</pr></trd>,
    <trd>&#x5BA3;&#x50B3;&#x8005; <pr>xu&amacron;nchu&aacute;nzh&ecaron;</pr></trd>
  </trdgrp>
</drv>
