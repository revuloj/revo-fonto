<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>arbitraci</rad>/o
</kap>
<drv mrk="arbitr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Decido pri ia grava malkonsento, farita de ju&gcirc;anto elektita de la
      disputantoj:
      <ekz>
        &ccirc;iu internacia malkonsento devus esti finita per
        <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schiedsspruch</trd>,
     <trd>Schiedsurteil</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_b;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>,
    <trd>&c_a;&c_r;&c_b;&c_ib;&c_t;&c_r;&c_a;&c_zh;&c_n;&c_y; &c_s;&c_u;&c_d;</trd>
  </trdgrp>
  <trd lng="en">arbitration</trd>
  <trd lng="fr">arbitrage (juridique)</trd>
  <trdgrp lng="hu">
     <trd>v&aacute;lasztott b&iacute;r&oacute;s&aacute;gi d&ouml;nt&eacute;s</trd>,
     <trd>v&aacute;lasztott b&iacute;r&oacute;s&aacute;gi &iacute;t&eacute;let</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_b;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j; &c_s;&c_u;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="arbitr1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ju&gcirc;i per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>durch Schiedsspruch <ind>entscheiden</ind></trd>,
     <trd>schlichten</trd>,
     <trd>vermitteln</trd>
  </trdgrp>
  <trd lng="en">arbitrate</trd>
  <trdgrp lng="hu">
     <trd>d&ouml;nt&odblac;b&iacute;r&aacute;skodik</trd>,
     <trd>d&ouml;nt&odblac;b&iacute;r&oacute;s&aacute;gi <ind>&iacute;t&eacute;letet hoz</ind></trd>
  </trdgrp>
  <trd lng="fr">arbitrer</trd>
</drv>

<drv mrk="arbitr1.0ulo">
  <kap><tld/>anto<fnt>Z</fnt>, <tld/>ulo</kap>
  <snc>
    <dif>
      Interkonsentita ju&gcirc;onto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_b;&c_ib;&c_t;&c_a;&c_r; <klr>(&c_u; &c_s;&c_u;&c_d;&c_z;&c_je;)</klr></trd>
  <trdgrp lng="de">
     <trd>Schlichter</trd>,
     <trd>Schiedsrichter</trd>,
     <trd>Vermittler</trd>
  </trdgrp>
  <trd lng="en">arbitrator</trd>
  <trd lng="fr">arbitre (juge)</trd>
  <trdgrp lng="hu">
     <trd>d&ouml;nt&odblac;b&iacute;r&oacute;</trd>,
     <trd>v&aacute;lasztott <ind>b&iacute;r&oacute;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_b;&c_i;&c_t;&c_r; <klr tip="amb">(&c_s;&c_u;&c_d;&c_mol;&c_ja;)</klr></trd>
</drv>
</art>
