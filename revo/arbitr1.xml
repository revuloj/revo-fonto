<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arbitr1.xml,v 1.23 2022/04/26 14:57:17 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>arbitraci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arbitr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="arbitr1.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="decid.0o">Decido</ref> pri ia grava malkonsento, farita de ju&gcirc;anto elektita de la
      disputantoj:
      <ekz>
        &ccirc;iu internacia malkonsento devus esti finita per
        <tld/>o;
      </ekz>
      <ekz>
        anta&ubreve; <klr>[&#8230;]</klr> <tld/>a ju&gcirc;antaro, la paroladoj povas da&ubreve;ri e&ccirc; plurajn
        tagojn
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Internacia <tld lit="A"/>a Kortumo en Hago
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1905 aperis la originala verko de <nom>Gaston Moch</nom> Historio Resuma de l&#8217; <tld
        lit="A"/>o Konstanta
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Ligo sukcesis almena&ubreve; eviti plurfoje militon inter malgrandaj landoj kaj ilin
        devigi al <tld/>o
        <fnt>
          <bib>FSp</bib>
          <lok>Tria Parto: Ligo De Nacioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la demando devas esti submetita al <tld/>a tribunalo, kiu konsistas el la
        &gcirc;enerala sekretario de UEA kaj du membroj de la Estraro
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="jugx.0o">ju&gcirc;o</ref>
    <ref tip="vid" cel="mediaci.0o">mediacio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_b;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>,
    <trd>&c_a;&c_r;&c_b;&c_ib;&c_t;&c_r;&c_a;&c_zh;&c_n;&c_y; &c_s;&c_u;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">arbitratge</trd>
  <trdgrp lng="cs">
    <trd>arbitr&aacute;&zcaron;</trd>,
    <trd>rozhodov&aacute;n&iacute;</trd>,
    <trd>sm&iacute;r&ccaron;&iacute; &rcaron;&iacute;zen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schiedsspruch</trd>,
    <trd>Schiedsurteil</trd>
  </trdgrp>
  <trd lng="en">arbitration</trd>
  <trd lng="es">arbitraje</trd>
  <trd lng="fr">arbitrage<klr> (juridique)</klr></trd>
  <trd lng="he">&bet;&vav;&resh;&resh;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;lasztott b&iacute;r&oacute;s&aacute;gi d&ouml;nt&eacute;s</trd>,
    <trd>v&aacute;lasztott b&iacute;r&oacute;s&aacute;gi &iacute;t&eacute;let</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_b;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j; &c_s;&c_u;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>arbitr&aacute;&zcaron;</trd>,
    <trd>rozsudok</trd>,
    <trd>zmierovac&iacute; s&uacute;d</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&c_b;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_je;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_s;&c_u;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="arbitr1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arbitr1.0i.JUR">
    <dif>
      <ref tip="super" cel="jugx.0i.JUR">Ju&gcirc;i</ref> per <tld/>o:
      <ekz>
        la milita le&gcirc;o rajtigis la armeon <tld/>i la konflikton
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        Unui&gcirc;intaj Nacioj ne sukcesis <tld/>i la konflikton en eksa Jugoslavio
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="&Steloj;monato2/006861.html"
              >&Steloj;monato2/006861.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la E&ubreve;ropa Ju&gcirc;kortumo <tld/>us disputojn inter la komisiono kaj la membro&scirc;tatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2014/011184.php"
              >E&ubreve;ropo: nepras forta, centra administrado</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">arbitrar</trd>
  <trdgrp lng="cs">
    <trd>rozhodnout rozhod&ccaron;&iacute;m</trd>,
    <trd>vyn&eacute;st rozsudek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>durch Schiedsspruch <ind>entscheiden</ind></trd>,
    <trd>schlichten</trd>,
    <trd>vermitteln</trd>
  </trdgrp>
  <trd lng="en">arbitrate</trd>
  <trd lng="es">arbitrar</trd>
  <trd lng="fr">arbitrer</trd>
  <trd lng="he">&lamed;&ayin;&shin;&vav;&tav; &bet;&vav;&resh;&resh;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>d&ouml;nt&odblac;b&iacute;r&aacute;skodik</trd>,
    <trd>d&ouml;nt&odblac;b&iacute;r&oacute;s&aacute;gi <ind>&iacute;t&eacute;letet hoz</ind></trd>
  </trdgrp>
  <trd lng="sk">vynies&tcaron; rozsudok</trd>
</drv>

<drv mrk="arbitr1.0ulo">
  <kap><tld/>anto, 
    <var><kap><tld/>ulo</kap></var>,
    <var><kap><tld/>isto</kap></var>
  </kap>
  <snc mrk="arbitr1.0ulo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Interkonsentita ju&gcirc;onto:
      <ekz>
        ne ekzistas inter ni <tld/>anto, kiu povus meti sian manon sur nin amba&ubreve;
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 9:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koncernaj partioj, interkonsente, decidas elekti trian sendependulon, nomatan
        <tld/>anto, kiu respondecos solvi la konflikton
        <fnt>
          <bib>Monato</bib>
          <aut>Grupo Esperanto Marsejlo</aut>
          <vrk><url ref="&Monato;/2014/011352.php"
              >&Ccirc;u ni devas timi TTIP?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mediaci.0isto">mediacianto</ref>,
      <ref cel="mediator.0o.JUR">mediatoro<sncref/></ref>,
      <ref cel="ombud.0o">ombudo</ref>
    </refgrp>
  </snc>
  <snc mrk="arbitr1.0ulo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Iu, kiu zorgas pri la respektado de la reguloj dum sporta konkurso:
      <ekz>
        <klr>[en la]</klr> futbalmat&ccirc;o inter Albanio kaj Nederlando <klr>[&#8230;]</klr> 
        venkis la nederlandanoj, kvankam dubinde, pro diskutebla decido de la brita
        <tld/>isto
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009336.php"
              >Brulanta afero</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        semajnfine &scirc;i agas kiel <tld/>antino &ccirc;e la mat&ccirc;oj de la <klr>[&#8230;]</klr> Virina
        Atleta Asocio
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009680.php"
              >Proksimaj kaj malproksimaj najbaroj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> timoplena <tld/>anto sukcesis ne elpeli serban futbaliston pro fa&ubreve;lo kaj tuj
        poste ne prifajfi klaran pun&scirc;oton por Italio
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010198.php"
              >Futbalejo a&ubreve; &ccirc;u batalejo?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        ru&gcirc;a karto montrata de <tld/>anto dum mat&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2011/010331.php"
              >Adia&ubreve;as fenomeno</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_b;&c_ib;&c_t;&c_a;&c_r; <klr>(&c_u; &c_s;&c_u;&c_d;&c_z;&c_je;)</klr></trd>
  <trdgrp lng="de">
    <trd>Schlichter</trd>,
    <trd>Schiedsrichter</trd>,
    <trd>Vermittler</trd>
  </trdgrp>
  <trd lng="en">arbitrator</trd>
  <trd lng="fr">arbitre (juge)</trd>
  <trd lng="he">&bet;&#1468;&vav;&#1465;&resh;&#1461;&resh;</trd>
  <trdgrp lng="hu">
    <trd>d&ouml;nt&odblac;b&iacute;r&oacute;</trd>,
    <trd>v&aacute;lasztott <ind>b&iacute;r&oacute;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_b;&c_i;&c_t;&c_r; <klr tip="amb">(&c_s;&c_u;&c_d;&c_mol;&c_ja;)</klr></trd>
  <trd lng="sk">arbiter</trd>
</drv>
</art>
