<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Ju&gcirc;i per <tld/>o.
</dif>
</snc>
<trd lng="fr">arbitrer</trd>
</drv>
<drv mrk="arbitr1.0ulo">
<kap><tld/>anto<fnt>Z</fnt>, <tld/>ulo</kap>
