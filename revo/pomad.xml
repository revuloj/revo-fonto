<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pomad.xml,v 1.22 2022/05/19 20:33:38 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>pomad</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="pomad.0o">
   <kap><tld/>o</kap>
   <snc mrk="pomad.0o.KOS">
     <uzo tip="fak">MED</uzo>
     <dif>
       Medikamenta a&ubreve; kosmetika &scirc;mira&jcirc;o:
      <ekz>
        li prenis <tld/>on el la po&scirc;o kaj tuj resanigis la malsanulon, kiu levi&gcirc;is en bona farto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj malsanuloj banis sin en fluanta akvo, aliajn oni &scirc;miris per bonodoraj <tld/>oj a&ubreve;
        incensis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la firmo liveros rapide &ccirc;iujn deziratajn artiklojn krom sentimentala
        butero, flat<tld/>o, ricin- a&ubreve; gad-oleo, kies provizo jam anta&ubreve; longe el&ccirc;erpi&gcirc;is
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_z;&c_mol;<klr> (&c_k;&c_a;&c_s;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;,
     &c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>,
     <trd>&c_k;&c_r;&c_e;&c_m;<klr> (&c_k;&c_a;&c_s;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;,
     &c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
   </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_m;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>mast na vlasy</trd>,
    <trd>pom&aacute;da</trd>
  </trdgrp>
  <trd lng="de">Pomade</trd>
  <trdgrp lng="en">
     <trd>pomade</trd>,
     <trd>ointment</trd>
   </trdgrp>
  <trd lng="fa">&f_p;&a_m;&a_A;&a_d;</trd>
  <trdgrp lng="fr">
     <trd>cr&egrave;me<klr> (pommade)</klr></trd>,
     <trd>pommade</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>kr&eacute;m <klr tip="amb"> (ken&odblac;cs)</klr></trd>,
     <trd>ken&odblac;cs</trd>
   </trdgrp>
  <trd lng="nl">pommade</trd>
  <trdgrp lng="pl">
    <trd>krem</trd>,
    <trd>ma&sacute;&cacute;</trd>,
    <trd>pomada</trd>
  </trdgrp>
  <trd lng="pt">pomada</trd>
  <trd lng="ro">crem</trd>
  <trdgrp lng="ru">
     <trd>&c_m;&c_a;&c_z;&c_mol;</trd>,
     <trd>&c_k;&c_r;&c_je;&c_m;</trd>
   </trdgrp>
  <trd lng="sk">mas&tcaron; na vlasy</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_a;&c_d;&c_a;</trd>,
    <trd>&#x43C;&c_a;&c_z;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="pomad.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="pomad.0i.KOS">
     <dif>
       <ref tip="vid" cel="sxmir.0i.surmeti">&Scirc;miri</ref>
       per <tld/>o:
      <ekz>
        <tld/>itaj, parfumitaj, brilantaj, bonodorantaj, torditaj en
        formo de misteraj ringoj, a&ubreve; falantaj sur la frunton en
        signifplena malordo, <klr>[la haroj]</klr> prezentis
        majstra&jcirc;on de la friza arto kaj tuj donis al la figuroj
        de la junaj komizoj elegantecon de tre alta grado
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;<klr> (&c_m;&c_a;&c_z;&c_mol;&c_z;&c_ju;,
   &c_k;&c_r;&c_e;&c_m;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="cs">
    <trd>napom&aacute;dovat</trd>,
    <trd>pom&aacute;dovat</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_m;&a_A;&a_d; &a_m;&a_A;&a_l;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">pommader</trd>
  <trd lng="nl">pommaderen</trd>
  <trdgrp lng="pl">
    <trd>smarowa&cacute; kremem ma&sacute;ci&aogonek;</trd>,
    <trd>pomadowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>se unge cu  <ind>crema</ind></trd>,
    <trd>se unge cu unguent</trd>,
    <trd>pomad&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_s;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;<klr> (&c_k;&c_r;&c_je;&c_m;&c_o;&c_m;, &c_m;&c_a;&c_z;&c_mol;&c_ju;)</klr></trd>
   </trdgrp>
  <trd lng="sk">pom&aacute;dova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_a;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43C;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
