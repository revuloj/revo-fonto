<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>pomad</rad>/o
</kap>
<drv mrk="pomad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Medikamenta a&ubreve; kosmetika &scirc;mira&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_z;&c_mol; <klr>(&c_k;&c_a;&c_s;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;, &c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_k;&c_r;&c_e;&c_m; <klr>(&c_k;&c_a;&c_s;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;, &c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kr&eacute;m <klr tip="amb"> (ken&odblac;cs)</klr></trd>,
    <trd>ken&odblac;cs</trd>
  </trdgrp>
  <trd lng="nl">pommade</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_z;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="pomad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_a;&c_z;&c_mol;&c_z;&c_ju;, &c_k;&c_r;&c_e;&c_m;&c_a;&c_m;)</klr></trd>
  <trd lng="nl">pommaderen</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_r;&c_je;&c_m;&c_o;&c_m;, &c_m;&c_a;&c_z;&c_mol;&c_ju;)</klr></trd>
  </trdgrp>
</drv>
</art>
