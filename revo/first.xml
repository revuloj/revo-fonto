<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>first</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="first.0o">
  <kap><tld/>o</kap>
  <snc mrk="first.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      La plej alta konveksa
      tegmento<ref tip="super" cel="supr.0o.KOMUNE">supro</ref>.  
    </dif>
    <refgrp tip="vid">
      <ref cel="egx.0o">e&gcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_l;&c_mol;&c_ch;&c_a;&c_k;</trd>
  <trd lng="cs">h&rcaron;eben <klr>(st&rcaron;echy| hory)</klr></trd>
  <trd lng="de">First</trd>
  <trd lng="en">coping</trd>
  <trdgrp lng="es">
    <trd>cumbrera</trd>,
    <trd>caballete</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cr&ecirc;te<klr tip="ind"> (d'un toit)</klr></trd>,
    <trd>faitage<klr tip="ind"> (d'un toit)</klr></trd>,
    <trd>fa&icirc;te<klr tip="ind"> (d'un toit)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>orom</trd>,
    <trd>tet&odblac;gerinc</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nok</trd>,
    <trd>dakvorst</trd>,
    <trd>vorst<klr tip="ind"> (dakvorst)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kalenica</trd>,
    <trd>szczyt dachu</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cumeeira</trd>,
    <trd>cumeada</trd>,
    <trd>cavalete de telhado</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k; &c_k;&c_r;&c_y;&c_sh;&c_i;
  </trd>
  <trd lng="sk">hrebe&ncaron; strechy</trd>
</drv>

<drv mrk="first.0olinio">
  <kap><tld/>olinio</kap>
  <snc>
    <dif>
      Tegmenta suprolinio.  
    </dif>
  </snc>
  <adm>
    PIV1 havas alian difinon, kiu ne referencas al tegmento.
    &Gcirc;i estas konsiderata geografia termino. <aut>MB</aut>
  </adm>
  <trd lng="de">Firstlinie</trd>
  <trd lng="fr">ligne de cr&ecirc;te<klr tip="ind"> (d'un toit)</klr>
  </trd>
  <trd lng="hu">&eacute;lgerinc</trd>
  <trd lng="nl">noklijn</trd>
  <trd lng="pt">linha de <ind>cumeeira</ind></trd>
  <trd lng="sk">hrebe&ncaron;ov&aacute; &ccaron;as&tcaron; pohoria</trd>
  <trd lng="cs">h&rcaron;ebenov&aacute; &ccaron;&aacute;ra
  poho&rcaron;&iacute;</trd>
</drv>
</art>
