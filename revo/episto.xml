<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>epistol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="episto.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Beletra versa letero: <ekz>la <tld/>oj de Horacio.</ekz>
    </dif>
    <trd lng="de">Epistel</trd>
    <trd lng="el">&Epsilon;&pi;&jota;&sigma;&tau;&omikron;&lambda;&eta_ton;</trd>
    <trdgrp lng="hu">
      <trd>k&ouml;lt&odblac;i lev&eacute;l</trd>,
      <trd>episztola</trd>
    </trdgrp>
    <trd lng="it">epistola</trd>
    <trd lng="pl">episto&lstroke;a</trd>
  </snc>
  <trd lng="pt">ep&iacute;stola</trd>
  <snc num="2">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Apostola letero en la Nova Testamento.  
    </dif>
    <trd lng="de">Apostelbrief</trd>
    <trdgrp lng="it">
      <trd>epistola<klr> (rel.)</klr></trd>,
      <trd>lettera<klr> (degli Apostoli)</klr></trd>
    </trdgrp>
    <trd lng="pl">list apostolski</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_p;&c_ib;&c_s;&c_t;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="en">epistle</trd>
  <trd lng="es">ep&iacute;stola</trd>
  <trd lng="fr">&eacute;p&icirc;tre</trd>
  <trd lng="hu">szentlecke</trd>
  <trd lng="pt">ep&iacute;stola</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_p;&c_i;&c_s;&c_t;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
