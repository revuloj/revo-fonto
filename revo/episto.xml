<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: episto.xml,v 1.20 2019/09/22 15:10:16 revo Exp $">
<kap>
  <rad>epistol</rad>/o <fnt><bib>NT</bib></fnt>
</kap>
<drv mrk="episto.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="episto.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="leter.0o.POSX">Letero</ref>:
    </dif>
    <subsnc mrk="episto.0o.BELE">
      <uzo tip="fak">BELE</uzo>
      <dif>
        Beletra versa letero:
        <ekz>
          la <tld/>oj de Horacio.
        </ekz>
      </dif>
      <trd lng="de">Epistel</trd>
      <trd lng="el">&Epsilon;&pi;&jota;&sigma;&tau;&omikron;&lambda;&eta_ton;</trd>
      <trdgrp lng="hu">
        <trd>k&ouml;lt&odblac;i lev&eacute;l</trd>,
        <trd>episztola</trd>
      </trdgrp>
      <trd lng="it">epistola</trd>
      <trd lng="pl">episto&lstroke;a</trd>
    </subsnc>
    <subsnc mrk="episto.0o.KAT">
      <uzo tip="fak">KAT</uzo>
      <dif>
        Apostola letero en la Nova Testamento:
        <ekz>
          mi, Tertio, kiu skribis &ccirc;i tiun <tld/>on, salutas vin en la Sinjoro
          <fnt>
            <bib>NT</bib>
            <lok>Romanoj 16:22</lok>
          </fnt>;
        </ekz>  
        <ekz>
          en mia <tld/>o mi skribis al vi, ke vi ne intermiksu vin kun mal&ccirc;astuloj
          <fnt>
            <bib>NT</bib>
            <lok>I. Korintanoj 5:9</lok>
          </fnt>;
        </ekz>
        <ekz>
          iuj partoj de la <tld/>oj de Pa&ubreve;lo estas inter la plej belaj en la Nova
          Testamento
          <fnt>
            <bib>Monato</bib>
            <aut>W. H. Simcock</aut>
            <vrk><url ref="https://www.monato.be/2006/008724.php"
                >Kortu&scirc;a tenero</url></vrk>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">Apostelbrief</trd>
      <trdgrp lng="it">
        <trd>epistola<klr> (rel.)</klr></trd>,
        <trd>lettera<klr> (degli Apostoli)</klr></trd>
      </trdgrp>
      <trd lng="pl">list apostolski</trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_p;&c_ib;&c_s;&c_t;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">ep&iacute;stola</trd>
  <trdgrp lng="cs">
    <trd>epi&scaron;tola</trd>,
    <trd>list v&ecaron;&rcaron;&iacute;c&iacute;m</trd>
  </trdgrp>
  <trd lng="en">epistle</trd>
  <trd lng="es">ep&iacute;stola</trd>
  <trd lng="fr">&eacute;p&icirc;tre</trd>
  <trd lng="hu">szentlecke</trd>
  <trd lng="pt">ep&iacute;stola</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_p;&c_i;&c_s;&c_t;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>epi&scaron;tola</trd>,
    <trd>list veriacim</trd>
  </trdgrp>
</drv>
</art>
