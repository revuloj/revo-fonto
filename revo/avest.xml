<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>avest</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="avest.0o">
  <kap><tld lit="A"/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Baza <ref tip="super" cel="skrib.SanktaS0o">sankta skribo</ref> de
      <ref tip="vid" cel="mazda.0ismo">mazdaismo</ref> redaktita en
      antikva irana lingvo, la&ubreve;tradicie parte verkita de <ref tip="vid"
      cel="zaratusxtr.0o">Zaratu&scirc;tro</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Awesta</trd>,
    <trd>Awesta-Schriften</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Avesta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Avesta</trd>
  </trdgrp>
</drv>

<drv mrk="avest.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld lit="A"/>o, al la irana lingvo, al la arameida
      skribo per kiuj &gcirc;i estis redaktita.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>awestisch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avestique</trd>
  </trdgrp>
</drv>
</art>

