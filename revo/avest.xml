<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: avest.xml,v 1.17 2023/10/28 08:33:11 revo Exp $">
<kap>
  <rad>Avest</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="avest.0o">
  <kap><tld/>o</kap>
  <snc mrk="avest.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="skrib.SanktaS0o">Sankta skribo</ref> de
      <ref tip="vid" cel="mazda.0ismo">mazdaismo</ref> redaktita en
      antikva irana lingvo, la&ubreve;tradicie parte verkita de <ref tip="vid" cel="zaratusxtr.0o">Zaratu&scirc;tro</ref>:
      <ekz>
        fakuloj opinias ke la traduko de profesoro <nom>Stanley Insler</nom> estas
        unu el la plej bonaj tradukoj el <tld/>o
        <fnt>
        <aut>Be&ccirc;jo</aut>,
          <vrk>El Avesto (2)</vrk>,
          <lok><url ref="http://becxjo.blogspot.com/">La mondo el mia
          vidpunkto</url>, 2005-11-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_v;&c_e;&c_s;&c_t;&c_a;</trd>
  <trd lng="ca">Avesta</trd>
  <trd lng="cs">Avesta</trd>
  <trdgrp lng="de">
    <trd>Awesta</trd>,
    <trd>Awesta-Schriften</trd>
  </trdgrp>
  <trd lng="es">Avesta</trd>
  <trd lng="fa">&a_A;&a_w;&a_s;&a_t;&a_A;</trd>
  <trd lng="fr">Avesta</trd>
  <trd lng="hu">Aveszta</trd>
  <trd lng="it">Avesta</trd>
  <trd lng="ja">&#x30A2;&#x30D9;&#x30B9;&#x30BF;</trd>
  <trd lng="ru">&c_A;&c_v;&c_je;&c_s;&c_t;&c_a;</trd>
  <trd lng="sk">Avesta</trd>
  <trd lng="uk">&c_A;&c_v;&c_je;&c_s;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x963F;&#x7DAD;&#x65AF;&#x9640;&#x8A9E; <pr>&#x101;w&#xE9;is&#x12B;tu&#xF3;y&#x1D4;</pr></trd>,
    <trd>&#x963F;&#x7EF4;&#x65AF;&#x9640;&#x8BED; <pr>&#x101;w&#xE9;is&#x12B;tu&#xF3;y&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="avest.0a">
  <kap><tld lit="a"/>a</kap>
  <snc mrk="avest.0a.REL">
    <dif>
      Rilata al <tld lit="A"/>o, al la stato de la irana lingvo, al
      la arameida skribo per kiuj &gcirc;i estis redaktita.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x963F;&#x7DAD;&#x65AF;&#x9640;&#x8A9E; <pr>&#x101;w&#xE9;is&#x12B;tu&#xF3;y&#x1D4;</pr></trd>,
    <trd>&#x963F;&#x7EF4;&#x65AF;&#x9640;&#x8BED; <pr>&#x101;w&#xE9;is&#x12B;tu&#xF3;y&#x1D4;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_v;&c_e;&c_s;&c_t;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">av&egrave;stic</trd>
  <trd lng="de">awestisch</trd>
  <trd lng="fa">&a_A;&a_w;&a_s;&a_t;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">avestique</trd>
  <trdgrp lng="hu">
    <trd>avesztai</trd>,
    <trd>avesztikus</trd>
  </trdgrp>
  <trd lng="it">avestico</trd>
  <trd lng="ru">&c_a;&c_v;&c_je;&c_s;&c_t;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
