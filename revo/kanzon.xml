<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kanzon</rad>/o
</kap>
<drv mrk="kanzon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Versa&jcirc;o, plejofte gaja, sprita a&ubreve; moka, konsistanta el
      egalaj strofoj fini&gcirc;antaj per rekanta&jcirc;o kaj destinita por
      esti kantata:
      <ekz>la <tld/>oj de B&eacute;ranger;</ekz>
      <ekz>la vin<tld/>oj de Horacio.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_n;&c_s;&c_o;&c_n;</trd>     
  <trd lng="br">kanaouenn</trd>
  <trdgrp lng="de">
     <trd>Kanzone</trd>,
     <trd>Chanson</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>song</trd>,
    <trd>ballad</trd>,
    <trd>chanson</trd>,
    <trd>canzone</trd>
  </trdgrp>
  <trd lng="fr">chanson</trd>
  <trdgrp lng="nl">
    <trd>lied</trd>,
    <trd>luisterlied</trd>,
    <trd>chanson</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>dal</trd>,
     <trd>n&oacute;ta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_s;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_l;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_c;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_s;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>
