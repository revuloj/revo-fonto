<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>lob</rad>/o
</kap>
<drv mrk="lob.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lob.0o.ANA">
    <dif>
      Rondforma elstara parto de iu <ref tip="malprt" 
      cel="organ.0o.BIO">organo</ref>:
      <ekz>
        &scirc;iaj oreloj ne estis de la sama grandeco: unu estis granda, kun 
        longega <tld/>o, kaj la alia estis eta, kaj preska&ubreve; sen <tld/>o
        <fnt>
          <aut>D. K. Jordan</aut>,
          <vrk>La Re&gcirc;idino Mal</vrk>,
          <lok><url ref="http://esperanto.ca/kea/j-enkonduko.html">La Fabeloj de Jordan</url>, [vidita en 2006-05-24]</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; <tld/>oj de nazo
        (<ref tip="sin" cel="al1.0o">alo</ref>);
      </ekz>
      <ekz>
        la <tld/>o de hepato.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_l;&c_ja; <klr tip="amb">(&c_v;&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="de">
     <trd>Lappen</trd>,
     <trd>Fl&uuml;gel</trd>,
     <trd>Lobus</trd>
  </trdgrp>
  <trd lng="el">&lambda;&omikron;&beta;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">lobe</trd>
  <trd lng="es">l&oacute;bulo</trd>
  <trd lng="fr">lobe</trd>
  <trdgrp lng="hu">
    <trd>lebeny</trd>,
    <trd>cimpa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lob</trd>,
    <trd>lel</trd>
  </trdgrp>
  <trd lng="pt">lobo</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_ja; <klr tip="amb">(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
</drv>
</art>
