<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>lob</rad>/o
</kap>
<drv mrk="lob.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Rondforma elstara parto de iu organo:
      <ekz>
        <tld/>o de orelo;
      </ekz>
      <ekz>
        amba&ubreve; <tld/>oj de nazo
        (<ref tip="sin" cel="al1.0o">alo</ref>);
      </ekz>
      <ekz>
        la <tld/>o de hepato.
      </ekz>
    </dif>
  </snc>
  <trd lng="el">&lambda;&omikron;&beta;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">lobe</trd>
  <trd lng="es">l&oacute;bulo</trd>
  <trdgrp lng="nl">
    <trd>lob</trd>,
    <trd>lel</trd>
  </trdgrp>
  <trd lng="pt">lobo</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_ja; <klr tip="amb">(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
</drv>
</art>
