<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tuberkul</rad>/o
</kap>
<drv mrk="tuberk1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsaneca tubereto ka&ubreve;zita de la bacilo de tuberkulozo kaj
      kreskanta precipe en pulmoj, intestoj, ostoj, &ccirc;e homo kaj besto.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;</trd>
  <trd lng="de">Tuberkel</trd>
  <trdgrp lng="hu">
    <trd>g&uuml;m&odblac;</trd>,
    <trd>gum&oacute;cska</trd>,
    <trd>tuberculum</trd>
  </trdgrp>
  <trd lng="nl">tuberkel</trd>
  <trd lng="pt">tub&eacute;rculo</trd>
  <trd lng="ru">&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;</trd>
</drv>
</art>
