<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tuberk1.xml,v 1.15 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>3</ofc><rad>tuberkul</rad>/o
</kap>
<drv mrk="tuberk1.0o">
  <kap><ofc>3</ofc><tld/>o</kap>
    <snc>
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="tuber.0o.sxvelajxo">Tubereto</ref>,
      i.a. tia malsaneca ka&ubreve;zita de la bacilo de 
      <ref tip="malprt" cel="tuberk.0o">tuberkulozo</ref> kaj
      kreskanta precipe en pulmoj, intestoj, ostoj, &ccirc;e homo kaj
      besto:
      <ekz>
        tuberkulozo a&ubreve; ftizo estas bakteria malsano karakterizata de <tld/>oj en organoj de homo a&ubreve; alia besto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24335">Tuberkulozo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
         trafita de peritoneito <tld/>a finstadia kun la ventro tre &scirc;velinta kaj rigida, kaj en grava &gcirc;enerala sanstato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=405650">Voja&gcirc;o al Lourdes</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj araneoj kun ok okuloj grupigitaj en <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=583742">Mesothelae</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;</trd>
  <trd lng="ca">tubercle<klr> (pat.)</klr></trd>
  <trd lng="de">Tuberkel</trd>
  <trd lng="fr">tubercule</trd>
  <trdgrp lng="hu">
    <trd>g&uuml;m&odblac;</trd>,
    <trd>gum&oacute;cska</trd>,
    <trd>tuberculum</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7D50;&#x7BC0;  <pr>&#x3051;&#x3063;&#x305B;&#x3064;</pr></trd>,
    <trd>&#x7D50;&#x6838;&#x7D50;&#x7BC0;  <pr>&#x3051;&#x3063;&#x304B;&#x304F;&#x3051;&#x3064;&#x305B;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">tuberkel</trd>
  <trdgrp lng="pl">
    <trd>ziarniak</trd>,
    <trd>gruze&lstroke;ek<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">tub&eacute;rculo</trd>
  <trd lng="ru">&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x431;&#x435;&#x440;&#x43A;&#x443;&#x43B;&#x430;</trd>,
    <trd>&#x433;&#x43E;&#x440;&#x431;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
