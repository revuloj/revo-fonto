<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hidrant.xml,v 1.7 2015/09/21 13:10:24 revo Exp $">
<kap>
  <rad>hidrant</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hidrant.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidrant.0o.AKV">
    <dif>
      Urba <ref tip="super" cel="fontan.0o.AKV">fontano</ref>
      destinita al surloka provizado de akvo kontra&ubreve; brulegoj:
      <ekz>
        je du metroj de la fenestro grandformata policisto de severa
        teni&gcirc;o apogis sin al <tld/>o
        <fnt>
          <aut>O Henry, trad. S. Johansson</aut>,
          <vrk>Policisto kaj himno</vrk>,
          <bib>LOdE</bib>, <lok>2002-11 (97)</lok>
        </fnt>;
      </ekz>
      <ekz>
        el&scirc;prucis akvo kaj &gcirc;i &scirc;prucis kiel el <tld/>o
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url
ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve; 
fabeloj</url></vrk>,
        <lok>Granda polica fabelo, [2002?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hydrant</trd>
  <trd lng="en">hydrant</trd>
  <trdgrp lng="fr">
    <trd>bouche d'incendie</trd>,
    <trd>poteau d'incendie</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>idrante</trd>,
    <trd><ind>pompa</ind> antincendio</trd>,
    <trd><ind>presa</ind> d'acqua</trd>
  </trdgrp>
  <trd lng="pl">hydrant</trd>
  <trd lng="pt">hidrante</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_d;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_y;&c_j;
&c_k;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>

