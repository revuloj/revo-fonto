<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dign.xml,v 1.28 2024/05/05 08:24:48 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>dign</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="dign.0o">
  <kap><tld/>o</kap>
  <snc mrk="dign.0o.respekto">
    <dif>
      <ref tip="super" cel="respek.0o">Respekto</ref>, &scirc;uldata al io (pro ties valoro),
      iu (pro ties konduto a&ubreve; pozicio) a&ubreve; si mem:
      <ekz>
        esta&jcirc;oj posedantaj privilegiojn, <tld/>on kaj feli&ccirc;on de duondioj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne estis e&ccirc; unu, kiu ne havus la senton de sia potenco kaj <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> pleni&gcirc;is de certa solena <tld/>o kiel kiam li la unuan fojon
        alportis la brandujon de la patro
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu ejo <klr>[&#8230;]</klr> plene respondas <klr>[&#8230;]</klr> al la <tld/>o de la oratoro
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj homoj estas denaske liberaj kaj egalaj la&ubreve; <tld/>o kaj rajtoj
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ven&gcirc;o konas nur nomojn kaj ne homojn <klr>[sed]</klr> por la homa <tld/>o de
        la estonta generacio, rigardu la homojn kaj ne la nomojn
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>!
      </ekz>
      <ekz>
        estingi&gcirc;is la homa <tld/>o kaj virto <klr>[&#8230;]</klr> kaj la malnobli&gcirc;intaj koroj
        ne kura&gcirc;is e&ccirc; allasi al si esperon
        <fnt>
          <bib>QuV</bib>
          <lok>Epilogo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>dignity</trd>,
      <trd>respect</trd>,
      <trd>worth</trd>
    </trdgrp>
    <trd lng="de">schuldiger Respekt</trd>
    <trdgrp lng="pl">
      <trd>godno&sacute;&cacute;</trd>,
      <trd>szacunek</trd>
    </trdgrp>
    <trd lng="zh">&#20511;&#26041; <pr>ji&egrave;f&amacron;ng</pr></trd>
  </snc>
  <snc mrk="dign.0o.sindetenemo">
    <dif>
      Sindetenema konduto de iu konscia pri la respekto, kiun li
      meritas:
      <ekz>
        &scirc;i akceptis la gastojn kun <tld/>o, kiu igis &scirc;in ankora&ubreve; pli respektinda
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sinjoroj degnis danci kun la kampulinoj, sed la sinjorinoj
        gardis sian <tld/>on
        <fnt><bib>Revadoj</bib>, <lok>Na&ubreve;a promenado</lok></fnt>;
      </ekz>
      <ekz>
        eksplodo de virina fiereco kaj de homa <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li alparolis la gloran &Scirc;emidon kun princa <tld/>o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="degn.0i">degno</ref>,
      <ref cel="ten.sinde0o">sindeteno</ref>,
      <ref cel="indign.0o">indigno</ref>
    </refgrp>
    <trd lng="de">W&uuml;rde</trd>
    <trdgrp lng="en">
      <trd>dignity</trd>,
      <trd>self-respect</trd>,
      <trd>self-worth</trd>
    </trdgrp>
    <trd lng="es">dignidad</trd>
    <trdgrp lng="pl">
      <trd>godno&sacute;&cacute;</trd>,
      <trd>dostojno&sacute;&cacute;</trd>,
      <trd>cze&sacute;&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="fr">dignit&eacute;</trd>
  <trd lng="he">&kaf;&bet;&vav;&dalet; &ayin;&tsadi;&mem;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#23041;&#21427;  <pr>&#12356;&#12370;&#12435;</pr></trd>,
    <trd>&#23562;&#21427;  <pr>&#12381;&#12435;&#12370;&#12435;</pr></trd>,
    <trd>&#21697;&#20301;  <pr>&#12402;&#12435;&#12356;</pr></trd>,
    <trd>&#33258;&#23562;&#24515;  <pr>&#12376;&#12381;&#12435;&#12375;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dignidade</trd>,
    <trd>decoro</trd>,
    <trd>brio</trd>,
    <trd>pundonor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>dignitet</trd>,
    <trd>v&auml;rdighet</trd>
  </trdgrp>
  <trd lng="sk">d&ocirc;stojnos&tcaron;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;stojenstv&iacute;</trd>,
    <trd>hodnost</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_ib;&c_d;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="dign.0a">
  <kap><tld/>a</kap>
  <snc mrk="dign.0a.respektinda">
    <dif>
      Meritanta respekton:
      <ekz>
        <tld/>a socia pozicio
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nia vojo &ccirc;iam devas esti <tld/>a kaj rekta<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eminen1.0a">eminenta</ref>,
      <ref cel="merit.0a">merita</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>respektgebietend</trd>,
      <trd>respektheischend</trd>
    </trdgrp>
    <trd lng="fr">digne<klr tip="ind">
    (m&eacute;ritant le respect)</klr></trd>
    <trdgrp lng="pl">
      <trd>godny</trd>,
      <trd>dostojny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>,
      <trd>
  &c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;
      &c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_o;&c_g;&c_o;
      &c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dign.0a.konscia">
    <dif>
      Konscia, ke oni meritas respekton; kondutanta tiel, ke la
      aliaj homoj sentas tian konscion; <tld/>oplena:
      <ekz>
        <klr>[la policano]</klr> estas alta viro, kiu sin tenas tre
        <tld/>e
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>w&uuml;rdig</trd>
    </trdgrp>
    <trd lng="fr">digne<klr tip="ind"> (grave, conscient de son
    m&eacute;rite)</klr></trd>
    <trdgrp lng="pl">
      <trd>godny</trd>,
      <trd>dostojny</trd>,
      <trd>dystyngowany</trd>
    </trdgrp>
    <trd lng="ru">
    &c_d;&c_je;&c_r;&c_zh;&c_a;&c_shch;&c_i;&c_j;&c_s;&c_ja;
    &c_s; &c_d;&c_o;&c_s;&c_t;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;&c_m;
    </trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="ind.0a">Inda</ref>, kapabla, meritanta:
      <ekz>
        rigardu, kiu estas la plej bona kaj plej <tld/>a el la filoj de
        via sinjoro
        <fnt><bib>MT</bib>, <lok>&Re2; 10:3</lok></fnt>;
      </ekz>
      <ekz>
        li mem ne sentis sin plu <ind>ne<tld/>a</ind>, por eniri en la
        <klr>[&#8230;]</klr> sanktejon
        <fnt><bib>Fab1</bib>, <lok>&Ccirc;efa momento</lok></fnt>;
        <trdgrp lng="fr">
          <trd>indigne</trd>,
          <trd>inapte</trd>
        </trdgrp>
        <trd lng="ru">
        &c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        kiel longe vi funebros pri <nom>Saul</nom>, kiun Mi trovis ne<tld/>a
        re&gcirc;i super <nom>Izrael</nom>?
        <fnt><bib>MT</bib>, <lok>&Sa1; 16:1</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>w&uuml;rdig</trd>
    </trdgrp>
    <trd lng="fr">digne<klr tip="ind"> (m&eacute;ritant de)</klr></trd>
    <trdgrp lng="pl">
      <trd>godny</trd>,
      <trd>godzien</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="bo">&t_nya;&t_ma;&t_sa;&t_tsheg;&t_da;&t_naro;&t_da;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="cs">d&uring;stojn&yacute;</trd>
  <trd lng="en">dignified</trd>
  <trd lng="es">digno</trd>
  <trd lng="he">&mem;&kaf;&vav;&bet;&dalet;</trd>
  <trdgrp lng="ja">
    <trd>&#23041;&#21427;&#12398;&#12354;&#12427;  <pr>&#12356;&#12370;&#12435;&#12398;&#12354;&#12427;</pr></trd>,
    <trd>&#22530;&#12293;&#12392;&#12375;&#12383;  <pr>&#12393;&#12358;&#12393;&#12358;&#12392;&#12375;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="sk">d&ocirc;stojn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20957;&#37325; <pr>n&iacute;ngzh&ograve;ng</pr></trd>,
    <trd>&#31471;&#24196; <pr>du&amacron;nzhu&amacron;ng</pr></trd>,
    <trd>&#23041;&#20005; <pr>w&emacron;iy&aacute;n</pr></trd>,
    <trd>&#38605;&#23481;&#21326;&#36149; <pr>y&omacron;ngr&oacute;nghu&aacute;gu&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dign.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="dign.0eco.PSI">
    <dif>
      Eco de io a&ubreve; iu <tld/>a:
      <ekz>
        &ccirc;u ni ne vivas en libera lando, kie, la&ubreve;tradicie,
        la digneco de homa persono estas respektata?
        <fnt><bib>Lasu</bib> <lok>La tri plendoj de s-ro
        Kursano</lok></fnt>.
      </ekz>
    </dif>
    <adm>
      Mankas ekz-oj por bone pritaksi la sencnuancon kun "~o".
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;stojnost</trd>,
    <trd>velebnost</trd>,
    <trd>v&aacute;&zcaron;nost</trd>
  </trdgrp>
  <trd lng="de">W&uuml;rde</trd>
  <trd lng="en">dignity</trd>
  <trd lng="es">dignidad</trd>
  <trdgrp lng="pl">
    <trd>godno&sacute;&cacute;</trd>,
    <trd>dostojno&sacute;&cacute;</trd>,
    <trd>dostoje&nacute;stwo</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;
  </trd>
  <trd lng="sk">d&ocirc;stojnos&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>dignitet</trd>,
    <trd>v&auml;rdighet</trd>
  </trdgrp>
  <trd lng="zh">&#23562;&#20005; <pr>z&umacron;ny&aacute;n</pr></trd>
</drv>

<drv mrk="dign.0oplena">
  <kap>
    <tld/>oplena
  </kap>
  <snc>
    <dif>
      Kun videbla, respektpostula <tld/>o:
      <ekz>
        la interesanta ekstera&jcirc;o de la juna virino, <klr>[&#8230;]</klr>
        la <ind><tld/>oplena teni&gcirc;o</ind> <klr>[&#8230;]</klr>
        pligrandigis la fervorecon de lia penado
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun facila, <tld/>oplena gracieco kaj afabla rideto en siaj bonkoraj, trankvilaj
        okuloj, li riverencis la &ccirc;eestantojn, dirante:
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        duo da belaj, fortmuskolaj &ccirc;evaloj &gcirc;in tiris, altpa&scirc;e kaj kun <tld/>oplena
        malrapideco
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">w&uuml;rdevoll</trd>
  <trd lng="en">dignified</trd>
  <trdgrp lng="fr">
    <trd>digne</trd>,
    <trd>respectable</trd>,
    <trd>grave<klr tip="ind"> (digne)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="dign.mem0o">
  <kap>mem<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dign.mem0o.PSI">
    <dif>
      <ref tip="dif" cel="estim.mem0o.PSI">Memestimo</ref>:
      <ekz>
        estis organizita milito kontra&ubreve; mia sento pri mem<tld/>o,
        kontra&ubreve; mia memvaloro
        <fnt><bib>Lasu</bib> <lok>La tri plendoj de s-ro
        Kursano</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&uring;stojnost</trd>,
    <trd>sebe&uacute;cta</trd>
  </trdgrp>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_p;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>
  <trd lng="de">Selbstachtung</trd>
  <trdgrp lng="en">
    <trd>dignity</trd>,
    <trd>self-respect</trd>,
    <trd>self-worth</trd>
  </trdgrp>
  <trd lng="es">autoestima</trd>
  <trdgrp lng="fr">
    <trd>estime de soi</trd>,
    <trd>amour-propre</trd>
  </trdgrp>
  <trd lng="it">autostima</trd>
  <trd lng="pl">pewno&sacute;&cacute; siebie</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o; &c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_g;&c_o; <ind>&c_d;&c_o;&c_s;&c_t;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_a;</ind></trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;stojnos&tcaron;</trd>,
    <trd>seba&uacute;cta</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33258;&#24859; <pr>z&igrave;&agrave;i</pr></trd>,
    <trd>&#33258;&#37325; <pr>z&igrave;zh&ograve;ng</pr></trd>,
    <trd>&#33258;&#23562; <pr>z&igrave;z&umacron;n</pr></trd>
  </trdgrp>
</drv>
</art>
