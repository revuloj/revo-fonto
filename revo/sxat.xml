<drv mrk="sxat.mal0i">
  <kap><ofc>*</ofc>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxat.mal0i.KOMUNE">
    <dif>
      Rigardi kiel tute neatentindan, nekalkulindan, preterlasindan,
      sensignifan, senvaloran, malgravan:
      <ekz>
        ne mal<tld/>u la instruon de via patro
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 15:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne mal<tld/>u vian patrinon, kiam &scirc;i maljuni&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 23:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia peto ne povis esti mal<tld/>ata
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sata animo mal<tld/>as mieltavolon
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 27:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mal<tld/>is sian &jcirc;uron kaj rompis lian interligon
        <fnt>
          <bib>MT</bib>
          <lok>&Jeh; 17:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        malestima mal<tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>e rigardi la aferon;
      </ekz>
      <ekz>
        mal<tld/>e rigardi iun de alte
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden</vrk>
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>e montri sian dorson al iu
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kracx.0i">kra&ccirc;i</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="atent">malatenti</ref>,
      <ref cel="konsid">malkonsideri</ref>,
      <ref cel="zorg.mal0i">malzorgi</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pland.0o">plando</ref>,
      <ref cel="super">super</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="hu">
