<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ta&ccirc;ment</rad>/o
</kap>
<drv mrk="tacxme.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Parto de regimento mar&scirc;anta a&ubreve; operacianta izole:
      <ekz>
        tuta <tld/>o da tamburistoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">detachement</trd>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_ja;&c_d;</trd>
    <trd lng="sv">truppavdelning</trd>
  </snc>
  <snc>
    <dif>
      Aro da homoj farantaj apartan laboron, a&ubreve; periode laborantaj dum
      alia aro ripozas ktp alterne.
    </dif>
    <trd lng="nl">ploeg</trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a; <klr tip="amb">(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_h;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Aro da ludantoj en konkurso; partio.
    </dif>
    <refgrp tip="sin">
      <ref cel="rot.0o">roto</ref>,
      <ref cel="kohort.0o">kohorto</ref>.
    </refgrp>
    <trd lng="nl">partij</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a; <klr tip="amb">(&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja;)</klr></trd>
  </snc>
  <trd lng="de">Trupp</trd>
</drv>
</art>
