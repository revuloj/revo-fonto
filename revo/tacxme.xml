<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tacxme.xml,v 1.34 2024/05/10 08:31:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ta&ccirc;ment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tacxme.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tacxme.0o.KOMUNE">
    <dif>
      Aparta <ref tip="super" cel="grup.0o.personoj">grupo</ref> komisiita
      pri iu <ref cel="misi.0o.komisio">misio</ref> a&ubreve; <ref cel="task.0o.ordonita">tasko</ref>:
      <ekz>
        <nom>Jakob</nom> <klr>[&#x2026;]</klr> dividis la homojn, kiuj estis kun li, kaj la &scirc;afojn kaj la
        bovojn kaj la kamelojn, en du <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:7</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="tacxme.0o.MIL">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Parto de <ref tip="malprt" cel="regime.0o">regimento</ref> mar&scirc;anta a&ubreve; operacianta izole:
        <ekz>
          anta&ubreve; unu horo trans la riveron iris <tld/>o da soldatoj el 200 personoj
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          lia <tld/>o kaj &gcirc;iaj kalkulitoj estas sepdek kvar mil sescent
          <fnt>
            <bib>MT</bib>
            <lok>Nombroj 2:4</lok>
          </fnt>;
        </ekz>
        <ekz>
          post ili iris du malgrandaj infanteriaj <tld/>oj, unu armita per lancoj, la alia
          per hakiloj
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro I</lok>
          </fnt>;
        </ekz>
        <ekz>
          tuta <tld/>o da tamburistoj
          <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de Feli&ccirc;o</lok></fnt>;
        </ekz>
        <ekz>
          partizanaj komunistaj <tld/>oj
          <fnt>
          <aut>Zlatko Ti&scaron;ljar</aut>
            <vrk><url ref="&Monato;/2001/007251.php">Ordo tordita -- dekstren
            maldekstren</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 7</lok>
          </fnt>;
        </ekz>
        <ekz>
          <ind>pac<tld/>oj de Unui&gcirc;intaj Nacioj</ind>
         <fnt>
	<aut>Garbhan MacAoidh</aut>
	<vrk><url ref="&Monato;/2000/007003.php">El flamo en fajron</url></vrk>
	<bib>Monato</bib>
	<lok>2000/06, p. 6</lok>
        </fnt>.
          <trd lng="de">UN-<ind>Friedenstruppen</ind></trd>
          <trd lng="nl">VN-<ind>vredesmacht</ind></trd>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="batali.0o">bataliono</ref>,
        <ref cel="brigad.0o.MIL">brigado<sncref/></ref>,
        <ref cel="kohort.0o.MIL">kohorto<sncref/></ref>
      </refgrp>
      <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_d;</trd>
      <trd lng="ca">destacament</trd>
      <trdgrp lng="de">
        <trd>Trupp</trd>,
        <trd>Abteilung</trd>,
        <trd>Heerschar</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>detachment<klr tip="ind"> (military unit)</klr></trd>
      </trdgrp>
      <trd lng="es">destacamento</trd> 
      <trdgrp lng="it">
        <trd>distaccamento<klr> (mil.)</klr></trd>,
        <trd>squadra<klr> (mil.)</klr></trd>,
        <trd>commando</trd>,
        <trd>reparto<klr> (mil.)</klr></trd>,
        <trd>drappello<klr> (mil.)</klr></trd>,
        <trd>manipolo<klr> (mil.)</klr></trd>,
        <trd>compagnia<klr> (mil.)</klr></trd>,
      </trdgrp>
      <trd lng="nl">detachement</trd>
      <trd lng="pl">oddzia&lstroke;</trd>
      <trdgrp lng="pt">
        <trd>destacamento</trd>,
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_ja;&c_d;</trd>
      <trd lng="sv">truppavdelning</trd>
    </subsnc>
    <subsnc mrk="tacxme.0o.LAB">
      <dif>
        Aro da homoj farantaj apartan laboron:
        <ekz>
          sur la fregato &#x201A;Danujo&#x2018; <klr>[&#x2026;]</klr> mi servis en la &scirc;ip<tld/>o
          <fnt>
            <bib>Fab2</bib>
            <lok>Dano Holger</lok>
          </fnt>;
        </ekz>
        <ekz>
          kun studentaj konstru<tld/>oj mi estis en <nom>Sa&hcirc;alin</nom>
          <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 18a, p. 108a</lok>
          </fnt>;
        </ekz>  
        <ekz>
          propaganda <tld/>o
          <fnt>
            <aut>Raymond Schwartz</aut>
            <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
            <lok>La Propaganda Ta&ccirc;mento</lok>
          </fnt>;
        </ekz>  
        <ekz>
          pordo malfermi&gcirc;is por trapasigi la <tld/>on de &gcirc;ardenistoj
          <fnt>
            <bib>KPr</bib>
          </fnt>;
        </ekz>
        <ekz>
          la studentan domon eniris perforte <tld/>o da serbaj policistoj kaj arestis lin
          <fnt><aut>Bardhyl Selimi</aut>: 
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
        </ekz>
        <ekz>
          la <tld/>anoj, laborante skipe, dum unu horo fosis profundan kavon
          <fnt>
            <bib>MkM</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="brigad.0o.FIG">brigado<sncref/></ref>,
        <ref cel="skip.0o">skipo</ref>
      </refgrp>
      <refgrp tip="vid">
         <ref cel="sekci.0o.POL">sekcio<sncref/></ref>
      </refgrp>
      <trd lng="be">&c_b;&c_r;&c_y;&c_g;&c_a;&c_d;&c_a;
      <klr>(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_y;&c_h;)</klr></trd>
      <trdgrp lng="ca">
        <trd>colla</trd>,
        <trd>torn</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Trupp</trd>,
        <trd>Abteilung</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>team</trd>,
        <trd>shift</trd>,
        <trd>gang</trd>,
        <trd>squad</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>nucleo<klr> (squadra)</klr></trd>,
        <trd>squadra</trd>,
        <trd>equipe</trd>,
        <trd>turno<klr> (squadra)</klr></trd>
      </trdgrp>
      <trd lng="nl">ploeg</trd>
      <trdgrp lng="pl">
        <trd>zmiana</trd>,
        <trd>hufiec</trd>,
        <trd>brygada</trd>
      </trdgrp>
      <trd lng="ru">&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a;
      <klr tip="amb">(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_h;)</klr></trd>
    </subsnc>
    <ref tip="sin" cel="trup.0o">trupo</ref>


<!-- ne trovi&gcirc;is ekz-oj, PIV ne havas tiun sencon...
    <subsnc mrk="tacxme.0o.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Kunludantoj en konkurso.
      </dif>
      <refgrp tip="vid">
        <ref cel="parti.0o.JUR">partio<sncref/></ref>,
        <ref cel="team.0o.SPO">teamo</ref>
      </refgrp>
      <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_a;
      <klr>(&c_s;&c_p;&c_a;&c_r;&c_t;.)</klr></trd>
      <trd lng="ca">equip</trd>
      <trdgrp lng="de">
        <trd>Mannschaft</trd>,
        <trd>Truppe</trd>,
        <trd>Gruppe</trd>,
        <trd>Elf<klr> <klr>(Fu&szlig;ball)</klr></klr></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>team</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>gruppo</trd>,
        <trd>squadra</trd>,
        <trd>team</trd>
      </trdgrp>
      <trd lng="nl">partij</trd>
      <trdgrp lng="pl">
        <trd>dru&zdot;yna</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a;
      <klr tip="amb">
      (&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja;)
      </klr></trd>
    </subsnc>
-->
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;tachement</trd>,
    <trd>&eacute;quipe</trd>,
    <trd>mission<klr tip="amb"> (&eacute;quipe)</klr></trd>
  </trdgrp>
  <trd lng="hu">csapat</trd>
  <trdgrp lng="ja">
    <trd>&#x5206;&#x9063;&#x968A;  <pr>&#x3076;&#x3093;&#x3051;&#x3093;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x90E8;&#x968A;  <pr>&#x3076;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x30C1;&#x30FC;&#x30E0;  <pr>&#x3061;&#x30FC;&#x3080;</pr></trd>,
    <trd>&#x73ED;  <pr>&#x306F;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_g;&c_ib;&c_n;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a; (&c_r;&c_o;&c_b;&c_ib;&c_t;&c_n;&c_i;&c_k;&c_ib;&c_v;)</trd>,
    <trd>&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x56E2;&#x961F; <pr>tu&#xE1;ndu&#xEC;</pr></trd>,
    <trd>&#x5718;&#x968A; <pr>tu&#xE1;ndu&#xEC;</pr></trd>,
    <trd>&#x961F; <pr>du&#xEC;</pr></trd>,
    <trd>&#x968A; <pr>du&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tacxme.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tacxme.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="kun.0igi">Kunigi</ref> <tld/>on kaj 
      <ref tip="vid" cel="task.0i.KOMUNE">taski</ref>
      &gcirc;in pri iu faro:
      <ekz>
        li tuj <tld/>u siajn plej ta&ubreve;gajn spertulinojn
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>El la vivo de bervala senta&ubreve;gulo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>eine Truppe <ind>zusammenstellen</ind></trd>,
      <trd>sammeln <klr>(Truppe)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol; <ind>&c_a;&c_t;&c_r;&c_a;&c_d;</ind></trd>,
    <trd>&c_a;&c_b;&#x2019;&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; &c_u;
    <ind>&c_a;&c_t;&c_r;&c_a;&c_d;</ind></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;tacher<klr> (mil.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5206;&#x9063;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x3088;&#x3053;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6D3E;&#x9063;&#x3059;&#x308B;  <pr>&#x306F;&#x3051;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
</drv>
</art>
