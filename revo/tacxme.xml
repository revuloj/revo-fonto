<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ta&ccirc;ment</rad>/o
</kap>
<drv mrk="tacxme.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Hom<ref tip="super" cel="grup.0o.personoj">grupo</ref> komisiita pri
      iu misio a&ubreve; tasko:
    </dif>
    <subsnc mrk="tacxme.0o.MIL">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Parto de regimento mar&scirc;anta a&ubreve; operacianta izole:
        <ekz>
          tuta <tld/>o da tamburistoj
          <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, II, kio okazis al la jura konsilisto</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_d;</trd>
      <trdgrp lng="de">
        <trd>Trupp</trd>,
        <trd>Abteilung</trd>
      </trdgrp>
      <trd lng="nl">detachement</trd>
      <trdgrp lng="pt">
        <trd>destacamento</trd>,
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_ja;&c_d;</trd>
      <trd lng="sv">truppavdelning</trd>
    </subsnc>
    <subsnc mrk="tacxme.0o.LAB">
      <dif>
        Aro da homoj farantaj apartan laboron, a&ubreve; periode
        laborantaj dum alia aro ripozas:
        <ekz>
          kun studentaj konstruta&ccirc;mentoj mi estis en Sa&hcirc;alin
          <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 18a, p. 108a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_b;&c_r;&c_y;&c_g;&c_a;&c_d;&c_a; <klr>(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_y;&c_h;)</klr></trd>
      <trdgrp lng="de">
        <trd>Trupp</trd>,
        <trd>Abteilung</trd>
      </trdgrp>
      <trd lng="nl">ploeg</trd>
      <trd lng="ru">&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a; <klr tip="amb">(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_h;)</klr></trd>
    </subsnc>
    <subsnc mrk="tacxme.0o.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Kunludantoj en konkurso.
      </dif>
      <refgrp tip="vid">
        <ref cel="parti.0o.JUR">partio <sncref/></ref>,
        <ref cel="team.0o.SPO">teamo</ref>
      </refgrp>
      <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_a; <klr>(&c_s;&c_p;&c_a;&c_r;&c_t;.)</klr></trd>
      <trdgrp lng="de">
        <trd>Mannschaft</trd>,
        <trd>Truppe</trd>,
        <trd>Gruppe</trd>,
        <trd>Elf<klr> [futbalo]</klr></trd>
      </trdgrp>
      <trd lng="nl">partij</trd>
      <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a; <klr tip="amb">(&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja;)</klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;tachement</trd>,
    <trd>&eacute;quipe</trd>
    <trd>mission<klr tip="amb"> (&eacute;quipe)</klr></trd>
  </trdgrp>
  <trd lng="hu">csapat</trd>
</drv>
</art>
