<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>azen</rad>/o
</kap>
<drv mrk="azen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="cxeval.0o.genro">&ccirc;evalo</ref>
      (<trd lng="la">Equus asinus</trd>), kun longaj oreloj, uzata kiel
      &scirc;ar&gcirc;besto.
    </dif>
    <bld lok="../bld/azen.jpg">
     <tld/>o
    </bld>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malklera&ccirc;a homo:
      <ekz>forveturis <tld/>ido kaj revenis <tld/>o<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="de">Esel</trd>
  <trd lng="en">ass</trd>
  <trd lng="es">asno</trd>
  <trd lng="fr">&acirc;ne</trd>
  <trd lng="hu">szam&aacute;r</trd>
  <trd lng="nl">ezel</trd>
  <trd lng="ru">&#1086;&#1089;&#1105;&#1083;</trd>
  <trd lng="tr">e&scedil;&scedil;ek</trd>
  <trd lng="sv">&aring;sna</trd>
</drv>
</art>
