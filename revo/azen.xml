<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>azen</rad>/o
</kap>
<drv mrk="azen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="azen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="cxeval.0o.genro">&ccirc;evalo</ref>
      (<trd lng="la">Equus asinus</trd>), kun longaj oreloj, uzata kiel
      &scirc;ar&gcirc;besto.
    </dif>
    <bld lok="../bld/azen.jpg">
      <tld/>o
    </bld>
  <trd lng="en">donkey</trd>
  </snc>
  <snc mrk="azen.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malklera&ccirc;a homo:
      <ekz>
        forveturis <tld/>ido kaj revenis <tld/>o<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_jo;&c_l;</trd>
  <trd lng="bg">&c_m;&c_a;&c_g;&c_a;&c_r;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>ase</trd>
    <trd>ruc</trd>
    <trd>burro</trd>
    <trd>somer</trd>
  </trdgrp>
  <trd lng="br">azen</trd>
  <trd lng="de">Esel</trd>
  <trd lng="en">ass</trd>
  <trdgrp lng="es">
    <trd>asno</trd>,
    <trd>burro</trd>
  </trdgrp>
  <trd lng="fr">&acirc;ne</trd>
  <trd lng="he">&het;&mem;&vav;&resh;</trd>
  <trd lng="hu">szam&aacute;r</trd>
  <trd lng="nl">ezel</trd>
  <trd lng="ru">&c_o;&c_s;&c_jo;&c_l;</trd>
  <trd lng="tr">e&scedil;&scedil;ek</trd>
  <trd lng="sv">&aring;sna</trd>
</drv>
</art>
