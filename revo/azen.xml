<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>azen</rad>/o
</kap>
<drv mrk="azen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="cxeval.0o.genro">&ccirc;evalo</ref>
      (<trd lng="la">Equus asinus</trd>), kun longaj oreloj, uzata kiel     
      &scirc;ar&gcirc;besto.
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Malklera&ccirc;a homo:
      <ekz>
        forveturis <tld/>ido kaj revenis <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="de">Esel</trd>
  <trd lng="en">ass</trd>
  <trd lng="fr">&acirc;ne</trd>
  <trd lng="tr">e&scedil;&scedil;ek</trd>
</drv>
</art>
