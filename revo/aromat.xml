<vortaro>
  <art mrk="$Id: aromat.xml,v 1.4 2024/09/02 21:26:13 afido Exp $">
    <kap>
      <rad>aromat</rad>/a
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="aromat.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <klr>(pri kombina&jcirc;o, molekulo)</klr>
      Havanta ciklan &ccirc;enon kiel &ccirc;e benzeno:
      <ekz>
        <tld/>aj aminoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138240"
              >Amino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        policiklaj <tld/>aj hidrokarbonaj partikloj, kiajn produktas a&ubreve;tomobilaj
        motoroj, tabak-fumado, kamparaj bruloj, hejma hejtado ktp
        <fnt>
          <aut>Paulo S&eacute;rgio Viana</aut>
          <vrk><url ref="&Monato;/2015/011485.php"
              >Malpura aero dama&gcirc;as infanan cerbon</url></vrk>
              <bib>Monato</bib>
          <lok>2015/06, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trd lng="de">aromatisch</trd>
    <trd lng="en">aromatic</trd>
    <trd lng="fr">aromatique</trd>
    <trd lng="nl">aromatisch</trd>
    <trd lng="pl">aromatyczny</trd>
    <trd lng="ru">&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
</drv>
  </art>

