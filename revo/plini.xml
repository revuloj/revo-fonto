<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plini.xml,v 1.20 2024/02/17 07:52:16 revo Exp $">
<kap>
  <rad>Plini</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="plini.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        legado de Aristotelo, <tld/>o, Strabono
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_l;&c_ib;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_P;&c_l;&c_i;&c_n;&c_i;&c_j;</trd>
  <trd lng="de">Plinius</trd>
  <trd lng="en">Pliny</trd>
  <trd lng="fr">Pline</trd>
  <trd lng="hu">Plinius</trd>
  <trd lng="it">Plinio</trd>    
  <trd lng="lat">Plinius</trd>
  <trd lng="nl">Plinius</trd>
  <trd lng="pl">Pliniusz</trd>
  <trd lng="pt">Pl&iacute;nio</trd>
  <trd lng="ru">&c_P;&c_l;&c_i;&c_n;&c_i;&c_j;</trd>
  <trd lng="uk">&#x41F;&#x43B;&#x456;&#x43D;&#x456;&#x439;</trd>
</drv>

<drv mrk="plini.0o_juna">
  <kap><tld/>o la Juna</kap>
   <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="advoka.0o" lst="voko:historiaj_personoj">Advokato</ref>, registo kaj beletristo
      romia (proksimume 61 &ndash;
      proksimume
      114), a&ubreve;toro de latinaj retora&jcirc;oj kaj leteroj, nepo kaj adoptita
      filo de <tld/>o la Maljuna:
      <ekz>
        de <klr>[Trajano]</klr> kaj lia amiko <tld/>o la pli juna postrestis 
        pluraj leteroj el ilia korespondado, famaj estas amba&ubreve; leteroj
        kiuj temas pri la maniero, kiel trakti kristanojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17181">Trajano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_l;&c_ib;&c_n;&c_ib;&c_j; &c_M;&c_a;&c_l;&c_o;&c_d;&c_sh;&c_y;</trd>
  <trd lng="bg">&c_P;&c_l;&c_i;&c_n;&c_i;&c_j; &c_M;&c_l;&c_a;&c_d;&c_i;</trd>
  <trd lng="de">Plinius der J&uuml;ngere</trd>
  <trd lng="en">Pliny the Younger</trd>
  <trd lng="fr">Pline le Jeune</trd>
  <trd lng="hu">ifjabb <ind>Plinius</ind></trd>
  <trd lng="it"><ind>Plinio</ind> il Giovane</trd>     
  <trd lng="lat">Caius <ind>Plinius Caecilius Secundus</ind></trd>
  <trd lng="nl">Plinius de Jongere</trd>
  <trd lng="pl">Gajusz <ind>Pliniusz</ind> Cecyliusz M&lstroke;odszy</trd>
  <trd lng="pt">Pl&iacute;nio, o Mo&ccedil;o</trd>
  <trd lng="ru">&c_P;&c_l;&c_i;&c_n;&c_i;&c_j; &c_M;&c_l;&c_a;&c_d;&c_sh;&c_i;&c_j;</trd>
  <trd lng="zh">&#x5C0F;&#x666E;&#x6797;&#x5C3C; <pr>xi&#x1CE;op&#x1D4;l&#xED;nn&#xED;</pr></trd>
</drv>

<drv mrk="plini.0o_maljuna">
  <kap><tld/>o la Maljuna</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Natur<ref tip="lst" cel="scienc.natur0isto" lst="voko:historiaj_personoj">sciencisto</ref> kaj oficiro
      Romia (23 &ndash; 79), a&ubreve;toro de latinaj
      gramatikaj, priartaj kaj sciencaj verkoj, onklo de <tld/>o la
      Juna:
      <ekz>
        <klr>[&#x2026;]</klr> konata natursciencisto, <tld/>o la maljuna, kiu
        cetere estis komandanto de la milit&scirc;iparo en Miseno
        <fnt><bib>IntArkeol</bib>, <lok>p. 25a</lok></fnt>;
      </ekz>
      <ekz>
        unukornulo ne ekzistas en la malnova greka mitologio sed jam la greka 
        historiisto Ktesio kaj la roma historiisto <tld/>o la pli maljuna konis &gcirc;in
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4313">Unukornulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_l;&c_ib;&c_n;&c_ib;&c_j; &c_S;&c_t;&c_a;&c_r;&c_e;&c_j;&c_sh;&c_y;</trd>
  <trd lng="bg">&c_P;&c_l;&c_i;&c_n;&c_i;&c_j; &c_S;&c_t;&c_a;&c_r;&c_i;</trd>
  <trd lng="de">Plinius der &Auml;ltere</trd>
  <trd lng="en">Pliny the Elder</trd>
  <trd lng="fr">Pline l'Ancien</trd>
  <trd lng="hu">id&odblac;sebb <ind>Plinius</ind></trd>
  <trd lng="it"><ind>Plinio</ind> il Vecchio</trd>    
  <trd lng="lat">Caius <ind>Plinius Secundus</ind></trd>
  <trd lng="nl">Plinius de Oudere</trd>
  <trd lng="pl">Gajusz <ind>Pliniusz</ind> Starszy</trd>
  <trd lng="pt">Pl&iacute;nio, o Velho</trd>
  <trd lng="ru">&c_P;&c_l;&c_i;&c_n;&c_i;&c_j; &c_S;&c_t;&c_a;&c_r;&c_sh;&c_i;&c_j;</trd>
  <trd lng="zh">&#x8001;&#x666E;&#x6797;&#x5C3C; <pr>l&#x1CE;op&#x1D4;l&#xED;nn&#xED;</pr></trd>
</drv>
</art>
