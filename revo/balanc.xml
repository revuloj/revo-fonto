<art mrk="$Id: balanc.xml,v 1.13 2022/04/26 15:00:32 revo Exp $">
<kap>
  <rad>balancel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="balanc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc mrk="balanc.0o.velsxipo">
    <dif>
      <ref tip="super" cel="sxip.vel0o">Vel&scirc;ipo</ref> uzata precipe &ccirc;irka&ubreve; Napolo &gcirc;is la 20a jc, 
      &ccirc;irka&ubreve; 10&#x202F;m longa, kun granda velo fiksita sur anteno:
      <ekz>
        italdevenaj elmigrintoj konstruis kaj uzis <tld/>ojn en la golfo de Sanfrancisko.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/61/Edward_William_Cooke_Vesuvius%2C_catalan_and_paranzella.jpg/640px-Edward_William_Cooke_Vesuvius%2C_catalan_and_paranzella.jpg" prm="PD"><tld/>o anta&ubreve; Vezuvio, pentra&jcirc;o de Edward William Cooke, anta&ubreve; 1847
        <fnt>
          <aut>Edward William Cooke, http://www.artrenewal.org/artwork/857/1857/31832/vesuvius,_catalan_and_paranzella-huge.jpg</aut>
          <lok><url ref="&WCW;/File:Edward_William_Cooke_Vesuvius,_catalan_and_paranzella.jpg">&WCW;/File:Edward_William_Cooke_Vesuvius,_catalan_and_paranzella.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">balancel&middot;la</trd>
  <trd lng="cs">mal&aacute; lo&dcaron; s velkou plachtou</trd>
  <trdgrp lng="de">
    <trd>Bilancella <klr>(Einmaster)</klr></trd>,
    <trd>Paranzella <klr>(Einmaster)</klr></trd>,
  </trdgrp>
  <trd lng="fr">balancelle <klr tip="ind">(voilier)</klr></trd>
  <trd lng="he">&#x5E0;&#x5B7;&#x5D3;&#x5B0;&#x5E0;&#x5B5;&#x5D3;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">balancella <klr tip="amb">egy&aacute;rbocos nagy haj&oacute;</klr></trd>
  <trd lng="sk">mal&aacute; lo&dcaron; s ve&lcaron;kou plachtou</trd>
  <trd lng="uk">&#x43E;&#x434;&#x43D;&#x43E;&#x449;&#x43E;&#x433;&#x43B;&#x43E;&#x432;&#x438;&#x439; &#x43A;&#x43E;&#x440;&#x430;&#x431;&#x435;&#x43B;&#x44C;</trd>
</drv>
