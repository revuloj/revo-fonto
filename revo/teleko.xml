<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: teleko.xml,v 1.16 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>telekomunik</rad>/i
</kap>
<drv mrk="teleko.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEL</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Sendi informon per elektromagnetaj rimedoj:
      <ekz>
        <tld/>i al la fiska administracio sian enspezodeklaron.
      </ekz>
    </dif>
    <ref tip="vid" cel="komuni1.0i.sciigi">komuniki <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z; &c_s;&c_r;&c_o;&c_d;&c_k;&c_ib; &c_s;&c_u;&c_v;&c_ja;&c_z;&c_ib;)</klr></trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;communiquer</trd>,
    <trd>envoyer par <ind>t&eacute;l&eacute;communication</ind></trd>
  </trdgrp>
  <trd lng="id">ber<ind>telekomunikasi</ind></trd>
  <trd lng="ja">&#x9060;&#x8DDD;&#x96E2;&#x901A;&#x4FE1;&#x3067;&#x9001;&#x308B;  <pr>&#x3048;&#x3093;&#x304D;&#x3087;&#x308A;&#x3064;&#x3046;&#x3057;&#x3093;&#x3067;&#x304A;&#x304F;&#x308B;</pr></trd>
  <trd lng="pl">telekomunikowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>telecomunicar</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&#x43B;&#x44F;&c_t;&c_i;/ &#x43F;&c_je;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43E;
  &c_t;&c_je;&#x43B;&c_je;&c_b;&c_a;&c_ch;&c_je;&#x43D;&#x43D;&#x44E;</trd>
</drv>

<drv mrk="teleko.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="teleko.0ado.KOMUNE">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <snc mrk="teleko.0ado.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref>
      pri la manieroj <tld/>i informon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_e;&c_l;&c_je;&c_k;&c_a;&c_m;&c_u;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_u;&c_v;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_m;&c_u;&c_n;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>komunikace na d&aacute;lku</trd>,
    <trd>telekomunika&ccaron;n&iacute; spojen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Telekommunikation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;communication</trd>,
    <trd>t&eacute;l&eacute;com</trd>
  </trdgrp>
  <trd lng="id">telekomunikasi</trd>
  <trdgrp lng="ja">
    <trd>&#x9060;&#x8DDD;&#x96E2;&#x901A;&#x4FE1;  <pr>&#x3048;&#x3093;&#x304D;&#x3087;&#x308A;&#x3064;&#x3046;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x96FB;&#x6C17;&#x901A;&#x4FE1;  <pr>&#x3067;&#x3093;&#x304D;&#x3064;&#x3046;&#x3057;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">telekomunikacja</trd>
  <trdgrp lng="pt">
    <trd>telecomunica&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_m;&c_m;&c_u;&c_n;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">komunik&aacute;cia na dia&lcaron;ku</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&#x43B;&c_je;&#x43A;&#x43E;&#x43C;&c_u;&#x43D;&c_ib;&#x43A;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_t;&c_je;&#x43B;&c_je;&c_z;&c_v;&#x2019;&#x44F;&c_z;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

