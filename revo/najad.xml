   <rad>najad</rad>/o
   <fnt><bib>Fab4</bib></fnt>
</kap>
<drv mrk="najad.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Nimfo de fontoj kaj riveroj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la patrino estis senhalta fonto de demandoj, la filino estis la bela <tld/>o de
        tiu fonto, kiu nedisigeble apartenis al &gcirc;i
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_ja;&c_d;&c_a;</trd>
  <trd lng="ca">n&agrave;iade</trd>
  <trdgrp lng="cs">
    <trd>jezern&iacute; panna</trd>,
    <trd>naj&aacute;da</trd>,
    <trd>rusalka</trd>,
    <trd>vodn&iacute; panna</trd>,
    <trd>vodn&iacute; v&iacute;la</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Najade</trd>,
     <trd>Quellnymphe</trd>,
     <trd>Flussnymphe</trd>
   </trdgrp>
  <trd lng="el">&ny;&alfa;&jota_trema;&alfa_ton;&delta;&alfa;</trd>
  <trdgrp lng="en">
     <trd>naiad</trd>,
     <trd>water nymph</trd>
   </trdgrp>
  <trd lng="es">n&aacute;yade</trd>
  <trd lng="fr">na&iuml;ade</trd>
  <trdgrp lng="hu">
     <trd>naj&aacute;d</trd>,
     <trd>v&iacute;zi <ind>t&uuml;nd&eacute;r</ind></trd>
   </trdgrp>
  <trd lng="ja">&#x30CA;&#x30FC;&#x30A4;&#x30A2;&#x30B9;</trd>
  <trd lng="nl">waternimf</trd>
  <trd lng="pl">najada</trd>
  <trdgrp lng="pt">
     <trd>n&aacute;iade</trd>,
     <trd>n&aacute;iada</trd>
   </trdgrp>
   <trd lng="ro">naiad&abreve;</trd>
  <trd lng="ru">&c_n;&c_a;&c_ja;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>najada</trd>,
    <trd>rusalka</trd>,
    <trd>vodn&aacute; v&iacute;la</trd>
  </trdgrp>
  <trd lng="sv">najad</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x44F;&#x434;&#x430;</trd>,
    <trd>&#x43D;&#x456;&#x43C;&#x444;&#x430; &#x432;&#x43E;&#x434;</trd>,
    <trd>&#x440;&#x456;&#x437;&#x443;&#x445;&#x430;</trd>
  </trdgrp>
</drv>
