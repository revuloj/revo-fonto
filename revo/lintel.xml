<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lintel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lintel.0o">
  <kap><tld/>o</kap>
  <snc mrk="lintel.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Scirc;tona, ligna a&ubreve; fera trabo, peco, bloko horizontale metita sur la
      fostoj de fenestro a&ubreve; pordo.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_sh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_shch;&c_u;&c_r;&c_c;</trd>
  <trdgrp lng="de">
    <trd><klr>T&uuml;r-, Fenster-</klr>Sturz</trd>,
    <trd>Oberschwelle</trd>
  </trdgrp>
  <trd lng="en">lintel</trd>
  <trd lng="fr">linteau</trd>
  <trdgrp lng="hu">
    <trd>szem&ouml;ld&ouml;kfa</trd>,
    <trd>tokfej</trd>
  </trdgrp>
  <trd lng="nl">dwarsbalk</trd>
  <trdgrp lng="pt">
    <trd>lintel</trd>,
    <trd>dintel</trd>,
    <trd>padieira</trd>,
    <trd>pavieira</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_t;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_y;&c_ch;&c_k;&c_a; <klr>&c_n;&c_a;&c_d; &c_p;&c_r;&c_o;&c_jo;&c_m;&c_o;&c_m;</klr></trd>
  </trdgrp>
</drv>
</art>
