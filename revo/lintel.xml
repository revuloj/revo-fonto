<art mrk="$Id: lintel.xml,v 1.21 2024/01/15 19:02:15 revo Exp $">
<kap>
  <rad>lintel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lintel.0o">
  <kap><tld/>o</kap>
  <snc mrk="lintel.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="trab.0o">Trabo</ref>, peco, bloko
      horizontale metita sur la
      <ref cel="fost.0o">fostoj</ref> de fenestro a&ubreve; pordo:
      <ekz>
        ni pensis, ke estus bona blago dumnokte farbi sur la <tld/>o de
        la &ccirc;efa enirejo
        la frazon: &leftquot;Koncesiita por muziko kaj por la vendado de vino kaj
        alkohola&jcirc;oj&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sojl.0o.KON">sojlo<sncref/></ref>,
       <ref cel="bret.fenestro0o">fenestrobreto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_sh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_shch;&c_u;&c_r;&c_c;</trd>
  <trd lng="cs">p&rcaron;eklad <klr>(stav.)</klr></trd>
  <trdgrp lng="de">
    <trd><klr>T&uuml;r-, Fenster-</klr>Sturz</trd>,
    <trd>Oberschwelle</trd>
  </trdgrp>
  <trd lng="en">lintel</trd>
  <trd lng="fr">linteau</trd>
  <trdgrp lng="hu">
    <trd>szem&ouml;ld&ouml;kfa</trd>,
    <trd>tokfej</trd>
  </trdgrp>
  <trd lng="ja">&#x307E;&#x3050;&#x3055;</trd>
  <trd lng="nl">dwarsbalk</trd>
  <trd lng="pl">nadpro&zdot;e</trd>
  <trdgrp lng="pt">
    <trd>lintel</trd>,
    <trd>dintel</trd>,
    <trd>padieira</trd>,
    <trd>pavieira</trd>
  </trdgrp>
  <trd lng="ro">buiandrug</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_t;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_y;&c_ch;&c_k;&c_a; <klr>&c_n;&c_a;&c_d; &c_p;&c_r;&c_o;&c_jo;&c_m;&c_o;&c_m;</klr></trd>
  </trdgrp>
  <trd lng="sk">preklad</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x435;&#x43C;&#x438;&#x447;&#x43A;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x6963; <pr>m&#xE9;i</pr></trd>,
    <trd>&#x6A2A;&#x6963; <pr>h&#xE9;ngm&#xE9;i</pr></trd>,
    <trd>&#x6A6B;&#x6963; <pr>h&#xE9;ngm&#xE9;i</pr></trd>,
    <trd>&#x6A2A;&#x6963;&#x5B50; <pr>h&#xE9;ngm&#xE9;izi</pr></trd>,
    <trd>&#x6A6B;&#x6963;&#x5B50; <pr>h&#xE9;ngm&#xE9;izi</pr></trd>
  </trdgrp>
</drv>
</art>
