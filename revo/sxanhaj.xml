<vortaro>
<art mrk="$Id$">
  <kap>
  <rad>&Scirc;anhaj</rad>o<fnt>Z</fnt>
 </kap>
<drv mrk="sxanhaj.SX0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> kaj ekonomia centro en suda
      <ref tip="prt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>:
      <ekz>
        la sistemo &leftquot;<nom>Transrapid</nom>&#8221;, jam instalita kaj provata inter la &ccirc;ina urbo <tld/>o
        kaj ties flughaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard F&ouml;ssmeier</aut>
          <vrk><url ref="&Monato;/2004/008200.html"
              >&Ccirc;u iam rapidos Transrapid?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        onklo <nom>Ma</nom> kaj lia edzino fiksas la rigardon al la &scirc;oseo kondukanta al <tld/>o, la
        plej granda urbo de &Ccirc;inio
        <fnt>
          <bib>Monato</bib>
          <aut>Xu Jinming</aut>
          <vrk><url ref="&Monato;/2004/008203.html"
              >Solas gefiloj, solas gepatroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la municipo <tld/>o la denseco estas 37&#8239;000 homoj je kvadrata kilometro
        <fnt>
          <bib>Monato</bib>
          <aut>MU Binghua</aut>
          <vrk><url ref="&Monato;/2004/008325.html"
              >Kvin defioj por &ccirc;inaj urboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i iom dormetis, kaj son&gcirc;is pri <tld/>o, pri siaj hejmo kaj gepatroj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php"
              >En fremda lando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Sh;&c_a;&c_n;&c_h;&c_a;&c_j;</trd>
    <trd lng="ca">Xangai</trd>
    <trd lng="de">Schanghai</trd>
    <trd lng="en">Shanghai</trd>
    <trd lng="fr">Shanghai</trd>
    <trd lng="hu">Sanghaj</trd>
    <trd lng="it">Shanghai</trd>
    <trd lng="pt">Xangai</trd>
    <trd lng="ru">&c_Sh;&c_a;&c_n;&c_h;&c_a;&c_j;</trd>
  </snc>
</drv>
 
