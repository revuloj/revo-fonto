<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxanhaj.xml,v 1.19 2023/11/30 01:36:12 revo Exp $">
  <kap>
  <rad>&Scirc;anhaj</rad>/o,
  <var><kap><rad var="&hcirc;">&Scirc;an&hcirc;aj</rad>/o <fnt><bib>FK</bib></fnt></kap></var>
 </kap>
<drv mrk="sxanhaj.SX0o">
  <kap><tld/>o, <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc mrk="sxanhaj.SX0o.urbo">
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> kaj ekonomia centro en
      suda
      <ref tip="prt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>:
      <ekz>
        la lo&gcirc;antoj de Kantono, <tld var="&hcirc;"/>o, Fut&scirc;ano a&ubreve; Amojo parolas &hcirc;ine
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sistemo &leftquot;<nom>Transrapid</nom>&#x201D;, jam instalita kaj provata inter la &ccirc;ina urbo
        <tld/>o
        kaj ties flughaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard F&ouml;ssmeier</aut>
          <vrk><url ref="&Monato;/2004/008200.html">&Ccirc;u iam rapidos Transrapid?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        onklo <nom>Ma</nom> kaj lia edzino fiksas la rigardon al la &scirc;oseo kondukanta al <tld/>o, la
        plej granda urbo de &Ccirc;inio
        <fnt>
          <bib>Monato</bib>
          <aut>Xu Jinming</aut>
          <vrk><url ref="&Monato;/2004/008203.html">Solas gefiloj, solas gepatroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la municipo <tld/>o la denseco estas 37&#x202F;000 homoj je kvadrata kilometro
        <fnt>
          <bib>Monato</bib>
          <aut>MU Binghua</aut>
          <vrk><url ref="&Monato;/2004/008325.html">Kvin defioj por &ccirc;inaj urboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i iom dormetis, kaj son&gcirc;is pri <tld/>o, pri siaj hejmo kaj gepatroj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En fremda lando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_Sh;&c_a;&c_n;&c_h;&c_a;&c_j;</trd>
    <trd lng="ca">Xangai</trd>
    <trd lng="de">Schanghai</trd>
    <trd lng="en">Shanghai</trd>
    <trd lng="fr">Shanghai</trd>
    <trd lng="hu">Sanghaj</trd>
    <trd lng="it">Shanghai</trd>
    <trd lng="ja">&#x30B7;&#x30E3;&#x30F3;&#x30CF;&#x30A4;</trd>
    <trd lng="pl">Szanghaj</trd>
    <trd lng="pt">Xangai</trd>
    <trd lng="ru">&c_Sh;&c_a;&c_n;&c_h;&c_a;&c_j;</trd>
    <trd lng="uk">&c_Sh;&c_a;&#x43D;&c_h;&c_a;&c_j;</trd>
</drv>
 </art>

