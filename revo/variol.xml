<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>variol</rad>/o
</kap>
<drv mrk="variol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Infekta malsano, karakterizata de febro kaj pustuloj sur la ha&ubreve;to,
      lasantaj cikatretojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="vakcin">vakcino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Pocken</trd>,
    <trd>Blattern</trd>
  </trdgrp>
  <trd lng="nl">pokken</trd>
  <trd lng="ru">&c_o;&c_s;&c_p;&c_a;</trd>
</drv>
</art>
