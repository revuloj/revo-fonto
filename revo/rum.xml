  <trd lng="nl">rum</trd>
  <trd lng="pl">rum</trd>
  <trd lng="pt">rum</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;</trd>
  <trd lng="sk">rum</trd>
  <trd lng="uk">&c_r;&#x43E;&#x43C;</trd>
  <trdgrp lng="zh">
