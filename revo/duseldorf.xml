<vortaro>
  <art mrk="$Id: duseldorf.xml,v 1.2 2022/01/19 08:33:38 revo Exp $">
    <kap>
      <rad>Duseldorf</rad>/o
    </kap>

<drv mrk="duseldorf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="german1.G0ujo">German</ref>a
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      &ccirc;e <ref cel="rejn.R0o">Rejno</ref>, norde de Kolonjo (6&deg;46&#8217;E, 51&deg;14&#8217;N):
      <ekz>
        kvankam lia familio lo&gcirc;is tiam apud Dresdeno, sub rusa okupado, Norberto restis
        en <tld/>o
        <fnt>
          <vrk><url ref="https://mondmilito.hypotheses.org/1457"
              >Intense kaj inverse, el Germanio Francien, Militrakonto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la 25-a Kongreso de Sennacieca Asocio Tutmonda okazis en <tld/>o dum a&ubreve;gusto 1952
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34329"
              >25-a SAT-Kongreso 1952</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_u;&c_s;&c_e;&c_l;&c_mol;&c_d;&c_o;&c_r;&c_f;</trd>
    <trd lng="de">D&uuml;sseldorf</trd>
  </snc>
</drv>

