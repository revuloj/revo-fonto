<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: gertrud.xml,v 1.5 2023/08/31 14:29:52 afido Exp $">
    <kap>
      <rad>Gertrud</rad>/o
    </kap>

<drv mrk="gertrud.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>,
      &gcirc;ermandevena, i.a. de la patrino de <ref cel="hamlet.0o">Hamleto</ref>:
      <ekz>
        nun forlasu nin, <tld/>o kara
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o levis al la &ccirc;ielo siajn brakojn
        <fnt>
          <aut>Henri Vallienne</aut>
          <vrk><url ref="&Tekstaro;?nomo=kastelo-de-prelongo">Kastelo de Prelongo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kio okazas, mia bona <tld/>o, demandis amike Maziero, vidante la mal&gcirc;ojegan
        mienon de tiu virino
        <fnt>
          <aut>Henri Vallienne</aut>
          <vrk><url ref="&Tekstaro;?nomo=chu-li">&Ccirc;u li?</url></vrk>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_je;&c_r;&c_t;&c_r;&c_u;&c_d;&c_a;</trd>
    <trd lng="da">Gertrud</trd>
    <trd lng="de">Gertrud</trd>
    <trd lng="fi">Gertrud</trd>
    <trd lng="sv">Gertrud</trd>
  </snc>
<trd lng="en">Gertrude</trd>
  <trd lng="ja">&#x30B2;&#x30EB;&#x30C8;&#x30EB;&#x30FC;&#x30C8;</trd>
</drv>

  </art>

