<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>alegori</rad>/o
</kap>
<drv mrk="alegor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Literatura a&ubreve; plastika prezento, per kiu oni homigas abstraktajn
      ideojn:
      <ekz>
        la Justeco kun siaj pesiloj estas <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Allegorie</trd>
  <trd lng="en">allegory</trd>
  <trd lng="fr">all&eacute;gorie</trd>
  <trd lng="hu">alleg&oacute;ria</trd>
  <trd lng="nl">allegorie</trd>
  <trd lng="pt">alegoria</trd>
  <trd
  lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="alegor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trd lng="hu">allegorikus</trd>
    <trd lng="nl">allegorisch</trd>
    <trd lng="pt">aleg&oacute;rico</trd>
  </snc>
</drv>
</art>
