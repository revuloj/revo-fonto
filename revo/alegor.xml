<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alegor.xml,v 1.26 2021/02/28 15:34:26 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>alegori</rad>/o
</kap>
<drv mrk="alegor.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BELE</uzo>
   <snc>
     <dif>
       Literatura a&ubreve; plastika prezento, per kiu oni homigas
       abstraktajn ideojn:
       <ekz>
         la Justeco kun siaj pesiloj estas <tld/>o.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_g;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_l;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">al&middot;legoria</trd>
  <trdgrp lng="cs">
    <trd>alegorie</trd>,
    <trd>jinotaj</trd>,
    <trd>podobenstv&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Gleichnis</trd>,
      <trd>Allegorie</trd>
   </trdgrp>
  <trd lng="en">allegory</trd>
  <trd lng="es">alegor&iacute;a</trd>
  <trd lng="fr">all&eacute;gorie</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DC;&#x5B5;&#x5D2;&#x5D5;&#x5B9;&#x5E8;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">alleg&oacute;ria</trd>
  <trd lng="it">allegoria</trd>
  <trd lng="nl">allegorie</trd>
  <trd lng="pt">alegoria</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>aleg&oacute;ria</trd>,
    <trd>aleg&#xF3;ria</trd>,
    <trd>inotaj</trd>
  </trdgrp>
</drv>

<drv mrk="alegor.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>o.
     </dif>
     <trd lng="be">&c_a;&c_l;&c_je;&c_g;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
     <trd lng="ca">al&middot;leg&ograve;ric</trd>
     <trdgrp lng="de">
      <trd>gleichnishaft</trd>,
      <trd>allegorisch</trd>
     </trdgrp>
     <trd lng="es">aleg&oacute;rico</trd>
     <trd lng="fr">all&eacute;gorique</trd>
     <trd lng="hu">allegorikus</trd>
     <trd lng="it">allegorico</trd>
     <trd lng="nl">allegorisch</trd>
     <trd lng="pt">aleg&oacute;rico</trd>
     <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
     <trd lng="sk">alegorick&yacute;</trd>
   </snc>
  <trd lng="cs">alegorick&#xFD;</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DC;&#x5B5;&#x5D2;&#x5D5;&#x5B9;&#x5E8;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
</drv>
</art>
