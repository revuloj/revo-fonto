<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
 <kap>
  <ofc>8</ofc>
  <rad>ital</rad>/o
  <fnt><bib>PIV1</bib></fnt>
 </kap>
 
 <drv mrk="ital.0o">
  <kap><tld/>o<fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
  <snc>
    <dif>
      <ref tip="lst" cel="sxtat.0ano"
      lst="voko:popolanoj">&Scirc;tatano</ref>, &scirc;tatanino de <ref tip="malprt"
      cel="ital.I0ujo"><tld lit="I"/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_i;&c_t;&c_a;&c_l;&c_i;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Italiener</trd>
  <trd lng="en">Italian</trd>
  <trd lng="es">Italiano, Italiana</trd>
   <trdgrp lng="fr">
     <trd>Italien</trd>,
     <trd>Italienne<klr> (subst.)</klr></trd>
   </trdgrp>
  <trd lng="fy">Italjaan</trd>
  <trd lng="hu">olasz</trd>
  <trdgrp lng="io">
    <trd>Italiano</trd>,
    <trd>Italo</trd>
  </trdgrp>
  <trd lng="nl">Italiaan</trd>
  <trd lng="no">italiener</trd>
  <trd lng="pl">W&lstroke;och</trd>
  <trd lng="pt">italiano<klr tip="amb"> (pessoa; subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="tp">jan Italija</trd>
  <trd lng="vo">Litaliy&auml;nan</trd>
 </drv>

 <drv mrk="ital.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>aj dialektoj, a&ubreve;toroj;
      </ekz>
      <ekz>
        <tld/>a lingvo, konstitucio.
      </ekz>
    </dif>
   <trd lng="en">Italian<klr> (adj.)</klr></trd>
   <trdgrp lng="fr">
     <trd>italien</trd>,
     <trd>italienne<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc>
    <dif>
      "la itala" = la nacia <ref tip="lst" cel="lingv.0o.homa"
        lst="voko:lingvoj">lingvo</ref> de Italujo
      <ekz><fnt><bib>Viki</bib>, <lok><url 
      ref="http://eo.wikipedia.org/wiki/Itala lingvo">Itala
      lingvo</url></lok></fnt></ekz>
    </dif>
   <trd lng="en">Italian<klr> (language)</klr></trd>
   <trd lng="fr">italien<klr> (langue)</klr></trd>
  </snc>  <trd
  lng="be">&c_ib;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_i;&c_t;&c_a;&c_l;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">italienisch</trd>
  <trd lng="fy">Italjaansk</trd>
  <trd lng="hu">olasz</trd>
  <trdgrp lng="io">
    <trd>Italiana</trd>,
    <trd>Itala</trd>
  </trdgrp>
  <trd lng="nl">Italiaans</trd>
  <trd lng="no">italiensk</trd>
  <trd lng="pl">w&lstroke;oski<klr tip="amb"> (od W&lstroke;och)</klr></trd>
  <trd lng="pt">italiano<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="ru">&c_i;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="vo">Litaliy&auml;nik</trd>
 </drv>

 <drv mrk="ital.I0ujo">
  <kap><tld lit="I"/>ujo
  (<var><kap><tld lit="I"/>/i/o</kap></var>)
    <fnt><bib>ZR</bib></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
     ref="http://eo.wikipedia.org/wiki/Italio">Italio</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      situanta en
      suda <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta norde al Alpoj. &Gcirc;ia &ccirc;efurbo estas Romo.
    </dif>
  </snc>
  <rim mrk="ital.I0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan
  diferencon en la uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
  <trd lng="be">&c_Ib;&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="ca">It&agrave;lia</trd>
  <trd lng="cs">It&aacute;lie</trd>
  <trd lng="da">Italien</trd>
  <trd lng="de">Italien</trd>
  <trd lng="en">Italy</trd>
  <trdgrp lng="fr">
    <trd>Italie</trd>,
    <trd>R&eacute;publique italienne</trd>
  </trdgrp>
  <trd lng="fy">Itaalje</trd>
  <trdgrp lng="hu">
    <trd>Olaszorsz&aacute;g</trd>,
    <trd>It&aacute;lia</trd>
  </trdgrp>
  <trd lng="id">Italia</trd>
  <trd lng="io">Italia</trd>
  <trd lng="lat">Italia</trd>
  <trd lng="nl">Itali&euml;</trd>
  <trd lng="no">Italia</trd>
  <trdgrp lng="pl">
    <trd>W&lstroke;ochy</trd>,
    <trd>Italia</trd>
  </trdgrp>
  <trd lng="pt">It&aacute;lia</trd>
  <trd lng="ru">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Taliansko</trd>
  <trd lng="tp">ma Italija</trd>
  <trd lng="vo">Litaliy&auml;n</trd>
 </drv>

</art>

