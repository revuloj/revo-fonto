<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ital.xml,v 1.34 2026/02/03 02:58:23 revo Exp $">
 <kap>
  <ofc>8</ofc>
  <rad>ital</rad>/o <fnt><bib>JED</bib></fnt>
 </kap>

 <drv mrk="ital.0o">
  <kap><tld/>o</kap>
  <snc mrk="ital.0o.sxtatano">
    <dif>
      <ref tip="lst" cel="sxtat.0ano" lst="voko:popolanoj">&Scirc;tatano</ref> de <ref tip="malprt"
      cel="ital.I0ujo"><tld lit="I"/>ujo</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> vendis marmorajn statuojn &ccirc;e la proksima stando
        <fnt>
          <aut>C. Rossetti</aut>
          <vrk>Kredu min, sinjorino!</vrk>
          <lok>1949</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_i;&c_t;&c_a;&c_l;&c_i;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd
  lng="bo">&#xF68;&#xF72;&#xF0B;&#xF4F;&#xF0B;&#xF63;&#xF72;&#xF60;&#xF72;&#xF0B;&#xF58;&#xF72;&#xF0B;</trd>
  <trd lng="de">Italiener</trd>
  <trd lng="en">Italian</trd>
  <trd lng="es">Italiano, Italiana</trd>
   <trdgrp lng="fr">
     <trd>Italien</trd>,
     <trd>Italienne<klr> (subst.)</klr></trd>
   </trdgrp>
  <trd lng="fy">Italjaan</trd>
  <trd lng="hu">olasz</trd>
  <trdgrp lng="io">
    <trd>Italiano</trd>,
    <trd>Italo</trd>
  </trdgrp>
  <trd lng="ja">&#x30A4;&#x30BF;&#x30EA;&#x30A2;&#x4EBA;
  <pr>&#x30A4;&#x30BF;&#x30EA;&#x30A2;&#12376;&#12435;</pr></trd>
  <trd lng="nl">Italiaan</trd>
  <trd lng="no">italiener</trd>
  <trdgrp lng="pl">
    <trd>W&lstroke;och</trd>,
    <trd>W&lstroke;oszka</trd>
  </trdgrp>
  <trd lng="pt">italiano<klr tip="amb"> (pessoa; subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="tok">jan pi ma Italija</trd>
  <trd lng="uk">&c_ib;&c_t;&c_a;&#x43B;&c_ib;&c_jeu;&c_c;&#x44C;</trd>
  <trd lng="vo">Litaliy&auml;nan</trd>
   <trdgrp lng="zh">
    <trd>&#x610F;&#22823;&#21033;&#x4EBA; <pr>Y&#xEC;d&#xE0;l&#xEC;r&#xE9;n</pr></trd>,
    <trd>&#x610F;&#22823;&#21033;&#x4EBA; <pr>y&#xEC;d&#xE0;l&#xEC;r&#xE9;n</pr></trd>
  </trdgrp>
</drv>

 <drv mrk="ital.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>aj dialektoj, a&ubreve;toroj;
      </ekz>
      <ekz>
        <tld/>a lingvo, konstitucio.
      </ekz>
    </dif>
   <trd lng="en">Italian<klr> (adj.)</klr></trd>
   <trdgrp lng="fr">
     <trd>italien</trd>,
     <trd>italienne<klr> (adj.)</klr></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>w&lstroke;oski<klr tip="amb"> (od W&lstroke;och)</klr></trd>,
     <trd>w&lstroke;oska<klr tip="amb"> (od W&lstroke;och)</klr></trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      La <tld/>a lingvo.
    </dif>
   <trd lng="en">Italian<klr> (language)</klr></trd>
   <trd lng="fr">italien<klr> (langue)</klr></trd>
   <trd lng="pl">j&eogonek;zyk <ind>w&lstroke;oski</ind></trd>
   <trd lng="tok">toki Italija</trd>
   <trd lng="vo">Litaliy&auml;nap&uuml;k</trd>
  </snc>
  <trd lng="be">&c_ib;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_i;&c_t;&c_a;&c_l;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd
  lng="bo">&#xF68;&#xF72;&#xF0B;&#xF4F;&#xF0B;&#xF63;&#xF72;&#xF60;&#xF72;&#xF0B;&#xF66;&#xF90;&#xF51;&#xF0B;</trd>
  <trd lng="de">italienisch</trd>
  <trd lng="fy">Italjaansk</trd>
  <trd lng="hu">olasz</trd>
  <trdgrp lng="io">
    <trd>Italiana</trd>,
    <trd>Itala</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30BF;&#x30EA;&#x30A2;&#x306E;  <pr>&#12356;&#x305F;&#x308A;&#12354;&#x306E;</pr></trd>,
    <trd>&#x30A4;&#x30BF;&#x30EA;&#x30A2;&#x8A9E;&#x306E;
    <pr>&#12356;&#x305F;&#x308A;&#12354;&#12372;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">Italiaans</trd>
  <trd lng="no">italiensk</trd>
  <trd lng="pt">italiano<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="ru">&c_i;&c_t;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="tok">pi ma Italija</trd>
  <trd lng="uk">&c_ib;&c_t;&c_a;&#x43B;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
  <trd lng="vo">Litaliy&auml;nik</trd>
  <trd lng="zh">&#x610F;&#22823;&#21033;&#x8A9E; <pr>y&#xEC;d&#xE0;l&#xEC;y&#x1D4;</pr></trd>
</drv>

 <drv mrk="ital.I0ujo">
  <kap><tld lit="I"/>ujo
    <fnt><bib>ZR</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">IT</mlg>
  <snc>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      situanta en suda <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta norde al <ref tip="prt" cel="alp.A0oj">Alpoj</ref>.
      &Gcirc;ia <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      estas <ref tip="prt" cel="rom.0o">Romo</ref>.
    </dif>
    <ref tip="sin" cel="itali.I0o">Italio</ref>
  </snc>
  <trd lng="be">&c_Ib;&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="ca">It&agrave;lia</trd>
  <trd lng="cs">It&aacute;lie</trd>
  <trd lng="da">Italien</trd>
  <trd lng="de">Italien</trd>
  <trd lng="en">Italy</trd>
  <trdgrp lng="fr">
    <trd>Italie</trd>,
    <trd>R&eacute;publique italienne</trd>
  </trdgrp>
  <trd lng="fy">Itaalje</trd>
  <trdgrp lng="hu">
    <trd>Olaszorsz&aacute;g</trd>,
    <trd>It&aacute;lia</trd>
  </trdgrp>
  <trd lng="id">Italia</trd>
  <trd lng="io">Italia</trd>
  <trd lng="lat">Italia</trd>
  <trd lng="nl">Itali&euml;</trd>
  <trd lng="no">Italia</trd>
  <trdgrp lng="pl">
    <trd>W&lstroke;ochy</trd>,
    <trd>Italia</trd>
  </trdgrp>
  <trd lng="pt">It&aacute;lia</trd>
  <trd lng="ru">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Taliansko</trd>
  <trd lng="tok">ma Italija</trd>
  <trd lng="vo">Litaliy&auml;n</trd>
   <trdgrp lng="zh">
    <trd>&#x4E49;&#22823;&#21033; <pr>Y&#xEC;d&#xE0;l&#xEC;</pr></trd>,
    <trd>&#x610F;&#22823;&#21033; <pr>Y&#xEC;d&#xE0;l&#xEC;</pr></trd>,
    <trd>&#x4E49;&#22823;&#21033; <pr>y&#xEC;d&#xE0;l&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

