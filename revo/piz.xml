<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>piz</rad>/o
</kap>
<drv mrk="piz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piz.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Pisum</trd>), legomo kun globformaj verdaj semgrajnoj.
    </dif>
  </snc>
  <snc mrk="piz.0o.grajno">
    <dif>
      La grajno mem.
    </dif>
  </snc>
  <trd lng="de">Erbse</trd>
  <trd lng="fr">petit pois</trd>
  <trd lng="en">pea, green pea</trd>
</drv>
</art>
