<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>piz</rad>/o
</kap>
<drv mrk="piz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piz.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Pisum</trd>), legomo kun globformaj verdaj semgrajnoj.
    </dif>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_h;</trd>
  </snc>
  <snc mrk="piz.0o.grajno">
    <dif>
      La grajno mem.
    </dif>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trd lng="de">Erbse</trd>
  <trd lng="fr">petit <ind>pois</ind></trd>
  <trd lng="en">green <ind>pea</ind></trd>
  <trd lng="nl">erwt</trd>
</drv>
</art>
