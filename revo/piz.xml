<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: piz.xml,v 1.32 2024/02/17 06:43:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>piz</rad>/o
</kap>
<drv mrk="piz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piz.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
       <ref tip="lst" cel="legom.0o.KUI" lst="voko:faboparencoj">Legomplanto</ref> 
       kun globformaj verdaj semgrajnoj;
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
       (<trd lng="la">Pisum</trd>):
      <ekz>
        volvotrunkoj de <tld/>oj pendis malsupren
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> rigardis sian malgrandan <tld/>&gcirc;ardenon, kiu konsistis el la unu
        sola <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis porkon en viaj <tld/>oj kaj forigis &gcirc;in de tie
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> semis lupenon, <tld/>on, fazeolon kaj hordeon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/04/88_Pisum_sativum_L.jpg/360px-88_Pisum_sativum_L.jpg" prm="PD">botanika tabulo de <tld/>o kultivata (Pisum sativum L.)
        <fnt>
          <aut>Am&eacute;d&eacute;e Masclef, Atlas des plantes de France. 1891</aut>
          <lok><url ref="&WCW;/File:88_Pisum_sativum_L.jpg">&WCW;/File:88_Pisum_sativum_L.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_g;&c_a;&c_r;&c_o;&c_h;</trd>
    <trd lng="ca">pesolera</trd>
    <trdgrp lng="es">
      <trd>pisum sativum</trd>,
      <trd>arvejo</trd>,
      <trd>ch&iacute;charo</trd>
    </trdgrp>
    <trd lng="hi">&#x92E;&#x91F;&#x930; [<ind>ma&#x1E6D;ar</ind>]</trd>
    <trd lng="hu">bors&oacute;</trd>
    <trdgrp lng="id">
      <trd>ercis</trd>,
      <trd>polong</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_h;</trd>
  </snc>
  <snc mrk="piz.0o.grajno">
    <dif>
      <tld lit="P"/>osemo, la <ref tip="super" cel="grajn.0o.BOT">grajno</ref> mem de <tld/>o:
      <ekz>
        fruktoj &scirc;eletaj: faboj, <tld/>oj, lentoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tra la dudek matracoj <klr>[&#x2026;]</klr> &scirc;i sentis la <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li metis unu <tld/>on en la pafilon kaj forpafis &gcirc;in
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sidis en la sunlumo por sen&scirc;eligi <tld/>ojn
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>, 
        <lok>FEL, 1999</lok></fnt>;
      </ekz>
      <ekz>
        dueta&gcirc;aj lignodomoj, similaj unu al la alia kiel <tld/>oj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_r;&c_o;&c_sh;&c_y;&c_n;&c_a;</trd>
    <trd lng="ca">p&egrave;sol</trd>
    <trdgrp lng="es">
      <trd>guisante</trd>,
      <trd>ch&iacute;charo</trd>,
      <trd>arveja</trd>
    </trdgrp>
    <trd lng="hu">bors&oacute;szem</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_n;&c_a;</trd>
  </snc>
  <ref tip="hom" cel="piz1.0o">Pizo (urbo)</ref>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_h;</trd>
  <trd lng="cs">hr&aacute;ch</trd>
  <trd lng="de">Erbse</trd>
  <trd lng="en">green <ind>pea</ind></trd>
  <trd lng="fr">petit <ind>pois</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30F3;&#x30C9;&#x30A6;</trd>,
    <trd>&#x3048;&#x3093;&#x3069;&#x3046;&#x8C46;  <pr>&#x3048;&#x3093;&#x3069;&#x3046;&#x307E;&#x3081;</pr></trd>
  </trdgrp>
  <trd lng="nl">erwt</trd>
  <trd lng="pl">groch</trd>
  <trd lng="pt">ervilha</trd>
  <trd lng="sk">hrach <klr>(rastlina)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43E;&#x440;&#x43E;&#x445;</trd>,
    <trd>&#x433;&#x43E;&#x440;&#x43E;&#x448;&#x43E;&#x43A; (&#x440;&#x43E;&#x441;&#x43B;&#x438;&#x43D;&#x430;)</trd>,
    <trd>&#x433;&#x43E;&#x440;&#x43E;&#x445;</trd>,
    <trd>&#x433;&#x43E;&#x440;&#x43E;&#x448;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x433;&#x43E;&#x440;&#x43E;&#x448;&#x43E;&#x43A; (&#x43D;&#x430;&#x441;&#x456;&#x43D;&#x43D;&#x44F;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9752;&#x8C46; <pr>q&#x12B;ngd&#xF2;u</pr></trd>,
    <trd>&#x8C4C;&#x8C46; <pr>w&#x101;nd&#xF2;u</pr></trd>
  </trdgrp>
</drv>
</art>
