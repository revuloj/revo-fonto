<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>

<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fibr</rad>/o
</kap>
<drv mrk="fibr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Pli-malpli filamentforma natura produkto <klr>(el katuno, lano
      ktp)</klr>:
      <ekz>
        lignaj <tld/>oj;
      </ekz> 
      <ekz>
        <tld/>oj de silka fadeno<klr>(f)</klr>;
      </ekz>
      <ekz>
        kontra&ubreve; tiu mensogo mi protestas per &ccirc;iuj <tld/>oj de
        mia koro<fnt>Z</fnt>.
      </ekz>  
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Filamentforma artefarita produkto 
      <ekz>
        nilonaj <tld/>oj;
      </ekz> 
      <ekz>
        vitraj <tld/>oj;
      </ekz> 
    </dif>
  </snc>
  <snc num="3">
    <dif>
      Analoga elemento de kelkaj mineralaj korpoj
    </dif>
  </snc>
  <trd lng="de">Faser</trd>
  <trd lng="en">fibre</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_o;</trd>,
    <trd>&c_f;&c_i;&c_b;&c_r;&c_a;</trd>,
    <trd>&c_zh;&c_i;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="fibr.0eto">
  <kap><tld/>eto</kap>
  <snc num="1">
    <dif>
      Harradiko
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Malgranda fibro la&ubreve;longe de muskolo kiu povas kontrahi
    </dif>
    <trd lng="ru">&c_f;&c_i;&c_b;&c_r;&c_i;&c_l;&c_l;&c_a;</trd>
  </snc>
  <trd lng="de">Fibrille</trd>
  <trd lng="en">fibril</trd>
</drv>
<drv mrk="fibr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i tia ke la fibroj aparti&gcirc;as el la tuto
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zerfasern</trd>,
    <trd>ausfransen</trd>
  </trdgrp>
  <trd lng="en">fray</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; &c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_a;</trd>
</drv>
</art>
