<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: revers.xml,v 1.11 2025/02/04 08:31:24 revo Exp $">
<kap>
    <rad>revers</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="revers.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="revers.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="dors.0o.malantauxa">Dorso<sncref/></ref>,
      <ke><ref tip="dif" cel="flank.dors0o.KOMUNE">dorsflanko</ref></ke>
      de plata objekto, kontraste al
      <ref tip="ant" cel="avers.0o.KOMUNE">averso</ref>:
      <ekz>
        en la flago de Paragvajo la frontflanko kaj <tld/>flanko estas diversaj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        speso <klr>[&#8230;]</klr> havis sur la averso la profilon
        de <nom>Zamenhof</nom> kaj sur la <tld/>o blazonon
        <fnt>
          <aut>-</aut>
          <vrk><url ref="http://www.esperantio.net/index.php?id=1080">
          De medalo al monero</url></vrk>
          <lok>HeKo, 2009-12-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;tkartoj kun nigrablankaj reprodukta&jcirc;oj de bildoj averse <klr>[&#8230;sed la]</klr>
        <tld/>oj netu&scirc;itaj de skribiloj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 6 (56)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="rovers.0o">roverso (refaldo)</ref>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_b;&c_o;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_k;&c_ib;&c_r;&c_u;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_e;&c_v;&c_e;&c_r;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mince</trd>,
    <trd>revers</trd>,
    <trd>rub l&aacute;tky</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>R&uuml;ckseite</trd>,
    <trd>Revers <klr>(M&uuml;nze)</klr></trd>
  </trdgrp>
  <trd lng="es">reverso</trd>
  <trdgrp lng="fr">
    <trd>face<klr> (c&ocirc;t&eacute; face)</klr></trd>,
    <trd>revers<klr> (c&ocirc;t&eacute; face)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35023;  <pr>&#12426;</pr></trd>,
    <trd>&#35023;&#38754;  <pr>&#12358;&#12425;&#12417;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">rewers</trd>
  <trdgrp lng="ro">
    <trd>spate</trd>,
    <trd>verso</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mince</trd>,
    <trd>reverz</trd>,
    <trd>rub l&aacute;tky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_j; &c_b;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_o;&c_r;&c_ib;&c_t;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_je;&c_r;&c_s;</trd>
  </trdgrp>
</drv>
</art>

