<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>okr</rad>/o
  <fnt>Z</fnt>
  <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="okr.0o">
  <kap><tld/>o</kap>
  <snc mrk="okr.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Argileca mineralo konsistanta grandparte el ferika oksido a&ubreve; ferika
      hidroksido kaj uzata kiel farbo.
    </dif>
    <refgrp tip="vid">
      <ref cel="hemati.0o">hematito</ref>,
      <ref cel="limonit.0o">limonito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_o;&c_h;&c_r;&c_a;</trd>
  <trd lng="de">Ocker</trd>
  <trd lng="el">&omega_ton;&chi;&rho;&alfa;</trd>
  <trd lng="fr">ocre<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>okker</trd>,
    <trd>s&aacute;rgaf&ouml;ld</trd>
  </trdgrp>
  <trd lng="nl">oker</trd>
  <trd lng="pt">ocre</trd>
  <trd lng="ru">&c_o;&c_h;&c_r;&c_a;</trd>
</drv>
</art>
