<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: okr.xml,v 1.25 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>okr</rad>/o
  <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="okr.0o">
  <kap><tld/>o</kap>
  <snc mrk="okr.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Argileca <ref tip="super" cel="minera.0o">mineralo</ref>, 
      konsistanta grandparte el ferika oksido a&ubreve; ferika
      hidroksido kaj uzata kiel <ref tip="super" cel="farb.0o.KOL">farbo</ref>:
      <ekz>
        la komenci&gcirc;anta produktado de artefaritaj feroksidoj kaj kemiaj kolorigiloj
        negative efikis la industrion pri natura <tld/>o en Roussillon
        <fnt><aut>Robert Roze</aut>,
        <vrk><url ref="&Monato;/2000/006945.php">Flagrantaj koloroj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        produktado de kupro kaj kolkotaro (ru&gcirc;a okro)
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2007/009114.php">Vizito al Takaha&scirc;i</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hemati.0o">hematito</ref>,
      <ref cel="limonit.0o">limonito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_o;&c_h;&c_r;&c_a;</trd>
  <trd lng="bg">&c_o;&c_h;&c_r;&c_a;</trd>
  <trd lng="cs">okr</trd>
  <trd lng="de">Ocker</trd>
  <trd lng="el">&omega_ton;&chi;&rho;&alfa;</trd>
  <trd lng="en">ochre</trd>
  <trd lng="fr">ocre<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>okker</trd>,
    <trd>s&aacute;rgaf&ouml;ld</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9EC4;&#x571F;  <pr>&#x304A;&#x3046;&#x3069;</pr></trd>,
    <trd>&#x30AA;&#x30FC;&#x30AB;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">oker</trd>
  <trd lng="pl">ochra</trd>
  <trd lng="pt">ocre</trd>
  <trd lng="ru">&c_o;&c_h;&c_r;&c_a;</trd>
  <trd lng="sk">oker</trd>
  <trd lng="uk">&#x43E;&c_h;&c_r;&c_a; (&c_f;&c_a;&c_r;&c_b;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x8D6D; <pr>zh&#x11B;</pr></trd>,
    <trd>&#x8D6D;&#x77F3; <pr>zh&#x11B;sh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
