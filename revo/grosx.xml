<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gro&scirc;</rad>/o
</kap>
<drv mrk="grosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tre malgranda monero en diversaj landoj:
      <ekz>nuda kaj kruda, sen <tld/>o en po&scirc;o  
        <klr>(tute mizera)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sold.0o">soldo</ref>,
      <ref cel="cend.0o">cendo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Groschen</trd>
  <trdgrp lng="fr">
    <trd>groschen</trd>,
    <trd>sou<klr tip="ind"> (monnaie de petite valeur)</klr></trd></trdgrp> 
  <trd lng="nl">groschen</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_sh;</trd>
</drv>
</art>

