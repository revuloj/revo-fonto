<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grosx.xml,v 1.22 2023/11/11 11:36:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gro&scirc;</rad>/o
</kap>
<drv mrk="grosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="grosx.0o.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Tre malgranda monero en diversaj landoj:
      <ekz>nuda kaj kruda, sen <tld/>o en po&scirc;o
        <klr>(tute mizera)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <rim>
    Pasintece gro&scirc;o en kelkaj landoj (ekz. en Litvo) havis
    <em>grandan</em> valoron. &Ccirc;u plivastigi la difinon?
    <aut>Dmitri Gabinski</aut>
    </rim>
    <refgrp tip="vid">
      <ref cel="sold.0o">soldo</ref>,
      <ref cel="cend.0o">cendo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_sh;</trd>
  <trd lng="ca">groschen</trd>
  <trd lng="de">Groschen</trd>
  <trdgrp lng="en">
    <trd>groschen<klr tip="ind"> (low value coin)</klr></trd>,
    <trd>grosz<klr tip="ind"> (low value coin)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>groschen</trd>,
    <trd>sou<klr tip="ind"> (monnaie de petite valeur)</klr></trd>
  </trdgrp>
  <trd lng="hu">garas</trd>
  <trdgrp lng="ja">
    <trd>&#x30B0;&#x30ED;&#x30C3;&#x30B7;&#x30A7;&#x30F3;</trd>,
    <trd>&#x306F;&#x3057;&#x305F;&#x91D1;  <pr>&#x306F;&#x3057;&#x305F;&#x304C;&#x306D;</pr></trd>
  </trdgrp>
  <trd lng="nl">groschen</trd>
  <trd lng="pl">grosz</trd>
  <trdgrp lng="pt">
   <trd>ceitil</trd>,
   <trd>vint&eacute;m</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_sh;</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x440;&#x456;&#x448;&#xA0;(&#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&#x430; &#x43E;&#x434;&#x438;&#x43D;&#x438;&#x446;&#x44F;)</trd>,
    <trd>&#x43A;&#x43E;&#x43F;&#x456;&#x439;&#x43A;&#x430;</trd>,
    <trd>&#x448;&#x430;&#x433;</trd>
  </trdgrp>
  <trd lng="zh">&#x5206;&#x5E01; <pr>f&#x113;nb&#xEC;</pr></trd>
</drv>
</art>

