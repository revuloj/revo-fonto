<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frost.xml,v 1.43 2023/12/25 09:26:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frost</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="frost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frost.0o.MET">
  <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="varm.mal0o.FIZ">Malvarmo</ref>, kiu glaciigas akvon:
      <ekz>
        dum la <tld/>o ili havas nenion, por sin kovri
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 24:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in terure turmentis la <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dika &scirc;afa pelto, malsekigita <klr>[&#8230;]</klr> rigidi&gcirc;is de <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        vintro venis frue, estis akra <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kial mi sentas tian <tld/>on en miaj ostoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>?
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la jura konsilisto eksentis <tld/>on en la dorso
        <klr>(ektimis)</klr>
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        urtikon <tld/>o ne difektas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glaci.0o">glacio</ref>,
      <ref cel="prujn.0o">prujno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;</trd>
  <trd lng="cs">mr&aacute;z <klr>(liter.)</klr></trd>
  <trdgrp lng="de">
    <trd>Frost</trd>,
    <trd>K&auml;lte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>frost</trd>,
    <trd>freeze<klr tip="ind"> (noun)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>helada<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>,
    <trd>fr&iacute;o glacial</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gel<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>,
    <trd>froid glacial</trd>
  </trdgrp>
  <trd lng="hu">fagy</trd>
  <trdgrp lng="ja">
    <trd>&#x53B3;&#x5BD2;  <pr>&#12370;&#12435;&#x304B;&#12435;</pr></trd>,
    <trd>&#x6C37;&#x70B9;&#x4E0B;&#x306E;&#x5BD2;&#x6C17; 
    <pr>&#12402;&#12423;&#12358;&#12390;&#12435;&#x304B;&#x306E;&#x304B;&#12435;&#x304D;</pr></trd>,
    <trd>&#12402;&#12393;&#12356;&#x5BD2;&#12369;  <pr>&#12402;&#12393;&#12356;&#12373;&#12416;&#12369;</pr></trd>
  </trdgrp>
  <trd lng="nl">vorst<klr tip="ind"> (koude)</klr></trd>
  <trd lng="pl">mr&oacute;z</trd>
  <trdgrp lng="pt">
    <trd>geada</trd>,
    <trd>frio</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;</trd>
  <trd lng="sk">mr&aacute;z</trd>

<trdgrp lng="th">
  <trd>&#xE04;&#xE27;&#xE32;&#xE21;&#xE2B;&#xE19;&#xE32;&#xE27;&#xE40;&#xE22;&#xE37;&#xE2D;&#xE01;&#xE41;&#xE02;&#xE47;&#xE07;</trd>,
  <trd>&#xE04;&#xE27;&#xE32;&#xE21;&#xE2B;&#xE19;&#xE32;&#xE27;&#xE08;&#xE31;&#xE14;</trd>
</trdgrp>
  <trd lng="uk">&c_t;&c_r;&c_je;&c_m;&c_t;&c_ib;&c_t;&c_i; &c_v;&c_ib;&c_d; &c_m;&c_o;&c_r;&c_o;&c_z;&c_u;</trd>
  <trdgrp lng="zh">
    <trd>&#x971C;&#x51BB; <pr>shu&amacron;ngd&#xF2;ng</pr></trd>,
    <trd>&#x971C; <pr>shu&amacron;ng</pr></trd>,
    <trd>&#x81F4;&#x51B7; <pr>zh&#xEC;l&#x11B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="frost.0a">
  <kap><tld/>a</kap>
  <snc mrk="frost.0a.MET">
  <uzo tip="fak">MET</uzo>
    <dif>
      Glaciige <ref tip="super" cel="varm.mal0a.fizike">malvarma</ref>:
      <ekz>
        hodia&ubreve; estas bela <tld/>a vetero, tial mi prenos miajn glitilojn kaj iros gliti
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la sekvanta tago estis serena <tld/>a vetero, kaj poste komencis degeli
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de la ne&gcirc;okovritaj stratoj en <tld/>a vintra nokto li mortis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a pikanta aero
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj dentoj ekmordis la <tld/>ajn buterpanojn
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia <tld/>a mieno iom degelis
        <fnt>
          <bib>HsT</bib>
          <lok>Kuni&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>mraziv&yacute;</trd>,
    <trd>studen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Frost-</trd>,
    <trd>frostig</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>frosty</trd>, <trd>freezing</trd></trdgrp>
  <trdgrp lng="es">
    <trd>fr&iacute;o</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>glac&eacute;</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trd lng="hu">fagyos</trd>
  <trdgrp lng="ja">
    <trd>&#x53B3;&#x5BD2;&#x306E;  <pr>&#12370;&#12435;&#x304B;&#12435;&#x306E;</pr></trd>,
    <trd>&#x51CD;&#x308A;&#12388;&#x304F;&#12424;&#12358;&#x306A;  <pr>&#12371;&#x304A;&#x308A;&#12388;&#x304F;&#12424;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ijskoud</trd>,
    <trd>ijzig</trd>
  </trdgrp>
  <trd lng="pl">mro&zacute;ny</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">mraziv&yacute;</trd>
<trdgrp lng="th">
  <trd>&#xE2B;&#xE19;&#xE32;&#xE27;&#xE40;&#xE22;&#xE37;&#xE2D;&#xE01;&#xE41;&#xE02;&#xE47;&#xE07;</trd>,
  <trd>&#xE2B;&#xE19;&#xE32;&#xE27;&#xE08;&#xE31;&#xE14;</trd>
</trdgrp>
  <trd lng="uk">&c_m;&c_o;&c_r;&c_o;&c_z;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x51C9;&#x4E1D;&#x4E1D; <pr>li&#xE1;ngs&#x12B;s&#x12B;</pr></trd>,
    <trd>&#x971C;&#x767D; <pr>shu&amacron;ngb&#xE1;i</pr></trd>,
    <trd>&#x5FAE;&#x51C9; <pr>w&emacron;ili&#xE1;ng</pr></trd>,
    <trd>&#x51B7;&#x51B0;&#x51B0; <pr>l&#x11B;ngb&#x12B;ngb&#x12B;ng</pr></trd>,
    <trd>&#x51B7;&#x5CFB;&#22320; <pr>l&#x11B;ngj&#xF9;nde</pr></trd>,
    <trd>&#x51D4; <pr>c&amacron;ng</pr></trd>,
    <trd>&#x51DB; <pr>l&#x1D0;n</pr></trd>,
    <trd>&#x4E0B;&#x971C; <pr>xi&#xE0;shu&amacron;ng</pr></trd>,
    <trd>&#x5BD2;&#x51B7; <pr>h&#xE1;nl&#x11B;ng</pr></trd>,
    <trd>&#x51B7;&#x98BC;&#x98BC; <pr>l&#x11B;ngs&#x14D;us&#x14D;u</pr></trd>,
    <trd>&#x51B7;&#x51BB; <pr>l&#x11B;ngd&#xF2;ng</pr></trd>,
    <trd>&#x901F;&#x51BB; <pr>s&#xF9;d&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="frost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="frost.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Esti <tld/>e malvarme:
      <ekz>
        ekstere estas tre malvarme, ne&gcirc;as kaj <tld/>as
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>!
      </ekz>
      <ekz>
        en la kamparo <klr>[&#8230;]</klr> <tld/>is ankora&ubreve; kelke da gradoj pli ol en la urbo
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frost.0i.senti">
    <dif>
      Senti <tld/>an malvarmon:
      <ekz>
        mi <tld/>as &ccirc;i tie &ccirc;e la fajro, kiel ili devas <tld/>i kaj tremi tie supre
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i frostis, sed ankora&ubreve; sentis la dol&ccirc;on de la panpeco kaj &scirc;i subite revivi&gcirc;is
        el sia apatio
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljunulina&ccirc;o bezonas tion, &scirc;i <tld/>as tiel facile
        <fnt>
          <bib>VRA</bib>
          <lok>Bj&ouml;rn Erik H&ouml;ijer</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_c;&c_mol; <klr tip="ind">(&c_b;&c_je;&c_z;&c_a;&c_s;&c_a;&c_b;.)</klr></trd>
  <trdgrp lng="cs">
    <trd>mrazit</trd>,
    <trd>mrznout</trd>,
    <trd>studit</trd>,
    <trd>trp&ecaron;t zimou</trd>
  </trdgrp>
  <trd lng="de">frieren</trd>
  <trd lng="en">to <ind>frost</ind></trd>
  <trd lng="es">helar</trd>
  <trd lng="fr">geler<klr tip="ind"> (impers.)</klr></trd>
  <trd lng="hu">fagy</trd>
  <trdgrp lng="ja">
    <trd>&#x51CD;&#12360;&#12390;&#12356;&#x308B;  <pr>&#12371;&#12372;&#12360;&#12390;&#12356;&#x308B;</pr></trd>,
    <trd>&#x51CD;&#12387;&#12390;&#12356;&#x308B;  <pr>&#12371;&#x304A;&#12387;&#12390;&#12356;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vriezen</trd>
  <trd lng="pl">mrozi&cacute;</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_b;&c_je;&c_z;&c_l;.)</klr></trd>
  <trd lng="sk">mrzn&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_je;&c_r;&c_z;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_r;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x611B;&#x51CD; <pr>&#xE0;id&#xF2;ng</pr></trd>,
    <trd>&#x7D50;&#x51B0; <pr>ji&#xE9;b&#x12B;ng</pr></trd>,
    <trd>&#x51BB; <pr>d&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="frost.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="frost.0igi.FIZ">
    <dif>
      Fari ion glaciige malvarma, glaciigi:
      <ekz>
        malvarmo <tld/>igis la riveron;
      </ekz>
      <ekz>
        ili fosis penege en la <tld/>igita tero;
      </ekz>
      <ekz>
        Se la naturo rigidi&gcirc;as &#x2AFD; De prem' de l'&#x202F;vintro <tld/>iganta
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igita viando
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igitaj, saligitaj kaj sekigitaj fungoj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2004/008448.html">Fungoj litova eksportvaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;iam<tld/>a grundo de la norda Siberio certe ku&scirc;as multaj miloj da
        <tld/>igitaj mamutoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011414.php">Glaciaj mumioj sor&ccirc;as A&ubreve;strion</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frost.0igi.sentigi">
    <dif>
      Sentigi <tld/>an malvarmon:
      <ekz>
        akra venteto, <klr>[&#8230;]</klr> tran&ccirc;etis lian viza&gcirc;on kaj lin <tld/>igis &gcirc;is ostoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glacio <klr>[&#8230;]</klr> <tld/>igis la sentojn en &scirc;ia brusto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        rigida, oficiala respondo min <tld/>igis kaj fortimigis
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite aperis en mi memoro, kiu <tld/>igis mian koron pro teruro
        <fnt>
          <bib>Kiso</bib>
          <lok>Son&gcirc;o Pri Maljuneco</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frost.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="halt.0igi">Haltigi</ref>,
      <ref cel="parali.0i.FIG">paralizi<sncref/></ref>:
      <ekz>
        se UN decidus <tld/>igi tiun monon, kiu profitus
        <fnt>
          <bib>Monato</bib>
          <aut>Serge Zandandu Ntomono Zola</aut>
          <vrk><url ref="&Monato;/2003/007982.html">Disrabado de naturaj ri&ccirc;a&jcirc;oj</url></vrk>
          <lok>2003</lok>
        </fnt>?
      </ekz>
      <ekz>
        &ccirc;u la dungantoj ne <tld/>igos la salajrojn
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2007/009205.php">Problemo pro salajroj</url></vrk>
          <lok>2007</lok>
        </fnt>?
      </ekz>
      <ekz>
        pastoroj <tld/>igas la rilaton kun adeptoj, kiuj ne plu kapablas doni multe
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2010/009910.php">Ventreligio, dua r aldoni&gcirc;u</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_r;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>zmrazit</trd>,
    <trd>zmrazovat</trd>,
    <trd>z&aacute;bst</trd>
  </trdgrp>
  <trd lng="de">einfrieren</trd>
  <trd lng="en">to <ind>freeze</ind><klr tip="ind">
  (transitive)</klr></trd>
  <trdgrp lng="es">
    <trd>helar</trd>,
    <trd>congelar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>geler<klr tip="ind"> (tr.)</klr></trd>,
    <trd>congeler</trd>,
    <trd>figer</trd>,
    <trd>jeter un <ind>froid</ind> (sur)</trd>,
    <trd>paralyser</trd>,
    <trd>transir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fagyaszt</trd>,
    <trd>befagyaszt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51CD;&#12360;&#12373;&#x305B;&#x308B;  <pr>&#12371;&#12372;&#12360;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x51CD;&#x308A;&#12388;&#x304B;&#x305B;&#x308B; 
    <pr>&#12371;&#x304A;&#x308A;&#12388;&#x304B;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x51B7;&#x51CD;&#12377;&#x308B;  <pr>&#x308C;&#12356;&#12392;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x51CD;&#x7D50;&#12377;&#x308B;  <pr>&#12392;&#12358;&#12369;&#12388;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
  <trdgrp lng="pl">
    <trd>zamrozi&cacute;</trd>,
    <trd>zamra&zdot;a&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">zamrzn&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_b;&c_je;&c_r;&c_ib;&c_g;&c_a;&c_t;&c_i; &c_v; &c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_l;&c_mol;&c_n;&c_ib;&c_j; &c_k;&c_a;&c_m;&c_je;&c_r;&c_ib;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_i;&c_t;&c_i; &c_m;&c_o;&c_r;&c_o;&c_z;&c_o;&c_m; (&c_r;&c_o;&c_s;&c_l;&c_i;&c_n;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51B0; <pr>b&#x12B;ng</pr></trd>,
    <trd>&#x51B7;&#x51BB; <pr>l&#x11B;ngd&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="frost.0igilo">
  <kap><tld/>igilo, <var><kap><tld/>ofako</kap></var></kap>
  <snc mrk="frost.0igilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Tiu parto de <ref tip="vid" cel="frid.0ujo">fridujo</ref>, kiu
      tenas sian temperaturon sub 0&deg;C.
    </dif>
