<drv mrk="frost.cxiam0o">
  <kap>
    &ccirc;iam<tld/>o,
    <var><kap>&ccirc;iama <tld/>o</kap></var>
  </kap>
  <snc mrk="frost.cxiam0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Stato de grundo, kiu restas konstante &ndash; almena&ubreve; dum du jaroj &ndash; frostita;
      <ref tip="sin" cel="permafrost.0o">permafrosto</ref>:
      <ekz>
        la ovo estis preska&ubreve; globa kaj mirinde
        bone konservi&gcirc;inta pro &ccirc;iama <tld/>o
        <fnt>
          <aut>K. Bulychov, trad. Barbulo</aut>
          <vrk><url ref="http://miresperanto.com/por_infanoj/knabino/knabino-03.htm">
          La knabineto, al kiu nenio okazos</url></vrk>
          <lok>[vidita en 2020]</lok>
        </fnt>;
      </ekz>
      <ekz>
        en norda Ruslando la &ccirc;iam<tld/>o atingas grundprofundon de 1.450 metroj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/%C4%88iamfrosta_grundo">&Ccirc;iamfrosta grundo</url></vrk>
          <lok>2019-09-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="negx.eterna0o">eterna ne&gcirc;o</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_je;&c_r;&c_z;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_n;&c_t; &c_v;&c_je;&c_ch;&c_n;&c_a;&c_m;&c_jo;&c_r;&c_z;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="en">permafrost</trd>
  <trd lng="fr">perg&eacute;lisol</trd>
  <trd lng="uk">&c_v;&c_ib;&c_ch;&c_n;&c_a; &c_m;&c_je;&c_r;&c_z;&c_l;&c_o;&c_t;&c_a;</trd>
  <trd lng="zh">&#x6C38;&#x51BB;&#x51B0; <pr>y&#x1D2;ngd&#xF2;ngb&#x12B;ng</pr></trd>
</drv>

