<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frost.xml,v 1.31 2020/04/15 18:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frost</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="frost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frost.0o.MET">
  <uzo tip="fak">MET</uzo>
    <dif>
      Malalta temperaturo, malvarmo, kiu glaciigas akvon:
      <ekz>
        la <tld/>o &ccirc;ion detruis en la &gcirc;ardeno;
      </ekz>
      <ekz>
        tremi pro <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la jura konsilisto eksentis <tld/>on en la dorso
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>
        <klr>(ektimis)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glaci.0o">glacio</ref>,
      <ref cel="prujn.0o">prujno</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;</trd>
  <trd lng="cs">mr&aacute;z <klr>(liter.)</klr></trd>
  <trdgrp lng="de">
    <trd>Frost</trd>,
    <trd>K&auml;lte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>frost</trd>,
    <trd>freeze<klr tip="ind"> (noun)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>helada<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>,
    <trd>fr&iacute;o glacial</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gel<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>,
    <trd>froid glacial</trd>
  </trdgrp>
  <trd lng="hu">fagy</trd>
  <trd lng="nl">vorst<klr tip="ind"> (koude)</klr></trd>
  <trd lng="pl">mr&oacute;z</trd>
  <trdgrp lng="pt">
    <trd>geada</trd>,
    <trd>frio</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;</trd>
  <trd lng="sk">mr&aacute;z</trd>

<trd lng="th">&#3588;&#3623;&#3634;&#3617;&#3627;&#3609;&#3634;&#3623;&#3648;&#3618;&#3639;&#3629;&#3585;&#3649;&#3586;&#3655;&#3591;, &#3588;&#3623;&#3634;&#3617;&#3627;&#3609;&#3634;&#3623;&#3592;&#3633;&#3604;</trd>
</drv>

<drv mrk="frost.0a">
  <kap><tld/>a</kap>
  <snc mrk="frost.0a.MET">
  <uzo tip="fak">MET</uzo>
    <dif>
      Glaciige malvarma:
      <ekz>
        <tld/>a vetero, vento;
      </ekz>
      <ekz>
        la <tld/>aj dezertoj de la poluso;
      </ekz>
      <ekz>
        hodia&ubreve; estas <tld/>e.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>mraziv&yacute;</trd>,
    <trd>studen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Frost-</trd>,
    <trd>frostig</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>frosty</trd>, <trd>freezing</trd></trdgrp>
  <trdgrp lng="es">
    <trd>fr&iacute;o</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>glac&eacute;</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trd lng="hu">fagyos</trd>
  <trdgrp lng="nl">
    <trd>ijskoud</trd>,
    <trd>ijzig</trd>
  </trdgrp>
  <trd lng="pl">mro&zacute;ny</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">mraziv&yacute;</trd>
<trd lng="th">&#3627;&#3609;&#3634;&#3623;&#3648;&#3618;&#3639;&#3629;&#3585;&#3649;&#3586;&#3655;&#3591;, &#3627;&#3609;&#3634;&#3623;&#3592;&#3633;&#3604;</trd>
</drv>

<drv mrk="frost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="frost.0i.MET">
  <uzo tip="fak">MET</uzo>
    <dif>
      Esti <tld/>e:
      <ekz>
        ofte <tld/>as ankora&ubreve; en marto;
      </ekz>
      <ekz>
        <tld/>as al mi en la piedoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_c;&c_mol; <klr tip="ind">(&c_b;&c_je;&c_z;&c_a;&c_s;&c_a;&c_b;.)</klr></trd>
  <trdgrp lng="cs">
    <trd>mrazit</trd>,
    <trd>mrznout</trd>,
    <trd>studit</trd>,
    <trd>trp&ecaron;t zimou</trd>
  </trdgrp>
  <trd lng="de">frieren</trd>
  <trd lng="en">to <ind>frost</ind></trd>
  <trd lng="es">helar</trd>
  <trd lng="fr">geler<klr tip="ind"> (impers.)</klr></trd>
  <trd lng="hu">fagy</trd>
  <trd lng="nl">vriezen</trd>
  <trd lng="pl">mrozi&cacute;</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_b;&c_je;&c_z;&c_l;.)</klr></trd>
  <trd lng="sk">mrzn&uacute;&tcaron;</trd>
</drv>

<drv mrk="frost.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="frost.0igi.FIZ">
    <dif>
      Fari ion glaciige malvarma, glaciigi:
      <ekz>
        vintra vento <tld/>igas nin &gcirc;isoste;
      </ekz>
      <ekz>
        malvarmo <tld/>igis la riveron;
      </ekz>
      <ekz>
        ili fosis penege en la <tld/>igita tero.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frost.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Haltigi,
      <ref tip="vid" cel="parali.0i.FIG">paralizi<sncref/></ref>:
      <ekz>
        tiuj vortoj <tld/>igis la entuziasmon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_r;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>zmrazit</trd>,
    <trd>zmrazovat</trd>,
    <trd>z&aacute;bst</trd>
  </trdgrp>
  <trd lng="de">einfrieren</trd>
  <trd lng="en">to <ind>freeze</ind><klr tip="ind">
  (transitive)</klr></trd>
  <trdgrp lng="es">
    <trd>helar</trd>,
    <trd>congelar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>geler<klr tip="ind"> (tr.)</klr></trd>,
    <trd>congeler</trd>,
    <trd>figer</trd>,
    <trd>jeter un <ind>froid</ind> (sur)</trd>,
    <trd>paralyser</trd>,
    <trd>transir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fagyaszt</trd>,
    <trd>befagyaszt</trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
  <trdgrp lng="pl">
    <trd>zamrozi&cacute;</trd>,
    <trd>zamra&zdot;a&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">zamrzn&uacute;&tcaron;</trd>
</drv>

<drv mrk="frost.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="frost.0igilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Tiu parto de <ref tip="vid" cel="frid.0ujo">fridujo</ref>, kiu
      tenas sian temperaturon sub 0&deg;C.
    </dif>
  <ref tip="vid" cel="frost.0ujo">frostujo</ref>  
  </snc>
  <trdgrp lng="en">
    <trd>freezer</trd>,
    <trd>freezer compartment</trd>
  </trdgrp>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Gefrierschrank</trd>
  <trdgrp lng="en">
    <trd>deep freeze</trd>,
    <trd>deep freezer</trd>
  </trdgrp>
  <trd lng="fr">cong&eacute;lateur</trd>
  <trdgrp lng="hu">
    <trd>fagyaszt&oacute;szekr&eacute;ny</trd>,
    <trd>m&eacute;lyh&udblac;t&odblac;szekr&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">diepvriezer</trd>
  <trd lng="pl">zamra&zdot;arka</trd>
  <trd lng="pt">freezer</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="frost.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="frost.0igxi.FIZ">
    <dif>
      I&gcirc;i malmola, rigida a&ubreve; difektita pro frosto:
      <ekz>
        rivereto, tero, floroj <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        la <tld/>i&gcirc;intaj fingroj ne plu povas teni la plumingon.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frost.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      I&gcirc;i haltigita:
      <ekz>
        ilia gajeco <tld/>i&gcirc;is &ccirc;e tiu terura vido.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_ja;&c_r;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">gefrieren</trd>
  <trd lng="en">to <ind>freeze</ind><klr tip="ind">
  (intransitive)</klr></trd>
  <trdgrp lng="es">
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>,
    <trd><ind>congelar</ind>se</trd>,
    <trd><ind>helar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>,
    <trd>se <ind>congeler</ind></trd>,
    <trd>se <ind>figer</ind></trd>,
    <trd>se <ind>paralyser</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megfagy</trd>,
    <trd>befagy</trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
 <trdgrp lng="pl">
   <trd>marzn&aogonek;&cacute;</trd>,
   <trd>zamarzn&aogonek;&cacute;</trd>
 </trdgrp> 
  <trd lng="ru">&c_z;&c_a;&c_m;&c_jo;&c_r;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>

<drv mrk="frost.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="frost.0ujo.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      &Scirc;ranko en kiu regas konstanta <tld/>a temperaturo, kaj en kiu oni
      povas konservi man&gcirc;a&jcirc;ojn kaj trinka&jcirc;ojn dum pluraj monatoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="glaci.0ujo0sxranko0tenejo">glaci&scirc;ranko</ref>,
      <ref cel="frid.0ujo">fridujo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Gefrierschrank</trd>
  <trdgrp lng="en">
    <trd>deep freeze</trd>,
    <trd>deep freezer</trd>
  </trdgrp>
  <trd lng="fr">cong&eacute;lateur</trd>
  <trdgrp lng="hu">
    <trd>fagyaszt&oacute;szekr&eacute;ny</trd>,
    <trd>m&eacute;lyh&udblac;t&odblac;szekr&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">diepvriezer</trd>
  <trd lng="pl">zamra&zdot;arka</trd>
  <trd lng="pt">freezer</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
<trd lng="th">&#3605;&#3641;&#3657;&#3649;&#3594;&#3656;&#3591;&#3649;&#3586;&#3655;&#3591;</trd>
</drv>

<drv mrk="frost.cxiam0o">
  <kap>
    &ccirc;iam<tld/>o,
    <var><kap>&ccirc;iama <tld/>o</kap></var>
  </kap>
  <snc mrk="frost.cxiam0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Stato de grundo, kiu restas konstante &ndash; almena&ubreve; dum du jaroj &ndash; frostita;
      <ref tip="sin" cel="permafrost.0o">permafrosto</ref>:
      <ekz>
        la ovo estis preska&ubreve; globa kaj mirinde
        bone konservi&gcirc;inta pro &ccirc;iama <tld/>o
        <fnt>
          <aut>K. Bulychov, trad. Barbulo</aut>
          <vrk><url ref="http://miresperanto.com/por_infanoj/knabino/knabino-03.htm">
          La knabineto, al kiu nenio okazos</url></vrk>
          <lok>[vidita en 2020]</lok>
        </fnt>;
      </ekz>
      <ekz>
        en norda Ruslando la &ccirc;iam<tld/>o atingas grundprofundon de 1.450 metroj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/%C4%88iamfrosta_grundo">&Ccirc;iamfrosta grundo</url></vrk>
          <lok>2019-09-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="negx.eterna0o">eterna ne&gcirc;o</ref>,
    </refgrp>
  </snc>
  <trd lng="en">permafrost</trd>
  <trd lng="fr">perg&eacute;lisol</trd>
</drv>

<drv mrk="frost.0ofako">
  <kap><tld/>ofako</kap>
  <snc mrk="frost.0ofako.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      Tiu parto de <ref tip="vid" cel="frid.0ujo">fridujo</ref>, en kiu
      regas <tld/>a temperaturo.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_e;&c_r;&c_a;
  <klr>(&c_h;&c_a;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Gefrierfach</trd>,
    <trd>Tiefk&uuml;hlfach</trd>
  </trdgrp>
    <trdgrp lng="en">
    <trd>freezer</trd>,
    <trd>freezer compartment</trd>
  </trdgrp>
  <trd lng="fr">bac &agrave; glace</trd>
  <trd lng="hu">m&eacute;lyh&udblac;t&odblac;<klr tip="amb">
  (h&udblac;t&odblac;szekr&eacute;nyben)</klr></trd>  
  <trdgrp lng="nl">
    <trd>vriesvak</trd>,
    <trd>diepvriesvak</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komora zamra&zdot;arki</trd>,
    <trd>zamra&zdot;alnik</trd>
  </trdgrp>
  <trd lng="pt">congelador</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_je;&c_r;&c_a;<klr>
  (&c_h;&c_o;&c_l;&c_o;&c_d;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;&c_a;)</klr></trd>
</drv>
</art>
