<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>frost</rad>/o</kap>
<drv mrk="frost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Malalta temperaturo, malvarmo, kiu glaciigas akvon:
      <ekz>
        la <tld/>o &ccirc;ion detruis en la &gcirc;ardeno;
      </ekz>
      <ekz>
        tremi pro <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la jura konsilisto eksentis <tld/>on en la dorso
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Galo&scirc;oj de feli&ccirc;o</vrk>)</fnt>
        <klr>(ektimis)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glaci.0o">glacio</ref>,
      <ref cel="prujn.0o">prujno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Frost</trd>,
    <trd>K&auml;lte</trd>
  </trdgrp>
  <trd lng="en">frost</trd>
  <trdgrp lng="fr">
    <trd>gel<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>,
    <trd>froid glacial</trd>
  </trdgrp>
  <trd lng="nl">vorst<klr tip="ind"> (koude)</klr></trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;</trd>
</drv>

<drv mrk="frost.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Glaciige malvarma:
      <ekz>
        <tld/>a vetero, vento;
      </ekz>
      <ekz>
        la <tld/>aj dezertoj de la poluso;
      </ekz>
      <ekz>
        hodia&ubreve; estas <tld/>e.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Frost-</trd>,
    <trd>frostig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>glac&eacute;</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ijskoud</trd>,
    <trd>ijzig</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="frost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Esti <tld/>e:
      <ekz>
        ofte <tld/>as ankora&ubreve; en marto;
      </ekz>
      <ekz>
        <tld/>as al mi en la piedoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">frieren</trd>
  <trd lng="fr">geler<klr tip="ind"> (impers.)</klr></trd>
  <trd lng="nl">vriezen</trd>
</drv>

<drv mrk="frost.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion glaciige malvarma, glaciigi:
      <ekz>
        vintra vento <tld/>igas nin &gcirc;isoste;
      </ekz>
      <ekz>
        malvarmo <tld/>igis la riveron;
      </ekz>
      <ekz>
        ili fosis penege en la <tld/>igita tero.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Haltigi:
      <ekz>
        tiuj vortoj <tld/>igis la entuziasmon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>geler<klr tip="ind"> (tr.)</klr></trd>,
    <trd>congeler</trd>,
    <trd>figer</trd>,
    <trd>jeter un <ind>froid</ind> (sur)</trd>,
    <trd>paralyser</trd>,
    <trd>p&eacute;trifier<klr tip="ind"> (figer)</klr></trd>,
    <trd>transir</trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="frost.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti malmoligita, rigidigita a&ubreve; difektita de la malvarmo:
      <ekz>
        rivereto, tero, floroj <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        la <tld/>i&gcirc;intaj fingroj ne plu povas teni la plumingon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti haltigita:
      <ekz>
        ilia gajeco <tld/>i&gcirc;is &ccirc;e tiu terura vido.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gefrieren</trd>
  <trdgrp lng="fr">
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>,
    <trd>se <ind>congeler</ind></trd>,
    <trd>se <ind>figer</ind></trd>,
    <trd>se <ind>paralyser</ind></trd>,
    <trd>se <ind>p&eacute;trifier</ind><klr tip="ind"> (se figer)</klr></trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_jo;&c_r;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>

<drv mrk="frost.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      &Scirc;ranko en kiu regas konstanta <tld/>a temperaturo, kaj en kiu oni
      povas konservi man&gcirc;a&jcirc;ojn kaj trinka&jcirc;ojn dum pluraj monatoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="glaci.0ujo0sxranko0tenejo">glaci&scirc;ranko</ref>,
      <ref cel="frid.0ujo">fridujo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Gefrierschrank</trd>
  <trd lng="nl">diepvriezer</trd>
</drv>

<drv mrk="frost.0ofako">
  <kap><tld/>ofako</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Tiu parto de <ref tip="vid" cel="frid.0ujo">fridujo</ref>, en kiu
      regas <tld/>a temperaturo.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>Gefrierfach</trd>,
    <trd>Tiefk&uuml;hlfach</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vriesvak</trd>,
    <trd>diepvriesvak</trd>
  </trdgrp>
</drv>
</art>
