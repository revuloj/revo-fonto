<drv mrk="frost.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="frost.0igxi.FIZ">
    <dif>
      I&gcirc;i malmola, rigida a&ubreve; difektita pro frosto:
      <ekz>
        <tld/>i&gcirc;as mar&ccirc;etoj sur la stratoj kaj la glacio krakas sub la piedoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        hirundoj flugas al la varmaj landoj, se iu el ili malfrui&gcirc;as, tiam &gcirc;i <tld/>i&gcirc;as
        tiel, ke &gcirc;i falas kvaza&ubreve; senviva
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidis tute rigide kaj silente, oni povus pensi, ke li <tld/>i&gcirc;is
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj en la malvarma Decembra nokto devas <tld/>i&gcirc;i
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        pensante ke mi jam sufi&ccirc;e <tld/>i&gcirc;is, mi iris en la kajuton
        <fnt>
          <bib>Kiso</bib>
          <lok>La Sakramento De La Komunio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sango <tld/>i&gcirc;is en miaj vejnoj
        <fnt>
          <bib>VaK</bib>
          <lok>Na&ubreve;a &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frost.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      I&gcirc;i haltigita, paralizi&gcirc;i:
      <ekz>
        sur liaj lipoj subite <tld/>i&gcirc;is la rido
        <fnt>
          <bib>VaK</bib>
          <lok>Na&ubreve;a &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is diplomatiaj rilatoj pro diversaj kialoj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/011128.php">Varmaj vortoj, malvarmaj rilatoj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_ja;&c_r;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">gefrieren</trd>
  <trd lng="en">to <ind>freeze</ind><klr tip="ind">
  (intransitive)</klr></trd>
  <trdgrp lng="es">
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>,
    <trd><ind>congelar</ind>se</trd>,
    <trd><ind>helar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>,
    <trd>se <ind>congeler</ind></trd>,
    <trd>se <ind>figer</ind></trd>,
    <trd>se <ind>paralyser</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megfagy</trd>,
    <trd>befagy</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51CD;&#12360;&#x308B;  <pr>&#12371;&#12372;&#12360;&#x308B;</pr></trd>,
    <trd>&#x51CD;&#x308A;&#12388;&#x304F;  <pr>&#12371;&#x304A;&#x308A;&#12388;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
 <trdgrp lng="pl">
   <trd>marzn&aogonek;&cacute;</trd>,
   <trd>zamarzn&aogonek;&cacute;</trd>
 </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_jo;&c_r;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_m;&c_je;&c_r;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_m;&c_je;&c_r;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D50;&#x51CD; <pr>ji&#xE9;d&#xF2;ng</pr></trd>,
    <trd>&#x7D50;&#x51B0; <pr>ji&#xE9;b&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="frost.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="frost.0ujo.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      &Scirc;ranko en kiu regas konstanta <tld/>a temperaturo, kaj en kiu oni
      povas konservi man&gcirc;a&jcirc;ojn kaj trinka&jcirc;ojn dum pluraj monatoj:
      <ekz>
        metu la &scirc;uon nelonge en <tld/>ujon kaj pro la malalta temperaturo e&ccirc; la plej
        obstina gumo malglui&gcirc;os
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010337.php">Konsiloj avinaj kaj neavinaj</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxrank.frosto0o">glaci&scirc;ranko</ref>,
      <ref cel="frid.0ujo">fridujo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Gefrierschrank</trd>
  <trdgrp lng="en">
    <trd>deep freeze</trd>,
    <trd>deep freezer</trd>
  </trdgrp>
  <trd lng="fr">cong&eacute;lateur</trd>
  <trdgrp lng="hu">
    <trd>fagyaszt&oacute;szekr&eacute;ny</trd>,
    <trd>m&eacute;lyh&udblac;t&odblac;szekr&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51B7;&#x51CD;&#x5EAB;  <pr>&#x308C;&#12356;&#12392;&#12358;&#12371;</pr></trd>,
    <trd>&#x30D5;&#x30EA;&#x30FC;&#x30B6;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">diepvriezer</trd>
  <trd lng="pl">zamra&zdot;arka</trd>
  <trd lng="pt">freezer</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
<trd lng="th">&#xE15;&#xE39;&#xE49;&#xE41;&#xE0A;&#xE48;&#xE07;&#xE41;&#xE02;&#xE47;&#xE07;</trd>
  <trd lng="zh">&#x51B7;&#x51BB;&#x67DC; <pr>l&#x11B;ngd&#xF2;nggu&#xEC;</pr></trd>
