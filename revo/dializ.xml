<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dializ.xml,v 1.20 2021/03/08 15:14:34 revo Exp $">
<kap>
  <rad>dializ</rad>/i
</kap>
<drv mrk="dializ.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Apartigi substancojn, uzante la diferencon de ilia penetra
      rapideco tra membrano.
    </dif>
    <trd lng="pl">dializowa&cacute;</trd>
  </snc>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Elfiltri urinon el la sango per <tld/>a metodo, tiel
      anstata&ubreve;igante la funkcion de malsana reno.
    </dif>
    <trd lng="pl">dializowa&cacute;</trd>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">prov&aacute;d&ecaron;t dial&yacute;zu</trd>
  <trd lng="de">dialysieren</trd>
  <trd lng="en">dialyse</trd>
  <trd lng="es">dializar</trd>
  <trd lng="fr">dialyser</trd>
  <trd lng="hu">dializ&aacute;l</trd>
  <trd lng="pt">dialisar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
    <ind>&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol;
    <ind>&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="sk">robi&tcaron; dial&yacute;zu</trd>
</drv>

<drv mrk="dializ.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_ib;&c_z;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dial&yacute;za</trd>,
    <trd>rozklad</trd>
  </trdgrp>
  <trd lng="de">Dialyse</trd>
  <trd lng="en">dialysis</trd>
  <trd lng="es">di&aacute;lisis</trd>
  <trd lng="fr">dialyse</trd>
  <trd lng="he">&#x5D3;&#x5D9;&#x5D0;&#x5DC;&#x5D9;&#x5D6;&#x5D4;</trd>
  <trd lng="hu">dial&iacute;zis</trd>
  <trd lng="pl">dializa</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;</trd>
  <trd lng="sk">dial&yacute;za</trd>
</drv>

<drv mrk="dializ.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="aparat.0o.TEK">Aparato</ref>
      por <tld/>i:
      <ekz>
        nur senprokrasta sango<tld/>o povas savi multajn malsanulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010451.php">E&ubreve;ropo en la fajro de la epidemio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aparte estos helpataj infanoj kun reno-problemoj: por ili oni a&ccirc;etos hejmajn
        <tld/>ilojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006882.html">Pollando: Kiam orkestro fari&gcirc;as orkesto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>dialyza&ccaron;n&iacute; p&rcaron;&iacute;stroj</trd>,
    <trd>dialyz&aacute;tor</trd>
  </trdgrp>
  <trd lng="de">Dialyseapparat</trd>
  <trd lng="en">dialyser</trd>
  <trd lng="fr">dialyseur</trd>
  <trd lng="he">&#x5DE;&#x5DB;&#x5E9;&#x5D9;&#x5E8; &#x5D3;&#x5D9;&#x5D0;&#x5DC;&#x5D9;&#x5D6;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>dializ&aacute;tor</trd>,
    <trd>m&udblac;vese</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;
    "&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;
    <ind>&c_p;&c_o;&c_ch;&c_k;&c_a;</ind>"</trd>
  </trdgrp>
  <trd lng="sk">dialyz&aacute;tor</trd>
</drv>
</art>
