<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dializ</rad>/i
</kap>
<drv mrk="dializ.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Apartigi substancojn, uzante la diferencon de ilia penetra rapideco
      tra membrano.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Elfiltri urinon el la sango per <tld/>a metodo, tiel anstata&ubreve;igante
      la funkcion de malsana reno.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">dialysieren</trd>
  <trd lng="es">dializar</trd>
  <trd lng="fr">dialyser</trd>
  <trd lng="hu">dializ&aacute;l</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol; <ind>&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;&c_u;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="dializ.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_ib;&c_z;</trd>
  <trd lng="de">Dialyse</trd>
  <trd lng="es">di&aacute;lisis</trd>
  <trd lng="fr">dialyse</trd>
  <trd lng="hu">dial&iacute;zis</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;</trd>
</drv>

<drv mrk="dializ.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato por <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Dialyseapparat</trd>
  <trd lng="fr">dialyseur</trd>
  <trdgrp lng="hu">
    <trd>dializ&aacute;tor</trd>,
    <trd>m&udblac;vese</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_a;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t; "&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_p;&c_o;&c_ch;&c_k;&c_a;</ind>"</trd>
  </trdgrp>
</drv>
</art>
