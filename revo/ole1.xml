<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ole</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ole1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ole1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda arbo, kies plej grava speco estas la
      <ref tip="sub" cel="oliv.0arbo">olivarbo</ref>;
      genro el familio <ref tip="super" cel="oleac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Olea</trd>), .
    </dif>
  </snc>
  <rim mrk="ole1.0o.RIM_HOM">
    Fundamente <ref tip="hom" cel="ole.0o">oleo</ref> estas
    grasa substanco simila al tiu, kiun oni premas el fruktoj
    de olivarbo.
  </rim>
  <trd lng="be">&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">g&egrave;nere Olea</trd>
  <trd lng="de">&Ouml;lbaum</trd>
  <trd lng="fr">olivier<klr> (genre)</klr></trd>
  <trd lng="hu">olajfa</trd>
  <trd lng="nl">olieboom</trd>
  <trd lng="pt">&oacute;lea</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
