<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ole1.xml,v 1.33 2021/06/04 05:58:38 revo Exp $">
<kap>
  <rad>ole</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ole1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ole1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda arbo, kies plej grava specio estas la
      <ref tip="sub" cel="oliv.0arbo">olivarbo</ref>;
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="oleac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Olea</trd>):
      <ekz>
        la folioj de la genro <tld/>o enhavas tri&hcirc;osklereidojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=465505">Oleo (genro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="ole1.0o.RIM_HOM">
    Fundamente <ref tip="hom" cel="ole.0o">oleo</ref> estas
    grasa substanco simila al tiu, kiun oni premas el fruktoj
    de olivarbo.
  </rim>
  <trd lng="af">olie</trd>
  <trd lng="am">&#x12D8;&#x12ED;&#x1275;</trd>
  <trd lng="ar">&#x632;&#x64A;&#x62A;</trd>
  <trd lng="az">neft</trd>
  <trd lng="be">&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="bn">&#x9A4;&#x9C7;&#x9B2;</trd>
  <trd lng="bo">&#xF66;&#xFA3;&#xF74;&#xF58;&#xF0B;</trd>
  <trd lng="ca">g&egrave;nere Olea</trd>
  <trd lng="da">olie</trd>
  <trd lng="de">&Ouml;lbaum</trd>
  <trd lng="en">Olea</trd>
  <trd lng="fr">olivier<klr> (genre)</klr></trd>
  <trd lng="hu">olajfa</trd>
  <trd lng="hy">&#x571;&#x565;&#x569;</trd>
  <trd lng="ne">&#x924;&#x947;&#x932;</trd>
  <trd lng="nl">olieboom</trd>
  <trd lng="pl">oliwka</trd>
  <trd lng="pt">&oacute;lea</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>nafta</trd>,
    <trd>olej</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nafta</trd>,
    <trd>olej</trd>
  </trdgrp>
  <trd lng="sq">naft&#xEB;s</trd>
  <trd lng="tl">langis</trd>
</drv>
</art>
