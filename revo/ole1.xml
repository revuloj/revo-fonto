<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ole</rad>/o
</kap>
<drv mrk="ole1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ole1.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Oleo</trd>), kies plej grava specio estas la
      <ref tip="sub" cel="oliv.0arbo">olivarbo</ref>.
    </dif>
  </snc>
  <trd lng="de">&Ouml;lbaum</trd>
</drv>
<drv mrk="ole1.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      lignaj plantoj (<trd lng="de">Oleaceae</trd>),
      al kiu apartenas ekzemple la genroj:
      <refgrp tip="sub">
        <ref cel="frakse.0o">frakseno</ref>,
        <ref cel="jasmen.0o">jasmeno</ref>,
        <ref cel="ligust.0o">ligustro</ref>,
        <ref cel="ole1.0o">oleo</ref> kaj
        <ref cel="siring.0o">siringo</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="de">&Ouml;lbaumgew&auml;chse</trd>
</drv>
</art>
