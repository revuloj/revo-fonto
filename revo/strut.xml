<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strut.xml,v 1.29 2024/03/25 07:28:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>strut</rad>/o
</kap>
<drv mrk="strut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="strute.0oj"><tld/>edoj</ref>
      (<trd lng="la">Struthio</trd>), kun nur unu vivanta specio;
      la plej granda el la nunaj birdoj, havanta longan kolon kaj
      longajn
      krurojn, ne kapabla flugi, sed eksterordinare rapidkura:
      <ekz>
        en&scirc;ovi <tld/>e la kapon en la sablon;
      </ekz>
      <ekz>
        fermi <tld/>e la okulojn anta&ubreve; la faktoj
        (<refgrp tip="sin">vd.
          <ref cel="ignor.0i">ignori</ref>,
          <ref cel="blind.0a">blinda</ref>
        </refgrp>);
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>a politiko</ind>
        <klr> (rifuzanta vidi la realajn malfacila&jcirc;ojn)</klr>.
        <trd lng="bg">&c_shch;&c_r;&c_a;&c_u;&c_s;&c_o;&c_v;&c_a; &c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_k;&c_a;</trd>
        <trd lng="de">Vogel-Strau&szlig;-Politik</trd>
        <trd lng="hu">struccpolitika</trd>
        <trd lng="nl">struisvogelpolitiek</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_w;&c_s;</trd>
  <trd lng="bg">&c_shch;&c_r;&c_a;&c_u;&c_s;</trd>
  <trd lng="ca">estru&ccedil;</trd>
  <trd lng="cs">p&scaron;tros</trd>
  <trd lng="de">Strau&szlig;</trd>
  <trd lng="en">ostrich</trd>
  <trd lng="fa">&a_sh;&a_t;&a_r;&a_m;&a_r;&a_gh;</trd>
  <trd lng="fr">autruche</trd>
  <trd lng="hu">strucc</trd>
  <trd lng="ja">&#x30C0;&#x30C1;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">struisvogel</trd>
  <trd lng="pl">stru&sacute;</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_u;&c_s;</trd>
  <trd lng="sk">p&scaron;tros</trd>
  <trd lng="sv">struts</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_u;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D15;&#x9CE5; <pr>tu&#xF3;ni&#x1CE;o</pr></trd>,
    <trd>&#x9E35;&#x9E1F; <pr>tu&#xF3;ni&#x1CE;o</pr></trd>,
    <trd>&#x9D15; <pr>tu&#xF3;</pr></trd>,
    <trd>&#x9E35; <pr>tu&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="strut.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Struthioniformes</trd>), de plej ofte grandaj
      birdoj, ne kapablaj flugi.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_w;&c_s;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="bg">&c_shch;&c_r;&c_a;&c_u;&c_s;&c_o;&c_p;&c_o;&c_d;&c_o;&c_b;&c_n;&c_i;</trd>
  <trd lng="ca">estrucioniformes </trd>
  <trd lng="de">Strau&szlig;env&ouml;gel</trd>
  <trd lng="en">ratites</trd>
  <trd lng="fa">&a_sh;&a_t;&a_r;&a_m;&a_r;&a_gh;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">Struthioniformes</trd>
  <trd lng="hu">struccalak&uacute;ak</trd>
  <trdgrp lng="nl">
    <trd>struisvogels</trd>,
    <trd>loopvogels</trd>
  </trdgrp>
  <trd lng="pl">strusiowate</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_u;&c_s;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_u;&c_s;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D15;&#x9CE5;&#x79D1; <pr>tu&#xF3;ni&#x1CE;ok&#x113;</pr></trd>,
    <trd>&#x9E35;&#x9E1F;&#x79D1; <pr>tu&#xF3;ni&#x1CE;ok&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
