<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>skism</rad>/o
</kap>
<drv mrk="skism.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Opa disi&gcirc;o de ia eklezio fare de homoj, kiuj, kvankam akceptante
      la
      esencon de la dogmaro, malakceptas la eklezian a&ubreve;toritaton kaj
      kelkajn neesencajn punktojn de la kredo.
    </dif>
    <refgrp tip="sin">
      <ref cel="herez.0o">herezo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Disi&gcirc;o pro malsameco de opinioj:
      <ekz>
        kiu ajn volus &scirc;an&gcirc;i la naturan iradon de nia afero, povos nur
        krei
        kelktempan <tld/>on kaj akiri la mal&gcirc;ojan gloron de malhelpanto
        kaj subfosanto<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fend.0o">fendo</ref>,
      <ref cel="romp">rompo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_h;&c_ib;&c_z;&c_m;&c_a;</trd>, 
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schisma</trd>,
    <trd>Spaltung</trd>
  </trdgrp>
  <trd lng="en">schism</trd>
  <trdgrp lng="hu">
    <trd>skizma</trd>,
    <trd>hitszakad&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schisma</trd>,
    <trd>scheuring</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_h;&c_i;&c_z;&c_m;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;</trd>
  </trdgrp>
</drv>
</art>
