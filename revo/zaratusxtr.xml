<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Zaratu&scirc;tr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="zaratusxtr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Persa profeto (proskimume 660 a. K. &ndash; proksimume 583 a. K.),
      reforminto de la antikva irana religio, kiu sekve i&gcirc;is 
<ref tip="vid" cel="mazda.0ismo">mazdaismo</ref>.
    </dif>
    <rim>
      La supraj vivdatoj valoras nur kiel indikoj pri la tradicie 
      akceptata hipotezo.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Z;&c_a;&c_r;&c_a;&c_a;&c_s;&c_t;&c_r;</trd>,
    <trd>&c_Z;&c_a;&c_r;&c_a;&c_t;&c_u;&c_s;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_Z;&c_o;&c_r;&c_o;&c_a;&c_s;&c_t;&c_malmol;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Zoroaster</trd>,
    <trd>Zarathustra</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_z;&a_r;&a_t;&a_sh;&a_t;</trd>,
    <trd>&a_z;&a_r;&a_d;&a_sh;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Zoroastre</trd>,
    <trd>Zarathoustra</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Zarathusztra</trd>,
    <trd>Zoroaszter</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Zaratustra</trd>,
    <trd>Zoroastro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_Z;&c_o;&c_r;&c_o;&c_a;&c_s;&c_t;&c_r;</trd>,
    <trd>&c_Z;&c_a;&c_r;&c_a;&c_t;&c_u;&c_s;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>Zaratustra</trd>,
    <trd>Zoroastro</trd>
  </trdgrp>
</drv>
</art>

