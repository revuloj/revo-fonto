<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gusx.xml,v 1.21 2023/10/15 18:38:19 revo Exp $">
<kap>
  <rad>gu&scirc;</rad>/o
</kap>
<drv mrk="gusx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Unufaka <ref tip="super" cel="frukt.malferm0o">malfermfrukto</ref>
      de la <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>,
      esti&gcirc;anta el unuopa fruktofolio kaj havanta du partojn, kiuj
      disi&gcirc;as &ccirc;e maturi&gcirc;o:
      <ekz>
        li e&ccirc; ne povus diri, &ccirc;u tio &ccirc;i estis paste&ccirc;oj a&ubreve; grandaj <tld/>oj a&ubreve; nuksoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> enhavas tripsinan inhibilon, <klr>[kies]</klr> 
        &ccirc;efa tasko ekz. je la sojo estas protektado kontra&ubreve; insekto, tripsino estas
        unu el digestaj enzimoj kaj nekuiritaj <tld/>oj povas ka&ubreve;zi gravajn digestajn problemojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20026">Gu&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;e Z aperas ankora&ubreve; la nomo <ref tip="sin" cel="sxel.0o.gusxo">&scirc;elo<sncref/></ref>.
    </rim>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="silikv.0o">silikvo</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_b; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_sh;&c_u;&c_sh;&c_u;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">llegum</trd>
  <trd lng="de">H&uuml;lse</trd>
  <trdgrp lng="en">
    <trd>pod</trd>,
    <trd>husk</trd>
  </trdgrp>
  <trd lng="fr">gousse</trd>
  <trd lng="hu">h&uuml;vely</trd>
  <trd lng="it">baccello</trd>
  <trd lng="ja">&#x83A2;  <pr>&#x3055;&#x3084;</pr></trd>
  <trd lng="nl">peul</trd>
  <trd lng="pl">str&aogonek;k</trd>
  <trd lng="pt">vagem</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_b; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>,
    <trd>&c_s;&c_t;&c_r;&c_u;&c_ch;&c_o;&c_k; <klr>(&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_b;&c_y;&c_t;&c_o;&c_v;&c_o;&c_je; &c_n;&c_a;&c_z;&c_v;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x442;&#x440;&#x443;&#x447;&#x43E;&#x43A;</trd>
</drv>
</art>
