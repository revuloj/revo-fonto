<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>racion</rad>/o
</kap>
<drv mrk="racio5.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">NEO</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <snc>
    <dif>
      <ref cel="racio5.0a"><tld lit="R"/>a</ref> 
      <ref tip="super" cel="nombr.0o.MAT">nombro</ref>.
    </dif>
    <ref tip="super" cel="reel.0o">reelo.</ref>
    <rim>
      La substantiva vico <ctl>entjero, raciono, reelo, komplekso</ctl> estas
      pli regula ol <ctl>entjero, racionala nombro, reelo, kompleksa
      nombro</ctl>; krome, <ctl>raciono</ctl> estas pli mallonga.
    </rim>
  </snc>
  <trd lng="de">rationale <ind>Zahl</ind></trd>
  <trd lng="en">rational <ind>number</ind></trd>
  <trd lng="fr">nombre rationnel</trd>
  <trd lng="nl">rationeel <ind>getal</ind></trd>
  <trd lng="ru">&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
</drv>
<drv mrk="racio5.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>SPIV</bib></fnt>
  <snc>
    <uzo tip="klr">(p.p. nombro)</uzo>
    <ref tip="dif" cel="racion.0a.nombro">racionala.</ref>
  </snc>
<trd lng="nl">rationeel<klr tip="ind"> (getal)</klr></trd>
</drv>

</art>
