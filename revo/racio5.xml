<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>racion</rad>/o <fnt>SPIV</fnt>
    </kap>
    <drv mrk="racio5.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">MAT</uzo>
        <uzo>(nova)</uzo>
        <dif>
          <ref tip="dif" cel="racion.0a.nombro">Racionala nombro</ref>.
        </dif>
        <ref tip="super" cel="reel.0o">reelo</ref>
      </snc>
      <rim>
        La substantiva vico <ctl>entjero, raciono, reelo</ctl> estas
        pli regula ol <ctl>entjero, racionala nombro, reelo</ctl>;
        krome, <ctl>raciono</ctl> estas pli mallonga.
      </rim>
      <trd lng="en">rational number</trd>
      <trd lng="ru">&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
    </drv>
  </art>

