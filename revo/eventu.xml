<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>eventual</rad>/a
</kap>

<drv mrk="eventu.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
    Povanta okazi, depende de necertaj cirkonstancoj:
    <ekz>la <tld/>aj profitoj de la afero.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ebl.0a">ebla</ref>,
      <ref cel="probab.0a">probabla</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_y;</trd>,
    <trd>&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_v;&c_e;&c_n;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eventuell</trd>,
    <trd>m&ouml;glich</trd>,
    <trd>etwaig</trd>,
    <trd>allf&auml;llig<klr> [&Ouml;s]</klr></trd>
  </trdgrp>
  <trd lng="en">contingent</trd>
  <trd lng="es">eventual</trd>
  <trd lng="fr">&eacute;ventuel</trd>
  <trd lng="hu">esetleges</trd>
  <trdgrp lng="pl">
    <trd>ewentualny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>eventual</trd>,
    <trd>contingente</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_p;&c_o;&c_l;&c_n;&c_je;
    &c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>eventuell</trd>,
    <trd>m&ouml;jlig</trd>
  </trdgrp>
</drv>

<drv mrk="eventu.0e">
  <kap><tld/>e</kap>
  <snc>
  <dif>
  En okazo de bezono: <ekz>skribu, <tld/>e telegrafu.</ekz>
  </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_v;&c_y;&c_p;&c_a;&c_d;&c_k;&c_u;</ind></trd>,
    <trd>&c_p;&c_r;&c_y;
    <ind>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>eventuell</trd>,
     <trd>evtl.<klr> [mll.]</klr></trd>,
     <trd>m&ouml;glicherweise</trd>,
     <trd>etwa</trd>
  </trdgrp>
  <trd lng="es">eventualmente</trd>
  <trd lng="fr">&eacute;ventuellement</trd>
  <trd lng="hu">esetleg</trd>
  <trd lng="pl">ewentualnie</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je;</trd>,
    <trd>&c_v; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je;
    &c_ch;&c_je;&c_g;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">eventuellt</trd>
</drv>

<drv mrk="eventu.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
  <dif>
  <tld lit="E"/>a fakto. 
  </dif>
  <refgrp tip="vid">
    <ref cel="ebl.0a">ebla</ref>,
    <ref cel="okaz.0o">okazo</ref>,
    <ref cel="hazard.0o">hazardo</ref>.
  </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_y;
    &c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_y;
    &c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Eventualit&auml;t</trd>,
     <trd>etwa eintretender Umstand</trd>,
     <trd>m&ouml;gliches Ereignis</trd>
  </trdgrp>
  <trd lng="es">eventualidad</trd>
  <trd lng="fr">&eacute;ventualit&eacute;</trd>
  <trd lng="hu">eshet&odblac;s&eacute;g</trd>
  <trd lng="pl">ewetualno&sacute;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;
    &c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_o;&c_je;
    &c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>
