<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eventu.xml,v 1.25 2022/04/27 14:38:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>eventual</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="eventu.0a">
  <kap><tld/>a</kap>
  <snc mrk="eventu.0a.KOMUNE">
    <dif>
      Povanta okazi, depende de necertaj cirkonstancoj:
      <ekz>
        <tld/>a vintra glacii&gcirc;o de la rivero povus estigi dan&gcirc;erajn inundojn printempe
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Steloj;monato2/006857.html">Trafiko: Danuba &scirc;iptrafiko paralizita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la uzantoj de Esperanto tre konscias pri la <tld/>a nekompreneblo de iu suka
        loklingva esprimo en internacia medio
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006813.html">Sa&gcirc;a kapo duonvorton komprenas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ebl.0a">ebla</ref>,
      <ref cel="kondic1.0a">kondi&ccirc;a</ref>,
      <ref cel="okaz.0a.situacia">okaza</ref>,
      <ref cel="probab.0a">probabla</ref>,
      <ref cel="sxajn.ver0a">ver&scirc;ajna</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_y;</trd>,
    <trd>&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_v;&c_e;&c_n;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>eventu&aacute;ln&iacute;</trd>,
    <trd>p&rcaron;&iacute;padn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eventuell</trd>,
    <trd>m&ouml;glich</trd>,
    <trd>etwaig</trd>,
    <trd>allf&auml;llig<klr> [&Ouml;s]</klr></trd>
  </trdgrp>
  <trd lng="en">contingent</trd>
  <trd lng="es">eventual</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_H;&a_t;&a_m;&a_A;&a_l;&f_y;</trd>,
    <trd>&a_m;&a_H;&a_t;&a_m;&a_l;&zwnj;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;ventuel</trd>
  <trd lng="hu">esetleges</trd>
  <trdgrp lng="pl">
    <trd>ewentualny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>eventual</trd>,
    <trd>contingente</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_p;&c_o;&c_l;&c_n;&c_je;
    &c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;padn&yacute;</trd>
  <trdgrp lng="sv">
    <trd>eventuell</trd>,
    <trd>m&ouml;jlig</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x436;&#x43B;&#x438;&#x432;&#x438;&#x439; (&#x437;&#x430; &#x43F;&#x435;&#x432;&#x43D;&#x438;&#x445; &#x43E;&#x431;&#x441;&#x442;&#x430;&#x432;&#x438;&#x43D;)</trd>,
    <trd>&#x439;&#x43C;&#x43E;&#x432;&#x456;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x435;&#x432;&#x435;&#x43D;&#x442;&#x443;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43D;&#x430;&#x433;&#x456;&#x434;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="eventu.0e">
  <kap><tld/>e</kap>
  <snc mrk="eventu.0e.KOMUNE">
    <dif>
      La&ubreve; la kondi&ccirc;oj, se <ref cel="ebl.0e">eble</ref>:
      <ekz>
        skribu, <tld/>e telegrafu;
      </ekz>
      <ekz>
        prokuroroj ekis ekzameni, minacas lin <tld/>e puno
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Steloj;monato2/006835.html">Germanio: Pekoj de
          patriarko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vice staras eksterlandanoj el orienta E&ubreve;ropo kaj el Afriko, petante permeson
        lo&gcirc;i kaj <tld/>e labori en Irlando
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Steloj;monato2/007030.html">Irlando: Ser&ccirc;ata: justa
          solvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ebl.laux0e">la&ubreve;eble</ref>,
      <ref cel="supoz.0eble">supozeble</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_v;&c_y;&c_p;&c_a;&c_d;&c_k;&c_u;</ind></trd>,
    <trd>&c_p;&c_r;&c_y;
    <ind>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF45;&#xF72;&#xF42;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;&#xF53;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>eventu&aacute;ln&ecaron;</trd>,
    <trd>pop&rcaron;&iacute;pad&ecaron;</trd>,
    <trd>p&rcaron;&iacute;padn&ecaron;</trd>,
    <trd>respektive</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>eventuell</trd>,
     <trd>evtl.<klr> [mll.]</klr></trd>,
     <trd>m&ouml;glicherweise</trd>,
     <trd>etwa</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>possibly</trd>,
    <trd>perhaps</trd>
  </trdgrp>
  <trd lng="es">eventualmente</trd>
  <trd lng="fr">&eacute;ventuellement</trd>
  <trd lng="hu">esetleg</trd>
  <trd lng="pl">ewentualnie</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je;</trd>,
    <trd>&c_v; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je;
    &c_ch;&c_je;&c_g;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;padne</trd>
  <trd lng="sv">eventuellt</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43D;&#x430;&#x433;&#x456;&#x434;&#x43D;&#x43E;</trd>,
    <trd>&#x43F;&#x440;&#x438; &#x43D;&#x430;&#x433;&#x43E;&#x434;&#x456;</trd>,
    <trd>&#x443; &#x440;&#x430;&#x437;&#x456; &#x447;&#x43E;&#x433;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="eventu.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="eventu.0ajxo.KOMUNE">
    <dif>
      <tld lit="E"/>a fakto, ebla okaza&jcirc;o dependa je
      cirkonstancoj:
      <ekz>
        mi dubas, &ccirc;u vi sukcesos <klr>[&#x2026;]</klr> kaj por &ccirc;iu <tld/>o, mi kunvokos la
        kunvenon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sortobatoj &gcirc;enerale rezultas de <tld/>oj neperceptitaj, a&ubreve; ne 
        sufi&ccirc;e atentitaj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkon1.0o">cirkonstanco</ref>,
      <ref cel="hazard.0o">hazardo</ref>,
      <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
      <ref cel="okaz.0o">okazo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_y;
    &c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_y;
    &c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">eventualita</trd>
  <trdgrp lng="de">
     <trd>Eventualit&auml;t</trd>,
     <trd>etwa eintretender Umstand</trd>,
     <trd>m&ouml;gliches Ereignis</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>contingency</trd>,
    <trd>eventuality</trd>
  </trdgrp>
  <trd lng="es">eventualidad</trd>
  <trd lng="fr">&eacute;ventualit&eacute;</trd>
  <trd lng="hu">eshet&odblac;s&eacute;g</trd>
  <trd lng="pl">ewetualno&sacute;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;
    &c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_o;&c_je;
    &c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">eventualita</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43F;&#x430;&#x434;&#x43A;&#x43E;&#x432;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x430;&#x434;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
