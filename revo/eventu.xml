<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>eventual</rad>/a
</kap>
<drv mrk="eventu.0a">
<kap><tld/>a</kap>
<snc>
<dif>
Povanta okazi, depende de necertaj cirkonstancoj:
<ekz>la <tld/>aj profitoj de la afero.</ekz>
</dif>
</snc>
<trd lng="en">contingent</trd>
<trdgrp lng="ru">
  <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_v;&c_p;&c_o;&c_l;&c_n;&c_je; &c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
</trdgrp>
<trdgrp lng="sv">
  <trd>eventuell</trd>,
  <trd>m&ouml;jlig</trd>
</trdgrp>
</drv>
<drv mrk="eventu.0e">
<kap><tld/>e</kap>
<snc>
<dif>
En okazo de bezono: <ekz>skribu, <tld/>e telegrafu.</ekz>
</dif>
</snc>
<trdgrp lng="ru">
  <trd>&c_p;&c_r;&c_i; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je;</trd>,
  <trd>&c_v; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je; &c_ch;&c_je;&c_g;&c_o;</trd>
</trdgrp>
<trd lng="sv">eventuellt</trd>
</drv>
<drv mrk="eventu.0ajxo">
<kap><tld/>(a&jcirc;)o</kap>
<snc>
<dif>
<tld/>a fakto. 
</dif>
<refgrp tip="sin">
<ref cel="ebl.0a">ebla</ref>,
<ref cel="okaz.0o">okazo</ref>,
<ref cel="hazard.0o">hazardo</ref>.
</refgrp>
</snc>
<trdgrp lng="ru">
  <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;</trd>,
  <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_o;&c_je; &c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
</trdgrp>
</drv>
</art>
