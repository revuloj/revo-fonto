<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>ost</rad>/o</kap>
<drv mrk="ost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Malmola, fortika, kalkeca substanco, el kiu konsistas la skeleto de
      vertebruloj:
      <ekz>
        jen estas li proprakorpe kun karno kaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>por malfrua gasto restas nur <tld/>o
        <klr>(sen viando)</klr>
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>&gcirc;i fari&gcirc;is por mi <tld/>o en la gor&gcirc;o
        <klr>(granda &gcirc;ena&jcirc;o)</klr>
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="en">bone</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Knochen</trd>,
    <trd>Bein</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&kappa;&omikron_ton;&kappa;&alfa;&lambda;&omikron;</trd>,
    <trd>&omikron_ton;&sigma;&tau;&omikron;&ypsilon;&ny;</trd>
  </trdgrp>
  <trd lng="fr">os</trd>
  <trd lng="nl">been<klr tip="ind"> (skelet)</klr></trd>
  <trd lng="pt">osso</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">ben<klr tip="ind"> (i skelettet)</klr></trd>
</drv>

<drv mrk="ost.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a malsano.
      </ekz>
    </dif>
    <trd lng="de">Knochen-</trd>
    <trd lng="en">bone</trd>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="fr">osseux</trd>
  <snc>
    <dif>
      Farita el <tld/>o:
      <ekz>
        <tld/>a &jcirc;etkubo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kn&ouml;chern</trd>,
      <trd>knochig</trd>,
     <trd>Wort</trd>
    </trdgrp>
    <trd lng="en">bony</trd>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_ja;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta dikajn <tld/>ojn:
      <ekz>
        <tld/>a viro.
      </ekz>
    </dif>
    <trd lng="de">grobknochig</trd>
    <trdgrp lng="en">
      <trd>large-boned</trd>,
      <trd>thick-boned</trd>,
      <trd>large-framed</trd>
    </trdgrp>
    <trd lng="fr">osseux</trd>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_l;&c_ja;&c_v;&c_y;&c_j;</trd>
  </snc>
  <trd lng="el">&omikron;&sigma;&tau;&epsilon;&omega_ton;&delta;&eta;&sigma_fina;</trd>
  <trd lng="pt">&oacute;sseo</trd>
</drv>

<drv mrk="ost.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Tuto de la ostoj de homo a&ubreve; besto.
    </dif>
    <trd lng="en">skeleton</trd>
  </snc>
  <refgrp tip="sin">
    <ref cel="skelet.0o.ANA">skeleto</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Gebein</trd>,
    <trd>Gebeine</trd>
  </trdgrp>
  <trd lng="el">&sigma;&kappa;&epsilon;&lambda;&epsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>ossements</trd>,
    <trd>squelette</trd>
  </trdgrp>
  <trd lng="nl">geraamte</trd>
  <trd lng="pt">esqueleto</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_t;&c_ja;&c_k;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_o;&c_v;</trd>,
    <trd>&c_s;&c_k;&c_je;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
</art>
