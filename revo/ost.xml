  </snc>
  <trd lng="de">Spielknochen</trd>
</drv>


<drv mrk="ost.pub0o">
  <kap><ofc>9</ofc>pub<tld/>o 
  <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="ost.pub0o.ANA">
  <uzo tip="fak">ANA</uzo>
    <dif>
      Malsupra kaj anta&ubreve;a parto de la koks<tld/>o:      
      <ekz>
        la fakto, ke io premas lian pub<tld/>on, donis ian kontentigon al tiu pozicio
        <fnt>
          <bib>ChM</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        monto de Venuso, nomi&gcirc;as la eta plialti&gcirc;o super la ina
        pub<tld/>o, konsistanta el subha&ubreve;ta grasa histo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227336">Puba
          monto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        maniraptorojn karakterizas la longaj brakoj kaj manoj,
        <klr>[&#8230;]</klr> rigida vosto, plumoj, kaj longa, malanta&ubreve;en
        direktita pub<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227336">Puba
          monto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_b;&c_k;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_b;&c_k;&c_o;&c_v;&c_a;&c_ja; <ind>&c_k;&c_o;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pubis</trd>,
    <trd>pubic bone</trd>,
  </trdgrp>
  <trd lng="de">Schambein</trd>
  <trd lng="fr">pubis<klr> (os du)</klr></trd>
  <trd lng="hu">szem&eacute;remcsont</trd> 
  <trdgrp lng="it">
    <trd>pube</trd>,
    <trd>osso pubico</trd>    
  </trdgrp>
  <trd lng="nl">schaambeen</trd>
  <trd lng="pl">ko&sacute;&cacute; &lstroke;onowa </trd>
  <trd lng="pt">p&uacute;bis</trd>
  <trd lng="ru">&c_l;&c_o;&c_b;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43B;&#x43E;&c_b;&#x43A;&#x43E;&c_v;&c_a; &#x43A;&c_ib;&c_s;&c_t;&#x43A;&c_a;</trd>
</drv>






<drv mrk="ost.vang0o">
  <kap>vang<tld/>o</kap>
  <snc mrk="ost.vang0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
