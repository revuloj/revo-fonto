    <trd>kostra</trd>,
    <trd>skelet</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gebein</trd>,
    <trd>Gebeine</trd>,
    <trd>Skelett</trd>,
    <trd>Knochenger&uuml;st</trd>
  </trdgrp>
  <trd lng="el">&sigma;&kappa;&epsilon;&lambda;&epsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">skeleton</trd>
  <trdgrp lng="es">
    <trd>esqueleto</trd>,
    <trd>osamenta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ossature</trd>
  </trdgrp>
  <trd lng="he">&shin;&lamed;&dalet;</trd>
  <trd lng="hu">csontozat</trd>
  <trd lng="it">scheletro</trd>
  <trd lng="ja">&#x9AA8;&#x683C;  <pr>&#x3053;&#x3063;&#x304B;&#x304F;</pr></trd>
  <trdgrp lng="nl">
