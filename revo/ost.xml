<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>ost</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ost.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malmola, fortika, kalkeca substanco, el kiu konsistas la skeleto
      de
      vertebruloj:
      <ekz>
        jen estas li proprakorpe kun karno kaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        por malfrua gasto restas nur <tld/>o
        <klr>(sen viando)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;i fari&gcirc;is por mi <tld/>o en la gor&gcirc;o
        <klr>(granda &gcirc;ena&jcirc;o)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">askorn</trd>
  <trdgrp lng="de">
    <trd>Knochen</trd>,
    <trd>Bein</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&kappa;&omikron_ton;&kappa;&alfa;&lambda;&omikron;</trd>,
    <trd>&omikron_ton;&sigma;&tau;&omikron;&ypsilon;&ny;</trd>
  </trdgrp>
  <trd lng="en">bone</trd>
  <trd lng="es">hueso</trd>
  <trd lng="fr">os</trd>
  <trd lng="he">&ayin;&tsadi;&fmem;</trd>
  <trd lng="hu">csont</trd>
  <trd lng="it">osso</trd>
  <trd lng="nl">been<klr tip="ind"> (skelet)</klr></trd>
  <trdgrp lng="pl">
    <trd>ko&sacute;&cacute;</trd>,
    <trd>o&sacute;&cacute;</trd>,
  </trdgrp>
  <trd lng="pt">osso</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">ben<klr tip="ind"> (i skelettet)</klr></trd>
</drv>

<drv mrk="ost.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a malsano.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_v;&c_y;</trd>
    <trd lng="bg">&c_k;&c_o;&c_s;&c_t;&c_je;&c_l;&c_i;&c_v;</trd>
    <trd lng="br">-<ind>eskern</ind></trd>
    <trd lng="de">Knochen-</trd>
    <trd lng="en">bone</trd>
    <trd lng="he">&shin;&lamed; <ind>&ayin;&tsadi;&mem;&vav;&tav;</ind></trd>
    <trd lng="nl">beender-</trd>
    <trd lng="it">osseo</trd>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Farita el <tld/>o:
      <ekz>
        <ind><tld/>a &jcirc;etkubo</ind>.
        <trd lng="hu">csontkocka</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_y;</trd>
    <trd lng="br">en <ind>askorn</ind></trd>
    <trdgrp lng="de">
      <trd>kn&ouml;chern</trd>,
      <trd>knochig</trd>,
    </trdgrp>
    <trdgrp lng="en">
      <trd>bony</trd>,
      <trd>bone</trd>
    </trdgrp>
    <trd lng="he">&mem;<ind>&ayin;&tsadi;&fmem;</ind></trd>
    <trd lng="nl">benen<klr tip="ind"> (uit been)</klr></trd>
    <trdgrp lng="it">
      <trd>di <ind>osso</ind></trd>,
      <trd>osseo</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_ja;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta dikajn <tld/>ojn:
      <ekz>
        <tld/>a viro.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_ch;&c_a;&c_v;&c_y;</trd>
    <trd lng="br">askornek</trd>
    <trd lng="de">grobknochig</trd>
    <trdgrp lng="en">
      <trd>large-boned</trd>,
      <trd>thick-boned</trd>,
      <trd>large-framed</trd>
    </trdgrp>
    <trd lng="es">huesudo</trd>
    <trd lng="it">ossuto<klr> (attenzione: con ossa grandi o sporgenti,
    ma 
indif. se magro o no)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_l;&c_ja;&c_v;&c_y;&c_j;</trd>
  </snc>
  <trd lng="el">
&omikron;&sigma;&tau;&epsilon;&omega_ton;&delta;&eta;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>en <ind>os</ind></trd>,
    <trd>osseux</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&lamed;&alef; <ind>&ayin;&tsadi;&mem;&vav;&tav;</ind></trd>,
    <trd>&shin;&lamed; <ind>&ayin;&tsadi;&mem;&vav;&tav;</ind></trd>,
    <trd>&gimel;&resh;&mem;&yod;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csont-</trd>,
    <trd>csontos</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kostny</trd>,
    <trd>ostny</trd>,
  </trdgrp>
  <trd lng="pt">&oacute;sseo</trd>
</drv>

<drv mrk="ost.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="ost.0aro.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tuto de la <tld/>oj de homo a&ubreve; besto:
      <ekz>
        li kune kun sia medicina ilaro kaj libraro alveturigis anka&ubreve;
        tiun &ccirc;i homan <tld/>aron, kiu anta&ubreve;e staris en
lia kabineto
        apud la skribotablo
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="skelet.0o.ANA">skeleto</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_ib;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_je;&c_l;&c_je;&c_t;</trd>
  <trd lng="br">skeledenn</trd>
  <trdgrp lng="de">
    <trd>Gebein</trd>,
    <trd>Gebeine</trd>,
    <trd>Skelett</trd>
  </trdgrp>
  <trd lng="el">
&sigma;&kappa;&epsilon;&lambda;&epsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">skeleton</trd>
  <trdgrp lng="es">
    <trd>esqueleto</trd>,
    <trd>osamenta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ossature</trd>
  </trdgrp>
  <trd lng="he">&shin;&lamed;&dalet;</trd>
  <trd lng="hu">csontozat</trd>
  <trd lng="it">scheletro</trd>
  <trdgrp lng="nl">
    <trd>skelet</trd>,
    <trd>geraamte</trd>
  </trdgrp>
  <trd lng="pl">ko&sacute;ciec</trd>
  <trd lng="pt">esqueleto</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_t;&c_ja;&c_k;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_o;&c_v;</trd>,
    <trd>&c_s;&c_k;&c_je;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="ost.gxis0a">
  <kap>&gcirc;is<tld/>a</kap>
  <snc mrk="ost.gxis0a.FIG">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Plene penetrita de ideo, konvikita sen hezito a&ubreve; cedemo:
      <ekz>
        &gcirc;is<tld/>aj sennaciuloj, t.e. homoj nun ne ekzistantaj
        <fnt><bib>Lanti</bib>, <lok>Esperanto estas la plej malfacile
        ellernebla lingvo</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;is<tld/>a esperantisto eniras televidil-vendejon kaj
        demandas: &Ccirc;u estas &ccirc;e Vi koloraj televidiloj
        <fnt>
        <aut>M. Zdankowski</aut>
        <vrk>Re: Anekdoto de l'tago</vrk>
        <lok>soc.culture.esperanto, 1999-03-29</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="radika.0a.radika">radikala <sncref/></ref>
    </refgrp>
    <trd lng="en">to the bone</trd>
    <trdgrp lng="fr">
      <trd>convaincu<klr tip="amb"> (s&ucirc;r de son fait)</klr></trd>,
      <trd>radical</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>overtuigd</trd>,
      <trd>radicaal</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>irriducibile</trd>, 
      <trd>fino al <ind>midollo</ind><klr> (inveterato)</klr></trd>,
      <trd>convinto<klr> (irriducibile)</klr></trd>,
      <trd>inveterato<klr> (ostinato sino in fondo)</klr></trd>,
      <trd>acerrimo</trd>,
      <trd>incallito</trd>,
      <trd>viscerale<klr> (fig.)</klr></trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_b;&c_je;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;
&c_d;&c_o; &c_m;&c_o;&c_z;&c_g;&c_a;
&c_k;&c_o;&c_s;&c_t;&c_je;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
