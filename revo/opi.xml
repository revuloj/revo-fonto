<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: opi.xml,v 1.24 2026/01/04 10:34:11 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>opi</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="opi.0o">
  <kap><tld/>o</kap>
  <snc mrk="opi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="drog.0o">Drogo</ref>, kvietiga la&ubreve; malgrandaj dozoj, venena la&ubreve;
      grandaj dozoj, kiun oni ekstraktas el suko de papavo:
      <ekz>
        <ind>fumejo de <tld/>o</ind>
        <fnt>
          <bib>FdO</bib>
        </fnt>;
        <trd lng="de">Opiumh&ouml;hle</trd>
        <trd lng="fr">fumerie d'opium</trd>
      </ekz>
      <ekz>
        e&ccirc; sen la <tld/>o oni povis revi dol&ccirc;an son&gcirc;on
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        safrana tinkturo de <tld/>o
        <klr>(<ref cel="lauxda.0o">la&ubreve;dano</ref>)</klr>
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al kamparanino, kiu plendis pri premo <klr>[&#x2026;]</klr> mi enver&scirc;is <tld/>on
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili financas sin per kultivado de papavoj kaj vendado de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009771.php"
              >Helpu Afganion!</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ri&ccirc;uloj kutimis uzi kokainon kaj <tld/>on, konsideratajn elegantaj drogoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_p;&c_ib;&c_ju;&c_m;</trd>
  <trd lng="cs">opium</trd>
  <trd lng="de">Opium</trd>
  <trd lng="el">&omikron_ton;&pi;&jota;&omikron;</trd>
  <trd lng="en">opium</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_r;&f_y;&a_A;&f_k;</trd>
    <trd>&a_A;&a_f;&f_y;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">opium</trd>
  <trd lng="hu">&oacute;pium</trd>
  <trd lng="ja">&#x30A2;&#x30D8;&#x30F3;  <pr>&#x3042;&#x3078;&#x3093;</pr></trd>
  <trd lng="nl">opium</trd>
  <trd lng="pl">opium</trd>
  <trd lng="pt">&oacute;pio</trd>
  <trd lng="ro">opiu</trd>
  <trd lng="ru">&c_o;&c_p;&c_i;&c_u;&c_m;</trd>
  <trd lng="sk">&oacute;pium</trd>
  <trd lng="sv">opium</trd>
  <trd lng="uk">&c_o;&c_p;&c_ib;&c_u;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D09;&#x7247; <pr>y&amacron;pi&agrave;n</pr></trd>,
    <trd>&#x9E26;&#x7247; <pr>y&amacron;pi&agrave;n</pr></trd>,
    <trd>&#x963F;&#x8299;&#x84C9; <pr>&amacron;f&uacute;r&oacute;ng</pr></trd>,
    <trd>&#x5927;&#x70DF; <pr>d&agrave;y&amacron;n</pr></trd>,
    <trd>&#x5927;&#x7159; <pr>d&agrave;y&amacron;n</pr></trd>,
    <trd>&#x963F;&#x7247; <pr>&amacron;pi&agrave;n</pr></trd>,
    <trd>&#x798F;&#x58FD;&#x818F; <pr>f&uacute;sh&ograve;ug&amacron;o</pr></trd>,
    <trd>&#x798F;&#x5BFF;&#x818F; <pr>f&uacute;sh&ograve;ug&amacron;o</pr></trd>
  </trdgrp>
</drv>
