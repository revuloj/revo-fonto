<art mrk="$Id: opi.xml,v 1.20 2023/09/03 07:12:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>opi</rad>/o
</kap>
<drv mrk="opi.0o">
  <kap><tld/>o</kap>
  <snc mrk="opi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Narkota substanco, kvietiga la&ubreve; malgrandaj dozoj, venena la&ubreve;
      grandaj
      dozoj, kiun oni ekstraktas el suko de papavo:
      <ekz>
        <ind>fumejo de <tld/>o</ind><fnt>B</fnt>.
        <trd lng="fr">fumerie d'opium</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_p;&c_ib;&c_ju;&c_m;</trd>
  <trd lng="cs">opium</trd>
  <trd lng="de">Opium</trd>
  <trd lng="el">&omikron_ton;&pi;&jota;&omikron;</trd>
  <trd lng="en">opium</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_r;&f_y;&a_A;&f_k;</trd>
    <trd>&a_A;&a_f;&f_y;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">opium</trd>
  <trd lng="hu">&oacute;pium</trd>
  <trd lng="ja">&#x30A2;&#x30D8;&#x30F3;  [<ind>&#x3042;&#x3078;&#x3093;</ind>]</trd>
  <trd lng="nl">opium</trd>
  <trd lng="pl">opium</trd>
  <trd lng="pt">&oacute;pio</trd>
  <trd lng="ro">opiu</trd>
  <trd lng="ru">&c_o;&c_p;&c_i;&c_u;&c_m;</trd>
  <trd lng="sk">&oacute;pium</trd>
  <trd lng="sv">opium</trd>
  <trd lng="uk">&#x43E;&#x43F;&#x456;&#x443;&#x43C;</trd>
</drv>
</art>
