<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>boston</rad>/o
</kap>

<drv mrk="boston.0o">
<kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Malrapida <ref tip="lst" cel="vals.0o" lst="voko:dancoj">valso</ref>,
      origina el Ameriko, karakterizata per modera movi&gcirc;o kaj konsekvenca
      akcentado de la unua kvarono de &ccirc;iu takto.  
    </dif>
    <rim>
      Bostono estas anka&ubreve; urbo kaj la&ubreve; &gcirc;i nomata kartludo <ref cel="boston1.0o">bostono</ref>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_s;&c_t;&c_o;&c_n; <klr
  tip="ind">(&c_v;&c_a;&c_l;&c_mol;&c_s;)</klr></trd>,
    <trd>&c_v;&c_a;&c_l;&c_mol;&c_s;-&c_b;&c_a;&c_s;&c_t;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">boston</trd>
  <trd lng="de">Boston<klr tip="amb"> (Tanz)</klr></trd>
  <trd lng="fr">boston <klr tip="ind">(valse)</klr></trd>
  <trd lng="pl">boston <klr> (taniec) ale tak&zdot;e gra w karty i
tkanina</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_mol;&c_s;-&c_b;&c_o;&c_s;&c_t;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">boston</trd>
</drv>
</art>
