<drv mrk="integr2.0ebla">
  <kap><tld/>ebla,
  <var><kap><tld/>hava<fnt><bib>MatVort</bib></fnt></kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="funkci.0o.MAT">funkcio</ref>)</klr>
      Posedanta <ref cel="integr2.0o"><tld/>on</ref>:
      <ekz>
        <ref cel="ekspon1.0o">eksponencialo</ref> estas <tld/>ebla en
        &ccirc;iu finia intervalo, sed ne estas en intervaloj de la tipo
        <frm am="[a,+oo)">[<k>a</k>, +&#x221E;[</frm>;
      </ekz>
      <ekz>
        la karakteriza funkcio de la aro de racionalaj nombroj ne estas
        rimane <tld/>ebla, sed ja lebege.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">integrierbar</trd>
  <trd lng="en">integrable</trd>
  <trd lng="fr">int&eacute;grable</trd>
  <trd lng="hu">integr&aacute;lhat&oacute;</trd>
  <trd lng="nl">integreerbaar</trd>
  <trd lng="pl">ca&lstroke;kowalny</trd>
  <trd lng="pt">integr&aacute;vel<klr tip="amb"> (matem&aacute;tica)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_u;&c_je;&c_m;&c_y;&c_j;</trd>
</drv>

