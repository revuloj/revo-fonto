<drv mrk="integr2.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="integr2.0ado.ago">
    <fnt><bib>MatStokTerm</bib><lok>p. 22</lok></fnt>
    <dif>
      Ago, maniero <ref cel="integr2.0i"><tld/>i</ref>:
      <ekz>
        <ind>la&ubreve;faktora <tld/>ado, poparta <tld/>ado</ind> 
        <klr>(uzante la econ, ke malderiva&jcirc;o de 
        <frm am="fg&minute;"><k>f</k><k>g</k>&minute;</frm> estas 
        <frm am="fg"><k>f</k><k>g</k></frm> minus malderiva&jcirc;o de 
        <frm am="gf&minute;"><k>g</k><k>f</k>&minute;</frm>)</klr>.
        <trd lng="de">Integration nach Teilen</trd>
        <trd lng="en">integration by parts</trd>
        <trd lng="fr">int&eacute;gration par parties</trd>
        <trd lng="hu">parci&aacute;lis <ind>integr&aacute;l&aacute;s</ind></trd>
        <trd lng="pl">ca&lstroke;kowanie przez cz&eogonek;&sacute;ci</trd>
        <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_t;&c_ja;&c_m;</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="integr2.0ado.teorio">
    <ref tip="dif" cel="kalkul.integrala0o"><tld/>a kalkulo.</ref>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>integrace</trd>,
    <trd>integrov&aacute;n&iacute;</trd>,
    <trd>integr&aacute;ln&iacute; po&ccaron;et</trd>
  </trdgrp>
  <trd lng="de">Integration</trd>
  <trd lng="en">integration</trd>
  <trd lng="es">integraci&oacute;n</trd>
  <trd lng="fr">int&eacute;gration</trd>
  <trd lng="hu">integr&aacute;l&aacute;s</trd>
  <trd lng="nl">integratie</trd>
  <trd lng="pl">ca&lstroke;kowanie</trd>
  <trd lng="pt">integra&ccedil;&atilde;o<klr tip="amb"> (matem&aacute;tica)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">integrovanie</trd>
</drv>

