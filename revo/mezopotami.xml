<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Mezopotami</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="mezopotami.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">HIS</uzo>
      <snc>
        <dif>
          <ref tip="malprt" cel="azi.A0o.okcidenta">Okcident-Azia</ref>
          regiono inter la meza kaj malsupra parto de la riveroj
          <ref cel="tigris.0o">Tigriso</ref> kaj
          <ref cel="euxfrat.0o">E&ubreve;frato</ref>, en la moderna
          <ref cel="irak.0o">Irako</ref>:
          <ekz>
            kaj Isaak forsendis Jakobon, kaj tiu iris <tld/>on, al
            Laban la filo de Betuel
            <fnt><bib>MT</bib>, <lok>&Gen; 28:5</lok>.</fnt>;
          </ekz><ekz>
            &ccirc;irka&ubreve; Babilono kaj Ninivo bolas movado, kiun oni sentas en
            la tuta <tld/>o
            <fnt><bib>Far1</bib>, <lok>&Ccirc;ap. 3.</lok></fnt>.
          </ekz>          
        </dif>
      </snc>
      <trd lng="de">Mesopotamien</trd>
      <trd lng="en">Mesopotamia</trd>
      <trd lng="nl">Mesopotami&euml;</trd>
      <trdgrp lng="ru">
        <trd>&c_D;&c_v;&c_u;&c_r;&c_je;&c_ch;&c_mol;&c_je;</trd>,
        <trd>&c_M;&c_je;&c_s;&c_o;&c_p;&c_o;&c_t;&c_a;&c_m;&c_i;&c_ja;</trd></trdgrp>
      <trd lng="tr">Mezopotamya</trd>
    </drv>
  </art>

