<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mezopotami.xml,v 1.24 2025/11/21 18:37:59 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Mezopotami</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mezopotami.M0o">
  <kap><tld/>o</kap>
    <snc mrk="mezopotami.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="malprt" cel="azi.OkcidentaA0o">Okcident-Azia</ref>
      regiono inter la meza kaj malsupra parto de la riveroj
      <ref cel="tigris.T0o">Tigriso</ref> kaj
      <ref cel="euxfrat.E0o">E&ubreve;frato</ref>, en la moderna
      <ref cel="irak.I0o">Irako</ref>:
      <ekz>
        kaj <nom>Isaak</nom> forsendis Jakobon, kaj tiu iris <tld/>on, al
        <nom>Laban</nom> la filo de <nom>Betuel</nom>
        <fnt><bib>MT</bib>, <lok>&Gen; 28:5</lok>.</fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; Babilono kaj Ninivo bolas movado, kiun oni
        sentas en la tuta <tld/>o
        <fnt><bib>Far1</bib>, <lok>&Ccirc;ap. 3.</lok></fnt>;
      </ekz>
      <ekz>
        la Mesio <klr>[rajdis]</klr> sur la antikva azeno, kiun solan konis la antikvaj
        civilizoj <tld lit="m"/>aj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 2 (100)</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabio <klr>[&#x2026;]</klr> estis menciita en antikvaj medicinaj trakta&jcirc;oj de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php"
              >Batalo kontra&ubreve; mortiga malsano</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_M;&c_e;&c_s;&c_a;&c_p;&c_a;&c_t;&c_a;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_M;&c_ib;&c_zh;&c_r;&c_e;&c_ch;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">Mesopot&agrave;mia</trd>
  <trd lng="cs">Mezopot&aacute;mie</trd>
  <trdgrp lng="de">
      <trd>Mesopotamien</trd>,
      <trd>Zweistromland</trd>
  </trdgrp>
  <trd lng="en">Mesopotamia</trd>
  <trd lng="es">Mesopotamia</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&a_A;&a_n;&zwnj;&a_r;&a_w;&a_d;&a_A;&a_n;</trd>,
    <trd>&a_b;&f_y;&a_n;&zwnj;&a_A;&a_l;&a_n;&a_h;&a_r;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">M&eacute;sopotamie</trd>
  <trd lng="hu">Mezopot&aacute;mia</trd>
  <trd lng="ja">&#x30E1;&#x30BD;&#x30DD;&#x30BF;&#x30DF;&#x30A2;</trd>
  <trd lng="nl">Mesopotami&euml;</trd>
  <trd lng="pl">Mezopotamia</trd>
  <trd lng="pt">Mesopot&acirc;mia</trd>
  <trdgrp lng="ru">
    <trd>&c_D;&c_v;&c_u;&c_r;&c_je;&c_ch;&c_mol;&c_je;</trd>,
    <trd>&c_M;&c_je;&c_s;&c_o;&c_p;&c_o;&c_t;&c_a;&c_m;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">Mezopot&aacute;mia</trd>
  <trd lng="tr">Mezopotamya</trd>
  <trdgrp lng="uk">
    <trd>&c_M;&c_je;&c_s;&c_o;&c_p;&c_o;&c_t;&c_a;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_M;&c_je;&c_zh;&c_i;&c_r;&c_ib;&c_ch;&c_ch;&c_ja;</trd>,
    <trd>&c_D;&c_v;&c_o;&c_r;&c_ib;&c_ch;&c_ch;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F8E;&#x7D22;&#x4E0D;&#x8FBE;&#x7C73;&#x4E9A;&#x4EBA; <pr>m&ecaron;isu&#x1D2;b&ugrave;d&aacute;m&#x1D0;y&agrave;r&eacute;n</pr></trd>,
    <trd>&#x7F8E;&#x7D22;&#x4E0D;&#x9054;&#x7C73;&#x4E9E;&#x4EBA; <pr>m&ecaron;isu&#x1D2;b&ugrave;d&aacute;m&#x1D0;y&agrave;r&eacute;n</pr></trd>,
    <trd>&#x4ECA;&#x4F0A;&#x62C9;&#x514B;&#x6240;&#x5728;&#x5730; <pr>j&imacron;ny&imacron;l&amacron;k&egrave;su&#x1D2;z&agrave;id&igrave;</pr></trd>,
    <trd>&#x7F8E;&#x7D22;&#x4E0D;&#x8FBE;&#x7C73;&#x4E9A; <pr>m&ecaron;isu&#x1D2;b&ugrave;d&aacute;m&#x1D0;y&agrave;</pr></trd>,
    <trd>&#x7F8E;&#x7D22;&#x4E0D;&#x9054;&#x7C73;&#x4E9E; <pr>m&ecaron;isu&#x1D2;b&ugrave;d&aacute;m&#x1D0;y&agrave;</pr></trd>
  </trdgrp>
</drv>
</art>
