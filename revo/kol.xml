<drv mrk="kol.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kol.0umi.KOMUNE">
    <dif>
      Kapti je la <tld/>o:
      <ekz>
        brako de malanta&ubreve;e <tld/>umis lin, premante la gor&gcirc;on,
        kaj tiris lin malanta&ubreve;en
        <fnt><aut>J. Francis</aut>
        <vrk>La Granda Kaldrono</vrk>,
        <lok>1978</lok></fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol; &c_z;&c_a; <ind>&c_sh;&c_y;&c_ju;</ind></trd>
  <trd lng="ca">agafar pel coll</trd>
  <trd lng="fr">colleter</trd>
  <trd lng="pl">chwyta&cacute; za <ind>szyj&eogonek;</ind></trd>
  <trd lng="ro">apuca de g&acirc;t</trd>
</drv>

<drv mrk="kol.0umo">
  <kap><ofc>*</ofc><tld/>umo</kap>
  <snc mrk="kol.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="vest.0o">vesto</ref>
      &ccirc;irka&ubreve;anta la <tld/>on:
      <ekz>
        altran&ccirc;i <tld/>umon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kuspita <tld/>umo
        <fnt>
          <bib>Kiso</bib>
          <lok>La Sakramento De La Komunio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lana &ccirc;emizo estis superkovrita per refaldita blanktola <tld/>umo
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu hajlero post la alia faladis malsupren de la <tld/>umo de lia jako
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la predikistoj <klr>[&#8230;]</klr> kun la rigidaj <tld/>umoj kaj longaj nigraj roboj
        fiksis siajn okulojn sur &scirc;iaj ru&gcirc;aj &scirc;uoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigraj bukloj volvi&gcirc;is sur <klr>[&#8230;]</klr> la <tld/>umo de la korsa&jcirc;o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        senripro&ccirc;a jako kun plu&scirc;a <tld/>umo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        varma pelta <tld/>umo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sukera&jcirc;isto kaptis min per la <tld/>umo pro la kuketoj, kiujn mi forman&gcirc;is
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_w;&c_n;&c_je;&c_r;</trd>
  <trd lng="bg">&c_ja;&c_k;&c_a;</trd>
  <trd lng="ca">coll <klr tip="amb">(d&#8217;un vestit)</klr></trd>
  <trd lng="cs">l&iacute;mec</trd>
  <trd lng="de">Kragen</trd>
  <trd lng="en">collar</trd>
  <trd lng="es">cuello <klr>de prenda de vestir</klr></trd>
  <trd lng="fr">col<klr tip="ind"> (d'un v&ecirc;tement)</klr></trd>
  <trd lng="he">&tsadi;&vav;&vav;&alef;&resh;&vav;&fnun;</trd>
  <trd lng="hu">gall&eacute;r</trd>
  <trd lng="id">kerah</trd>
  <trdgrp lng="ja">
    <trd>&#12459;&#12521;&#12540;</trd>,
    <trd>&#12360;&#12426;</trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>boord<klr tip="ind"> (kraag)</klr></trd>,
        <trd>kraag</trd>
      </trdgrp>
  <trd lng="pl">ko&lstroke;nierz</trd>
  <trdgrp lng="pt">
    <trd>colarinho</trd>,
    <trd>gola</trd>
  </trdgrp>
  <trd lng="ro">guler </trd>
  <trdgrp lng="ru">
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
  <trd lng="sk">golier</trd>
  <trd lng="sv">krage</trd>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_ib;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#34915;&#38936; <pr>y&#299;l&#464;ng</pr></trd>,
    <trd>&#38936; <pr>l&#464;ng</pr></trd>,
    <trd>&#38936;&#23376; <pr>l&#464;ngzi</pr></trd>,
    <trd>&#34943; <pr>j&#299;n</pr></trd>,
    <trd>&#33046;&#38936; <pr>b&oacute;l&#464;ng</pr></trd>,
    <trd>&#36645;&#29872; <pr>g&#468;nhu&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kol.blank0umulo">
  <kap>blank<tld/>umulo</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="kol.blank0umulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Oficisto &mdash; tiel nomata alude al la blankkola
      &ccirc;emizo, kian en iuj landoj tradicie portis la oficistoj
      sub
      oficeja kompleto:
      <ekz>
        la blank<tld/>umulo ela&ubreve;ti&gcirc;is &ccirc;e la pordo
        de granda kompanio
        <fnt>
          <aut>Xiong Yanling, trad. Helo</aut>
          <vrk>Ekzamena Intervidi&gcirc;o</vrk>
          <lok><url ref="http://www.elerno.cn/penseo/PEN185.pdf">
              Penseo, 2007-03 (185)</url></lok>
        </fnt>;
      </ekz><ekz>
        la ideologio de sinreguligo de merkatoj sendube
        flankenpu&scirc;is la batalon kontra&ubreve; krimado fare de
        blank<tld/>umuloj
        <fnt>
          <aut>I. Warde, trad. M. Leon</aut>,
          <vrk><url ref="&Mondediplo;article1558.html">
              Madoff, sub la nazoj...</url></vrk>,
          <lok>le Monde diplomatique, 2009-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- jam signita per EVI
  <rim mrk="kol.blank0umulo.RIM_EVI">
    Iuj rigardas tiun idioma&jcirc;on malklara, malmoderna kaj
    evitinda.
  </rim>
-->
<!-- ne traduku evitindajn
  <trdgrp lng="ca">
    <trd>oficinista</trd>,
    <trd>empleat d'oficina</trd>
  </trdgrp>
  <trd lng="de">Wei&szlig;kragen <klr>(B&uuml;roangestellter)</klr></trd>
  <trd lng="en">white-collared worker</trd>
  <trd lng="fr">col-blanc</trd>
  <trd lng="id"><baz>kerja</baz>pekerja kerah putih</trd>
  <trd lng="pl">bia&lstroke;y <ind>ko&lstroke;nierzyk</ind><klr> (potocznie o
  urz&eogonek;dniku)</klr></trd>
  <trd lng="ro">guler alb</trd>
-->
  <trd lng="zh">&#30333;&#38936; <pr>b&aacute;il&#464;ng</pr></trd>
</drv>

