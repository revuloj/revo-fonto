<drv mrk="kol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kol.0o.korpo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la korpo kuniganta la kapon kun la trunko:
      <ekz>
        belulo <klr>[&#x2026;]</klr> eletendis sian gracian kolon
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Dua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vinicius</nom> devis sin deteni,
        por ne <ind>&jcirc;eti sin sur la <tld/>on</ind> de <nom>Petronius</nom>
        <klr>(por montri sian amon)</klr>
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLI</lok>
        </fnt>;
        <trd lng="de">sich <klr>(jmd.)</klr> an den Hals werfen</trd>
        <trd lng="fr">se jeter au <ind>cou</ind> de qn</trd>
        <trd lng="hu">nyak&aacute;ba ugrik</trd>
        <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;
        <ind>&c_n;&c_a; &c_sh;&c_je;&c_ju;</ind>
        &c_k;&c_o;&c_m;&c_u;-&c_l;.</trd>
        <trd lng="sv">kasta sig om <ind>halsen</ind> p&aring;
        n&aring;gon</trd>
      </ekz>
      <ekz>
        mi metis braceletojn sur viajn manojn kaj <tld/>&ccirc;enon
        sur vian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun &scirc;nuro &ccirc;irka&ubreve; la <tld/>o mar&scirc;i
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        junuloj <klr>[&#x2026;]</klr> pretaj <ind>riski sian <tld/>on</ind>
        <klr>(vivon)</klr>
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>Morto de artisto</vrk>
          <lok>p. 226</lok>
        </fnt>;
        <trd lng="de">seinen <ind>Hals</ind> riskieren</trd>
        <trd lng="fr">manquer de se casser le <ind>cou</ind></trd>
        <trd lng="hu">fej&eacute;t kock&aacute;ztatja</trd>
        <trd lng="ru">&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;
        &c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>rompa </ind><klr>(tre laciga)</klr> laboro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
        <trd lng="de">halsbrecherisch</trd>
      </ekz>
      <ekz>
        <ind>mizero piedojn sanigas, <tld/>on elastigas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">megtan&iacute;t a nyomor az
        al&aacute;zatoss&aacute;gra</trd>
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="gorgx.0o">gor&gcirc;o</ref>,
      <ref cel="nuk.0o">nuko</ref>.
    </refgrp>
    <trd lng="be">&c_sh;&c_y;&c_ja;</trd>
    <trd lng="fr">cou</trd>
    <trdgrp lng="nl">
      <trd>hals</trd>,
      <trd>nek</trd>
    </trdgrp>
    <trd lng="de">Hals</trd>
    <trd lng="pl">szyja</trd>
    <trd lng="pt">pesco&ccedil;o</trd>
    <trd lng="ru">&c_sh;&c_je;&c_ja;</trd>
    <trd lng="tr">boyun</trd>
  </snc>
  <snc mrk="kol.0o.objekto">
    <dif>
      Longa mallar&gcirc;a <tld/>forma parto de objekto:
      <ekz>
        <ind><tld/>o de botelo</ind>
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
        <trd lng="ru">&c_g;&c_o;&c_r;&c_l;&c_y;&c_sh;&c_k;&c_o;
          <klr>(&c_b;&c_u;&c_t;&c_y;&c_l;&c_k;&c_i;)</klr></trd>
      </ekz>
      <ekz>
        la mallar&gcirc;a <tld/>o de vazo
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz><ekz>
        <ind><tld/>o de la femurosto</ind>;
        <trd lng="ru">&c_sh;&c_je;&c_j;&c_k;&c_a;
           <klr>(&c_b;&c_je;&c_d;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
      </ekz><ekz>
        <ref cel="kol.mar0o">mar<tld/>o</ref>,
        <ref cel="kol.ter0o">ter<tld/>o</ref>,
        <ref cel="kol.mont0o">mont<tld/>o</ref>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_y;&c_j;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_y;&c_l;&c_mol;&c_c;&c_a;
      <klr>(&c_b;&c_u;&c_t;&c_e;&c_l;&c_mol;&c_k;&c_ib;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Hals</trd>,
      <trd>Enge</trd>,
      <trd>Engpass</trd>,
      <trd>Engstelle</trd>
    </trdgrp>
    <trd lng="fr">col<klr tip="ind"> (d'un objet)</klr></trd>
    <trd lng="nl">hals</trd>
    <trd lng="pl">szyjka</trd>
  </snc>
  <snc mrk="kol.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <fnt><bib>RokPop</bib></fnt>
    <dif>
      <tld lit="K"/>simila parto de korda muzikilo, la loko de la
      fingrotabulo.
    </dif>
    <trd lng="be">&c_g;&c_r;&c_y;&c_f; <klr>(&c_m;&c_u;&c_z;.
    &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;)</klr></trd>
    <trd lng="ru">&c_g;&c_r;&c_i;&c_f;<klr> (&c_m;&c_u;&c_z;.)</klr></trd>
  </snc>
  <trd lng="af">nek</trd>
  <trd lng="am">&#x12A0;&#x1295;&#x1308;&#x1275;</trd>
  <trd lng="ar">&a_A;&a_l;&a_r;&a_q;&a_b;&a_t_marbuta;</trd>
