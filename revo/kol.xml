  <trd lng="ro">trec&abreve;toare</trd>
</drv>

<drv mrk="kol.ter0o">
  <kap><ofc>*</ofc>ter<tld/>o</kap>
  <snc mrk="kol.ter0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Mallar&gcirc;a strio da tero inter du akvujoj (maroj, lagoj
      ktp), kuniganta du terpartojn;
      <ref tip="sin" cel="istm.0o">istmo</ref>:
      <ekz>
        <ctl>Postkorto de Usono</ctl>, la centramerika ter<tld/>o
        &jcirc;us flami&gcirc;is
        <fnt>
          <vrk><url ref="http://eo.mondediplo.com/article1392.html">&leftquot;Le Monde diplomatique&rightquot; en
              Esperanto</url>
          </vrk>
        </fnt>
      </ekz><ekz>
        <nom>Pasul Ghime&scedil;-Palanca</nom> (Terkolo de Djime&scirc;o,
        hungare: <nac>Gyimesi-szoros</nac>) estas ter<tld/>o en Rumanio
        en Orientaj
        Karpatoj, kiu el Baseno de &Ccirc;iko kondukas tra la valo
        Tatro&scirc;o al Moldavio je alteco de 1006 m super
        marnivelo
        <fnt><vrk><url ref="http://eo.pandapedia.com/wiki/Terkolo_de_Djime%C5%9Do">Terkolo de Djime&scirc;o</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_y;&c_je;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_v;&c_l;&c_a;&c_k;</trd>
  <trd lng="ca">istme</trd>
  <trdgrp lng="de">
    <trd>Isthmus</trd>,
    <trd>Landenge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>isthmus</trd>,
    <trd>neck<klr> (of land)</klr></trd>
  </trdgrp>
  <trd lng="es">istmo</trd>
  <trd lng="fr">isthme</trd>
  <trd lng="hu">f&ouml;ldszoros</trd>
  <trd lng="nl">landengte</trd>
  <trd lng="pl">przesmyk</trd>
  <trd lng="ro">istm</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
  <trd lng="sk">zemsk&aacute; &uacute;&zcaron;ina</trd>
  <trd lng="cs">zemsk&aacute; &scaron;&iacute;je</trd>
