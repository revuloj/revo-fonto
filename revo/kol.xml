</drv>

<drv mrk="kol.gxis0e">
  <kap>&gcirc;is<tld/>e</kap>
  <snc mrk="kol.gxis0e.rektasence">
    <dif>
      &Gcirc;is la <tld/>o:
      <ekz>
        en vesta&jcirc;o, butonumita &gcirc;is<tld/>e
	    <fnt>
	      <aut>Jen&odblac; Rejt&odblac;, trad. L&aacute;szl&oacute;
	      Bal&aacute;zs</aut>:
	      <lok><url ref="http://www.freeweb.hu/verkoj/06.htm">La nevidebla legio</url></lok>
	    </fnt>;
      </ekz><ekz>
        ili enterigis &gcirc;is<tld/>e mian onklinon en Afganio kaj
        ili mortigis &scirc;in per &scirc;tonoj
        <fnt>
          <lok><url ref="http://zh-tw.lernu.net/komunikado/forumo/temo.php?t=1960&amp;p=25">Lernu!</url></lok>
        </fnt>;
      </ekz><ekz>
        jen estis liaj du preferataj katidoj, tremantaj, malgajaj,
        &gcirc;is<tld/>e en &scirc;a&ubreve;mo de sapeca akvo
        <fnt>
          <aut>T. Turner, trad. R. McGrath</aut>:
          <lok><url ref="http://i-espero.info/files/elibroj/eo%20-%20turner,%20ethel%20-%20sep%20junaj%20australianoj.pdf">Sep
          junaj a&ubreve;stralianoj</url></lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="en">up to one's neck<klr> (in water etc)</klr></trd>
    <trd lng="es">hasta el cuello</trd>
  <trd lng="id"><baz>leher</baz>seleher <klr>(ttg ketinggian air dsb.)</klr></trd>
  </snc>
  <snc mrk="kol.gxis0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plene, abunde, tro multe:
      <ekz>
        <ind>&gcirc;is<tld/>e mi satas</ind> pri viaj ruza&jcirc;oj
        <trd lng="es">estoy harto</trd>
        <trd lng="ru">&c_s;&c_y;&c_t; &c_p;&c_o;
        &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
        <fnt>
          <aut>K. &Ccaron;apek, trad. J. Vondrou&scaron;ek</aut>:
          <vrk><url ref="http://www.literatura.bucek.name/capekk/capek-karel_afero%20makropulos.doc">Afero
          Makropulos</url></vrk>
        </fnt>;
      </ekz><ekz>
        <ind>&gcirc;is<tld/>e armita</ind>.
        <trd lng="es">armado hasta el cuello</trd>
        <trd lng="nl">tot de <ind>tand</ind>en gewapend</trd>
        <trd lng="ru">&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_jo;&c_n;
        &c_d;&c_o; &c_z;&c_u;&c_b;&c_o;&c_v;</trd>
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>more than enough</trd>,
      <trd>enough and to spare</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>hasta arriba</trd>,
      <trd>m&aacute;s que suficiente</trd>,
      <trd>hasta el mo&ntilde;o</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7DBD;&#x7DBD;&#x6709;&#x9918; <pr>chu&#xF2;chu&#xF2;y&#x1D2;uy&#xFA;</pr></trd>,
    <trd>&#x5BCC;&#x5BCC;&#x6709;&#x4F59; <pr>f&#xF9;f&#xF9;y&#x1D2;uy&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a; <ind>&c_g;&c_o;&c_r;&c_l;&c_a;</ind></trd>,
    <trd>&c_d;&c_a; <ind>&c_z;&c_u;&c_b;&c_o;&c_w;</ind></trd>
  </trdgrp>
  <trd lng="pl">po <ind>szyj&eogonek;</ind><klr> (przeno&sacute;nie)</klr></trd>
  <trdgrp lng="ro">
    <trd>p&acirc;n&abreve;-n pl&abreve;sele</trd>,
    <trd>p&acirc;n&abreve; la g&acirc;t</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
</drv>

