<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kol</rad>/o
</kap>
<drv mrk="kol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kol.0o.korpo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la korpo kuniganta la kapon kun la trunko:
      <ekz>
        havi gracian, &gcirc;irafan (tro longan) <tld/>on;
      </ekz>
      <ekz>
        &jcirc;eti sin sur la <tld/>on de iu<fnt>Z</fnt>
        <klr>(por montri sian amon)</klr>;
      </ekz>
      <ekz>
        mi metis braceletojn sur viajn manojn kaj <tld/>&ccirc;enon
        sur vian <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>mizero piedojn sanigas, <tld/>on elastigas<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        riski sian <tld/>on<fnt>Z</fnt>
        <klr>(vivon)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>rompa <klr>(tre laciga)</klr> laboro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="gorgx.0o">gor&gcirc;o</ref>,
      <ref cel="nuk.0o">nuko</ref>.
    </refgrp>
    <trd lng="fr">cou</trd>
    <trdgrp lng="nl">
      <trd>hals</trd>,
      <trd>nek</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_je;&c_ja;</trd>
  </snc>
  <snc mrk="kol.0o.objekto">
    <dif>
      Longa mallar&gcirc;a <tld/>forma parto de objekto:
      <ekz>
        <tld/>o de botelo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        retorto, veziko;
      </ekz>
      <ekz>
        <tld/>o de la femurosto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mar<tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ter<tld/>o <klr>(istmo)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mont<tld/>o <klr>(monta trapasejo)</klr>.
      </ekz>
    </dif>
    <trd lng="fr">col</trd>
    <trd lng="nl">hals</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_r;&c_l;&c_y;&c_sh;&c_k;&c_o; <klr>(&c_b;&c_u;&c_t;&c_y;&c_l;&c_k;&c_i;)</klr></trd>,
      <trd>&c_sh;&c_je;&c_j;&c_k;&c_a; <klr>(&c_b;&c_je;&c_d;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">krk</trd>
  <trd lng="en">neck</trd>
</drv>

<drv mrk="kol.0umo">
  <kap><tld/>umo<fnt>Z</fnt></kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Parto de <ref tip="malprt" cel="vest.0o">vesto</ref> &ccirc;irka&ubreve;anta la
      <tld/>on:
      <ekz>
        <tld/>umo de &ccirc;emizo, surtuto;
      </ekz>
      <ekz>
        tola, punta <tld/>umo.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">collar</trd>
  <trd lng="fr">col</trd>
  <trdgrp lng="nl">
    <trd>boord<klr tip="ind"> (kraag)</klr></trd>,
    <trd>kraag</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
