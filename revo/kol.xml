<drv mrk="kol.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kol.0umi.KOMUNE">
    <dif>
      Kapti je la <tld/>o:
      <ekz>
        brako de malanta&ubreve;e <tld/>umis lin, premante la gor&gcirc;on,
        kaj tiris lin malanta&ubreve;en
        <fnt><aut>J. Francis</aut>
        <vrk>La Granda Kaldrono</vrk>,
        <lok>1978</lok></fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol; &c_z;&c_a; <ind>&c_sh;&c_y;&c_ju;</ind></trd>
  <trd lng="fr">colleter</trd>
  <trd lng="pl">chwyta&cacute; za <ind>szyj&eogonek;</ind></trd>     
  <trd lng="ro">apuca de g&acirc;t</trd>
</drv>

<drv mrk="kol.0umo">
  <kap><ofc>*</ofc><tld/>umo<fnt>Z</fnt></kap>
  <snc mrk="kol.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="vest.0o">vesto</ref>
      &ccirc;irka&ubreve;anta la
      <tld/>on:
      <ekz>
        <tld/>umo de &ccirc;emizo, surtuto;
      </ekz><ekz>
        tola, punta <tld/>umo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_w;&c_n;&c_je;&c_r;</trd>
  <trd lng="bg">&c_ja;&c_k;&c_a;</trd>
  <trd lng="cs">l&iacute;mec</trd>
  <trd lng="de">Kragen</trd>
  <trd lng="en">collar</trd>
  <trd lng="es">cuello <klr>de prenda de vestir</klr></trd>
  <trd lng="fr">col<klr tip="ind"> (d'un v&ecirc;tement)</klr></trd>
  <trd lng="he">&tsadi;&vav;&vav;&alef;&resh;&vav;&fnun;</trd>
  <trd lng="hu">gall&eacute;r</trd>
  <trd lng="id">kerah</trd>
  <trdgrp lng="nl">
        <trd>boord<klr tip="ind"> (kraag)</klr></trd>,
        <trd>kraag</trd>
      </trdgrp>
  <trd lng="pl">ko&lstroke;nierz</trd>
  <trdgrp lng="pt">
    <trd>colarinho</trd>,
    <trd>gola</trd>
  </trdgrp>
  <trd lng="ro">guler </trd>
  <trdgrp lng="ru">
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
  <trd lng="sk">golier</trd>
  <trd lng="sv">krage</trd>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_ib;&c_r;</trd>
</drv>

<drv mrk="kol.blank0umulo">
  <kap>blank<tld/>umulo,
  <var><kap>blanka<tld/>umulo</kap></var></kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="kol.blank0umulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Oficisto &mdash; tiel nomata alude al la blankkola
      &ccirc;emizo, kian en iuj landoj tradicie portis la oficistoj
      sub
      oficeja kompleto:
      <ekz>
        la blank<tld/>umulo ela&ubreve;ti&gcirc;is &ccirc;e la pordo
        de granda kompanio
        <fnt>
          <aut>Xiong Yanling, trad. Helo</aut>
          <vrk>Ekzamena Intervidi&gcirc;o</vrk>
          <lok><url ref="http://www.elerno.cn/penseo/PEN185.pdf">
              Penseo, 2007-03 (185)</url></lok>
        </fnt>;
      </ekz><ekz>
        la ideologio de sinreguligo de merkatoj sendube
        flankenpu&scirc;is la batalon kontra&ubreve; krimado fare de
        blank<tld/>umuloj 
        <fnt>
          <aut>I. Warde, trad. M. Leon</aut>,
          <vrk><url ref="&Mondediplo;article1558.html">
              Madoff, sub la nazoj...</url></vrk>, 
          <lok>le Monde diplomatique, 2009-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kol.blank0umulo.RIM_EVI">
    Iuj rigardas tiun idioma&jcirc;on malklara, malmoderna kaj
    evitinda.
  </rim>
  <trd lng="en">white-collared worker</trd>
  <trd lng="fr">col-blanc</trd>
  <trd lng="pl">bia&lstroke;y <ind>ko&lstroke;nierzyk</ind><klr> (potocznie o
  urz&eogonek;dniku)</klr></trd>    
  <trd lng="ro">guler alb</trd>
</drv>

<drv mrk="kol.blu0umulo">
  <kap>blu<tld/>umulo, <var><kap>blua<tld/>umulo</kap></var></kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="kol.blu0umulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Fabrika laboristo &mdash; tiel nomata alude al la tradiciaj
      bluaj 
      laborvestoj en fabrikoj kaj kontraste al
      <ref tip="vid" cel="kol.blank0umulo.POL">blankkolumulo</ref>:
      <ekz>
        la&ubreve; la analizo pri la bezono de laboruloj de diversaj
        fakoj
        en
        2002,
        laboruloj de manufakturado, precipe kapablaj fakuloj
        (alinome 
        altklasaj blu<tld/>umuloj), estas tre bonvenaj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.espero.com.cn/old/epch/09e/09e-3f/03-09e-3-4.htm">Laboruloj de 5
          fakoj...</url></vrk>,
          <lok>[2003?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kol.blu0umulo.RIM_EVI">
    Iuj rigardas tiun nomon malklara kaj evitinda.
  </rim>
  <trd lng="en">blue-collared worker</trd>
  <trd lng="fr">col-bleu</trd>
  <trd lng="pl">niebieski <ind>ko&lstroke;nierzyk</ind><klr> (potocznie o
  robotniku)</klr></trd> 
  <trd lng="ro">guler albastru</trd>
</drv>

<drv mrk="kol.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <snc mrk="kol.cxirkaux0o.ligilo">
    <dif>
      Ligilo metata &ccirc;irka&ubreve; la <tld/>on de besto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_sh;&c_y;&c_j;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_a;&c_b;&c_r;&c_o;&c_zh;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="bg">&c_n;&c_a;&c_sh;&c_i;&c_j;&c_n;&c_i;&c_k;</trd>
    <trdgrp lng="fr">
      <trd>licou</trd>,
      <trd>licol</trd>,
      <trd>collier<klr tip="ind"> (d'animal)</klr></trd>
    </trdgrp>
    <trd lng="hu">nyak&ouml;rv</trd>
    <trd lng="he">&qof;&vav;&lamed;&resh;</trd>
    <trd lng="nl">halsband</trd>
    <trdgrp lng="pl">
      <trd>obro&zdot;a</trd>,
      <trd>chom&aogonek;to</trd>
    </trdgrp>   
    <trd lng="pt">coleira</trd>
    <trd lng="ro">zgard&abreve;</trd>
    <trd lng="ru">&c_o;&c_sh;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="kol.cxirkaux0o.ornamajxo">
    <dif>
      Ornama&jcirc;o portata &ccirc;irka&ubreve; la <tld/>o:
      <ekz>
        &scirc;i incensis <klr>[al la dioj]</klr>, ornaminte sin per
        siaj ringoj kaj siaj &ccirc;irka&ubreve;koloj
        <fnt><bib>MT</bib>, <lok>&Hos; 2:13</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxen.0o">&ccirc;eno</ref>,
      <ref cel="ring.0o">ringo</ref>.
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;&c_ib;</trd>
    <trd lng="bg">&c_o;&c_g;&c_malmol;&c_r;&c_l;&c_i;&c_c;&c_a;</trd>
    <trd lng="fr">collier<klr tip="ind"> (ornement)</klr></trd>
    <trd lng="he">&ayin;&nun;&qof;<klr tip="amb">
    (&tav;&kaf;&shin;&yod;&tet;
        &lamed;&tsadi;&vav;&vav;&alef;&resh;)</klr></trd>
    <trd lng="hu">nyak&eacute;k</trd>
    <trd lng="nl">halsketting</trd>
    <trd lng="pl">naszyjnik</trd>
    <trd lng="ru">&c_o;&c_zh;&c_je;&c_r;&c_je;&c_l;&c_mol;&c_je;</trd>
  </snc>
  <trdgrp lng="en">
    <trd>collar</trd>,
    <trd>necklace</trd>
  </trdgrp>
  <trd lng="es">collar</trd>
  <trd lng="de">Halsband</trd>
  <trd lng="sv">halsband</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_m;&c_i;&c_s;&c_t;&c_o;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_a;&c_l;&c_ib;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_jeu;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_i;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="kol.gxis0e">
  <kap>&gcirc;is<tld/>e</kap>
  <snc mrk="kol.gxis0e.rektasence">
    <dif>
      &Gcirc;is la <tld/>o:
      <ekz>
        en vesta&jcirc;o, butonumita &gcirc;is<tld/>e
	    <fnt>
	      <aut>Jen&odblac; Rejt&odblac;, trad. L&aacute;szl&oacute;
	      Bal&aacute;zs</aut>:
	      <lok><url ref="http://www.freeweb.hu/verkoj/06.htm">La nevidebla legio</url></lok>
	    </fnt>;
      </ekz><ekz>
        ili enterigis &gcirc;is<tld/>e mian onklinon en Afganio kaj
        ili mortigis &scirc;in per &scirc;tonoj
        <fnt>
          <lok><url
          ref="http://zh-tw.lernu.net/komunikado/forumo/temo.php?t=1960&amp;p=25">Lernu!</url></lok>
        </fnt>;
      </ekz><ekz>
        jen estis liaj du preferataj katidoj, tremantaj, malgajaj,
        &gcirc;is<tld/>e en &scirc;a&ubreve;mo de sapeca akvo
        <fnt>
          <aut>T. Turner, trad. R. McGrath</aut>:
          <lok><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20turner,%20ethel%20-%20sep%20junaj%20australianoj.pdf">Sep
          junaj a&ubreve;stralianoj</url></lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="en">up to one's neck<klr> (in water etc)</klr></trd>
    <trd lng="es">hasta el cuello</trd>
  </snc>
  <snc mrk="kol.gxis0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plene, abunde, tro multe:
      <ekz>
        <ind>&gcirc;is<tld/>e mi satas</ind> pri viaj ruza&jcirc;oj
        <trd lng="es">estoy harto</trd>
        <trd lng="ru">&c_s;&c_y;&c_t; &c_p;&c_o;
        &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
        <fnt>
          <aut>K. &Ccaron;apek, trad. J. Vondrou&scaron;ek</aut>:
          <vrk><url
          ref="http://www.literatura.bucek.name/capekk/capek-karel_afero%20makropulos.doc">Afero
          Makropulos</url></vrk>
        </fnt>;
      </ekz><ekz>
        <ind>&gcirc;is<tld/>e armita</ind>.
        <trd lng="es">armado hasta el cuello</trd>
        <trd lng="nl">tot de <ind>tand</ind>en gewapend</trd>
        <trd lng="ru">&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_jo;&c_n;
        &c_d;&c_o; &c_z;&c_u;&c_b;&c_o;&c_v;</trd>
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>more than enough</trd>,
      <trd>enough and to spare</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>hasta arriba</trd>,
      <trd>m&aacute;s que suficiente</trd>,
      <trd>hasta el mo&ntilde;o</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a; <ind>&c_g;&c_o;&c_r;&c_l;&c_a;</ind></trd>,
    <trd>&c_d;&c_a; <ind>&c_z;&c_u;&c_b;&c_o;&c_w;</ind></trd>
  </trdgrp>
  <trd lng="pl">po <ind>szyj&eogonek;</ind><klr> (przeno&sacute;nie)</klr></trd>  
  <trdgrp lng="ro">
    <trd>p&acirc;n&abreve;-n pl&abreve;sele</trd>,
    <trd>p&acirc;n&abreve; la g&acirc;t</trd>
  </trdgrp> 
  <trd lng="ru">&c_p;&c_o; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
</drv>

<drv mrk="kol.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="kol.mar0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de maro a&ubreve; oceano, akva strio inter pecoj da seka
      tero, komunikiganta najbarajn marojn kaj oceanojn.
    </dif>
  </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_r;&c_a;&c_l;&c_ib;&c_w;</trd>,
        <trd>&c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
      </trdgrp>
  <trdgrp lng="bg">
        <trd>&c_p;&c_r;&c_o;&c_t;&c_o;&c_k;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>
      </trdgrp>
      <trdgrp lng="cs">
    <trd>mo&rcaron;sk&aacute; &uacute;&zcaron;ina</trd>,
    <trd>mo&rcaron;sk&aacute; &scaron;&iacute;je</trd>,
    <trd>mo&rcaron;sk&yacute; pr&uring;liv</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Meerenge</trd>,
        <trd>Meeresstra&szlig;e</trd>,
        <trd>Sund</trd>
      </trdgrp>
  <trdgrp lng="en">
        <trd>strait</trd>
      </trdgrp>
  <trd lng="es">estrecho <klr>(geograf&iacute;a)</klr></trd>
  <trd lng="fr">d&eacute;troit<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trd lng="hu">tengerszoros</trd>
  <trdgrp lng="nl">
        <trd>zeestraat</trd>,
        <trd>straat<klr tip="ind"> (zeestraat)</klr></trd>
      </trdgrp>
  <trd lng="pl">cie&sacute;nina</trd>
  <trd lng="pt">estreito</trd>
  <trd lng="ro">str&acirc;mtori</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>morsk&aacute; &uacute;&zcaron;ina</trd>,
    <trd>morsk&yacute; prieplav</trd>
  </trdgrp>
  <trd lng="sv">sund<klr tip="ind"> (geog.)</klr></trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_t;&c_o;&c_k;&c_a;</trd>
</drv>

<drv mrk="kol.mont0o">
  <kap>mont<tld/>o,
  <var><kap>monto<tld/>o</kap></var></kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kol.mont0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="mont.inter0o">Intermonto</ref>:
      <ekz>
        <nom>Asta Sunlilio</nom> iris apud lia flanko survoje okcidenten
        trans la monto<tld/>on
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_l;</trd>
  <trd lng="fr">col<klr tip="ind"> (de montagne)</klr></trd>
  <trd lng="hu">h&aacute;g&oacute;</trd>
  <trdgrp lng="nl">
    <trd>col<klr tip="ind"> (bergpas)</klr></trd>,
    <trd>bergpas</trd>
  </trdgrp>
  <trd lng="pl">prze&lstroke;&eogonek;cz</trd>
  <trd lng="ro">trec&abreve;toare</trd>
</drv>

<drv mrk="kol.ter0o">
  <kap><ofc>*</ofc>ter<tld/>o</kap>
  <snc mrk="kol.ter0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Mallar&gcirc;a strio da tero inter du akvujoj (maroj, lagoj
      ktp), kuniganta du terpartojn;
      <ref tip="sin" cel="istm.0o">istmo</ref>:
      <ekz>
        <ctl>Postkorto de Usono</ctl>, la centramerika ter<tld/>o
        &jcirc;us flami&gcirc;is
        <fnt>
          <vrk><url ref="&Mondediplo;article1392.html">&leftquot;Le Monde
          diplomatique&rightquot; en
              Esperanto</url>
          </vrk>
        </fnt>
      </ekz><ekz>
        <nom>Pasul Ghime&scedil;-Palanca</nom> (Terkolo de Djime&scirc;o,
        hungare: <nac>Gyimesi-szoros</nac>) estas ter<tld/>o en Rumanio
        en Orientaj
        Karpatoj, kiu el Baseno de &Ccirc;iko kondukas tra la valo
        Tatro&scirc;o al Moldavio je alteco de 1006 m super
        marnivelo
        <fnt><vrk><url ref="http://eo.pandapedia.com/wiki/Terkolo_de_Djime%C5%9Do">Terkolo de
        Djime&scirc;o</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_y;&c_je;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_v;&c_l;&c_a;&c_k;</trd>
  <trd lng="ca">istme</trd>
  <trdgrp lng="de">
    <trd>Isthmus</trd>,
    <trd>Landenge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>isthmus</trd>,
    <trd>neck<klr> (of land)</klr></trd>
  </trdgrp>
  <trd lng="es">istmo</trd>
  <trd lng="fr">isthme</trd>
  <trd lng="hu">f&ouml;ldszoros</trd>
  <trd lng="nl">landengte</trd>
  <trd lng="pl">przesmyk</trd>
  <trd lng="ro">istm</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
  <trd lng="sk">zemsk&aacute; &uacute;&zcaron;ina</trd>
  <trd lng="cs">zemsk&aacute; &scaron;&iacute;je</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_i;&c_j;&c_o;&c_k;</trd>
</drv>

  <drv mrk="kol.BeringaMar0o">
    <kap>Beringa Mar<tld/>o</kap>
    <snc mrk="kol.BeringaMar0o.GEOG">
      <uzo tip="fak">GEOG</uzo>
      <dif>
        Mar<tld/>o disiganta <ref cel="azi.A0o">Azion</ref> disde
        <ref cel="amerik.A0o">Ameriko</ref>,
        kaj liganta la Arktan Oceanon kun Pacifiko (169&deg;W,
        65&deg;N).
      </dif>
      <refgrp tip="vid">
        Mapon &ccirc;e la artikolo pri
        <ref cel="bering.B0o">Beringo</ref>
      </refgrp>
    </snc>
    <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;&c_a;&c_v;&c_a;
    &c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
    <trd lng="bg">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;&c_o;&c_v; &c_p;&c_r;&c_o;&c_t;&c_o;&c_k;</trd>
    <trd lng="de">Beringstra&szlig;e</trd>
    <trd lng="en">Bering Strait</trd>
    <trd lng="es">Estrecho de Bering</trd>
    <trd lng="fr">D&eacute;troit de Bering</trd>
    <trd lng="hu">Bering-szoros</trd>
    <trd lng="nl">Beringstraat</trd>
    <trd lng="pl">Cie&sacute;nina Beringa</trd>     
    <trd lng="ro">Str&acirc;mtoarea Bering</trd>
    <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;&c_o;&c_v;
    &c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>
  </drv>
</art>
