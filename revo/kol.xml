  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kol.mont0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="mont.inter0o">Intermonto</ref>:
      <ekz>
        <nom>Asta Sunlilio</nom> iris apud lia flanko survoje okcidenten
        trans la monto<tld/>on
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">col<klr tip="ind"> (de montagne)</klr></trd>
  <trd lng="hu">h&aacute;g&oacute;</trd>
  <trdgrp lng="nl">
