    <drv mrk="kol.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="kol.0o.korpo">
        <uzo tip="fak">ANA</uzo>
        <dif>
          Parto de la korpo kuniganta la kapon kun la trunko:
          <ekz>
            havi gracian, &gcirc;irafan <klr>(tro longan)</klr>
            <tld/>on;
          </ekz><ekz>
            <ind>&jcirc;eti sin sur la <tld/>on de iu</ind><fnt>Z</fnt>
            <klr>(por montri sian amon)</klr>;
            <trd lng="fr">se jeter au <ind>cou</ind> de qn</trd>
            <trd lng="hu">nyak&aacute;ba ugrik</trd>
            <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_n;&c_a; &c_sh;&c_je;&c_ju;</ind> &c_k;&c_o;&c_m;&c_u;-&c_l;.</trd>
            <trd lng="sv">kasta sig om <ind>halsen</ind> p&aring; n&aring;gon</trd>
          </ekz><ekz>
            mi metis braceletojn sur viajn manojn kaj <tld/>&ccirc;enon
            sur vian <tld/>on<fnt>Z</fnt>;
          </ekz><ekz>
            <ind>mizero piedojn sanigas, <tld/>on elastigas</ind>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="hu">megtan&iacute;t a nyomor az al&aacute;zatoss&aacute;gra</trd>
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <ind>riski sian <tld/>on</ind><fnt>Z</fnt>
            <klr>(vivon)</klr>;
            <trd lng="fr">manquer de se casser le <ind>cou</ind></trd>
            <trd lng="hu">fej&eacute;t kock&aacute;ztatja</trd>
            <trd lng="ru">&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>rompa <klr>(tre laciga)</klr> laboro<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="prt">
          <ref cel="gorgx.0o">gor&gcirc;o</ref>,
          <ref cel="nuk.0o">nuko</ref>.
        </refgrp>
        <trd lng="fr">cou</trd>
        <trdgrp lng="nl">
          <trd>hals</trd>,
          <trd>nek</trd>
        </trdgrp>
        <trd lng="de">Hals</trd>
        <trd lng="pl">szyja</trd>
        <trd lng="pt">pesco&ccedil;o</trd>
        <trd lng="ru">&c_sh;&c_je;&c_ja;</trd>
        <trd lng="tr">boyun</trd>
      </snc>
      <snc mrk="kol.0o.objekto">
        <dif>
          Longa mallar&gcirc;a <tld/>forma parto de objekto:
          <ekz>
            <tld/>o de botelo<fnt>Z</fnt>,
          </ekz><ekz>
            retorto, veziko;
          </ekz><ekz>
            <tld/>o de la femurosto;
          </ekz><ekz>
            <ref cel="kol.mar0o">mar<tld/>o</ref>;
          </ekz><ekz>
            <ref cel="kol.ter0o">ter<tld/>o</ref>;
          </ekz><ekz>
            <ref cel="kol.mont0o">ter<tld/>o</ref>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Hals</trd>,
          <trd>Enge</trd>,
          <trd>Engpass</trd>,
          <trd>Engstelle</trd>
        </trdgrp>
        <trd lng="fr">col<klr tip="ind"> (d'un objet)</klr></trd>
        <trd lng="nl">hals</trd>
        <trd lng="pl">szyjka</trd>
        <trdgrp lng="ru">
          <trd>&c_g;&c_o;&c_r;&c_l;&c_y;&c_sh;&c_k;&c_o; <klr>(&c_b;&c_u;&c_t;&c_y;&c_l;&c_k;&c_i;)</klr></trd>,
          <trd>&c_sh;&c_je;&c_j;&c_k;&c_a; <klr>(&c_b;&c_je;&c_d;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>hrdlo</trd>,
    <trd>krk</trd>
  </trdgrp>
  <trd lng="en">neck</trd>
  <trd lng="es">cuello</trd>
  <trd lng="he">&tsadi;&vav;&vav;&alef;&resh;</trd>
  <trd lng="hu">nyak</trd>
  <trd lng="id">leher</trd>
  <trdgrp lng="sk">
    <trd>hrdlo</trd>,
    <trd>krk</trd>
  </trdgrp>
  <trd lng="sv">hals</trd>
</drv>

    <drv mrk="kol.gxis0e">
      <kap>&gcirc;is<tld/>e</kap>
      <snc mrk="kol.gxis0e.rektasence">
        <dif>
          &Gcirc;is la <tld/>o:
          <ekz>
            en vesta&jcirc;o, butonumita &gcirc;is<tld/>e
	    <fnt>
	      <aut>Jen&odblac; Rejt&odblac;, tr. L&aacute;szl&oacute; Bal&aacute;zs</aut>:
	      <lok><url ref="http://www.freeweb.hu/verkoj/06.htm"
		  >La nevidebla legio</url></lok>
	    </fnt>;
          </ekz><ekz>
            ili enterigis &gcirc;is<tld/>e mian onklinon en Afganio kaj
            ili mortigis &scirc;in per &scirc;tonoj
            <fnt>
              <lok><url
 ref="http://zh-tw.lernu.net/komunikado/forumo/temo.php?t=1960&amp;p=25"
                  >Lernu!</url></lok>
            </fnt>;
          </ekz><ekz>
            jen estis liaj du preferataj katidoj, tremantaj, malgajaj,
            &gcirc;is<tld/>e en &scirc;a&ubreve;mo de sapeca akvo
            <fnt>
              <aut>T. Turner, tr. R. McGrath</aut>:
              <lok><url
 ref="http://i-espero.info/files/elibroj/eo%20-%20turner,%20ethel%20-%20sep%20junaj%20australianoj.pdf"
                  >Sep junaj a&ubreve;stralianoj</url></lok>
            </fnt>
          </ekz>
        </dif>
        <trd lng="en">up to one's neck<klr> (in water etc)</klr></trd>
        <trd lng="es">hasta el cuello</trd>
      </snc>
      <snc mrk="kol.gxis0e.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Plene, abunde, tro multe:
          <ekz>
            <ind>&gcirc;is<tld/>e mi satas</ind> pri viaj ruza&jcirc;oj
            <trd lng="es">estoy harto</trd>
            <trd lng="ru">&c_s;&c_y;&c_t; &c_p;&c_o; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
            <fnt>
              <aut>K. &Ccaron;apek, tr. J. Vondrou&scaron;ek</aut>:
              <vrk><url
                  ref="http://www.literatura.bucek.name/capekk/capek-karel_afero%20makropulos.doc"
                  >Afero Makropulos</url></vrk>
            </fnt>;
          </ekz><ekz>
            <ind>&gcirc;is<tld/>e armita</ind>.
            <trd lng="es">armado hasta el cuello</trd>
            <trd lng="nl">tot de <ind>tand</ind>en gewapend</trd>
            <trd lng="ru">&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_jo;&c_n; &c_d;&c_o; &c_z;&c_u;&c_b;&c_o;&c_v;</trd>
          </ekz>
        </dif>
        <trdgrp lng="en">
          <trd>more than enough</trd>,
          <trd>enough and to spare</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>hasta arriba</trd>,
          <trd>m&aacute;s que suficiente</trd>,
          <trd>hasta el mo&ntilde;o</trd>
        </trdgrp>
      </snc>
      <trd lng="ru">&c_p;&c_o; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
    </drv>

    <drv mrk="kol.0umo">
      <kap><ofc>*</ofc><tld/>umo<fnt>Z</fnt></kap>
      <snc mrk="kol.0umo.TEKS">
        <uzo tip="fak">TEKS</uzo>
        <dif>
          Parto de <ref tip="malprt" cel="vest.0o">vesto</ref>
          &ccirc;irka&ubreve;anta la
          <tld/>on:
          <ekz>
            <tld/>umo de &ccirc;emizo, surtuto;
          </ekz><ekz>
            tola, punta <tld/>umo.
          </ekz>
        </dif>
      </snc>
  <trd lng="bg">&c_ja;&c_k;&c_a;</trd>
  <trd lng="cs">l√≠mec</trd>
  <trd lng="de">Kragen</trd>
  <trd lng="en">collar</trd>
  <trd lng="es">cuello <klr>de prenda de vestir</klr></trd>
  <trd lng="fr">col<klr tip="ind"> (d'un v&ecirc;tement)</klr></trd>
  <trd lng="he">&tsadi;&vav;&vav;&alef;&resh;&vav;&fnun;</trd>
  <trd lng="hu">gall&eacute;r</trd>
  <trd lng="id">kerah</trd>
  <trdgrp lng="nl">
        <trd>boord<klr tip="ind"> (kraag)</klr></trd>,
        <trd>kraag</trd>
      </trdgrp>
  <trd lng="pl">ko&lstroke;nierz</trd>
  <trdgrp lng="ru">
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
  <trd lng="sk">golier</trd>
  <trd lng="sv">krage</trd>
</drv>

    <drv mrk="kol.blank0umulo">
      <kap>blank<tld/>umulo,
      <var><kap>blanka<tld/>umulo</kap></var></kap>
      <uzo tip="stl">EVI</uzo>
      <snc mrk="kol.blank0umulo.POL">
        <uzo tip="fak">POL</uzo>
        <dif>
          Oficisto &mdash; tiel nomata alude al la blankkola
          &ccirc;emizo, kian en iuj landoj tradicie portis la oficistoj sub
          oficeja kompleto:
          <ekz>
            la blank<tld/>umulo ela&ubreve;ti&gcirc;is &ccirc;e la pordo de granda kompanio
            <fnt>
              <aut>Xiong Yanling, trad. Helo</aut>,
              <vrk>Ekzamena Intervidi&gcirc;o</vrk>,
              <lok><url ref="http://www.elerno.cn/penseo/PEN185.pdf">
                  Penseo, 2007-03, numero 185a</url></lok>
            </fnt>;
          </ekz><ekz>
            la ideologio de sinreguligo de merkatoj sendube
            flankenpu&scirc;is la batalon kontra&ubreve; krimado fare de
            blank<tld/>umuloj 
            <fnt>
              <aut>I. Warde, trad. M. Leon</aut>,
              <vrk><url ref="http://eo.mondediplo.com/article1558.html">
                  Madoff, sub la nazoj...</url></vrk>, 
              <lok>le Monde diplomatique, 2009-12</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <rim mrk="kol.blank0umulo.RIM_EVI">
        Iuj rigardas tiun idioma&jcirc;on malklara, malmoderna kaj evitinda.
      </rim>
      <trd lng="fr">col-blanc</trd>
    </drv>

    <drv mrk="kol.blu0umulo">
      <kap>blu<tld/>umulo, <var><kap>blua<tld/>umulo</kap></var></kap>
      <uzo tip="stl">EVI</uzo>
      <snc mrk="kol.blu0umulo.POL">
        <uzo tip="fak">POL</uzo>
        <dif>
          Fabrika laboristo &mdash; tiel nomata alude al la tradiciaj bluaj 
          laborvestoj en fabrikoj kaj konstraste al
          <ref tip="vid" cel="kol.blank0umulo.POL">blankkolumulo</ref>:
          <ekz>
            la&ubreve; la analizo pri la bezono de laboruloj de diversaj fakoj
            en
            2002,
            laboruloj de manufakturado, precipe kapablaj fakuloj
            (alinome 
            altklasaj blu<tld/>umuloj), estas tre bonvenaj
            <fnt>
              <aut>-</aut>,
              <vrk><url
                  ref="http://www.espero.com.cn/old/epch/09e/09e-3f/03-09e-3-4.htm"
                  >Laboruloj de 5 fakoj...</url></vrk>,
              <lok>[2003?]</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <rim mrk="kol.blu0umulo.RIM_EVI">
        Iuj rigardas tiun nomon malklara kaj evitinda.
      </rim>
      <trd lng="fr">col-bleu</trd>
    </drv>

    <drv mrk="kol.cxirkaux0o">
      <kap>&ccirc;irka&ubreve;<tld/>o</kap>
      <snc mrk="kol.cxirkaux0o.ligilo">
        <dif>
          Ligilo metata &ccirc;irka&ubreve; la <tld/>on de besto.
        </dif>
        <trdgrp lng="be">
          <trd>&c_a;&c_sh;&c_y;&c_j;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_a;&c_b;&c_r;&c_o;&c_zh;&c_a;&c_k;</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>licou</trd>,
          <trd>licol</trd>,
          <trd>collier<klr tip="ind"> (d'animal)</klr></trd>
        </trdgrp>
        <trd lng="hu">nyak&ouml;rv</trd>
        <trd lng="he">&qof;&vav;&lamed;&resh;</trd>
        <trd lng="nl">halsband</trd>
        <trd lng="pl">obro&zdot;a</trd>
        <trd lng="pt">coleira</trd>
        <trd lng="ru">&c_o;&c_sh;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
      </snc>
      <snc mrk="kol.cxirkaux0o.ornamajxo">
        <dif>
          Ornama&jcirc;o portata &ccirc;irka&ubreve; la <tld/>o:
          <ekz>
            &scirc;i incensis <klr>[al la dioj]</klr>, ornaminte sin per
            siaj ringoj kaj siaj &ccirc;irka&ubreve;koloj
            <fnt><bib>MT</bib>, <lok>&Hos; 2:13</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="cxen.0o">&ccirc;eno</ref>,
          <ref cel="ring.0o">ringo</ref>.
        </refgrp>
        <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;&c_ib;</trd>
        <trd lng="fr">collier<klr tip="ind"> (ornement)</klr></trd>
        <trd lng="he">&ayin;&nun;&qof;<klr tip="amb"> (&tav;&kaf;&shin;&yod;&tet;
            &lamed;&tsadi;&vav;&vav;&alef;&resh;)</klr></trd>
        <trd lng="hu">nyak&eacute;k</trd>
        <trd lng="nl">halsketting</trd>
        <trd lng="ru">&c_o;&c_zh;&c_je;&c_r;&c_je;&c_l;&c_mol;&c_je;</trd>
      </snc>
      <trd lng="es">collar</trd>
      <trd lng="de">Halsband</trd>
      <trd lng="sv">halsband</trd>
    </drv>

    <drv mrk="kol.mar0o">
      <kap>mar<tld/>o</kap>
      <snc mrk="kol.mar0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Parto de maro a&ubreve; oceano, akva strio inter pecoj da seka
