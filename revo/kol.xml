<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>kol</rad>/o
    </kap>
    <drv mrk="kol.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="kol.0o.korpo">
        <uzo tip="fak">ANA</uzo>
        <dif>
          Parto de la korpo kuniganta la kapon kun la trunko:
          <ekz>
            havi gracian, &gcirc;irafan <klr>(tro longan)</klr>
            <tld/>on;
          </ekz>
          <ekz>
            <ind>&jcirc;eti sin sur la <tld/>on de iu</ind><fnt>Z</fnt>
            <klr>(por montri sian amon)</klr>;
            <trd lng="fr">se jeter au <ind>cou</ind> de qn</trd>
            <trd lng="hu">nyak&aacute;ba ugrik</trd>
            <trd lng="sv">kasta sig om <ind>halsen</ind> p&aring;
            n&aring;gon</trd>
          </ekz>
          <ekz>
            mi metis braceletojn sur viajn manojn kaj <tld/>&ccirc;enon
            sur vian <tld/>on<fnt>Z</fnt>;
          </ekz>
          <ekz>
            <ind>&gcirc;is<tld/>e armita</ind>;
            <trd lng="nl">tot de <ind>tand</ind>en gewapend</trd>
          </ekz>
          <ekz>
            <ind>mizero piedojn sanigas, <tld/>on elastigas</ind>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="hu">megtan&iacute;t a nyomor az al&aacute;zatoss&aacute;gra</trd>
          </ekz>
          <ekz>
            <uzo tip="stl">FIG</uzo>
            <ind>riski sian <tld/>on</ind><fnt>Z</fnt>
            <klr>(vivon)</klr>;
            <trd lng="fr">manquer de se casser le <ind>cou</ind></trd>
            <trd lng="hu">fej&eacute;t kock&aacute;ztatja</trd>
          </ekz>
          <ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>rompa <klr>(tre laciga)</klr> laboro<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="prt">
          <ref cel="gorgx.0o">gor&gcirc;o</ref>,
          <ref cel="nuk.0o">nuko</ref>.
        </refgrp>
        <trd lng="fr">cou</trd>
        <trdgrp lng="nl">
          <trd>hals</trd>,
          <trd>nek</trd>
        </trdgrp>
        <trd lng="de">Hals</trd>
        <trd lng="pl">szyja</trd>
        <trd lng="pt">pesco&ccedil;o</trd>
        <trd lng="ru">&c_sh;&c_je;&c_ja;</trd>
        <trd lng="tr">boyun</trd>
      </snc>
      <snc mrk="kol.0o.objekto">
        <dif>
          Longa mallar&gcirc;a <tld/>forma parto de objekto:
          <ekz>
            <tld/>o de botelo<fnt>Z</fnt>,
          </ekz>
          <ekz>
            retorto, veziko;
          </ekz>
          <ekz>
            <tld/>o de la femurosto;
          </ekz>
          <ekz>
            <ref cel="kol.mar0o">mar<tld/>o</ref>;
          </ekz>
          <ekz>
            <ref cel="kol.ter0o">ter<tld/>o</ref>;
          </ekz>
          <ekz>
            <ref cel="kol.mont0o">ter<tld/>o</ref>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Hals</trd>,
          <trd>Enge</trd>,
          <trd>Engpass</trd>,
          <trd>Engstelle</trd>
        </trdgrp>
        <trd lng="fr">col<klr tip="ind"> (d'un objet)</klr></trd>
        <trd lng="nl">hals</trd>
        <trd lng="pl">szyjka</trd>
        <trdgrp lng="ru">
          <trd>&c_g;&c_o;&c_r;&c_l;&c_y;&c_sh;&c_k;&c_o; <klr>(&c_b;&c_u;&c_t;&c_y;&c_l;&c_k;&c_i;)</klr></trd>,
          <trd>&c_sh;&c_je;&c_j;&c_k;&c_a; <klr>(&c_b;&c_je;&c_d;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
        </trdgrp>
      </snc>
      <trd lng="cs">krk</trd>
      <trd lng="en">neck</trd>
      <trd lng="he">&tsadi;&vav;&vav;&alef;&resh;</trd>
      <trd lng="hu">nyak</trd>
      <trd lng="sv">hals</trd>  
    </drv>

    <drv mrk="kol.0umo">
      <kap><ofc>*</ofc><tld/>umo<fnt>Z</fnt></kap>
      <snc>
        <uzo tip="fak">TEKS</uzo>
        <dif>
          Parto de <ref tip="malprt" cel="vest.0o">vesto</ref>
          &ccirc;irka&ubreve;anta la
          <tld/>on:
          <ekz>
            <tld/>umo de &ccirc;emizo, surtuto;
          </ekz>
          <ekz>
            tola, punta <tld/>umo.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Kragen</trd>
      <trd lng="en">collar</trd>
      <trd lng="fr">col<klr tip="ind"> (d'un v&ecirc;tement)</klr></trd>
      <trd lng="he">&tsadi;&vav;&vav;&alef;&resh;&vav;&fnun;</trd>
      <trd lng="hu">gall&eacute;r</trd>  
      <trdgrp lng="nl">
        <trd>boord<klr tip="ind"> (kraag)</klr></trd>,
        <trd>kraag</trd>
      </trdgrp>
      <trd lng="pl">ko&lstroke;nierz</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
        <trd>&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
      <trd lng="sv">krage</trd>
    </drv>

    <drv mrk="kol.cxirkaux0o">
      <kap>&ccirc;irka&ubreve;<tld/>o</kap>
      <snc mrk="kol.cxirkaux0o.ligilo">
        <dif>
          Ligilo metata &ccirc;irka&ubreve; la <tld/>on de besto.
        </dif>
        <trdgrp lng="be">
          <trd>&c_a;&c_sh;&c_y;&c_j;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_a;&c_b;&c_r;&c_o;&c_zh;&c_a;&c_k;</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>licou</trd>,
          <trd>licol</trd>,
          <trd>collier<klr tip="ind"> (d'animal)</klr></trd>
        </trdgrp>
        <trd lng="hu">nyak&ouml;rv</trd>
        <trd lng="he">&qof;&vav;&lamed;&resh;</trd>
        <trd lng="nl">halsband</trd>
        <trd lng="pl">obro&zdot;a</trd>
        <trd lng="pt">coleira</trd>
        <trd lng="ru">&c_o;&c_sh;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
      </snc>
      <snc mrk="kol.cxirkaux0o.ornamajxo">
        <dif>
          Ornama&jcirc;o portata &ccirc;irka&ubreve; la <tld/>o:
          <ekz>
            &scirc;i incensis <klr>[al la dioj]</klr>, ornaminte sin per
            siaj ringoj kaj siaj &ccirc;irka&ubreve;koloj
            <fnt><bib>MT</bib><lok>&Hos; 2:13</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="cxen.0o">&ccirc;eno</ref>,
          <ref cel="ring.0o">ringo</ref>.
        </refgrp>
        <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;&c_ib;</trd>
        <trd lng="fr">collier<klr tip="ind"> (ornement)</klr></trd>
        <trd lng="he">&ayin;&nun;&qof;<klr tip="amb"> (&tav;&kaf;&shin;&yod;&tet;
        &lamed;&tsadi;&vav;&vav;&alef;&resh;)</klr></trd>
        <trd lng="hu">nyak&eacute;k</trd>
        <trd lng="nl">halsketting</trd>
        <trd lng="ru">&c_o;&c_zh;&c_je;&c_r;&c_je;&c_l;&c_mol;&c_je;</trd>
      </snc>
      <trd lng="de">Halsband</trd>
      <trd lng="sv">halsband</trd>
    </drv>

    <drv mrk="kol.mar0o">
      <kap>mar<tld/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Parto de maro a&ubreve; oceano, akva strio inter pecoj da seka
          tero, komunikiganta najbarajn marojn kaj oceanojn.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>strait</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Meerenge</trd>,
        <trd>Meeresstra&szlig;e</trd>,
        <trd>Sund</trd>
      </trdgrp>
      <trd lng="fr">d&eacute;troit<klr tip="ind"> (g&eacute;og.)</klr></trd>
      <trd lng="hu">tengerszoros</trd>
      <trdgrp lng="nl">
        <trd>zeestraat</trd>,
        <trd>straat<klr tip="ind"> (zeestraat)</klr></trd>
      </trdgrp>
      <trd lng="pl">cie&sacute;nina</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>
      <trd lng="sv">sund<klr tip="ind"> (geog.)</klr></trd>
    </drv>

    <drv mrk="kol.mont0o">
      <kap>mont<tld/>o</kap>
      <fnt><bib>PV</bib></fnt>
      <uzo tip="stl">RAR</uzo>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <ref tip="dif" cel="mont.inter0o0pasejo">montpasejo,
        intermonto</ref>
      </snc>
      <trd lng="fr">col<klr tip="ind"> (de montagne)</klr></trd>
      <trd lng="hu">h&aacute;g&oacute;</trd>
      <trdgrp lng="nl">
        <trd>col<klr tip="ind"> (bergpas)</klr></trd>,
        <trd>bergpas</trd>
      </trdgrp>  
      <trd lng="pl">prze&lstroke;&eogonek;cz</trd>
    </drv>

    <drv mrk="kol.ter0o">
      <kap><ofc>*</ofc>ter<tld/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Mallar&gcirc;a strio da tero inter du akvujoj (maroj, lagoj ktp),
          kuniganta du terpartojn:
          <ekz>
            <ctl>Postkorto de Usono</ctl>, la centramerika ter<tld/>o
            &jcirc;us flami&gcirc;is
            <fnt>
              <vrk><url ref="http://eo.mondediplo.com/article1392.html"
                        >&leftquot;Le Monde diplomatique&rightquot; en Esperanto</url>
              </vrk>
            </fnt>
          </ekz><ekz>
            Pasul Ghime&scedil;-Palanca (Terkolo de Djime&scirc;o, hungare:
            Gyimesi-szoros) estas ter<tld/>o en Rumanio en Orientaj
            Karpatoj, kiu el Baseno de &Ccirc;iko kondukas tra la valo
            Tatro&scirc;o al Moldavio je alteco de 1006m super marnivelo
            <fnt><vrk><url
            ref="http://eo.pandapedia.com/wiki/Terkolo_de_Djime%C5%9Do"
                           >Terkolo de Djime&scirc;o</url></vrk>
            </fnt>
          </ekz>
        </dif>
      </snc>
      <ref tip="sin" cel="istm.0o">istmo</ref>
      <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_y;&c_je;&c_k;</trd>
      <trd lng="ca">istme</trd>
      <trdgrp lng="de">
        <trd>Isthmus</trd>,
        <trd>Landenge</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>isthmus</trd>,
        <trd>neck<klr> (of land)</klr></trd>
      </trdgrp>
      <trd lng="es">istmo</trd>
      <trd lng="fr">isthme</trd>
      <trd lng="hu">f&ouml;ldszoros</trd> 
      <trd lng="nl">landengte</trd> 
      <trd lng="pl">przesmyk</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
    </drv>

    <drv mrk="kol.BeringaMar0o">
      <kap>Beringa Mar<tld/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Mar<tld/>o disiganta <ref cel="azi.A0o">Azion</ref> disde
          <ref cel="amerik.A0o">Ameriko</ref>,
          kaj liganta la Arktan Oceanon kun Pacifiko (169&ring;W,
          65&ring;N).
        </dif>
        <refgrp tip="vid">
          Mapon &ccirc;e la artikolo pri
          <ref cel="bering.0o">Beringo</ref>
        </refgrp>
      </snc>
      <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;&c_a;&c_v;&c_a; &c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
      <trd lng="de">Beringstra&szlig;e</trd>
      <trd lng="en">Bering Strait</trd>
      <trd lng="es">Estrecho de Bering</trd>
      <trd lng="fr">D&eacute;troit de Bering</trd>
      <trd lng="hu">Bering-szoros</trd>
      <trd lng="nl">Beringstraat</trd>
      <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;&c_o;&c_v; &c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>
    </drv>

  </art>
