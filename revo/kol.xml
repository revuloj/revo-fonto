<drv mrk="kol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kol.0o.korpo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la korpo kuniganta la kapon kun la trunko:
      <ekz>
        havi gracian, &gcirc;irafan <klr>(tro longan)</klr>
        <tld/>on;
      </ekz><ekz>
        <ind>&jcirc;eti sin sur la <tld/>on de iu</ind><fnt>Z</fnt>
        <klr>(por montri sian amon)</klr>;
        <trd lng="fr">se jeter au <ind>cou</ind> de qn</trd>
        <trd lng="hu">nyak&aacute;ba ugrik</trd>
        <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;
        <ind>&c_n;&c_a; &c_sh;&c_je;&c_ju;</ind>
        &c_k;&c_o;&c_m;&c_u;-&c_l;.</trd>
        <trd lng="sv">kasta sig om <ind>halsen</ind> p&aring;
        n&aring;gon</trd>
      </ekz><ekz>
        mi metis braceletojn sur viajn manojn kaj <tld/>&ccirc;enon
        sur vian <tld/>on<fnt>Z</fnt>;
      </ekz><ekz>
        <ind>mizero piedojn sanigas, <tld/>on elastigas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">megtan&iacute;t a nyomor az
        al&aacute;zatoss&aacute;gra</trd>
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>riski sian <tld/>on</ind><fnt>Z</fnt>
        <klr>(vivon)</klr>;
        <trd lng="fr">manquer de se casser le <ind>cou</ind></trd>
        <trd lng="hu">fej&eacute;t kock&aacute;ztatja</trd>
        <trd lng="ru">&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;
        &c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>rompa <klr>(tre laciga)</klr> laboro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="gorgx.0o">gor&gcirc;o</ref>,
      <ref cel="nuk.0o">nuko</ref>.
    </refgrp>
    <trd lng="fr">cou</trd>
    <trdgrp lng="nl">
      <trd>hals</trd>,
      <trd>nek</trd>
    </trdgrp>
    <trd lng="de">Hals</trd>
    <trd lng="pl">szyja</trd>
    <trd lng="pt">pesco&ccedil;o</trd>
    <trd lng="ru">&c_sh;&c_je;&c_ja;</trd>
    <trd lng="tr">boyun</trd>
  </snc>
  <snc mrk="kol.0o.objekto">
    <dif>
      Longa mallar&gcirc;a <tld/>forma parto de objekto:
      <ekz>
        <ind><tld/>o de botelo</ind><fnt>Z</fnt>,
        <trd lng="ru">&c_g;&c_o;&c_r;&c_l;&c_y;&c_sh;&c_k;&c_o;
      <klr>(&c_b;&c_u;&c_t;&c_y;&c_l;&c_k;&c_i;)</klr></trd>
      </ekz><ekz>
        retorto, veziko;
      </ekz><ekz>
        <ind><tld/>o de la femurosto</ind>;
        <trd lng="ru">&c_sh;&c_je;&c_j;&c_k;&c_a;
           <klr>(&c_b;&c_je;&c_d;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
      </ekz><ekz>
        <ref cel="kol.mar0o">mar<tld/>o</ref>;
      </ekz><ekz>
        <ref cel="kol.ter0o">ter<tld/>o</ref>;
      </ekz><ekz>
        <ref cel="kol.mont0o">mont<tld/>o</ref>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Hals</trd>,
      <trd>Enge</trd>,
      <trd>Engpass</trd>,
      <trd>Engstelle</trd>
    </trdgrp>
    <trd lng="fr">col<klr tip="ind"> (d'un objet)</klr></trd>
    <trd lng="nl">hals</trd>
    <trd lng="pl">szyjka</trd>
  </snc>
  <snc mrk="kol.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <fnt><bib>RokPop</bib></fnt>
    <dif>
      <tld lit="K"/>simila parto de korda muzikilo, la loko de la
      fingrotabulo.
    </dif>
    <trd lng="ru">&c_g;&c_r;&c_i;&c_f;<klr> (&c_m;&c_u;&c_z;.)</klr></trd>
  </snc>
  <trd lng="af">nek</trd>
  <trd lng="am">&#x12A0;&#x1295;&#x1308;&#x1275;</trd>
  <trd lng="ar">&#x627;&#x644;&#x631;&#x642;&#x628;&#x629;</trd>
