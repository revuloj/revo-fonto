<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: jalt.xml,v 1.7 2023/12/03 07:09:06 afido Exp $">
    <kap>
      <rad>Jalt</rad>/o
    </kap>

<drv mrk="jalt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="jalt.0o.urbo">
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref>
      &ccirc;e <ref cel="mar.NigraM0o">Nigra Maro</ref>,
      en la sudo de <ref tip="malprt" cel="krime.0o">Krimeo</ref>:
      <ekz>
        el Jalto, la vapor&scirc;ipo <nom>Mi&hcirc;ail</nom> nin forportis preter la Krima montaro, nun laste
        vidita, &gcirc;is <tld/>o
        <fnt>
          <aut>Valdemar Langlet</aut>
          <vrk><url ref="&Tekstaro;?nomo=vojaghimpresoj">Voja&gcirc;impresoj</url></vrk>
          <lok>Impresoj de svedoj, voja&gcirc;antaj en Rusujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ivano <klr>[&#x2026;]</klr> ru&gcirc;i&gcirc;inte komencis balbuti ion pri voja&gcirc;o al sanatorio en
        <tld/>o
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        vizitinda urbo estas <tld/>o <klr>[&#x2026;]</klr>, mara ripozloko sude en la duoninsulo
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2012/010666.php">Krimeo</url></vrk>
          <lok>2012/07, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ja;&c_l;&c_t;&c_a;</trd>
    <trd lng="de">Jalta</trd>
    <trd lng="en">Yalta</trd>
    <trd lng="fr">Yalta</trd>
    <trd lng="nl">Jalta</trd>
    <trd lng="ru">&c_Ja;&c_l;&c_t;&c_a;</trd>
    <trd lng="uk">&c_Ja;&c_l;&c_t;&c_a;</trd>
    <trd lng="zh">&#x96C5;&#x5C14;&#x5854; <pr>Y&#x1CE;&#x11B;rt&#x1CE;</pr></trd>
</snc>
  <trd lng="ja">&#x30E4;&#x30EB;&#x30BF;</trd>
</drv>

  </art>

