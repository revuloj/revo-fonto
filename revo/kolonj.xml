<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kolonj</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="kolonj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Nomo de kelkaj Romiaj <ref cel="koloni.0o.setlejo">kolonioj</ref>.
    </dif>
    <trd lng="lat">Colonia</trd>
    <trd lng="pt">Col&ocirc;nia</trd>
  </snc>
  <snc>
    <fnt><bib>ZR</bib></fnt>
    <fnt><bib>PIV1</bib></fnt>
    <fnt><bib>BEE</bib></fnt>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en okcidenta <ref
      tip="malprt" cel="german1.G0ujo">Germanujo</ref> (6&ring;58'E,
      50&ring;56'N), situanta &ccirc;e la rivero Rejno, plej granda urbo de la
      federacia lando <ref tip="vid" cel="nordrejnvest.0o">Nordrejn-Vestfalio</ref>.
    </dif>
    <trd lng="be">&c_K;&c_jo;&c_l;&c_mol;&c_n;</trd>
    <trd lng="de">K&ouml;ln</trd>
    <trd lng="fr">Cologne</trd>
    <trd lng="hu">K&ouml;ln</trd>
    <trd lng="lat">Colonia Agrippina</trd>
  </snc>
  <trd lng="nl">Keulen</trd>
  <trd lng="pt">Col&ocirc;nia</trd>
</drv>
</art>
