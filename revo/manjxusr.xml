<vortaro>
<art mrk="$Id: manjxusr.xml,v 1.7 2023/10/19 15:13:16 afido Exp $">
<kap>
  <rad>Man&jcirc;usr</rad>/o
</kap>

<drv mrk="manjxusr.0o">
  <kap><tld/>o</kap>
  <snc mrk="manjxusr.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Iu el la <ref tip="super" cel="bodisatv.0o">bodisatvo</ref>j, simbolo de <ref
      cel="sagx.0o">sa&gcirc;o</ref>
      kaj <ref cel="lum.en0igxo">enlumi&gcirc;o</ref>:
      <ekz>
        metiisto: &leftquot;konsentite! mi faru <tld/>on, kiu gardos vin en vivo kaj postmondo&rightquot;
        <fnt>
          <aut>-, trad. Simatani T.</aut>
          <vrk><url
          ref="http://esperanto.sannasubi.com/noakom/n_bussi.htm">Statu-metiisto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la statuo de <tld/>o <klr>[li]</klr> absorbi&gcirc;is en sia citado de sutro
        <fnt>
          <vrk><url ref="&CRI;581/2015/09/15/173s180124.htm"
              >Sangpo, lamao el la 90aj jaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_n;&c_d;&c_zh;&c_u;&c_sh;&c_r;&c_y;</trd>
  <trd
  lng="bo">&t_-a;&t_ja;&t_ma;&t_tsheg;&t_da;&t_pa;&t_la;&t_tsheg;&t_da;&t_ba;&t_yata;&t_nga;&t_sa;&#3853;</trd>
  <trd lng="de">Manjushri</trd>
  <trdgrp lng="fr">
    <trd>Manjushri</trd>,
    <trd>Manjusri</trd>
  </trdgrp>
  <trd lng="ja">&#25991;&#27530;&#33769;&#34217; <pr>&#12418;&#12435;&#12376;&#12421;&#12412;&#12373;&#12388;</pr></trd>
  <trd lng="pl">Manjushri</trd>
  <trd lng="ro">Manjushri</trd>
  <trd lng="zh">&#25991;&#27530;</trd>
</drv>
</art>

