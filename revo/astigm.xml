</dif>
</snc>
<trdgrp lng="fr">
   <trd>astigmatique</trd>,
   <trd>astigmate</trd></trdgrp>
<trd 
lng="ru">&#1072;&#1089;&#1090;&#1080;&#1075;&#1084;&#1072;&#1090;&#1080;&#1095;&#1077;&#1089;&#1082;&#1080;&#1081;</trd>
</drv>
