<vortaro>
<art mrk="$Id: astigm.xml,v 1.21 2023/10/13 17:08:38 revo Exp $">
<kap><rad>astigmat</rad>/a</kap>

<drv mrk="astigm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta <tld/>econ a&ubreve; <tld/>ismon.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">astigmatick&yacute;</trd>
  <trd lng="de">astigmatisch</trd>
  <trd lng="en">astigmatic</trd>
  <trd lng="es">astigm&aacute;tico</trd>
  <trdgrp lng="fr">
    <trd>astigmatique</trd>,
    <trd>astigmate</trd>
  </trdgrp>
  <trd lng="hu">asztigmatikus</trd>
  <trd lng="it">astigmatico</trd>
  <trdgrp lng="ja">
    <trd>&#x975E;&#x70B9;&#x53CE;&#x5DEE;&#x306E;  <pr>&#x3072;&#x3066;&#x3093;&#x304A;&#x3055;&#x3080;&#x3055;&#x306E;</pr></trd>,
    <trd>&#x4E71;&#x8996;&#x306E;  <pr>&#x3089;&#x3093;&#x3057;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">astigmatisch</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">astigmatick&yacute;</trd>
  <trd lng="uk">&#x430;&#x441;&#x442;&#x438;&#x433;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="astigm.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="aberac.0o.FOT">Aberacio</ref>
      de optika sistemo, en kiu la bilda fokuso estas
      anstata&ubreve;ata de du fokusaj strekoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">astigmati&ccaron;nost</trd>
  <trd lng="de">Astigmatismus</trd>
  <trd lng="en">astigmatism</trd>
  <trd lng="es">astigmatismo</trd>
  <trd lng="fr">astigmatisme</trd>
  <trd lng="hu">asztigmatizmus</trd>
  <trd lng="it">astigmatismo</trd>
  <trd lng="ja">&#x975E;&#x70B9;&#x53CE;&#x5DEE;  <pr>&#x3072;&#x3066;&#x3093;&#x3057;&#x3085;&#x3046;&#x3055;</pr></trd>
  <trd lng="nl">astigmatisme</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">astigmatickos&tcaron;</trd>
  <trd lng="uk">&#x430;&#x441;&#x442;&#x438;&#x433;&#x43C;&#x430;&#x442;&#x438;&#x437;&#x43C;</trd>
</drv>

<drv mrk="astigm.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vida malneteco pro malegaleco de okullensa kurbi&gcirc;o:
      <ekz>
        medicine, <tld/>ismo estas difekto de la okulo, kiu, ne havante unu saman
        kurbecon en la diversaj meridianoj, estigas vidon neegale klaran en &ccirc;iuj
        direktoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=226867">Astigmatismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">astigmatizmus</trd>
  <trd lng="de">Astigmatismus</trd>
  <trd lng="en">astigmatism</trd>
  <trd lng="es">astigmatismo</trd>
  <trd lng="fr">astigmatisme</trd>
  <trd lng="he">&#x5D0;&#x5E1;&#x5D8;&#x5D9;&#x5D2;&#x5DE;&#x5E6;&#x5D9;&#x5D4;</trd>
  <trd lng="hu">asztigmatizmus</trd>
  <trd lng="it">astigmatismo</trd>
  <trd lng="ja">&#x4E71;&#x8996;  <pr>&#x3089;&#x3093;&#x3057;</pr></trd>
  <trd lng="nl">astigmatisme</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">astigmatizmus</trd>
  <trd lng="uk">&#x430;&#x441;&#x442;&#x438;&#x433;&#x43C;&#x430;&#x442;&#x438;&#x437;&#x43C;</trd>
</drv>
</art>
