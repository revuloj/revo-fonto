<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kanistr</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kanistr.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanistr.0o.ujo">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Ferlada fermebla skatolo por teni teon ks.
    </dif>
    <trdgrp lng="pl">
      <trd>kanister</trd>,
      <trd>puszka</trd>
    </trdgrp>
  </snc>
  <snc mrk="kanistr.0o.benzinujo">
    <ref tip="dif" cel="benzin.0ujo">bezninujo</ref>
    <trdgrp lng="pl">
      <trd>kanister</trd>,
      <trd>bak</trd>,
      <trd>zbiornik <ind>paliwa</ind></trd>
    </trdgrp>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>