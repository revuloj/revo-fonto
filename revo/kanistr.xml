<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kanistr.xml,v 1.11 2022/10/26 07:35:09 revo Exp $">
<kap>
  <rad>kanistr</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kanistr.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanistr.0o.VAZ">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Fermebla <ref tip="super" cel="vaz.0o.VAZ">vazo</ref>
      el rezistema materialo (ferlado, rigida plasto)
      ta&ubreve;ga por teno kaj transporto de flua&jcirc;oj:
      <ekz>
        matene le&gcirc;obeemaj civitanoj envici&gcirc;is kun <tld/>oj
        kaj boteloj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://humurino.retejo.info/Pri/pri1_t.htm">
          La 1-a de Aprilo</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_sh;&c_a;&c_n;&c_k;&c_a; <klr>(&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="ca">bid&oacute;</trd>
  <trd lng="cs">kanistr</trd>
  <trd lng="en">canister <klr>(metal)</klr></trd>
  <trd lng="fr">bidon</trd>
  <trdgrp lng="pl">
      <trd>kanister</trd>,
      <trd>puszka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bid&atilde;o</trd>,
    <trd>gal&atilde;o<klr> (vasilhame)</klr></trd>,
    <trd>lata<klr> (vasilhame)</klr></trd>
  </trdgrp>    
  <trd lng="ro">canistr&abreve;</trd>
  <trd lng="sk">kanister</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_sh;&c_a;&c_n;&c_a; &c_k;&c_o;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kanistr.benzin0o">
  <kap>
    benzin<tld/>o
  </kap>
  <snc mrk="kanistr.benzin0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref cel="kanistr.0o.VAZ">Kanistro<sncref/></ref>,
      portebla benzinujo kun tenilo kaj ordinare
      kun demetebla ver&scirc;o-tubo:
      <ekz>
        armita[j] per gasbotelego kaj benzin<tld/>o, ili minacis eksplodigi &ccirc;ion
        <fnt>
          <aut>-</aut>
          <vrk><url
          ref="ttp://neniammilitointerni.over-blog.com/2022/01/la-familio-salhiya-de-cheikh-jarrah-en-orienta-jerusalemo-fine-estis-elpelita.html"
              >La familio Salhiya de Cheikh Jarrah en Orienta Jerusalemo fine estis
              elpelita</url></vrk>
          <lok>Neniam milito inter ni, 2022-01-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_b;&c_e;&c_n;&c_z;&c_y;&c_n;&c_u;)</klr></trd>
  <trd lng="de">Benzinkanister</trd>
  <trdgrp lng="en">
    <trd>jerry can</trd>,
    <trd>gas can<klr> (usone)</klr></trd>,
    <trd>petrol can<klr> (brite)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jerrican</trd>,
    <trd>jerrycan</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>kanister</trd>,
      <trd>bak</trd>,
      <trd>zbiornik <ind>paliwa</ind></trd>
  </trdgrp>
  <trd lng="pt">gal&atilde;o de gasolina</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="sv">bensindunk</trd>
</drv>
</art>

