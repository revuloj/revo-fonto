<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: regal.xml,v 1.31 2024/04/13 20:15:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>regal</rad>/i
</kap>
<drv mrk="regal.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="regal.0i.nutri">
    <dif>
      Bone kaj plezurige <ref tip="super" cel="trakt.0i">trakti</ref>
      iun, donante al li <ref cel="mangx.0ajxo">man&gcirc;a&jcirc;ojn</ref> kaj
      <ref cel="trink.0ajxo">trinka&jcirc;ojn</ref>:
      <ekz>
        <tld/>i la princon per la fruktoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        urbanoj <tld/>is lin per biero kaj bona man&gcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        invitu &ccirc;iujn parencojn, mi <tld/>os ilin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        eniru kaj <tld/>u vin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel oni vin taksas, tiel oni vin <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="mangx.0igi.mangxanta">man&gcirc;igi<sncref/></ref>,
       <ref cel="serv.0i.komplezi">servi<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="gast.0iganto.KOMUNE">gastiganto<sncref/></ref>,
      <ref cel="mastr.0o">mastro</ref>,
      <ref cel="kelner.0o">kelnero</ref>
    </refgrp>
    <trdgrp lng="bg">
      <trd>&c_ch;&c_je;&c_r;&c_p;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_ch;&c_je;&c_r;&c_p;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bewirten</trd>,
      <trd>festlich bewirten</trd>,
      <trd>freihalten</trd>,
      <trd>laben</trd>
    </trdgrp>
    <trd lng="es">invitar</trd>
    <trdgrp lng="hu">
      <trd>megvend&eacute;gel</trd>,
      <trd>j&oacute;l tart</trd>
    </trdgrp>
    <trd lng="nl">tracteren</trd>
  </snc>
  <snc mrk="regal.0i.trakti">
    <dif>
      Bone kaj plezurige trakti iun, liverante al li ion bonan:
      <ekz>
        la gazetoj <klr>[deziras]</klr> nur <tld/>i la legantojn per
        <klr>[&#x2026;]</klr> nova&jcirc;o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu &scirc;teliston <tld/>as, mem &scirc;teliston egalas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rekomp.0i">rekompenci</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>erg&ouml;tzen</trd>,
      <trd>erfreuen</trd>
    </trdgrp>
    <trd lng="es">obsequiar</trd>
    <trdgrp lng="hu">
      <trd>ell&aacute;t</trd>,
      <trd>meg&ouml;rvendeztet</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>tracteren</trd>,
      <trd>vermaken</trd>
    </trdgrp>
  </snc>
  <snc mrk="regal.0i.puni">
    <uzo tip="klr">(ironie)</uzo>
    <dif>
      Malbone trakti:
      <ekz>
        <ind><tld/>i <klr>[iun]</klr> per la plato de la glavo</ind>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
        <trd lng="hu">kardlappal simogat</trd>
      </ekz>
      <ekz>
        la malestimo, kiun seninduloj <tld/>as al merito
        efektiva
        <fnt>
          <aut>Zamenhof</aut>:
          <vrk><url ref="&Viki;/Hamleto">Hamleto</url></vrk>
        </fnt>
      </ekz>
      <ekz>
        <tld/>i homon nur per la maldol&ccirc;a kaliko da suferoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        por via bono vin <tld/>as bastono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as per &scirc;tonoj, tiun oni dankas per bastonoj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li alportis vipon kaj tiel <tld/>is &scirc;in, ke la kaprino forkuris salte
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pun.0i">puni</ref>,
      <ref cel="pag.re0i">repagi</ref>,
      <ref cel="vengx.0i">ven&gcirc;i</ref>.
    </refgrp>
    <trd lng="de">traktieren</trd>
    <trdgrp lng="es">
      <trd>dar palos <klr>(iron&iacute;a)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ell&aacute;tja a baj&aacute;t</trd>,
      <trd>trakt&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">tracteren</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pohostit</trd>,
    <trd>uct&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>entertain</trd>,
    <trd>regale</trd>,
    <trd>treat</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;galer</trd>
  <trdgrp lng="ja">
    <trd>&#x3054;&#x3061;&#x305D;&#x3046;&#x3059;&#x308B;</trd>,
    <trd>&#x3082;&#x3066;&#x306A;&#x3059;</trd>,
    <trd>&#x6B53;&#x5F85;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x305F;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x304F;&#x3089;&#x308F;&#x3059;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cz&eogonek;stowa&cacute;</trd>,
    <trd>fundowa&cacute;  <klr>(ugo&sacute;ci&cacute;)</klr></trd>,
    <trd>go&sacute;ci&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_o;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hosti&tcaron; jedlom</trd>,
    <trd>n&aacute;pojom</trd>,
    <trd>&ccaron;astova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_g;&#x43E;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BB4; <pr>y&#xE0;n</pr></trd>,
    <trd>&#x91BC; <pr>y&#xE0;n</pr></trd>,
    <trd>&#x98E8; <pr>xi&#x1CE;ng</pr></trd>,
    <trd>&#x9957; <pr>xi&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="regal.0o">
  <kap>
    <tld/>o, <var><kap><tld/>ado</kap></var>
  </kap>
  <snc mrk="regal.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        sen <tld/>o ne estas balo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> estis kortu&scirc;ita, sed samtempe embarasata, &ccirc;ar tian <tld/>on &scirc;i
        ne atendis
        <fnt>

          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2007/008968.php">Sekretoj de la konfesejo</url></vrk>
          <bib>Monato</bib>
          <lok>2007/02, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        en via malica <tld/>o vin atendas anka&ubreve; pokalo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>ato, tia <tld/>ado
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        realaj problemoj de la reala vivo, ege malproksime de imagitaj <tld/>adoj kaj
        festenoj
        <fnt>

          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010685.php">Fikciaj fi&scirc;oj</url></vrk>
          <bib>Monato</bib>
          <lok>2012/07, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Bewirtung</trd>
  <trdgrp lng="ja">
    <trd>&#x3054;&#x3061;&#x305D;&#x3046;</trd>,
    <trd>&#x3082;&#x3066;&#x306A;&#x3057;</trd>,
    <trd>&#x6B53;&#x5F85;  <pr>&#x304B;&#x3093;&#x305F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">traktatie (het trakteren)</trd>
  <trdgrp lng="pl">
    <trd>przyj&eogonek;cie</trd>,
    <trd>feta</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_g;&#x43E;&c_shch;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&c_ch;&c_a;&c_s;&c_t;&c_u;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x597D;&#x5BA2; <pr>h&#xE0;ok&#xE8;</pr></trd>
</drv>

<drv mrk="regal.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="regal.0ajxo.nutrajxo">
    <dif>
      Tio, kion oni <tld/>as, servas:
      <ekz>
        lardo, tio estis la tuta <tld/>a&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        terpomoj kaj malforta biero fari&gcirc;os <tld/>a&jcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x6B63;&#x9910; <pr>zh&#xE8;ngc&#x101;n</pr></trd>
</snc>
  <trd lng="be">&c_p;&c_a;&c_ch;&c_a;&c_s;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Bewirtung <klr>(Speise)</klr></trd>,
    <trd>Angebot</trd>,
    <trd>Speise</trd>,
    <trd>Mahl</trd>
  </trdgrp>
  <trd lng="nl">traktatie</trd>
  <trd lng="pl">pocz&eogonek;stunek</trd>
  <trd lng="zh">&#x5448;&#x4EA4; <pr>ch&#xE9;ngji&#x101;o</pr></trd>
</drv>

<drv mrk="regal.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="regal.for0i.malmilde">
    <dif>
      Malmilde forpu&scirc;i:
      <ekz>
        kiel bele &scirc;i lin for<tld/>is, kiam &ccirc;iun tagon li faradis al &scirc;i sian proponon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ha! sed vi donis al &scirc;i bonan for<tld/>on!
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rifuz.0i.peton">rifuzi<sncref/></ref>
    <trdgrp lng="de">
      <trd>abservieren</trd>,
      <trd>eine <ind>Abfuhr</ind> erteilen</trd>,
      <trd>einen <ind>Korb</ind> geben</trd>,
      <trd>jemanden hinaus <ind>bef&ouml;rdern</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">odporou&ccaron;et</trd>
  <trdgrp lng="en">
    <trd>to send someone on their way</trd>,
    <trd>show someone the door</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mandar a tomar viento</trd>,
    <trd>ense&ntilde;ar la puerta a alguien para que se vaya</trd>
  </trdgrp>
  <trd lng="ja">&#x624B;&#x3072;&#x3069;&#x304F;&#x3042;&#x3057;&#x3089;&#x3046;  <pr>&#x3066;&#x3072;&#x3069;&#x304F;&#x3042;&#x3057;&#x3089;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>afschepen</trd>,
    <trd>de <ind>bons</ind> geven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyprasza&cacute;</trd>,
    <trd>wyprosi&cacute; za drzwi</trd>
  </trdgrp>
  <trd lng="sk">odpor&uacute;&ccaron;a&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&#x43F;&c_r;&#x43E;&c_s;&c_i;&c_t;&c_i; &c_z;&c_a; &c_d;&c_v;&c_je;&c_r;&c_ib;</trd>
</drv>
</art>
