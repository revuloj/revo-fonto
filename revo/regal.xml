<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>regal</rad>/i
</kap>
<drv mrk="regal.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Bone kaj plezurige <ref tip="super" cel="trakt.0i">trakti</ref> 
      iun, donante al li <ref cel="mangx.0ajxo">man&gcirc;a&jcirc;ojn</ref> kaj 
      <ref cel="trink.0ajxo">trinka&jcirc;ojn</ref>:
      <ekz>
        <tld/>i iun per fruktoj, per glaso da biero, per luksa
        vesperman&gcirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>kia <tld/>ato, tia <tld/>ado
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="taks.0i">taksi</ref>.
    </refgrp>
    <trdgrp lng="bg">
      <trd>&c_ch;&c_je;&c_r;&c_p;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_ch;&c_je;&c_r;&c_p;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bewirten</trd>,
      <trd>festlich bewirten</trd>,
      <trd>freihalten</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>megvend&eacute;gel</trd>,
    <trd>j&oacute;l tart</trd>
  </trdgrp>
    <trd lng="nl">tracteren</trd>
  </snc>
  <snc>
    <dif>
      Bone kaj plezurige trakti iun, liverante al li ion bonan:
      <ekz>
        la artisto <tld/>is nin per du eksterprogramaj
        kantoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la gazetoj deziras nur <tld/>i la legantojn per
        nova&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rekomp.0i">rekompenci</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>erg&ouml;tzen</trd>,
      <trd>erfreuen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ell&aacute;t</trd>,
      <trd>meg&ouml;rvendeztet</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>tracteren</trd>,
      <trd>vermaken</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(ironie)</uzo>
    <dif>
      Malbone trakti:
      <ekz>
        <ind><tld/>i iun per la plato de la glavo</ind><fnt>B</fnt>;
        <trd lng="hu">kardlappal simogat</trd>
      </ekz>
      <ekz>kiu <tld/>as per &scirc;tonoj, tiun oni dankas per bastonoj
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la malestimo, kiun seninduloj <tld/>as al merito
        efektiva<fnt><aut>Zamenhof</aut>:    
            <vrk><url
            ref="http://eo.wikipedia.org/wiki/Hamleto">Hamleto</url></vrk></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pun.0i">puni</ref>,
      <ref cel="pag.re0i">repagi</ref>,
      <ref cel="vengx.0i">ven&gcirc;i</ref>.
    </refgrp>
    <trd lng="de">traktieren</trd>
    <trdgrp lng="hu">
      <trd>ell&aacute;tja a baj&aacute;t</trd>,
      <trd>trakt&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">tracteren</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>entertain</trd>,
    <trd>regale</trd>,
    <trd>treat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_o;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hostiť jedlom</trd>,
    <trd>nápojom</trd>,
    <trd>častovať</trd>
  </trdgrp>
</drv>

<drv mrk="regal.for0i">
  <kap>for<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malmilde forpu&scirc;i.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>afschepen</trd>,
    <trd>de <ind>bons</ind> geven</trd>
  </trdgrp>
  <trd lng="sk">odporúčať</trd>
</drv>
</art>
