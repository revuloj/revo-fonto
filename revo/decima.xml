<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>decimal</rad>/a
</kap>
<drv mrk="decima.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <ref tip="dif" cel="dek.0uma">dekuma.</ref>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
  <trd lng="de">dezimal</trd>
  <trd lng="es">decimal</trd>
  <trd lng="en">decimal</trd>
  <trd lng="fr">d&eacute;cimal</trd>
  <trdgrp lng="hu">
    <trd>tizes<klr tip="ind"> (sz&aacute;mrendszer)</klr></trd>,
    <trd>decim&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>decimaal</trd>,
    <trd>tiendelig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dziesi&eogonek;tny</trd>,
    <trd>dziesi&aogonek;tkowy</trd>,
    <trd>decymalny</trd></trdgrp> 
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">desiatkov&yacute;</trd>
</drv>

<drv mrk="decima.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
      <klr>(en dekuma <ref cel="frakci.pozicia0o">pozicia frakcio</ref>)</klr> 
      &Ccirc;iu el la <ref tip="super" cel="cifer.0o">ciferoj</ref>, aperantaj 
      dekstre de la <ref cel="on.0punkto.komo">onkomo</ref>, t.e. prezentantaj
      la <ref cel="part.frakcia0o">frakcian parton</ref>:
      <ekz>
        kalkulu la kvocienton de <frm>22</frm> per <frm>7</frm> &gcirc;is la 
        kvina <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dezimale</trd>
  <trdgrp lng="en">
    <trd>digit after the radix point</trd>,
    <trd>decimal digit</trd>,
    <trd>decimal</trd>
  </trdgrp>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_l;&c_ib;&c_ch;&c_b;&c_a;</trd>
  <trd lng="es">decimal</trd>
  <trd lng="fr">d&eacute;cimale</trd>
  <trd lng="pl">dziesi&eogonek;tna</trd>
  <trd lng="hu">tizedes <ind>t&ouml;rt</ind></trd>
  <trd lng="nl">decimaal</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_c;&c_i;&c_f;&c_r;&c_a;</ind></trd>
  <trd lng="sk">desatinn&yacute;</trd>
</drv>
</art>
