<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: decima.xml,v 1.37 2023/11/04 13:16:26 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>decimal</rad>/a
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="decima.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc mrk="decima.0a.frakcio">
    <dif>
      <klr>(frakcio)</klr> reprezentata per entjero kaj <tld/>oj:
      <ekz>
        iun kalkulon <klr>[&#x2026;]</klr> mi ne sciis kiel fari, kvankam estis simpla <tld/>a
        frakcio
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la anglan influon oni rimarkas en la uzado de punktoj anstata&ubreve; <tld/>aj komoj
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2008/009567.php">Japanio tra homoj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="decima.0a.dekuma">
    <dif>
      <ref tip="dif" cel="dek.0uma">Dekuma</ref>:
      <ekz>
        la parolataj keltaj lingvoj uzas, en numerado post dek na&ubreve;, ne la <tld/>an
        sistemon, sed la pli antikvan bazitan sur la nombro dudek
        <fnt>
          <bib>Monato</bib>
          <vrk>Matematiko</vrk>
          <lok>93/11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>decim&aacute;ln&iacute;</trd>,
    <trd>desetinn&yacute; (&ccaron;&iacute;slo)</trd>,
    <trd>des&iacute;tkov&yacute; <klr>(syst&eacute;m)</klr></trd>
  </trdgrp>
  <trd lng="de">dezimal</trd>
  <trd lng="en">decimal</trd>
  <trd lng="es">decimal</trd>
  <trd lng="fr">d&eacute;cimal</trd>
  <trd lng="he">&ayin;&shin;&resh;&vav;&nun;&yod;</trd>
  <trdgrp lng="hu">
    <trd>tizes<klr tip="ind"> (sz&aacute;mrendszer)</klr></trd>,
    <trd>decim&aacute;lis</trd>
  </trdgrp>
  <trd lng="ja">&#x5341;&#x9032;&#x6CD5;&#x306E; <pr>&#x3058;&#x3063;&#x3057;&#x3093;&#x307B;&#x3046;&#x306E;</pr></trd>
  <trd lng="ne">&#x926;&#x936;&#x92E;&#x932;&#x935;</trd>
  <trdgrp lng="nl">
    <trd>decimaal<klr tip="ind"> (adj.)</klr></trd>,
    <trd>tiendelig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dziesi&eogonek;tny</trd>,
    <trd>dziesi&aogonek;tkowy</trd>,
    <trd>decymalny</trd></trdgrp>
  <trd lng="pt">decimal</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>desiatkov&yacute;</trd>,
    <trd>desiatkov&yacute; <klr>(syst&eacute;m)</klr></trd>
  </trdgrp>
  <trd lng="sv">decimal<klr> (med talbas tio)</klr></trd>
  <trd lng="uk">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
  <trd lng="zh">&#x5C0F;&#x6570; <pr>xi&#x1CE;osh&#xF9;</pr></trd>
</drv>

<drv mrk="decima.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
      <klr>(en dekuma <ref cel="frakci.pozicia0o">pozicia frakcio</ref>)</klr>
      &Ccirc;iu el la <ref tip="super" cel="cifer.0o">ciferoj</ref>,
      aperantaj dekstre de la <ref cel="on.0komo">onkomo</ref>, t.e.
      prezentantaj la <ref cel="part.frakcia0o">frakcian parton</ref>:
      <ekz>
        kalkulu la kvocienton de <frm>22</frm> per <frm>7</frm> &gcirc;is la
        kvina <tld/>o;
      </ekz>
      <ekz>
        gajnis premion tiu, kiu kapablis enmemorigi kiel eble plej multe da <tld/>oj
        post la komo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011496.php">3-14-15: la jarcenta pi-tago estis celebrita</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_l;&c_ib;&c_ch;&c_b;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>desetinn&eacute; &ccaron;&iacute;slo</trd>,
    <trd>&ccaron;&iacute;slice za desetinnou te&ccaron;kou</trd>
  </trdgrp>
  <trd lng="de">Dezimale</trd>
  <trdgrp lng="en">
    <trd>digit after the radix point</trd>,
    <trd>decimal digit</trd>,
    <trd>decimal</trd>
  </trdgrp>
  <trd lng="es">decimal</trd>
  <trd lng="fr">d&eacute;cimale</trd>
  <trd lng="he">&ayin;&shin;&resh;&vav;&nun;&yod;</trd>
  <trd lng="hu">tizedes <ind>t&ouml;rt</ind></trd>
  <trd lng="ja">&#x5C0F;&#x6570;  <pr>&#x3057;&#x3087;&#x3046;&#x3059;&#x3046;</pr></trd>
  <trd lng="nl">decimaal<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="pl">dziesi&eogonek;tna</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_c;&c_i;&c_f;&c_r;&c_a;</ind></trd>
  <trdgrp lng="sk">
    <trd>desatinn&yacute;</trd>,
    <trd>desatinn&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
  <trd lng="sv">decimal</trd>
  <trdgrp lng="zh">
    <trd>&#x5341;&#x8FDB;&#x5236;&#x5C0F;&#x6570; <pr>sh&#xED;j&#xEC;nzh&#xEC;xi&#x1CE;osh&#xF9;</pr></trd>,
    <trd>&#x5341;&#x8FDB;&#x6570; <pr>sh&#xED;j&#xEC;nsh&#xF9;</pr></trd>,
    <trd>&#x5C0F;&#x6570;&#x4F4D; <pr>xi&#x1CE;osh&#xF9;w&#xE8;i</pr></trd>
  </trdgrp>
</drv>
</art>
