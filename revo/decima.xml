<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: decima.xml,v 1.33 2023/08/31 10:43:58 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>decimal</rad>/a
</kap>
<drv mrk="decima.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <ref tip="dif" cel="dek.0uma">dekuma.</ref>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>decim&#xE1;ln&#xED;</trd>,
    <trd>desetinn&#xFD; (&#x10D;&#xED;slo)</trd>,
    <trd>des&#xED;tkov&#xFD; <klr>(syst&#xE9;m)</klr></trd>
  </trdgrp>
  <trd lng="de">dezimal</trd>
  <trd lng="en">decimal</trd>
  <trd lng="es">decimal</trd>
  <trd lng="fr">d&eacute;cimal</trd>
  <trd lng="he">&#x5E2;&#x5E9;&#x5E8;&#x5D5;&#x5E0;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>tizes<klr tip="ind"> (sz&aacute;mrendszer)</klr></trd>,
    <trd>decim&aacute;lis</trd>
  </trdgrp>
  <trd lng="ja">&#x5341;&#x9032;&#x6CD5;&#x306E;</trd>
  <trd lng="ne">&#x926;&#x936;&#x92E;&#x932;&#x935;</trd>
  <trdgrp lng="nl">
    <trd>decimaal<klr tip="ind"> (adj.)</klr></trd>,
    <trd>tiendelig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dziesi&eogonek;tny</trd>,
    <trd>dziesi&aogonek;tkowy</trd>,
    <trd>decymalny</trd></trdgrp>
  <trd lng="pt">decimal</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>desiatkov&yacute;</trd>,
    <trd>desiatkov&#xFD; <klr>(syst&#xE9;m)</klr></trd>
  </trdgrp>
  <trd lng="sv">decimal<klr> (med talbas tio)</klr></trd>
  <trd lng="uk">&#x434;&#x435;&#x441;&#x44F;&#x442;&#x43A;&#x43E;&#x432;&#x438;&#x439;</trd>
</drv>

<drv mrk="decima.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
      <klr>(en dekuma <ref cel="frakci.pozicia0o">pozicia
      frakcio</ref>)</klr> 
      &Ccirc;iu el la <ref tip="super" cel="cifer.0o">ciferoj</ref>,
      aperantaj 
      dekstre de la <ref cel="on.0komo">onkomo</ref>, t.e.
      prezentantaj
      la <ref cel="part.frakcia0o">frakcian parton</ref>:
      <ekz>
        kalkulu la kvocienton de <frm>22</frm> per <frm>7</frm> &gcirc;is la 
        kvina <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_l;&c_ib;&c_ch;&c_b;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>desetinn&#xE9; &#x10D;&#xED;slo</trd>,
    <trd>&#x10D;&#xED;slice za desetinnou te&#x10D;kou</trd>
  </trdgrp>
  <trd lng="de">Dezimale</trd>
  <trdgrp lng="en">
    <trd>digit after the radix point</trd>,
    <trd>decimal digit</trd>,
    <trd>decimal</trd>
  </trdgrp>
  <trd lng="es">decimal</trd>
  <trd lng="fr">d&eacute;cimale</trd>
  <trd lng="he">&#x5E2;&#x5E9;&#x5E8;&#x5D5;&#x5E0;&#x5D9;</trd>
  <trd lng="hu">tizedes <ind>t&ouml;rt</ind></trd>
  <trd lng="ja">&#x5C0F;&#x6570;  [<ind>&#x3057;&#x3087;&#x3046;&#x3059;&#x3046;</ind>]</trd>
  <trd lng="nl">decimaal<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="pl">dziesi&eogonek;tna</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_c;&c_i;&c_f;&c_r;&c_a;</ind></trd>
  <trdgrp lng="sk">
    <trd>desatinn&yacute;</trd>,
    <trd>desatinn&#xE9; &#x10D;&#xED;slo</trd>
  </trdgrp>
  <trd lng="sv">decimal</trd>
</drv>
</art>
