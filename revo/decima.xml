<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>decimal</rad>/a
</kap>
<drv mrk="decima.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <ref tip="dif" cel="dek.0uma">dekuma.</ref>
  <trd lng="de">dezimal</trd>
  <trd lng="en">decimal</trd>
  <trd lng="fr">d&eacute;cimal</trd>
  <trdgrp lng="nl">
   <trd>decimaal</trd>,
   <trd>tiendelig</trd>
</trdgrp>
<trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="decima.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
      <klr>(en <ref cel="frakci.dekuma0o">dekuma frakcio</ref>)</klr> 
      <ref tip="super" cel="on.0o.cifero">Ono</ref>; alidire: 
      <ref tip="super" cel="cifer.0o">cifero</ref> aperanta dekstre de la
      <ref cel="on.0punkto.komo">onkomo</ref>.
    </dif>
  </snc>
  <trd lng="de">Dezimale</trd>
  <trd lng="en">decimal <ind>digit</ind></trd>
  <trd lng="fr">d&eacute;cimale</trd>
  <trd lng="nl">decimaal</trd>
<trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_c;&c_i;&c_f;&c_r;&c_a;</ind></trd>
</drv>
</art>
