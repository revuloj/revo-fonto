<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: malaki.xml,v 1.24 2025/08/03 22:34:10 revo Exp $">
<kap>
  <ofc>2</ofc><rad>malakit</rad>/o <fnt><bib>JED</bib></fnt>,
  <var><kap><rad var="hx">mala&hcirc;it</rad>/o
  <fnt><bib>Viki</bib></fnt></kap></var>
</kap>
<drv mrk="malaki.0o">
  <kap><ofc>2</ofc><tld/>o, <var><kap><tld var="hx"/>o</kap></var></kap>
  <snc mrk="malaki.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Kupra hidrokarbonato konsistiganta belan verdan polureblan
      &scirc;tonon uzatan por fari juvelojn, ornamajn vazojn kaj ceterajn:
      <ekz>
        uzu oron, ar&gcirc;enton, <tld/>on, turkisojn <klr>...</klr> por &ccirc;i
        tiu
        sinjoro, por ke nenio manku al li
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 4a</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;i ofte aperas kun <tld var="hx"/>o kiel sekundara produkto de kuproercoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37831">Azurito</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_d;&a_h;&a_n;&a_j;</trd>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_h;&c_ib;&c_t;</trd>
  <trd lng="br">malakit</trd>
  <trd lng="ca">malaquita</trd>
  <trd lng="cs">malachit</trd>
  <trd lng="de">Malachit</trd>
  <trd lng="en">malachite</trd>
  <trd lng="es">malaquita</trd>
  <trd lng="fa">&a_m;&a_A;&a_l;&a_A;&a_sh;&f_y;&a_t;</trd>
  <trd lng="fi">malakiitti</trd>
  <trd lng="fr">malachite</trd>
  <trd lng="hu">malachit</trd>
  <trd lng="io">malakito</trd>
  <trdgrp lng="ja">
    <trd>&#x5B54;&#x96C0;&#x77F3;  <pr>&#x304F;&#12376;&#12419;&#x304F;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x30DE;&#x30E9;&#x30AB;&#x30A4;&#x30C8;</trd>
  </trdgrp>
  <trd lng="lfn">malacita</trd>
  <trd lng="nl">malachiet</trd>
  <trd lng="pl">malachit</trd>
  <trd lng="pt">malaquita</trd>
  <trd lng="ro">malachit</trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_a;&c_h;&c_i;&c_t;</trd>
  <trd lng="sk">malachit</trd>
  <trd lng="uk">&#x43C;&c_a;&#x43B;&c_a;&c_h;&c_ib;&c_t;</trd>
</drv>
</art>
