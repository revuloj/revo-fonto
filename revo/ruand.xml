<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Ruand</rad>/o
</kap>

<drv mrk="ruand.R0o">
  <kap>
    <tld lit="R"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="ruand.R0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Kigalio.
      Teritorio de Respubliko Ruando.
      ISO-3166 kodo: RW.
      &Gcirc;i limas kun
      <ref tip="vid" cel="burund.B0o.GEOG">Burundo</ref>
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="ugand.U0o.GEOG">Ugando</ref>
      kaj <ref tip="vid" cel="tanzani.T0o.GEOG">Tanzanio</ref>.
    </dif>
    </snc>
    <trd lng="be">&c_R;&c_u;&c_a;&c_n;&c_d;&c_a;</trd>
    <trd lng="bg">&c_R;&c_u;&c_a;&c_n;&c_d;&c_a;</trd>
    <trd lng="de">Ruanda</trd>
    <trd lng="en">Rwanda</trd>
    <trdgrp lng="fr">
      <trd>Rwanda</trd>,
      <trd>R&eacute;publique du Rwanda</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Rwanda</trd>,
      <trd>Republika <ind>Rwandy</ind></trd>,
      <trd>Ruanda</trd>,
      <trd>Republika <ind>Ruandy</ind></trd>,
    </trdgrp>
    <trd lng="pt">Ruanda</trd>
</drv>

</art>

