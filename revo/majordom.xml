<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>majordom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="majordom.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;ef<ref tip="super" cel="serv.0isto">servisto</ref>, gvidanta la
      internan funkciadon de nobela domo a&ubreve; palaco:
      <ekz>
        la <tld/>o de la duko, viro de burleska kaj sprita humoro
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 36a, p.
        601a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="intend.0o.privata">intendanto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_zh;&c_a;&c_r;&c_d;&c_o;&c_m;</trd>
  <trd lng="fr">majordome</trd>
  <trd lng="hu">h&aacute;znagy</trd>
</drv>
</art>

