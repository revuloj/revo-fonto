<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Abas</rad>/o
    </kap>
    
    <drv mrk="abas.0o">
      <kap>
        <tld/>o
      </kap>
      <snc>
        <uzo tip="fak">NOM</uzo>
        <dif>
           Vira anta&ubreve;nomo arabdevena (la&ubreve;vorte <ctl>severa</ctl>,
           <ctl>malserena</ctl>), uzata precipe en la islama mondo.
        </dif>
      </snc>
      <trd lng="en">Abbas</trd>
      <trd lng="es">Abb&aacute;s</trd>
      <trd lng="hu">Abbasz</trd>
      <trd lng="pt">Abas</trd>
      <trd lng="ru">&c_A;&c_b;&c_b;&c_a;&c_s;</trd>
    </drv>

    <drv mrk="abas.0ido">
      <kap>
        <tld lit="a"/>ido
      </kap>
      <snc>
        <uzo tip="fak">HIS</uzo>
        <dif>
           Ano de la dua dinastio da <ref cel="kalif.0o">&hcirc;alifoj</ref>
           (749&#x2013;1298), rektaj posteuloj de <tld/>o la onklo de
           <ref cel="muhama.0o.profeto">Muhamado</ref>.
        </dif>
      </snc>
      <trd lng="en">Abbasid</trd>
      <trdgrp lng="es">
           <trd>abas&iacute;</trd>,
           <trd>abasida</trd>
      </trdgrp>
      <trd lng="hu">Abbasszida</trd>
      <trd lng="pt">ab&aacute;ssida</trd>
      <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_s;&c_i;&c_d;</trd>
    </drv>

  </art>

