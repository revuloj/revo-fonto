    <trd>&#28082;&#21270;&#12377;&#12427;  <pr>&#12360;&#12365;&#12363;&#12377;&#12427;</pr></trd>,
    <trd>&#28082;&#29366;&#21270;&#12377;&#12427;  <pr>&#12360;&#12365;&#12376;&#12423;&#12358;&#12363;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">vloeibaar maken</trd>
