<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>likv</rad>/a
</kap>
<drv mrk="likv.0a">
  <kap><tld/>a</kap>
  <snc mrk="likv.0a.fluajxa">
  <uzo tip="fak">FIZ</uzo>
    <ref tip="dif" cel="flu.0ajxa.FIZ">flua&jcirc;a</ref>
  </snc>
  <adm>
    PV konsekvence uzas "fluajxo", "flueca" anstataux la pli modernajn
    "likvajxo", "likva". Sxajnas, ke PIV2 preferos "likvo"... Necesas
    havi koheran regulon en Revo: cxu ni restas je la Fundamenta
    "flu" aux transiras al "likv"? <aut>MB</aut>
  </adm>
  <trd lng="be">&c_v;&c_a;&c_d;&c_k;&c_ib;</trd>
  <trd lng="ca">l&iacute;quid</trd>
  <trd lng="de">fl&uuml;ssig</trd>
  <trd lng="el">&ypsilon;&gamma;&rho;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">liquid</trd>
  <trd lng="es">l&iacute;quido</trd>
  <trd lng="fr">liquide<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>foly&eacute;kony</trd>,
    <trd>cseppfoly&oacute;s</trd>
  </trdgrp>
  <trd lng="nl">vloeibaar</trd>
  <trdgrp lng="pt">
    <trd>l&iacute;quido <klr>(adj.)</klr></trd>,
    <trd>l&iacute;q&uuml;&iacute;do <klr>(adj.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="likv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="likv.0ajxo.fluajxo">
  <uzo tip="fak">FIZ</uzo>
    <ref tip="dif" cel="flu.0ajxo.FIZ">flua&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">l&iacute;quid</trd>
  <trd lng="de">Fl&uuml;ssigkeit</trd>
  <trd lng="el">&ypsilon;&gamma;&rho;&omikron_ton;</trd>
  <trd lng="en">liquid</trd>
  <trd lng="es">l&iacute;quido</trd>
  <trd lng="fr">liquide<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">folyad&eacute;k</trd>
  <trd lng="nl">vloeistof</trd>
  <trdgrp lng="pt">
    <trd>l&iacute;quido <klr>(subst.)</klr></trd>,
    <trd>l&iacute;q&uuml;&iacute;do <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="tp">telo</trd>
  <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="likv.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="likv.0igi.FIZ">
    <dif>
      Meti en <tld/>an staton.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_w;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;
&c_u; <ind>&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>,
    <trd>&c_z;&c_v;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">liquar</trd>
  <trd lng="de">verfl&uuml;ssigen</trd>
  <trdgrp lng="el">
    <trd>&tau;&eta_ton;&kappa;&omega;</trd>,
    <trd>&ypsilon;&gamma;&rho;&omikron;&pi;&omikron;&jota;&omega_ton;</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>liquefy</trd>, <trd>liquify</trd></trdgrp>
  <trd lng="es">licuar</trd>
  <trd lng="fr">liqu&eacute;fier</trd>
  <trdgrp lng="hu">
    <trd>cseppfoly&oacute;s&iacute;t</trd>,
    <trd>elfoly&oacute;s&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">vloeibaar maken</trd>
  <trdgrp lng="pt">
    <trd>l&iacute;equefazer</trd>,
    <trd>l&iacute;q&uuml;&iacute;efazer</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;
&c_v; &c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
