<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kapitu.xml,v 1.24 2018/12/25 15:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapitulac</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kapitu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kapitu.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Venkite transdoni sin kaj sian armeon a&ubreve; fortika&jcirc;ojn
      al la
      malamiko, je precizaj kondi&ccirc;oj fiksitaj kune kun li:
      <ekz>
        la sie&gcirc;itoj <tld/>is post kvar monatoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kapitulieren</trd>,
      <trd><klr>sich </klr>ergeben</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>capitulate</trd>,
      <trd>surrender</trd>
    </trdgrp>
    <trd lng="es">capitular</trd>
    <trdgrp lng="nl">
      <trd>capituleren</trd>,
      <trd>zich <ind>overgeven</ind></trd>
    </trdgrp>
    <trd lng="pl">kapitulowa&cacute;</trd>     
    <trd lng="ro">capitula</trd>
  </snc>
  <snc mrk="kapitu.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rezigni, forlasi entreprenon:
      <ekz>
        Don Juan post kvardeko certe devus <tld/>i pli ol unu
        fojon<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_t;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>aufgeben</trd>,
      <trd>resignieren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>capitulate</trd>,
      <trd>give up</trd>,
      <trd>resign</trd>
    </trdgrp>
    <trd lng="es">rendirse</trd>
    <trdgrp lng="nl">
      <trd>opgeven</trd>,
      <trd>aftreden</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kapitulowa&cacute;</trd>,
      <trd>podda&cacute; si&eogonek;</trd>
    </trdgrp> 
     <trd lng="ro">capitula</trd>  
  </snc>
  <trd lng="cs">kapitulovat</trd>
  <trd lng="de">kapitulieren</trd>
  <trd lng="fa">&a_t;&a_s;&a_l;&f_y;&a_m; &a_sh;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>capituler</trd>,
    <trd>abandonner<klr tip="ind"> (renoncer)</klr></trd>,
    <trd>d&eacute;poser les <ind>arme</ind>s</trd>,
    <trd>s'<ind>incliner</ind><klr tip="ind"> (renoncer)</klr></trd>,
    <trd>renoncer</trd>,
    <trd>se <ind>rendre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megadja mag&aacute;t</trd>,
    <trd>kapitul&aacute;l</trd>
  </trdgrp>
  <trd lng="pl">podda&cacute; si&eogonek;</trd>     
  <trd lng="ro">capitula</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_p;&c_i;&c_t;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kapitulova&tcaron;</trd>,
    <trd>podda&tcaron; sa</trd>,
    <trd>vzda&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="kapitu.0o">
  <kap><tld/>o</kap>
  <snc mrk="kapitu.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Agnosko de venkiteco, milita
      <ref tip="super" cel="rezign.0o.KOMUNE">rezigno</ref>:
      <ekz>
        senkondi&ccirc;a <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>capitulaci&oacute;n</trd>,
    <trd>rendici&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>capitulation<klr> (reddition)</klr></trd>,
    <trd>reddition</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kapitulacja</trd>,
    <trd>poddanie si&eogonek;</trd>
  </trdgrp>   
  <trd lng="ro">capitulare</trd>
</drv>
</art>
