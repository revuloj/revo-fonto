<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mu.xml,v 1.8 2026/02/26 22:52:42 revo Exp $">
<kap>
  <rad>mu</rad>, <var><kap><rad var="uu">muu</rad></kap></var>
</kap>
<drv mrk="mu.0">
  <kap><tld/>, <var><kap><tld var="uu"/> <fnt><bib>NePiv</bib></fnt></kap></var></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="mu.0.SON">
     <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de bovo a&ubreve; simila sono:
      <ekz>
        unu bovino mu&gcirc;as <tld/>
        <fnt>
          <aut>Babelo Filmoj</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_gh9w7QpyQtY/">
          Dek bovinoj</url></vrk>
          <lok>2016</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bovo ekmu&gcirc;is: <tld var="uu"/>-<tld var="uu"/>-<tld var="uu"/>
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun., J. Horvath</aut>,
          <vrk><url ref="http://www.vortaro.hu/mindenesszotar/jozohor/doros.htm">
          Fabloj kaj Aforismoj</url></vrk>,
          <lok>[2002]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;!</trd>
  <trd lng="en">moo <klr>(onomatopaeia)</klr></trd>
  <trd lng="fr">meuh !</trd>
  <trd lng="ja">&#12511;&#12517;&#12540;</trd>
  <trd lng="pl">muuu</trd>
</drv>
</art>

