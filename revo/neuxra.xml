<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: neuxra.xml,v 1.17 2021/03/08 22:54:32 revo Exp $">
<kap>
  <rad>ne&ubreve;ralgi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="neuxra.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Forta <ref tip="lst" cel="dolor.0o" lst="voko:malsanoj">doloro</ref> 
      sentata &ccirc;e la nervtrairo:
      <ekz>
        zostero estas <klr>[&#8230;]</klr> karakterizata de herpetforma blaza erupcio la&ubreve;longe
        de la sensaj nervoj kaj akompanata de <klr>[&#8230;]</klr> akra <tld/>a (la&ubreve;longe
        de la nerva irejo) kontinua doloro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=226306"
              >Zostero (medicino)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_r;&c_a;&c_l;&c_mol;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">neuralgia</trd>
  <trdgrp lng="de">
    <trd>Neuralgie</trd>,
    <trd>Nervenschmerz</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&ypsilon;&rho;&alfa;&lambda;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="en">neuralgia</trd>
  <trd lng="es">neuralgia</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&a_S;&a_b;&zwnj;&a_d;&a_r;&a_d;</trd>,
    <trd>&a_d;&a_r;&a_d; &a_A;&a_ayn;&a_S;&a_A;&a_b;</trd>,
    <trd>&a_n;&a_w;&a_r;&a_A;&a_l;&f_zh;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;vralgie</trd>
  <trdgrp lng="hu">
    <trd>idegf&aacute;j&aacute;s</trd>,
    <trd>idegf&aacute;jdalom</trd>,
    <trd>idegzs&aacute;ba</trd>,
    <trd>neuralgia</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>neuralgia</trd>,
    <trd><ind>nyeri</ind> urat saraf</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zenuwpijn</trd>,
    <trd>neuralgie</trd>
  </trdgrp>
  <trd lng="pl">neuralgia</trd>
  <trdgrp lng="pt">
    <trd>neuralgia</trd>,
    <trd>nevralgia</trd>
  </trdgrp>
  <trd lng="ro">nevralgie</trd>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_a;&c_l;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
