<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ne&ubreve;ralgi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="neuxra.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Forta doloro sentata &ccirc;e la nervtrairo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_r;&c_a;&c_l;&c_mol;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Neuralgie</trd>,
    <trd>Nervenschmerz</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&ypsilon;&rho;&alfa;&lambda;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="en">neuralgia</trd>
  <trdgrp lng="hu">
    <trd>idegf&aacute;j&aacute;s</trd>,
    <trd>idegf&aacute;jdalom</trd>,
    <trd>idegzs&aacute;ba</trd>,
    <trd>neuralgia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zenuwpijn</trd>,
    <trd>neuralgie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>neuralgia</trd>,
    <trd>nevralgia</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_a;&c_l;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
