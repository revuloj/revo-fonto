<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anod</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="anod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Pozitiva
      <ref tip="super" cel="elektr3.0o">elektrodo</ref> de
      <ref tip="malprt" cel="pil.0o">pilo</ref>,
      <ref tip="malprt" cel="valv.0o.ELE">valvo</ref> a&ubreve; alia
      elektra aparato.
    </dif>
    <refgrp tip="ant">
      <ref cel="katod.0o">katodo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Pluspol</trd>,
    <trd>Anode</trd>
  </trdgrp>
  <trd lng="en">anode</trd>
  <trd lng="es">&aacute;nodo</trd>
  <trd lng="fr">anode</trd>
  <trd lng="hu">an&oacute;d</trd>
  <trd lng="nl">anode</trd>
  <trd lng="ru">&c_a;&c_n;&c_o;&c_d;</trd>
  <trd lng="sk">an&oacute;da</trd>
  <trd lng="sv">anod</trd>
</drv>
</art>
