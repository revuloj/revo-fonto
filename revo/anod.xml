<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anod.xml,v 1.21 2021/02/28 16:37:32 revo Exp $">
<kap>
  <rad>anod</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="anod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Pozitiva
      <ref tip="super" cel="elektr3.0o">elektrodo</ref> de
      <ref tip="malprt" cel="pil.0o">pilo</ref>,
      <ref tip="malprt" cel="valv.0o.ELE">valvo</ref> a&ubreve; alia
      elektra aparato:
      <ekz>
        foto&ccirc;elo <klr>[&#x2026;]</klr> similas al elektrontuboj, &ccirc;ar en la vakua vitro&ccirc;elo
        trovi&gcirc;as <tld/>o kaj katodo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39616">Foto&ccirc;elo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="katod.0o">katodo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_o;&c_d;</trd>
  <trd lng="bg"/>
  <trdgrp lng="cs">
    <trd>anoda</trd>,
    <trd>kladn&aacute; elektroda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pluspol</trd>,
    <trd>Anode</trd>
  </trdgrp>
  <trd lng="en">anode</trd>
  <trd lng="es">&aacute;nodo</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_n;&a_d;</trd>,
    <trd>&a_q;&a_T;&a_b; &a_m;&a_th;&a_b;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">anode</trd>
  <trd lng="he">&#x5D0;&#x5B8;&#x5E0;&#x5D5;&#x5B9;&#x5D3;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">an&oacute;d</trd>
  <trd lng="nl">anode</trd>
  <trd lng="pl">anoda</trd>
  <trd lng="ru">&c_a;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>an&oacute;da</trd>,
    <trd>an&oacute;da</trd>
  </trdgrp>
  <trd lng="sv">anod</trd>
</drv>
</art>
