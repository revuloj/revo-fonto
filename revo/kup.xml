<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kup.xml,v 1.21 2023/12/03 09:59:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kup</rad>/o
</kap>
<drv mrk="kup.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kup.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malgranda klo&scirc;eto el vitro, kiun oni almetas sur parton de la
      korpo,
      kaj en kiu oni maldensigas la aeron, por allevi la ha&ubreve;ton, kaj
      altiri tien la sangon.
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_mol;&c_k;&c_a; <klr tip="amb">(&c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>      
    <trd lng="br">mandoz<klr tip="ind"> (kloc'hig gwer)</klr></trd>
    <trd lng="de">Schr&ouml;pfkopf</trd>
    <trd lng="en">cupping glass</trd>      
    <trd lng="fr">ventouse<klr> (en verre)</klr></trd>
    <trd lng="hu">k&ouml;p&ouml;ly</trd>
    <trdgrp lng="nl">
      <trd>laatkop</trd>,
      <trd>zuignap</trd>
    </trdgrp>
    <trd lng="pl">ba&nacute;ka <klr>(med.)</klr></trd>     
    <trd lng="ro">balon</trd>
    <trd lng="ru">&c_b;&c_a;&c_n;&c_k;&c_a; <klr>(&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
    <trd lng="zh">&#x62D4;&#x7F50; <pr>b&#xE1;gu&#xE0;n</pr></trd>
</snc>
  <snc mrk="kup.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ekstera organeto klo&scirc;forma de diversaj bestoj,
      kiu per muko a&ubreve; elpelo de aero faciligas fiksi&gcirc;on
      a&ubreve; alsu&ccirc;on:
      <ekz>
        la vermo trafis &scirc;ian nudan brakon per urtika <tld/>o
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.0ilo.ZOO">su&ccirc;ilo<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_s;&c_o;&c_s;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_s;&c_o;&c_s;&c_a;&c_k;</trd>,
      <trd>&c_s;&c_y;&c_s;&c_o;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Saugnapf</trd>
    <trdgrp lng="fr">
      <trd>cupule</trd>,
      <trd>ventouse</trd>
    </trdgrp>
    <trd lng="hu">tapad&oacute;korong</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5438;&#x3044;&#x7389;  <pr>&#x3059;&#x3044;&#x305F;&#x307E;</pr></trd>,
    <trd>&#x5438;&#x76E4;  <pr>&#x304D;&#x3085;&#x3046;&#x3070;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&#x43D;&#x43A;&c_a; (&#x43C;&c_je;&c_d;&c_i;&c_ch;&#x43D;&c_a;)</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_s;&#x43E;&c_s;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
