<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>persekut</rad>/i
</kap>
<drv mrk="persek.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="persek.0i.postkuri">
    <dif>
      Kursekvi por atingi a&ubreve; malamike trakti:
      <ekz>
        la &ccirc;asisto <tld/>as leporon;
      </ekz>
      <ekz>
        rabobirdoj <tld/>is la kolombojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        mort<tld/>i cervon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel.0i">peli</ref>,
      <ref cel="kur.post0icxas0i">postkuri</ref>.
    </refgrp>
    <trd lng="fr">poursuivre</trd>
  </snc>
  <snc mrk="persek.0i.turmenti">
    <dif>
      Konstante kruele turmenti:
      <ekz>
        la kristanoj estis <tld/>ataj;
      </ekz>
      <ekz>
        la registaro <tld/>adis la gvidantojn de la opozicio.
      </ekz>
    </dif>
    <trd lng="fr">pers&eacute;cuter</trd>
  </snc>
  <snc mrk="persek.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Fari al iu proceson, voki iun anta&ubreve; ju&gcirc;iston:
      <ekz>
        <tld/>i debitoron.
      </ekz>
    </dif>
    <trd lng="fr">poursuivre en justice</trd>
  </snc>
  <snc mrk="persek.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konstante maltrankviligi iun:
      <ekz>
        en tiu lo&gcirc;ejo doloraj rememoroj <tld/>as min;
      </ekz>
      <ekz>
        la registaron &ccirc;iam <tld/>is timo pri ribelo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="siegx.0i.FIG">sie&gcirc;i <sncref/></ref>.
    </refgrp>
    <trd lng="fr">pers&eacute;cuter <klr>(fig.)</klr></trd>
  </snc>
  <trd
lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">verfolgen</trd>
  <trd lng="en">persecute</trd>
  <trdgrp lng="nl">
    <trd>vervolgen</trd>,
    <trd>achtervolgen</trd>
  </trdgrp>
  <trd
lng="ru">&c_p;&c_r;&c_je;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="persek.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="persek.0isto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficisto komisiita por plenumi la decidojn de tribunalo.
    </dif>
  </snc>
  <trd lng="de">Gerichtsvollzieher</trd>
  <trd lng="fr">huissier</trd>
  <trd lng="nl">deurwaarder</trd>
  <trd lng="ru">&c_s;&c_u;&c_d;&c_je;&c_b;&c_n;&c_y;&c_j;
&c_i;&c_s;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
