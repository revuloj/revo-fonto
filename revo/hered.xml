    <trd>lasciare in <ind>eredit&agrave;</ind></trd>,
    <trd>far <ind>testamento</ind> (in favore di)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x76F8;&#x7D9A;&#x3055;&#x305B;&#x308B;  <pr>&#x305D;&#x3046;&#x305E;&#x304F;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x53D7;&#x3051;&#x7D99;&#x304C;&#x305B;&#x308B;  <pr>&#x3046;&#x3051;&#x3064;&#x304C;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x907A;&#x4F1D;&#x3055;&#x305B;&#x308B;  <pr>&#x3044;&#x3067;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>
