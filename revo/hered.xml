<drv mrk="hered.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hered.0igi.JUR">
    <dif>
      Fari, ke iu <tld/>u ion:
      <ekz>
        <tld/>igi ion al iu;
      </ekz>
      <ekz>
        <tld/>igi iun pri io;
      </ekz>
      <ekz>
        posedu &ccirc;i tiun bonan landon kaj <tld/>igu &gcirc;in al viaj
        infanoj post vi
        <fnt><bib>MT</bib>, <lok>&Kr1; 28:8</lok></fnt>;
      </ekz>
      <ekz>
        bonulo <tld/>igas la nepojn
        <fnt><bib>MT</bib>, <lok>&Sen; 13:22</lok></fnt>;
      </ekz>
      <ekz>
        mi al ili <tld/>igis la honton samtempe kun la mono
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_c;&c_mol; &c_u; <ind>&c_s;&c_p;&c_a;&c_d;&c_ch;&c_y;&c_n;&c_u;</ind> <klr tip="amb">(&c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_p;&c_a;&c_d;&c_ch;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_m;</ind> <klr tip="amb">(&c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
