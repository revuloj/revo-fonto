  <trd lng="ru">&c_l;&c_i;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_n;&c_a;&c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trd lng="sk">vydedi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_z;&c_b;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_s;&c_p;&c_a;&c_d;&c_shch;&c_i;&c_n;&c_i;</trd>,
