<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: okup.xml,v 1.28 2022/03/05 16:40:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>okup</rad>/i <fnt><bib>VdE</bib></fnt>
</kap>
<drv mrk="okup.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="okup.0i.preni">
    <dif>
      <ref tip="super" cel="pren.0i">Preni</ref> en posedon iun lokon:
      <ekz>
        ili venis en la urbon kaj <tld/>is &gcirc;in, kaj rapide ekbruligis la urbon per fajro
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 8:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili iris <klr>[&#8230;]</klr> kaj <tld/>is la transirejon de <nom>Jordan</nom> <klr>[&#8230;]</klr> kaj
        permesis al neniu transiri
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 3:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        alia <klr>[&#8230;]</klr> <tld/>os lian lokon
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 4:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ramzes XIII-a</nom> ek<tld/>is la tronon kaj ornamis sian 
        frunton per <nom>ureus</nom>
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la malamiko <tld/>us defendan pozicion, mi ne movi&gcirc;us
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        printempaj vegeta&jcirc;oj <tld/>as la lokon de la mallevi&gcirc;antaj akvoj
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        paseroj <tld/>is la neston de la cikonioj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia &gcirc;eneva gazeto certe &gcirc;ojis presi dekon da artikoloj pri
        <tld/>ata Polujo
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 34a, p. 82a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akir.0i">akiri</ref>,
      <ref cel="okup.ek0i">ekokupi</ref>,
      <ref cel="instal.0i.iun">instali sin</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="okupaci.0i">okupacii</ref>,
      <ref cel="uzurp.0i">uzurpi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_k;&c_u;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>besetzen</trd>,
      <trd>einnehmen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elfoglal</trd>,
      <trd>bevesz</trd>
    </trdgrp>    
    <trd lng="nl">bezetten</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_k;&c_k;&c_u;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="okup.0i.lokigxi">
    <dif>
      <ref tip="super" cel="ten.0i.enpove">Teni</ref> en posedo iun 
      lokon a&ubreve; tempon; da&ubreve;re loki&gcirc;i ie:
      <ekz>
        la akvo <tld/>is la teron dum cent kvindek tagoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 7:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efan kaj plej bonan lokon <tld/>is maljuna dika viandisto
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arta lago <nom>Moeris</nom> <tld/>is 300 kilometrojn kvadratajn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; sia malalta popola deveno, li <tld/>is gravan postenon en la &scirc;tato,
        dank' al esceptaj kapabloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        voja&gcirc;antoj <tld/>is tri eta&gcirc;ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        tablo, &scirc;ar&gcirc;ita per paperoj <klr>[&#8230;]</klr> <tld/>is la tutan spacon inter du
        fenestroj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i sen celo lokon sub &ccirc;ielo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi anka&ubreve; lokon sur la tero <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kovr.0i.kasxi">kovri<sncref/></ref>,
      <ref cel="kusx.0i.okupi_lokon">ku&scirc;i<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>einnehmen</trd>,
      <trd>versperren</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>foglal</trd>,
      <trd>lefoglal</trd>
    </trdgrp>
    <trd lng="nl">in <ind>beslag</ind> nemen</trd>
    <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="okup.0i.laborigi">
    <dif>
      <ref tip="super" cel="tir.0i.logi">Tiri</ref> al si la atenton, 
      intereson a&ubreve; aktivecon de iu:
      <ekz>
        kiu <tld/>as sin je &hcirc;emio, estas &hcirc;emiisto
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>as min per aferoj <klr>[&#8230;]</klr> neatingeblaj por mi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 131:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj, kiuj sin <tld/>as pri sanktaj aferoj, man&gcirc;as el la templaj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 9:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        profunda enpensi&gcirc;o <tld/>is lian menson
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        lin tre vigle <tld/>is la penso, kiajn profitojn oni havus
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u vin per la grekaj regimentoj kaj kiel eble plej balda&ubreve;
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tutan tagon <klr>[&#8230;]</klr> la multe<tld/>ita generacio dedi&ccirc;as al la grandaj
        Anglujo kaj Skotujo
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia amo al <nom>Else</nom> plene lin <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxen.0i">&gcirc;eni</ref>,
      <ref cel="klopod.0i">klopodi</ref>,
      <ref cel="posed.0i">posedi</ref>,
      <ref cel="zorg.0i">zorgi</ref>
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>sich <ind>besch&auml;ftigen</ind> (mit)</trd>,
      <trd><ind>eingenommen</ind> sein (von)</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>foglalkoztat</trd>,
    <trd>elfoglal</trd>
  </trdgrp>
    <trd lng="nl">bezig houden</trd>
    <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <rim>
    &Z; atentigas, ke &leftquot;la senco de <ekz><tld/>i</ekz> havas du nuancojn:
    preni en posedon kaj teni en posedo; per la formo
    <ekz>mi estas <tld/>ita</ekz> ni esprimas samtempe amba&ubreve; nuancojn,
    dum la formo <ekz>mi estas <tld/>ata</ekz> esprimus nur la unuan
    nuancon. <klr>[&#8230;]</klr> La kutimo de la popoloj <klr>[&#8230;]</klr>
    postulas, ke ni diru
    <ekz>mi estas <tld/><em>i</em>ta</ekz>&rightquot;
    <fnt><bib>LR</bib></fnt>.
  </rim>
  <trd lng="en">occupy</trd>
  <trd lng="es">ocupar</trd>
  <trd lng="fr">occuper</trd>
  <trd lng="pl">zajmowa&cacute;</trd>
  <trd lng="pt">ocupar</trd>
  <trdgrp lng="sk">
    <trd>prideli&tcaron;</trd>,
    <trd>priradi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obsadit</trd>,
    <trd>okupovat</trd>,
    <trd>zabrat</trd>,
    <trd>zam&ecaron;stnat</trd>,
    <trd>zast&aacute;vat</trd>,
    <trd>zaujmout</trd>,
    <trd>zmocnit se</trd>
  </trdgrp>
</drv>

<drv mrk="okup.0o">
  <kap><tld/>o</kap>
  <snc mrk="okup.0o.gxenerale">
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        havi nenian <tld/>on<fnt>B</fnt>;
      </ekz><ekz>
        la kantado estas agrabla <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rol.0o">rolo</ref>,
      <ref cel="task.0o">tasko</ref>
    </refgrp>
    <trd lng="en">occupation</trd>
    <trd lng="ru">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="okup.0o.perlabora">
    <dif>
      Da&ubreve;ra <ref cel="labor.0o">laboro</ref> per kiu oni sin vivtenas:
      <ekz>
        Mi perdis la <tld/>on en la kudrejo, en kiu mi laborenspezadis
        dudek spesdekojn por tago
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.0o">ofico</ref>,
      <ref cel="labor.0loko">laborloko</ref>,
      <ref cel="dung.0ito">dungito</ref>,
      <ref cel="meti.0o">metio</ref>,
      <ref cel="profes.0o">profesio</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>employment</trd>,
      <trd>business</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>povol&aacute;n&iacute;</trd>,
    <trd>zab&yacute;v&aacute;n&iacute; se</trd>,
    <trd>zam&ecaron;stn&aacute;n&iacute;</trd>,
    <trd>zam&ecaron;stn&aacute;n&iacute;</trd>,
    <trd>zaujatost <klr>(z&aacute;jem)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Besch&auml;ftigung</trd>,
    <trd>Besetzung</trd>
  </trdgrp>
  <trd lng="es">ocupaci&oacute;n</trd>
  <trd lng="fr">occupation</trd>
  <trdgrp lng="hu">
    <trd>foglalkoz&aacute;s</trd>,
    <trd>elfoglalts&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">bezigheid</trd>
  <trd lng="pl">zaj&eogonek;cie</trd>
  <trd lng="pt">ocupa&ccedil;&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>povolanie</trd>,
    <trd>zamestnanie</trd>,
    <trd>zaoberanie sa</trd>,
    <trd>zaujatos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="okup.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        la efektiva posedo rezultas de da&ubreve;ra <tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>obsazen&iacute;</trd>,
    <trd>okupace</trd>,
    <trd>zabr&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Besch&auml;ftigen</trd>,
    <trd>Besetzen</trd>
  </trdgrp>
  <trd lng="fr">occupation</trd>
  <trdgrp lng="hu">
    <trd>elfoglal&aacute;s</trd>,
    <trd>foglalkoztat&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">bezetting</trd>
  <trd lng="pl">zaj&eogonek;cie</trd>
  <trd lng="pt">ocupa&ccedil;&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>okup&aacute;cia</trd>,
    <trd>zabratie</trd>
  </trdgrp>
</drv>

<drv mrk="okup.0igxo0ateco">
  <kap><tld/>i&gcirc;o, <var><kap><tld/>ateco</kap></var></kap>
  <snc>
    <dif>
      Stato de iu <tld/>ata.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">zam&ecaron;stnanost</trd>
  <trdgrp lng="de">
    <trd>Besch&auml;ftigtsein</trd>
  </trdgrp>
  <trd lng="fr">indisponibilit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>foglalkoz&aacute;s</trd>,
    <trd>elfoglalts&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">beleg<klr tip="ind"> (bezetting)</klr></trd>
  <trdgrp lng="pl">
    <trd>zaj&eogonek;to&sacute;&cacute;</trd>,
    <trd>zaj&eogonek;cie</trd>
  </trdgrp>
  <trd lng="pt">ocupa&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zamestnanos&tcaron;</trd>,
    <trd>zanepr&aacute;zdnenos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="okup.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin <tld/>i pri io.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd><klr>sich </klr>besch&auml;ftigen (mit)</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>occuper</ind> de</trd>
  <trd lng="hu">foglalkozik</trd>  
  <trd lng="nl">zich <ind>bezig</ind> houden met</trd>
  <trd lng="pl">zajmowa&cacute; si&eogonek;</trd>
  <trd lng="pt">ocupar-se</trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="okup.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="O"/>i 1.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>obsadit</trd>,
    <trd>zaujmout postaven&iacute;</trd>,
    <trd>zmocnit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beginnen, sich (mit etwas) zu <ind>besch&auml;ftigen</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elfoglal</trd>,
    <trd>birtokba vesz</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>meester</ind> maken van</trd>
  <trd lng="pl">zaj&aogonek;&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">zauja&tcaron; postavenie</trd>
</drv>

<drv mrk="okup.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forlasi <tld/>itan lokon:
      <ekz>
        post tri jaroj la trupoj mal<tld/>is la venkitan landon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="evaku.0i">evakui</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>r&auml;umen</trd>,
    <trd>entsetzen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;vacuer</trd>,
    <trd>se <ind>retirer</ind> de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&uuml;r&iacute;t</trd>,
    <trd>evaku&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">ontruimen</trd>
  <trd lng="pl">zwalnia&cacute;</trd>
  <trd lng="pt">desocupar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_v;&c_o;&c_b;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_o;&c_k;&c_k;&c_u;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>opusti&tcaron;</trd>,
    <trd>vypr&aacute;zdni&tcaron;</trd>,
    <trd>zanecha&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="okup.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Havanta nenian <tld/>i&gcirc;on:
      <ekz>
        Mi duone sidis sur la angulo de granda tablo, <klr>[...]</klr>
        kun senkura&gcirc;a kaj sen<tld/>a mieno
        <fnt>
          <aut>Alain Fournier, trad. Roger Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 121</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
