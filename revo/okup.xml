<drv mrk="okup.0igxo0ateco">
  <kap><tld/>i&gcirc;o, <var><kap><tld/>ateco</kap></var></kap>
  <snc>
    <dif>
      Stato de iu <tld/>ata:
      <ekz>
        eliras homo por sia <tld/>i&gcirc;o, por sia laboro &gcirc;is la vespero
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 104:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la diverseco de la profesioj kaj aliaj <tld/>i&gcirc;oj ka&ubreve;zas la lingvan heterogenecon
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malaltigo de salajroj malaltigas la postuladon, la konsumadon kaj la
        <tld/>atecon en Japanio
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011179.php">Abenomiko, a&ubreve;daca defio</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro sia troa <tld/>ateco <klr>[&#x2026;li]</klr> ne plu deziris esti prezidanto de la
        filio de LEA
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 6 (104)</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi preferis silenti kaj nur da&ubreve;rigi la disdonon de la flavaj &scirc;toneroj, pli poste
        mi sukcesis konati&gcirc;i e&ccirc; kun pli fantaziaj <tld/>i&gcirc;oj
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">zam&ecaron;stnanost</trd>
  <trdgrp lng="de">
    <trd>Besch&auml;ftigung</trd>,
    <trd>Besch&auml;ftigtsein</trd>
  </trdgrp>
  <trd lng="fr">indisponibilit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>foglalkoz&aacute;s</trd>,
    <trd>elfoglalts&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F93;&#x4E8B;  [<ind>&#x3058;&#x3085;&#x3046;&#x3058;</ind>]</trd>,
    <trd>&#x5F93;&#x696D;  [<ind>&#x3058;&#x3085;&#x3046;&#x304E;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x5C02;&#x5FF5;  [<ind>&#x305B;&#x3093;&#x306D;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">beleg<klr tip="ind"> (bezetting)</klr></trd>
  <trdgrp lng="pl">
    <trd>zaj&eogonek;to&sacute;&cacute;</trd>,
    <trd>zaj&eogonek;cie</trd>
  </trdgrp>
  <trd lng="pt">ocupa&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zamestnanos&tcaron;</trd>,
    <trd>zanepr&aacute;zdnenos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&c_j;&#x43C;&c_a;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
</drv>

<drv mrk="okup.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin <tld/>i pri io:
      <ekz>
        Franjo <tld/>i&gcirc;is pri la tema&scirc;ino, kaj mi iom legis
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        en nia magazeno virinoj neniam <tld/>i&gcirc;as per vendado de komerca&jcirc;oj, tion faras
        nur viroj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj pensoj komencas <tld/>i&gcirc;i pri ili mem anstata&ubreve; pri iliaj diskutoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la cezaro <klr>[&#x2026;]</klr> nevolonte <tld/>i&gcirc;as per fremdaj aferoj, eble e&ccirc; ne
        volos helpi
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>i&gcirc;as pri spiritismo
        <fnt>
          <bib>IKr</bib>
          <lok>Rakonto Pri Spiritoj</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="labor.0i">labori</ref>,
      <ref cel="zorg.0i">zorgi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="de"><klr>sich </klr>besch&auml;ftigen <klr>(mit)</klr></trd>
  <trd lng="fr">s'<ind>occuper</ind> de</trd>
  <trd lng="hu">foglalkozik</trd>  
  <trdgrp lng="ja">
    <trd>&#x5C02;&#x5FF5;&#x3059;&#x308B;  [<ind>&#x305B;&#x3093;&#x306D;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5FD9;&#x6BBA;&#x3055;&#x308C;&#x308B;  [<ind>&#x307C;&#x3046;&#x3055;&#x3064;&#x3055;&#x308C;&#x308B;</ind>]</trd>,
    <trd>&#x5F93;&#x4E8B;&#x3059;&#x308B;  [<ind>&#x3058;&#x3085;&#x3046;&#x3058;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>bezig</ind> houden met</trd>
  <trd lng="pl">zajmowa&cacute; si&eogonek;</trd>
  <trd lng="pt">ocupar-se</trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="uk">&c_z;&c_a;&c_j;&#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="okup.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="O"/>i<sncref ref="okup.0i.preni"/>:
      <ekz>
        <nom>Ramzes XIII-a</nom> ek<tld/>is la tronon kaj ornamis sian 
        frunton per <nom>ureus</nom>
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Iljon</nom> kiun oni sie&gcirc;as kaj kiun oni ne ek<tld/>as
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>obsadit</trd>,
      <trd>zaujmout postaven&iacute;</trd>,
      <trd>zmocnit se</trd>
    </trdgrp>
    <trd lng="de">besetzen</trd>
    <trdgrp lng="hu">
      <trd>elfoglal</trd>,
      <trd>birtokba vesz</trd>
    </trdgrp>
    <trd lng="nl">zich <ind>meester</ind> maken van</trd>
    <trd lng="pl">zaj&aogonek;&cacute; si&eogonek;</trd>
    <trd lng="ru">&c_z;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
    <trd lng="sk">zauja&tcaron; postavenie</trd>
  </snc>
  <snc mrk="okup.ek0i.subite">
    <dif>
      Subite <tld/>i<sncref ref="okup.0i.laborigi"/>:
      <ekz>
        iru do &gcirc;in ser&ccirc;i, kaj rapidu, ekkriis Fernando, kiun kolero ek<tld/>is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in ek<tld/>is tiu nedifinebla maltrankviligo, kiun ni sentas, kiam iu nin fikse
        rigardas
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste li ree ek<tld/>is sin pri siaj aferoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de"><ind>befallen</ind> werden</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_u;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5360;&#x53D6;&#x3059;&#x308B;  [<ind>&#x3046;&#x3089;&#x306A;&#x3068;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5360;&#x6709;&#x3059;&#x308B;  [<ind>&#x305B;&#x3093;&#x3086;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5360;&#x9818;&#x3059;&#x308B;  [<ind>&#x305B;&#x3093;&#x308A;&#x3087;&#x3046;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&c_j;&#x43D;&#x44F;&c_t;&c_i;</trd>
</drv>

<drv mrk="okup.mal0i">
