  </trdgrp>
</drv>

<drv mrk="okup.pri0o">
  <kap>
    pri<tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i<sncref ref="okup.0i.preni"/> a&ubreve; <sncref ref="okup.0i.laborigi"/>   
      <ekz>
        venteto-petolulo helpas miajn pri<tld/>ojn: li &ccirc;e &ccirc;iu stratangulo blovas sub
        knabinajn jupojn
        <fnt>
          <bib>LSB</bib>
          <lok>Printempa Varianto</lok>
        </fnt>;
      </ekz>
      <ekz>
        seksaj plezuroj invadis pli kaj pli la terenon de liaj pri<tld/>oj &ndash; jen de la
        sento regi
        <fnt>
          <bib>ChV</bib>
          <lok>12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="akir.0o">akiro</ref>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_ja;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Eroberung</trd>,
    <trd>Nachstellung</trd>
  </trdgrp>
</drv>






<drv mrk="okup.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Havanta nenian <tld/>i&gcirc;on:
      <ekz>
        <nom>Knut</nom> estis donita al &scirc;uisto por lernado, la gepatroj ne povis plu lasi lin
        sen<tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vidas amason da kolekti&gcirc;intaj sen<tld/>aj laboristoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi duone sidis sur la angulo de granda tablo, <klr>[&#x2026;]</klr>
        kun senkura&gcirc;a kaj sen<tld/>a 
        <klr>(vd <ref cel="indife.0a">indiferenta</ref>)</klr> mieno
        <fnt>
          <aut>Alain Fournier, trad. Roger Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 121</lok>
        </fnt>;
      </ekz>
      <ekz>
        vintro venis, kaj Kristoforo estis sen<tld/>a
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni petas min dispeli la enuon de sen<tld/>aj horoj per rakonto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="labor.sen0a">senlabora</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_j;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ohne <ind>Besch&auml;ftigung</ind></trd>,
    <trd>arbeitslos</trd>,
    <trd>unt&auml;tig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;s&oelig;uvr&eacute;</trd>,
    <trd>d&eacute;soeuvr&eacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z; (&c_zh;&#x43E;&c_d;&#x43D;&c_i;&c_h;) &c_z;&c_a;&#x43D;&#x44F;&c_t;&#x44C;</trd>,
    <trd>&c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&#x43E;&c_b;&c_ib;&c_t;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
