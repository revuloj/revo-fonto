  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;vacuer</trd>,
    <trd>se <ind>retirer</ind> de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&uuml;r&iacute;t</trd>,
    <trd>evaku&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x68C4;&#x3059;&#x308B;  [<ind>&#x307B;&#x3046;&#x304D;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x89E3;&#x653E;&#x3059;&#x308B;  [<ind>&#x304B;&#x3044;&#x307B;&#x3046;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">ontruimen</trd>
  <trd lng="pl">zwalnia&cacute;</trd>
