<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <ofc>2</ofc> <ofc>7</ofc>
  <rad>paradoks</rad>/o <fnt><bib>PV</bib></fnt></kap> 
<drv mrk="parado.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parado.0o.LOG">
    <dif>
      Opinio &scirc;oka, surpriza, &scirc;ajnanta malsa&gcirc;a.
    </dif>
    <subsnc mrk="parado.0o.FIL">
      <uzo tip="fak">FIL</uzo>
      <dif>
        Opinio &scirc;oka por racio, por logiko.
      </dif>
    </subsnc>
    <subsnc mrk="parado.0o.KOMUNE">
      <uzo tip="stl">KOMUNE</uzo>
      <dif>
        Opinio malakorda kun komunaj anta&ubreve;supozoj, malkutima.
        <ekz>
          mi diros pri tio &ccirc;i nur kelkajn vortojn, por ke mia opinio
          ne &scirc;ajnu tro <tld/>a
          <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
    </snc>
  <snc mrk="parado.0o.aporio">
    <ref tip="dif" cel="apori.0o.FIL">aporio</ref>
    <ekz>
      Zenono el Elajo kreis anka&ubreve; unuajn <tld/>ojn,
      per kiuj pruvas, ke movo ne ekzistas
      <fnt><bib>Viki</bib>,
      <lok><url ref="http://eo.wikipedia.org/wiki/Filozofio">
      Filozofio</url>, 2012-04-27</lok></fnt>.
    </ekz>
    <trd lng="ca">aporia</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_o;&c_k;&c_s;</trd>
  <trd lng="ca">paradoxa</trd>
  <trdgrp lng="cs">
    <trd>nesouhlas</trd>,
    <trd>paradox</trd>,
    <trd>protismysl</trd>,
    <trd>rozpor</trd>
  </trdgrp>
  <trd lng="de">Paradox</trd>
  <trd lng="fr">paradoxe</trd>
  <trdgrp lng="hu">
    <trd>paradoxon</trd>,
    <trd>l&aacute;tsz&oacute;lagos <ind>ellentmond&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="nl">paradox</trd>
  <trd lng="pt">paradoxo</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_d;&c_o;&c_k;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>paradox</trd>,
    <trd>protizmysel</trd>,
    <trd>rozpor</trd>
  </trdgrp>
</drv>

<drv mrk="parado.0a">
  <kap><ofc>*</ofc><ofc>ekz</ofc> <tld/>a</kap>
  <snc mrk="parado.0a.LOG">
    <dif>
      Prezentanta paradokson:
      <ekz>
      nun mi diros pri tio &ccirc;i nur kelkajn vortojn, por ke mia opinio ne
      &scirc;ajnu tro paradoksa <fnt><bib>F</bib>,
      <lok>Anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

</art>
