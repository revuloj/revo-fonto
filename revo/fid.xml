<drv mrk="fid.tromem0a">
  <kap>tromem<tld/>a</kap>
  <snc mrk="fid.tromem0a.PSI">
    <dif>
        Tro <tld/>a al si, havanta tro altan
      takson de sia kapablo, de sia merito:
      <ekz>
        la ricevitaj vundoj faras nin kelkfoje modestaj,
        la donitaj &ccirc;iam tromem<tld/>aj
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        Katerino <klr>[&#8230;]</klr> faris indiferentan kaj tromem<tld/>an mienon
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne estas plu la tromem<tld/>a knabo, kia mi venis &ccirc;i tien
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplomb.0a">aplomba</ref>,
      <ref cel="arogan.0a">aroganta</ref>,
      <ref cel="auxdac.0a">a&ubreve;daca</ref>,
      <ref cel="modest.mal0a">malmodesta</ref>,
      <ref cel="temerar.0a">temerara</ref>,
      <ref cel="vant.0a.iu">vanta</ref>
    </refgrp>
  </snc>
  <trd lng="be"><klr>(&c_z;&c_a;&c_n;&c_a;&c_d;&c_t;&c_a;)</klr> &c_s;&c_a;&c_m;&c_a;&c_n;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>sich <ind>&uuml;berlegen</ind> gebend</trd>,
    <trd>&uuml;berm&uuml;tig</trd>,
    <trd>&uuml;berheblich</trd>,
    <trd>eitel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pr&eacute;tentieux</trd>,
    <trd>pr&eacute;somptueux</trd>
  </trdgrp>
