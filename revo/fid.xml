<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fid.xml,v 1.51 2024/01/26 21:21:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fid</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fid.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="fid.0i.PSI">
    <dif>
      Plenkredi kaj <ref cel="kalkul.0i.esperi">kalkuli</ref>
      je la kapablo, fideleco, servo, helpo de iu, a&ubreve; je
      la efiko de io kontra&ubreve; dan&gcirc;ero, risko, malsano ktp:
      <ekz>
        li <tld/>is sian junecon, sian forton kaj laboremecon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ne esperas, ke mi povos min helpi, mi ne povas <tld/>i min mem
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        la&ubreve;doj kaj malla&ubreve;doj de flankaj personoj havas por ni nur tre malgrandan
        signifon, <klr>[&#8230;]</klr> ni devas <tld/>i nur niajn proprajn fortojn, nian
        propran paciencon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la junulo pli &scirc;atis &ccirc;ion toleri, prefere ol perfidi sian amantinon, kiu <tld/>is
        al lia lojaleco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is la embuskon, kiun ili aran&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        vian vorton mi <tld/>as
        <fnt><bib>MT</bib>, <lok>&Psa; 119:114</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>u al mi, la veturilo estos &gcirc;ustatempe en la arbaro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu <tld/>as la krurojn de sia &ccirc;evalo, alia la nazon de sia azeno, tria siajn
        proprajn krurojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vicre&gcirc;o hezitis <tld/>i al la renegato kaj konfidi al li la kristanojn
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi pli <tld/>as neraciajn instigojn ol pruvojn plej firme starigitajn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        esperanto <tld/>as, ne la sciojn, sed la inteligenton: la
        kapablon dedukti
        <fnt><bib>BonaLingvo</bib></fnt>;
      </ekz>
      <ekz>
        al amiko nova ne <tld/>u sen provo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>u fidelulon, <tld/>u propran okulon
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne <tld/>u heredon, <tld/>u posedon
        <klr>(<tld/>u havata&jcirc;on pli ol havota&jcirc;on)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atend.0i.antauxvidi">atendi<sncref/></ref>,
      <ref cel="esper.0i">esperi</ref>,
      <ref cel="kalkul.0i.esperi">kalkuli<sncref/></ref>,
      <ref cel="konfid.0i">konfidi</ref>,
      <ref cel="kred.0i">kredi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_ja;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;&rcaron;ovat</trd>,
    <trd>spol&eacute;hat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>trauen</trd>,
    <trd>vertrauen</trd>,
    <trd>zutrauen</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>rely</trd>,
     <trd>trust</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>confiar</trd>,
   <trd>tener fe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avoir <ind>foi</ind> (en)</trd>,
    <trd>avoir <ind>confiance</ind> (en, dans)</trd>,
    <trd>faire <ind>confiance</ind> (&agrave;)</trd>,
    <trd><ind>compter</ind> (sur)</trd>,
    <trd>se <ind>confier</ind> (en)<klr tip="ind"> (faire confiance
    &agrave;)</klr></trd>,
    <trd>se <ind>fier</ind> (&agrave;)</trd>,
    <trd>s'en <ind>rapporter</ind> (&agrave;)</trd>,
    <trd>s'en <ind>remettre</ind> (&agrave;)</trd>,
    <trd>se <ind>reposer</ind> (sur)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&iacute;zik</trd>,
    <trd>bizakodik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4FE1;&#x983C;&#12377;&#x308B;  <pr>&#12375;&#12435;&#12425;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x4FE1;&#29992;&#12377;&#x308B;  <pr>&#12375;&#12435;&#12424;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vertrouwen op</trd>
  <trdgrp lng="pl">
    <trd>ufa&cacute; <klr>(komu&sacute;)</klr></trd>,
    <trd>wierzy&cacute; <klr>(komu&sacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>crer</trd>,
    <trd>confiar</trd>,
    <trd>fiar-se</trd>,
    <trd>contar com</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_n;&c_a; &c_k;&c_o;&c_g;&c_o;-&c_l;...)</klr></trd>,
    <trd>&c_d;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;verova&tcaron;</trd>,
    <trd>spo&lcaron;ahn&uacute;&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_v;&c_ib;&c_r;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_l;&c_a;&c_d;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_n;&c_a; &c_k;&c_o;&c_g;&c_o;-&c_n;.</trd>,
    <trd>&c_v;&c_ib;&c_r;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FE1;&#x5FC3; <pr>x&#xEC;nx&#x12B;n</pr></trd>,
    <trd>&#x76F8;&#x4FE1; <pr>xi&amacron;ngx&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fid.0o">
  <kap><tld/>o</kap>
  <snc mrk="fid.0o.PSI">
    <dif>
      Sento de <tld/>anto:
      <ekz>
        malgra&ubreve; <klr>[&#8230;]</klr> sia <tld/>o al la rapideco de sia &ccirc;evalo, &scirc;i ne &ccirc;iam
        estis tute sentima, kiam &scirc;i rajdis sola
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;rinde vi ne opiniis, ke mi estas sufi&ccirc;e inda de via <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ripro&ccirc;is sin pro manko de dankemo kaj <tld/>o kaj pro plendemo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur la <tld/>o de mia kaptinto je la mortiga efiko de la
        dolorigilo savis mian vivon
        <fnt>
          <aut>Harry Harrison, tr. R. F&ouml;ssmeier k.a.</aut>:
          <vrk>Naski&gcirc;o de la rustimuna &scirc;talrato</vrk>
          <lok>Sezonoj, 1995</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i perdis sian <tld/>on je Dio, kaj nun atakis &scirc;in mallumaj
        pensoj
        <fnt><bib>Fab4</bib>, <lok>Infano en la tombo</lok></fnt>;
      </ekz>
      <ekz>
        ili metu sian <tld/>on sur Dion
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 78:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trompis la <tld/>on de la familio al kiu li &ccirc;ion &scirc;uldis
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi kun iu en <tld/>o kaj konfido
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e plej granda <tld/>o memoru pri perfido
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_p;&c_e;&c_w;&c_n;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_v;&c_je;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>reliance</trd>
      <trd>faith</trd>,
      <trd>trust</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>confiance</trd>,
      <trd>foi<klr tip="ind"> (confiance)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fid.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Religia <ref tip="super" cel="virt.0o">virto</ref> konsistanta en firma kredo je sankteco kaj
      potenco
      de Dio kaj <tld/>ema submeti&gcirc;o al ties volo:
      <ekz>
         &scirc;i vidis en unu tago perei siajn sep filojn kaj &scirc;i toleris
         tion sen&scirc;ancele, dank' al sia <tld/>o je la Sinjoro
         <fnt><aut>Tr. G. Berveling</aut>:
            <vrk>Dua-kanonaj bibliaj libroj</vrk>
            <lok>2Makabeoj 7:20</lok></fnt>;
      </ekz><ekz>
          kaj Jesuo, vidinte ilian <tld/>on, diris al la paralizulo:
          filo, viaj pekoj estas pardonitaj
	  <fnt><bib>NT</bib>, <lok>&Mat; 9:2</lok></fnt>;
      </ekz><ekz>
          estu al vi la&ubreve; via <tld/>o
	  <fnt><bib>NT</bib>, <lok>&Mat; 9:29</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="islam.0o">islamo</ref>
    <rim>
      Kompare kun opinio kaj
      <ref cel="kred.0o.je">kredo<sncref/></ref>,
      <tld/>o emfazas akcepton kaj submeti&gcirc;on. Jakobo diras:
      <ekz>
         &leftquot;Vi ja kredas, ke Dio estas unu, vi faras bone,
         la demonoj anka&ubreve; kredas kaj tremas&rightquot;
         <fnt><bib>NT</bib>, <lok>&Jak; 2:19</lok></fnt> &mdash;
      </ekz>
       tamen la demonoj malhavas la <tld/>on. Kaj krome,
      <ekz>
        &leftquot;kiel la korpo aparte de la spirito estas malviva,
        tiel anka&ubreve; <tld/>o aparte de faroj estas malviva&rightquot;
        <fnt><bib>NT</bib>, <lok>&Jak; 2:26</lok></fnt>.
      </ekz>
    </rim>
    <trd lng="be">&c_v;&c_je;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Gottvertrauen</trd>,
      <trd>Glaube</trd>
    </trdgrp>
    <trd lng="en">faith</trd>
    <trd lng="fr">foi<klr> (religion)</klr></trd>
    <trd lng="ru">&c_v;&c_je;&c_r;&c_a;</trd>
  </snc>
  <trd lng="bo">&t_ba;&t_lata;&t_naro;&t_tsheg;&t_-a;&t_kha;&t_drengbu;&t_la;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;ra</trd>,
    <trd>p&rcaron;esv&ecaron;d&ccaron;en&iacute;</trd>,
    <trd>spol&eacute;h&aacute;n&iacute;</trd>,
    <trd>v&iacute;ra</trd>
  </trdgrp>
  <trd lng="de">Vertrauen</trd>
  <trdgrp lng="es">
   <trd>confianza</trd>,
   <trd>fe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bizalom</trd>,
    <trd>bizakod&aacute;s</trd>,
    <trd>hit</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4FE1;&#x983C;  <pr>&#12375;&#12435;&#12425;&#12356;</pr></trd>,
    <trd>&#x4FE1;&#29992;  <pr>&#12375;&#12435;&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">vertrouwen<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="sk">d&ocirc;vera</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_v;&c_ib;&c_r;&#8217;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_v;&c_ib;&c_r;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FE1;&#x4EFB; <pr>x&#xEC;nr&#xE8;n</pr></trd>,
    <trd>&#x8AA0;&#x4FE1; <pr>ch&#xE9;ngx&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fid.0ema">
  <kap><tld/>ema,
  <var><kap><tld/>oplena</kap></var></kap>
  <snc mrk="fid.0ema.PSI">
    <dif>
      Kiu facile, plene <tld/>as:
      <ekz>
        blinde <tld/>ema je la sa&gcirc;o kaj justeco de l' edzo, &scirc;i
        tamen klopodis por moligi liajn ju&gcirc;ojn en okazo de puno
        <fnt><bib>VivZam</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;iaj mildaj, <tld/>oplenaj okuloj sin turnas supren
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/IX.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antoj de <nom>Orkney</nom> estas bonaj, honestaj, <tld/>emaj kaj komplezemaj
        <fnt>
          <bib>KrM</bib>
          <lok>38. Du Botoj Faras Paron.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_je;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="bo">&t_ra;&t_mata;&t_naro;&t_nga;&t_tsheg;&t_zha;&t_drengbu;&t_na;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>vertrauensvoll</trd>,
    <trd>vertraulich</trd>
  </trdgrp>
  <trd lng="en">trusting</trd>
  <trd lng="fr">confiant</trd>
  <trd lng="ja">&#x4FE1;&#12376;&#12420;&#12377;&#12356;  <pr>&#12375;&#12435;&#12376;&#12420;&#12377;&#12356;</pr></trd>
  <trd lng="uk">&c_p;&c_o;&c_v;&c_n;&c_i;&c_j; &c_d;&c_o;&c_v;&c_ib;&c_r;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x79C1;&#x4E0B;&#x8AC7;&#x8A71; <pr>s&#x12B;xi&#xE0;t&#xE1;nhu&#xE0;</pr></trd>,
    <trd>&#x809D;&#x80C6;&#x76F8;&#29031; <pr>g&amacron;nd&#x1CE;nxi&amacron;ngzh&#xE0;o</pr></trd>,
    <trd>&#x5BC6; <pr>m&#xEC;</pr></trd>,
    <trd>&#x809D;&#x80C6;&#x76F8;&#29031; <pr>g&amacron;nd&#x1CE;nxi&amacron;ngzh&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fid.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="fid.0inda.PSI">
    <dif>
      Inda je <tld/>o:
      <ekz>
        mi bezonus personon <tld/>indan kaj sindoneman, kiu min sciigus pri &ccirc;io
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la elektristo montris sin <tld/>inda homo, la subtegmentan
        lampon en la vira parto jam protektis dratreto
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 10a</lok></fnt>;
      </ekz>
      <ekz>
        nun oni devis meti lin en <tld/>indan gardejon
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
      </ekz>
      <ekz>
        flugma&scirc;inon &scirc;i neniam trovis <tld/>inda
        <fnt>
          <bib>CKv</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
         ia ne tro <tld/>inda traduko
         <fnt>
           <aut>Marjorie Boulton</aut>:
           <vrk>Ne nur leteroj de plumamikoj</vrk>. <lok>1984</lok>
         </fnt>;
      </ekz>
      <ekz>
        <tld/>inda kopio de la originalo
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2010/010009.php">Parto 1: Survoje al iu libro-normo</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        statistiko el 1934, ne tre <tld/>inda
        <fnt>
          <vrk>Esperanto en perspektivo</vrk>,
          <lok>1974</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj proponoj por pli rigoraj kaj <tld/>indaj metodoj ekaperis,
        <klr>[sed]</klr> neniu el tiuj alternativoj estis sufi&ccirc;e kontentiga
        <fnt>
          <bib>Kon11</bib>
          <lok>Scienco</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_k;&c_ib; &c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_o;&c_w;&c_v;&c_a;&c_je; <ind>&c_d;&c_a;&c_v;&c_je;&c_r;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;ryhodn&yacute;</trd>,
    <trd>spolehliv&yacute;</trd>,
    <trd>v&ecaron;rohodn&yacute;</trd>,
    <trd>v&ecaron;rohodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vertrauensw&uuml;rdig</trd>,
    <trd>zuverl&auml;ssig</trd>,
    <trd>verl&auml;sslich</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>reliable</trd>,
     <trd>trustworthy</trd>
  </trdgrp>
  <trd lng="es">confiable</trd>
  <trdgrp lng="fr">
    <trd>digne de <ind>confiance</ind></trd>,
