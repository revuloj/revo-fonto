<drv mrk="fid.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="fid.mem0o.PSI">
    <dif>
      <tld lit="F"/>o je si mem, je sia sa&gcirc;o, kapablo, potenco:
      <ekz>
        lia mem<tld/>o kaj aristokrata &scirc;ajno konfuzis la knabinon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju pli li parolis, des pli lia &scirc;ajno de mem<tld/>o paligis Herbenon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mem<tld/>o kreskis, la fingroj lerti&gcirc;is, la tonoj fari&gcirc;is pli belsonaj kaj
        klaraj,
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trankvileco kaj mem<tld/>o de la juna virino &scirc;ajne inspiris al li konfidon
        kaj esperon
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplomb.0o">aplombo</ref>,
      <ref cel="arogan">aroganteco</ref>,
      <ref cel="fier">fiereco</ref>,
