<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>greg</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="greg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc num="1">
    <dif>
      Brutaro:
      <ekz><tld/>o da bovoj<fnt>Z</fnt>;</ekz>
      <ekz>la nobeloj traktis ilin kiel <tld/>on.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senorda kunanaro. 
    </dif>
    <refgrp tip="sin">
      <ref cel="hord.0o">hordo</ref>,
      <ref cel="band.0o">bando</ref>,
      <ref cel="trup.0o">trupo</ref>,
      <ref cel="ar1.0o">aro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Herde</trd>
  <trd lng="sv">hjord</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_o;</trd>
</drv>
<drv mrk="greg.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o karakterizanta <tld/>on:
      <ekz><tld/>a sento.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Herden-</trd>,
     <trd>Massen-</trd></trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="greg.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Loko, kie oni gardas <tld/>on.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Weide</trd>,
     <trd>Pferch</trd></trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_b;&c_i;&c_shch;&c_je;</trd>
</drv>
</art>
