<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>6</ofc>
<rad>greg</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="greg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Brutaro:
      <ekz>
        <tld/>o da bovoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la nobeloj traktis ilin kiel <tld/>on.
      </ekz>
    </dif>
    <trd lng="hu">csorda</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senorda kunanaro.
    </dif>
    <refgrp tip="vid">
      <ref cel="hord.0o">hordo</ref>,
      <ref cel="band.0o">bando</ref>,
      <ref cel="trup.0o">trupo</ref>,
      <ref cel="ar1.0o">aro</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>csorda<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>ny&aacute;j<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_t;&c_a;&c_k;</trd>
  <trd lng="de">Herde</trd>
  <trd lng="fr">troupeau</trd>
  <trd lng="nl">troep</trd>
  <trdgrp lng="pt">
   <trd>grei</trd>, 
   <trd>rebanho</trd>, 
   <trd>gado</trd> 
  </trdgrp>
  <trd lng="sv">hjord</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_o;</trd>
</drv>

<drv mrk="greg.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, karakterizanta <tld/>on:
      <ekz>
        <tld/>a sento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_t;&c_k;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Herden-</trd>,
    <trd>Massen-</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gr&eacute;gaire</trd>,
    <trd>de <ind>troupeau</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csorda-</trd>,
    <trd>ny&aacute;j-<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trd lng="nl">kudde-</trd>
  <trd lng="pt">greg&aacute;rio</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="greg.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Loko, kie oni gardas <tld/>on:
      <ekz>
        Mi kolektos la resta&jcirc;on de Izrael, Mi kunigos ilin kiel
        &scirc;afojn en &scirc;afejo, kiel <tld/>o en <tld/>ejo ili ekbruos de
        multhomeco
        <fnt><bib>MT</bib>, <lok>&Mih; 2:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Weide</trd>,
    <trd>Pferch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enclos<klr tip="ind"> (d'un troupeau)</klr></trd>,
    <trd>p&acirc;turage</trd></trdgrp> 
  <trd lng="hu">kar&aacute;m</trd>
  <trdgrp lng="pt">
   <trd>curral</trd>, 
   <trd>redil</trd> 
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_b;&c_i;&c_shch;&c_je;</trd>
</drv>
</art>
