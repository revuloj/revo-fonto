<vortaro>
<art mrk="$Id: algxeb.xml,v 1.12 2020/02/13 14:10:24 revo Exp $">
  <kap>
    <rad>al&gcirc;ebr</rad>/o
  </kap>
<drv mrk="algxeb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="algebr.lineara0o.strukturo">Lineara
      algebro<sncref/></ref>:
      <ekz>
        prezento de la al&gcirc;ebro <frm am="Cg">Cg</frm> sur 
        vektora spaco <frm am="V">V</frm> estas la al&gcirc;ebra 
        homomorfio <frm am="CCG->End(V)"><k>&#8450;G</k> &#10230;
        <k>End(V)</k></frm>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=87410"
              >Grupa al&gcirc;ebro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja; <ind>&c_a;&c_l;&c_mol;&c_g;&c_je;&c_b;&c_r;&c_a;</ind></trd>
  <trd lng="de">lineare Algebra</trd>
  <trd lng="en">linear algebra</trd>
  <trd lng="es">&aacute;lgebra lineal</trd>
  <trd lng="fa">&a_j;&a_b;&a_r; &a_kh;&a_T;&f_y;</trd>
  <trd lng="pt">&aacute;lgebra linear</trd>
</drv>
</art>

