<kap>
  <ofc>2</ofc>
  <rad>diapazon</rad>/o
</kap>
<drv mrk="diapaz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="agord.0ilo.MUZ">Agordilo</ref>, en formo de metala dubraka forko:
      <ekz>
        la klasika metodo por a&ubreve;digi la &ccirc;ambrotonon estas la <tld/>o, alternative
        ekzistas anka&ubreve; agordofajfiloj, nuntempe elektronikaj agordiloj estas pli kaj
        pli disvastigitaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=251732">&Ccirc;ambrotono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/e0/Stimmgabel90degree.jpg/320px-Stimmgabel90degree.jpg" prm="CC-BY-SA-3.0"><tld/>o por &ccirc;ambrotono &leftquot;a&rightquot;
        <fnt>
          <aut>Wollschaf</aut>
          <lok><url ref="&WCW;/File:Stimmgabel90degree.jpg">&WCW;/File:Stimmgabel90degree.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_t;&c_o;&c_n;</trd>
    <trd lng="de">Stimmgabel</trd>
    <trd lng="es">diapas&oacute;n</trd>
    <trd lng="fr">diapason</trd>
    <trd lng="hu">hangvilla</trd>
    <trd lng="it">diapason</trd>
    <trdgrp lng="pl">
      <trd>kamerton</trd>,
      <trd>stroik</trd>
    </trdgrp>
    <trd lng="pt">diapas&atilde;o</trd>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_t;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>diapazon</trd>,
    <trd>hlasov&eacute; rozp&ecaron;t&iacute;</trd>,
    <trd>t&oacute;nov&eacute; rozp&ecaron;t&iacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5E7;&#x5D5;&#x5DC;&#x5DF;</trd>
  <trdgrp lng="ja">
    <trd>&#x5168;&#x97F3;&#x57DF;  <pr>&#x305C;&#x3093;&#x304A;&#x3093;&#x3044;&#x304D;</pr></trd>,
    <trd>&#x97F3;&#x53C9;  <pr>&#x304A;&#x3093;&#x3055;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;smo</trd>,
    <trd>rozp&auml;tie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x456;&#x430;&#x43F;&#x430;&#x437;&#x43E;&#x43D;</trd>,
    <trd>&#x43A;&#x430;&#x43C;&#x435;&#x440;&#x442;&#x43E;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x97F3;&#x53C9; <pr>y&#x12B;nch&#x101;</pr></trd>
