<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxapel.xml,v 1.54 2025/12/18 14:39:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;apel</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="cxapel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxapel.0o.kapvesto">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.kap0o.TEKS">Kapvesto</ref> kun elstara
      &ccirc;irka&ubreve;rando:
      <ekz>
        li portas rozokoloran superveston kaj teleroforman &ccirc;apelon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        kancelariano kun lia lar&gcirc;a <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigrafran&gcirc;a triangula <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        velura <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>felta <tld/>o</ind>
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
        <trd lng="de">Filzhut</trd>
      </ekz>
      <ekz>
        kun via <tld/>o vi similas pajacon, &dash; ne metu vian <tld/>on sur unu orelon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili aspektis imprese: pintaj <tld/>oj, hakilo en la mano,
        da&ubreve;re drinkemaj kaj batalemaj
        <fnt>
          <aut>W. Klag</aut>,
          <vrk>Vakeroj de Alpoj</vrk>,
          <bib>Monato</bib>,
          <lok>2001:1, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;</trd>,
      <trd>&c_b;&c_r;&c_y;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>barret</trd>,
      <trd>capell</trd>
    </trdgrp>
    <trd lng="el">&kappa;&alfa;&pi;&epsilon_ton;&lambda;&lambda;&omikron;</trd>
    <trd lng="en">hat</trd>
    <trd lng="es">sombrero</trd>
    <trd lng="fr">chapeau</trd>
    <trd lng="hu">kalap</trd>
    <trd lng="id">topi</trd>
    <trd lng="it">cappello</trd>
    <trd lng="es">sombrero</trd>
    <trd lng="nl">hoed</trd>
    <trd lng="pl">kapelusz</trd>
    <trd lng="pt">chap&eacute;u</trd>
    <trd lng="ro">p&abreve;l&abreve;rie</trd>
    <trd lng="ru">&c_sh;&c_l;&c_ja;&c_p;&c_a;</trd>
    <trd lng="sk">klob&uacute;k</trd>
    <trd lng="tr">&scedil;apka</trd>
  </snc>
  <snc mrk="cxapel.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Supra, <tld/>oforma parto de la fruktokorpo
      de fungo:
      <ekz>
        la rando de la <tld/>o de la malgranda faskofungo
        estas striita
        <fnt>
          <aut>P. Desmet'</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/petro_desmet/259965">
          &Hcirc;ameleona Fungeto?</url></vrk>,
          <lok>2010-06-30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_p;&c_a;&c_ch;&c_k;&c_a; <klr>(&c_g;&c_r;&c_y;&c_b;&c_a;)</klr></trd>
    <trdgrp lng="ca">
      <trd>barret <klr tip="amb">(de bolet)</klr></trd>,
      <trd>capell <klr tip="amb">(de bolet)</klr></trd>,
      <trd>pili</trd>
    </trdgrp>
    <trd lng="en">cap <klr>(of a mushroom)</klr></trd>
    <trd lng="nl">hoed</trd>
    <trd lng="pl">kapelusz</trd>
  </snc>
  <snc mrk="cxapel.0o.supersigno">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="diakrit.0ilo.LIN">Diakritilo</ref>,
      tegmentoforma supersigno, uzata en diversaj alfabetoj
      (interalie en esperanta, por formi la literojn &ccirc;, &gcirc;,
      &hcirc;, &jcirc;, &scirc;), en matematikaj simboloj kaj en programlingvoj (en tiuj
      lastaj &gcirc;i aperas kiel memstara signo, kies
      <ref tip="malprt" cel="aski.0o">Askia</ref> numero estas 94):
      <ekz>
        surogatoj kun iksoj kaj askiaj <tld/>oj (^)
        <fnt>
          <bib>Monato</bib>
          <aut>Grigorij Arosev, Andrej Grigorjevskij</aut>
          <vrk>Abonu!</vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj en la vortaraj tekstoj aspektas normale post instalo
        de specifaj tiparoj
        <fnt><aut>S. Derevjanni&hcirc;</aut>,
        <vrk>&Ccirc;u vi
        tradukas france?</vrk>, <bib>Monato</bib>,
        <lok>2002:4, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="h.0.konsonanto">h</ref>,
      <ref cel="iks.0o">x</ref>
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;&c_y;&c_k;
    <klr>(&c_d;&c_y;&c_ja;&c_k;&c_r;&c_y;&c_t;&c_y;&c_ch;&c_n;&c_y;
    &c_z;&c_n;&c_a;&c_k;)</klr></trd>
    <trd lng="ca">circumflex</trd>
    <trdgrp lng="de">
      <trd>Zirkumflex</trd>,
      <trd>Dach<klr> (diakritisches Zeichen)</klr></trd>
    </trdgrp>
    <trd lng="el">&pi;&epsilon;&rho;&jota;&sigma;&pi;&omega;&my;&epsilon_ton;&ny;&jota;</trd>
    <trdgrp lng="en">
      <trd>hat <klr>(typography)</klr></trd>,
      <trd>caret</trd>,
      <trd>circumflex</trd>
    </trdgrp>
    <trd lng="fr">accent circonflexe</trd>
    <trdgrp lng="hu">
      <trd>kalap<klr tip="ind"> (&eacute;kezet)</klr></trd>,
      <trd>k&uacute;pos <ind>&eacute;kezet</ind></trd>
    </trdgrp>
    <trd lng="id">sirkumfleks</trd>
    <trdgrp lng="it">
      <trd><klr>accento </klr>circonflesso</trd>, <trd>caret</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>hoedje<klr tip="ind"> (^)</klr></trd>,
      <trd>kapje<klr tip="ind"> (^)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>cyrkumfleks</trd>,
      <trd>daszek<klr> (pot.)</klr></trd>
    </trdgrp>
    <trd lng="pt">circunflexo</trd>
    <trd lng="ro">circumflex</trd>
    <trd lng="ru">&c_k;&c_r;&c_y;&c_sh;&c_a;</trd>
    <trdgrp lng="sk">
      <trd>vok&aacute;&ncaron;</trd>,
      <trd>strie&scaron;ka</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_ma;&t_cha;&t_naro;&t_da;&t_tsheg;&t_kha;&t_nga;&t_tsheg;</trd>
  <trd lng="cs">klobouk</trd>
  <trd lng="de">Hut</trd>
  <trd lng="he">&kaf;&#x5BC;&vav;&#x5B9;&bet;&#x5B7;&ayin;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30AF;&#x30B5;&#x30F3;&#x30B7;&#x30EB;&#x30B3;&#x30F3;&#x30D5;&#x30EC;&#x30C3;&#x30AF;&#x30B9;</trd>,
    <trd>&#x30CF;&#x30C3;&#x30C8;</trd>
  </trdgrp>
  <trd lng="sv">hatt</trd>
  <trd lng="sw">kofia</trd>
  <trd lng="uk">&c_k;&c_a;&c_p;&c_je;&c_l;&c_ju;&c_h;</trd>
</drv>

<drv mrk="cxapel.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="cxapel.0i.provizi">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Provizi per <tld/>o:
      <ekz>
        kun japaneska riverenco de sia skunke <tld/>ita silueto <klr>[li&#8230;]</klr> foriris
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Robin Hood</nom> en <nom>Sherwood</nom> staris, &#11005; jam sin kapu&ccirc;is, <tld/>is kaj
        &scirc;trumpis kaj &scirc;uis
        <fnt>
          <aut>Jack Warren</aut>
          <vrk><url ref="https://legacy.esperanto.org.uk/lbe/arkivo/978/lbe-978.pdf"
              >La enigmo de Robin Hood</url></vrk>
          <lok>La Brita Esperantisto, printempo 2015, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">mit einem <ind>Hut</ind> versehen</trd>
  </snc>
  <snc mrk="cxapel.0i.roli">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Servi kiel <tld/>o, funkcii kvaza&ubreve; <tld/>o:
      <ekz>
        grandaj literoj <klr>[&#8230;]</klr> <tld/>is la artikolojn, frapis la knabon rekte en
        la koron
        <fnt>
          <aut>Claude Piron</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0049/0033.php"
              >Vere a&ubreve; fantazie</url></vrk>
          <lok>32. La &Scirc;losita &Scirc;ranko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berdachen</trd>
  </snc>
</drv>

<drv mrk="cxapel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="cxapel.0isto.KOMUNE">
    <dif>
      Homo kiu profesie vendas <tld/>ojn kaj aliajn kapvestojn:
      <ekz>
        la heroino Sofi estas 18-jara <tld/>istino
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>
        <lok>La Militregna Epoko..., 2006-01-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>barreter</trd>,
    <trd>barretaire</trd>,
    <trd>capeller</trd>
  </trdgrp>
  <trd lng="de">Hutmacher</trd>
  <trdgrp lng="en">
    <trd>milliner</trd>,
    <trd>hatter</trd>
  </trdgrp>
  <trd lng="fr">chapelier</trd>
  <trd lng="id"><baz>jual</baz>penjual topi</trd>
  <trdgrp lng="it">
    <trd>cappellaio<klr> (per uomo)</klr></trd>,
    <trd>modista<klr> (per donna)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E3D;&#x5B50;&#x88FD;&#36896;&#x696D;&#32773;
    <pr>&#x307C;&#12358;&#12375;&#x305B;&#12356;&#x305E;&#12358;&#x304E;&#12423;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#x5E3D;&#x5B50;&#x5C4B;  <pr>&#x307C;&#12358;&#12375;&#12420;</pr></trd>
  </trdgrp>
  <trd lng="pl">kapelusznik</trd>
  <trd lng="ro">p&abreve;l&abreve;rier</trd>
  <trdgrp lng="zh">
    <trd>&#22919;&#22899;&#29992;&#x5E3D;&#x5E97; <pr>f&ugrave;n&#x1DA;y&ograve;ngm&agrave;odi&agrave;n</pr></trd>,
    <trd>&#x5E3D;&#21830; <pr>m&agrave;osh&amacron;ng</pr></trd>,
    <trd>&#22899;&#x5E3D;&#21280; <pr>n&#x1DA;m&agrave;oji&agrave;ng</pr></trd>,
    <trd>&#22899;&#x5E3D;&#21830; <pr>n&#x1DA;m&agrave;osh&amacron;ng</pr></trd>,
    <trd>&#21046;&#x5E3D;&#x4EBA; <pr>zh&igrave;m&agrave;or&eacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cxapel.0ita">
  <kap>
    <tld/>ita
  </kap>
  <snc mrk="cxapel.0ita.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Provizita per <tld/>forma supersigno:
      <ekz>
        anstata&ubreve;igi la <tld/>itajn literojn per iksoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pierre Grollemund</aut>
          <vrk><url ref="&Monato;/2014/011249.php"
              >Kolekto da lingvaj artikoloj, aperintaj en Heroldo</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la intenco estis &ccirc;i-foje skribi Dol&ccirc;amar <tld/>ite, sed fakte mi forgesis &scirc;an&gcirc;i
        tion en la kovrilo
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">mit einem <ind>Dach</ind> versehen <klr>(Zeichen)</klr></trd>
</drv>

<drv mrk="cxapel.bul0o">
  <kap>bul<tld/>o,
  <var><kap>melon<tld/>o</kap></var></kap>
  <snc mrk="cxapel.bul0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="cxapel.0o.kapvesto"><tld lit="&Ccirc;"/>o</ref>
      el felto kun rigida duonbula supro kaj supren kuspita rando,
      populara precipe en la fino de la 19a kaj komenco de la 20a
      jarcentoj:
      <ekz>
        li surhavis <klr>[&#8230;]</klr> bonkvalitan striitan vestokompleton, lakledajn &scirc;uojn
        kaj bul<tld/>on
        <fnt>
          <bib>MkM</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la junulo kun la melon<tld/>o <klr>[&#8230;]</klr> staris ekster la
        pordo, mirigite
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ja;&c_l;&c_o;&c_k; <klr
  tip="amb">(&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;)</klr></trd>
  <trdgrp lng="ca">
    <trd>bolet <klr tip="amb">(capell)</klr></trd>,
    <trd>barret fort</trd>
  </trdgrp>
  <trd lng="de">Melone <klr>(Hut)</klr></trd>
  <trd lng="en">bowler <ind>hat</ind></trd>
  <trd lng="fr">chapeau-melon</trd>
  <trd lng="id"><ind>topi</ind> bowler</trd>
  <trd lng="ja">&#x5C71;&#x9AD8;&#x5E3D;  <pr>&#12420;&#x307E;&#x305F;&#x304B;&#x307C;&#12358;</pr></trd>
  <trd lng="pl">melonik</trd>
  <trdgrp lng="ro">
    <trd>melonier&abreve;</trd>,
    <trd>p&abreve;l&abreve;rie bombat&abreve;</trd>
  </trdgrp>
  <trd lng="zh">&#22278;&#39030;&#x786C;&#x793C;&#x5E3D;
  <pr>yu&aacute;nd&#x1D0;ngy&igrave;ngl&#x1D0;m&agrave;o</pr></trd>
</drv>

<drv mrk="cxapel.cilindro0o">
  <kap>cilindro<tld/>o, <var><kap>cilindra <tld/>o</kap></var></kap>
  <snc mrk="cxapel.cilindro0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Alta, cilindroforma <tld/>o, pliofte nigra,
      moda &ccirc;e okcidentaj viroj de alta klaso en la
      19a jarcento:
      <ekz>
        unu el ili surhavis e&ccirc; cilindran &ccirc;apelon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fiera cilindro<tld/>o ricevis piedpu&scirc;on kaj
        ruli&gcirc;is en fosa&jcirc;on
        <fnt>
          <aut>W. Kabel, trad. M. Lennartz</aut>
          <vrk>La domo &ccirc;e la mueleja kanalo</vrk>
          <lok>2017</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_l;&c_ib;&c_n;&c_d;&c_a;&c_r; <klr
  tip="amb">(&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;)</klr></trd>
  <trdgrp lng="ca">
    <trd>barret de copa</trd>,
    <trd>barret de copa alta</trd>,
    <trd>copalta</trd>
  </trdgrp>
  <trd lng="de">Zylinder (Hut)</trd>
  <trd lng="en">top <ind>hat</ind></trd>
  <trd lng="fr">haut-de-forme</trd>
  <trd lng="id"><ind>topi</ind> tinggi</trd>
  <trd lng="pl">cylinder  <klr>(kapelusz)</klr></trd>
  <trd lng="ro">p&abreve;l&abreve;rie cilindric&abreve;</trd>
  <trd lng="zh">&#22823;&#x793C;&#x5E3D; <pr>d&agrave;l&#x1D0;m&agrave;o</pr></trd>
</drv>

<drv mrk="cxapel.klak0o">
  <kap>klak<tld/>o</kap>
  <snc mrk="cxapel.klak0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Alta <ref tip="super" cel="cxapel.0o.kapvesto"><tld/>o</ref>,
      povanta plati&gcirc;i, por ke oni &gcirc;in portu sub la brako,
      populara en okcidento en la 19a jarcento:
      <ekz>
        frake vestitaj sinjoroj, kun la haroj kombitaj sur la frunto kaj la klak<tld/>o
        sub la brako, &ccirc;irka&ubreve;is &scirc;in
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_p;&c_a;&c_k;&c_l;&c_ja;&c_k;</trd>
  <trd lng="ca">clac <klr tip="amb">(capell)</klr></trd>
  <trdgrp lng="de">
    <trd>Klappzylinder</trd>,
    <trd>Chapeau Claque</trd>
  </trdgrp>
  <trd lng="en">opera <ind>hat</ind></trd>
  <trdgrp lng="fr">
    <trd>claque<klr tip="ind"> (chapeau claque)</klr></trd>,
    <trd>chapeau claque</trd>,
    <trd>gibus</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>klakk</trd>,
    <trd>rug&oacute;s <ind>cilinder</ind> <klr tip="ind">(kalap)</klr></trd>
  </trdgrp>
  <trd lng="id"><ind>topi</ind> opera</trd>
  <trd lng="ja">&#x30AA;&#x30DA;&#x30E9;&#x30CF;&#x30C3;&#x30C8;</trd>
  <trd lng="nl">klakhoed</trd>
  <trdgrp lng="pl">
    <trd>szapoklak</trd>,
    <trd>cylinder sk&lstroke;adany</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>fedora</trd>,
    <trd>p&abreve;l&abreve;rie cu boruri late</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_p;&c_o;&c_k;&c_l;&c_ja;&c_k;</trd>
</drv>

<drv mrk="cxapel.pajlo0o">
  <kap>
    pajlo<tld/>o, <var><kap>pajla <tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o plektita el pajlo:
      <ekz>
        &scirc;i embarase ta&ubreve;zas la pajlo<tld/>on en la manoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        pajlo<tld/>o kun vasta rando
        <fnt>
          <bib>Kiso</bib>
          <lok>La Bovinoj De La Paro&hcirc;estro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pajla <tld/>o</ind>, ornamita per rubando
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_y;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh; <klr>(&c_s;&c_a;&c_l;&c_a;&c_m;&c_ja;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="de">Strohhut</trd>
  <trd lng="en">straw hat</trd>
  <trd lng="fr">chapeau de paille</trd>
</drv>

<drv mrk="cxapel.levi_la_0on">
  <kap>
    levi la <tld/>on,
    <var><kap>demeti la <tld/>on</kap></var>
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="cxapel.levi_la_0on.saluti">
    <dif>
      <ref cel="salut.0i.MOR">Saluti</ref>,
      gratuli, esprimi <ref cel="respek.0o.elmontro">respekton</ref>
      <klr>(iam en Okcidento efektiva saluta gesto flanke de viroj)</klr>:
      <ekz>
        ili levi&gcirc;is, demetis respekte la <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjoro, mi levas pro vi la <tld/>on... vi estas la sola homo kiu tiel malka&scirc;e
        parolis al mi en la da&ubreve;ro de la nuna jaro
        <fnt>
          <aut>J. Merchant</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/merchant_klem.html">Kompatinda
          Klem</url></vrk>
          <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;
  <ind>&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;</ind></trd>
  <trd lng="ca">treure's el barret</trd>
  <trd lng="de">den Hut ziehen</trd>
  <trd lng="en">tip one's <ind>hat</ind></trd>
  <trd lng="fr">tirer son chapeau</trd>
  <trd lng="id"><ind>angkat</ind> topi</trd>
  <trdgrp lng="pl">
    <trd>zdj&aogonek;&cacute; <ind>kapelusz</ind></trd>,
    <trd>zdj&aogonek;&cacute; <ind>czapk&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>a se ridica  <ind>p&abreve;l&abreve;ria</ind></trd>,
    <trd>a se scoati  <ind>p&abreve;l&abreve;ria</ind></trd>
  </trdgrp>
</drv>
