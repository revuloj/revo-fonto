<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;apel</rad>/o
</kap>
<drv mrk="cxapel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxapel.0o.kapvesto">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.kap0o">Kapvesto</ref> kun elstara
      &ccirc;irka&ubreve;rando:
      <ekz>
        ili aspektis imprese: pintaj <tld/>oj, hakilo en la mano, da&ubreve;re
        drinkemaj kaj batalemaj<fnt><aut>Walter Klag</aut>: <vrk>Vakeroj
        de Alpoj</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a, p.
        10a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_p;&c_ja;&c_l;&c_ju;&c_sh;</trd>,
      <trd>&c_b;&c_r;&c_y;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="el">&kappa;&alfa;&pi;&epsilon_ton;&lambda;&lambda;&omikron;</trd>
    <trd lng="en">hat</trd>
    <trd lng="es">sombrero</trd>
    <trd lng="hu">kalap</trd>
    <trd lng="es">sombrero</trd>
    <trd lng="nl">hoed</trd>
    <trd lng="pt">chap&eacute;u</trd>
    <trd lng="ru">&c_sh;&c_l;&c_ja;&c_p;&c_a;</trd>
    <trd lng="sk">klob&uacute;k</trd>
  </snc>
  <snc mrk="cxapel.0o.supersigno">
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="diakrit.0ilo.LIN">Diakritilo</ref>,
      tegmentoforma supersigno, uzata en diversaj alfabetoj
      (interalie en esperanta, por formi la literojn &ccirc;, &gcirc;,
      &hcirc;, &jcirc;,
      &scirc;), en matematikaj simboloj kaj en programlingvoj (en tiuj
      lastaj &gcirc;i aperas kiel memstara signo, kies <ref tip="malprt"
      cel="aski.0o">Askia</ref> numero estas 94):
      <ekz>
        la <tld/>oj en la vortaraj tekstoj aspektas normale post instalo
        de specifaj tiparoj<fnt><aut>Sergeo Dereviannykh</aut>: <vrk>&Ccirc;u vi
        tradukas france?</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero
        4a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <trd
    lng="el">&pi;&epsilon;&rho;&jota;&sigma;&pi;&omega;&my;&epsilon_ton;&ny;&jota;</trd>
    <trdgrp lng="en">
      <trd>hat</trd>, <trd>caret</trd>, <trd>circumflex</trd>
    </trdgrp>
    <trd lng="fr">circonflexe</trd>
    <trdgrp lng="hu">
      <trd>kalap<klr tip="ind"> (&eacute;kezet)</klr></trd>,
      <trd>k&uacute;pos <ind>&eacute;kezet</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
   <trd>hoedje<klr tip="ind"> (^)</klr></trd>,
   <trd>kapje<klr tip="ind"> (^)</klr></trd>
  </trdgrp>
  <trd lng="pt">circunflexo</trd>
    <trd lng="ru">&c_k;&c_r;&c_y;&c_sh;&c_a;</trd>
    <trdgrp lng="sk">
      <trd>vok&aacute;&ncaron;</trd>,
      <trd>strie&scaron;ka</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">klobouk</trd>
  <trd lng="de">Hut</trd>
  <trd lng="sv">hatt</trd>
</drv>
</art>
