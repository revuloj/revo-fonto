<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krizalid</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="krizal.0">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      La dua stato de la ra&ubreve;po de lepidopteroj, anta&ubreve; ol fari&gcirc;i papilio.
    </dif>
    <refgrp tip="vid">
      <ref cel="kokon.0o">kokono</ref>,
      <ref cel="larv.0o">larvo</ref>,
      <ref cel="rauxp.0o">ra&ubreve;po</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Chrysalide</trd>,
     <trd>Puppe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chrysalis</trd>,
    <trd>pupa</trd>
  </trdgrp>
  <trd lng="es">cris&aacute;lida</trd>
  <trd lng="hu">b&aacute;b</trd>
  <trd lng="nl">pop<klr tip="ind"> (v.insect)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;<klr> (&c_b;&c_a;&c_b;&c_o;&c_ch;&c_k;&c_i;)</klr></trd>
</drv>
</art>
