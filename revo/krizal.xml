<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krizal.xml,v 1.22 2022/12/04 18:31:14 revo Exp $">
<kap>
  <rad>krizalid</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krizal.0o">
  <kap><tld/>o</kap>
  <snc mrk="krizal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La dua stato de lepidoptero post ra&ubreve;po kaj anta&ubreve; ol
      &gcirc;i fari&gcirc;as papilio.
    </dif>
    <refgrp tip="vid">
      <ref cel="kokon.0o.TEKS">kokono</ref>,
      <ref cel="larv.0o.ZOO">larvo</ref>,
      <ref cel="rauxp.0o">ra&ubreve;po</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_k;&c_a;&c_v;&c_i;&c_d;&c_a;</trd>
  <trd lng="ca">cris&agrave;lide</trd>
  <trdgrp lng="cs">
    <trd>kukla hmyzu</trd>,
    <trd>pupa</trd>
  </trdgrp>
    <trdgrp lng="de">
     <trd>Nymphe</trd>,
      <trd>Chrysalide</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>chrysalis</trd>,
    <trd>pupa</trd>
  </trdgrp>
  <trd lng="es">cris&aacute;lida</trd>
  <trd lng="fr">chrysalide</trd>
  <trd lng="hu">b&aacute;b</trd>
  <trdgrp lng="id">
    <trd>kepompong</trd>,
    <trd>pupa</trd>
  </trdgrp>
  <trd lng="nl">pop<klr tip="ind"> (v.insect)</klr></trd>
  <trd lng="pl">poczwarka</trd>
  <trd lng="pt">cris&aacute;lida</trd>
  <trd lng="ro">pup&abreve;</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;<klr> (&c_b;&c_a;&c_b;&c_o;&c_ch;&c_k;&c_i;)</klr></trd>
  <trd lng="sk">kukla hmyzu</trd>
  <trd lng="uk">&#x43B;&#x44F;&#x43B;&c_je;&c_ch;&#x43A;&c_a; (&#x43A;&#x43E;&#x43C;&c_a;&c_h;&c_i;)</trd>
</drv>

<drv mrk="krizal.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krizal.0igxi.ZOO">
    <dif>
      &Scirc;an&gcirc;i sian formon al <tld/>o:
      <ekz>
        ili estas ra&ubreve;poj somermeze, <tld/>i&gcirc;os
        kaj eli&gcirc;os kiel flugantaj papilioj en a&ubreve;gusto
        <fnt>
          <aut>Sakari</aut>,
          <vrk><url
          ref="https://nordanaturo.wordpress.com/2010/08/04/nova-papilia-generacio-kreskas/">
          Nova papilia generacio kreskas</url></vrk>,
          <lok>Norda naturo, 2010-08-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_k;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">se <ind>chrysalider</ind></trd>
  <trd lng="pl">przepoczwarzy&cacute; si&eogonek;</trd>     
  <trd lng="ro">transforma</trd>
</drv>
</art>
