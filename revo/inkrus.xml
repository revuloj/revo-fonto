<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inkrust</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="inkrus.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami la supra&jcirc;on de objekto per enigitaj pecetoj de alia materio:
      <ekz>
        kesteto <tld/>ita per ar&gcirc;ento.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>inkrustieren</trd>,
    <trd>einlegen</trd>,
    <trd><klr>als </klr>Einlegearbeit <ind>ausf&uuml;hren</ind></trd>
  </trdgrp>
  <trd lng="es">incrustar</trd>
  <trd lng="fr">incrustater</trd>
  <trd lng="nl">inleggen</trd>
  <trd
  lng="ru">&c_i;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="inkrus.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Desegno farita sur objekto per <tld/>ado:
      <ekz>
        meblo kun <tld/>a&jcirc;oj de perlamoto, eburo,
      </ekz>
      ktp.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Inkrustation</trd>,
    <trd>Einlegearbeit</trd>
  </trdgrp>
  <trd lng="en">inlay work</trd>
  <trd lng="es">incrustaci&oacute;n</trd>
  <trd lng="fr">incrustation</trd>
  <trd lng="nl">inlegwerk</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
