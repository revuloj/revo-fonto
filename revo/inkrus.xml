<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inkrus.xml,v 1.22 2023/11/19 10:22:13 revo Exp $">
<kap>
  <rad>inkrust</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="inkrus.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="inkrus.0i.BELA">
    <dif>
      <ref tip="super" cel="ornam.0i">Ornami</ref> la supra&jcirc;on de objekto per enigitaj pecetoj de alispeca
      materio:
      <ekz>
        ora &scirc;ildo, <tld/>ita per multekostaj &scirc;tonoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ligna&jcirc;istoj <klr>[&#x2026;]</klr> montris se&gcirc;ojn, tablojn, kanapojn,
        portilojn kaj veturilojn <klr>[&#x2026;]</klr> <tld/>itajn per
        diverskolora ligno, perlamoto kaj eburo
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 22a</lok></fnt>;
      </ekz>
      <ekz>
        muroj, parte <tld/>itaj per ru&gcirc;a marmoro, parte pentrornamitaj per arboj, fi&scirc;oj,
        birdoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        Nero, enirinte, sidi&gcirc;is sur se&gcirc;on, <tld/>itan per testuda &scirc;elo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Vinicius</nom> levis subite la kapon <klr>[&#x2026;]</klr> al la &ccirc;ielo, <tld/>ita per steloj,
        komencis pre&gcirc;i
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        baldakeno el rozokoloraj aranea&jcirc;oj, <tld/>itaj per plej &ccirc;armaj malgrandaj verdaj
        mu&scirc;oj, kiuj brilis kiel multekostaj &scirc;tonoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">ozdobn&ecaron; vykl&aacute;dat</trd>
  <trdgrp lng="de">
    <trd>inkrustieren</trd>,
    <trd>einlegen</trd>,
    <trd><klr>als </klr>Einlegearbeit <ind>ausf&uuml;hren</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>encrust</trd>,
    <trd>incrust</trd>
  </trdgrp>
  <trd lng="es">incrustar</trd>
  <trd lng="fr">incruster</trd>
  <trdgrp lng="hu">
    <trd>berak&aacute;ssal d&iacute;sz&iacute;t</trd>,
    <trd>inkruszt&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8C61;&#x773C;&#x3059;&#x308B;  <pr>&#x305E;&#x3046;&#x304C;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x306F;&#x3081;&#x8FBC;&#x307F;&#x7D30;&#x5DE5;&#x3092;&#x65BD;&#x3059;  <pr>&#x306F;&#x3081;&#x3053;&#x307F;&#x3055;&#x3044;&#x304F;&#x3092;&#x307B;&#x3069;&#x3053;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">inleggen</trd>
  <trd lng="pt">incrustar<klr tip="amb"> (adornar)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vkladan&iacute;m ozdobi&tcaron; povrch nie&ccaron;oho</trd>,
    <trd>vklada&tcaron;</trd>,
    <trd>vsadi&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_ib;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x9472;&#x5D4C; <pr>xi&#x101;ngqi&#xE0;n</pr></trd>,
    <trd>&#x7D50;&#x6BBC; <pr>ji&#xE9;k&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="inkrus.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="inkrus.0ajxo.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      Desegno farita sur objekto per <tld/>ado:
      <ekz>
        meblo kun <tld/>a&jcirc;oj de perlamoto, eburo, ktp;
      </ekz>
      <ekz>
        la dekoraciaj elementoj estis kreitaj sur&scirc;mirante farbon, stukon,
        <klr>[enigante]</klr> &scirc;ton<tld/>a&jcirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50289">Ta&gcirc;-Mahalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mozaik.0o">mozaiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Einlegearbeit</trd>,
    <trd>Inkrustation</trd>,
    <trd>Intarsie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>inlay work</trd>,
    <trd>intarsia</trd>
  </trdgrp>
  <trd lng="es">incrustaci&oacute;n</trd>
  <trd lng="fr">incrustation</trd>
  <trdgrp lng="hu">
    <trd>berak&aacute;sos d&iacute;sz&iacute;t&eacute;s</trd>,
    <trd>inkruszt&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8C61;&#x773C;  <pr>&#x305E;&#x3046;&#x304C;&#x3093;</pr></trd>,
    <trd>&#x306F;&#x3081;&#x8FBC;&#x307F;&#x7D30;&#x5DE5;  <pr>&#x306F;&#x3081;&#x3053;&#x307F;&#x3055;&#x3044;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">inlegwerk</trd>
  <trd lng="pt">incrusta&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_ib;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>
  <trd lng="zh">&#x9472;&#x5D4C; <pr>xi&#x101;ngqi&#xE0;n</pr></trd>
</drv>
</art>
