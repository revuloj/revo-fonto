<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: avokad.xml,v 1.18 2023/10/28 08:33:11 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>avokad</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="avokad.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="avokad.0o.KUI">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref> de <ref cel="avokad.0ujo"><tld/>ujo</ref>, kun verda
      a&ubreve; bruneta &scirc;elo, tre nutra, butereca karno, kaj unu sola tre
      granda malmola kerno:
      <ekz>
        la tiel nomataj &leftquot;bonaj grasoj&rightquot;, troveblaj ene de olivoleo, <tld/>o kaj migdaloj,
        reale protektas la koron
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2014/011298.php">Ela&ccirc;etas sin (kelkaj) grasoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; botanika vidpunkto <tld/>o estas
        <ref tip="super" cel="ber.0o.BOT">bero</ref>
        <fnt>
        <bib>Viki</bib>,
        <lok>avokado, 2005-03-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x9C77;&#x68A8; <pr>&#xE8;l&#xED;</pr></trd>,
    <trd>&#x9CC4;&#x68A8; <pr>&#xE8;l&#xED;</pr></trd>,
    <trd>&#x916A;&#x68A8; <pr>lu&#xF2;l&#xED;</pr></trd>,
    <trd>&#x725B;&#x6CB9;&#x679C; <pr>ni&#xFA;y&#xF3;ugu&#x1D2;</pr></trd>,
    <trd>&#x6CB9;&#x68A8; <pr>y&#xF3;ul&#xED;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_k;&c_a;&c_d;&c_a;
  <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;</trd>
  <trd lng="de">Avocado</trd>
  <trd lng="en">avocado</trd>
  <trd lng="es">aguacate, palta (fruto) [Sudam&eacute;rica]</trd>
  <trd lng="fa">&a_A_madda;&a_w;&a_w;&f_k;&a_A;&a_d;&a_w;</trd>
  <trd lng="fr">avocat<klr tip="amb"> (fruit)</klr></trd>
  <trd lng="gl">aguacate</trd>
  <trd lng="he">&alef;&bet;&vav;&qof;&dalet;&vav;</trd>
  <trd lng="hu">avok&aacute;d&oacute;</trd>
  <trdgrp lng="id">
    <trd>alpukat</trd>,
    <trd>avokad</trd>
  </trdgrp>
  <trd lng="it">avocado</trd>
  <trd lng="ja">&#x30A2;&#x30DC;&#x30AB;&#x30C9;</trd>
  <trd lng="pt">abacate, abacado</trd>
  <trd lng="ru">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;<klr> (&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="uk">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;&nbsp;(&c_p;&c_l;&c_ib;&c_d;)</trd>
</drv>

<drv mrk="avokad.0ujo">
  <kap><ofc>9</ofc><tld/>ujo,
  <var><kap><ofc>9</ofc><tld/>arbo</kap></var></kap>
  <snc mrk="avokad.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el Centra Ameriko, el la familio
      <ref tip="super" cel="lauxrac.0oj">la&ubreve;racoj</ref>
      (<trd lng="la">Persea americana</trd>),
      kultivata en tropikaj landoj pro siaj
      fruktoj.
    </dif>
    <bld lok="&WCU;/thumb/2/23/Avocado_6.JPG/640px-Avocado_6.JPG" prm="CC BY-SA 4.0">tigo kun fruktoj de <tld/>ujo
        <fnt>
          <aut>Petar43, propra verko</aut>
          <lok><url ref="&WCW;/File:Avocado_6.JPG">https://commons.wikimedia.org/wiki/File:Avocado_6.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_k;&c_a;&c_d;&c_a;
  <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;</trd>
  <trd lng="de">Avocadobaum</trd>
  <trd lng="en">avocado</trd>
  <trd lng="es">aguacate, palta (&aacute;rbol) [Sudam&eacute;rica]</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;
  <ind>&a_A_madda;&a_w;&a_w;&f_k;&a_A;&a_d;&a_w;</ind></trd>
  <trd lng="fr">avocatier</trd>
  <trd lng="gl">aguacate</trd>
  <trd lng="hu">avok&aacute;d&oacute;fa</trd>
  <trd lng="it">avocado</trd>
  <trd lng="ja">&#x30A2;&#x30DC;&#x30AB;&#x30C9;</trd>
  <trd lng="pt">abacateiro</trd>
  <trd lng="ru">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;<klr> (&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;&nbsp;(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</trd>
</drv>
</art>
