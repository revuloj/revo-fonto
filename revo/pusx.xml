<drv mrk="pusx.pied0i">
  <kap>
    pied<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="P"/>i per piedo:
      <ekz>
        restoraciaj vi&scirc;tukoj <klr>[&#x2026;]</klr> glitadis sur la tuta &scirc;ar&gcirc;oplato, <nom>Gruntin</nom>
        <klr>[&#x2026;]</klr> pied<tld/>is ilin malata&ubreve;en kaj &ccirc;esis atenti ilin
        <fnt>
          <bib>MkM</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li akceptas pied<tld/>on de l&#x2019; subkapitano kaj <tld/>i&gcirc;as sub la ferdekon
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="tret.0i">treti</ref>
  </snc>
  <trd lng="de">treten</trd>
  <trd lng="en">kick</trd>
  <trdgrp lng="zh">
    <trd>&#x8E36; <pr>d&#xEC;</pr></trd>,
    <trd>&#x8E39; <pr>chu&#xE0;i</pr></trd>,
