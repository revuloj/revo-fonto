<drv mrk="pusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="pusx.0o.ekpremo">
    <dif>
      Ekpremo a&ubreve; ekfrapo, kiu delokigas:
      <ekz>
        ricevi <tld/>ojn de sia patro;
      </ekz>
      <ekz>
        la glitveturilo flugis anta&ubreve;en: de tempo al tempo oni sentis <tld/>on, kvaza&ubreve; oni veturus super kavojn a&ubreve; arbeta&jcirc;ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        donu al &scirc;i malgrandan <tld/>on tiel, ke &scirc;i falu en la akvon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[pro]</klr> fortegaj vent<tld/>oj <klr>[&#x2026;]</klr> en la aero 
        flugadis simile al cigna lanugo la &scirc;a&ubreve;mo el la maro
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam nia afero ricevis potencan <tld/>on anta&ubreve;en
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sto&szlig;</trd>,
      <trd>Schubs</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tol&aacute;s</trd>,
      <trd>l&ouml;k&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pchni&eogonek;cie</trd>,
      <trd>popchni&eogonek;cie</trd>,
      <trd>szturchni&eogonek;cie</trd>
    </trdgrp>
  </snc>
  <snc mrk="pusx.0o.malamika">
    <dif>
      <ref tip="dif" cel="atak.0o">Atako</ref>:
      <ekz>
        furioza <tld/>o de pasio atakas nun subite la
        pastrinon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        se <klr>[&#x2026;]</klr> per unu <tld/>o de ponardo
        li povus sin de &ccirc;io liberigi
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; &ccirc;iuj ventoj kaj <tld/>oj Esperanto vivis kaj kreskis regule
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>.
       </ekz>
     </dif>
    <trd lng="hu">roham</trd>
    <trdgrp lng="pl">
      <trd>pchni&eogonek;cie</trd>,
      <trd>popchni&eogonek;cie</trd>,
      <trd>szturchni&eogonek;cie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_h;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_h;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">empuje</trd>
  <trdgrp lng="fr">
    <trd>impulsion</trd>,
    <trd>pouss&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>duw</trd>,
    <trd>stoot</trd>
  </trdgrp>
  <trd lng="no">dytt</trd>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_ch;&c_o;&c_k;</trd>
</drv>

<drv mrk="pusx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="pusx.0igxi.MOV">
    <dif>
      Esti <tld/>ata; ricevi <tld/>ojn:
      <ekz>
        <tld/>i&gcirc;i per la kapo al la muro;
     </ekz>
     <ekz>
        ili <tld/>i&gcirc;as je tiom da malhelpoj <klr>[&#x2026;]</klr> ke ili
        perdas &ccirc;ian paciencon
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 7a</lok></fnt>;
     </ekz>
     <ekz>
        vi iros sendan&gcirc;ere vian vojon kaj via piedo ne
        fal<tld/>i&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 3:23</lok>
        </fnt>;
     </ekz>
     <ekz>
        &ccirc;ies libero limi&gcirc;as tie, kie &gcirc;i <tld/>i&gcirc;as
        kontra&ubreve; la libero de aliaj;
     </ekz>
     <ekz>
        <ref tip="vid" cel="fal.0pusxigxi">fal<tld/>i&gcirc;i</ref>
        &ccirc;e
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 9:32</lok>
        </fnt>,
        sur
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 91:12</lok>
        </fnt>
        &scirc;tono;
     </ekz>
     <ekz>
        <uzo tip="stl">FIG</uzo>
        la entrepreno <tld/>i&gcirc;as je malhelpoj
        <fnt>
          <bib>EE</bib>
        </fnt>.
     </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="bat">albati&gcirc;i</ref>,
      <ref cel="kolizi.0i">kolizii</ref>,
      <ref cel="pel">sinpelado</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">getrieben sein</trd>
  <trd lng="fr">se <ind>heurter</ind></trd>
  <trdgrp lng="hu">
    <trd>&uuml;tk&ouml;zik</trd>,
    <trd>l&ouml;k&odblac;dik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>z. <ind>stoten</ind></trd>,
    <trd>aanbotsen</trd>,
    <trd>ge<ind>duw</ind>d worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pcha&cacute; si&eogonek;</trd>,
    <trd>popychcha&cacute; si&eogonek;</trd>,
    <trd>przepycha&cacute; si&eogonek;</trd>,
    <trd>szturcha&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.antauxen0i">
  <kap>
     anta&ubreve;en<tld/>i
  </kap>
  <snc mrk="pusx.antauxen0i.antauenigi">
    <dif>
      <ref tip="dif" cel="antaux.0enigi">Anta&ubreve;enigi</ref>:
      <ekz>
        mia mano samzorge turnis la klinkon de la pordo, kaj &gcirc;in anta&ubreve;en<tld/>is 
        kvaza&ubreve; &scirc;telisto surprizonta trankvilan lo&gcirc;anton
        <fnt>
          <bib>Monato</bib>
          <aut>Eriko Navaro</aut>
          <vrk><url ref="&Monato;/2014/011034.php">Sonorila a&ubreve;guro - Rememoriga plendletero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio estas la sola ebleco anta&ubreve;en<tld/>i la aferon
        <fnt><aut>S. &Scaron;timec</aut>, <vrk>Ombro sur interna pejza&gcirc;o</vrk>, 
        <lok>Edistudio, 1984</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la financpova alianco de firmaoj kaj politiko multloke energie klopodas anta&ubreve;en<tld/>i privatigon
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2009/009801.php">La blua miraklo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>anschieben</trd>,
      <trd>voranbringen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>avancer</ind></trd>
    <trd>promouvoir</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pcha&cacute;</trd>,
    <trd>popycha&cacute;</trd>,
    <trd>rusza&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.de0i.MOV">
    <dif>
      Faligi <tld/>ante:
      <ekz>
        de<tld/>inte la kapu&ccirc;on de l' oreloj, li a&ubreve;skultis<fnt>B</fnt>;
      </ekz>
      <ekz>
        e&ccirc; se ili suprenirus en la &ccirc;ielon, mi de tie de<tld/>os
        ilin
        <fnt>
          <bib>MT</bib>
          <lok>Amos 9:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nia lingvo ne bezonas jam timi, ke io pereige de<tld/>us &gcirc;in de
        tiu natura rekta vojo, la&ubreve; kiu &gcirc;i evoluas
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">herunter <ind>sto&szlig;en</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_ib;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;stabiliser</trd>,
    <trd>faire tomber <klr>(en poussant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lel&ouml;k</trd>,
    <trd>letasz&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">afstoten</trd>
  <trd lng="pl">spycha&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;<klr tip="amb"> (&c_ch;&c_t;&c_o;-&c_l;. &c_s; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
 </drv>

<drv mrk="pusx.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.dis0i.MOV">
    <dif>
      Disigi <tld/>ante:
      <ekz>
        la talpo ekpremis per sia lar&gcirc;a nazo <klr>[&#x2026;]</klr> kaj dis<tld/>is la teron
        tiamaniere, ke fari&gcirc;is granda truo, tra kiu penetris lumo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la policistoj estis devigitaj dis<tld/>i ilin por liberigi la vojon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zersto&szlig;en</trd>,
      <trd>auseinander <ind>sto&szlig;en</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_p;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_p;&c_ib;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;carter</trd>
  <trd lng="hu">sz&eacute;tl&ouml;k</trd>
  <trd lng="nl">uiteen <ind>duwen</ind></trd>
  <trd lng="pl">rozpycha&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_i;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_i;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.el0i">
  <kap>el<tld/>i,
  <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.el0i.MOV">
    <dif>
      Forigi <tld/>ante:
      <ekz>
        el<tld/>i iun de lia loko;
      </ekz>
      <ekz>
        for<tld/>i almozulon el la korto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        de l' fremdulo for<tld/>as min la timo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estos el<tld/>ita el via hereda&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 17:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto neniom celas el<tld/>i la naciajn
        lingvojn
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
       </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel.for0i">forpeli</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_p;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_p;&c_ib;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kil&ouml;k</trd>,
      <trd>f&eacute;lrel&ouml;k</trd>
    </trdgrp>
    <trd lng="pl">wypycha&cacute; <klr>(sk&aogonek;d&sacute;)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pusx.el0i.FIG">
    <dif>
      Malkonsenti, malakcepti, rifuzi:
      <ekz>
        for<tld/>i peton
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 102:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        for<tld/>i admonon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:32</lok>
        </fnt>;
      </ekz>
      <ekz>
       for<tld/>i instruon
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:19</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 13:18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bala">forbalai</ref>,
      <ref cel="for.0igi">forigi</ref>,
      <ref cel="jxet.for0i">for&jcirc;eti</ref>
    </refgrp>
    <trd lng="de">versto&szlig;en</trd>
    <trd lng="hu">elutas&iacute;t</trd>
    <trd lng="pl">odrzuca&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_m;&c_je;&c_s;&c_t;&c_i;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>aussto&szlig;en</trd>,
    <trd>wegsto&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pousser hors de</trd>,
    <trd>repousser</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wegstoten</trd>,
    <trd>uitstoten</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.en0i.PSI">
    <dif>
      <tld lit="P"/>i en ion:
      <ekz>
        la knabino <klr>[&#x2026;]</klr> superforte en<tld/>is la piedon en la &scirc;uon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        en<tld/>i al iu la tran&ccirc;ilon en la korpon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">hineinsto&szlig;en</trd>
  </snc>
  <trd lng="fr">enfoncer</trd>
</drv>

<drv mrk="pusx.inter0igxo">
  <kap>inter<tld/>i&gcirc;o</kap>
  <snc mrk="pusx.inter0igxo.MOV">
    <dif>
      Reciprokaj premi&gcirc;ado kaj repu&scirc;i&gcirc;ado:
      <ekz>
        nia laborejo <klr>[&#x2026;]</klr> havas plej bonan reputacion <klr>[&#x2026;]</klr> tial 
        laboristinoj sin pu&scirc;as al ni, amase <klr>[&#x2026;]</klr> estas vera interpu&scirc;i&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la amaso fari&gcirc;is neimagebla inter<tld/>i&gcirc;o
        <fnt>
          <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
          <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
          Kristanismo kaj Patriotismo</url></vrk>,
          <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tumult.0o.KOMUNE">tumulto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Zusammensto&szlig;</trd>,
    <trd>Gedr&auml;nge</trd>,
    <trd>Andrang</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bousculade</trd>,
    <trd>cohue</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <snc mrk="pusx.kun0igxi.MOV">
    <dif>
      <tld lit="P"/>i&gcirc;i unu kontra&ubreve; alia:
      <ekz>
        amba&ubreve; aliras samtempe kaj kun<tld/>i&gcirc;as per la fruntoj
        <fnt>
          <aut>N. V. Gogol, trad. L. L. Zamenhof</aut>,
          <vrk>La revizoro</vrk>,
          <lok>5:5</lok>.
        </fnt>;
      </ekz>
      <ekz>
         en la pordo ili kun<tld/>i&gcirc;as kun la revenanta policano
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
         ni povas perei en la okazo, se ia el la kometoj kun<tld/>i&gcirc;os kun la tero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la lingvoj mem, konstante kun<tld/>i&gcirc;ante unuj kun la aliaj, &ccirc;iam pli kaj pli konfuzi&gcirc;as, kripli&gcirc;as 
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">collide</trd>
  <trdgrp lng="de">
    <trd>zusammensto&szlig;en</trd>,
    <trd>kollidieren</trd>
  </trdgrp>
  <trd lng="fr">se <ind>buter</ind></trd>
  <trd lng="pl">stuka&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.re0i.MOV">
    <dif>
      En kontra&ubreve;an direkton <tld/>i:
      <ekz>
        dum pafado la du &leftquot;brakoj&#x201D; de la arko &leftquot;re<tld/>i&gcirc;as&#x201D;, kaj la rezultanta energio pluigas la forton de la eligata sago
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2008/009409.php">Ne nur sporto, anka&ubreve; vivoformo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        re<tld/>i atakon, malamikojn;
      </ekz>
      <ekz>
        re<tld/>i iun kun malestimo;
      </ekz>
      <ekz>
        re<tld/>i <klr>(refuti)</klr> argumenton;
      </ekz>
      <ekz>
        re<tld/>i la sa&gcirc;ulojn malanta&ubreve;en kaj malsa&gcirc;igas ilian scion
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 44:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        David re<tld/>is siajn homojn per <klr>[siaj]</klr> vortoj, kaj ne permesis al ili levi&gcirc;i kontra&ubreve; Saulon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 24:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili re<tld/>as influon de Va&scirc;ingtono kaj lamigas Unui&gcirc;intajn Naciojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009572.php">Perdata influo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;cksto&szlig;en</trd>,
      <trd>zur&uuml;ckdr&auml;ngen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="fr">repousser</trd>
  <trdgrp lng="hu">
    <trd>visszal&ouml;k</trd>,
    <trd>visszanyom</trd>
  </trdgrp>
  <trd lng="nl">terugdringen</trd>
  <trd lng="pl">odpiera&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.tra0i.MOV">
    <dif>
      Traigi <tld/>ante:
      <ekz>
        la nigre vestita virino rapide tra<tld/>i&gcirc;adis tra la amasoj da pasantoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiamaniere mi povos tra<tld/>i min tra la mustruo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>durchsto&szlig;en</trd>,
      <trd>hindurchzw&auml;ngen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="fr">se <ind>frayer</ind> un chemin</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tl&ouml;k</trd>,
    <trd>&aacute;ttasz&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>duwen door</trd>,
    <trd>doorstoten</trd>
  </trdgrp>
  <trd lng="pl">przepycha&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_i;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

