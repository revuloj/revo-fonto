<drv mrk="pusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="pusx.0o.ekpremo">
    <dif>
      Ekpremo a&ubreve; ekfrapo, kiu delokigas:
      <ekz>
        ricevi <tld/>ojn de sia patro;
      </ekz>
      <ekz>
        la glitveturilo flugis anta&ubreve;en: de tempo al tempo oni sentis <tld/>on, kvaza&ubreve; oni veturus
        super kavojn a&ubreve; arbeta&jcirc;ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        donu al &scirc;i malgrandan <tld/>on tiel, ke &scirc;i falu en la akvon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[pro]</klr> fortegaj vent<tld/>oj <klr>[&#x2026;]</klr> en la aero
        flugadis simile al cigna lanugo la &scirc;a&ubreve;mo el la maro
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam nia afero ricevis potencan <tld/>on anta&ubreve;en
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sto&szlig;</trd>,
      <trd>Schubs</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tol&aacute;s</trd>,
      <trd>l&ouml;k&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pchni&eogonek;cie</trd>,
      <trd>popchni&eogonek;cie</trd>,
      <trd>szturchni&eogonek;cie</trd>
    </trdgrp>
  </snc>
  <snc mrk="pusx.0o.malamika">
    <dif>
      <ref tip="dif" cel="atak.0o">Atako</ref>:
      <ekz>
        furioza <tld/>o de pasio atakas nun subite la
        pastrinon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        se <klr>[&#x2026;]</klr> per unu <tld/>o de ponardo
        li povus sin de &ccirc;io liberigi
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; &ccirc;iuj ventoj kaj <tld/>oj Esperanto vivis kaj kreskis regule
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>.
       </ekz>
     </dif>
    <trd lng="hu">roham</trd>
    <trdgrp lng="pl">
      <trd>pchni&eogonek;cie</trd>,
      <trd>popchni&eogonek;cie</trd>,
      <trd>szturchni&eogonek;cie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_h;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_h;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">empuje</trd>
  <trdgrp lng="fr">
    <trd>impulsion</trd>,
    <trd>pouss&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x62BC;&#x3057;  <pr>&#x3044;&#x3061;&#x304A;&#x3057;</pr></trd>,
    <trd>&#x4E00;&#x7A81;&#x304D;  <pr>&#x3072;&#x3068;&#x3064;&#x304D;</pr></trd>,
    <trd>&#x4E00;&#x6483;  <pr>&#x3044;&#x3061;&#x3052;&#x304D;</pr></trd>,
    <trd>&#x6253;&#x6483;  <pr>&#x3060;&#x3052;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
