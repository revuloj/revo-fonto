<drv mrk="pusx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.re0i.MOV">
    <dif>
      En kontra&ubreve;an direkton <tld/>i:
      <ekz>
        dum pafado la du &leftquot;brakoj&#x201D; de la arko &leftquot;re<tld/>i&gcirc;as&#x201D;, kaj la rezultanta energio
        pluigas la forton de la eligata sago
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2008/009409.php">Ne nur sporto, anka&ubreve; vivoformo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        re<tld/>i atakon, malamikojn;
      </ekz>
      <ekz>
        re<tld/>i iun kun malestimo;
      </ekz>
      <ekz>
        re<tld/>i <klr>(refuti)</klr> argumenton;
      </ekz>
      <ekz>
        re<tld/>i la sa&gcirc;ulojn malanta&ubreve;en kaj malsa&gcirc;igas ilian scion
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 44:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> re<tld/>is siajn homojn per <klr>[siaj]</klr> vortoj, kaj ne permesis al ili levi&gcirc;i
        kontra&ubreve; <nom>Saulo</nom>n
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 24:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili re<tld/>as influon de Va&scirc;ingtono kaj lamigas Unui&gcirc;intajn Naciojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009572.php">Perdata influo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;cksto&szlig;en</trd>,
      <trd>zur&uuml;ckdr&auml;ngen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="fr">repousser</trd>
  <trdgrp lng="hu">
    <trd>visszal&ouml;k</trd>,
    <trd>visszanyom</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x62BC;&#x3057;&#x8FD4;&#x3059;  <pr>&#x304A;&#x3057;&#x304B;&#x3048;&#x3059;</pr></trd>,
    <trd>&#x6483;&#x9000;&#x3059;&#x308B;  <pr>&#x3052;&#x304D;&#x305F;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6291;&#x5727;&#x3059;&#x308B;  <pr>&#x3088;&#x304F;&#x3042;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">terugdringen</trd>
  <trd lng="pl">odpiera&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#x6421; <pr>s&#x1CE;ng</pr></trd>
</drv>

<drv mrk="pusx.tra0i">
