<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pu&scirc;</rad>/i
</kap>
<drv mrk="pusx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pusx.0i.delokigi">
    <dif>
      Delokigi per premo a&ubreve; frapo:
      <ekz>
        <tld/>i iun per kubuto, genuo, &scirc;tonon per piedo;
      </ekz>
      <ekz>
        li <tld/>is min eksteren;
      </ekz>
      <ekz>
        &ccirc;ies libero limi&gcirc;as tie, kie &gcirc;i <tld/>as
        kontra&ubreve; la libero de aliaj;
      </ekz>
      <ekz>
        &scirc;a&ubreve;mo <tld/>ata de la vento fluas sur la bordo<fnt>B</fnt>;
      </ekz>
      <ekz>
        ia nevenkebla deziro <tld/>adis lin anta&ubreve;en;
      </ekz>
      <ekz>
        en<tld/>i al iu la tran&ccirc;ilon en la korpon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel.0i">peli</ref>,
      <ref cel="sxov.0i">&scirc;ovi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>sto&szlig;en</trd>,
      <trd>schieben</trd>,
      <trd>r&uuml;cken</trd>,
      <trd>schubsen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tol</trd>,
      <trd>l&ouml;k</trd>
    </trdgrp>
  </snc>
  <snc mrk="pusx.0i.peni">
    <dif>
      Peni meti iun en ian staton:
      <ekz>
        li <tld/>as Egipton al la milito<fnt>B</fnt>;
      </ekz>
      <ekz>
        vi min elpelus el la regno kaj vi <tld/>us min al la
        mizero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li al generaleco sin <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiuj malbonaj kondi&ccirc;oj <tld/>as ilin sur la vojon al striko;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        reciproka malkonfido <tld/>as ilin al kontra&ubreve;staro<fnt>B</fnt>;
      </ekz>
      <ekz>
        malplena sako <tld/>as al peko
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>ata de la deziro vidi<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ata de la entuziasmo<fnt>B</fnt>;
      </ekz>
      <ekz>
        &scirc;ia forta pedagogia talento <tld/>is &scirc;in al la instruista
        profesio;
      </ekz>
      <ekz>
        pro la persisto de la intergentaj malamoj la homaro estus fatale
        <tld/>ata al novaj katastrofoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="instig.0i">instigi</ref>
    </refgrp>
    <rim>
      <em>Pu&scirc;i</em> distingi&gcirc;as de <em>peli</em> &ccirc;ar &gcirc;i
      entenas ideon de
      senpera kontakto, kaj metafore tiun de pli kruda forto.
    </rim>
    <trd lng="de">treiben</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_ib;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">strkat</trd>
  <trd lng="en">push</trd>
  <trd lng="es">empujar</trd>
  <trd lng="fr">pousser</trd>
  <trd lng="hu">tasz&iacute;t</trd>
  <trdgrp lng="nl">
    <trd>duwen</trd>,
    <trd>stoten</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="pusx.0o.ekpremo">
    <dif>
      Ekpremo a&ubreve; ekfrapo, kiu delokigas:
      <ekz>
        per unu <tld/>o de ponardo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ricevi <tld/>ojn de sia patro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam nia afero ricevis potencan <tld/>on anta&ubreve;en<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sto&szlig;</trd>,
      <trd>Schubs</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tol&aacute;s</trd>,
      <trd>l&ouml;k&eacute;s</trd>
    </trdgrp>
  </snc>
  <snc mrk="pusx.0o.de_sento">
    <dif>
      Ekatako de sento:
      <ekz>
        furioza <tld/>o de pasio atakas nun subite la
        pastrinon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">roham</trd>
  </snc>
  <snc mrk="pusx.0o.malamika">
    <dif>
      Malamika atako:
      <ekz>
        malgra&ubreve; &ccirc;iuj ventoj kaj <tld/>oj Esperanto vivis kaj kreskis
        regule<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">roham</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_h;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_h;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">empuje</trd>
  <trdgrp lng="fr">
    <trd>impulsion</trd>,
    <trd>pouss&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>duw</trd>,
    <trd>stoot</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_ch;&c_o;&c_k;</trd>
</drv>

<drv mrk="pusx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti <tld/>ata; ricevi <tld/>ojn:
      <ekz>
        <tld/>i&gcirc;i per la kapo al la muro;
      </ekz>
      <ekz>
        vi iros sendan&gcirc;ere vian vojon kaj via piedo ne
        <tld/>i&gcirc;os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fal<tld/>i&gcirc;i je &scirc;tono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la entrepreno <tld/>i&gcirc;is je malhelpoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        longe kun<tld/>i&gcirc;is kun la indiferenteco de la publiko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel">sinpelado</ref>,
      <ref cel="kolizi.0i">kolizii</ref>,
      <ref cel="bat">albati&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">getrieben sein</trd>
  <trdgrp lng="fr">
    <trd>se <ind>heurter</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;tk&ouml;zik</trd>,
    <trd>l&ouml;k&odblac;dik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>z. <ind>stoten</ind></trd>,
    <trd>aanbotsen</trd>,
    <trd>ge<ind>duw</ind>d worden</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Faligi <tld/>ante:
      <ekz>
        de<tld/>inte la kapu&ccirc;on de l' oreloj, li a&ubreve;skultis<fnt>B</fnt>;
      </ekz>
      <ekz>
        e&ccirc; se ili suprenirus en la &ccirc;ielon, mi de tie de<tld/>os
        ilin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nia lingvo ne bezonas jam timi, ke io pereige de<tld/>us &gcirc;in de
        tiu natura rekta vojo, la&ubreve; kiu &gcirc;i evoluas<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_ib;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;stabiliser</trd>,
    <trd>faire tomber <klr>(en poussant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lel&ouml;k</trd>,
    <trd>letasz&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">afstoten</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;<klr tip="amb"> (&c_ch;&c_t;&c_o;-&c_l;. &c_s; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi <tld/>ante.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_p;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_p;&c_ib;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;carter</trd>
  <trd lng="hu">sz&eacute;tl&ouml;k</trd>
  <trd lng="nl">uiteen <ind>duwen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_i;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_i;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.el0ifor0i">
  <kap>el<tld/>i, for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi <tld/>ante:
      <ekz>
        el<tld/>i iun de lia loko;
      </ekz>
      <ekz>
        for<tld/>i almozulon el la korto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        de l' fremdulo for<tld/>as min la timo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estos el<tld/>ita el via hereda&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto neniom celas el<tld/>i la naciajn lingvojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel.for0i">forpeli</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_p;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_p;&c_ib;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kil&ouml;k</trd>,
      <trd>f&eacute;lrel&ouml;k</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_a;&c_l;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Malkonsenti, malakcepti, rifuzi:
      <ekz>
        for<tld/>i peton<fnt>Z</fnt>,
      </ekz>
      <ekz>
        admonon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        instruon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bala">forbalai</ref>,
      <ref cel="for.0igi">forigi</ref>,
      <ref cel="jxet.for0i">for&jcirc;eti</ref>
    </refgrp>
    <trd lng="hu">elutas&iacute;t</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_m;&c_je;&c_s;&c_t;&c_i;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>pousser hors de</trd>,
    <trd>repousser</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wegstoten</trd>,
    <trd>uitstoten</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      En kontra&ubreve;an direkton <tld/>i:
      <ekz>
        re<tld/>i atakon, malamikojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        re<tld/>i iun kun malestimo;
      </ekz>
      <ekz>
        re<tld/>i (refuti) argumenton.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_a;&c_d;&c_sh;&c_t;&c_u;&c_r;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="fr">repousser</trd>
  <trdgrp lng="hu">
    <trd>visszal&ouml;k</trd>,
    <trd>visszanyom</trd>
  </trdgrp>
  <trd lng="nl">terugdringen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pusx.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Traigi <tld/>ante:
      <ekz>
        kiamaniere mi povos tra<tld/>i min tra la mustruo?<fnt>Z</fnt>
      </ekz>
      <ekz>
        li tra<tld/>i&gcirc;is tra la amasoj da pasantoj;
      </ekz>
      <ekz>
        sin tra<tld/>adi<fnt>Z</fnt>
        <klr>(sin entrudi)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="fr">se <ind>frayer</ind> un chemin</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tl&ouml;k</trd>,
    <trd>&aacute;ttasz&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>duwen door</trd>,
    <trd>doorstoten</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_i;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
