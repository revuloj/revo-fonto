  <trd lng="ru">&c_t;&c_o;&c_l;&c_ch;&c_o;&c_k;</trd>
  <trd lng="uk">&c_p;&c_o;&c_sh;&c_t;&c_o;&c_v;&c_h;</trd>
</drv>

<drv mrk="pusx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="pusx.0igxi.MOV">
    <dif>
      Esti <tld/>ata; ricevi <tld/>ojn:
      <ekz>
        <tld/>i&gcirc;i per la kapo al la muro;
     </ekz>
     <ekz>
        ili <tld/>i&gcirc;as je tiom da malhelpoj <klr>[&#x2026;]</klr> ke ili
        perdas &ccirc;ian paciencon
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 7a</lok></fnt>;
     </ekz>
     <ekz>
        vi iros sendan&gcirc;ere vian vojon kaj via piedo ne
        fal<tld/>i&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 3:23</lok>
        </fnt>;
     </ekz>
     <ekz>
        &ccirc;ies libero limi&gcirc;as tie, kie &gcirc;i <tld/>i&gcirc;as
        kontra&ubreve; la libero de aliaj;
     </ekz>
     <ekz>
        <ref tip="vid" cel="fal.0pusxigxi">fal<tld/>i&gcirc;i</ref>
        &ccirc;e
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 9:32</lok>
        </fnt>,
        sur
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 91:12</lok>
        </fnt>
        &scirc;tono;
     </ekz>
     <ekz>
        <uzo tip="stl">FIG</uzo>
        la entrepreno <tld/>i&gcirc;as je malhelpoj
        <fnt>
          <bib>EE</bib>
        </fnt>.
     </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="bat">albati&gcirc;i</ref>,
      <ref cel="kolizi.0i">kolizii</ref>,
      <ref cel="pel">sinpelado</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">getrieben sein</trd>
  <trd lng="fr">se <ind>heurter</ind></trd>
  <trdgrp lng="hu">
    <trd>&uuml;tk&ouml;zik</trd>,
    <trd>l&ouml;k&odblac;dik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3076;&#x3064;&#x304B;&#x308B;</trd>,
    <trd>&#x7A81;&#x304D;&#x5F53;&#x305F;&#x308B;  <pr>&#x3064;&#x304D;&#x3042;&#x305F;&#x308B;</pr></trd>,
    <trd>&#x62BC;&#x3057;&#x5408;&#x3046;  <pr>&#x304A;&#x3057;&#x3042;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>z. <ind>stoten</ind></trd>,
    <trd>aanbotsen</trd>,
    <trd>ge<ind>duw</ind>d worden</trd>
  </trdgrp>
