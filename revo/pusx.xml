<drv mrk="pusx.vento0o">
  <kap>vento<tld/>o, <var><kap>vent<tld/>o</kap></var></kap>
  <snc mrk="pusx.vento0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Subita, iom forta ventoblovo:
      <ekz>
        sekvis ankora&ubreve; unu vent<tld/>o, kaj &ccirc;iuj sur &scirc;nuro pendantaj fi&scirc;oj ekbati&gcirc;is al la muro de la domo,
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vent<tld/>o trafas &gcirc;in, la tuta konstrua&jcirc;o 
        skui&gcirc;as
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/tekstoj/johansson/hobi.html">
          Stranga hobio</url></vrk>,
          <lok>[2014?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> kriis, kiam vento<tld/>o &gcirc;in &jcirc;etis sur la dorson
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="burask.0o.MET">burasko</ref>
    <trdgrp lng="de">
      <trd>Windsto&szlig;</trd>,
      <trd>B&ouml;</trd>,
      <trd>Windb&ouml;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>bourrasque</trd>,
    <trd>coup de vent</trd>,
    <trd>grain<klr> (coup de vent)</klr></trd>    
  </trdgrp>
  <trd lng="nl">windstoot</trd>
</drv>
