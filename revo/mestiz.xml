<vortaro>
<art mrk="$Id$">
<kap>
<rad>mestiz</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mestiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="mestiz.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="ras.miks0ulo.BIO">Miksrasulo</ref>, ido de
      blankrasa kaj indianrasa gepatroj:
      <ekz>
        homoj el tiuj landoj, e&ccirc; se ili havas e&ubreve;ropajn
        nomojn, estas a&ubreve; indianoj <klr>...</klr> a&ubreve; partindianoj
        (<klr>...</klr> &leftquot;<tld/>o&rightquot;)
        <fnt>
        <aut>D. J. Harlow</aut>,
        <vrk>Re: Pripensinda statististiko</vrk>,
        <lok>soc.culture.esperanto, 2001-05-27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gauxcx.0o">ga&ubreve;&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_y;&c_s;</trd>
  <trd lng="ca">mest&iacute;s</trd>
  <trd lng="de">Mestize</trd>
  <trd lng="en">mestizo</trd>
  <trd lng="es">mestizo</trd>
  <trd lng="fr">m&eacute;tis<klr tip="amb"> (blanc et indien)</klr>
  </trd>
  <trd lng="hu">mesztic</trd>
  <trd lng="nl">mesties</trd>
  <trd lng="pt">mesti&ccedil;o</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_i;&c_s;</trd>
</drv>
</art>
