<vortaro>
<art mrk="$Id$">
<kap>
  <rad>diafiz</rad>/o
</kap>
<drv mrk="diafiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La longa parto de osto inter &gcirc;iaj du ekstremoj:
      <ref cel="apofiz.0o">apofizo</ref> kaj <ref
      cel="epifiz.0o">epifizo</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Diaphyse</trd>,
    <trd>Diaphysis</trd>,
    <trd>R&ouml;hrenknochenmittelst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fr">diaphyse</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_f;&c_i;&c_z;</trd>
</drv>
</art>
