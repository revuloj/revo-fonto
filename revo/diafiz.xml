<art mrk="$Id: diafiz.xml,v 1.17 2022/03/15 15:50:55 revo Exp $">
<kap><rad>diafiz</rad>/o</kap>
<drv mrk="diafiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La longa parto de <ref tip="malprt" cel="ost.0o">osto</ref> inter
      &gcirc;iaj du ekstremoj: <ref cel="apofiz.0o">apofizo</ref> kaj <ref cel="epifiz.0o">epifizo</ref>:
      <ekz>
        la flava osta medolo <klr>[&ccirc;e]</klr> plenkreskuloj <klr>[&#x2026;]</klr> situas interne
        de la <tld/>oj de longaj ostoj, kaj estas fonto de energia rezervo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157685">Osta medolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_f;&c_ib;&c_z;</trd>
  <trd lng="cs">diaf&yacute;za</trd>
  <trdgrp lng="de">
    <trd>Diaphyse</trd>,
    <trd>Diaphysis</trd>,
    <trd>R&ouml;hrenknochenmittelst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fr">diaphyse</trd>
  <trd lng="hu">diafizis</trd>
  <trd lng="it">diafisi</trd>
  <trd lng="nl">diafyse</trd>
  <trd lng="pl">korpus <klr>(ko&sacute;ci)</klr></trd>
  <trd lng="pt">di&aacute;fise</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_f;&c_i;&c_z;</trd>
  <trd lng="sk">stredn&aacute; &ccaron;as&tcaron; dlh&yacute;ch kost&iacute;</trd>
  <trd lng="uk">&#x434;&#x456;&#x430;&#x444;&#x456;&#x437;</trd>
</drv>
</art>
