<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nobl</rad>/a
</kap>
<drv mrk="nobl.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta altajn kaj malvulgarajn moralajn kvalitojn,
      senripro&ccirc;e justa, brava, honesta kaj malegoisma:
      <ekz>
        <tld/>a karaktero, konduto, homo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <uzo tip="fak">KEM</uzo>
        <ref cel="gas.nobla0o"><tld/>a gaso</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etik.0a">etika</ref>,
      <ref cel="ideal.0a">ideala</ref>,
      <ref cel="hero">heroa</ref>,
      <ref cel="altrui">altruista</ref>,
      <ref cel="avar.mal0a">malavara</ref>,
      <ref cel="model.0a">modela</ref>,
      <ref cel="sublim.0a">sublima</ref>.
    </refgrp>
  </snc>
  <rim>
    Ne konfuzu &leftquot;<tld/>a&rightquot; kaj
    &leftquot;<ref cel="nobel.0a">nobela</ref>&rightquot;!
  </rim>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_a;&c_k;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_je;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_k;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">noble</trd>
  <trdgrp lng="de">
    <trd>edel</trd>,
    <trd>edelm&uuml;tig</trd>,
    <trd>vornehm</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&ypsilon;&gamma;&epsilon;&ny;&eta_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>noble</trd>,
    <trd>great</trd>,
    <trd>honorable</trd>
  </trdgrp>
  <trd lng="hu">nemes<klr tip="ind"> (jellem&udblac;)</klr></trd>
  <trdgrp lng="nl">
    <trd>edel</trd>,
    <trd>nobel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>nobre <klr>(adj.)</klr></trd>,
    <trd>magn&acirc;nimo</trd>,
    <trd>generoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="tp">sewi</trd>
</drv>

<drv mrk="nobl.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc>
    <dif>
      Morale malbona, hontinda, malestiminda,
      <ref tip="super" cel="fi.0a">fia</ref>:
      <ekz>
        ho, kiel mi estas mal<tld/>a, kanajla, krima!
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        ni forte malestimas tiun &ccirc;i mal<tld/>an homon
        <fnt><bib>F</bib>, <lok>&FE;33.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_y;</trd>,
    <trd>&c_n;&c_ib;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">innoble</trd>
  <trdgrp lng="en">
    <trd>ignoble</trd>,
    <trd>base</trd>,
    <trd>vile</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="nobl.mal0ulo">
  <kap><ofc>*</ofc>mal<tld/>ulo</kap>
  <snc>
    <dif>
      Homo mal<tld/>a, fiulo:
      <ekz>
        li estas mensogisto kaj mal<tld/>ulo
        <fnt><bib>F</bib>, <lok>&FE;37.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_l;&c_ju;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">br&egrave;tol</trd>
  <trdgrp lng="en">
    <trd>scoundrel</trd>,
    <trd>scurvy dog</trd>,
    <trd>knave</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_l;&c_je;&c_c;</trd>
</drv>
</art>
