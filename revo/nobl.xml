  <trdgrp lng="cs">
    <trd>hanebn&yacute;</trd>,
    <trd>ne&scaron;lechetn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unedel</trd>,
    <trd>unw&uuml;rdig</trd>,
    <trd>sch&auml;ndlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ignoble</trd>,
    <trd>base</trd>,
    <trd>vile</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ignoble</trd>,
    <trd>vil</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5351;&#x52A3;&#x306A;  <pr>&#x3072;&#x308C;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x3072;&#x304D;&#x3087;&#x3046;&#x306A;</trd>,
    <trd>&#x5351;&#x3057;&#x3044;  <pr>&#x3044;&#x3084;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezecny</trd>,
    <trd>nikczemny</trd>,
    <trd>haniebny</trd>,
    <trd>sromotny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>nes&abreve;buit&abreve;</trd>,
    <trd>r&abreve;u</trd>,
    <trd>ru&#x219;inoas&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ne&scaron;&lcaron;achetn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_c;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x5351;&#x9119; <pr>b&#x113;ib&#x1D0;</pr></trd>
</drv>

<drv mrk="nobl.mal0ajxo">
  <kap>
    mal<tld/>a&jcirc;o
  </kap>
  <snc mrk="nobl.mal0ajxo.PSI">
    <dif>
      Mal<tld/>a, malmorala, malestiminda faro:
      <ekz>
        ho kia &gcirc;ojo, konfidi sole al si mem sen ia skrupulo, hipokrito a&ubreve; hezito, tiujn
        malfacilajn konfesojn, la proprajn mal<tld/>a&jcirc;ojn, respektante nur la memoran
