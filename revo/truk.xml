        <fnt>
        <aut>Stefan Maul </aut>
        <vrk><url ref="&Monato;/2009/009818.php">Mirakloj mankas</url></vrk>
        <bib>Monato</bib>
        <lok>2009/06, p. 7</lok>
        </fnt>.
 </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_k;</trd>
  <trd lng="ca">truc</trd>
