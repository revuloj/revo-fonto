<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>truk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="truk.0o">
  <kap><tld/>o</kap>
  <snc mrk="truk.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="artifi.0o.KOMUNE">Artifiko</ref>, kiu
      iluzias
      al la spektantoj:
      <ekz>
        kiam mi dis&scirc;iris ilin kaj rekunmetis ilin, tio estis nur
        man<tld/>o
        <fnt><bib>DOz</bib>, <lok>&leftquot;Doroteo plukas la
        princinon&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Trick</trd>
  <trdgrp lng="fr">
    <trd>astuce</trd>,
    <trd>trucage</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trik</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trd lng="pt">truque</trd>
</drv>
</art>

