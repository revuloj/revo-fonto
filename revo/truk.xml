<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: truk.xml,v 1.11 2022/09/21 08:47:43 revo Exp $">
<kap>
  <rad>truk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="truk.0o">
  <kap><tld/>o</kap>
  <snc mrk="truk.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="artifi.0o.KOMUNE">Artifiko</ref>, kiu
      iluzias
      al la spektantoj:
      <ekz>
        kiam mi dis&scirc;iris ilin kaj rekunmetis ilin, tio estis nur
        man<tld/>o
        <fnt><bib>DOz</bib>, <lok>&leftquot;Doroteo plukas la
        princinon&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_k;</trd>
  <trd lng="ca">truc</trd>
  <trdgrp lng="cs">
    <trd>filmov&yacute; trik</trd>,
    <trd>obratn&yacute; kousek</trd>,
    <trd>trik</trd>
  </trdgrp>
  <trd lng="de">Trick</trd>
  <trd lng="es">truco</trd>
  <trdgrp lng="fr">
    <trd>astuce</trd>,
    <trd>trucage</trd>,
    <trd>truc</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trik</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trd lng="pt">truque</trd>
  <trdgrp lng="sk">
    <trd>f&iacute;ge&lcaron;</trd>,
    <trd>trik</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&#x44E;&#x43A;</trd>,
    <trd>&c_s;&#x43F;&c_r;&c_i;&c_t;&#x43D;&c_i;&c_j; &#x43F;&c_r;&c_i;&c_j;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>

