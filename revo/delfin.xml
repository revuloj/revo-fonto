  <trd lng="en">larkspur</trd>
  <trd lng="fr">dauphinelle</trd>
  <trd lng="he">&#x5D3;&#x5D5;&#x5DC;&#x5E4;&#x5D9;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>szarkal&aacute;b</trd>,
    <trd>sarkvir&aacute;g</trd>
  </trdgrp>
  <trd lng="pt">delf&iacute;nio</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_p;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
