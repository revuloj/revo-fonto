<art mrk="$Id: delfin.xml,v 1.20 2022/03/15 15:50:54 revo Exp $">
<kap>
  <rad>delfini</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="delfin.0o">
  <kap><tld/>o</kap>
  <snc mrk="delfin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara vegeta&jcirc;o kun bluaj spronhavaj floroj,
      el kiu pluraj specioj estas &gcirc;ardene
      kulturataj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Delphinium</trd>, <trd lng="la">Consolida</trd>):
      <ekz>
        sprono botanike estas plilongiga&jcirc;o de iaj floroj, ofte kornoforma kaj pinta, ekz. <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20299">Sprono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="spron.kavalir0o">kavalirsprono</ref>
  </snc>
  <bld lok="&WCU;/thumb/f/f2/Delphinium_cultorum_cv.01.JPG/576px-Delphinium_cultorum_cv.01.JPG" prm="CC BY-SA 3.0">kulturata delfinio (Delphinium &#xD7; cultorum) el la kolekto de Botanika &Gcirc;ardeno de la Akademio de Sciencoj en Moskvo
       <fnt>
         <aut>Kor!An (&c_A;&c_n;&c_d;&c_r;&c_je;&c_j; &c_K;&c_o;&c_r;&c_z;&c_u;&c_n;), propra verko</aut>
         <lok><url ref="&WCW;/File:Delphinium_cultorum_cv.01.JPG">&WCW;/File:Delphinium_cultorum_cv.01.JPG</url></lok>
       </fnt>
  </bld>

