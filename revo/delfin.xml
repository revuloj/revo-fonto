<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>delfini</rad>/o
</kap>
<drv mrk="delfin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genroj el familio <ref tip="super" cel="ranunk.0acoj">ranunkolacoj</ref>
      (<trd lng="la">Delphinium</trd>, <trd lng="la">Consolida</trd>),
      kun bluaj spronhavaj floroj. Pluraj specioj estas &gcirc;ardene
      kulturataj.
    </dif>
    <ref tip="sin" cel="spron.kavalir0o">kavalirsprono</ref>
  </snc>
  <bld lok="../bld/delfini.jpg">
    <ind>kampa <tld/>o</ind>
    <trd lng="la">Consolida regalis</trd>
  </bld>
  <trd lng="de">Rittersporn</trd>
</drv>
</art>
