<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fresk.xml,v 1.27 2024/04/05 19:18:44 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fresk</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="fresk.0o">
  <kap><tld/>o</kap>
  <snc mrk="fresk.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="pentr.mur0ajxo">Murpentra&jcirc;o</ref> per
      akvaj koloriloj sur fre&scirc;a stuka&jcirc;o:
      <ekz>
        konservi&gcirc;is kelkloke <tld/>oj de la deksepa jarcento
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 3a, p.
        21a</lok></fnt>;
      </ekz>
      <ekz>
        en la kastelo <klr>[&#8230;]</klr> li pentris <tld/>ojn
        <fnt>
          <aut>Paul Bennemann</aut>,
          <vrk>La Vendreda Klubo</vrk>,
          <lok><url ref="&Gutenberg;ebooks/47259">
        gutenberg.org</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en unu <klr>[&#8230;]</klr> pavilono oni povas trarigardi imponajn <tld/>ojn de la
        a&ubreve;stria rokoko-pentristo <nom>Johann Bergl</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk>Ser&ccirc;i la perditan paradizon</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la antikvaj &ccirc;inaj <tld/>oj klasifiki&gcirc;as en kvar grupojn: la hala, hipogea,
        kaverna kaj templa
        <fnt>
          <bib>Kon11</bib>
          <lok>El &Ccirc;inio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_s;&c_k;&c_a;</trd>
  <trd lng="ca">fresc <klr>(mural)</klr></trd>
  <trd lng="cs">fresko</trd>
  <trdgrp lng="de">
    <trd>Freske</trd>,
    <trd>Fresko</trd>
  </trdgrp>
  <trd lng="en">fresco</trd>
  <trd lng="es">pintura al <ind>fresco</ind></trd>
  <trdgrp lng="fa">
    <trd>&a_d;&f_y;&a_w;&a_A;&a_r;&a_n;&f_g;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_f;&a_r;&a_s;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">fresque</trd>
  <trd lng="hu">fresk&oacute;</trd>
  <trd lng="ja">&#12501;&#12524;&#12473;&#12467;&#30011;  <pr>&#12501;&#12524;&#12473;&#12467;&#12364;</pr></trd>
  <trd lng="nl">fresco</trd>
  <trd lng="pl">fresk</trd>
  <trdgrp lng="pt">
    <trd>afresco</trd>,
    <trd>fresco</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">freska</trd>
  <trd lng="sv">fresk</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_r;&c_je;&c_s;&c_k;&c_o;&c_v;&c_i;&c_j; &c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="zh">&#28287;&#22721;&#30011; <pr>sh&imacron;b&igrave;hu&agrave;</pr></trd>
</drv>
</art>
