<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>fresk</rad>/o
</kap>
<drv mrk="fresk.0o">
  <kap><tld/>o</kap>
  <snc mrk="fresk.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="pentr.0ajxo.BELA">Pentra&jcirc;o</ref> per akvaj
      koloriloj sur fre&scirc;a stuka&jcirc;o de muro:
      <ekz>
        <tld/>e pentri;
      </ekz>
      <ekz>
        konservi&gcirc;is kelkloke <tld/>oj de la deksepa jarcento
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 3a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Freske</trd>,
    <trd>Fresko</trd>
  </trdgrp>
  <trd lng="es">pintura al <ind>fresco</ind></trd>
  <trd lng="fr">fresque</trd>
  <trd lng="hu">fresk&oacute;</trd>
  <trd lng="nl">fresco</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="sv">fresk</trd>
</drv>
</art>
