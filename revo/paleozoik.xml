<art mrk="$Id$">
<kap>
  <rad>paleozoik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="paleozoik.0o">
  <kap><tld/>o</kap>
  <snc mrk="paleozoik.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Praa epoko de la tero, en kiu maraj viva&jcirc;oj diversi&gcirc;is kaj multi&gcirc;is,
      proksimume inter 570 kaj 245 milionoj da jaroj a.K.:
      <ekz>
        karbona&jcirc;o el la dua duono de la <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;man&gcirc;i la nigron&rightquot;, p.
        81a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="primar.0a">primara</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Pal&auml;ozoikum</trd>,
    <trd>Erdaltertum</trd>
  </trdgrp>
  <trd lng="fr">pal&eacute;ozo&iuml;que</trd>
  <trd lng="pt">paleoz&oacute;ico</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_l;&c_je;&c_o;&c_z;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_o;&c_z;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_e;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>

