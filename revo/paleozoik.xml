<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: paleozoik.xml,v 1.17 2023/10/20 14:25:19 revo Exp $">
<kap>
  <rad>paleozoik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="paleozoik.0o">
  <kap><tld/>o</kap>
  <snc mrk="paleozoik.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">PAL</uzo>
    <dif>
      La unua <ref tip="lst" cel="era.0o.GEOL" lst="voko:geologiaj_eraoj" val="-542000000">erao</ref> 
      de <ref tip="malprt" cel="fanerozoik.0o.GEOL">fanerozoiko</ref>,
      en kiu maraj viva&jcirc;oj diversi&gcirc;is kaj multi&gcirc;is,
      proksimume anta&ubreve; inter 570 kaj 245 milionoj da jaroj:
      <ekz>
        karbona&jcirc;o el la dua duono de la <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, 
        <lok>&leftquot;man&gcirc;i la nigron&rightquot;, p. 81a</lok>
        </fnt>;
      </ekz>
      <ekz>
        al <tld/>o apartenas periodoj:
        kambrio, ordovico, silurio, devonio, karbonio kaj permio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arkean.0o.GEOL">arkeano</ref>,
      <ref cel="kenozoik.0o.GEOL">kenozoiko</ref>,
      <ref cel="mezozoik.0o.GEOL">mezozoiko</ref>,
      <ref cel="primar.0a.GEOL">primara</ref>,
      <ref cel="proterozoik.0o.GEOL">proterozoiko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_je;&c_a;&c_z;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_a;&c_z;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">paleozoic</trd>
  <trdgrp lng="cs">
    <trd>paleozoikum</trd>,
    <trd>prvohory</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Pal&auml;ozoikum</trd>,
      <trd>Erdaltertum</trd>
    </trdgrp>
  <trd lng="fr">pal&eacute;ozo&iuml;que</trd>
  <trdgrp lng="ja">
    <trd>&#x53E4;&#x751F;&#x4EE3;  <pr>&#x3053;&#x305B;&#x3044;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x53E4;&#x751F;&#x754C;  <pr>&#x3053;&#x305B;&#x3044;&#x304B;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
      <trd>paleozo&iuml;cum</trd>,
      <trd>primair<klr tip="ind"> (paleozo&iuml;cum)</klr></trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>paleozoik</trd>,
      <trd>era <ind>paleozoiczna</ind></trd>
    </trdgrp>
  <trd lng="pt">paleoz&oacute;ico</trd>
  <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_l;&c_je;&c_o;&c_z;&c_o;&c_j;</trd>,
      <trd>
      &c_p;&c_a;&c_l;&c_je;&c_o;&c_z;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; 
      &c_e;&c_r;&c_a;
      </trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>paleozoikum</trd>,
    <trd>prvohory</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x43B;&#x435;&#x43E;&#x437;&#x43E;&#x439;</trd>,
    <trd>&#x43F;&#x430;&#x43B;&#x435;&#x43E;&#x437;&#x43E;&#x439;&#x441;&#x44C;&#x43A;&#x430; &#x435;&#x440;&#x430;</trd>
  </trdgrp>
</drv>
</art>

