<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&gcirc;iraf</rad>/o
</kap>
<drv mrk="gxiraf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gxiraf1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Giraffa</trd>), vivanta sude de Saharo,
      kun tre longa kolo kaj makulita felo.  
    </dif>
  </snc>
  <trd lng="de">Giraffe</trd>
  <trd lng="en">giraffe</trd>
  <trd lng="es">jirafa</trd>
  <trd lng="nl">giraf</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;&c_a;&c_f;</trd>
  <trd lng="tr">z&uuml;rafa</trd>
  <trd lng="sv">giraff</trd>
</drv>

</art>
