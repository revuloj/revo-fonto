<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&gcirc;iraf</rad>/o
</kap>
<drv mrk="gxiraf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gxiraf.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Giraffa</trd>), vivanta sude de Saharo,
      kun tre longa kolo kaj makulita felo.  
    </dif>
  </snc>
  <trd lng="de">Giraffe</trd>
  <trd lng="en">giraffe</trd>
  <trd lng="es">jirafa</trd>
  <trd lng="tr">z&uuml;rafa</trd>
</drv>
<drv mrk="gxiraf.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="de">Giraffidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Giraffen</trd>
</drv>
</art>
