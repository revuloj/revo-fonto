<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxiraf.xml,v 1.35 2023/11/11 11:36:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&gcirc;iraf</rad>/o
</kap>

<drv mrk="gxiraf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="gxiraf1.0oj"><tld/>edoj</ref>
      (<trd lng="la"><ind>Giraffa</ind> camelopardalis</trd>), vivanta
      sude 
      de Saharo, kun makulita felo kaj tre longaj kruroj kaj kolo:
      <ekz>La Bradipo ekvidas la <tld lit="&Gcirc;"/>on kun ties terure longa
      kolo ron&gcirc;i foliojn de sur la arboj.
        <fnt>
          <aut>Janos Dorosmai</aut>:
          <vrk>Fabloj kaj Aforismoj</vrk>.
          <lok>&Ccirc;iu per la propraj okuloj</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_r;&c_a;&c_f;</trd>
  <trd lng="bg">&c_zh;&c_i;&c_r;&c_a;&c_f;</trd>
  <trd lng="br">jirafenn</trd>
  <trd lng="de">Giraffe</trd>
  <trd lng="el">
&kappa;&alfa;&my;&ypsilon;&lambda;&omikron;&pi;&alfa_ton;&rho;&delta;&alfa;&lambda;&eta;
</trd>
  <trd lng="en">giraffe</trd>
  <trd lng="es">jirafa</trd>
  <trd lng="fa">&a_z;&a_r;&a_A;&a_f;&a_h;</trd>
  <trd lng="fr">girafe</trd>
  <trd lng="hu">zsir&aacute;f</trd>
  <trd lng="id">jerapah</trd>
  <trd lng="it">giraffa</trd>
  <trd lng="nl">giraf</trd>
  <trd lng="pl">&zdot;yrafa</trd>
  <trd lng="pt">girafa</trd>
  <trd lng="ro">giraf&abreve;</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;&c_a;&c_f;</trd>
  <trd lng="tr">z&uuml;rafa</trd>
  <trd lng="sv">giraff</trd>
  <trdgrp lng="zh">
    <trd>&#x9577;&#x9838;&#x9E7F; <pr>ch&#xE1;ngj&#x1D0;ngl&#xF9;</pr></trd>,
    <trd>&#x99DD;&#x8C79; <pr>tu&#xF3;b&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gxiraf.Gx0o">
  <kap><tld lit="&Gcirc;"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Cam</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd lng="la">Camelopardalis</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_Zh;&c_y;&c_r;&c_a;&c_f; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="bg">&c_Zh;&c_i;&c_r;&c_a;&c_f;<klr tip="amb"> 
(&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="de">Giraffe</trd>
  <trd lng="en">Camelopardalis</trd>
  <trd lng="fa">&a_z;&a_r;&a_A;&a_f;&a_h;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trd lng="fr">Girafe<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="hu">Zsir&aacute;f</trd>
  <trd lng="it">Giraffa</trd>
  <trd lng="es">Jirafa</trd>
  <trd lng="ja">&#x30AD;&#x30EA;&#x30F3;</trd>
  <trd lng="pl">&Zdot;yrafa  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Girafa  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_r;&c_a;&c_f;</trd>,
    <trd>&c_zh;&c_i;&c_r;&c_a;&c_f;&c_a;</trd>
  </trdgrp>
</drv>
</art>

