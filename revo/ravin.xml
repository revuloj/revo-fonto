<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ravin.xml,v 1.17 2022/03/26 18:10:07 revo Exp $">
<kap>
  <rad>ravin</rad>/o  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ravin.0o">
  <kap><tld/>o</kap>
  <snc mrk="ravin.0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Malvasta profundega&jcirc;o inter du kruta&jcirc;oj:
      <ekz>
        la vojo serpentumas inter arbustoj, malsupreni&gcirc;ante
        en negrandajn <tld/>ojn, kie fluas bruaj riveretoj
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_r;</trd>,
    <trd>&c_l;&c_o;&c_g;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_o;&c_g;</trd>,
    <trd>&c_l;&c_a;&c_g;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_l;&c_o;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>rokle</trd>,
    <trd>str&#x17E;</trd>,
    <trd>&#xFA;&#x17E;labina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schlucht</trd>,
    <trd>Klamm</trd>
  </trdgrp>
  <trd lng="en">ravine</trd>
  <trd lng="fr">ravin</trd>
  <trd lng="hr">jaruga</trd>
  <trd lng="hu">szurdok</trd>
  <trd lng="nl">ravijn</trd>
  <trd lng="pt">ravina</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_v;&c_r;&c_a;&c_g;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>roklina</trd>,
    <trd>v&#xFD;mo&#x13E;</trd>,
    <trd>&#xFA;&#x17E;&#x13E;abina</trd>
  </trdgrp>
  <trd lng="sv">ravin</trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x440;</trd>,
    <trd>&#x43B;&#x43E;&#x449;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x432;&#x438;&#x434;&#x43E;&#x43B;&#x438;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&#x431;&#x430;&#x43B;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
