<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ravin</rad>/o
</kap>
<drv mrk="ravin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Malvasta profundega&jcirc;o inter du krutaj masivoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="kanjon.0o">kanjono</ref>,
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_r;</trd>,
    <trd>&c_l;&c_o;&c_g;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_o;&c_g;</trd>,
    <trd>&c_l;&c_a;&c_g;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schlucht</trd>,
    <trd>Klamm</trd>
  </trdgrp>
  <trd lng="nl">ravijn</trd>
  <trd lng="pt">ravina</trd> 
  <trdgrp lng="ru">
    <trd>&c_o;&c_v;&c_r;&c_a;&c_g;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">ravin</trd>
</drv>
</art>
