<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ament</rad>/o
</kap>
<drv mrk="ament.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, ordinare
      pendanta spiko a&ubreve; grapolo de masklaj neokulfrapaj
      floroj, defalanta entute post el&jcirc;eto de la poleno:
      <ekz>
        <tld/>o de avelo, poplo, alno, juglandujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">K&auml;tzchen</trd>
  <trdgrp lng="en">
    <trd>amentum</trd>,
    <trd>catkin</trd>
  </trdgrp>
  <trd lng="hu">barka</trd>
  <trd lng="nl">katje</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_jo;&c_zh;&c_k;&c_a;
<klr>(&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">h&auml;nge</trd>
</drv>
</art>
