<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ament</rad>/o
</kap>
<drv mrk="ament.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, ordinare
      pendanta spiko a&ubreve; grapolo de masklaj neokulfrapaj
      floroj, defalanta entute post el&jcirc;eto de la poleno:
      <ekz>
        <tld/>o de avelo, poplo, alno, juglandujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">K&auml;tzchen</trd>
  <trdgrp lng="en">
    <trd>amentum</trd>,
    <trd>catkin</trd>
  </trdgrp>
</drv>
</art>

