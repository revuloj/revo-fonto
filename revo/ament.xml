<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ament</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ament.0o">
  <kap><tld/>o</kap>
  <snc mrk="ament.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, ordinare
      pendanta spiko a&ubreve; grapolo de masklaj neokulfrapaj
      floroj, defalanta entute post el&jcirc;eto de la poleno:
      <ekz>
        <tld/>o de avelo, poplo, alno, juglandujo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_c;&c_ib;&c_k; <klr tip="amb">(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="de">K&auml;tzchen</trd>
  <trdgrp lng="en">
    <trd>amentum</trd>,
    <trd>catkin</trd>
  </trdgrp>
  <trd lng="fr">chaton<klr> (botanique)</klr></trd>
  <trd lng="hu">barka</trd>
  <trd lng="nl">katje</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_jo;&c_zh;&c_k;&c_a; <klr>(&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">jah≈àada</trd>
  <trd lng="sv">h&auml;nge</trd>
</drv>
</art>
