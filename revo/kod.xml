  <snc mrk="kod.mal0i.SCI">
    <dif>
      Analizi <tld/>ita&jcirc;on por &gcirc;in kompreni a&ubreve;
      mal&ccirc;ifri:
      <ekz>
        sciencistoj de la universitato de Vieno mal<tld/>is la &leftquot;sekretan lingvon&#8221; de elefantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://www.monato.net/2013/010876.php">Elefanta komunikado malkodita</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>entkodieren</trd>,
      <trd>entschl&uuml;sseln</trd>
    </trdgrp>
