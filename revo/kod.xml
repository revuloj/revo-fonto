  <snc mrk="kod.0igi.JUR">
    <dif>
      Fari le&gcirc;ojn, kutimojn ktp parto de jura <tld/>o:
      <ekz>
        <tld/>igi familiajn le&gcirc;ojn;
      </ekz>
      <ekz>
        <tld/>igi pensmanierojn en Esperanto estas multe pli facile ol en etnaj lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="http://www.esperanto.be/fel/2003/008051.html">Bruselo-Babelo: lingvo-kulturo en E&ubreve;ropa Unio</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kodifizieren</trd>
    </trdgrp>
    <trd lng="en">codify</trd>
    <trdgrp lng="es">
      <trd>cifrar</trd>,
      <trd>codificar</trd>
    </trdgrp>
    <trd lng="fr">codifier</trd>
    <trdgrp lng="hu">
      <trd>t&ouml;rv&eacute;nybe foglal</trd>,
      <trd>kodifik&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">codificeren</trd>
    <trdgrp lng="pl">
      <trd>kodyfikowa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">
&c_k;&c_o;&c_d;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kodifikovat</trd>,
    <trd>nechat zak&oacute;dovat</trd>
  </trdgrp>
  <trd lng="sk">kodifikova&tcaron;</trd>
</drv>

<drv mrk="kod.mal0i">
