<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>muel</rad>/i
</kap>
<drv mrk="muel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="muel.0i.AGR">
    <dif>
      Pisti a&ubreve; pecetigi grenon per ia ilo por fari farunon a&ubreve; grion.
    </dif>
    <trdgrp lng="de">
      <trd>mahlen</trd>,
      <trd>zermahlen</trd>,
      <trd>ausmahlen</trd>
    </trdgrp>
    <trd lng="es">moler</trd>
    <trd lng="nl">malen</trd>
    <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Pisti grajnojn a&ubreve; fruktojn por diversaj uzoj:
      <ekz>
        <tld/>i kafon, pipron, olivojn
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dent.0o">dento</ref>
    </refgrp>
    <trd lng="de">mahlen</trd>
    <trd lng="es">moler</trd>
    <trd lng="nl">malen</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_r;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_o;&c_l;&c_o;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Afliktegi kaj senenergiigi, <ref tip="sin"
      cel="prem.0i.gxeni">premi</ref>:
      <ekz>
        la sekreto <tld/>is lian koron
        (<ref tip="vid" cel="kor.0o">koro</ref>);
      </ekz>
      <ekz>
        amaraj pensoj <tld/>is kaj venenis lian energion;
      </ekz>
      <ekz>
        anim<tld/>anta silento;
      </ekz>
      <ekz>
        kor<tld/>anta &ccirc;agreno;
      </ekz>
      <ekz>
        labor<tld/>ita kamparano;
      </ekz>
      <ekz>
        sort<tld/>ita kadukulo.
      </ekz>
      <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
    </dif>
    <trdgrp lng="de">
      <trd>zerr&uuml;tten</trd>,
      <trd>nagen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kwellen</trd>,
      <trd>verlammen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senhalte ripetadi:
      <ekz>
        &ccirc;iutage la gurdego de la karuselo <tld/>is sian plej allogan
        ariaron;
      </ekz>
      <ekz>
        <tld/>e sencele babili<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">herunterleiern</trd>
    <trd lng="nl">herhalen <klr tip="ind"> (tot vervelens toe)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_a;&c_l;&c_d;&c_y;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">grind</trd>
  <trd lng="fr">moudre</trd>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_c;&c_mol;</trd>
  <trd lng="sv">mala</trd>
</drv>

<drv mrk="muel.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="muel.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Konstrua&jcirc;o destinita por <tld/>i:
      <ekz>
        la filo dungi&gcirc;is en <tld/>ejo
        <fnt><bib>ElektFab</bib>, <lok>Tablo &leftquot;kovru vin&rightquot;, la orazeno kaj
        bastono el sako</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_y;&c_n; </trd>,
    <trd>&c_v;&c_ja;&c_t;&c_r;&c_a;&c_k; <klr>(&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y; &c_m;&c_l;&c_y;&c_n;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&uuml;hle</trd>,
    <trd>M&uuml;hlhaus</trd>
  </trdgrp>
  <trd lng="es">molino</trd>
  <trd lng="fr">moulin<klr> (b&acirc;timent)</klr></trd>
  <trd lng="nl">molen<klr tip="ind"> (gebouw)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a; <klr>(&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="muel.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="muel.0ilo.TEK">
    <dif>
      Aparato por <tld/>i:
      <ekz>
        <ind>akvo<tld/>ilo</ind>,
        <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</ind></trd>
      </ekz>
      <ekz>
        <ind>vento<tld/>ilo</ind>,
        <trd lng="ru">&c_v;&c_je;&c_t;&c_r;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</ind></trd>
      </ekz>
      <ekz>
        lia lango babilas kiel <tld/>ilo <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas akvo al lia <tld/>ilo
        <klr>(profita afero por li)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>M&uuml;hle</trd>,
    <trd>M&uuml;hlwerk</trd>
  </trdgrp>
  <trd lng="es">molino</trd>
  <trd lng="fr">moulin<klr> (appareil)</klr></trd>
  <trd lng="nl">molen<klr tip="ind"> (werktuig)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a; <klr>(&c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_m;)</klr></trd>
</drv>

<drv mrk="muel.kaf0ilo">
  <kap>kaf<tld/>ilo</kap>
  <snc mrk="muel.kaf0ilo.KOMUNE">
    <dif>
      Aparato kiu pulvorigas kafgrajnojn:
      <ekz>
        la koboldo metis la parolilon sur la kaf<tld/>ilon
        <fnt><bib>Fab3</bib>, <lok>Koboldo &ccirc;e la butikisto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">moulin &agrave; caf&eacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_m;&c_o;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="muel.vort0i">
  <kap>vort<tld/>i</kap>
  <snc mrk="muel.vort0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre abunde, rapide, ripete <ref tip="vid"
      cel="babil.0i.babilacxi">babili</ref>
      <ekz>
        oni parolas, paroladas, vort<tld/>as
        <fnt>
        <aut>Sten Johansson</aut>,
        <vrk><url
        ref="http://esperanto.net/literaturo/novel/novlibr/lasparolplurec.html">Anonime
        kaj nekonkrete</url></vrk>,
        <lok>[2004?]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>caqueter<klr> (bavarder)</klr></trd>,
    <trd>jacasser<klr> (bavarder)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="muel.0sxtono">
  <kap><tld/>&scirc;tono</kap>
  <snc mrk="muel.0sxtono.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Platronda &scirc;tono servanta por <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>M&uuml;hlstein</trd>,
    <trd>Mahlstein</trd>
  </trdgrp>
  <trd lng="es">piedra de <ind>molino</ind></trd>
  <trd lng="fr">meule</trd>
  <trd lng="nl">molensteen</trd>
  <trd lng="ru">&c_zh;&c_jo;&c_r;&c_n;&c_o;&c_v;</trd>
</drv>
</art>
