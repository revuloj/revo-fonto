<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muel.xml,v 1.36 2022/08/22 08:39:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>muel</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="muel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="muel.0i.AGR">
    <dif>
      <ref cel="pist.0i">Pisti</ref> a&ubreve; pecetigi per ilo <ref cel="gren.0o.grajno">greno</ref>n, 
      grajnojn, fruktojn, malmolajn materialojn:
      <ekz>
        mueli kafon
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        prenu manan <tld/>ilon kaj <tld/>u <ref cel="farun.0o">faruno</ref>n
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 47:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        por prepari paston oni <tld/>as maizon por havi farunon
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2006/008903.php">Fornoj kaj formoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is la kafograjnojn
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2014/011225.php">La sa&gcirc;o de Salomono kaj
          salamo</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita viando
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2008/009624.php">Veneno frostigita</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        slovakoj &scirc;atas <tld/>itan &scirc;affroma&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php">Oro blanka</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu pli frue venas, pli frue <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dent.0o">dento</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>mahlen</trd>,
      <trd>zermahlen</trd>,
      <trd>ausmahlen</trd>
    </trdgrp>
    <trd lng="es">moler</trd>
    <trd lng="fr">moudre</trd>
    <trd lng="nl">malen</trd>
    <trdgrp lng="pl">
      <trd>mieli&cacute;</trd>,
      <trd>mle&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_r;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_o;&c_l;&c_o;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="muel.0i.FIG_premi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senenergiigi, <ref tip="sin" cel="prem.0i.gxeni">premi</ref>:
      <ekz>
        li sidi&gcirc;is teren, en la mezo de la salono, <tld/>ita kaj disrompita de la tia
        danca ekzercado
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la senfrukta penado vere <tld/>is mian cerbon
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxerp.el0i">el&ccirc;erpi</ref>,
      <ref cel="turmen.0i">turmenti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zerr&uuml;tten</trd>,
      <trd>ausquetschen <klr>(Kr&auml;fte)</klr></trd>,
      <trd>zermartern</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kwellen</trd>,
      <trd>verlammen</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </snc>
  <snc mrk="muel.0i.FIG_ripeti">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senhalte <ref tip="super" cel="ripet.0i">ripetadi</ref>:
      <ekz>
        babilas, <tld/>as, kion lango elpelas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen&ccirc;ese kaj nehaltigeble la pensoj <tld/>adis en lia kapo, kvaza&ubreve; ili volus
        polvigi &ccirc;iun &gcirc;ojon
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        la propralanda radio <tld/>is &ccirc;iam la samajn aferojn <klr>[kaj oni]</klr>
        tedi&gcirc;is pro la monotonaj insultoj kontra&ubreve; la najbara lando
        <fnt>
          <bib>LGA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="gurd.0i.FIG">gurdi<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>wiederk&auml;uen <klr>(Worte)</klr></trd>,
      <trd>herunterleiern</trd>,
      <trd>im <ind>Kreis</ind> drehen <klr>(Gedanken)</klr></trd>
    </trdgrp>
    <trd lng="fr">mouliner</trd>
    <trd lng="nl">herhalen <klr tip="ind"> (tot vervelens
    toe)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_a;&c_l;&c_d;&c_y;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_o;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ml&iacute;t</trd>,
    <trd>rozeml&iacute;t</trd>
  </trdgrp>
  <trd lng="en">grind</trd>
  <trd lng="pl">mle&cacute;</trd>
  <trd lng="pt">moer</trd>
  <trd lng="sk">vymlie&tcaron; <klr>(obilie)</klr></trd>
  <trd lng="sv">mala</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x43B;&#x43E;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_t;&#x43E;&c_v;&#x43A;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="muel.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="muel.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Konstrua&jcirc;o destinita por <tld/>i:
      <ekz>
        la filo dungi&gcirc;is en <tld/>ejo
        <fnt><bib>ElektFab</bib>,
        <lok>Tablo &leftquot;kovru vin&rightquot;, la orazeno kaj
        bastono el sako</lok></fnt>;
      </ekz>
      <ekz>
        la rivereto <klr>[&#8230;]</klr> turnadis radon de <tld/>ejo
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_y;&c_n; <klr>(&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;)</klr></trd>,
    <trd>&c_v;&c_ja;&c_t;&c_r;&c_a;&c_k; <klr>(&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y; &c_m;&c_l;&c_y;&c_n;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ml&yacute;n</trd>,
    <trd>ml&yacute;nice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&uuml;hle</trd>,
    <trd>M&uuml;hlhaus</trd>
  </trdgrp>
  <trd lng="es">molino</trd>
  <trd lng="fr">moulin<klr> (b&acirc;timent)</klr></trd>
  <trd lng="nl">molen<klr tip="ind"> (gebouw)</klr></trd>
  <trd lng="pl">m&lstroke;yn</trd>
  <trd lng="pt">moinho<klr tip="ind"> (constru&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a; <klr>(&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">mlyn</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43B;&c_i;&#x43D; (&#x43F;&c_r;&c_i;&#x43C;&c_ib;&c_shch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&c_r;&c_u;&c_d;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="muel.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="muel.0ilo.TEK">
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por <tld/>i:
      <ekz>
        la sklavoj en kelkcento da <tld/>iloj kaj pistiloj preparis la farunon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas akvo al lia <tld/>ilo
        <klr>(profita afero por li)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="muel.0ilo.AGR">
      <uzo tip="fak">AGR</uzo>
      <dif>
        Ma&scirc;ino per kiu oni faras el greno <ref cel="gri.0o">grio</ref>n 
        a&ubreve; <ref cel="farun.0o">faruno</ref>n:
        <ekz>
          oni a&ubreve;dis bone la krakadon de la <tld/>iloj, la kantadon de la servistoj kaj
          servistinoj, kaj oni vidis iliajn kamparajn dancojn
          <fnt>
            <bib>Fab2</bib>
            <lok>Monteto de elfoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          la aloj de <tld/>iloj &scirc;ajnis gigantaj brakoj
          <fnt>
            <bib>DKM</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="muel.0ilo.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Malgranda kuireja aparato per kiu
        oni <tld/>as grajnojn, spicojn kaj similajn:
        <ekz>
          jen vi vidas anta&ubreve; &ccirc;io <tld/>ilon, kiu kostas al mi 10 &scirc;ilingojn
          <fnt><bib>FK</bib></fnt>;
        </ekz>  
        <ekz>
          &scirc;iaj viglaj okuloj jam observis <tld/>ilon kaj kafujon sur breto, kaj en la
          posta momento &scirc;i sidis kun la <tld/>ilo inter la genuoj
          <fnt>
            <bib>HsT</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_y;&c_n; <klr>(&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_z;&c_m;)</klr></trd>,
    <trd>&c_d;&c_r;&c_a;&c_b;&c_ib;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">ml&yacute;nek</trd>
  <trdgrp lng="de">
    <trd>M&uuml;hle</trd>,
    <trd>M&uuml;hlwerk</trd>
  </trdgrp>
  <trd lng="es">molino</trd>
  <trd lng="fr">moulin<klr> (appareil)</klr></trd>
  <trd lng="nl">molen<klr tip="ind"> (werktuig)</klr></trd>
  <trdgrp lng="pl">
    <trd>m&lstroke;yn</trd>,
    <trd>m&lstroke;ynek</trd>
  </trdgrp>
  <trd lng="pt">moinho<klr tip="ind"> (equipamento)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a; <klr>(&c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_m;)</klr></trd>
  <trdgrp lng="sk">
    <trd>mlec&iacute; stroj</trd>,
    <trd>mlyn</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x43B;&c_i;&#x43D; (&#x43C;&c_je;&c_h;&c_a;&#x43D;&c_ib;&c_z;&#x43C;)</trd>
</drv>

<drv mrk="muel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="muel.0isto.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="meti.0isto">Metiisto</ref>, 
      kiu <tld/>as, precipe tiu, kiu <tld/>as grenon:
      <ekz>
        la dua filo venis al <tld/>isto kaj fari&gcirc;is lia lernanto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ilo haltas, <tld/>isto eksaltas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Oreko-la-<tld/>isto povis <tld/>i tritikon por la kamparanoj
        pli malmultekoste ol iu ajn alia <tld/>isto
        <fnt>
          <aut>D. K. Jordan</aut>
          <vrk>La Avara Muelisto</vrk>
          <lok><url ref="http://esperanto.ca/kea/j-enkonduko.html">
          La Fabeloj de Jordan</url>, [vidita en 2006]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_y;&c_n;&c_a;&c_r;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_m;&c_u;&c_k;&c_a;&c_m;&c_o;&c_l;</trd>
  </trdgrp>
  <trd lng="cs">mlyn&aacute;&rcaron;</trd>
  <trd lng="de">M&uuml;ller</trd>
  <trd lng="fr">meunier</trd>
  <trd lng="pl">m&lstroke;ynarz</trd>
  <trd lng="pt">moleiro</trd>
  <trd lng="sk">mlyn&aacute;r</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&#x43B;&#x44C;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&#x43C;&c_ib;&c_r;&#x43E;&c_sh;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="muel.akvo0ilo">
  <kap>akvo<tld/>ilo, <var><kap>akva <tld/>ejo</kap></var></kap>
  <snc mrk="muel.akvo0ilo.AGR">
    <dif>
      <ref tip="super" cel="muel.0ilo.AGR"><tld lit="M"/>ilo</ref>
      kies me&hcirc;anismon pelas trog-rado turnata de rivera fluo:
      <ekz>
        lia metiejo trovi&gcirc;is tute apude de unu el la malnovaj &scirc;tonaj pontoj, sub kiu
        staris sen&ccirc;ese bruanta malalta akva <tld/>ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antoj de la regiono nomis la riveron <nom>M&lstroke;yn&oacute;wka</nom> 
        <klr>[&#8230;]</klr> pro multaj akvo<tld/>iloj trovi&gcirc;antaj &ccirc;e &gcirc;iaj 
        bordoj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="http://www2.polskieradio.pl/eo/dokument.aspx?iid=34584">
          Kolbuszowa</url></vrk>
          <lok>Pola Radio, 2006-03-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y; <ind>&c_m;&c_l;&c_y;&c_n;</ind></trd>
  <trd lng="de">Wasserm&uuml;hle</trd>
  <trd lng="fr">moulin &agrave; aube</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</ind></trd>
</drv>

<drv mrk="muel.kaf0ilo">
  <kap>kaf<tld/>ilo</kap>
  <snc mrk="muel.kaf0ilo.KOMUNE">
    <dif>
      Aparato por prepari kafpulvoron el rostitaj kafofaboj:
      <ekz>
        la koboldo metis la parolilon sur la kaf<tld/>ilon, ha, kiel &gcirc;i komencis krakadi
        <fnt><bib>Fab3</bib>,
        <lok>Koboldo &ccirc;e la butikisto</lok></fnt>!
      </ekz>     
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_m;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_l;&c_y;&c_n;&c_o;&c_k; &c_d;&c_l;&c_ja; &c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&aacute;voml&yacute;nek</trd>,
    <trd>ml&yacute;nek na k&aacute;vu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kaffeem&uuml;hle</trd>
  </trdgrp>
  <trd lng="es">molinillo de caf&eacute;</trd>
  <trd lng="fr">moulin &agrave; caf&eacute;</trd>
  <trd lng="pl">m&lstroke;ynek do kawy</trd>
  <trd lng="pt">moedor de caf&eacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_m;&c_o;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">mlyn&ccaron;ek na k&aacute;vu</trd>
  <trd lng="uk">&#x43C;&#x43B;&c_i;&#x43D;&#x43E;&#x43A; &c_d;&#x43B;&#x44F; &#x43A;&c_a;&c_v;&c_i;</trd>
</drv>

<drv mrk="muel.vent0ilo">
  <kap>vent<tld/>ilo</kap>
  <snc mrk="muel.vent0ilo.AGR">
    <dif>
      <ref tip="super" cel="muel.0ilo.AGR"><tld lit="M"/>ilo</ref>
      kies me&hcirc;anismon pelas venta blovo tra helico:
      <ekz>
        ruli&gcirc;anta kiel vent<tld/>ilo
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_t;&c_r;&c_a;&c_k; <klr>(&c_m;&c_l;&c_y;&c_n;)</klr></trd>
  <trd lng="bo">&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;&#xF58;&#xF46;&#xF72;&#xF42;&#xF0B;</trd>
  <trd lng="de">Windm&uuml;hle</trd>
  <trd lng="fr">moulin &agrave; vent</trd>
  <trd lng="pt">moinho de vento</trd>
  <trd lng="ru">&c_v;&c_je;&c_t;&c_r;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</ind></trd>
  <trd lng="uk">&c_v;&c_ib;&c_t;&c_r;&#x44F;&#x43A; (&#x43C;&#x43B;&c_i;&#x43D;)</trd>
</drv>

<drv mrk="muel.vort0i">
  <kap>vort<tld/>i</kap>
  <snc mrk="muel.vort0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre abunde, rapide, ripete <ref tip="vid" cel="babil.0i">babili</ref>
      <ekz>
        oni parolas, paroladas, vort<tld/>as
        <fnt>
          <aut>S. Johansson</aut>
          <vrk><url ref="http://esperanto.net/literaturo/novel/novlibr/lasparolplurec.html">
          Anonime kaj nekonkrete</url></vrk>,
          <lok>[2004?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
