<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>purpur</rad>/o
</kap>
<drv mrk="purpur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Violru&gcirc;a <ref tip="super" cel="kolor.0o.propre">koloro</ref>:
      <ekz>li surmetis vulpofelan jakon, tegitan per melongene <tld/>a veluro
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p. 123a</lok></fnt></ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      &Scirc;tofo el tiu koloro:
      <ekz>
        plej delikata tolo kaj <tld/>o estas &scirc;iaj vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la re&gcirc;a, kardinala <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Purpur</trd>
  <trdgrp lng="fr">
    <trd>pourpre</trd>
  </trdgrp>
  <trd lng="hu">b&iacute;bor</trd>
  <trd lng="nl">purper</trd>
  <trd lng="ru">&c_p;&c_u;&c_r;&c_p;&c_u;&c_r;</trd>
</drv>
</art>
