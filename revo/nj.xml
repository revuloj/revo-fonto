<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nj.xml,v 1.21 2021/03/24 11:58:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  -<rad>nj</rad>/
</kap>

<subart>
<drv mrk="nj.0">
  <kap>-<tld/></kap>
  <snc>
    <dif>
      Sufikso karesa almetata post la unua a&ubreve; unuaj silaboj
      de ina nomo a&ubreve; alia vorto indikanta inon:
      <ekz>
        Klaron kaj Sofion iliaj gepatroj nomas <klr>...</klr> Kla<tld/>o
        kaj So<tld/>o (a&ubreve; Sofi<tld/>o)
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        kuzino! Kuzineto! Kuzi<tld/>o
        <fnt><bib>Marta</bib></fnt>!
      </ekz>
      <ekz>
        Alice, permesu, ke mi nomu vin Ali<tld/>o
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="in.0">in</ref>,
      <ref cel="cxj.0">&ccirc;j</ref>,
      <ref cel="et._0.sufikso">et</ref>
    </refgrp>
  </snc>
  <trd lng="de"><klr>-</klr>i</trd>
  <trd lng="en">diminutive feminine suffix of endearment (-y angle estas
  amba&ubreve;seksa)</trd>
  <trd lng="fr">-ette<klr tip="ind"> (diminutif
  f&eacute;minin)</klr></trd>
  <trdgrp lng="hu">
    <trd>-<ind>ka</ind></trd>,
    <trd>-<ind>ke</ind></trd>,
    <trd>-<ind>i</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>-<ind>&c_k;&c_a;</ind><klr tip="ind"> (&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s;
    &c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>-<ind>&c_ch;&c_k;&c_a;</ind><klr tip="ind"> (&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s;
    &c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>-<ind>&c_n;&c_mol;&c_k;&c_a;</ind><klr tip="amb"> (&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s;
    &c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_a;)</klr></trd>
  </trdgrp>
</drv>
</subart>

<subart>
<dif>
Samsignifa memstara vortero.
</dif>
<drv mrk="nj.avi0o">
  <kap>avi<tld/>o</kap>
  <snc mrk="nj.avi0o.FAM">
    <dif>
      Karesvorto por <ref tip="sin" cel="av.0ino">avino</ref>:
      <ekz>
        Panjo! Pa&ccirc;jo! Avi<tld/>o! Ne ju&gcirc;u min severe
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 15a, p. 91a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_a;&c_b;&c_ch;&c_je;</trd>
  <trd lng="cs">babi&ccaron;ka</trd>
  <trdgrp lng="de">
    <trd>Omi</trd>
  </trdgrp>
  <trd lng="en">granny</trd>
  <trdgrp lng="fr">
    <trd>mamie</trd>,
    <trd>m&eacute;m&eacute;</trd>
  </trdgrp>
  <trd lng="sk">babi&ccaron;ka</trd>
</drv>

<drv mrk="nj.o0o">
  <kap>o<tld/>o</kap>
  <snc mrk="nj.o0o.FAM">
    <dif>
      Karesvorto por <ref tip="sin" cel="onkl.0ino">onklino</ref>.
    </dif>
  </snc>
  <trd lng="cs">teti&ccaron;ka</trd>
  <trdgrp lng="fr">
    <trd>tata<klr> (tante)</klr></trd>,
    <trd>tatie<klr> (tante)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_jo;&c_t;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">tetu&scaron;ka</trd>
</drv>

<drv mrk="nj.pa0o">
  <kap>pa<tld/>o</kap>
  <snc mrk="nj.pa0o.FAM">
    <dif>
      Karesvorto por <ref tip="sin" cel="patr.0ino.GEN">patrino</ref>:
      <ekz>
        pa<tld/>o! &#8208; &scirc;i diris, &ccirc;irka&ubreve;prenante la
        kolon de la patrino
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_a;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>maminka</trd>,
    <trd>mati&ccaron;ka</trd>
  </trdgrp>
  <trd lng="en">mommy</trd>
  <trd lng="fr">maman</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_m;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mami&ccaron;ka</trd>
</drv>

<drv mrk="nj.i0o">
  <kap>i<tld/>o</kap>
  <snc mrk="nj.i0o.ino">
    <dif>
      Karesvorto indikanta <ref tip="sin" cel="in.0o">inon</ref>
      <ekz>
        kvankam mi &scirc;atas la i<tld/>ojn, <klr>...</klr> mi certe
        estos tro lacega por e&ccirc; rimarki ilin!
        <fnt>
        <vrk><url
        ref="http://miresperanto.narod.ru/humuro/rikardo-cash-5.htm"
        >&Scirc;ercoj de Rikardo Cash-5</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="ru">&c_b;&c_a;&c_b;&c_o;&c_n;&c_mol;&c_k;&c_a;</trd>
    <trd lng="tp">meli</trd>
</drv>

<drv mrk="nj.va0o">
  <kap>va<tld/>o</kap>
  <snc mrk="nj.va0o.FAM">
    <dif>
      Karesvorto por <ref tip="sin" cel="vart.0istino">vartistino</ref>:
      <ekz>
        Va<tld/>o Li, iama mamnutristino de Jado
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 8a, volumo 1a, p.
        121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">nounou</trd>
  <trd lng="ru">&c_n;&c_ja;&c_n;&c_je;&c_ch;&c_k;&c_a;</trd>
</drv>
</subart>
</art>
