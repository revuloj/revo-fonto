<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>nj</rad>
</kap>
<drv mrk="nj.0">
  <kap><ofc>*</ofc> -<tld/></kap>
  <gra><vspec>sufikso</vspec></gra>
  <snc>
    <dif>
      Karesa sufikso almetata post la unua a&ubreve; unuaj silaboj de virina nomo:
      <ekz>
        Klaron kaj Sofion iliaj gepatroj nomas <klr>...</klr> Kla<tld/>o
        kaj So<tld/>o (a&ubreve; Sofi<tld/>o)
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        kuzino! Kuzineto! Kuzi<tld/>o
        <fnt><bib>Marta</bib></fnt>!
      </ekz>
      <ekz>
        Alice, permesu, ke mi nomu vin Ali<tld/>o
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxj.0">&ccirc;j</ref>,
      <ref cel="et">et</ref>
    </refgrp>
  </snc>
  <trd lng="de"><klr>-</klr>i</trd>
  <trd lng="fr">-ette<klr tip="ind"> (diminutif f&eacute;minin)</klr></trd>
  <trdgrp lng="hu">
    <trd>-<ind>ka</ind></trd>,
    <trd>-<ind>ke</ind></trd>,
    <trd>-<ind>i</ind></trd>
  </trdgrp>
</drv>

<drv mrk="nj.o0o">
  <kap>o<tld/>o</kap>
  <snc mrk="nj.o0o.FAM">
    <dif>
      Karesa nomo de <ref tip="sin" cel="onkl.0ino">onklino</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>tata<klr> (tante)</klr></trd>,
    <trd>tatie<klr> (tante)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="nj.pa0o">
  <kap>pa<tld/>o</kap>
  <snc mrk="nj.pa0o.FAM">
    <dif>
      Karesa nomo de <ref tip="sin" cel="patr.0ino">patrino</ref>:
      <ekz>
        pa<tld/>o! &ndash; &scirc;i diris, &ccirc;irka&ubreve;prenante la kolon de la
        patrino
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">maman</trd>
</drv>

<drv mrk="nj.va0o">
  <kap>va<tld/>o</kap>
  <snc mrk="nj.va0o.FAM">
    <dif>
      Karesa nomo de <ref tip="sin" cel="vart.0istino">vartistino</ref>:
      <ekz>
        Va<tld/>o Li, iama mamnutristino de Jado
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 8a, volumo 1a, p.
        121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">nounou</trd>
</drv>
</art>
