<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suzeren.xml,v 1.10 2023/11/29 01:36:31 revo Exp $">
<kap>
  <rad>suzeren</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suzeren.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="suzeren.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="dif" cel="feuxd.0estro.HIS">Fe&ubreve;destro</ref>:
      <ekz>
        li &ccirc;iujn kalumnias /
        kaj ne &scirc;parus e&ccirc; la re&gcirc;on, /
        lian bonan <tld/>on
        <fnt>
          <aut>trad. P.-J. Doumen</aut>,
          <vrk>Karlo kaj Elegasto</vrk>,
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la duko de Burgonjo estas mem <tld/>o de la sekvantaj graflandoj
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=464552">Listo de dukoj de
          Burgonjo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la grafo Vilhelmo la 1-a <klr>[&#8230;]</klr> venkis la saracenojn dum
        la Batalo de
        Turturo en 973 kaj <klr>[&#8230;]</klr> fari&gcirc;is <tld/>o de Provenco
        kaj kreis la
        provencan fe&ubreve;decon disdonante al siaj vasaloj la &jcirc;us
        rekonkeritan landon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=165890">Vilhelmo la 1-a
          (Provenco)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ju;&c_z;&c_e;&c_r;&c_e;&c_n;</trd>
  <trd lng="cs">suzer&eacute;n</trd>
  <trdgrp lng="de">
    <trd>Suzer&auml;n</trd>,
    <trd>Lehnsherr</trd>,
    <trd>Oberhoheit</trd>
  </trdgrp>
  <trd lng="en">suzerain</trd>
  <trd lng="fr">suzerain</trd>
  <trd lng="pl">suzeren</trd>
  <trdgrp lng="sk">
    <trd>suzer&eacute;n</trd>,
    <trd>vrchn&yacute; p&aacute;n l&eacute;na</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#x44E;&c_z;&c_je;&c_r;&c_je;&#x43D;</trd>
</drv>
</art>

