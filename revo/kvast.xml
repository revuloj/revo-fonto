<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>kvast</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvast.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvast.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama <ref tip="super" cel="fask.0o.KOMUNE">fasko</ref> el 
      mallongaj fadenoj lanaj a&ubreve; silkaj pendanta de vesto, kuteno, 
      meblo...
    </dif>
    <refgrp tip="vid">
      <ref cel="frangx.0o">fran&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_t;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Quaste</trd>,
    <trd>Troddel</trd>
  </trdgrp>
  <trd lng="en">tassel</trd>
  <trd lng="fr">gland<klr> (ornement)</klr></trd>
  <trd lng="hu">bojt</trd>
  <trd lng="nl">kwast</trd>
  <trdgrp lng="pt">
    <trd>borla</trd>,
    <trd>penacho</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_t;&c_mol; <klr>(&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">tofs</trd>
</drv>
</art>
