<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>kvast</rad>/o
</kap>
<drv mrk="kvast.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Kuna&jcirc;o el mallongaj fadenoj lanaj a&ubreve; silkaj, kun formo de tufo a&ubreve;
      peniko, kiu servas kiel ornamo por vestoj a&ubreve; meblaro.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_t;&c_a;&c_s;</trd>
  <trdgrp lng="de">
     <trd>Quaste</trd>,
     <trd>Troddel</trd>
  </trdgrp>
  <trd lng="en">tassel</trd>
  <trd lng="hu">bojt</trd>
  <trd lng="nl">kwast</trd>
  <trdgrp lng="pt">
    <trd>borla</trd>
    <trd>penacho</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_t;&c_mol; <klr>(&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">tofs</trd>
</drv>
</art>
