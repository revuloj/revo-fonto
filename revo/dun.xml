<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dun.xml,v 1.27 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>dun</rad>/o
</kap>

<drv mrk="dun.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="amas.0o">Amaso</ref> da
      <ref tip="prt" cel="sabl.0o">sablo</ref> formita de vento:
      <ekz>
        apudmara <tld/>o;
      </ekz><ekz>
        <ind>dezerta <tld/>o</ind>;
        <trd lng="be">&c_b;&c_a;&c_r;&c_h;&c_a;&c_n;</trd>
        <trd lng="de">D&uuml;ne</trd>
        <trd lng="ru">&c_b;&c_a;&c_r;&c_h;&c_a;&c_n;</trd>
      </ekz><ekz>
        tiam la sabloblovado &ccirc;esi&gcirc;is sed la <tld/>oj
        &ccirc;iam ankora&ubreve; staras
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
      </ekz><ekz>
        grandaj sablaj <tld/>oj staris tie tiel same kiel hejme &ccirc;e
        la Norda maro <klr>[...]</klr> kiamaniere ili venis &ccirc;i
        tien, tri mejlojn internen de la lando, kaj estis tiel
        same altaj kaj grandaj kiel &ccirc;e la bordo?
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>
      </ekz>
    </dif>
    <ref tip="malprt" cel="dezert.0o">dezerto</ref>
    <ref tip="vid" cel="dig.0o">digo</ref>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ju;&c_n;&c_a;</trd>
  <trd lng="bg">&c_d;&c_ju;&c_n;&c_a;</trd>
  <trd lng="cs">duna <klr>(p&#xED;se&#x10D;n&#xE1;)</klr></trd>
  <trd lng="de">D&uuml;ne</trd>
  <trd lng="en">dune</trd>
  <trd lng="es">duna</trd>
  <trd lng="fr">dune</trd>
  <trdgrp lng="he">
    <trd>&het;&vav;&lamed;&yod;&tav;</trd>,
    <trd>&dalet;&yod;&vav;&nun;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>d&udblac;ne</trd>,
    <trd>homokbucka</trd>
  </trdgrp>
  <trd lng="ja">&#x7802;&#x4E18;  <pr>&#x3055;&#x304D;&#x3085;&#x3046;</pr></trd>
  <trd lng="nl">duin</trd>
  <trdgrp lng="pl">
    <trd>wydma</trd>,
    <trd>diuna</trd>
  </trdgrp>
  <trd lng="pt">duna</trd>
  <trd lng="ru">&c_d;&c_ju;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>duna</trd>,
    <trd>pieskov&#xFD; n&#xE1;syp</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x44E;&#x43D;&#x430;</trd>
  <trd lng="zh">&#x6C99;&#x4E18; <pr>sh&#x101;qi&#x16B;</pr></trd>
</drv>

<drv mrk="dun.negx0o">
  <kap>ne&gcirc;<tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="amas.0o">Amaso</ref> da
      <ref tip="prt" cel="negx.0o">ne&gcirc;o</ref> formita de
      vento.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_r;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_jo;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schneewehe</trd>,
    <trd>Schneebrett</trd>
  </trdgrp>
  <trd lng="fr">cong&egrave;re</trd>
  <trd lng="he">&#x5D3;&#x5D9;&#x5D5;&#x5E0;&#x5EA; &#x5E9;&#x5DC;&#x5D2;</trd>
  <trd lng="hu">h&oacute;torlasz</trd>
  <trd lng="pl">zaspa</trd>
  <trd lng="ru">&c_s;&c_u;&c_g;&c_r;&c_o;&c_b;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43C;&#x435;&#x442;</trd>,
    <trd>&#x441;&#x43D;&#x456;&#x433;&#x43E;&#x432;&#x430; &#x43A;&#x443;&#x447;&#x443;&#x433;&#x443;&#x440;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x5439;&#x96EA; <pr>chu&#x12B;xu&#x11B;</pr></trd>
</drv>

</art>
