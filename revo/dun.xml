<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>dun</rad>/o
    </kap>

    <drv mrk="dun.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  <ref tip="super" cel="amas.0o">Amaso</ref> da
	  <ref tip="prt" cel="sabl.0o">sablo</ref> formita de vento:
	  <ekz>
	    apudmara <tld/>o;
	  </ekz><ekz>
	    <ind>dezerta <tld/>o</ind>;
	    <trd lng="be">&c_b;&c_a;&c_r;&c_h;&c_a;&c_n;</trd>
	    <trd lng="ru">&c_b;&c_a;&c_r;&c_h;&c_a;&c_n;</trd>
	  </ekz><ekz>
	    tiam la sabloblovado &ccirc;esi&gcirc;is sed la <tld/>oj
	    &ccirc;iam ankora&ubreve; staras
	    <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
	  </ekz><ekz>
	    grandaj sablaj <tld/>oj staris tie tiel same kiel hejme &ccirc;e
	    la Norda maro <klr>[...]</klr> kiamaniere ili venis &ccirc;i
	    tien, tri mejlojn internen de la lando, kaj estis tiel
	    same altaj kaj grandaj kiel &ccirc;e la bordo?
	    <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>
	  </ekz>
	</dif>
	<ref tip="malprt" cel="dezert.0o">dezerto</ref>
	<ref tip="vid" cel="dig.0o">digo</ref>
      </snc>
      <trd lng="be">&c_d;&c_z;&c_ju;&c_n;&c_a;</trd>
      <trd lng="de">D&uuml;ne</trd>
      <trd lng="en">dune</trd>
      <trd lng="es">duna</trd>
      <trd lng="fr">dune</trd>
      <trdgrp lng="he">
		<trd>&het;&vav;&lamed;&yod;&tav;</trd>,
		<trd>&dalet;&yod;&vav;&nun;&he;</trd>
      </trdgrp>
      <trdgrp lng="hu">
		<trd>d&udblac;ne</trd>,
		<trd>homokbucka</trd>
      </trdgrp>
      <trd lng="nl">duin</trd>
      <trd lng="ru">&c_d;&c_ju;&c_n;&c_a;</trd>
    </drv>

    <drv mrk="dun.negx0o">
      <kap>ne&gcirc;<tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  <ref tip="super" cel="amas.0o">Amaso</ref> da
	  <ref tip="prt" cel="negx.0o">ne&gcirc;o</ref> formita de vento.
	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_g;&c_u;&c_r;&c_b;&c_a;</trd>,
        <trd>&c_s;&c_u;&c_m;&c_jo;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="de">
	<trd>Schneewehe</trd>,
	<trd>Schneebrett</trd>
      </trdgrp>
      <trd lng="hu">h&oacute;torlasz</trd>
      <trd lng="ru">&c_s;&c_u;&c_g;&c_r;&c_o;&c_b;</trd>
    </drv>

  </art>
