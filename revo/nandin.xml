<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nandin.xml,v 1.10 2024/02/01 15:25:12 revo Exp $">
<kap>
  <rad>nandin</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="nandin.0o">
  <kap><tld/>o</kap>
  <snc mrk="nandin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda <ref tip="super" cel="arb.0o.BOT">arbeto</ref> el
      Hindujo, &Ccirc;inujo, Japanujo,
      kun plumformaj folioj, blankaj-flavaj floroj, ru&gcirc;aj
      globofruktetoj,
      unuspecia genro el familio berberisacoj
      (<trd lng="la">Nandina domestica</trd>):
      <ekz>
        oni kune metis pinon, bambuon, umeon, <tld/>on kaj ornambrasikon
        en unu poto
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
        <lok>Pinornamoj por la nova jaro, 2005-12-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_n;&c_d;&c_y;&c_n;&c_a;</trd>
  <trd lng="ca">nandina</trd>
  <trdgrp lng="de">
    <trd>Himmelsbambus</trd>,
    <trd>Nandine</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&f_y; &a_m;&a_q;&a_d;&a_s;</trd>
  <trd lng="fr">nandina<klr tip="amb"> (bot.)</klr></trd>
  <trd lng="ja">&#x30CA;&#x30F3;&#x30C6;&#x30F3;</trd>
  <trd lng="nl">hemelse bamboe</trd>
  <trd lng="pl">nandina domowa</trd>
  <trd lng="ro">acas&abreve; nandina</trd>
  <trd lng="pt">nandina</trd>
  <trdgrp lng="zh">
    <trd>&#21335;&#22825;&#x7AFA; <pr>n&#xE1;nti&amacron;nzh&#xFA;</pr></trd>,
    <trd>&#21335;&#22825;&#x7AF9; <pr>n&#xE1;nti&amacron;nzh&#xFA;</pr></trd>,
    <trd>&#21335;&#22825;&#x7AF9;&#x5C5E; <pr>n&#xE1;nti&amacron;nzh&#xFA;sh&#x1D4;</pr></trd>,
    <trd>&#21335;&#22825;&#x7AF9;&#x5C6C; <pr>n&#xE1;nti&amacron;nzh&#xFA;sh&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>

