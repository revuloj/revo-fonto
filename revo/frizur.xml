<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frizur.xml,v 1.5 2019/10/05 08:10:16 revo Exp $">
<kap>
  <rad>frizur</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="frizur.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <dif>
    <ref tip="dif" cel="friz.0ajxo.KOMUNE">Friza&jcirc;o</ref>:
    <ekz>
      la harojn li lasis kreski &gcirc;is tia longeco, kiu ebligis fari 
      la&ubreve;modan <tld/>on
      <fnt>
        <aut>J. Baghy</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/migranta/melonkapo.html">
        Sinjoro Melonkapo</url></vrk>,
        <lok>en: Migranta plumo, 1929</lok>
      </fnt>;
    </ekz>
    <ekz>
      neniu venis kun tia &ccirc;emizo, tia <tld/>o... 
      <fnt>
        <aut>S. MacGill</aut>,
        <vrk><url
        ref="http://en.lernu.net/biblioteko/gazetoj/juna_amiko/artikoloj/rapid.php">
        Portu al mi, rapide!</url></vrk>,
        <lok>Juna amiko, 1998:4</lok>
      </fnt>.
    </ekz>
  </dif>
  <!-- formon evitindan prefere ne traduku
  <trdgrp lng="sk">
    <trd>ondul&aacute;cia</trd>,
    <trd>&uacute;&ccaron;es</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>friz&uacute;ra</trd>,
    <trd>ondulace</trd>,
    <trd>&uacute;&ccaron;es</trd>
  </trdgrp> -->
</drv>
</art>

