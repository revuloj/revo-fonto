<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
    <art mrk="$Id$">
      <kap>
        <rad>bijekci</rad>/o
      </kap>
      <drv mrk="bijekc.0o">
        <kap><tld/>o
        <fnt><aut>O. Reiers&#xf8;l</aut>:
           <vrk>
             Matematika kaj stokastika terminaro Esperanta.</vrk>,
             <lok>Universitato de Oslo, 1987. P.8</lok></fnt>
        </kap>
        <snc>
          <uzo tip="fak">MAT</uzo>
          <ref tip="dif" cel="jxet.dissur0o">dissur&jcirc;eto.</ref>
        </snc>
        <trd lng="fr">bijection</trd>
      </drv>

      <drv mrk="bijekc.0a">
        <kap><tld/>a</kap>
        <snc>
          <uzo tip="fak">MAT</uzo>
          <dif>
            <klr>(p.p. <ref cel="bild.0igo">bildigo</ref>)</klr>
            Havanta la ecojn de <tld/>o:
            <ekz>
              konstanta reela funkcio ne estas <tld/>a.
            </ekz>
          </dif>
        </snc>
        <trdgrp lng="fr">
          <trd>bijectif</trd>,
          <trd>biunivoque</trd></trdgrp>
        <trd lng="ru">&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o;
<ind>&c_o;&c_d;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;&c_j;</ind></trd>
      </drv>

    </art>

