<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bijekc.xml,v 1.18 2022/03/21 17:23:35 revo Exp $">
<kap>
<rad>bijekci</rad>/o
</kap>
<drv mrk="bijekc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref>, kiu estas
      <ref tip="super" cel="enjekc.0o">enjekcio</ref> kaj
      <ref tip="super" cel="surjek.0o">surjekcio</ref>:
      <ekz>
        <tld/>o &ccirc;iam estas inversigebla.
      </ekz>
    </dif>
    <ref tip="sin" cel="jxet.dissur0o">dissur&jcirc;eto;</ref>
    <ref tip="vid" cel="kardin.0o.MAT">kardinalo.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_je;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_u;&c_z;&c_a;&c_je;&c_m;&c_n;&c_a; &c_a;&c_d;&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_a;&c_s;&c_c;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="cs">bijekce</trd>
  <trd lng="de">Bijektion</trd>
  <trdgrp lng="en">
    <trd>bijection</trd>,
    <trd>biunique correspondence</trd>,
    <trd>one-to-one mapping</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>biyecci&oacute;n</trd>,
    <trd>funci&oacute;n biyectiva</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bijection</trd>,
    <trd>application bijective</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;tir&aacute;ny&uacute; <ind>kibocs&aacute;t&aacute;s</ind></trd>,
    <trd>k&ouml;lcs&ouml;n&ouml;sen egy&eacute;rtelm&udblac; <ind>megfeleltet&eacute;s</ind></trd>,
    <trd>one-to-one mapping</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>funkcja odwracalna</trd>,
    <trd>funkcja wzajemnie jednoznaczna</trd>,
    <trd>bijekcja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o; &c_o;&c_d;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_o;&c_je; <ind>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>,
    <trd>&c_b;&c_i;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">bijekcia</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x437;&#x430;&#x454;&#x43C;&#x43D;&#x43E; &#x43E;&#x434;&#x43D;&#x43E;&#x437;&#x43D;&#x430;&#x447;&#x43D;&#x430; &#x432;&#x456;&#x434;&#x43F;&#x43E;&#x432;&#x456;&#x434;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x431;&#x456;&#x454;&#x43A;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="bijekc.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>)</klr>
      Havanta la ecojn de <tld/>o:
      <ekz>
        konstanta reela funkcio ne estas <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_z;&c_a;&c_je;&c_m;&c_n;&c_a; <ind>&c_a;&c_d;&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bijektiv</trd>,
    <trd>eineindeutig</trd></trdgrp> 
  <trdgrp lng="en">
    <trd>bijective</trd>,
    <trd>one-to-one</trd></trdgrp> 
  <trd lng="es">biyectiva</trd>
  <trdgrp lng="fr">
    <trd>bijectif</trd>,
    <trd>biunivoque</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bijektywny</trd>,
    <trd>odwracalny</trd>,
    <trd>wzajemnie jednoznaczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd> &c_b;&c_i;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; </trd>,
    <trd>&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o; <ind>&c_o;&c_d;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;&c_j;</ind></trd></trdgrp> 
  <trd lng="uk">&#x431;&#x456;&#x454;&#x43A;&#x446;&#x456;&#x439;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
