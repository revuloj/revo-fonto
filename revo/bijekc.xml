<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bijekci</rad>/o
</kap>
<drv mrk="bijekc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref>, kiu estas
      <ref tip="super" cel="enjekc.0o">enjekcio</ref> kaj
      <ref tip="super" cel="surjek.0o">surjekcio</ref>:
      <ekz>
        <tld/>o &ccirc;iam estas inversigebla.
      </ekz>
    </dif>
    <ref tip="sin" cel="jxet.dissur0o">dissur&jcirc;eto.</ref>
  </snc>
  <trd lng="de">Bijektion</trd>
  <trdgrp lng="en">
    <trd>bijection</trd>,
    <trd>biunique correspondence</trd>,
    <trd>one-to-one mapping</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bijection</trd>,
    <trd>application bijective</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o; &c_o;&c_d;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_o;&c_je; <ind>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>,
    <trd>&c_b;&c_i;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="bijekc.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>)</klr>
      Havanta la ecojn de <tld/>o:
      <ekz>
        konstanta reela funkcio ne estas <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>bijektiv</trd>,
    <trd>eineindeutig</trd></trdgrp> 
  <trdgrp lng="en">
    <trd>bijective</trd>,
    <trd>one-to-one</trd></trdgrp> 
  <trdgrp lng="fr">
    <trd>bijectif</trd>,
    <trd>biunivoque</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd> &c_b;&c_i;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; </trd>,
    <trd>&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o; <ind>&c_o;&c_d;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;&c_j;</ind></trd></trdgrp> 
</drv>
</art>
