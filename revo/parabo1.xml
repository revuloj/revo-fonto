<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parabo1.xml,v 1.10 2024/02/16 19:24:10 revo Exp $">
<kap>
  <rad>paraboloid</rad>/o
</kap>
<drv mrk="parabo1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 36</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="kvadri1.0o">Kvadriko</ref> kun
      ekvacio de la tipo 
      <frm am="x^2//a+y^2//b=z"><k>x</k><sup>2</sup>/<k>a</k>+<k>y</k><sup>2</sup>/<k>b</k> =
      <k>z</k></frm>:
      <ekz>
        elipsa <tld/>o <klr>(kun <ref cel="sign.sam0a">samsignaj</ref>
        <frm am="a"><k>a</k></frm> kaj <frm am="b"><k>b</k></frm>)</klr>;
      </ekz>
      <ekz>
        hiperbola <tld/>o <klr>(kun 
        <ref cel="sign.kontraux0a">kontra&ubreve;signaj</ref>
        <frm am="a"><k>a</k></frm> kaj <frm am="b"><k>b</k></frm>)</klr>;
      </ekz>
      <ekz>
        la sekcoj de rotacia a&ubreve; rivolua <tld/>o
        <klr>(kun <frm am="a=b"><k>a</k> = <k>b</k></frm>)</klr>
        per ebeno enhavanta la rotacian akson estas 
        <ref cel="parabo.0o.linio">paraboloj</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_b;&c_a;&c_l;&c_jo;&c_ib;&c_d;</trd>
  <trd lng="pt">parabol&oacute;ide</trd>
  <trd lng="de">Paraboloid</trd>
  <trd lng="es">paraboloide</trd>
  <trd lng="en">paraboloid</trd>
  <trd lng="fr">parabolo&iuml;de</trd>
  <trd lng="hu">paraboloid</trd>
  <trd lng="nl">parabolo&iuml;de</trd>
  <trd lng="pl">paraboloida</trd>
  <trd lng="pt">parabol&oacute;ide</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_b;&c_o;&c_l;&c_o;&c_i;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x629B;&#x7269;&#x9762; <pr>p&#x101;ow&#xF9;mi&#xE0;n</pr></trd>,
    <trd>&#x62CB;&#x7269;&#x9762; <pr>p&#x101;ow&#xF9;mi&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>

