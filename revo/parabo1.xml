<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>paraboloid</rad>/o
</kap>
<drv mrk="parabo1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 36</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="kvadri1.0o">Kvadriko</ref> kun
      ekvacio de la tipo 
      <frm><k>x</k><sup>2</sup>/<k>a</k>+<k>y</k><sup>2</sup>/<k>b</k> =
      <k>z</k></frm>:
      <ekz>
        elipsa <tld/>o <klr>(kun <ref cel="sign.sam0a">samsignaj</ref>
        <frm><k>a</k></frm> kaj <frm><k>b</k></frm>)</klr>;
      </ekz>
      <ekz>
        hiperbola <tld/>o <klr>(kun 
        <ref cel="sign.kontraux0a">kontra&ubreve;signaj</ref>
        <frm><k>a</k></frm> kaj <frm><k>b</k></frm>)</klr>;
      </ekz>
      <ekz>
        la sekcoj de rotacia a&ubreve; rivolua <tld/>o
        <klr>(kun <frm><k>a</k> = <k>b</k></frm>)</klr>
        per ebeno enhavanta la rotacian akson estas 
        <ref cel="parabo.0o.linio">paraboloj</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Paraboloid</trd>
  <trd lng="es">paraboloide</trd>
  <trd lng="en">paraboloid</trd>
  <trd lng="fr">parabolo&iuml;de</trd>
  <trd lng="pl">paraboloida</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_b;&c_o;&c_l;&c_o;&c_i;&c_d;</trd>
</drv>
</art>

