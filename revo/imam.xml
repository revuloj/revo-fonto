<vortaro>
<art mrk="$Id$">
<kap>
  <rad>imam</rad>/o
</kap>
<drv mrk="imam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ISL</uzo>
  <snc num="1">
    <dif>
      Teologo kaj kultoficanto de <ref tip="vid" cel="moske.0o">moskeo</ref>.
    </dif>
  </snc>  
  <snc num="2">
    <dif>
      Religia kaj politika suvereno de islamanoj, titolo de la
      <ref tip="vid" cel="kalif.0o">kalifoj</ref>.
    </dif>
  </snc>
  <trd lng="de">Imam</trd>
  <trdgrp lng="fr">
    <trd>imam</trd>,
    <trd>iman</trd></trdgrp> 
  <trd lng="nl">imam</trd>
  <trd lng="ru">&c_i;&c_m;&c_a;&c_m;</trd>
</drv>
</art>

