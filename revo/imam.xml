   <trd>imamo<klr tip="amb"> (islamismo)</klr></trd>, 
   <trd>imame<klr tip="amb"> (islamismo)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_a;&c_m;</trd>
</drv>
</art>

