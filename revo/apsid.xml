<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apsid</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="apsid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
  Tiuj punktoj de la orbito de <ref cel="astro">astro</ref> 
  a&ubreve; <ref cel="artefarita_satelito">artefarita satelito</ref>, kie tiu 
  estas plej proksima a&ubreve; plej malproksima de la centra &ccirc;irka&ubreve;ata objekto.
    </dif>
  <refgrp tip="sub">
    <ref cel="afeli.0o">afelio</ref>,
    <ref cel="perihe.0o">perihelio</ref>,
    <ref cel="perige.0o">perigeo</ref>
  </refgrp>
  </snc>
  <trd lng="de">Apside</trd>
  <trd lng="es">&aacute;pside</trd>
  <trdgrp lng="en">
     <trd>apsis</trd>,
     <trd>apse</trd>
  </trdgrp>
  <trd lng="fr">apside</trd>
  <trd lng="pt">apside</trd>
  <trd lng="ru">&c_a;&c_p;&c_s;&c_i;&c_d;&c_a;</trd>
</drv>
</art>
