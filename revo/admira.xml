<art mrk="$Id: admira.xml,v 1.30 2021/02/28 09:26:17 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>admiral</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="admira.0o">
  <kap><tld/>o</kap>
  <snc mrk="admira.0o.MIL">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      En mararmeo, 
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref> kun
      rango ekvivalenta al tiu de <ref cel="genera1.0o">generalo</ref>:
      <ekz>
        <tld/>o Avelan kun oficiroj de sia stabo surbordi&gcirc;is
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="sxip.0arestro">&scirc;iparestro</ref>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_ib;&c_r;&c_a;&c_l;</trd>
  <trd lng="bg">&c_a;&c_d;&c_m;&c_i;&c_r;&c_a;&c_l;</trd>
  <trd lng="ca">almirall</trd>
  <trd lng="cs">admir&aacute;l</trd>
  <trd lng="de">Admiral</trd>
  <trd lng="en">admiral</trd>
  <trd lng="es">almirante</trd>
  <trd lng="fa">&a_d;&a_r;&f_y;&a_A;&a_s;&a_A;&a_l;&a_A;&a_r;</trd>
  <trd lng="fr">amiral</trd>
  <trd lng="he">&#x5E8;&#x5D1; &#x5D7;&#x5D5;&#x5D1;&#x5DC;</trd>
  <trdgrp lng="hu">
    <trd>tengernagy</trd>,
    <trd>admir&aacute;lis</trd>
  </trdgrp>
  <trd lng="id">admiral</trd>
  <trd lng="io">admiralo</trd>
  <trd lng="it">ammiraglio</trd>
  <trdgrp lng="ja">
    <trd><klr tip="amb">&#xFF08;&#x6D77;&#x8ECD;&#x306E;&#xFF09;</klr>&#x5C06;&#x5B98; [<ind>&#x3057;&#x3087;&#x3046;&#x304B;&#x3093;</ind>]</trd>,
    <trd><klr tip="ind">&#xFF08;&#x6D77;&#x8ECD;&#x306E;&#xFF09;</klr>&#x63D0;&#x7763; [<ind>&#x3066;&#x3044;&#x3068;&#x304F;</ind>]</trd>,
    <trd>&#x6D77;&#x8ECD;&#x5927;&#x5C06; [<ind>&#x304B;&#x3044;&#x3050;&#x3093;&#x305F;&#x3044;&#x3057;&#x3087;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">admiraal</trd>
  <trd lng="pl">admira&lstroke;</trd>
  <trd lng="pt">almirante</trd>
  <trd lng="ro">amiral</trd>
  <trd lng="ru">&c_a;&c_d;&c_m;&c_i;&c_r;&c_a;&c_l;</trd>
  <trd lng="sk">admir&aacute;l</trd>
  <trd lng="sv">amiral</trd>
</drv>
</art>
