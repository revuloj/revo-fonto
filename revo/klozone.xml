<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klozone</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="klozone.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <ref tip="dif" cel="emajl.cxel0o">&ccirc;elemajlo</ref>
    <ekz>
      <tld/>o <klr>[...]</klr> estas speco de emajlo, en kiu diversaj 
      farboj estas entenataj en &ccirc;eletoj
      <fnt><bib>EncCxi</bib> <lok>&ccirc;apitro &leftquot;Klozoneo&rightquot;,
      p. 222a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Cloisonn&eacute;</trd>
  <trdgrp lng="fr">
    <trd>cloisonn&eacute;<klr tip="ind"> (technique de joaillerie)</klr></trd>
  </trdgrp>
  <trd lng="hu">rekeszzom&aacute;nc</trd>  
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_r;&c_o;&c_d;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_e;&c_m;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_l;&c_u;&c_a;&c_z;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="klozone.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>en <ind>cloisonn&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rekeszzom&aacute;ncb&oacute;l k&eacute;sz&uuml;lt</trd>,
    <trd>rekeszzom&aacute;nc</trd>
  </trdgrp>
</drv>

<drv mrk="klozone.0a&jcirc;o">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Arta&jcirc;o el <tld/>o:
      <ekz>
        en farado de grandaj <tld/>a&jcirc;oj oni plibonigis la teknikon pretigi
        la fono kaj lamenetojn
        <fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Klozoneo&rightquot;,
        p. 223a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cloisonn&eacute;<klr tip="ind"> (objet en cloisonn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">rekeszzom&aacute;nc <klr tip="ind">(m&udblac;)</klr></trd>  
  <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z; <ind>&c_k;&c_l;&c_u;&c_a;&c_z;&c_o;&c_n;&c_je;</ind></trd>
</drv>
</art>
