<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klozone</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klozone.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="emajl.cxel0o">&ccirc;elemajlo</ref>
      <ekz>
        <tld/>o [...] estas speco de emajlo, en kiu diversaj farboj estas
        entenataj en &ccirc;eletoj<fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Klozoneo&rightquot;,
        p. 222a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cloisonn&eacute;<klr> (joaillerie)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="klozone.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>en <ind>cloisonn&eacute;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="klozone.0a&jcirc;o">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Arta&jcirc;o el <tld/>o:
      <ekz>
        en farado de grandaj <tld/>a&jcirc;oj oni plibonigis la teknikon pretigi
        la fono kaj lamenetojn<fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Klozoneo&rightquot;,
        p. 223a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cloisonn&eacute;<klr> (objet, joaillerie)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>