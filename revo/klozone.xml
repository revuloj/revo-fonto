<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klozone.xml,v 1.16 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>klozone</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="klozone.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="klozone.0o.cxelemajlo">
    <dif>
      <ref tip="dif" cel="emajl.cxel0o">&Ccirc;elemajlo</ref>:
      <ekz>
        <tld/>o <klr>[...]</klr> estas speco de emajlo, en kiu diversaj 
        farboj estas entenataj en &ccirc;eletoj
        <fnt><bib>EncCxi</bib> <lok>&ccirc;apitro &leftquot;Klozoneo&rightquot;,
        p. 222a</lok></fnt>;
      </ekz>
      <ekz>
        du agloformaj fiboloj fabrikitaj el oro, bronzo kaj vitro pere
        de la tekniko de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=597687">Alovera</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">email "cloisonn&eacute;"</trd>
  <trd lng="de">Cloisonn&eacute;</trd>
  <trd lng="en">cloisonn&eacute;</trd>
  <trdgrp lng="fr">
    <trd>cloisonn&eacute;<klr tip="ind"> (technique de joaillerie)</klr></trd>
  </trdgrp>
  <trd lng="hu">rekeszzom&aacute;nc</trd>
  <trd lng="ja">&#x6709;&#x7DDA;&#x4E03;&#x5B9D;  <pr>&#x3086;&#x3046;&#x305B;&#x3093;&#x3057;&#x3063;&#x307D;&#x3046;</pr></trd>
  <trd lng="nl">cloisonn&eacute;<klr tip="ind"> (zn.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_r;&c_o;&c_d;&c_ch;&c_a;&c_t;&c_a;&c_ja;
    &c_e;&c_m;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_l;&c_u;&c_a;&c_z;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">email "cloisonn&eacute;"</trd>
  <trd lng="uk">&c_k;&c_l;&c_u;&c_a;&c_z;&c_o;&c_n;&c_je; (&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;&c_mol;&c_k;&c_a; &c_je;&c_m;&c_a;&c_l;&c_mol;)</trd>
  <trd lng="zh">&#x666F;&#x6CF0;&#x84DD; <pr>j&#x1D0;ngt&#xE0;il&#xE1;n</pr></trd>
</drv>

<drv mrk="klozone.0a">
  <kap><tld/>a</kap>
  <snc mrk="klozone.0a.farita_per">
    <dif>
      Farita, ornamita per <tld/>o.
    </dif>
  </snc>
  <trd lng="en">cloisonn&eacute;</trd>
  <trdgrp lng="fr">
    <trd>en <ind>cloisonn&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rekeszzom&aacute;ncb&oacute;l k&eacute;sz&uuml;lt</trd>,
    <trd>rekeszzom&aacute;nc</trd>
  </trdgrp>
   <trd lng="nl">cloisonn&eacute;<klr tip="ind"> (bn.)</klr></trd>  
</drv>

<drv mrk="klozone.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="klozone.0ajxo.artajxo">
    <dif>
      Arta&jcirc;o el <tld/>o:
      <ekz>
        en farado de grandaj <tld/>a&jcirc;oj oni plibonigis la teknikon
        pretigi la fonon kaj lamenetojn
        <fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Klozoneo&rightquot;,
        p. 223a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cloisonn&eacute;<klr tip="ind"> (objet en cloisonn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">rekeszzom&aacute;nc <klr tip="ind">(m&udblac;)</klr></trd> 
  <trd lng="nl">voorwerp in <ind>cloisonn&eacute;</ind></trd> 
  <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z;
  <ind>&c_k;&c_l;&c_u;&c_a;&c_z;&c_o;&c_n;&c_je;</ind></trd>
  <trd lng="zh">&#x73D0; <pr>f&#xE0;</pr></trd>
</drv>
</art>
