<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>atrofi</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="atrofi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsaneca malkresko de iu organo.
    </dif>
    <refgrp tip="sin">
      <ref cel="kripl.0a">kripla</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="de">Atrophie</trd>
  <trd lng="en">atrophy</trd>
  <trd lng="es">atrofia</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_l;&a_A;&a_s;&f_y;&a_d;&f_g;&f_y;</trd>,
    <trd>&f_k;&a_A;&a_h;&f_y;&a_d;&f_g;&f_y;</trd>,
    <trd>&a_A_madda;&a_t;&a_r;&a_w;&a_f;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">atrophie</trd>
  <trdgrp lng="hu">
    <trd>sorvad&aacute;s</trd>,
    <trd>atr&oacute;fia</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">atrofia</trd>
</drv>

<drv mrk="atrofi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      En stato de <tld/>o.
    </dif>
    <ekz>
      lia koro, rilate al amo, jam estis malviva;
      tro <tld/>a por senti
      <fnt><bib>I&scirc;tar</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="en">atrophied</trd>
  <trd lng="es">atrofiarse</trd>
  <trd lng="fr">s'<ind>atrophier</ind></trd>
  <trd lng="hu">sorvad</trd>
  <trd lng="sk">atrofova&tcaron;</trd>
</drv>

<drv mrk="atrofi.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
    <ekz>
      dum la interplaneda vojagxo [...] la membroj certe iom atrofios
      <fnt><bib>Monato</bib></fnt>. <!-- (Monato 2010-08 p.29) -->
    </ekz>
    <trd lng="en">be atrophied</trd>
  </snc>
  <snc>
    <fnt>PIV</fnt>
    <ref tip="dif" cel="atrofi.0igi">atrofiigi</ref>
    <rim>Estas malkonsento, &ccirc;u la transitiva difino sencas.</rim>
  </snc>
</drv>

<drv mrk="atrofi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="en">
    <trd>undergo atrophy</trd>,
    <trd>atrophy</trd>
  </trdgrp>
  <trd lng="es">atrofiarse</trd>
  <trd lng="fr">s'<ind>atrophier</ind></trd>
  <trd lng="hu">sorvad</trd>
  <trd lng="sk">atrofova&tcaron;</trd>
</drv>

<drv mrk="atrofi.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cause atrophy</trd>,
    <trd>atrophy</trd>
  </trdgrp>
  <trd lng="es">atrofiar</trd>
  <trd lng="fr">atrophier</trd>
  <trd lng="hu">sorvaszt</trd>
</drv>
</art>
