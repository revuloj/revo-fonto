<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atrofi.xml,v 1.27 2023/08/30 18:23:38 revo Exp $">
<kap>
  <rad>atrofi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="atrofi.0o">
  <kap><tld/>o</kap>
  <snc mrk="atrofi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malgrandi&gcirc;o de organo, histo, &ccirc;elo, natura a&ubreve; malsaneca:
      <ekz>
        pro la detruo de ne&ubreve;ronoj (nervaj &ccirc;eloj) &ccirc;e malsanuloj aperas muskola <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010269.php">Poliomjelito: &ccirc;u la dua tutmonde likvidota infektmalsano?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de l&#x2019; umbilika &scirc;nuro
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distrofi.0o">distrofio</ref>,
      <ref cel="kripl.0a">kripla</ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="hipert.0o.MED">hipertrofio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="ca">atr&ograve;fia</trd>
  <trdgrp lng="cs">
    <trd>senilita</trd>,
    <trd>sta&rcaron;eck&aacute; demence</trd>,
    <trd>sta&rcaron;eck&yacute; marasmus</trd>
  </trdgrp>
  <trd lng="de">Atrophie</trd>
  <trd lng="en">atrophy</trd>
  <trd lng="es">atrofia</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_l;&a_A;&a_s;&f_y;&a_d;&f_g;&f_y;</trd>,
    <trd>&f_k;&a_A;&a_h;&f_y;&a_d;&f_g;&f_y;</trd>,
    <trd>&a_A_madda;&a_t;&a_r;&a_w;&a_f;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">atrophie</trd>
  <trd lng="he">&he;&tav;&nun;&vav;&vav;&nun;&vav;&tav; &shin;&lamed; &alef;&yod;&bet;&resh;</trd>
  <trdgrp lng="hu">
    <trd>sorvad&aacute;s</trd>,
    <trd>atr&oacute;fia</trd>
  </trdgrp>
  <trd lng="it">atrofia</trd>
  <trd lng="ja">&#x840E;&#x7E2E;  [<ind>&#x3044;&#x3057;&#x3085;&#x304F;</ind>]</trd>
  <trd lng="nl">atrofie</trd>
  <trd lng="ru">&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>atrofia</trd>,
    <trd>senilita</trd>,
    <trd>stareck&aacute; demencia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_t;&c_r;&c_o;&c_f;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_i;&c_r;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_m;&c_je;&c_n;&c_sh;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="atrofi.0a">
  <kap><tld/>a</kap>
  <snc mrk="atrofi.0a.MED">
    <dif>
      En stato de <tld/>o:
      <ekz>
        paralizita kaj <tld/>a dekstra piedo, tipa por la sekvoj de poliomjelito
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010269.php">Poliomjelito: &ccirc;u la dua tutmonde likvidota infektmalsano?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia koro, rilate al amo, jam estis <klr>[&#x2026;]</klr> tro atrofia por senti e&ccirc; la
        indignon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>atr&ograve;fic</trd>,
    <trd>atrofiat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>atrofick&yacute;</trd>,
    <trd>zakrn&ecaron;l&yacute;</trd>,
    <trd>zmen&scaron;en&yacute;</trd>
  </trdgrp>
  <trd lng="de">verk&uuml;mmert</trd>
  <trd lng="en">atrophied</trd>
  <trd lng="es">atrofiarse</trd>
  <trd lng="fr">atrophi&eacute;<klr> (adj.)</klr></trd>
  <trd lng="hu">sorvad</trd>
  <trdgrp lng="it">
    <trd>atrofico</trd>,
    <trd>atrofizzato</trd>
  </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>atrofick&yacute;</trd>,
    <trd>atrofova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="atrofi.0i">
  <kap><tld/>i</kap>
  <snc mrk="atrofi.0i.MED">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      (Ek)esti <tld/>a:
      <ekz>
        dum la interplaneda voja&gcirc;o <klr>[&#x2026;]</klr> la membroj certe iom <tld/>os
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>estar atr&ograve;fic</trd>,
      <trd>patir d'atr&ograve;fia</trd>
    </trdgrp>
    <trd lng="en">be atrophied</trd>
    <trdgrp lng="it">
      <trd>essere atrofico</trd>
      <trd>essere atrofizzato</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_y;&c_t;&c_mol; <ind>&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ch;&c_n;&c_y;&c_m;</ind></trd>
  </snc>
  <snc>
    <fnt>PIV</fnt>
    <gra><vspec>tr</vspec></gra>
    <ref tip="dif" cel="atrofi.0igi">atrofiigi</ref>
    <rim>
      Estas malkonsento pri la transitiveco.
    </rim>
  </snc>
  <trdgrp lng="cs">
    <trd>b&yacute;t atrofick&yacute;</trd>,
    <trd>zakrn&ecaron;l&yacute;</trd>
  </trdgrp>
  <trd lng="de">verk&uuml;mmern</trd>
  <trd lng="ja">&#x840E;&#x7E2E;&#x3055;&#x305B;&#x308B;  [<ind>&#x3044;&#x3057;&#x3085;&#x304F;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  <trd lng="sk">by&tcaron; atrofick&yacute;</trd>
</drv>

<drv mrk="atrofi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="atrofi.0igxi.MED">
    <dif>
      I&gcirc;i <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="en">
    <trd>undergo atrophy</trd>,
    <trd>atrophy</trd>
  </trdgrp>
  <trd lng="ca">atrofiar-se</trd>
  <trd lng="de">verk&uuml;mmern</trd>
  <trd lng="es">atrofiarse</trd>
  <trd lng="fr">s'<ind>atrophier</ind></trd>
  <trd lng="hu">sorvad</trd>
  <trd lng="it">atrofizzarsi</trd>
  <trd lng="ja">&#x840E;&#x7E2E;&#x3059;&#x308B;  [<ind>&#x3044;&#x3057;&#x3085;&#x304F;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="nl">wegkwijnen</trd>
  <trd lng="ru">&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">atrofova&tcaron;</trd>
</drv>

<drv mrk="atrofi.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atrofi.0igi.MED">
    <dif>
      Ka&ubreve;zi <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">atrofiar</trd>
  <trdgrp lng="en">
    <trd>cause atrophy</trd>,
    <trd>atrophy</trd>
  </trdgrp>
  <trd lng="es">atrofiar</trd>
  <trd lng="fr">atrophier</trd>
  <trd lng="hu">sorvaszt</trd>
  <trd lng="it">atrofizzare</trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; <ind>&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ju;</ind></trd>
</drv>
</art>
