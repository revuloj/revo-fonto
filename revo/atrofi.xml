<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>atrofi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="atrofi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsaneca malkresko de iu organo.
    </dif>
    <refgrp tip="sin">
      <ref cel="kripl.0a">kripla</ref>.
    </refgrp>
  </snc>
  <trd lng="es">atrofia</trd>
  <trd lng="fr">atrophie</trd>
  <trdgrp lng="hu">
    <trd>sorvad&aacute;s</trd>,
    <trd>atr&oacute;fia</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_t;&c_r;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">atrofia</trd>
</drv>

<drv mrk="atrofi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti en stato de <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="abort.0i">aborti</ref>.
    </refgrp>
  </snc>
  <trd lng="es">atrofiarse</trd>
  <trd lng="fr">s'<ind>atrophier</ind></trd>
  <trd lng="hu">sorvad</trd>
  <trd lng="sk">atrofova&tcaron;</trd>
</drv>

<drv mrk="atrofi.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on.
    </dif>
  </snc>
  <trd lng="es">atrofiar</trd>
  <trd lng="fr">atrophier</trd>
  <trd lng="hu">sorvaszt</trd>
</drv>
</art>
