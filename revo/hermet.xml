<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hermet.xml,v 1.21 2017/07/02 12:10:15 revo Exp $">
<kap>
  <rad>hermet</rad>/a <fnt>Z</fnt>
</kap>
<drv mrk="hermet.0a">
  <kap><tld/>a</kap>
  <snc mrk="hermet.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="hermetik.0a.TEK">Hermetika<sncref/></ref>:
      <ekz>
        en premkuirantaj multekuiriloj la kovrilo estas <tld/>a kaj havas valvon por reguli la kuirpremon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=526163">Multekuirilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">hermetisch</trd>
  </snc>
  <snc mrk="hermet.0a.malklara">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="hermetik.0a.malklara">Hermetika<sncref/></ref>:
      <ekz>
        kelkaj, a&ubreve; eble multaj, trovas <klr>...</klr> liajn poemojn
        malfacilaj, nekompreneblaj, <tld/>aj
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk><url ref="http://www.liberafolio.org/2006/kantojdeanteo">
          Poeto &ccirc;e mar&ccirc;ando kun Anubo</url></vrk>,
          <lok>libera folio, 2008-01-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">unzug&auml;nglich</trd>
  </snc>
  <trd lng="ca">herm&egrave;tic</trd>
  <trdgrp lng="cs">
    <trd>hermetick&yacute;</trd>,
    <trd>neprody&scaron;n&yacute;</trd>,
    <trd>vzduchot&ecaron;sn&yacute;</trd>
  </trdgrp>
  <trd lng="en">hermetic</trd>
  <trd lng="fr">herm&eacute;tique</trd>
  <trd lng="it">ermetico</trd>
  <trd lng="pt">herm&eacute;tico</trd>
  <trdgrp lng="sk">
    <trd>hermetick&yacute;</trd>,
    <trd>nepriedu&scaron;n&yacute;</trd>
  </trdgrp>
  <adm>Mi ne trovis prubon pri la Zamehofeco de tiu vorto <aut>WD</aut></adm>
</drv>
</art>
