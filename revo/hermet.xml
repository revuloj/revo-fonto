<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hermet(ik)</rad>/a<fnt>Z</fnt>
</kap>
<drv mrk="hermet.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Lasanta eniri a&ubreve; eliri neniun fluida&jcirc;on; provizita per
      tia fermosistemo:
      <ekz>
        <tld/>a fermo;
      </ekz>
      <ekz>
        <tld/>e &scirc;topita botelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_m;&c_je;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>hermetisch</trd>,
    <trd>luft- und <ind>wasserdicht</ind></trd>
  </trdgrp>
  <trd lng="el">&epsilon;&rho;&my;&eta;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">herm&eacute;tico</trd>
  <trdgrp lng="fr">
    <trd>herm&eacute;tique</trd>,
    <trd>&eacute;tanche</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hermetikus</trd>,
    <trd>t&ouml;m&iacute;tett</trd>,
    <trd>szigetelt</trd>
  </trdgrp>
  <trd lng="nl">hermetisch</trd>
  <trd lng="pt">herm&eacute;tico</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_r;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_o; &c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
