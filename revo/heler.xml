<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: heler.xml,v 1.13 2022/03/20 15:11:17 revo Exp $">
<kap>
  <rad>heler</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="heler.0o">
  <kap><tld/>o</kap>
  <snc mrk="heler.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iama <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">mona
      subunuo</ref> egala
      al centono de <ref tip="malprt" cel="kron2.0o.EKON">krono</ref>
      germana a&ubreve; a&ubreve;stra; tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        oni a&ccirc;etis al li lignan pladeton por kelke da <tld/>oj
        <fnt><bib>ElektFab</bib>, <lok>maljuna avo kaj nepo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_l;&c_je;&c_r; <klr>(&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>hal&#xE9;&#x159;</trd>,
    <trd>hal&#xED;&#x159;</trd>
  </trdgrp>
  <trd lng="de">Heller<klr tip="amb"> (M&uuml;nze)</klr></trd>
  <trd lng="en">heller<klr tip="amb"> (monetary unit)</klr></trd>
  <trd lng="fr">heller<klr tip="amb"> (monnaie)</klr></trd>
  <trdgrp lng="hu">
    <trd>heller</trd>,
    <trd>fill&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>heller</trd>,
    <trd>haller</trd>
  </trdgrp>
  <trd lng="nl">heller</trd>
  <trd lng="pl">halerz</trd>
  <trd lng="pt">heller</trd>
  <trd lng="sk">halier</trd>
  <trd lng="uk">&#x433;&#x435;&#x43B;&#x435;&#x440; (&#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&#x430; &#x43E;&#x434;&#x438;&#x43D;&#x438;&#x446;&#x44F;)</trd>
</drv>
</art>

