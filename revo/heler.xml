<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>heler</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="heler.0o">
  <kap><tld/>o</kap>
  <snc mrk="heler.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iama <ref tip="super" cel="unu.monasub0o">mona subunuo</ref> egala
      al centono de <ref tip="malprt" cel="kron2.0o.EKON">krono</ref>
      germana a&ubreve; a&ubreve;stra; tiomvalora <ref tip="super"
      cel="mon.0ero">monero</ref>:
      <ekz>
        oni a&ccirc;etis al li lignan pladeton por kelke da <tld/>oj
        <fnt><bib>ElektFab</bib>, <lok>maljuna avo kaj nepo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_l;&c_je;&c_r; <klr>(&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  <trd lng="de">Heller<klr tip="amb"> (M&uuml;nze)</klr></trd>
  <trd lng="en">heller<klr tip="amb"> (monetary unit)</klr></trd>
  <trd lng="fr">heller<klr tip="amb"> (monnaie)</klr></trd>
  <trdgrp lng="hu">
    <trd>heller</trd>,
    <trd>fill&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>heller</trd>,
    <trd>haller</trd>
  </trdgrp>
  <trd lng="nl">heller</trd>
  <trd lng="pl">halerz</trd>
  <trd lng="pt">heller</trd>
</drv>
</art>

