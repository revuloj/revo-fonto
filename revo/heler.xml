<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>heler</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="heler.0o">
  <kap><tld/>o</kap>
  <snc mrk="heler.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iama <ref tip="super" cel="unu.monasub0o">mona subunuo</ref> egala
      al centono de <ref tip="malprt" cel="kron2.0o.EKON">krono</ref>
      germana a&ubreve; a&ubreve;stra; tiomvalora <ref tip="super"
      cel="mon.0ero">monero</ref>:
      <ekz>
        oni a&ccirc;etis al li lignan pladeton por kelke da <tld/>oj
        <fnt><bib>ElektFab</bib>, <lok>maljuna avo kaj nepo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">heller<klr tip="amb"> (monnaie)</klr></trd>
  <trdgrp lng="hu">
    <trd>heller</trd>,
    <trd>fill&eacute;r</trd>
  </trdgrp>
</drv>
</art>

