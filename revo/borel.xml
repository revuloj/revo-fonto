<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: borel.xml,v 1.9 2022/05/11 07:31:47 revo Exp $">
<kap>
<rad>Borel</rad>/o
</kap>
<drv mrk="borel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve:
      <nom>&Eacute;mile</nom> <trd lng="fr">Borel</trd>, 1871-1956, franca
      <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_r;&c_e;&c_l;&c_mol;</trd>
  <trd lng="cs">Borel</trd>
  <trd lng="de">&Eacute;mile <ind>Borel</ind></trd>
  <trd lng="en">Borel</trd>
  <trd lng="it">Borel</trd>
  <trd lng="nl">Emile <ind>Borel</ind></trd>
  <trd lng="pl">Borel</trd>
  <trd lng="ru">&c_B;&c_o;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">Borel</trd>
</drv>
<drv mrk="borel.b0a">
  <kap><tld lit="b"/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Nomita la&ubreve; <tld/>o:
      <ekz>
        <tld lit="b"/>a lemo estas grava rezulto pri diferencialaj ekvacioj en partaj
        deriva&jcirc;oj nomita pro <nom>&Eacute;mile Borel</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=257981"
              >Borela lemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="algebr.borelasigma0o"><tld lit="b"/>a &sigma;-algebro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_r;&c_e;&c_l;&c_je;&c_w;</trd>,
    <trd>&c_B;&c_a;&c_r;&c_e;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Borelsch</trd>
  <trd lng="en">Borel['s]</trd>
  <trd lng="fr">de <ind>Borel</ind></trd>
  <trd lng="it">di <ind>Borel</ind></trd>
  <trd lng="nl">van <ind>Borel</ind></trd>
  <trdgrp lng="pl">
    <trd>Borela</trd>,
    <trd>borelowski</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_je;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_B;&c_o;&c_r;&c_je;&c_l;&c_ja;</trd></trdgrp> 
</drv>
</art>

