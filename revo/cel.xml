<drv mrk="cel.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="cel.sen0a.KOMUNE">
    <dif>
      Senutila, vana:
      <ekz>
        sen<tld/>a diskutado;
      </ekz><ekz>
        sen<tld/>aj klopodoj;
      </ekz><ekz>
        muele sen<tld/>e
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz><ekz>
        esploradi la komparan valoron de &ccirc;iuj religioj <klr>[...]</klr>
        estus afero tute sen<tld/>a
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz><ekz>
        plej sen<tld/>a kaj senutila sensenca&jcirc;o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz><ekz>
        multe da sen<tld/>aj aferoj ekbrilis anta&ubreve; la mondo rapide, sed
        anka&ubreve; rapide falis
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_m;&c_e;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_c;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>
