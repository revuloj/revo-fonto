<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trafik.xml,v 1.23 2022/03/28 14:44:12 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>trafik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trafik.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Komerca survoja a&ubreve; surakva interkomunikado kaj
      inter&scirc;an&gcirc;o:
      <ekz>
        la &ccirc;iam kreskanta mar<tld/>o;
      </ekz>
      <ekz>
        havena <tld/>o;
      </ekz>
      <ekz>
        <ind>var<tld/>o</ind>;
        <trd lng="hu">&aacute;ruforgalom</trd>
        <trd lng="pl">transport</trd>
      </ekz>
      <ekz>
        fervoja person<tld/>o;
      </ekz>
      <ekz>
        <ind>transira <tld/>o</ind>
        <klr>(transito)</klr>.
        <trd lng="hu">&aacute;tmen&odblac; <ind>forgalom</ind></trd>
        <trd lng="pl">tranzyt</trd>
      </ekz>
    </dif>
    <trd lng="ca">tr&agrave;fic</trd>
    <trd lng="hu">forgalom</trd>    
    <trdgrp lng="pl">
      <trd>komunikacja</trd>,
      <trd>transport</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Cirkulado sur vojoj:
      <ekz>
        sur la stratoj regas vigla <tld/>o;
      </ekz>
      <ekz>
        turista <tld/>o;
      </ekz>
      <ekz>
        la urbestro ordonis la dekstraflankan <tld/>on por &ccirc;iuj
        veturiloj.
      </ekz>
    </dif>
    <trd lng="ca">tr&agrave;nsit</trd>
    <trd lng="hu">k&ouml;zleked&eacute;s</trd>    
    <trd lng="pl">ruch<klr> (drogowy)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_u;&c_v;&c_ja;&c_z;&c_mol;</trd>,
    <trd>&c_z;&c_n;&c_o;&c_s;&c_ib;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cestovn&iacute; ruch</trd>,
    <trd>doprava</trd>,
    <trd>frekvence</trd>,
    <trd>provoz</trd>,
    <trd>p&rcaron;eprava</trd>
  </trdgrp>
  <trd lng="de">Verkehr</trd>
  <trd lng="en">traffic</trd>
  <trdgrp lng="fr">
    <trd>circulation<klr> (trafic)</klr></trd>,
    <trd>trafic<klr> (circulation)</klr></trd>
  </trdgrp>
  <trd lng="id">lalu lintas</trd>
  <trd lng="nl">verkeer</trd>
  <trd lng="pt">tr&aacute;fego</trd>
  <trd lng="ro">circula&tcedil;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_o;&c_je; &c_i;
    &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_s;&c_o;&c_o;&c_b;&c_shch;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>,
    <trd>&c_t;&c_r;&c_a;&c_f;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>doprava</trd>,
    <trd>prem&aacute;vka</trd>,
    <trd>ruch</trd>
  </trdgrp>
  <trd lng="sv">trafik</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x443;&#x445; (&#x432;&#x443;&#x43B;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x434;&#x43E;&#x440;&#x43E;&#x436;&#x43D;&#x456;&#x439;)</trd>,
    <trd>&#x441;&#x43F;&#x43E;&#x43B;&#x443;&#x447;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x43D;&#x441;&#x43F;&#x43E;&#x440;&#x442;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x432;&#x435;&#x437;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x444;&#x456;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="trafik.0i">
  <kap><tld/>i</kap>
  <snc mrk="trafik.0i.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="vid" cel="cirkul.0i.TRA">Cirkuli</ref>,
      movi&gcirc;adi sur vojoj:
      <ekz>
        pro glacio la havenaj &scirc;ipetoj ne povis <tld/>i.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">transitar</trd>
  <trdgrp lng="cs">
    <trd>dopravovat</trd>,
    <trd>provozovat dopravu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>circuler<klr> (en v&eacute;hicule)</klr></trd>,
    <trd>se <ind>d&eacute;placer</ind><klr> (par un moyen de
    transport)</klr></trd>
  </trdgrp>
  <trd lng="id">ber<ind>lalu lintas</ind></trd>
  <trd lng="sk">dopravova&tcaron;</trd>
  <trd lng="uk">&#x446;&#x438;&#x440;&#x43A;&#x443;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="trafik.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="trafik.0ilo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="vid" cel="vetur.0ilo.TRA">Veturilo</ref>,
      <ref tip="vid" cel="port.trans0ilo.TRA">transportilo</ref>,
      ordinara rimedo por <tld/>i:
      <ekz>
        la plej oportuna <tld/>ilo por tia voja&gcirc;o estus
        aeroplano.
      </ekz>
    </dif>    
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="ca">mitj&agrave; de transport</trd>
  <trd lng="cs">dopravn&iacute; prost&rcaron;edek</trd>
  <trd lng="fr">moyen de transport</trd>
  <trd lng="hu">k&ouml;zleked&eacute;si eszk&ouml;z</trd>
  <trd lng="id"><ind>alat</ind> transportasi</trd>
  <trdgrp lng="pl">
    <trd>&sacute;rodek <ind>komunikacji</ind></trd>,
    <trd>&sacute;rodek <ind>transportu</ind></trd>
  </trdgrp>
  <trd lng="sk">dopravn&yacute; prostriedok</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x441;&#x456;&#x431; &#x441;&#x43F;&#x43E;&#x43B;&#x443;&#x447;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x43D;&#x441;&#x43F;&#x43E;&#x440;&#x442;&#x43D;&#x438;&#x439; &#x437;&#x430;&#x441;&#x456;&#x431;</trd>
  </trdgrp>
</drv>
</art>
