<art mrk="$Id: filodendr.xml,v 1.5 2022/05/21 08:37:43 revo Exp $">
<kap>
  <rad>filodendr</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="filodendr.0o">
  <kap><tld/>o</kap>
  <snc mrk="filodendr.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Vegeta&jcirc;o ofte kreskanta sur aliaj vegeta&jcirc;oj (epifita),
      kun grandaj folioj, ofte kun aeraj radikoj,
      el la tropikaj partoj de Ameriko;
      multspecia genro el familio araceoj
      (<trd lng="la">philodendron</trd>):
      <ekz>
        de la altaj folioj de ornama <tld/>o
        densaj gutoj iom post iom falis
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u &scirc;i mortu tra-fike?</vrk>, 
        <lok>1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_d;&c_e;&c_n;&c_d;&c_r;&c_a;&c_n;</trd>
  <trd lng="cs">filodenron</trd>
  <trd lng="en">philodendron</trd>
  <trd lng="fr">philodendron</trd>
  <trd lng="sk">filodendr&#xF3;n</trd>
  <trd lng="uk">&c_f;&c_ib;&#x43B;&#x43E;&c_d;&c_je;&#x43D;&c_d;&c_r;&#x43E;&#x43D;</trd>
</drv>
</art>

