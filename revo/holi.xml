<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: holi.xml,v 1.4 2023/03/26 13:28:56 afido Exp $">
<kap>
  <rad>Holi</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>

<drv mrk="holi.0o">
  <kap><tld/>o</kap>
  <snc mrk="holi.0o.HIN">
    <uzo tip="fak">HIN</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Hindua printempa <ref tip="lst" lst="voko:festoj" cel="fest.0o.KAL">festo</ref>
      precipe fama pro tio,
      ke homoj en publikaj lokoj iuj al aliaj &jcirc;etas akvon
      kaj kolorajn pulvorojn:
      <ekz>
        &ccirc;ar <tld/>o estas hindua festo, multaj islamanoj konsideras, ke festi &gcirc;in estas
        profana ago
        <fnt>
        <bib>Monato</bib>
          <aut>Nadipedia</aut>
          <vrk><url ref="&Monato;/2019/012464.php">Holio: printempa festo</url></vrk>
          <lok>Monato, 2019:06</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nouxruz.0o">No&ubreve;ruzo</ref>,
    </refgrp>
  </snc>
  <trd lng="de">Holi</trd>
  <trd lng="en">Holi</trd>
  <trd lng="fr">Holi</trd>
  <trd lng="it">Holi</trd>
</drv>
</art>

