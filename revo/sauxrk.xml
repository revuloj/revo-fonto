<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sa&ubreve;rkra&ubreve;t</rad>/o
</kap>
<drv mrk="sauxrk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentinta lamenigita <ref tip="super" cel="brasik.0o">brasiko</ref>:
      <ekz>
        acida brasiko, acidbrasiko a&ubreve; <tld/>o estas per laktoacida fermento konservita
        blanka brasiko, tiu konserva metodo venas ver&scirc;ajne el &Ccirc;inio, acida brasiko
        estas man&gcirc;ata kiel aldona nutra&jcirc;o &ccirc;e diversaj man&gcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31881"
              >Acida brasiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la uzo de citronaj likva&jcirc;oj, c-vitaminaj tablojdoj oni man&gcirc;is vintre
        C-vitaminri&ccirc;an <tld/>on en la plej nordaj landoj de E&ubreve;ropo, <nom>James Lind</nom>
        disvolvigis la teorion ke fruktoj de citrusoj kuracas skorbuton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32071"
              >Skorbuto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="brasik.acida0o">acida brasiko</ref>
    <bld
      lok="&WCU;/thumb/b/b5/Kiszona_kapusta.JPG/466px-Kiszona_kapusta.JPG"
      prm="CC-BY-SA-3.0"
      ><tld/>o kun iom da spicherboj
        <fnt>
          <aut>Alina Zienowicz Ala z, propra verko</aut>
          <lok><url ref="&WCW;/File:Kiszona_kapusta.JPG"
              >https://commons.wikimedia.org/wiki/File:Kiszona_kapusta.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_ja; 
       <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>, 
    <trd>&c_k;&c_v;&c_a;&c_sh;&c_a;&c_n;&c_a;&c_ja; 
       <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd> 
  </trdgrp>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_o; &c_z;&c_je;&c_l;&c_je;</trd>
  <trd lng="ca">xucrut</trd>
  <trd lng="de">Sauerkraut</trd>
  <trd lng="en">sauerkraut</trd>
  <trd lng="fr">choucroute</trd>
  <trd lng="he">&kaf;&resh;&vav;&bet; &kaf;&bet;&vav;&shin;</trd>
  <trd lng="hu">savany&uacute; <ind>k&aacute;poszta</ind></trd>
  <trd lng="it">crauti</trd>
  <trd lng="nl">zuurkool</trd>
  <trdgrp lng="pl">
    <trd>kiszona <ind>kapusta</ind></trd>,
    <trd>kwaszona <ind>kapusta</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_s;&c_l;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>,
    <trd>&c_k;&c_v;&c_a;&c_sh;&c_je;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  </trdgrp>
</drv>
</art>
