<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>violon</rad>/o
</kap>
<drv mrk="violon.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc>
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Kvarkorda ar&ccirc;a <ref tip="super"
        cel="instru2.kord0o">kordinstrumento</ref>:
        <ekz>
          ne el &ccirc;iu ligno oni faras <tld/>on
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          se vi prenis la <tld/>on, prenu anka&ubreve; la ar&ccirc;on
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_p;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Geige</trd>,
      <trd>Violine</trd>
    </trdgrp>
    <trd lng="el">&beta;&jota;&omikron;&lambda;&jota_ton;</trd>
    <trd lng="hu">heged&udblac;</trd>
    <trd lng="nl">viool</trd>
    <trd lng="pt">violino</trd>
    <trd lng="ru">&c_s;&c_k;&c_r;&c_i;&c_p;&c_k;&c_a;</trd>
    <trd lng="fr">violon</trd>
</drv>

<drv mrk="violon.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Ludi <tld/>on:
        <ekz>
          <uzo tip="stl">FIG</uzo>
          la a&ubreve;tuna vento <tld/>as tra la arboj.
        </ekz>
      </dif>
    </snc>
    <rim>
    &Ccirc;iuj verboj, derivitaj de nomoj de muzikiloj,
    konsekvence devas esti transitivaj, &ccirc;ar kiel rekta objekto
    servas ludata muzika&jcirc;o, ekz.: <tld/>i suiton. (Jurij Finkel)
    </rim>
    <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a;
      <ind>&c_s;&c_k;&c_r;&c_y;&c_p;&c_c;&c_y;</ind></trd>
    <trd lng="de">geigen</trd>
    <trd lng="fr">jouer du <ind>violon</ind></trd>
    <trd lng="hu">heged&uuml;l</trd>
    <trd lng="nl">viool spelen</trd>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; 
<ind>&c_s;&c_k;&c_r;&c_i;&c_p;&c_k;&c_je;</ind></trd>
</drv>
</art>
