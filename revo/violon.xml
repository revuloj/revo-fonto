<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>violon</rad>/o
</kap>
<drv mrk="violon.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kvarkorda ar&ccirc;a <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>:
      <ekz>ne el &ccirc;iu ligno oni faras
        <tld/>on
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>se vi prenis la <tld/>on, prenu anka&ubreve; la
        ar&ccirc;on
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Geige</trd>,
    <trd>Violine</trd>
  </trdgrp>
  <trd lng="hu">heged&udblac;</trd>
  <trd lng="nl">viool</trd>
</drv>

<drv mrk="violon.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ludi <tld/>on: (f)
      <ekz>
        la a&ubreve;tuna vento <tld/>as tra la arboj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">geigen</trd>
  <trdgrp lng="fr">
    <trd>violon</trd>
  </trdgrp>
  <trd lng="hu">heged&uuml;l</trd>
  <trd lng="nl">viool spelen</trd>
</drv>
</art>
