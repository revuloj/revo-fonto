    <trd>&#x43B;&#x456;&#x441;&#x43E;&#x43F;&#x438;&#x43B;&#x44C;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x456;&#x441;&#x43E;&#x43F;&#x438;&#x43B;&#x44C;&#x43D;&#x438;&#x439; &#x437;&#x430;&#x432;&#x43E;&#x434;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x92F8;&#x6728;&#x5EE0; <pr>j&#xF9;m&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#x952F;&#x6728;&#x5382; <pr>j&#xF9;m&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#x92F8;&#x6728;&#x5EE0; <pr>j&#x16B;m&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#x952F;&#x6728;&#x5382; <pr>j&#x16B;m&#xF9;ch&#x1CE;ng</pr></trd>
  </trdgrp>
