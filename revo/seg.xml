    <trd>&c_l;&c_ib;&c_s;&c_o;&c_p;&c_i;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_ib;&c_s;&c_o;&c_p;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37624;&#26408;&#24288; <pr>j&ugrave;m&ugrave;ch&#462;ng</pr></trd>,
    <trd>&#38191;&#26408;&#21378; <pr>j&ugrave;m&ugrave;ch&#462;ng</pr></trd>,
