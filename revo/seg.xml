<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>seg</rad>/i
</kap>
<drv mrk="seg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tran&ccirc;i malmolan materion per &scirc;tala klingo provizita per dentoj, kiun
      oni alterne anta&ubreve;en kaj malanta&ubreve;en sur&scirc;ovas.
    </dif>
  </snc>
  <trd lng="de">s&auml;gen</trd>
  <trd lng="en">saw</trd>
  <trd lng="nl">zagen</trd>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="seg.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo destinita por <tld/>i:
      <ekz>
        grizaj montoj desegnas &ccirc;e l' horizonto <tld/>ilformajn konturojn
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dent.0o">dento</ref>.
    </refgrp>
  </snc>
  <trd lng="de">S&auml;ge</trd>
  <trd lng="en">saw</trd>
  <trd lng="nl">zaag</trd>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_a;</trd>
</drv>
</art>
