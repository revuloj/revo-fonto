<drv mrk="seg.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="seg.0ajxo.TEK">
    <dif>
      <ref tip="super" cel="fal.de0ajxo">Defala&jcirc;o</ref> de <tld/>ado konsistanta el 
      <ref cel="lign.0ero">ligner</ref>etoj:
      <ekz>
        specialistoj komencis plenigi la internon de l' mortinto per forte odorantaj
        herboj, <tld/>a&jcirc;oj, kaj e&ccirc; ili ver&scirc;is tien odorajn arbajn sukojn,
        seninterrompe pre&gcirc;ante
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la cirko, priver&scirc;ita per lumo, ekde trapezoj, fiksitaj sub la 
        plafono <klr>...</klr> &gcirc;is fre&scirc;aj <tld/>a&jcirc;oj, kovrantaj 
        la arenon, &mdash; estis sub la potenco de la elektraj lustroj, 
        semantaj gajan ekstazon    
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rabot.0ajxo">rabota&jcirc;o</ref>,
      <ref cel="split.0o.ligna">splito<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_l;&c_a;&c_v;&c_ib;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">serradures</trd>
