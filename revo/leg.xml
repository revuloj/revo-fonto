  <trd lng="ru">&c_p;&c_r;&c_o;&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">cel&eacute; pre&ccaron;&iacute;ta&tcaron;</trd>
</drv>

<drv mrk="leg.vocx0i">
  <kap>vo&ccirc;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="leg.vocx0i.TIP">
    <dif>
      <tld lit="L"/>i <ref cel="vocx.0e">vo&ccirc;e</ref>,
      elparolante la legatan:
      <ekz>
        &scirc;i enver&scirc;is la teon, mem prezentis al li tason
        da &gcirc;i kaj poste prenis libron kaj la&ubreve;te
        vo&ccirc;<tld/>is
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        la vortoj elsaltas de sur la pa&gcirc;oj, tiel ke oni povas
        &gcirc;oje ne nur <tld/>i sed anka&ubreve; vo&ccirc;<tld/>i ilin
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Ekvivas klasika&jcirc;o</vrk>,
          <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        kaj oni vo&ccirc;<tld/>is anta&ubreve; ili &ccirc;iujn
        vortojn de la libro de
        interligo, kiu estis trovita en la domo de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Kr2; 34:30</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_t;&c_a;&c_c;&c_mol; &c_u;&c_g;&c_o;&c_l;&c_a;&c_s;</trd>
  <trd lng="br">lenn a vouezh uhel</trd>
  <trdgrp lng="de">
    <trd>laut lesen</trd>,
    <trd>vorlesen</trd>
  </trdgrp>
  <trd lng="en">to read aloud</trd>
  <trdgrp lng="es">
    <trd>leer en voz alta</trd>,
    <trd>leer de viva voz</trd>
  </trdgrp>
  <trd lng="fr"><ind>lire</ind> &agrave; haute voix</trd>
  <trd lng="hu">felolvas</trd>  
  <trdgrp lng="it">
    <trd>leggere<klr> (a voce alta)</klr></trd>
  </trdgrp>
  <trd lng="nl">hardop <ind>lezen</ind></trd>
  <trd lng="pl"><ind>czyta&cacute;</ind> na g&lstroke;os</trd>
  <trd lng="pt">ler em voz alta</trd>
  <trd lng="ro">citeste cu voce tare</trd>
  <trd lng="ru">&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol; <ind>&c_v;&c_s;&c_l;&c_u;&c_h;</ind></trd>
  <trd lng="uk">&#x447;&#x438;&#x442;&#x430;&#x442;&#x438; &#x432;&#x433;&#x43E;&#x43B;&#x43E;&#x441;</trd>
</drv>

<drv mrk="leg.0opova">
  <kap><tld/>opova,
  <var><kap><tld/>ipova</kap></var></kap>
  <snc mrk="leg.0opova.TIP">
  <uzo tip="fak">TIP</uzo>
    <dif>
      Kapabla <tld/>i:
      <ekz>
        oni ne uzu la vorton <ctl>alfabetigado</ctl> en la 
        &gcirc;enerala senco de <tld/>opovigo, &ccirc;ar 
        multaj popoloj estas <tld/>opovaj sen alfabeta 
        skribsistemo kaj ne deziras <ctl>alfabetigon</ctl>!
        <fnt>
          <aut>E. Grimley Evans</aut>,
          <vrk><url ref="https://www.esperanto.be/fel/mon/rec/dmpo.html">Utilega
          konsultlibro</url></vrk>
          <bib>Monato</bib>,
          <lok>1995:2, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas kaj sa&gcirc;a kaj &ccirc;arma, kaj krome
        iom <tld/>opova
        <fnt>
          <aut>Bakin, trad. Vejdo</aut>,
          <vrk><url ref="http://www.elerno.net/elibro/familio26.htm">La
          Familio</url></vrk>
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>Semajna Resumo</ctl> elstarigas, ke <ctl>legado kaj
        skribado estas nemalhaveblaj por inkludo de individuo en la
        <tld/>ipovan socion</ctl>
        <fnt>
          <aut>J. de Paiva Netto </aut>
          <vrk><url ref="https://www.paivanetto.com/eo/homaj-rajtoj/rasismo-socia-kancero">Rasismo
          &ndash; socia kancero</url></vrk>
          <lok>[vidita en 2019]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trd lng="de">lesef&auml;hig</trd>
  <trd lng="en">literate</trd>
  <trdgrp lng="es">
    <trd>capaz de <ind>leer</ind> y escribir</trd>,
    <trd>no analfabeto</trd>
  </trdgrp>
  <trd lng="fr">alphab&eacute;tis&eacute;</trd>
  <trdgrp lng="id">
    <trd><ind>melek</ind> aksara</trd>,
    <trd><ind>melek</ind> huruf</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>capace di <ind>leggere</ind> e scrivere</trd>,
    <trd>alfabetizzato</trd>
  </trdgrp>
  <trd lng="pl">pi&sacute;mienny</trd>
  <trd lng="pt">alfabetizado<klr tip="ind"> (algu&eacute;m)</klr></trd>
  <trdgrp lng="ro">
    <trd>scris</trd>,
    <trd>cu &#x219;tiin&#x21B;&abreve; de carte</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="leg.0opovo">
  <kap><tld/>opovo,
  <var><kap><tld/>ipovo</kap></var></kap>
  <snc mrk="leg.0opovo.TIP">
    <dif>
      Kapablo <tld/>i:
      <ekz>
        li multe kura&gcirc;igis <tld/>opovon <klr>[&#x2026;]</klr>
        <fnt>
          <aut>R. Brisebois</aut>,
          <vrk><url ref="http://www.geocities.com/hilelista_komunumo/hilelisto31.htm">
          John Wesley</url></vrk>,
          <lok>Hilelisto, 2003:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        en indi&gcirc;enaj komunumoj de meksiko, dulingvaj instruistoj notas, 
        ke lernantoj kiuj komencas la elementan lernejadon en la 
        gepatra lingvo akiras <tld/>ipovon pli rapide
        <fnt>
          <aut>A Giridhar RAO</aut>
          <vrk><url ref="http://www.ipernity.com/blog/giridhar/93260">
          Kial lingvoj gravas - nova libreto</url></vrk>
          <lok>2008-09-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Lesef&auml;higkeit</trd>
  <trd lng="en">literacy</trd>
  <trd lng="es">alfabetizado</trd>
  <trd lng="fr">alphab&eacute;tisme</trd>
  <trd lng="id">literasi</trd>
  <trdgrp lng="it">
    <trd>saper <ind>leggere</ind> e scrivere</trd>,
    <trd>essere alfabetizzato</trd>
  </trdgrp>
  <trd lng="pl">alfabetyzm</trd>     
  <trd lng="ro">alfabetizare</trd>
</drv>

<drv mrk="leg.0opovigo">
  <kap><tld/>opovigo</kap>
  <snc mrk="leg.0opovigo.TIP">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Ago instrui al iu <tld/>i (kaj, kutime, anka&ubreve; skribi):
      <ekz>
        <klr>[...]</klr> postulis ke oni mobilizu por <tld/>opovigo
        <klr>[...]</klr>
        <fnt>
          <aut>M. Smith, trad. R. Dumain</aut>
          <vrk><url ref="http://www.autodidactproject.org/my/SEU.html">
          Sociaj Aspektoj de la Esperanto-movado...</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_r;&c_a;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="en">teaching of literacy</trd>
  <trd lng="es">alfabetizaci&oacute;n</trd>
  <trd lng="fr">alphab&eacute;tisation</trd>
  <trd lng="it">alfabetizzazione</trd>
  <trd lng="pl">alfabetyzacja</trd>
  <trd lng="pt">alfabetiza&ccedil;&atilde;o<klr tip="ind">
  (de algu&eacute;m)</klr></trd>
  <trd lng="ro">alfabetizare</trd>
</drv>

<drv mrk="leg.ne0opova">
  <kap>ne<tld/>opova,
  <var><kap>ne<tld/>ipova</kap></var></kap>
  <snc mrk="leg.ne0opova.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Kiu ne povas <tld/>i, ne konas literojn;
      <ref tip="sin" cel="analfa.0a">analfabeta</ref>:
      <ekz>
        la ne<tld/>opova popolo <klr>[&#x2026;]</klr> blinde
        obeis al lokaj oficistoj kaj bienposedantoj
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">Kristanismo
         kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>;
      </ekz>
      <ekz>
        bezonas ili ne komfortigan brakon, sed platon da man&gcirc;a&jcirc;o kaj 
        akvimunajn &scirc;uojn tiel ke ili povas frekventi lernejon; multaj 
        estas ne<tld/>ipovaj kaj anka&ubreve; siajn infanojn ne povos 
        sendi al lernejo 
        <fnt>
          <aut>E. de Groen</aut>
          <vrk><url ref="http://www.elerno.cn/elibro/Kroniko%20de%20Balkanio.pdf">Kiel
          ka&scirc;i la fra&ubreve;don?</url></vrk>
          <lok>Balkana Verda Stelo, 2017-03-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="en">
    <trd>illiterate</trd>,
    <trd>analphabet</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>analfabeto</trd>,
    <trd>iletrado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>analphab&egrave;te</trd>,
    <trd>illettr&eacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>buta</ind> huruf</trd>,
    <trd>tunaaksara</trd>
  </trdgrp>
  <trd lng="pl">niepi&sacute;mienny</trd>     
  <trd lng="ro">analfabet</trd>
</drv>
