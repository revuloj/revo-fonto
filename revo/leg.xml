<drv mrk="leg.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="leg.tra0i.TIP">
    <dif>
      <tld lit="L"/>i de la komenco &gcirc;is la fino:
      <ekz>
        &ccirc;iu, anta&ubreve; ol komenci la <tld/>adon de la
        esperanta literaturo,
        tra<tld/>u atente la Fundamentan Krestomation
        <fnt>
          <aut>&Z;</aut>:
          <vrk>Anta&ubreve;parolo al la unua eldono
          de la Fundamenta Krestomatio</vrk>.</fnt>;
      </ekz>
      <ekz>
        kiam la re&gcirc;o de <nom>Izrael</nom> tra<tld/>is la leteron, li
        dis&scirc;iris siajn vestojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 5:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne povis tra<tld/>i la surskribon, nek klarigi al la re&gcirc;o
        &gcirc;ian signifon
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 5:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen mi tra<tld/>os al vi leteron, kiun mi ricevis
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tre dankas ilin kaj tute aparte la plej akrevidan
        erar&ccirc;asiston, s-ron <nom>Edmund Grimley Evans</nom>, kiu
        tra<tld/>is la tutan tekston
        <fnt>
          <aut>H. Laine</aut>
          <vrk>Teknikaj notoj pri prezentado de
          &leftquot;Proverbaro Esperanta&rightquot;</vrk>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_ch;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">lenn penn-da-benn</trd>
  <trdgrp lng="cs">
    <trd>pro&ccaron;&iacute;st</trd>,
    <trd>p&rcaron;e&ccaron;&iacute;st</trd>
  </trdgrp>
  <trd lng="de">durchlesen</trd>
  <trd lng="en">to read through</trd>
  <trdgrp lng="es">
    <trd>leer de principio a fin</trd>,
    <trd>leer<klr> de cabo a rabo</klr></trd>,
    <trd>leer<klr> de cabo a cabo</klr></trd>
  </trdgrp>
  <trd lng="fr">lire d'un bout &agrave; l'autre</trd>
  <trd lng="hu">v&eacute;gigolvas</trd>
  <trd lng="nl">doorlezen</trd>
  <trdgrp lng="pl">
    <trd>przeczyta&cacute;</trd>,
    <trd>czyta&cacute; od deski do deski<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>lectura</trd>,
    <trd>citi&#x21B;i din copert&abreve; &icirc;n copert&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
