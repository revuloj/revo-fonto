<art mrk="$Id: brigx.xml,v 1.13 2023/08/31 08:48:53 revo Exp $">
<kap><rad>bri&gcirc;</rad>/o</kap>
<drv mrk="brigx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kart.0ludo">Kartludo</ref> por kvar
      personoj, dividitaj en du duopojn, ludata per 52 kartoj:
      <ekz>
        kvar ludantoj de <tld/>o: la maldekstra ludanto estas la deklarinto, lia
        partnero trans la tablo malka&scirc;is siajn kartojn sur la tablo kiel la &leftquot;fantomo&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47332">Bri&gcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_r;&c_y;&c_d;&c_zh;</trd>
  <trd lng="ca">bridge</trd>
  <trd lng="de">Bridge</trd>
  <trd lng="en">bridge</trd>
  <trd lng="fr">bridge</trd>
  <trd lng="nl">bridge</trd>
  <trd lng="hu">bridzs</trd>
  <trd lng="pl">bryd&zdot;</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_d;&c_zh;</trd>
  </snc>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D9;&#x5D3;&#x5D2;'</trd>
  <trd lng="ja">&#x30D6;&#x30EA;&#x30C3;&#x30B8;</trd>
  <trd lng="uk">&#x431;&#x440;&#x438;&#x434;&#x436;</trd>
</drv>

<drv mrk="brigx.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="person.0o.homo">Persono</ref>
      iom serioze interesita en <tld/>o, regule ludanta
      <tld/>on:
      <ekz>
        <nom>Andrzej Wilkosz</nom> &ndash; pola <tld/>isto, Monda &Ccirc;efmajstro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=577527">Andrzej Wilkosz</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_r;&c_y;&c_d;&c_zh;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">jugador de bridge</trd>
  <trd lng="de">Bridgespieler</trd>
  <trd lng="en">bridge player</trd>
  <trd lng="fr">joueur de bridge</trd>
  <trd lng="hu">bridzsj&aacute;t&eacute;kos</trd>
  <trd lng="nl">bridgespeler</trd>
  <trd lng="pl">bryd&zdot;ysta</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_d;&c_zh;&c_i;&c_s;&c_t;</trd>
  </snc>
  <trd lng="he">&#x5E9;&#x5D7;&#x5E7;&#x5DF; &#x5D1;&#x5E8;&#x5D9;&#x5D3;&#x5D2;'</trd>
</drv>
</art>
