<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: legiti.xml,v 1.31 2023/09/03 18:27:46 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>legitim</rad>/i <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="legiti.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="legiti.0i.JUR_iun">
    <dif>
      Pruvi per dokumentoj la identecon, la memecon de iu; la&ubreve;rajtigi:
      <ekz>
        sin <tld/>i per pasporto;
      </ekz>
      <ekz>
        la kunvenintoj <tld/>is sin la&ubreve; siaj familioj, la&ubreve; siaj
        patrodomoj, la&ubreve; la
        nombroj de la nomoj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 1:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> a&ccirc;etis la valora&jcirc;ojn surbaze de dokumentoj
        originintaj en Libano
        kaj la&ubreve;&scirc;ajne <tld/>antaj posedorajton
        <fnt>
          <aut>Karlo Juh&aacute;sz</aut>
            <vrk><url ref="&Monato;/2000/007077.php">Servico de Sevso</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 12</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="de">ausweisen</trd>
    <trdgrp lng="hu">
      <trd>igazol</trd>,
      <trd>tan&uacute;s&iacute;t</trd>,
      <trd>legitim&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">legitimeren</trd>
    <trd lng="pl">legitymowa&cacute;</trd>
  </snc>
  <snc mrk="legiti.0i.JUR_ion">
    <dif>
      Konfirmi a&ubreve;tentikecon de dokumento:
      <ekz>
        <tld/>i subskribon, akton.
      </ekz>
    </dif>
    <trd lng="de">beglaubigen</trd>
    <trd lng="hu">hiteles&iacute;t</trd>
    <trd lng="nl">echt verklaren</trd>
    <trd lng="pl">autoryzowa&cacute;</trd>
  </snc>
  <snc mrk="legiti.0i.laulegxigi">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="legx.laux0igi">La&ubreve;le&gcirc;igi</ref>:
      <ekz>
        rasismo estas la &gcirc;enerala kaj definitiva taksado de realaj a&ubreve;
        imagaj diferencoj,
        favore al la akuzanto kaj dama&gcirc;e al la viktimo, cele <tld/>i
        agreson a&ubreve;
        privilegiojn
        <fnt>
	<aut>Jorge Camacho</aut>
	<vrk><url ref="&Monato;/2000/006810.php">Diskutige kaj diskutinde</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
    <!-- ne traduku evitindajn -->
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>legitimovat</trd>,
    <trd>ov&ecaron;&rcaron;it</trd>,
    <trd>zjistit toto&zcaron;nost</trd>
  </trdgrp>
  <trd lng="el">&ny;&omikron;&my;&jota;&my;&omikron;&pi;&omikron;&jota;&omega_ton;</trd>
  <trd lng="en">legitimize</trd>
  <trd lng="fr">l&eacute;gitimer</trd>
  <trdgrp lng="ja">
    <trd>&#xFF5E;&#x306E;&#x8EAB;&#20803;&#12434;&#x8A3C;&#x660E;&#12377;&#x308B; 
    [&#xFF5E;<ind>&#x306E;&#x307F;&#12418;&#12392;&#12434;&#12375;&#12423;&#12358;&#12417;&#12356;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x8A8D;&#x77E5;&#12377;&#x308B;  [<ind>&#x306B;&#12435;&#12385;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x672C;&#29289;&#12391;&#12354;&#x308B;&#12392;&#x8A3C;&#x660E;&#12377;&#x308B; 
    [<ind>&#x307B;&#12435;&#12418;&#x306E;&#12391;&#12354;&#x308B;&#12392;&#12375;&#12423;&#12358;&#12417;&#12356;&#12377;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="pt">legitimar</trd>
  <trd lng="ro">justifica</trd>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>legitimova&tcaron;</trd>,
    <trd>preuk&aacute;za&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_z;&c_a;&#x43A;&#x43E;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_z;&c_a;&#x43A;&#x43E;&#x43D;&#x43D;&c_u; &c_s;&c_i;&#x43B;&c_u;
    (&c_d;&#x43E;&#x43A;&c_u;&#x43C;&c_je;&#x43D;&c_t;&c_u;)</trd>,
    <trd>&c_v;&c_i;&c_z;&#x43D;&c_a;&c_v;&c_a;&c_t;&c_i; &c_z;&c_a;&#x43A;&#x43E;&#x43D;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&#x43B;&c_je;&c_gu;&c_ib;&c_t;&c_i;&#x43C;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="legiti.0a">
  <kap><tld/>a</kap>
  <snc mrk="legiti.0a.aktive">
    <dif>
      Rajtiga.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_u;&c_z;&c_a;&c_k;&c_o;&c_n;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="legiti.0a.pasive">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <refgrp tip="dif">
          <ref cel="legx.laux0a">La&ubreve;le&gcirc;a</ref>,
          <ref cel="legxitim.0a">le&gcirc;itima</ref>:
      </refgrp>
      <ekz>
        <tld/>aj rimedoj
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>;
      </ekz>
      <ekz>
        la jugoslava armeo ne plu havis <tld/>an estron kaj estis
        kaperita de la
        beograda serba regantaro
        <fnt><aut>Michel Duc Goninaz</aut>,
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: kiu kiun mistifikas?</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili e&ccirc; neas la <tld/>econ de Israelo kiel suverena juda &scirc;tato
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008494.php">Veneno
          antisemita</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_g;&c_ib;&c_t;&c_y;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>legitimn&iacute;</trd>,
    <trd>opr&aacute;vn&ecaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beglaubigt</trd>,
    <trd>ausgewiesen</trd>,
  </trdgrp>
  <trd lng="el">&ny;&omikron_ton;&my;&jota;&my;&omikron;&sigma_fina;</trd>
  <trd lng="fr">l&eacute;gitime</trd>
  <trdgrp lng="hu">
     <trd>legitim</trd>,
     <trd>jogszer&udblac;</trd>,
     <trd>hiteles</trd>,
     <trd>t&ouml;rv&eacute;nyes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x7D71;&#x306A;  [<ind>&#x305B;&#12356;&#12392;&#12358;&#x306A;</ind>]</trd>,
    <trd>&#x6CD5;&#x306E;&#x8A8D;&#12417;&#x308B;  [<ind>&#x307B;&#12358;&#x306E;&#x307F;&#12392;&#12417;&#x308B;</ind>]</trd>,
    <trd>&#x5AE1;&#x51FA;&#x306E;  [<ind>&#12385;&#12419;&#x304F;&#12375;&#12421;&#12388;&#x306E;</ind>]</trd>,
    <trd>&#x672C;&#29289;&#x306E;  [<ind>&#x307B;&#12435;&#12418;&#x306E;&#x306E;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>legitiem</trd>,
    <trd>wettig</trd>
  </trdgrp>
  <trd lng="pl">prawowity</trd>
  <trd lng="pt">leg&iacute;timo</trd>
  <trd lng="ro">legitim</trd>
  <trdgrp lng="sk">
    <trd>legit&iacute;mny</trd>,
    <trd>z&aacute;konit&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="legiti.0ajxo">
  <kap><tld/>a&jcirc;o,
  <var><kap><tld/>ilo</kap></var></kap>
  <snc mrk="legiti.0ajxo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Dokumento a&ubreve; alia atestilo, kiu <tld/>as:
      <ekz>
        poseda&jcirc;oj <klr>[&#8230;]</klr> rapide vendeblaj per
        falsaj <tld/>a&jcirc;oj
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 6a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu, kiu posedas &scirc;parlibreton kaj volas enpagi iun sumon, devos
        montri
        <tld/>a&jcirc;on al la bankoficisto
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006922.php">&Scirc;parlibretoj: fino por la anonimeco</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; sia <tld/>ilo <nom>Zhao Bochuan</nom> naski&gcirc;is en 1874
        <fnt>
          <aut>Sabeo Fu</aut>
            <vrk><url ref="&Monato;/2000/007127.php">Escepto-recepto</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        la persona <tld/>ilo certe estas &ccirc;e li &#8208; se okazis
        akcidento, oni nepre trovos &gcirc;in kaj sciigos min
        <fnt>
          <aut>J. Modest</aut>,
          <vrk><url ref="">
          Averto pri murdo</url></vrk>,
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ausweis</trd>,
      <trd>Beglaubigung</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>,
    <trd>&c_s;&c_e;&c_r;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>papiers<klr tip="amb"> (d'identit&eacute; etc.)</klr></trd>,
    <trd>pi&egrave;ces justificatives</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8EAB;&#20998;&#x8A3C;&#x660E;&#x66F8;  [<ind>&#x307F;&#12406;&#12435;&#12375;&#12423;&#12358;&#12417;&#12356;&#12375;&#12423;</ind>]</trd>,
    <trd>&#x8EAB;&#20803;&#x8A3C;&#x660E;&#x66F8;  [<ind>&#x307F;&#12418;&#12392;&#12375;&#12423;&#12358;&#12417;&#12356;&#12375;&#12423;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">legitimatiebewijs</trd>
  <trdgrp lng="pl">
    <trd>dow&oacute;d</trd>,
    <trd>legitymacja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>carte de identitate</trd>,
    <trd>card</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&c_s;&c_v;&c_ib;&c_d;&c_ch;&c_je;&#x43D;&#x43D;&#x44F; (&#x43E;&c_s;&#x43E;&c_b;&c_i;)</trd>
</drv>

<drv mrk="legiti.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="legiti.0igxi.JUR">
    <dif>
      Pruvi sian rajton, sian funkcion:
      <ekz>
        kvara gasto <klr>[&#8230;]</klr> <tld/>i&gcirc;is &ccirc;e la mastro kiel
        kriminala policisto el Parizo
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>ausweisen</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_g;&c_ib;&c_t;&c_y;&c_m;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_mol;&c_v;&c_ja;&c_r;&c_d;&c_zh;&c_a;&c_c;&c_mol; &c_s;&c_v;&c_a;&c_je; <ind>&c_p;&c_a;&c_w;&c_n;&c_a;&c_m;&c_o;&c_c;&c_t;&c_v;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se faire <ind>agr&eacute;er</ind> comme</trd>,
    <trd>prouver son identit&eacute;</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>legitimeren</ind></trd>
  <trdgrp lng="pl">
    <trd>legalizowa&cacute;</trd>,
    <trd>uprawomocni&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>valida</trd>,
    <trd>legaliza</trd>
  </trdgrp>
</drv>
</art>
