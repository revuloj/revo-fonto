<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>komplet</rad>/a
</kap>
<drv mrk="komple2.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizas ion, al kiu mankas neniu el la eroj, kiuj devas &gcirc;in 
      konsistigi:
      <ekz><tld/>a teleraro, traduko, kolekto, vortaro;</ekz>
      <ekz>ne<tld/>a objekto<fnt>Z</fnt>,</ekz>
      <ekz>ka&ubreve;zo<fnt>Z</fnt>;</ekz>
      (f) <ekz>
        ili beda&ubreve;rinde akceptis en la familion <tld/>an 
        vagiston<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plen.0a">plena 2</ref>,
      <ref cel="tut.0a">tuta</ref>,
      <ref cel="mank.sen0a">senmanka</ref>,
      <ref cel="perfek.0a">perfekta</ref>.
    </refgrp>
  </snc>
  <snc mrk="komple2.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      (p. p. <ref cel="spac.metrika0o">metrika spaco</ref>) Tia, ke &ccirc;iuj
      &gcirc;iaj <ref cel="vic.kosxia0o">ko&scirc;iaj vicoj</ref>
      konver&gcirc;as en &gcirc;i.
    </dif>
  </snc>
  <trd lng="fr">complet</trd>
  <trdgrp lng="nl">
    <trd>compleet</trd>,
    <trd>volledig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="komple2.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Io <tld/>a:
      <ekz><tld/>o da vestoj<fnt>Z</fnt>;</ekz>
      <ekz><tld/>o da mebloj;</ekz>
      <ekz>sin prezenti en plena <tld/>o<fnt>Z</fnt>.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <tld lit="K"/>o da vestoj:
      <ekz>li surhavis belan <tld/>on el blua drapo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>complet</trd>,
    <trd>pak</trd>,
    <trd>volledig <ind>stel</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
</drv>
<drv mrk="komple2.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de io <tld/>a:
      <ekz>prezenti ion en plena <tld/>eco.</ekz>
    </dif>
  </snc>
  <trd lng="nl">volledigheid</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="komple2.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>a:
      <ekz><tld/>igi siajn sciojn<fnt>Z</fnt>,</ekz>
      <ekz>kolekton, verkon;</ekz>
      <ekz>la ora senfino de la maro <tld/>igis tiun grandiozan pejza&gcirc;on;</ekz>
      <ekz><tld/>iga kurso <klr>(por <tld/>igi la instruon)</klr>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>aanvullen</trd>,
    <trd>vervolledigen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_o;&c_l;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_o;&c_l;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
