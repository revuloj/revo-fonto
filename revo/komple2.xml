<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>komplet</rad>/a
</kap>
<drv mrk="komple2.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizas ion, al kiu mankas neniu el la eroj, kiuj devas &gcirc;in
      konsistigi:
      <ekz>
        <tld/>a teleraro, traduko, kolekto, vortaro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili beda&ubreve;rinde akceptis en la familion <tld/>an
        vagiston<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plen.0a.kompleta">plena</ref>,
      <ref cel="tut.0a">tuta</ref>,
      <ref cel="mank.sen0a">senmanka</ref>,
      <ref cel="perfek.0a">perfekta</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>vollst&auml;ndig</trd>,
      <trd>vollz&auml;hlig</trd>,
      <trd>vollkommen</trd>,
      <trd>komplett</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>complet<klr tip="ind"> (adj.)</klr></trd>,
      <trd>absolu<klr tip="ind"> (ironiquement)</klr></trd>,
      <trd>achev&eacute;</trd>,
      <trd>consomm&eacute;<klr tip="ind"> (ironiquement)</klr></trd>,
      <trd>entier<klr tip="ind"> (adj.)</klr></trd>,
      <trd>int&eacute;gral</trd>,
      <trd>parfait<klr tip="ind"> (ironiquement)</klr></trd>,
      <trd>pur<klr tip="ind"> (ironiquement)</klr></trd>,
      <trd>plein</trd>,
      <trd>rempli</trd>,
      <trd>total<klr tip="ind"> (ironiquement)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="komple2.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="komple2.0a.spaco">
      <fnt><bib>EKV</bib><lok>&para;209</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="spac.metrika0o">metrika spaco</ref>)</klr>
        Tia, ke &ccirc;iuj &gcirc;iaj
        <ref cel="vic.kosxia0o">ko&scirc;iaj vicoj</ref>
        konver&gcirc;as en &gcirc;i.
      </dif>
      <trd lng="de">vollst&auml;ndig<klr tip="ind"> (-er Raum)</klr></trd>
      <trd lng="en">complete<klr tip="ind"> (space)</klr></trd>
      <trd lng="fr"><klr tip="ind">(espace) </klr>complet</trd>
      <trd lng="pl"><klr tip="amb">(przestrze&nacute;) </klr>zupe&lstroke;na</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </subsnc>
    <subsnc mrk="komple2.0a.latiso">
      <dif>
        <klr>(p.p. <ref cel="latis.0o.MAT">latiso</ref>)</klr>
        Tia, ke ajna subaro de &gcirc;i (ne nur duelementa) akceptas
        <ref cel="infim.0o">infimon</ref> kaj
        <ref cel="suprem.0o">supremon</ref>.
      </dif>
      <trd lng="de">vollst&auml;ndig<klr tip="ind"> (-er Verband)</klr></trd>
      <trd lng="en">complete<klr tip="ind"> (lattice)</klr></trd>
      <trd lng="fr"><klr tip="ind">(treillis) </klr>complet</trd>
      <trd lng="pl"><klr tip="amb">(krata) </klr>zupe&lstroke;na</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="komple2.0a.grafeo">
      <fnt><bib>KompLeks</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Tia, ke du ajnaj verticoj de &gcirc;i &ccirc;iam estas
        <ref cel="najbar.0a.grafee">najbaraj</ref>.
      </dif>
      <trd lng="de">vollst&auml;ndig<klr tip="ind"> (-er Graph)</klr></trd>
      <trd lng="en">complete<klr tip="ind"> (graph)</klr></trd>
      <trd lng="fr"><klr tip="ind">(graphe) </klr>complet</trd>
      <trd lng="pl"><klr tip="ind">(graf) </klr>pe&lstroke;ny</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_g;&c_r;&c_a;&c_f;)</klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="nl">
    <trd>compleet</trd>,
    <trd>volledig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="komple2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        <tld/>o da vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o da mebloj;
      </ekz>
      <ekz>
        sin prezenti <ind>en plena <tld/>o</ind><fnt>Z</fnt>.
        <trd lng="fr">au grand <ind>complet</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>jeu<klr tip="ind"> (d'objets assortis)</klr></trd>,
      <trd>assortiment<klr tip="ind"> (d'objets assortis)</klr></trd>,
      <trd>ensemble<klr tip="ind"> (d'objets assortis)</klr></trd>,
      <trd>lot<klr tip="ind"> (d'objets assortis)</klr></trd>,
      <trd>set<klr tip="ind"> (de table)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="komple2.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="komple2.vesto0o">vesto<tld/>o</ref>
    <ekz>
      li surhavis belan <tld/>on el blua drapo.
    </ekz>
    <trd lng="fr">complet<klr tip="ind"> (costume)</klr></trd>
  </snc>
  <trdgrp lng="nl">
    <trd>complet</trd>,
    <trd>pak</trd>,
    <trd>volledig <ind>stel</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
</drv>

<drv mrk="komple2.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de io <tld/>a:
      <ekz>
        prezenti ion <ind>en plena <tld/>eco</ind>.
        <trdgrp lng="fr">
          <trd>en <ind>totalit&eacute;</ind></trd>,
          <trd>en <ind>int&eacute;gralit&eacute;</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">volledigheid</trd>
  <trdgrp lng="fr">
    <trd>compl&eacute;tude</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="komple2.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>a:
      <ekz>
        <tld/>igi siajn sciojn<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kolekton, verkon;
      </ekz>
      <ekz>
        la ora senfino de la maro <tld/>igis tiun grandiozan pejza&gcirc;on;
      </ekz>
      <ekz>
        <ind><tld/>iga</ind> kurso <klr>(por <tld/>igi la instruon)</klr>.
        <trdgrp lng="fr">
          <trd>compl&eacute;mentaire<klr tip="ind"> (additionnel)</klr></trd>,
          <trd>additionnel</trd>,
          <trd>suppl&eacute;mentaire</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>compl&eacute;ter</trd>,
    <trd>parachever</trd>,
    <trd>parfaire</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanvullen</trd>,
    <trd>vervolledigen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_o;&c_l;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_o;&c_l;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="komple2.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <ref tip="dif" cel="komple2.ne0a">ne<tld/>a</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>incomplet</trd>,
    <trd>fragmentaire</trd>,
    <trd>imparfait<klr tip="ind"> (incomplet)</klr></trd>,
    <trd>inachev&eacute;</trd>,
    <trd>partiel</trd>
  </trdgrp>
</drv>

<drv mrk="komple2.ne0a">
  <kap>ne<tld/>a</kap>
  <snc>
    <ref tip="ant" cel="komple2.0a"><tld/>a</ref>
    <ekz>
      ne<tld/>a objekto<fnt>Z</fnt>,
    </ekz>
    <ekz>
      ka&ubreve;zo<fnt>Z</fnt>;
    </ekz>
    <ekz>
      <klr>[tia talento]</klr> en la plej bona okazo estas kapabla
      krei nur objektojn kriplajn, &hcirc;aosajn kaj ne<tld/>ajn
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>incomplet</trd>,
    <trd>fragmentaire</trd>,
    <trd>imparfait<klr tip="ind"> (incomplet)</klr></trd>,
    <trd>inachev&eacute;</trd>,
    <trd>partiel</trd>
  </trdgrp>
</drv>

<drv mrk="komple2.vesto0o">
  <kap>vesto<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o da vestoj:
      <ekz>
        li surhavis multekostan grizan vesto<tld/>on, kun kiu
        kolore akordis liaj eksterlandaj &scirc;uoj
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="komple2.0o.TEKS"><tld/>o <sncref/></ref>
  </snc>
  <trd lng="fr">complet<klr tip="ind"> (costume)</klr></trd>
</drv>
</art>
