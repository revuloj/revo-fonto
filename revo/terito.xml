<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>teritori</rad>/o
</kap>
<drv mrk="terito.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Difinita lando konsiderata kiel submetata al iu a&ubreve;toritato:
      <ekz>
        la <tld/>o de la urbo;
      </ekz>
      <ekz>
        la Turkoj invadis a&ubreve;stran <tld/>on;
      </ekz>
      <ekz>
        sur la tuta <tld/>o de la palaco malaperis la ordo<fnt>B</fnt>;
      </ekz>
      <ekz>
        privilegio pri ekster<tld/>eco
        <klr>(koncesiita al ambasadorejoj)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="teren.0o">tereno</ref>,
      <ref cel="region.0o">regiono</ref>,
      <ref cel="distri1.0o">distrikto</ref>,
      <ref cel="provin.0o">provinco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Territorium</trd>
  <trd lng="es">territorio</trd>
  <trdgrp lng="nl">
    <trd>grondgebied</trd>,
    <trd>territorium</trd>
  </trdgrp>
  <trd lng="pl">terytorium</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_r;&c_i;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
