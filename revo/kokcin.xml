<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kokcin.xml,v 1.27 2023/12/03 09:58:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kokcinel</rad>/o
</kap>
<drv mrk="kokcin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kokcin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="skarab.mari0o.ZOO">Mariskarabo</ref>:
      <ekz>
        malgranda makulkolora <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; sciencaj studoj uzataj de la germana registaro
        <klr>[&#x2026;]</klr> la enplantita
        geno povas dama&gcirc;i ne nur la malbonan tineon, sed anka&ubreve; mortigi
        larvojn de la
        &ccirc;armaj kaj utilaj <tld/>oj, same abelojn kaj organismojn en
        akvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009818.php">Mirakloj mankas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_w;&c_k;&c_a;-&c_b;&c_a;&c_g;&c_o;&c_w;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_i;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>buoc'h-Doue</trd>,
    <trd>c'hwilig-Doue</trd>,
    <trd>yarig-Doue</trd>
  </trdgrp>
  <trd lng="cs">slun&eacute;&ccaron;ko</trd>
  <trd lng="de">Marienk&auml;fer <klr tip="ind">(Gattung)</klr></trd>
  <trdgrp lng="en">
    <trd>ladybird</trd>,
    <trd>ladybird beetle</trd>,
    <trd>lady beetle</trd>,
    <trd>ladybug</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mariquita</trd>,
    <trd>vaquita de San Antonio</trd>,
    <trd>vaquita de San Ant&oacute;n</trd>,
    <trd>chinita</trd>,
    <trd>mariquitilla</trd>,
    <trd>sarantont&oacute;n</trd>,
    <trd>catarina</trd>,
    <trd>San Antonio</trd>
  </trdgrp>
  <trd lng="fr">coccinelle</trd>
  <trd lng="hu">katicabog&aacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#x30CA;&#x30CA;&#x30DB;&#x30B7;&#x30C6;&#x30F3;&#x30C8;&#x30A6;</trd>,
    <trd>&#x30C6;&#x30F3;&#x30C8;&#x30A6;&#x30E0;&#x30B7;</trd>
  </trdgrp>
  <trd lng="nl">lieveheersbeestje</trd>
  <trdgrp lng="pl">
    <trd>biedronka</trd>,
    <trd>bo&zdot;a kr&oacute;wka</trd>
  </trdgrp>   
  <trd lng="ro">buburuz&abreve;</trd>
  <trd lng="ru">&c_b;&c_o;&c_zh;&c_mol;&c_ja; &c_k;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">lienka</trd>
  <trd lng="uk">&#x441;&#x43E;&#x43D;&#x435;&#x447;&#x43A;&#x43E;</trd>
  <trd lng="zh">&#x74E2;&#x866B; <pr>pi&#xE1;och&#xF3;ng</pr></trd>
</drv>
</art>
