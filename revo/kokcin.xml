<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kokcinel</rad>/o
</kap>
<drv mrk="kokcin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kokcin.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Coccinella</trd>), koleoptero, kun duonsfera korpo
      kaj beletaj rondaj makuletoj sur &gcirc;ia kiraso, nutrantanta sin i.a. 
      de pedikoj.
    </dif>
  </snc>
  <trd lng="de">Marienk&auml;fer <klr tip="ind">(Gattung)</klr></trd>
  <trdgrp lng="en">
    <trd>ladybird</trd>,
    <trd>ladybird beetle</trd>,
    <trd>lady beetle</trd>,
    <trd>ladybug</trd>
  </trdgrp>
  <trd lng="fr">coccinelle</trd>
</drv>

</art>
