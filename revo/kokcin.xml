<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kokcinel</rad>/o
</kap>
<drv mrk="kokcin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kokcin.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Coccinella</trd>), koleoptero, kun duonsfera korpo
      kaj beletaj rondaj makuletoj sur &gcirc;ia kiraso, nutrantanta sin i.a. 
      de pedikoj.
    </dif>
  </snc>
  <trd lng="de">Marienk&auml;fer <klr tip="ind">(Gattung)</klr></trd>
  <trd lng="fr">coccinelle</trd>
</drv>
<drv mrk="kokcin.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Coccinellidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Marienk&auml;fer <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
