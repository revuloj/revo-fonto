<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: toluen.xml,v 1.14 2023/09/04 19:01:52 revo Exp $">
<kap>
  <rad>toluen</rad>/o
</kap>
<drv mrk="toluen.0o">
  <kap><tld/>o
    <fnt><bib>PIV1</bib></fnt>
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>6</sub>H<sub>5</sub>CH<sub>3</sub></frm>;
      likva senkolora <ref tip="super" cel="hidrok.0o">hidrokarbido</ref>,
      bolanta &ccirc;e temperaturo de 111&#x202F;&#x2DA;C kaj uzata ekz. kiel 
      <ref cel="solv.0ilo">solvilo</ref>, 
      <ref cel="reakci.0ilo">reakciilo</ref> kaj kiel konsista
      parto de iuj benzinoj:
      <ekz>
        <tld/>o, <klr>[&#x2026;]</klr> formas 15&ndash;20&#x202F;% de la malpeza oleo, 
        oni povas produkti &gcirc;in el terkarba gudro, sed plej ofte oni
        produktas per katalitika reformado de krudbezino, oni uzas 
        &gcirc;in por produktado de trinitro-toluolo (TNT), benzoeacido,
        sa&hcirc;arino, koloriga&jcirc;oj, fotokemia&jcirc;oj kaj medikamentoj, &gcirc;i
        estas uzata anka&ubreve; kiel <klr>[&#x2026;]</klr> frot-malpliiga aldona&jcirc;o &ccirc;e aviadiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=136055">Tolueno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/e/e6/Toluen.svg" prm="PD" tip="img" lrg="77">strukturformulo de <tld/>o
        <fnt>
          <aut>Wesalius, propra verko</aut>
          <lok><url ref="&WCW;/File:Toluen.svg">&WCW;/File:Toluen.svg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_u;&c_o;&c_l;</trd>
  <trd lng="bg">&c_t;&c_o;&c_l;&c_u;&c_je;&c_n;</trd>
  <trd lng="ca">tolu&egrave;</trd>
  <trd lng="cs">toluen</trd>
  <trdgrp lng="de">
    <trd>Toluen</trd>,
    <trd>Toluol</trd>
  </trdgrp>
  <trd lng="en">toluene</trd>
  <trd lng="es">tolueno</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_w;&a_l;&a_w;&a_y_hamza;&a_n;</trd>,
    <trd>&a_m;&a_t;&f_y;&a_l; &a_b;&a_n;&a_z;&a_w;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">tolu&egrave;ne</trd>
  <trd lng="hu">toluol</trd>
  <trd lng="ja">&#x30C8;&#x30EB;&#x30A8;&#x30F3;</trd>
  <trd lng="nl">tolueen</trd>
  <trd lng="pl">toluen</trd>
  <trd lng="pt">tolueno</trd>
  <trd lng="sk">tolu&eacute;n</trd>
  <trd lng="sv">toluen</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43E;&#x43B;&#x443;&#x435;&#x43D;</trd>,
    <trd>&#x442;&#x43E;&#x43B;&#x443;&#x43E;&#x43B;</trd>
  </trdgrp>
</drv>
</art>

