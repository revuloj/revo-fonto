<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>toluen</rad>/o
</kap>
<drv mrk="toluen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      C<sub>6</sub>H<sub>5</sub>CH<sub>3</sub>.
      Likva senkolora <ref tip="super" cel="hidrok.0o">hidrokarbido</ref>,
      bolanta en temperaturo de 111&ring;C. Uzata kiel 
      <ref cel="solv.0ilo">solvilo</ref>, 
      <ref cel="reakci.0ilo">reakciilo</ref> kaj kiel konsista
      parto de iuj benzinoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_u;&c_o;&c_l;</trd>
  <trd lng="fr">tolu&egrave;ne</trd>
</drv>
</art>

