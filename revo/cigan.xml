<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cigan.xml,v 1.30 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>cigan</rad>/o
</kap>
<drv mrk="cigan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo el vaganta gento devena el Hindujo, konata en E&ubreve;ropo
      sub
      diversaj nomoj:
      <ekz>
        en Litovio <tld/>oj aperis meze de la 15a
        jarcento<fnt><aut>Last</aut>: <vrk>Ne estas malbonaj
        popoloj</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 12a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="roma.0o">romao</ref>
  <trd lng="be">&c_c;&c_y;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="br">
    <trd>Tsigan</trd>,
    <trd>Jipsian</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gitano</trd>,
    <trd>z&iacute;ngar<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="de">Zigeuner</trd>
  <trdgrp lng="el">
    <trd>&tau;&sigma;&ypsilon;&gamma;&gamma;&alfa_ton;&ny;&omikron;&sigma_fina;</trd>,
    <trd>&gamma;&ypsilon_ton;&phi;&tau;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gypsy</trd>,
    <trd>romani</trd>,
    <trd>romany</trd>
  </trdgrp>
  <trd lng="es">gitano<klr> (subst.)</klr></trd>
  <trdgrp lng="fr">
    <trd>tsigane<klr> (subst.)</klr></trd>,
    <trd>tzigane<klr> (subst.)</klr></trd>,
    <trd>gitan</trd>,
    <trd>manouche</trd>
  </trdgrp>
  <trd lng="hu">cig&aacute;ny</trd>
  <trdgrp lng="it">
    <trd>zingaro<klr> (sost.)</klr></trd>,
    <trd>gitano<klr> (sost.)</klr></trd>,
    <trd>rom</trd>
  </trdgrp>
  <trd lng="nl">zigeuner</trd>
  <trd lng="pl">cygan</trd>
  <trd lng="pt">cigano</trd>
  <trd lng="ru">&c_c;&c_y;&c_g;&c_a;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>Cik&aacute;n</trd>,
    <trd>R&oacute;m</trd>
  </trdgrp>
  <trd lng="he">&#x5E6;&#x5D5;&#x5E2;&#x5E0;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30D7;&#x30B7;&#x30FC;</trd>,
    <trd>&#x30ED;&#x30DE;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>Cig&aacute;n</trd>,
    <trd>R&oacute;m</trd>
  </trdgrp>
  <trd lng="uk">&#x446;&#x438;&#x433;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x5409;&#x666E;&#x8CFD;&#x4EBA; <pr>j&#xED;p&#x1D4;s&#xE0;ir&#xE9;n</pr></trd>,
    <trd>&#x5409;&#x535C;&#x8CFD;&#x4EBA; <pr>j&#xED;b&#x1D4;s&#xE0;ir&#xE9;n</pr></trd>,
    <trd>&#x5409;&#x666E;&#x8CFD; <pr>j&#xED;p&#x1D4;s&#xE0;i</pr></trd>,
    <trd>&#x8328;&#x5188;&#x4EBA; <pr>c&#xED;g&#x101;ngr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cigan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>oj a&ubreve; al ili rilata:
      <ekz>
        la opereto [....] <ctl><tld lit="C"/>a barono</ctl>
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>Jubileo de
        valsre&gcirc;o</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 22a</lok>.</fnt>
      </ekz>
    </dif>
  <trd lng="be">&c_c;&c_y;&c_g;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="br">tsigan</trd>
  <trdgrp lng="ca">
    <trd>gitano</trd>,
    <trd>z&iacute;ngar<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="de">zigeunerisch</trd>
  <trdgrp lng="en">
    <trd>gypsy</trd>,
    <trd>romani</trd>,
    <trd>romany</trd>
  </trdgrp>
  <trd lng="es">gitano<klr> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>tsigane<klr> (adj.)</klr></trd>,
    <trd>tzigane<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">cig&aacute;ny</trd>
  <trdgrp lng="it">
    <trd>zingaro<klr> (agg.)</klr></trd>,
    <trd>gitano<klr> (agg.)</klr></trd>,
    <trd>zingaresco</trd>
  </trdgrp>
  <trd lng="nl">zigeuner-</trd>
  <trd lng="pl">cyga&nacute;ski</trd>
  <trd lng="ru">&c_c;&c_y;&c_g;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="cs">cik&aacute;nsk&yacute;</trd>
  <trd lng="he">&#x5E6;&#x5D5;&#x5E2;&#x5E0;&#x5D9;</trd>
  <trd lng="sk">cig&aacute;nsky</trd>
  <trd lng="uk">&#x446;&#x438;&#x433;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
</drv>
</art>
