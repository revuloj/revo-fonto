<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: siliciat.xml,v 1.1 2024/12/11 22:31:39 afido Exp $">
<kap>
    <rad>siliciat</rad>/o <fnt><bib>PIV2</bib></fnt>,
    <var><kap><rad var="k">silikat</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="siliciat.0o">
  <kap><tld/>o,
  <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="siliciat.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kombina&jcirc;o de <ref tip="vid" cel="silici.0o">silicio</ref> kun
      <ref tip="vid" cel="oksige.0o">oksigeno</ref>
      <frm>(Si<sub>x</sub>O<sub>y</sub>)</frm>:
      <ekz>
        stilbito estas speco de <tld/>o de natrio kaj kalcio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=493142">Axel
          Fredrik Cronstedt</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fakto, ke la oceano havas rokan fundon, implicas, ke la akvo estas en kontakto kun <tld
        var="k"/>oj
        <fnt>
          <aut>R. Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011292.php">
          Oceano, kaj eble vivo, sur saturna satelito</url></vrk>
          <lok>Monato, 2014/07, p. 21a </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_l;&c_ib;&c_k;&c_a;&c_t;</trd>
  <trd lng="cs">k&rcaron;emi&ccaron;itan</trd>
  <trdgrp lng="de">
    <trd>Silikat</trd>,
    <trd>Silicat<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="fr">silicate</trd>
  <trd lng="id">silikat</trd>
  <trdgrp lng="pl">
    <trd>krzemian</trd>,
    <trd>sylilkat</trd>,
    <trd>tlenek krzemu</trd>
  </trdgrp>
  <trd lng="sk">kremi&ccaron;itan</trd>
  <trd lng="sv">silikat</trd>
  <trd lng="uk">&c_s;&c_i;&#x43B;&c_ib;&#x43A;&c_a;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#30789;&#37240;&#x76D0; <pr>gu&#x12B;su&amacron;ny&#xE1;n</pr></trd>,
    <trd>&#30789;&#37240;&#x9E7D; <pr>gu&#x12B;su&amacron;ny&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

