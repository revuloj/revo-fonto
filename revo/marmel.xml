<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>marmelad</rad>/o
</kap>
<drv mrk="marmel.0o">
  <kap><tld/>o</kap>
  <snc mrk="marmel.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Speco de konfita&jcirc;o el fruktoj kuiritaj kun sukero sufi&ccirc;e longe, por
      ke ili konsistigu densan ka&ccirc;on.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_m;&c_e;&c_l;&c_ja;&c_d;</trd>
  <trd lng="de">Marmelade</trd>
  <trdgrp lng="en">
    <trd>marmelade</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mermelada</trd>,
    <trd>jalea</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiture<klr> (marmelade)</klr></trd>,
    <trd>marmelade</trd>
  </trdgrp>
  <trd lng="hu">lekv&aacute;r</trd>
  <trdgrp lng="nl">
    <trd>marmelade</trd>,
    <trd>moes</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trd lng="pt">gel&eacute;ia de frutas</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_m;&c_je;&c_l;&c_a;&c_d;</trd>
  <trd lng="sv">marmelad</trd>
  <trd lng="tr">re&ccedil;el</trd>
</drv>
</art>
