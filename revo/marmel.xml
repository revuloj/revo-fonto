<kap>
  <ofc>1</ofc>
  <rad>marmelad</rad>/o
</kap>
<drv mrk="marmel.0o">
  <kap><tld/>o</kap>
  <snc mrk="marmel.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="konfit.0ajxo">Konfita&jcirc;o</ref>
      el fruktoj kuiritaj kun sukero sufi&ccirc;e longe,
      por ke ili konsistigu densan ka&ccirc;on:
      <ekz>
        la tajloreto, prenis panon el la &scirc;ranko, tran&ccirc;is pecon tra la
        tuta bulo kaj sur&scirc;miris la <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        frukta&jcirc;oj, kiel ekzemple fruktoj sekigitaj, fruktaj gelatenoj,
        <tld/>oj, konfita&jcirc;oj, fruktaj sukoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_m;&c_e;&c_l;&c_ja;&c_d;</trd>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_m;&c_a;&c_l;&c_a;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>melmelada</trd>,
    <trd>compota</trd>,
    <trd>confitura</trd>
  </trdgrp>
  <trd lng="cs">marmel&aacute;da</trd>
  <trd lng="de">Marmelade</trd>
  <trdgrp lng="en">
    <trd>marmelade</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mermelada</trd>,
    <trd>jalea</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_r;&a_b;&a_A;</trd>,
    <trd>&a_m;&a_A;&a_r;&a_m;&a_A;&a_l;&a_A;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiture<klr> (marmelade)</klr></trd>,
    <trd>marmelade</trd>
  </trdgrp>
  <trd lng="hu">lekv&aacute;r</trd>
  <trd lng="id">selai</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30FC;&#x30DE;&#x30EC;&#x30FC;&#x30C9;</trd>,
    <trd>&#x30B8;&#x30E3;&#x30E0;  <pr>&#x3058;&#x3083;&#x3080;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>marmelade</trd>,
    <trd>moes</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trd lng="pl">marmolada</trd>
  <trd lng="pt">gel&eacute;ia de frutas</trd>
  <trd lng="ro">marmelad&abreve;</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_m;&c_je;&c_l;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>d&zcaron;em</trd>,
    <trd>lekv&aacute;r</trd>
  </trdgrp>
  <trd lng="sv">marmelad</trd>
  <trd lng="tr">re&ccedil;el</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x440;&#x43C;&#x435;&#x43B;&#x430;&#x434;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x679C;&#x9171; <pr>gu&#x1D2;ji&#xE0;ng</pr></trd>,
    <trd>&#x679C;&#x91AC; <pr>gu&#x1D2;ji&#xE0;ng</pr></trd>,
    <trd>&#x679C;&#x5B50;&#x9171; <pr>gu&#x1D2;ziji&#xE0;ng</pr></trd>,
    <trd>&#x679C;&#x5B50;&#x91AC; <pr>gu&#x1D2;ziji&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
