<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>marmelad</rad>/o
</kap>
<drv mrk="marmel.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Speco de konfita&jcirc;o el fruktoj kuiritaj kun sukero sufi&ccirc;e longe, por
      ke ili konsistigu densan ka&ccirc;on.
    </dif>
  </snc>
  <trd lng="en">marmelade</trd>
  <trdgrp lng="es">
    <trd>mermelada</trd>,
    <trd>jalea</trd>
  </trdgrp>
  <trd lng="fr">marmelade</trd>
  <trdgrp lng="nl">
    <trd>marmelade</trd>,
    <trd>moes</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_m;&c_je;&c_l;&c_a;&c_d;</trd>
  <trd lng="sv">marmelad</trd>
  <trd lng="tr">re&ccedil;el</trd>
</drv>
</art>
