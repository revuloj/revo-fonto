<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zori.xml,v 1.14 2023/09/05 09:43:57 revo Exp $">
<kap>
  <rad>zori</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="zori.0o">
  <kap><tld/>o</kap>
  <snc mrk="zori.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="sandal.0o">Sandalo</ref>
      konsistanta el plandumo kaj nura v-forma rimeno,
      kies pinto restas inter la dika kaj la sekvaj pied-fingroj:
      <ekz>
        origine <tld/>oj estas la japanaj sandaloj, tradicie plektitaj el rizo-pajlo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50174">Zorio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">xancleta</trd>
  <trd lng="cs">japonsk&yacute; sand&aacute;l</trd>
  <trdgrp lng="de">
    <trd>Zehenstegsandale</trd>,
    <trd>Dianette</trd>,
    <trd>Flipflop</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>thong</trd>,
    <trd>flip-flop</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>chancla</trd>,
    <trd>chancleta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tong</trd>
  </trdgrp>
  <trd lng="hu">vietnami <ind>papucs</ind></trd>
  <trdgrp lng="it">
    <trd>infradito</trd>,
    <trd>zori<klr> (se alla giapponese)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x305E;&#x3046;&#x308A;</trd>
  <trd lng="ru">&c_v;&c_mol;&c_je;&c_t;&c_n;&c_a;&c_m;&c_k;&c_a;
<klr tip="amb"> (&c_o;&c_b;&c_u;&c_v;&c_mol;)</klr></trd>
  <trd lng="sk">japonsk&yacute; sand&aacute;l</trd>
  <trd lng="uk">&#x434;&#x437;&#x43E;&#x440;&#x456;</trd>
</drv>
</art>

