<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zori</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="zori.0o">
  <kap><tld/>o</kap>
  <snc mrk="zori.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sandal.0o">Sandalo</ref>
      konsistanta el plandumo kaj nura v-forma rimeno,
      kies pinto restas inter la dika
      kaj la sekvaj pied-fingroj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zehensandale</trd>,
    <trd>Dianette</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>thong</trd>,
    <trd>flip-flop</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tong</trd>
  </trdgrp>
  <trd lng="hu">vietnami <ind>papucs</ind></trd>
  <trd lng="ru">&c_v;&c_mol;&c_je;&c_t;&c_n;&c_a;&c_m;&c_k;&c_a;<klr tip="amb"> (&c_o;&c_b;&c_u;&c_v;&c_mol;)</klr></trd>
</drv>
</art>

