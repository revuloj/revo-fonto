<art mrk="$Id: zori.xml,v 1.17 2024/09/27 09:31:03 revo Exp $">
<kap>
  <rad>zori</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="zori.0o">
  <kap><tld/>o</kap>
  <snc mrk="zori.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="sandal.0o">Sandalo</ref>
      konsistanta el plandumo kaj nura v-forma rimeno,
      kies pinto restas inter la dika kaj la sekvaj pied-fingroj:
      <ekz>
        origine <tld/>oj estas la japanaj sandaloj, tradicie plektitaj el rizo-pajlo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50174">Zorio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&#8217;&c_je;&c_t;&c_n;&c_a;&c_m;&c_k;&c_a; <klr tip="amb">(&c_a;&c_b;&c_u;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="ca">xancleta</trd>
  <trd lng="cs">japonsk&yacute; sand&aacute;l</trd>
  <trdgrp lng="de">
    <trd>Zehenstegsandale</trd>,
    <trd>Dianette</trd>,
    <trd>Flipflop</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>thong</trd>,
    <trd>flip-flop</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>chancla</trd>,
    <trd>chancleta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tong</trd>
  </trdgrp>
  <trd lng="hu">vietnami <ind>papucs</ind></trd>
  <trdgrp lng="it">
    <trd>infradito</trd>,
    <trd>zori<klr> (se alla giapponese)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x305E;&#12358;&#x308A;</trd>
  <trd lng="pl">japonki<klr>  (sanda&lstroke;y)</klr></trd>
  <trd lng="ru">&c_v;&c_mol;&c_je;&c_t;&c_n;&c_a;&c_m;&c_k;&c_a;
<klr tip="amb"> (&c_o;&c_b;&c_u;&c_v;&c_mol;)</klr></trd>
  <trd lng="sk">japonsk&yacute; sand&aacute;l</trd>
  <trd lng="uk">&c_d;&c_z;&#x43E;&c_r;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EBA;&#x5B57;&#x62D6;&#x978B; <pr>r&#xE9;nz&#xEC;tu&#x14D;xi&#xE9;</pr></trd>,
    <trd>&#22841;&#x811A;&#x62D6;&#x978B; <pr>ji&#xE1;ji&#x1CE;otu&#x14D;xi&#xE9;</pr></trd>,
    <trd>&#x593E;&#33139;&#x62D6;&#x978B; <pr>ji&#xE1;ji&#x1CE;otu&#x14D;xi&#xE9;</pr></trd>,
    <trd>&#22841;&#x811A;&#x62D6; <pr>ji&amacron;ji&#x1CE;otu&#x14D;</pr></trd>,
    <trd>&#x593E;&#33139;&#x62D6; <pr>ji&amacron;ji&#x1CE;otu&#x14D;</pr></trd>,
    <trd>&#x4EBA;&#x5B57;&#x62D6; <pr>r&#xE9;nz&#xEC;tu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>

