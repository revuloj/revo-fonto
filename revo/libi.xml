<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Libi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="libi.L0o">
  <kap>
    <ofc>8</ofc><tld lit="L"/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Libio">Libio</url></lok></fnt>
  </kap>
  <snc mrk="libi.L0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la norda Afriko kun &ccirc;efurbo Tripolio.
      Teritorio de <em>Granda Araba Popola Socialisma &Jcirc;amahirio
      Libio</em>.
      ISO-3166-1 kodo: LY.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="egipt.E0ujo">Egiptujo</ref>,
      <ref tip="vid" cel="nigxeri.N0o.GEOG">Ni&gcirc;erio</ref>,
      <ref tip="vid" cel="sudan.S0o.GEOG">Sudano</ref>,
      <ref tip="vid" cel="tunizi.T0o.GEOG">Tunizio</ref>
      kaj <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteraneo</ref>.
    </dif>
    <trdgrp lng="ar">
      <trd>&a_b;&a_y;&a_t_marbuta;</trd>
      <trd>&a_A;&a_l;&a_j;&a_m;&a_A;&a_h;&a_y;&a_r;&a_y;&a_t_marbuta; &a_A;&a_l;&a_ayn;&a_r;&a_b;&a_y;&a_t_marbuta; &a_A;&a_l;&a_l;&a_y;&a_b;&a_y;&a_t_marbuta;
      &a_A;&a_l;&a_sh;&a_ayn;&a_b;&a_y;&a_t_marbuta; &a_A;&a_l;&a_A_hamza_sube;&a_sh;&a_t;&a_r;&a_A;&a_k;&a_y;&a_t_marbuta; &a_A;&a_l;&a_ayn;&a_Z;&a_m;&a_A_maqsura;</trd>
    </trdgrp>
    <trd lng="be">&c_L;&c_ib;&c_b;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_L;&c_i;&c_b;&c_i;&c_ja;</trd>
    <trd lng="de">Libyen</trd>
    <trd lng="en">Libya</trd>
    <trd lng="fr">Libye</trd>
    <trd lng="es">Libia</trd> 
    <trd lng="fa">&a_l;&f_y;&a_b;&f_y;</trd>
    <trd lng="hu">L&iacute;bia</trd>
    <trd lng="id">Libya</trd>
    <trd lng="ca">L&iacute;bia</trd>
    <trd lng="nl">Libi&euml;</trd>
    <trd lng="oc">Libia</trd>
    <trdgrp lng="pl">
      <trd>Libia</trd>,
      <trd>Wielka Arabska <ind>Libijska</ind> D&zdot;amahirijja 
      Ludowo-Socjalistyczna<klr> (nazwa oficjalna)</klr></trd>
    </trdgrp>
    <trd lng="pt">L&iacute;bia</trd>
  </snc>
</drv>

<drv mrk="libi.0ano">
  <kap><tld lit="l"/>ano</kap>
  <snc>
    <dif>
      Civitano de Libio
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Libien</trd>
    <trd>Libienne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="libi.0a">
  <kap><tld lit="l"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Libio
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>libien</trd>
    <trd>libienne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

