<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>past</rad>/o
</kap>
<drv mrk="past.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Faruno knedita kun akvo por fari panon, kukon ks. 
    </dif>
    <trd lng="ru">&c_t;&c_je;&c_s;&c_t;&c_o;</trd>
  </snc>
  <snc num="2">
    <dif>
      Mola knedebla substanco similanta tian <tld/>on.  
    </dif>
    <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_a;</trd>
  </snc>
  <trd lng="de">Teig</trd>
  <trd lng="fr">p&acirc;te</trd>
  <trd lng="nl">deeg</trd>
</drv>
<drv mrk="past.0ajxo">
  <kap><tld/>a&jcirc;o</kap> 
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif> 
      Sekigita lamenigita a&ubreve; fadenigita <tld/>o el faruno
      kaj ovoj, kiun oni kuiras en bolanta akvo.
    </dif>
    <refgrp tip="vid">
      <ref cel="vermic.0o">vermi&ccirc;elo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Teigware</trd>
  <trd lng="fr">p&acirc;te</trd>
  <trd lng="nl">pasta</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_p;&c_sh;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_m;&c_i;&c_sh;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="past.modlo0o">
  <kap>modlo<tld/>o, kned<tld/>o</kap> 
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Argilsimila diverskolora pasto, uzata por lude
      <ref tip="vid" cel="modl.0i">modli</ref>.
    </dif>
  </snc>
  <trd lng="en">plasticine</trd>
  <trdgrp lng="de">
    <trd>Plastilin</trd>,
    <trd>Knetmasse</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_l;&c_i;&c_n;</trd>
</drv>
</art>
