<drv mrk="past.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="past.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="prt" cel="farun.0o">Faruno</ref> <ref cel="kned.0i">knedi</ref>ta
      kun akvo a&ubreve; lakto ks kaj aliaj aldona&jcirc;oj por fari panon, kukon,
      nudelon a&ubreve; alian <tld/>a&jcirc;on:
      <ekz>
        fermentinta <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 13:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis <tld/>on, knedis &gcirc;in, preparis anta&ubreve; liaj okuloj, kaj bakis la kuketojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la servantinoj per siaj rulpremiloj rulpremis la <tld/>on tro maldike, duono de
        la macoj karbi&gcirc;is en la forno
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nac>kisjel</nac> (acida&jcirc;o), estas litva man&gcirc;a&jcirc;o, speco de gelato, preparata el avena
        <tld/>o, kiun oni lavas per akvo, &gcirc;is kiam &ccirc;iuj farun-partetoj estas forigitaj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        sukerita malt<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007144.html">Bierlando Belgio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o por la nuksa rulkuko pufi&gcirc;is en iu grandega ujo
        <fnt><aut>Spomenka &Scirc;timec</aut>,
          <vrk>Morto en Melburna tramo</vrk>,
          <bib>Monato</bib>, <lok>jaro 1997a, numero 8a, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;affroma&gcirc;aj nokoj (slovake <nac>bryndzov&eacute; halu&scaron;ky</nac>, faritaj el terpoma <tld/>o kaj
        kuiritaj en akvo, al kiuj oni kutime aldonas rostitajn lardopecojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2013/011027.php">Oro blanka</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la homoj estas kneditaj el unu <tld/>o <klr>[&#8230;]</klr> &ccirc;iu estas kapabla fari
        malsa&gcirc;a&jcirc;on
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        se oni amas la gaston, oni zorgas la <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_je;&c_s;&c_t;&c_a;</trd>
    <trd lng="de">Teig</trd>
    <trd lng="en">dough</trd>
    <trdgrp lng="es">
      <trd>masa <klr>alimenticia</klr></trd>,
      <trd>pasta</trd>
    </trdgrp>
    <trd lng="hu">t&eacute;szta</trd>
    <trd lng="nl">deeg</trd>
    <trdgrp lng="pt">
      <trd>massa <klr>aliment&iacute;cia</klr></trd>,
      <trd>pasta</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_je;&c_s;&c_t;&c_o;</trd>
  </snc>
  <snc mrk="past.0o.molajxo">
    <dif>
      Homogena mola knedebla substanco similanta tian <tld/>on:
      <ekz>
        tomat<tld/>o, ara&hcirc;ida <tld/>o, dento<tld/>o, modlo<tld/>o;
      </ekz>
      <ekz>
        &scirc;iaj trajtoj estas &scirc;miritaj per koloraj <tld/>oj
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        trulo <klr>[estas]</klr> &ccirc;erpilo kaj albatilo por mason<tld/>o
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        preni da estingita kalko pulvorigita <klr>[&#8230;]</klr>, da ovoblanko <klr>[&#8230;]</klr>
        kaj da aluno pulvorigita <klr>[&#8230;]</klr> miksi &gcirc;in, bone disfrotante, kaj
        ricevinte specon de nedensa <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kacx.0o">ka&ccirc;o</ref>,
      <ref cel="sxmir.0ajxo">&scirc;mira&jcirc;o</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Paste</trd>,
      <trd>Creme</trd>
    </trdgrp>
    <trd lng="en">paste</trd>
    <trd lng="es">pasta</trd>
    <trdgrp lng="hu">
      <trd>paszta</trd>,
      <trd>kr&eacute;m</trd>
    </trdgrp>
    <trd lng="nl">pasta</trd>
    <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_a;</trd>
  </snc>
  <trd lng="bo">&t_ga;&t_rata;&t_naro;&t_tsheg;&t_zha;&t_gigu;&t_ba;&t_tsheg;</trd>
  <trd lng="br">toaz</trd>
  <trd lng="ca">pasta</trd>
  <trdgrp lng="cs">
    <trd>pasta</trd>,
    <trd>t&ecaron;sto</trd>
  </trdgrp>
  <trd lng="es">pasta</trd>
  <trd lng="fr">p&acirc;te</trd>
  <trd lng="id"><ind>adon</ind>an</trd>
  <trdgrp lng="ja">
    <trd>&#12506;&#12540;&#12473;&#12488;</trd>,
    <trd>&#12397;&#12426;&#31881;  <pr>&#12397;&#12426;&#12371;&#12394;</pr></trd>,
    <trd>&#29983;&#22320;  <pr>&#12379;&#12356;&#12385;</pr></trd>,
    <trd>&#12506;&#12540;&#12473;&#12488;&#29366;&#12398;&#12418;&#12398;  <pr>&#12506;&#12540;&#12473;&#12488;&#12376;&#12423;&#12358;&#12398;&#12418;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">ciasto</trd>
  <trdgrp lng="pt">
    <trd>pasta</trd>,
    <trd>massa</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pr&abreve;jitur&abreve;</trd>,
    <trd>aluat</trd>
  </trdgrp>
  <trd lng="sk">cesto</trd>
  <trd lng="tok">ko</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_ib;&c_s;&c_t;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24425;&#27877; <pr>c&#462;in&iacute;</pr></trd>,
    <trd>&#27877; <pr>n&iacute;</pr></trd>,
    <trd>&#29983;&#38754;&#22242; <pr>sh&emacron;ngmi&agrave;ntu&aacute;n</pr></trd>,
    <trd>&#29983;&#40629;&#31984; <pr>sh&emacron;ngmi&agrave;ntu&aacute;n</pr></trd>,
    <trd>&#38754;&#22242; <pr>mi&agrave;ntu&aacute;n</pr></trd>,
    <trd>&#40629;&#22296; <pr>mi&agrave;ntu&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="past.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="past.0ajxo.cxia">
    <dif>
      Man&gcirc;a&jcirc;o el <tld/>o<sncref ref="past.0o.KUI"/>:
      <ekz>
        man&gcirc;u plej modere <klr>[&#8230;]</klr> terpomojn, <tld/>a&jcirc;ojn kaj rizon, preferante
        eventuale la integrajn
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="https://www.monato.be/2013/010934.php">Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malracia nutrado per nuraj <tld/>a&jcirc;oj kaj lakta&jcirc;oj ofte
        ka&ubreve;zas mankon de vitaminoj en la organismo de la
        lernejanoj;
      </ekz>
      <ekz>
        la rusa kuirarto preferas <tld/>a&jcirc;ojn bakitajn ol
        bolkuiritajn.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_u;&c_ch;&c_n;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
    <trd lng="en"><ind>bake</ind>d good</trd>
    <trd lng="es">plato de pasta</trd>
    <trd lng="hu">t&eacute;szta&eacute;tel</trd>
    <trd lng="nl">deegwaren</trd>
    <trd lng="ru">&c_m;&c_u;&c_ch;&c_n;&c_o;&c_je;</trd>
    <trd lng="zh">&#30333;&#26696; <pr>b&aacute;i&agrave;n</pr></trd>
</snc>
  <snc mrk="past.0ajxo.nudelo">
    <dif>
      <ref tip="dif" cel="nudel.0o.cxia">Nudelo<sncref/></ref>
      &ccirc;iaspeca:
      <ekz>
        ni man&gcirc;is en &ccirc;e&hcirc;a restoracio <tld/>a&jcirc;ojn
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj kun legomsa&ubreve;co
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="https://www.monato.be/2006/008903.php">Fornoj kaj formoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Evidente la <tld/>a&jcirc;o<sncref ref="past.0ajxo.nudelo"/>
      estas subnocio de <tld/>a&jcirc;o<sncref ref="past.0ajxo.cxia"/>;
      sed mi opinias stranga, senlogika kaj ne de &ccirc;iuj komprenata
      la implica&jcirc;on de PV kaj PIV, ke <tld/>a&jcirc;o estas <em>nur</em>
      io bolkuirita. En multaj lingvoj vorto strukture egala al
      &leftquot;<tld/>a&jcirc;o&rightquot; povas signifi kuketon ktp (interalie, e&ccirc; la
      itala <em>pasta</em> povas havi tiun sencon en la itala).
      Vd anka&ubreve; la noton pri <ref cel="vermicxel.0o.rimFund">vermi&ccirc;elo</ref><sncref ref="vermicxel.0o.cxia"/>.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_k;&c_a;&c_r;&c_o;&c_n;&c_n;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_t;&c_a;</trd>,
      <trd>&c_l;&c_o;&c_k;&c_sh;&c_y;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Pasta<klr> (Nudeln)</klr></trd>
    <trd lng="en">pasta</trd>
    <trd lng="es">pasta<klr> (italiana)</klr></trd>
    <trd lng="fr">p&acirc;te<klr> (pr&ecirc;te &agrave; cuire)</klr></trd>
    <trd lng="hu">sz&aacute;razt&eacute;szta</trd>
    <trd lng="id">pasta</trd>
    <trd lng="nl">pasta</trd>
    <trd lng="pl">makaron</trd>
    <trd lng="ro">paste</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_a;&c_p;&c_sh;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_k;&c_a;&c_r;&c_o;&c_n;&c_n;&c_y;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
<trd lng="de">Teigware</trd>
  <trdgrp lng="ja">
    <trd>&#12397;&#12426;&#29289;  <pr>&#12397;&#12426;&#12406;&#12388;</pr></trd>,
    <trd>&#12497;&#12473;&#12479;</trd>
  </trdgrp>
</drv>

<drv mrk="past.dento0o">
  <kap>dento<tld/>o</kap>
  <snc>
    <dif>
      Puriga <ref tip="super" cel="past.0o.molajxo"><tld/>o<sncref/></ref>,
      kiun oni &scirc;miras sur la dentojn por brosado:
      <ekz>
        &ccirc;iaspecaj varoj, de dent<tld/>o &gcirc;is dinamoma&scirc;ino
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        el&scirc;ovi dento<tld/>on sur broseton
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro proponas malhelpi dentan karion per bakteriofagoj
        <klr>[kiujn]</klr> oni enbu&scirc;igu en dent<tld/>o, pulvoro a&ubreve;
        ma&ccirc;gumo
        <fnt>
          <aut>Vladimir Lemelev</aut>,
          <vrk><url ref="&Monato;/2001/006431.html">Malamikoj de bakterioj</url></vrk>,
          <bib>Monato</bib>, <lok>jaro 2000a, numero 1a, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_a;&c_ja; &c_p;&c_a;&c_s;&c_t;&c_a;</trd>
  <trd lng="bg">&c_p;&c_a;&c_s;&c_t;&c_a; &c_z;&c_a; &c_z;&c_malmol;&c_b;&c_i;&c_t;&c_je;</trd>
  <trd lng="bo">&t_sa;&t_naro;&t_tsheg;&t_sa;&t_mata;&t_na;&t_tsheg;</trd>
  <trd lng="br">toaz-dent</trd>
  <trd lng="cs">pasta na zuby</trd>
  <trd lng="de">Zahnpasta</trd>
  <trd lng="en">toothpaste</trd>
  <trdgrp lng="es">
    <trd>pasta de dientes</trd>,
    <trd>dentr&iacute;fico</trd>
  </trdgrp>
  <trd lng="fr">dentifrice</trd>
  <trd lng="hu">fogkr&eacute;m</trd>
  <trd lng="id"><ind>pasta</ind> gigi</trd>
  <trd lng="ja">&#32244;&#12426;&#27503;&#30952;  <pr>&#12397;&#12426;&#12399;&#12415;&#12364;&#12365;</pr></trd>
  <trd lng="nl">tandpasta</trd>
  <trd lng="pl">pasta do z&eogonek;b&oacute;w</trd>
  <trd lng="ro">past&abreve; de din&tcomma;i</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_n;&c_a;&c_ja; &c_p;&c_a;&c_s;&c_t;&c_a;</trd>
  <trd lng="sk">pasta na zuby</trd>
  <trd lng="uk">&c_z;&c_u;&c_b;&c_n;&c_a; &c_p;&c_a;&c_s;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#29273;&#33167; <pr>y&aacute;g&amacron;o</pr></trd>,
    <trd>&#27905;&#40831;&#21058; <pr>ji&eacute;ch&#464;j&igrave;</pr></trd>,
    <trd>&#28500;&#40786;&#21137; <pr>ji&eacute;ch&#464;j&igrave;</pr></trd>
  </trdgrp>
</drv>

