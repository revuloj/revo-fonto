<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: heksagon.xml,v 1.7 2019/03/18 18:10:24 revo Exp $">
<kap>
<rad>heksagon</rad>/o
</kap>
<drv mrk="heksagon.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="dif" cel="later.n0o">Seslatero</ref>:
      <ekz>
        <tld/>a &scirc;ako de <nom>Glinski</nom>, unu el multaj variantoj de &scirc;ako
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4074">&Scirc;ako</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        per la menciita procedo analoge al diamanto oni povas produkti anka&ubreve; el la
        <tld/>a modifa&jcirc;o de bornitrido kuban bornitridon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18450">Diamanto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hexagon</trd>,
    <trd>pravideln&yacute; &scaron;esti&uacute;heln&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Hexagon</trd>
  <trd lng="fr">hexagone</trd>
  <trd lng="it">esagono</trd>
  <trd lng="pl">heksagon</trd>
  <trd lng="sk">pravideln&yacute; &scaron;es&tcaron;uholn&iacute;k</trd>
</drv>
</art>

