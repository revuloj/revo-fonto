<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hektometr</rad>/o
</kap>
<drv mrk="hektom.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo, egala
      al
      cent <ref tip="prt" cel="metr.0o.unuo">metroj</ref>; simb. hm.
    </dif>
    <refgrp tip="super">
      <ref cel="kilome.0o">kilometro</ref>,
    </refgrp>
    <refgrp tip="prt">
      <ref cel="dekame.0o">dekametro</ref>,
      <ref cel="metr.0o">metro</ref>,
      <ref cel="decime.0o">decimetro</ref>,
      <ref cel="centim1.0o">centimetro</ref>,
      <ref cel="milime.0o">milimetro</ref>
    </refgrp>
  </snc>                        
  <trd lng="be">&c_g;&c_je;&c_k;&c_t;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="br">hektometr(ad)</trd>
  <trd lng="de">Hektometer</trd>
  <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron_ton;&my;&epsilon;&tau;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>hectometer</trd>,
    <trd>hectometre</trd>
  </trdgrp>
  <trd lng="es">hect&oacute;metro</trd>
  <trd lng="fa">&a_h;&f_k;&a_t;&a_w;&a_m;&a_t;&a_r;</trd>
  <trd lng="fr">hectom&egrave;tre</trd>
  <trd lng="it">ettometro</trd>
  <trd lng="nl">hectometer</trd>
  <trd lng="pt">hect&ocirc;metro</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
