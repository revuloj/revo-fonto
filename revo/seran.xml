<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: seran.xml,v 1.3 2023/08/18 21:23:04 revo Exp $">
<kap>
  <rad>seran</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="seran.0o">
  <kap><tld/>o</kap>
  <snc mrk="seran.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda karnoman&gcirc;a marfi&scirc;o kun diversaj
      koloraj strioj a&ubreve; makuloj, amba&ubreve;seksa,
      vivanta precipe en Atlantiko kaj Mediteraneo;
      genro el familio seranedoj
      (<trd lng="la">Serranus</trd>):
      <ekz>
        ni man&gcirc;is &gcirc;issate salikokojn, <tld/>ojn, kaj pektenojn
        <fnt>
        <aut>Ken Miner</aut>,
        <vrk><url ref="http://www.airbits.com/~miner/TAGLIBRO_package/Junio_2006.html">
        taglibro</url></vrk>,
        <lok>2006-06-21</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>comber</trd>,
    <trd>serranus</trd>
  </trdgrp>
  <trd lng="fr">serran</trd>
  <trd lng="pl">strz&eogonek;piel  <klr>(ryba)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43C;&#8217;&#x44F;&#x43D;&c_i;&c_j; &#x43E;&#x43A;&c_u;&#x43D;&#x44C;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j; &#x43E;&#x43A;&c_u;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

