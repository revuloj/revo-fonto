    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>comber</trd>,
    <trd>serranus</trd>
  </trdgrp>
  <trd lng="fr">serran</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43C;&#x2019;&#x44F;&#x43D;&#x438;&#x439; &#x43E;&#x43A;&#x443;&#x43D;&#x44C;</trd>,
    <trd>&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43E;&#x43A;&#x443;&#x43D;&#x44C;</trd>
  </trdgrp>
