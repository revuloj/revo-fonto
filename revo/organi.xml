<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>organik</rad>/a
      <fnt>Z</fnt>
    </kap>
    <drv mrk="organi.0a">
      <kap><tld/>a</kap>
      <snc mrk="organi.0a.KEM">
        <uzo tip="fak">KEM</uzo>
        <dif>
          Rilata al
          <ref cel="karbon.0o">karbon</ref>kombina&jcirc;oj:
          <ekz>
            <ind><tld/>a &hcirc;emio</ind>;
            <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_h;&c_ib;&c_m;&c_ib;&c_ja;</ind></trd>
            <trd lng="hu">szerves k&eacute;mia</trd>
          </ekz><ekz>
            <ind><tld/>aj kombina&jcirc;oj</ind>.
            <trdgrp lng="be">
              <trd>&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;&c_ja; <ind>&c_r;&c_e;&c_ch;&c_y;&c_v;&c_y;</ind></trd>,
              <trd>&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;&c_ja; <ind>&c_z;&c_l;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ib;</ind></trd>
            </trdgrp>
            <trd lng="hu">szerves vegy&uuml;letek</trd>
          </ekz>
        </dif>
        <trd lng="de">organisch</trd>
      </snc>
      <snc mrk="organi.0a.GEOL">
        <uzo tip="fak">GEOL</uzo>
        <dif>
          Rilata al la substanco de korpoj de animaloj
          a&ubreve; vegeta&jcirc;oj:
          <ekz>
            <tld/>aj fosilioj.
          </ekz>
        </dif>
      </snc>
      <rim>
        Ne konfuzu la vortojn <tld/>a kaj
        <ref tip="vid" cel="organi1.0a">organisma</ref> a&ubreve;
        <ref tip="vid" cel="organ.0a">organa</ref>!
      </rim>
      <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
      <trdgrp lng="hu">
        <trd>szerves</trd>,
        <trd>organikus</trd>
      </trdgrp>
      <trd lng="nl">organisch</trd>
      <trd lng="pt">org&acirc;nico</trd>
      <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
  </art>
