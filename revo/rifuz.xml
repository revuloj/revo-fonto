<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>rifuz</rad>/i <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="rifuz.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="rifuz.0i.peton">
        <dif>
          Ne konsenti doni, disponigi a&ubreve; permesi ion petitan:
          <ekz>
            ne <tld/>u al mi anta&ubreve; ol mi mortos<fnt>Z</fnt>;
          </ekz><ekz>
            mi vokis kaj vi <tld/>is<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i al iu krediton<fnt>B</fnt>,
          </ekz><ekz>
            la obeon, siajn rajtojn;
          </ekz><ekz>
            <tld/>i peton<fnt>Z</fnt>,
          </ekz><ekz>
            al peto<fnt>B</fnt>;
          </ekz><ekz>
            <tld/>i humili&gcirc;i<fnt>Z</fnt>,
          </ekz><ekz>
            fari<fnt>B</fnt>;
          </ekz><ekz>
            la komitato <tld/>is akcepti la proponon<fnt>Z</fnt>;
          </ekz><ekz>
            ili <tld/>is la pridiskuton de la propono;
          </ekz><ekz>
            la interesoj de l' afero postulas lian <tld/>on fari tuj
            gravan
            vortaron<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="surd.0a">surda</ref>
        </refgrp>
        <trd lng="de">verweigern</trd>
        <trd lng="hu">megtagad</trd>    
      <trdgrp lng="ru">
        <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
      </snc>
      <snc mrk="rifuz.0i.peton.FIG">
        <uzo tip="stl">FIG</uzo>
        <uzo tip="klr">(io)</uzo>
        <dif>
          Ne kapabli, ne povi:
          <ekz>
            miaj piedoj <tld/>as plu min porti<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>verweigern</trd>,
          <trd>weigern</trd>
        </trdgrp>
        <trd lng="hu">nem <ind>engedelmeskedik</ind></trd>    
        <trdgrp lng="ru">
          <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr> (&c_ch;&c_t;&c_o;-&c_l;. &c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;)</klr></trd>,
          <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr> (&c_ch;&c_t;&c_o;-&c_l;. &c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;)</klr></trd>,
        </trdgrp>
      </snc>
      <snc mrk="rifuz.0i.proponon">
        <ref tip="dif" cel="akcept.mal0i.KOMUNE">Malakcepti</ref>
        <ekz>
          <tld/>i donacon, postenon
        </ekz>
        <rim>
          Tia uzo estas evitinda kaj konfuza, &ccirc;ar normale
          <tld/>as<sncref ref="rifuz.0i.peton"/> la petato, kaj
          malakceptas tiu, al kiu oni proponas; do
          <ekz>malakcepti proponitan cigaredon</ekz>,
          sed
          <ekz>
            <tld/>i<sncref ref="rifuz.0i.peton"/> cigaredon al
            almozulo.
          </ekz>
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trdgrp lng="de">
          <trd>verweigern</trd>,
          <trd>ablehnen</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>visszautas&iacute;t</trd>,
          <trd>elutas&iacute;t</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
          <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
        </trdgrp>
      </snc>
      <snc mrk="rifuz.0i.rezigni">
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="rezign.0i">Rezigni</ref>
        <ekz>
          la a&ubreve;toro <tld/>is &ccirc;iujn personajn rajtojn rilate tiun
          lingvon<fnt>Z</fnt>.
        </ekz>
      <trd lng="ru">&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_a;&c_d;&c_m;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="en">refuse</trd>
      <trd lng="fr">refuser</trd>
      <trdgrp lng="nl">
        <trd>weigeren</trd>,
        <trd>afwijzen</trd>
      </trdgrp>
      <trd lng="pt">recusar</trd>
    </drv>

    <drv mrk="rifuz.for0i">
      <kap>for<tld/>i, <var><kap><tld/>i&gcirc;i</kap></var></kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="rifuz.for0i.KOMUNE">
        <dif>
          Tute <ref tip="vid" cel="rezign.0i">rezigni</ref>:
          <ekz>
            <tld/>i&gcirc;i tiun filon<fnt>Z</fnt>;
          </ekz><ekz>
            for<tld/>i &ccirc;iujn profitojn<fnt>Z</fnt>;
          </ekz><ekz>
            forte mi dezirus for<tld/>i tiun tro turmentan
            rolon<fnt>Z</fnt>;
          </ekz><ekz>
            for<tld/>i la mondon kaj mona&hcirc;i&gcirc;i.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_v;&c_y;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>,
        <trd>&c_z;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>,
        <trd>&c_a;&c_d;&c_c;&c_u;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_v;&c_y;&c_c;&c_u;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="fr">renoncer &agrave;</trd>
      <trdgrp lng="hu">
        <trd>lemond <klr tip="ind">(vmir&odblac;l)</klr></trd>,
        <trd>eltasz&iacute;t <klr tip="ind">(mag&aacute;t&oacute;l)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>afstaan</trd>,
        <trd>verstoten</trd>,
        <trd>van de <ind>hand</ind> wijzen</trd>,
        <trd>opgeven</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_o;&c_t;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
    </drv>
  </art>
