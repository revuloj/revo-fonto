<drv mrk="incit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        ne <tld/>i&gcirc;u pro la malvirtuloj
        <fnt><bib>MT</bib><lok>&Psa; 37:1</lok></fnt>;
      </ekz>
      <ekz>
        kion do vi ankora&ubreve; diras? &dash; sufokate de
        <ind><tld/>i&gcirc;o</ind> Azazello preska&ubreve; siblis
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 19</lok></fnt>;
        <trdgrp lng="fr">
          <trd>agacement</trd>,
          <trd>&eacute;nervement</trd>,
          <trd>excitation<klr tip="ind"> (&eacute;tat)</klr></trd>,
          <trd>irritation<klr tip="ind"> (&eacute;tat de
          col&egrave;re)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        la tono de <tld/>i&gcirc;o estis sufi&ccirc;a indiko
        pri la animstato de la inspektoro
        <fnt><bib>UrdHadda</bib><lok>p. 8</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_z;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_z;&c_n;&c_ja; &c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>agacer</ind></trd>,
    <trd>s'<ind>&eacute;nerver</ind></trd>,
    <trd>s'<ind>exciter</ind><klr tip="ind">
    (s'&eacute;nerver)</klr></trd>,
    <trd>s'<ind>irriter</ind><klr tip="ind"> (se mettre en
    col&egrave;re)</klr></trd>,
    <trd>se mettre en <ind>col&egrave;re</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>izgalomba j&ouml;n</trd>,
    <trd>felizgatja mag&aacute;t</trd>
  </trdgrp>
  <trd lng="pt">irritar-se</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="incit.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Facile <tld/>i&gcirc;anta:
      <ekz>
        <tld/>i&gcirc;ema karaktero, humoro;
      </ekz>
      <ekz>
        malsa&gcirc;ulo estas <tld/>i&gcirc;ema kaj memfidema
        <fnt><bib>MT</bib><lok>&Sen; 14:16</lok></fnt>;
      </ekz>
      <ekz>
        malsa&gcirc;ulon mortigas la kolero,
        kaj sensenculon pereigas la <tld/>i&gcirc;emeco
        <klr>(a&ubreve;: <tld/>i&gcirc;emo)</klr>
        <fnt><bib>MT</bib><lok>&Ijo; 5:2</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="incit.0ebla"><tld/>ebla</ref>
    </refgrp>
  </snc>
  <adm>
    Mi ne certas,
    ke "~igxema" kaj "~ebla" estas veraj sinonimoj (ecx se
    la tradukoj identas). <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_z;&c_n;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>
