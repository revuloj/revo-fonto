<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: keton.xml,v 1.4 2022/05/01 08:09:24 afido Exp $">
    <kap>
      <rad>keton</rad>/o
    </kap>

<drv mrk="keton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia organika <ref tip="super" cel="kombin.0ajxo.KEM">kombina&jcirc;o</ref> kun nefina grupo
      <frm>&gt;C=O</frm>:
      <ekz>
        la plej simpla <tld/>o kun tri karbonatomoj estas 
        <ref tip="sub" cel="aceton.0o">acetono</ref> (<frm>CH<sub>3</sub>&ndash;CO&ndash;CH<sub>3</sub></frm>);
      </ekz>
      <ekz>
        <ref cel="aldehi.0o">aldehido</ref> estas organika <klr>[&#x2026;]</klr> kombina&jcirc;o, en kiu la
        karbonilgrupo (<frm>C=O</frm>)
        ligi&gcirc;as al la &ccirc;enfina karbonatomo, tio diferencigas de la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17030">Aldehido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        estis instalitaj novaj aparatoj, ekzemple por <tld/>o-toluena sen-vaksigilo kaj
        sen-oleigilo
        <fnt>
          <bib>Monato</bib>
          <aut>Wu Guojiang</aut>
          <vrk><url ref="&Monato;/2008/009548.php">Rafinejo jubileas</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_je;&c_t;&c_o;&c_n;</trd>
    <trd lng="de">Keton</trd>
    <trd lng="en">keton</trd>
    <trd lng="fr">c&eacute;ton</trd>
    <trd lng="ru">&c_k;&c_je;&c_t;&c_o;&c_n;</trd>
  </snc>
  <trd lng="uk">&#x43A;&#x435;&#x442;&#x43E;&#x43D;</trd>
</drv>

  </art>

