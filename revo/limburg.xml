<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Limburg</rad>/o
</kap>
<drv mrk="limburg.L0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="limburg.L0o.prov">
    <dif>
      Nomo de du <ref tip="super" cel="provin.0o">provincoj</ref>, unu
      en
      <ref tip="malprt" cel="flandr.F0ujo.reg">Flandrujo</ref>, en norda
      Belgujo, kaj unu en
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>.
    </dif>
  </snc>
  <snc mrk="limburg.L0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      inter Kolonjo kaj
      <ref tip="vid" cel="frankfurt.F0oMajno">Frankfurto &ccirc;e
      Majno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_ib;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="bg">&c_L;&c_i;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="ca">Limburg</trd>
  <trd lng="de">Limburg</trd>
  <trd lng="en">Limburg</trd>
  <trd lng="es">Limburgo</trd>
  <trd lng="fr">Limbourg</trd>
  <trd lng="hu">Limburg</trd>
  <trd lng="it">Limburgo</trd>
  <trd lng="nl">Limburg</trd>
  <trd lng="pl">Limburgia</trd>
  <trd lng="pt">Limburgo</trd>
  <trd lng="ru">&c_L;&c_i;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
</drv>
</art>
