<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Limburg</rad>/o
</kap>
<drv mrk="limburg.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="limburg.0o.prov">
    <dif>
      Nomo de du <ref tip="super" cel="provin.0o">provincoj</ref>, unu en
      <ref tip="malprt" cel="flandr.F0ujo.reg">Flandrujo</ref>, en norda
      Belgujo, kaj unu en
      <ref tip="malprt" cel="nederl.0o">Nederlando</ref>.
    </dif>
  </snc>
  <snc mrk="limburg.0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      inter Kolonjo kaj
      <ref tip="vid" cel="frankfurt.0o.cxemajno">Frankfurto &ccirc;e Majno</ref>.
    </dif>
  </snc>
  <trd lng="es">Limburgo</trd>
  <trd lng="fr">Limbourg</trd>
  <trd lng="it">Limburgo</trd>
  <trd lng="nl">Limburg</trd>
  <trd lng="pl">Limburgia</trd>
  <trd lng="pt">Limburgo</trd>
  <trd lng="ru">&c_L;&c_i;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
</drv>
</art>
