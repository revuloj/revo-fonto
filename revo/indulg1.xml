<vortaro>
<art mrk="$Id: indulg1.xml,v 1.21 2023/08/31 16:40:57 revo Exp $">
<kap>
  <rad>indulgenc</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="indulg1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Plena a&ubreve; parta anta&ubreve;liberigo je la punoj merititaj
      pro pekoj, konsentita de la Eklezio la&ubreve; difinitaj
      kondi&ccirc;oj (fasto, pre&gcirc;o, almozo ktp):
      <ekz>
        gajni, meriti <tld/>ojn;
      </ekz>
      <ekz>
        la buleisto supreniris la podion kaj komencis la
        predikon por kura&gcirc;igi la homojn ne resti sen tiom
        da bono kaj <tld/>o, kiom la sanktaj buleoj reprezentas
        <fnt>
          <aut>E. de Bonaero</aut>,
          <vrk>La Friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_u;&c_l;&c_mol;&c_g;&c_je;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_u;&c_l;&c_g;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>indulgencie</trd>,
    <trd>odpustky</trd>,
    <trd>odpu&#x161;t&#x11B;n&#xED; h&#x159;&#xED;ch&#x16F;</trd>
  </trdgrp>
  <trd lng="de">Ablass</trd>
  <trd lng="en">indulgence<klr tip="amb"> (catholic)</klr></trd>
  <trd lng="es">indulgencia</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_dh;&a_sh;&a_t;</trd>,
    <trd>&f_ch;&a_sh;&a_m;&zwnj;&f_p;&a_w;&a_sh;&f_y;</trd>,
    <trd>&a_A;&a_gh;&a_m;&a_A;&a_D;</trd>
  </trdgrp>
  <trd lng="fr">indulgence<klr tip="ind"> (cath.)</klr></trd>
  <trdgrp lng="hu">
    <trd>b&uacute;cs&uacute;<klr tip="ind"> (b&udblac;nbocs&aacute;nat)
    </klr></trd>,
    <trd>b&udblac;nbocs&aacute;nat</trd>
  </trdgrp>
  <trd lng="it">indulgenza<klr> (rel.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8D16;&#x5BA5;  [<ind>&#x3057;&#x3087;&#x304F;&#x3086;&#x3046;</ind>]</trd>,
    <trd>&#x514D;&#x7F6A;  [<ind>&#x3081;&#x3093;&#x3056;&#x3044;</ind>]</trd>,
    <trd>&#x8D16;&#x5BA5;&#x72B6;  [<ind>&#x3057;&#x3087;&#x304F;&#x3086;&#x3046;&#x3058;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x514D;&#x7F6A;&#x7B26;  [<ind>&#x3081;&#x3093;&#x3056;&#x3044;&#x3075;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">aflaat</trd>
  <trd lng="pl">odpust</trd>
  <trd lng="pt">indulg&ecirc;ncia<klr tip="amb"> (catolicismo)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_d;&c_u;&c_l;&c_mol;&c_g;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je; &c_g;&c_r;&c_je;&c_h;&c_o;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odpustenie hriechov</trd>,
    <trd>odpustky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x43F;&#x443;&#x441;&#x442;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x43F;&#x443;&#x449;&#x435;&#x43D;&#x43D;&#x44F; &#x433;&#x440;&#x456;&#x445;&#x456;&#x432;</trd>,
    <trd>&#x456;&#x43D;&#x434;&#x443;&#x43B;&#x44C;&#x491;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
