<vortaro>
<art mrk="$Id$">
<kap>
  <rad>indulgenc</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="indulg1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Plena a&ubreve; parta anta&ubreve;liberigo je la punoj merititaj pro pekoj,
      konsentita de la Eklezio la&ubreve; difinitaj kondi&ccirc;oj (fasto, pre&gcirc;o,
      almozo ktp):
      <ekz>gajni, meriti <tld/>ojn.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_u;&c_l;&c_mol;&c_g;&c_je;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Ablass</trd>
  <trd lng="es">indulgencia</trd>
  <trd lng="fr">indulgence<klr tip="ind"> (cath.)</klr></trd>
  <trdgrp lng="hu">
    <trd>b&uacute;cs&uacute;<klr tip="ind"> (b&udblac;nbocs&aacute;nat)</klr></trd>,
    <trd>b&udblac;nbocs&aacute;nat</trd>
  </trdgrp>
  <trd lng="nl">aflaat</trd>
  <trd lng="pt">indulg&ecirc;ncia<klr tip="amb"> (catolicismo)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_d;&c_u;&c_l;&c_mol;&c_g;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je; &c_g;&c_r;&c_je;&c_h;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
