<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;erp</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxerp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxerp.0i.KOMUNE">
    <dif>
      Preni flua&jcirc;on el iu loko a&ubreve; ujo:
      <ekz>
        <tld/>i akvon el fonto, puto, kru&ccirc;o;
      </ekz>
      <ekz>
        longe <tld/>as la kru&ccirc;o, &gcirc;is &gcirc;i fine
        rompi&gcirc;as<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>draw<klr tip="amb"> (water)</klr></trd>,
      <trd>ladle</trd>,
      <trd>scoop</trd>
    </trdgrp>
    <trd lng="hu">mer<klr tip="ind"> (vizet)</klr></trd>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="cxerp.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elpreni, eltiri:
      <ekz>
        <tld/>i dokumentojn el la fonto mem;
      </ekz>
      <ekz>
        <tld/>i librojn el la biblioteko;
      </ekz>
      <ekz>
        <tld/>i el fremda monujo<fnt>B</fnt>;
      </ekz>
      <ekz>
        el la tempo pasinta ni <tld/>as instruon por la tempo
        venonta<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i utilon el voja&gcirc;o, el kurso;
      </ekz>
      <ekz>
        <tld/>i inspiron el io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el tiuj periodaj festoj ni <tld/>as
        kura&gcirc;on por pluaj laboroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el tio ni povos <tld/>i la konkludon, ke.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>extract</trd>,
    <trd>withdraw</trd>,
    <trd>take</trd>
  </trdgrp>
  <trd lng="fr">puiser</trd>
  <trd lng="hu">mer&iacute;t</trd>
  <trd lng="it">attingere</trd>
  <trd lng="pt">tirar</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;erpa&tcaron;</trd>
</drv>

<drv mrk="cxerp.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxerp.de0i.KOMUNE">
    <dif>
      Preni <tld/>ante:
      <ekz>
        li etendis sian voston, kiel faras la musoj en la laktejo,
        kiam ili de<tld/>as la kremon de plado kaj poste lekas sian
        voston
        <fnt><bib>Fab3</bib>, <lok>Supo el kolbasaj
        bastonetoj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>draw<klr tip="amb"> (water)</klr></trd>,
      <trd>ladle</trd>,
      <trd>scoop</trd>
    </trdgrp>
    <trd lng="fr">puiser</trd>
    <trd lng="it">attingere</trd>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="cxerp.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxerp.el0i.el">
    <dif>
      <tld lit="&Ccirc;"/>i el:
      <ekz>
        el<tld/>i vinon el la barelo.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_e;&c_r;&c_p;&c_a;&c_c;&c_mol;<klr> (&c_z;...)</klr></trd>
    <trdgrp lng="en">
      <trd>ladle out</trd>,
      <trd>scoop out</trd>
    </trdgrp>
    <trd lng="hu">kimer</trd>
    <trdgrp lng="it">
      <trd>spillare</trd>,  
      <trd>prendere<klr> (trarre)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_ch;&c_je;&c_r;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="cxerp.el0i.cxion">
    <dif>
      &Gcirc;isfine <tld/>i, tute preni, plenkonsumi:
      <ekz>
        el<tld/>i la tutan vinon de la barelo;
      </ekz>
      <ekz>
        eldono, libro nun el<tld/>ita;
      </ekz>
      <ekz>
        la programo, mia pacienco
        estas el<tld/>ita;
      </ekz>
      <ekz>
        tiu peniga suprenirado lacegigis kaj tute
        el<tld/>is miajn fortojn;
      </ekz>
      <ekz>
        mia tuta mono el<tld/>i&gcirc;is;
      </ekz>
      <ekz>
        tia solvo ne el<tld/>is la problemon;
      </ekz>
      <ekz>
        el<tld/>i &ccirc;iujn eblecojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sek.0igi">sekigi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="trink">eltrinki</ref>,
      <ref cel="uz.for0i">foruzi</ref>,
      <ref cel="konsum.0i">konsumi</ref>
    </refgrp>
    <trd lng="be">&c_v;&c_y;&c_ch;&c_a;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="en">
      <trd>consume</trd>,
      <trd>use up</trd>,
      <trd>exhaust</trd>
    </trdgrp>
    <trd lng="fr">&eacute;puiser</trd>
    <trd lng="hu">kimer&iacute;t</trd>
    <trdgrp lng="it">
      <trd>esaurire</trd>, 
      <trd>scolare</trd>, 
      <trd>svuotare<klr> (finire)</klr></trd>,
      <trd>stremare</trd>, 
      <trd>estenuare</trd>, 
      <trd>sfinire</trd>
    </trdgrp>
    <trd lng="pl">wyczerpa&cacute;</trd>
    <trd lng="pt">esgotar</trd>
    <trd lng="ru">&c_i;&c_s;&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trd lng="de">sch&ouml;pfen</trd>
  <trd lng="sk">vy&ccaron;erpa&tcaron;</trd>
</drv>

<drv mrk="cxerp.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="cxerp.for0i.KOMUNE">
    <ref tip="dif" cel="cxerp.el0i.cxion">
    el<tld/>i <sncref ref="cxerp.el0i.cxion"/>
    </ref>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_ch;&c_a;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>consume</trd>,
    <trd>use up</trd>,
    <trd>exhaust</trd>
  </trdgrp>
  <trd lng="hu">kimer&iacute;t</trd>
  <trdgrp lng="it">
      <trd>esaurire</trd>, 
      <trd>svuotare<klr> (finire)</klr></trd>,
      <trd>scolare</trd>,
      <trd>stremare</trd>, 
      <trd>estenuare</trd>, 
      <trd>sfinire</trd>
    </trdgrp>
  <trd lng="pl">wyczerpa&cacute;</trd>
  <trd lng="ru">&c_i;&c_s;&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;
</trd>
</drv>

<drv mrk="cxerp.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cxerp.0eto.KOMUNE">
    <dif>
      Kvanto de malgranda <tld/>o, kulerpleno:
      <ekz>
        <tld/>eto da akvo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">spoonful</trd>
  <trdgrp lng="fr">
    <trd>cuill&eacute;r&eacute;e</trd>,
    <trd>lamp&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mer&eacute;snyi</trd>,
    <trd>kan&aacute;lnyi</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cucchiaiata<klr> (quantit&agrave;)</klr></trd>,
    <trd>goccio<klr> (quantit&agrave;)</klr></trd>
  </trdgrp>
</drv>
</art>
