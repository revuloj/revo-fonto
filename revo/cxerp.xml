<vortaro>
<art mrk="$Id: cxerp.xml,v 1.33 2022/03/17 13:47:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;erp</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxerp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxerp.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="pren.0i.mane">Preni</ref> flua&jcirc;on el iu loko a&ubreve; ujo:
      <ekz>
        &scirc;i tuj lavis sian kru&ccirc;on kaj <tld/>is akvon en la plej pura loko de la fonto
        <fnt>
          <bib>F</bib>
          <lok>&FE; 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i malsupreniris al la fonto kaj <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i elver&scirc;is el sia kru&ccirc;o en la trinkujon, kaj kuris denove al la puto, por
        <tld/>i por &ccirc;iuj liaj kameloj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu venis al la vinpremejo, por <tld/>i kvindek mezurojn
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;agaj 2:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filino <klr>[&#x2026;]</klr> staris &ccirc;e la puto kaj supren<tld/>is akvon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> per la manoj <tld/>is <klr>[la]</klr> malbonodoran akvon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        siteloj <tld/>is la akvon el la lageto kaj ver&scirc;is &gcirc;in en trogon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Mefres</nom> prenis en la manon grandan kuleron kaj <tld/>inte el la kaldrono bolantan
        pe&ccirc;on, diris per la&ubreve;ta vo&ccirc;o <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        longe <tld/>as la kru&ccirc;o, &gcirc;is &gcirc;i fine
        rompi&gcirc;as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i akvon per kribrilo
        <klr>(vane klopodi, kp <ref cel="sizif.0o">Sizifo</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>draw<klr tip="amb"> (water)</klr></trd>,
      <trd>ladle</trd>,
      <trd>scoop</trd>
    </trdgrp>
    <trd lng="hu">mer<klr tip="ind"> (vizet)</klr></trd>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="cxerp.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elpreni, <ref tip="super" cel="tir.el0i">eltiri</ref>:
      <ekz>
        <klr>[ili]</klr> kantis pre&gcirc;ojn, el kiuj ili <tld/>is konsolon kaj kura&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        el kiu fonto vi <tld/>is la sciigojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>?
      </ekz>
      <ekz>
        &scirc;i <tld/>is sian kura&gcirc;on el la patrina amo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i el komuna fonto de e&ubreve;ropaj lingvoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj dezirantoj povu <tld/>i kulturon kaj eduki siajn infanojn en sen&scirc;ovinisma
        spirito
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        el la tempo pasinta ni <tld/>u instruon por la tempo venonta
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">sch&ouml;pfen</trd>
  <trdgrp lng="en">
    <trd>extract</trd>,
    <trd>withdraw</trd>,
    <trd>take</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>extraer</trd>
    <trd>sacar</trd>
  </trdgrp>
  <trd lng="fr">puiser</trd>
  <trd lng="he">&lamed;&shin;&alef;&vav;&bet;</trd>
  <trd lng="hu">mer&iacute;t</trd>
  <trd lng="it">attingere</trd>
  <trd lng="pt">tirar</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;erpa&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x435;&#x440;&#x43F;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x442;&#x44F;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x440;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x431;&#x438;&#x440;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="cxerp.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cxerp.0eto.KOMUNE">
    <dif>
      <ref tip="super" cel="kvant.0o">Kvanto</ref> de malgranda <tld/>o, kulerpleno:
      <ekz>
        mi volus vin peti alporti al mi <tld/>eton da akvo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="man.0pleno">manpleno</ref>,
      <ref cel="busx.plen0o">plenbu&scirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_zh;&c_k;&c_a; <klr>(&c_m;&c_je;&c_r;&c_a;)</klr></trd>
  <trd lng="en">spoonful</trd>
  <trdgrp lng="es">
    <trd>cucharada</trd>,
    <trd>sorbo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cuill&eacute;r&eacute;e</trd>,
    <trd>lamp&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mer&eacute;snyi</trd>,
    <trd>kan&aacute;lnyi</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cucchiaiata<klr> (quantit&agrave;)</klr></trd>,
    <trd>goccio<klr> (quantit&agrave;)</klr></trd>
  </trdgrp>
  <trd lng="pl">kapka</trd>
</drv>

<drv mrk="cxerp.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="cxerp.0ilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ujo kun tenilo, per kiu oni <tld/>as:
      <ekz>
        <ref tip="sub" cel="trul.0o">trulo</ref> &mdash; <tld/>ilo kaj albatilo por masonpasto
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi nenian <tld/>ilon havas, kaj la puto estas profunda, de kie do vi havas tiun
        <klr>[&#x2026;]</klr> akvon
        <fnt>
          <bib>NT</bib>
          <lok>Johano 4:11</lok>
        </fnt>?
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> <tld/>is plenan tason da vino
        per kupra <tld/>ilo
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_p;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sch&ouml;pfgef&auml;&szlig;</trd>,
    <trd>Kelle</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuchar&oacute;n</trd>,
    <trd>cazo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>louche</trd>,
    <trd>puisette</trd>
  </trdgrp>
  <trd lng="he">&mem;&tsadi;&qof;&tav;</trd>
  <trdgrp lng="pl">
    <trd>czerpak</trd>,
    <trd>chochla</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x447;&#x435;&#x440;&#x43F;&#x430;&#x43A;</trd>,
    <trd>&#x43A;&#x43E;&#x440;&#x44F;&#x43A;</trd>,
    <trd>&#x43E;&#x43F;&#x43E;&#x43B;&#x43E;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43B;&#x438;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x430; &#x43B;&#x43E;&#x436;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="cxerp.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxerp.de0i.KOMUNE">
    <dif>
      Preni <tld/>ante:
      <ekz>
        li etendis sian voston, kiel faras la musoj en la laktejo,
        kiam ili de<tld/>as la kremon de plado kaj poste lekas sian
        voston
        <fnt><bib>Fab3</bib>, <lok>Supo el kolbasaj
        bastonetoj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd><klr tip="amb"> (water)</klr></trd>,
      <trd>ladle</trd>,
      <trd>scoop</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>servir con cuchar&oacute;n</trd>,
      <trd>sacar con cuchara</trd>
    </trdgrp>
    <trd lng="fr">puiser</trd>
    <trd lng="it">attingere</trd>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_ch;&c_e;&c_r;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ch;&c_e;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_ch;&c_e;&c_r;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">absch&ouml;pfen</trd>
  <trd lng="he">&lamed;&mem;&tsadi;&vav;&tav;</trd>
</drv>

<drv mrk="cxerp.el0i">
  <kap>el<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
<!-- fakte ne trovi&gcirc;as ekz-oj de tiu ne fin&ccirc;erpa senco:
  <snc mrk="cxerp.el0i.el">
    <dif>
      <tld lit="&Ccirc;"/>i el:
      <ekz>
        el<tld/>i vinon el la barelo.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_e;&c_r;&c_p;&c_a;&c_c;&c_mol;<klr> (&c_z;...)</klr></trd>
    <trdgrp lng="en">
      <trd>ladle out</trd>,
      <trd>scoop out</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>vaciar</trd>
    </trdgrp>
    <trd lng="hu">kimer</trd>
    <trdgrp lng="it">
      <trd>spillare</trd>,  
      <trd>prendere<klr> (trarre)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>czerpa&cacute;</trd>,
      <trd>zaczerpn&aogonek;&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_ch;&c_je;&c_r;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
-->
  <snc mrk="cxerp.el0i.cxion">
    <dif>
      &Gcirc;isfine <tld/>i, tute preni, plenkonsumi:
      <ekz>
        la pano el<tld/>i&gcirc;is en nia sako
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 9:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> rivero el<tld/>i&gcirc;as kaj elseki&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 14:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        gardu vin kontra&ubreve; la ekstremo, el<tld/>u simplajn, honestajn rimedojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li jam el<tld/>is mian paciencon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj fortoj, kiel li sentis, el<tld/>i&gcirc;adis, &gcirc;is la tero estis ankora&ubreve; distanco
        de kelkaj kabloj, sed venis helpo, alproksimi&gcirc;is boato
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sek.0igi">sekigi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="trink">eltrinki</ref>,
      <ref cel="uz.for0i">foruzi</ref>,
      <ref cel="konsum.0i">konsumi</ref>,
      <ref cel="muel.0i.FIG_premi">mueli<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_v;&c_y;&c_ch;&c_a;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="en">
      <trd>consume</trd>,
      <trd>use up</trd>,
      <trd>exhaust</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>agotar</trd>,
      <trd>extenuar</trd>
    </trdgrp>
    <trd lng="fr">&eacute;puiser</trd>
    <trd lng="hu">kimer&iacute;t</trd>
    <trdgrp lng="it">
      <trd>esaurire</trd>, 
      <trd>scolare</trd>, 
      <trd>svuotare<klr> (finire)</klr></trd>,
      <trd>stremare</trd>, 
      <trd>estenuare</trd>, 
      <trd>sfinire</trd>
    </trdgrp>
    <trd lng="pl">wyczerpa&cacute;</trd>
    <trd lng="pt">esgotar</trd>
    <trd lng="ru">&c_i;&c_s;&c_ch;&c_je;&c_r;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trd lng="de">sch&ouml;pfen</trd>
  <trd lng="sk">vy&ccaron;erpa&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x447;&#x435;&#x440;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43F;&#x43E;&#x440;&#x43E;&#x436;&#x43D;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43F;&#x43E;&#x440;&#x43E;&#x436;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x43E;&#x440;&#x43E;&#x436;&#x43D;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x43E;&#x440;&#x43E;&#x436;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x431;&#x438;&#x440;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="cxerp.el0ita">
  <kap>el<tld/>ita</kap>
  <snc mrk="cxerp.el0ita.provizo">
    <dif>
      Senigita tute je provizo:
      <ekz>
        la akvo en la putoj, rapide elfositaj, jam estis el<tld/>ita
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar miaj provizoj estas el<tld/>itaj, mi devas prunti
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_ch;&c_a;&c_r;&c_p;&c_a;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>alle <klr>(leer)</klr></trd>,
      <trd>ausgesch&ouml;pft</trd>
    </trdgrp>
  </snc>
  <snc mrk="cxerp.el0ita.FIZL">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senigita je sia tuta forto:
      <ekz>
        mi estis el<tld/>ita, morale kaj fizike,
        pli kaj pli nebulis en mia kapo,
        mi pa&scirc;is a&ubreve;tomate
        <fnt>
          <aut>J. Valano</aut>
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asteni.0a.MED">astenia</ref>,
      <ref cel="lac.0ega">lacega</ref>
    </refgrp>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ja;&c_s;&c_ib;&c_l;&c_je;&c_n;&c_y;</trd>  
    <trd lng="bo">&t_tha;&t_nga;&t_tsheg;&t_cha;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
    <trd lng="de">ersch&ouml;pft</trd>
    <trdgrp lng="es">
      <trd>agotado</trd>,
      <trd>exhausto</trd>,
      <trd>extenuado</trd>
    </trdgrp>
    <trd lng="fr">&eacute;puis&eacute;</trd>
  </snc>
</drv>

<!-- manke de uzo nur var de el~i!
<drv mrk="cxerp.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="cxerp.for0i.KOMUNE">
    <ref tip="dif" cel="cxerp.el0i.cxion">
    el<tld/>i <sncref ref="cxerp.el0i.cxion"/>
    </ref>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_ch;&c_a;&c_r;&c_p;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>consume</trd>,
    <trd>use up</trd>,
    <trd>exhaust</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>agotar</trd>,
    <trd>extenuar</trd>
  </trdgrp>
  <trd lng="hu">kimer&iacute;t</trd>
  <trdgrp lng="it">
      <trd>esaurire</trd>, 
      <trd>svuotare<klr> (finire)</klr></trd>,
      <trd>scolare</trd>,
      <trd>stremare</trd>, 
      <trd>estenuare</trd>, 
      <trd>sfinire</trd>
