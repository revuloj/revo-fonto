<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kanap</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kanap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kanap.0o.MEBL">
    <dif>
      Lar&gcirc;a apog<ref tip="super" cel="segx.0o.meblo">se&gcirc;o</ref> por
      pluraj sidantoj:
      <ekz>
        el sub la <tld/>o la muso kuris sub la liton
        <fnt><bib>F</bib><lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i sidi&gcirc;is sur la <tld/>on kaj lante
        &ccirc;irka&ubreve;rigardis
        <fnt><aut>Julian Modest</aut>: <vrk>En la ombro de
        la silento</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 3a,
        p.
        14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="divan.0o.MEBL">divano</ref>,
      <ref cel="sof.0o.MEBL">sofo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_p;&c_a;</trd>          
  <trd lng="bg">&c_k;&c_a;&c_n;&c_a;&c_p;&c_je;</trd>
  <trd lng="br">gourvezvank</trd>
  <trdgrp lng="de">
    <trd>Kanapee</trd>,
    <trd>Sofa</trd>
  </trdgrp>
  <trd lng="en">sofa <klr>with half-back and one head-end</klr></trd>
  <trdgrp lng="fr">
    <trd>canap&eacute;<klr tip="ind"> (meuble)</klr></trd>,
    <trd>banquette<klr tip="ind"> (meuble)</klr></trd>
  </trdgrp>
  <trd lng="hu">kanap&eacute;</trd>
  <trd lng="nl">canap&eacute;</trd>
  <trd lng="no">sjeselong</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_je;</trd>,
    <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">kanap&eacute;</trd>
</drv>
</art>
