<art mrk="$Id: kanap.xml,v 1.32 2025/07/07 02:26:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kanap</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="kanap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kanap.0o.MEBL">
    <dif>
      Lar&gcirc;a apog<ref tip="lst" lst="voko:mebloj" cel="segx.0o.meblo">se&gcirc;o</ref> por
      pluraj sidantoj:
      <ekz>
        el sub la <tld/>o la muso kuris sub la liton
        <fnt><bib>F</bib><lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i sidi&gcirc;is sur la <tld/>on kaj lante
        &ccirc;irka&ubreve;rigardis
        <fnt><aut>Julian Modest</aut>: <vrk>En la ombro de
        la silento</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 3a,
        p.
        14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="divan.0o.MEBL">divano</ref>,
      <ref cel="sof.0o.MEBL">sofo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_p;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_n;&c_a;&c_p;&c_je;</trd>
  <trd lng="br">gourvezvank</trd>
  <trdgrp lng="ca">
    <trd>canap&eacute;</trd>,
    <trd>sof&agrave;</trd>
  </trdgrp>
  <trd lng="cs">topinka z b&iacute;l. pe&ccaron;iva pikantn&ecaron; oblo&zcaron;en&aacute;</trd>
  <trdgrp lng="de">
    <trd>Kanapee</trd>,
    <trd>Sofa</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sofa <klr>with half-back and one head-end</klr></trd>,
    <trd>settee</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>canap&eacute;<klr tip="ind"> (mueble)</klr></trd>,
    <trd>sof&aacute;<klr tip="ind"> (mueble)</klr></trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_A;&a_n;&a_A;&f_p;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>canap&eacute;<klr tip="ind"> (meuble)</klr></trd>,
    <trd>banquette<klr tip="ind"> (meuble)</klr></trd>
  </trdgrp>
  <trd lng="hu">kanap&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#38263;&#26885;&#23376;  <pr>&#12394;&#12364;&#12356;&#12377;</pr></trd>,
    <trd>&#12477;&#12501;&#12449;&#12540;</trd>,
    <trd>&#12459;&#12490;&#12483;&#12506;</trd>
  </trdgrp>
  <trd lng="nl">canap&eacute;</trd>
  <trd lng="no">sjeselong</trd>
  <trd lng="pl">kanapa</trd>
  <trd lng="ro">canapea</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_je;</trd>,
    <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">hrianka z bieleho pe&ccaron;iva pikantne oblo&zcaron;en&aacute;</trd>
  <trd lng="sv">kanap&eacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26803;&#21270; <pr>sh&umacron;hu&agrave;</pr></trd>,
    <trd>&#38263;&#27801;&#30332; <pr>ch&aacute;ngsh&amacron;f&amacron;</pr></trd>,
    <trd>&#27067;&#24202; <pr>t&agrave;chu&aacute;ng</pr></trd>
  </trdgrp>
</drv>
</art>
