<drv mrk="moleku.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Plej malgranda ero de &hcirc;emia korpo konservanta ankora&ubreve; la
ecojn de la
