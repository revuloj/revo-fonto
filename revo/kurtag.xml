<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kurta&gcirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kurtag.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Procenta&jcirc;o, pago al makleristo.
    </dif>
  </snc>
  <snc>
    <dif>
      Speciala procenta&jcirc;o pagata &ccirc;e borsa transakcio al borsa agento kune
      kun la eventuala imposto.
    </dif>
    <refgrp tip="sin">
      <ref cel="diskon.0o">diskonto</ref>,
      <ref cel="divide.0o">dividendo</ref>,
      <ref cel="intere1.0o">interezo</ref>,
      <ref cel="makler">maklero</ref>,
      <ref cel="procen.0o">procento</ref>,
      <ref cel="rent">rentumo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Maklergeb&uuml;hr</trd>,
    <trd>Maklerkurtage</trd>,
    <trd>Courtage</trd>,
    <trd>Kurtage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>broker commission</trd>,
    <trd>broker fee</trd>,
    <trd>brokerage</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>courtage</trd>,
    <trd>makelaarsloon</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_c;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
