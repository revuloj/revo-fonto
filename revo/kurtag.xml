<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kurtag.xml,v 1.14 2022/03/22 15:44:14 revo Exp $">
<kap>
  <rad>kurta&gcirc;</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="kurtag.0o">
  <kap><tld/>o</kap>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>BEE</bib></fnt>
  <snc mrk="kurtag.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Procenta&jcirc;o, pago al makleristo:
      <ekz>
        lia salajro ne multe varias, sed ofte la firmao
        aldonas etan <tld/>on pro la mendoj akiritaj pere de li
        <fnt>
          <aut>C. Rossetti</aut>,
          <vrk>Kredu Min, Sinjorino!</vrk>,
          <lok>1950</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">alkuszd&iacute;j</trd>
  </snc>
  <snc>
    <dif>
      Speciala procenta&jcirc;o pagata &ccirc;e borsa transakcio al borsa agento
      kune
      kun la eventuala imposto.
    </dif>
    <refgrp tip="sin">
      <ref cel="diskon.0o">diskonto</ref>,
      <ref cel="divide.0o">dividendo</ref>,
      <ref cel="intere1.0o">interezo</ref>,
      <ref cel="makler">maklero</ref>,
      <ref cel="procen.0o">procento</ref>,
      <ref cel="rent">rentumo</ref>.
    </refgrp>
    <trd lng="hu">&uuml;gyn&ouml;ki <ind>jutal&eacute;k</ind></trd>
  </snc><trdgrp lng="be">
    <trd>&c_k;&c_u;&c_r;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_j;&c_n;&c_a;&c_je; &c_w;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Maklergeb&uuml;hr</trd>,
    <trd>Maklerkurtage</trd>,
    <trd>Courtage</trd>,
    <trd>Kurtage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>broker commission</trd>,
    <trd>broker fee</trd>,
    <trd>brokerage</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>courtage</trd>,
    <trd>makelaarsloon</trd>
  </trdgrp>
  <trd lng="pl">prowizja</trd> 
  <trdgrp lng="pt">
    <trd>corretagem</trd>,
    <trd>comiss&atilde;o <klr tip="amb">(de corretor)</klr></trd>
  </trdgrp>
  <trd lng="ro">comision</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_c;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x443;&#x440;&#x442;&#x430;&#x436;</trd>,
    <trd>&#x43A;&#x43E;&#x43C;&#x456;&#x441;&#x456;&#x439;&#x43D;&#x438;&#x439; &#x432;&#x456;&#x434;&#x441;&#x43E;&#x442;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
