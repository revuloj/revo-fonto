<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>markez</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="markez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Speco de tegmento, tre ofte vitra, super la enirejo de domo, vilao,
      por &scirc;irmi kontra&ubreve; pluvo a&ubreve; troa suno:
      <ekz>
        sonis la sonorilo, du-tri fojojn, anta&ubreve; ol li kura&gcirc;is &scirc;alti la
        lumon sub la <tld/>o kaj zorge malfermi la pordon<fnt><aut>Will
        Green</aut>: <vrk>Gantoj</vrk>, <bib>Monato</bib><lok>jaro 1997a,
        numero 5a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Regendach</trd>,
    <trd>Wetterdach</trd>
  </trdgrp>
  <trd lng="en">marquee</trd>
  <trdgrp lng="fr">
    <trd>auvent</trd>,
    <trd>marquise<klr> (auvent)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eresztet&odblac;</trd>,
    <trd>v&eacute;d&odblac;tet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">luifel</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_s;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_i;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
