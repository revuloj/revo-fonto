<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>leptospiroz</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="leptospiroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="leptospiroz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">Malsano</ref>
      ka&ubreve;zata de <ref tip="vid"
      cel="leptospir.0o.BAK">leptospiroj</ref>,
      difektanta precipe la funkcion de renoj,
      konta&gcirc;ata de malgrandaj mambestoj, ron&gcirc;uloj, hundoj kaj aliaj:
      <ekz>
        la <tld/>o estis propagata <klr>...</klr> de la kloakratoj
        <fnt>
          <aut>C. Bertin</aut>,
          <vrk><url
          ref="http://www.bertin.biz/index.php?titre=kojpo.htm">
          La kojpo</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">leptospirosis</trd>
  <trd lng="fr">leptospirose</trd>
</drv>
</art>

