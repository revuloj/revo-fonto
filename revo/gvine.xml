<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gvine</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="gvine.0o">
  <kap>
    <tld lit="G"/>o
    <ofc>8</ofc>
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gvine.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Konakrio.
      Teritorio de Respubliko Gvineo.
      ISO-3166 kodo: GN.
      &Gcirc;i limas kun
      <ref tip="vid" cel="ebur.0bordo.GEOG">Ebur-Bordo</ref>,
      <ref tip="vid"
      cel="gvine.0obisauxo.GEOG">Gvineo-Bisa&ubreve;o</ref>
      <ref tip="vid" cel="mali.0o.GEOG">Malio</ref>,
      <ref tip="vid" cel="liberi.0o.GEOG">Liberio</ref>,
      <ref tip="vid" cel="senegal.0o.GEOG">Senegalo</ref>,
      Sieraleono
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_G;&c_v;&c_i;&c_n;&c_je;&c_ja;</trd>
    <trd lng="de">Guinea</trd>
    <trd lng="en">Guinea</trd>
    <trdgrp lng="pl">
      <trd>Gwinea</trd>,
      <trd>Republika <ind>Gwinei</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="gvine.0obisauxo">
  <kap>
    <tld lit="G"/>o-Bisa&ubreve;o
    <ofc>8</ofc>
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gvine.0obisauxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Bisa&ubreve;o.
      Teritorio de Respubliko Gvineo-Bisa&ubreve;o.
      ISO-3166 kodo: GW.
      &Gcirc;i limas kun
      <ref tip="vid" cel="gvine.0o.GEOG">Gvineo</ref>
      <ref tip="vid" cel="senegal.0o.GEOG">Senegalo</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_G;&c_v;&c_i;&c_n;&c_je;&c_ja;-&c_B;&c_i;&c_s;&c_a;&c_u;</trd>
    <trd lng="de">Guinea-Bissau</trd>
    <trd lng="en">Guinea-Bissau</trd>
    <trdgrp lng="pl">
      <trd>Gwinea Bissau</trd>,
      <trd>Republika <ind>Gwinei Bissau</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="gvine.ekvatora0o">
  <kap>
    Ekvatora <tld lit="G"/>o
    <ofc>8</ofc>
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gvine.ekvatora0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kaj en kelkaj insuloj de Gvinea Golfo, kun &ccirc;efurbo Malabo.
      Teritorio de Respubliko Ekvatora Gvineo.
      ISO-3166 kodo: GQ.
      &Gcirc;i limas kun
      <ref tip="vid" cel="gabon.0o.GEOG">Gabono</ref>,
      <ref tip="vid" cel="kamerun.0o.GEOG">Kameruno</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_Je;&c_k;&c_v;&c_a;&c_t;&c_o;&c_r;&c_i;&c_a;&c_l;&c_n;&c_a; &c_G;&c_v;&c_i;&c_n;&c_je;&c_ja;</trd>
    <trd lng="de">&Auml;quatorialguinea</trd>
    <trd lng="en">Equatorial Guinea</trd>
    <trdgrp lng="pl">
      <trd>Gwinea R&oacute;wnikowa</trd>,
      <trd>Republika <ind>Gwinei R&oacute;wnikowej</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

