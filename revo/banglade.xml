<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>banglade&scirc;</rad>/o
</kap>

<drv mrk="banglade.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="banglade.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda Azio kun &ccirc;efurbo Dako.
      Teritorio de Popola Respubliko Banglade&scirc;o.
      ISO-3166 kodo: BD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>,
      <ref tip="vid" cel="birm.0o.GEOG">Birmo</ref>
      kaj <ref tip="vid" cel="hind.H0aOceano">Hinda Oceano</ref> 
      (Bengala Golfo).
    </dif>
    <trdgrp lng="pl">
      <trd>Bangladesz</trd>,
      <trd>Ludowa Republika <ind>Bangladeszu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

