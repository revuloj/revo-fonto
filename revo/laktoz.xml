<art mrk="$Id: laktoz.xml,v 1.22 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>laktoz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="laktoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="laktoz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Sukereca kaj solida substanco ekstraktata el
      <ref tip="vid" cel="lakt.0o">lakto</ref>;
      <ref tip="sin" cel="suker.lakto0o">laktosukero</ref>:
      <ekz>
        fari froma&gcirc;on ebligis malpliigon de la kvanto
        de <tld/>o en lakto
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/programoj/2013/1/1/rv196-20130101.html">
          Radio Verda</url></vrk>,
          <lok>RV196, 2013-01-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  <trd lng="br">laktoz</trd>
  <trd lng="ca">lactosa</trd>
  <trdgrp lng="cs">
    <trd>lakt&oacute;za</trd>,
    <trd>ml&eacute;&ccaron;n&yacute; cukr</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Laktose</trd>,
     <trd>Milchzucker</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&alfa;&kappa;&tau;&omikron;&sigma;&alfa_ton;&kappa;&chi;&alfa;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>milk <ind>sugar</ind></trd>,
    <trd>lactose</trd>
  </trdgrp>
  <trd lng="es">lactosa</trd>
  <trdgrp lng="fr">
    <trd>lactose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tejcukor</trd>,
    <trd>lakt&oacute;z</trd>
  </trdgrp>
  <trd lng="it">lattosio</trd>
  <trd lng="ja">&#x4E73;&#x7CD6;  <pr>&#x306B;&#x3085;&#x3046;&#x3068;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>melksuiker</trd>,
    <trd>lactose</trd>
  </trdgrp>
  <trd lng="pl">laktoza</trd>
  <trd lng="pt">lactose</trd>
  <trd lng="ro">lactoz&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_a;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mlie&ccaron;ny cukor</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x43B;&#x43E;&#x447;&#x43D;&#x438;&#x439; &#x446;&#x443;&#x43A;&#x43E;&#x440;</trd>,
    <trd>&#x43B;&#x430;&#x43A;&#x442;&#x43E;&#x437;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E73;&#x7CD6; <pr>r&#x1D4;t&#xE1;ng</pr></trd>
</drv>
</art>
