<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>laktoz</rad>/o
</kap>
<drv mrk="laktoz.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Sukereca kaj solida substanco ekstraktata el
      <ref tip="vid" cel="lakt.0o">lakto</ref>.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="suker.lakto0o">laktosukero</ref>
  </refgrp>
  <trdgrp lng="en">
    <trd>milk <ind>sugar</ind></trd>,
    <trd>lactose</trd>
  </trdgrp>
  <trd lng="es">lactosa</trd>
  <trdgrp lng="fr">
    <trd>lactose</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>melksuiker</trd>,
    <trd>lactose</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_a;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
