<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>matriark</rad>/o
</kap>

<drv mrk="matria.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Estrino de familio, tribo a&ubreve; etna a&ubreve; religia grupo.
    </dif>
  </snc>
  <trd lng="ca">matriarca</trd>
  <trd lng="de">Matriarchin</trd>
  <trd lng="en">matriarch</trd>
  <trd lng="es">matriarca</trd>
  <trd lng="fr">matriarche</trd>
  <trdgrp lng="nl">
    <trd>matriarch</trd>,
    <trd>aartsmoeder</trd>
  </trdgrp>
  <trd lng="pt">matriarca</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_r;&c_i;&c_a;&c_r;&c_h;</trd>
  <trd lng="sv">matriark</trd>
</drv>

<drv mrk="matria.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Socia organizo, en kiu la patrino estras sian familion, havanta
      a&ubreve;toritaton je viroj kaj infanoj.
    </dif>
  </snc>
  <trd lng="de">Matriarchat</trd>
  <trd lng="en">matriarchy</trd>
  <trd lng="es">matriarcado</trd>
  <trd lng="fr">matriarcat</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_r;&c_i;&c_a;&c_r;&c_h;&c_a;&c_t;</trd>
  <trd lng="sv">matriarkat</trd>
</drv>
</art>

