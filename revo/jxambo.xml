<art mrk="$Id: jxambo.xml,v 1.21 2023/08/31 18:25:29 revo Exp $">
<kap>
  <rad>&jcirc;ambore</rad>/o
</kap>
<drv mrk="jxambo.0o">
  <kap><tld/>o
</kap>
  <snc>
    <dif>
      Granda internacia <ref tip="super" cel="ven.kun0o">kunveno</ref>
      de <ref cel="skolt.0o.SPO">skoltoj</ref>:
      <ekz>
        de 1931 oni aran&gcirc;is multajn kursojn inter la skoltoj, por uzi <klr>[E-on]</klr>
        dum la mondtendaro en a&ubreve;g. 1933 en <nom>G&ouml;d&ouml;ll&odblac;</nom> (apud Budape&scirc;to), la <tld/>o
        alportis rimarkindan sukceson por Esperanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18432">Esperanto-movado en Hungario</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Armstrong</nom> salutis la skoltojn: mi &scirc;atus saluti al &ccirc;iuj miaj geskoltoj &ccirc;e la
        &Scirc;tata Parko <nom>Farragut</nom> en Idaho kie ili havas Nacian <tld lit="&Jcirc;"/>on tiun &ccirc;i
        semajnon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16645">Neil Armstrong</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_l;&c_jo;&c_t; <klr tip="amb">
  (&c_s;&c_k;&c_a;&c_w;&c_t;&c_a;&c_w;)</klr></trd>
  <trd lng="ca">jamboree</trd>
  <trd lng="de">Pfadfinderkongress</trd>
  <trd lng="en">jamboree</trd>
  <trd lng="fr">jamboree</trd>
  <trd lng="hu">dzsembori</trd>
  <trd lng="id">jambore</trd>
  <trd lng="it">jamboree</trd>
  <trd lng="ja">&#x30B8;&#x30E3;&#x30F3;&#x30DC;&#x30EA;&#x30FC;</trd>
  <trd lng="nl">jamboree</trd>
  <trd lng="pl">jamboree</trd>
  <trd lng="pt">jamboree</trd>
  <trd lng="ru">&c_s;&c_l;&c_jo;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x436;&#x430;&#x43C;&#x431;&#x43E;&#x440;&#x456;</trd>,
    <trd>(&#x43C;&#x456;&#x436;&#x43D;&#x430;&#x440;&#x43E;&#x434;&#x43D;&#x438;&#x439;) &#x437;&#x43B;&#x456;&#x442; &#x441;&#x43A;&#x430;&#x443;&#x442;&#x456;&#x432;</trd>
  </trdgrp>
</drv>
</art>

