&Scirc;timec</aut>:
         <vrk>Morto en Melburna tramo</vrk>, <bib>Monato</bib><lok>jaro
         1997a, numero 8a, p. 24a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_u;&c_n;&c_ib;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>
  <trdgrp lng="br">
     <trd>lifre<klr tip="ind"> (unwisk)</klr></trd>,
     <trd>unwisk</trd>
   </trdgrp>
  <trd lng="ca">uniforme</trd>
  <trdgrp lng="cs">
    <trd>stejnokroj</trd>,
    <trd>uniforma</trd>
  </trdgrp>
  <trd lng="de">Uniform</trd>
  <trd lng="el">&sigma;&tau;&omikron;&lambda;&eta_ton;</trd>
  <trd lng="en">uniform</trd>
  <trd lng="es">uniform</trd>
  <trdgrp lng="fa">
     <trd>&a_h;&a_m;&a_s;&a_A;&a_n;&a_h;</trd>,
     <trd>&a_l;&a_b;&a_A;&a_s; &a_f;&a_r;&a_m;</trd>,
     <trd>&a_A;&a_w;&a_n;&f_y;&a_f;&a_w;&a_r;&a_m;</trd>
   </trdgrp>
  <trd lng="fi">univormu</trd>
  <trd lng="fr">uniforme</trd>
  <trdgrp lng="he">
     <trd>&mem;&dalet;&yod;&fmem;</trd>,
     <trd>&tav;&lamed;&bet;&vav;&shin;&tav;
&alef;&het;&yod;&dalet;&he;</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>egyenruha</trd>,
     <trd>uniformis</trd>,
     <trd>mund&eacute;r</trd>
   </trdgrp>
  <trdgrp lng="it">
   <trd>uniforme<klr> (sost.)</klr></trd>, 
   <trd>divisa<klr> (sost.)</klr></trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5236;&#x670D;  [<ind>&#x305B;&#x3044;&#x3075;&#x304F;</ind>]</trd>,
    <trd>&#x30E6;&#x30CB;&#x30D5;&#x30A9;&#x30FC;&#x30E0;</trd>
  </trdgrp>
  <trd lng="nl">uniform</trd>
  <trd lng="pt">uniforme</trd>
  <trdgrp lng="ru">
     <trd>&c_m;&c_u;&c_n;&c_d;&c_i;&c_r;</trd>,
     <trd>&c_f;&c_o;&c_r;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
&c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;</trd>,
     <trd>&c_u;&c_n;&c_i;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>
