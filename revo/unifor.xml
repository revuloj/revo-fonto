  <trdgrp lng="sk">
    <trd>rovno&#x161;ata</trd>,
    <trd>uniforma</trd>
  </trdgrp>
  <trd lng="sv">uniform</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x443;&#x43D;&#x434;&#x438;&#x440;</trd>,
