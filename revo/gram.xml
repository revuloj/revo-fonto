<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gram</rad>/o
</kap>
<drv mrk="gram.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">g</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> 
      de maso, milono de <ref tip="malprt"
      cel="kilogr.0o">kilogramo</ref>.
    </dif>
    <rim>Internacia Sistemo de unuoj difinas <tld/>on surbaze de
    kilogramo.</rim>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">gram</trd>
  <trd lng="de">Gramm</trd>
  <trd lng="en">gram</trd>
  <trd lng="es">gramo</trd>
  <trd lng="fr">gramme</trd>
  <trd lng="hu">gramm</trd>
  <trd lng="nl">gram</trd>
  <trd lng="pt">grama<klr tip="amb"> (unidade de massa)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  <trd lng="tr">gram</trd>
</drv>
</art>
