<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gram</rad>/o
</kap>
<drv mrk="gram.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de maso, milono 
      de <ref tip="malprt" cel="kilogr.0o">kilogramo</ref>; simb. g.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">Gramm</trd>
  <trd lng="en">gram</trd>
  <trd lng="es">gramo</trd>
  <trd lng="fr">gramme</trd>
  <trd lng="hu">gramm</trd>
  <trd lng="nl">gram</trd>
  <trd lng="pt">grama<klr tip="amb"> (unidade de massa)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
</drv>
</art>
