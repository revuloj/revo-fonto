  </trdgrp>
  <trd lng="hu">&ouml;sszeragaszt</trd>
  <trdgrp lng="ja">
    <trd>&#x304F;&#12387;&#12388;&#12369;&#x308B;</trd>,
    <trd>&#x306F;&#x308A;&#21512;&#x308F;&#x305B;&#x308B; 
    <pr>&#x306F;&#x308A;&#12354;&#x308F;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x63A5;&#30528;&#12377;&#x308B;  <pr>&#x305B;&#12387;&#12385;&#12419;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">samenkleven<klr tip="ind"> (ov.)</klr></trd>
  <trd lng="pl">sklei&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zlepi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43B;&c_je;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43B;&c_ib;&#x43F;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22359;&#x591A; <pr>ku&#xE0;idu&#x14D;</pr></trd>,
    <trd>&#x7C98;&#22312;&#x4E00;&#x8D77; <pr>zh&amacron;nz&#xE0;iy&#xEC;q&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glu.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi du kun<tld/>itajn objektojn:
      <ekz>
        estas ja konate, ke la po&scirc;tistoj kapablas mal<tld/>i
        &ccirc;ian ajn koverton
        <fnt>
          <aut>Karel &Ccirc;APEK, tr. Milo&scirc; LUKA&Scirc;</aut>:
