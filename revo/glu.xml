<drv mrk="glu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="glu.0i.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
    <ref tip="super" cel="fiks.0i.flagon">Fiksi</ref> per <tld/>ilo:
      <ekz>
        &scirc;i &ccirc;irka&ubreve;tran&ccirc;is unu post la alia &ccirc;iujn presajn literojn <klr>[&#8230;]</klr> kaj ilin
        <tld/>is sur la paperfolio <klr>[&#8230;]</klr> tia, ke ili faru vortojn
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> rulis cigaredon <klr>[&#8230;]</klr>, <tld/>is &gcirc;in per la lango, iom
        &gcirc;in su&ccirc;is kaj ekfumis
        <fnt>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u afi&scirc;on sur tiun fenestron
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mastik.0i">mastiki</ref>,
      <ref cel="lut.0i">luti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>leimen</trd>,
      <trd>heften</trd>,
    </trdgrp>
    <trd lng="el">&kappa;&omikron;&lambda;&lambda;&alfa_ton;&omega;
    &eta_ton; &kappa;&omikron;&lambda;&lambda;&omega_ton;</trd>
    <trd lng="en">glue</trd>
    <trd lng="es">pegar</trd>
    <trd lng="fr">coller<klr tip="ind"> (tr. dir.)</klr></trd>
    <trdgrp lng="hu">
      <trd>enyvez</trd>,
      <trd>ragaszt</trd>
    </trdgrp>
    <trd lng="id">menge<ind>lem</ind></trd>
    <trdgrp lng="nl">
      <trd>plakken<klr tip="ind"> (ov.)</klr></trd>,
      <trd>kleven<klr tip="ind"> (ov.)</klr></trd>
    </trdgrp>
    <trd lng="pl">klei&cacute;</trd>
    <trd lng="tr">yap&inodot;&scedil;t&inodot;rmak</trd>
  </snc>
  <snc mrk="glu.0i.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Fiksi kiel <tld/>ilo:
      <ekz>
        la pluvo <tld/>is la buklojn de liaj longaj haroj en &scirc;nurojn
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mielo <klr>[&#8230;]</klr> povas ja <tld/>i, &gcirc;i sendube estas <tld/>ilo
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnis al &scirc;i, ke &scirc;in tu&scirc;as io <tld/>anta kaj malvarma
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>itaj estas iliaj okuloj, por ke ili ne vidu
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 44:18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_k;&c_l;&c_je;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kl&iacute;&zcaron;it</trd>,
    <trd>lepit</trd>
  </trdgrp>
  <trd lng="de">kleben</trd>
  <trdgrp lng="ja">
    <trd>&#x306F;&#x308B;</trd>,
    <trd>&#x306F;&#x308A;&#12388;&#12369;&#x308B;</trd>,
    <trd>&#x63A5;&#30528;&#12377;&#x308B;  <pr>&#x305B;&#12387;&#12385;&#12419;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>colar</trd>,
    <trd>grudar</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">lepi&tcaron;</trd>
  <trd lng="uk">&#x43A;&#x43B;&c_je;&c_ji;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x8CBC; <pr>ti&emacron;</pr></trd>,
    <trd>&#x7C98;&#30528; <pr>ni&#xE1;nzhu&#xF3;</pr></trd>,
    <trd>&#x7C98;&#x63A5; <pr>zh&amacron;nji&emacron;</pr></trd>,
    <trd>&#22359;&#x591A; <pr>ku&#xE0;idu&#x14D;</pr></trd>,
    <trd>&#x7C98;&#22312;&#x4E00;&#x8D77; <pr>zh&amacron;nz&#xE0;iy&#xEC;q&#x1D0;</pr></trd>,
    <trd>&#x7CCA; <pr>h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="glu.0ilo"><tld lit="G"/>ilo</ref>:
      <ekz>
        aldoninte al tiu &ccirc;i miksa&jcirc;o pli da ovoblanko
        a&ubreve; da <tld/>o solvita en nafto, por &gcirc;in pli maldensigi, oni
        povas &gcirc;in uzi por laki vazojn kaj diversajn objektojn
	<fnt><bib>FK</bib></fnt>;
      </ekz>
      <ekz>
        ne metu <tld/>on sur la fenestron, metu &gcirc;in sur la dorson de la afi&scirc;o
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la majstro bezonis duonsitelon da <tld/>o, kiun li uzis por alfiksi
        <klr>[&#8230;]</klr> plandumojn
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2006/008678.php">Esti&gcirc;i la plej alta</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
<!-- kontrolinda, &ccirc;u "gluteno" (proteino) a&ubreve; &ccirc;u gluilo farita el faruno kaj akvo...?
      <ekz>
        farun<tld/>o;
      </ekz><ekz>
        <ind>fi&scirc;<tld/>o</ind>;
        <trd lng="fr">ichtyocolle</trd>
        <trd lng="hu">halenyv</trd>
        <trd lng="nl">vislijm</trd>
      </ekz
-->
    </dif>
    <ref tip="vid" cel="adher.0i">adheri</ref>
  <trd lng="be">&c_k;&c_l;&c_je;&c_j;</trd>
  </snc>
  <snc mrk="glu.0o.kunigo">
    <dif>
      Kunigo per <tld/>ilo:
      <ekz>
        interna mar&gcirc;eno iomete tro mallar&gcirc;a, por facile legi la tekston, sen dama&gcirc;i la
        <tld/>on kuntenantan la pa&gcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009082.php">Majstra verko de a&ubreve;toro preska&ubreve;
          nekonata</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_l;&c_je;&c_j;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Leimung</trd>,
      <trd>Klebung</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
     <ref cel="bend.glu0o">glubendo</ref>,
     <ref cel="bild.glu0o">glubildo</ref>,
     <ref cel="mark.glu0o">glumarko</ref>
  </refgrp>
  <trdgrp lng="cs">
    <trd>kl&iacute;h</trd>,
    <trd>lepidlo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Leim</trd>,
    <trd>Klebstoff</trd>,
    <trd>Klebebindung</trd>
  </trdgrp>
  <trd lng="es">pegamento</trd>
  <trd lng="fr">colle<klr tip="ind"> (adh&eacute;sif)</klr></trd>
  <trdgrp lng="hu">
    <trd>enyv</trd>,
    <trd>ragaszt&oacute;</trd>
  </trdgrp>
  <trd lng="id">lem</trd>
  <trdgrp lng="ja">
    <trd>&#x7CCA;  <pr>&#x306E;&#x308A;</pr></trd>,
    <trd>&#x81A0;  <pr>&#x306B;&#12409;</pr></trd>
  </trdgrp>
  <trd lng="nl">lijm</trd>
  <trd lng="pl">klej</trd>
  <trd lng="pt">cola</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;</trd>
  <trd lng="sk">lepidlo</trd>
  <trd lng="uk">&#x43A;&#x43B;&c_je;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x80F6;&#21512;&#21058; <pr>ji&amacron;oh&#xE9;j&#xEC;</pr></trd>,
    <trd>&#x80F6;&#x7C98;&#21058; <pr>ji&amacron;ozh&amacron;nj&#xEC;</pr></trd>,
    <trd>&#x7CCA; <pr>h&#xFA;</pr></trd>,
    <trd>&#x7C98;&#21512;&#21137; <pr>ni&#xE1;nh&#xE9;j&#xEC;</pr></trd>,
    <trd>&#x7C98;&#x63A5;&#26448;&#26009; <pr>zh&amacron;nji&emacron;c&#xE1;ili&#xE0;o</pr></trd>,
    <trd>&#x7C98;&#21058; <pr>zh&amacron;nj&#xEC;</pr></trd>,
    <trd>&#x5BC6;&#x5C01;&#x80F6; <pr>m&#xEC;f&emacron;ngji&amacron;o</pr></trd>,
    <trd>&#x7C98;&#x80F6; <pr>zh&amacron;nji&amacron;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glu.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="fiks.0igxi">Fiksi&gcirc;i</ref> per <tld/>a substanco:
      <ekz>
        la lango <tld/>i&gcirc;is al la palato
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia viza&gcirc;o pali&gcirc;is, la haroj <tld/>i&gcirc;is de l&#8217; &scirc;vito
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> vi uzas ne iun mirindan paton, al kiu nenio povas
        <tld/>i&gcirc;i, sed la ordinaran, kiu emas kuni&gcirc;i
        kun ajna fritata&jcirc;o poreterne
        <fnt>
          <aut>Je. Katajeva</aut>:
          <vrk>Recepto de legomaj benjetoj</vrk>.
          <lok><url
          ref="http://www.abonu.com/archive/sano.vegetarismo/200209/26085859.html">Vegetarismo:
          komforto sen perforto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="glu.0igxi.FIG">
    <dif>
      Fiksi&gcirc;i forte kvaza&ubreve; per <tld/>ilo:
      <ekz>
        la akvo &scirc;prucis el sub liaj nudaj piedoj, la flava vesto <tld/>i&gcirc;is al lia
        maljuna malgrasa korpo
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la oro <tld/>i&gcirc;is al &scirc;i kaj kovris &scirc;in tutan
        <fnt>
          <bib>ElektFab</bib>
          <lok>La maljunulino Holle</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e vojkurbi&gcirc;oj <klr>[en]</klr> plena rapideco <klr>[&#8230;]</klr> ni preska&ubreve;
        <tld/>i&gcirc;is al la sidlokoj pro la centrifuga forto
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj okuloj <tld/>i&gcirc;is sur la besto, La leono subite anta&ubreve;enmovi&gcirc;is
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz><ekz>
        la a-fina&jcirc;o la&ubreve;sence <tld/>i&gcirc;as ne senpere al la verba
        radiko
        <fnt>
          <vrk>La vortfarado en Esperanto</vrk>
          <lok>Aktoj de la Akademio, 1963&ndash;1967
            (Oficiala Bulteno de la Akademio de Esperanto, n-ro
            9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="adher.0i">adheri</ref>,
    <ref cel="koher.0i">koheri</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_je;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_k;&c_l;&c_je;&c_j;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ankleben</trd>,
    <trd>sich <ind>heften</ind> <klr>(an etwas)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&#x43A;&#x43B;&c_je;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  <trd lng="zh">&#x9ECF;&#x8CBC; <pr>ni&#xE1;nti&emacron;</pr></trd>
</drv>

<drv mrk="glu.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="glu.0ajxo.ilo">
    <dif>
      <ref tip="dif" cel="glu.0ilo"><tld lit="G"/>ilo</ref>:<ekz>
        minimume 600 kg da <tld/>a&jcirc;o kaj 50 kg da farboj
        necesis entute por la ja&hcirc;to
	<fnt>
	  <aut>Jean-Yves SANTERRE</aut>:
          <vrk> Persistemo donis plezuron!</vrk>
	  <bib>Monato</bib>
	</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la memoro estas <tld/>a&jcirc;o, kiu kuntenas nian estadon
        <fnt>
          <aut>Hjalmar S&ouml;derberg, tr. Sten Johansson</aut>:
          <vrk>G&auml;rda</vrk>.  <lok>Sk&ouml;vde: Al-fab-et-o,
          1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_je;&c_j;</trd>
    <trdgrp lng="de">
      <trd>Kleber</trd>,
      <trd>Klebstoff</trd>
    </trdgrp>
  </snc>
  <snc mrk="glu.0ajxo.maso">
    <dif>
      <tld lit="G"/>a substanco:
      <ekz>
        miaj &scirc;uoj estis plenaj de la ru&gcirc;a
        <tld/>a&jcirc;o
	<fnt>
	  <aut>Jen&odblac; Rejt&odblac;, tr. L&aacute;szl&oacute;
	     Bal&aacute;zs</aut>:
	  <lok><url ref="http://www.freeweb.hu/verkoj/02.htm">La anta&ubreve;en&scirc;ovita garnizono</url></lok>
	</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;&c_ja; &c_m;&c_a;&c_s;&c_a;</trd>

    <trd lng="ru">&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;&c_ja;
    &c_m;&c_a;&c_s;&c_s;&c_a;</trd>
  </snc>
  <snc mrk="glu.0ajxo.bildo">
    <dif>
      Bildeto kun <tld/>ilo dorsflanke, kiun oni povas ie al<tld/>i,
      i.a. <ref tip="sub" cel="mark.posxt0o">po&scirc;tmarko</ref>, <ref tip="sub"
      cel="bild.glu0o"><tld/>bildo</ref>:
      <ekz>
        &scirc;i gaje malfermis la libron kaj ne povis ne rimarki la belan <tld/>a&jcirc;on en &gcirc;i:
        ekslibriso kun papilio
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/010053.php">Ekslibriso</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        mem<tld/>a&jcirc;o <klr>[&#8230;]</klr>, ekzemple, <tld/>bildoj en via a&ubreve;to
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Dereviannykh</aut>
          <vrk><url ref="&Monato;/2004/007562.html">Kiu instigas nin al farado, kiel
          plej multe faras</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_l;&c_je;&c_p;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_y;&c_k;&c_je;&c_r;</trd>
    </trdgrp>
    <trd lng="de">Aufkleber</trd>
    <trd lng="en">sticker</trd>
    <trd lng="fr">autocollant</trd>
    <trdgrp lng="zh">
    <trd>&#x5E26;&#x4E0D;&#x5E72;&#x80F6;&#30340;&#x4FBF;&#26465;
    <pr>d&#xE0;ib&#xF9;g&amacron;nji&amacron;odebi&#xE0;nti&#xE1;o</pr></trd>,
    <trd>&#x80F6;&#x7EB8; <pr>ji&amacron;ozh&#x1D0;</pr></trd>,
    <trd>&#26631;&#x8D34; <pr>bi&amacron;oti&emacron;</pr></trd>,
    <trd>&#21363;&#x65F6;&#x8D34; <pr>j&#xED;sh&#xED;ti&emacron;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x63A5;&#30528;&#21092;  <pr>&#x305B;&#12387;&#12385;&#12419;&#x304F;&#12374;&#12356;</pr></trd>
  <trd lng="uk">&#x43A;&#x43B;&c_je;&c_j;</trd>
</drv>

<drv mrk="glu.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Duonlikva <ref tip="super" cel="sxmir.0ajxo">&scirc;mira&jcirc;o</ref>, kiu seki&gcirc;ante firme
      kunigas kunmetitajn objektojn:
      <ekz>
        la saketo estas fiksata sur la plandoj per <tld/>ilo por
        8&ndash;12 horoj tage a&ubreve; nokte
        <fnt>
          <lok><url
          ref="http://webcenter.ru/~espmik/arbotrezoro/uzo-esp.htm">Arbotrezoro</url></lok>
        </fnt>;
      </ekz><ekz>
        la fadenoj <klr>(de aranea&jcirc;o)</klr> ne enhavas
        <tld/>ilon,
        sed la insektoj kapti&gcirc;as per siaj haroj en la
        implikita
        strukturo
        <fnt>
          <aut>Tr. Leo De Cooman</aut>:
          <lok><url
          ref="http://www.xs4all.nl/~ednieuw/Spiders/InfoNed/araneajxo_kaj_fadeno.html">Araneo</url></lok>
        </fnt>;
      </ekz><ekz>
        rekapti la mar&gcirc;enigitajn junulojn kiuj snufas
        <tld/>ilon
        <fnt>
          <aut>Jacques DENIS</aut>:
          <vrk>La sono de la Brazilo de
          &leftquot;Lula&rightquot;</vrk>.
          <lok><url ref="&Mondediplo;article848.html">Le Monde diplomatique, Decembro
          2003</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="gluten.0o">gluteno</ref>,
      <ref cel="ihxtio.0o">i&hcirc;tiokolo</ref>,
      <ref cel="mucila.0o">mucilago</ref>,
      <ref cel="muk.0o">muko</ref>,
      <ref cel="rezin.0o">rezino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Leim</trd>,
    <trd>Klebstoff</trd>
  </trdgrp>
  <trd lng="es">pegamento</trd>
  <trd lng="fr">colle<klr tip="ind"> (adh&eacute;sif)</klr></trd>
  <trdgrp lng="hu">
    <trd>enyv</trd>,
    <trd>ragaszt&oacute;</trd>
  </trdgrp>
  <trd lng="id">lem</trd>
  <trd lng="nl">lijm</trd>
  <trd lng="pl">klej</trd>
  <trd lng="pt">cola</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;</trd>
  <trd lng="tr">tutkal</trd>
  <trdgrp lng="zh">
    <trd>&#x7C98;&#21512;&#21137; <pr>ni&#xE1;nh&#xE9;j&#xEC;</pr></trd>,
