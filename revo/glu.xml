<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>glu</rad>/o</kap>
<drv mrk="glu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Duonflueca &scirc;mira&jcirc;o, kiu seki&gcirc;ante kapabli&gcirc;as
      forte kunigi du objektojn:
      <ekz>
        <tld/>o el araba gumo;
      </ekz>
      <ekz>
        farun<tld/>o;
      </ekz>
      <ekz>
        <ind>fi&scirc;<tld/>o</ind>;
        <trd lng="fr">ichtyocolle</trd>
        <trd lng="nl">vislijm</trd>
      </ekz>
      <ekz>
        <tld/>(ec)a substanco;
      </ekz>
      <ekz>
        <ind><tld/>a&ccirc;a</ind> ha&ubreve;to de angilo.
        <trd lng="fr">gluant</trd>
        <trd lng="nl">kleverig</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="adher.0i">adheri.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Leim</trd>,
    <trd>Klebstoff</trd>
  </trdgrp>
  <trd lng="fr">colle<klr tip="ind"> (adh&eacute;sif)</klr></trd>
  <trd lng="nl">gom</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;</trd>
</drv>

<drv mrk="glu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunigi per <tld/>o:
      <ekz>
        <tld/>i afi&scirc;on sur muron;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i sian atenton al io.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mastik.0i">mastiki</ref>,
      <ref cel="lut.0i">luti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>leimen</trd>,
    <trd>kleben</trd>,
    <trd>heften</trd>
  </trdgrp>
  <trd lng="en">glue</trd>
  <trd lng="fr">coller<klr tip="ind"> (tr. dir.)</klr></trd>
  <trd lng="nl">kleven<klr tip="ind"> (ov.)</klr></trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="glu.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="G"/>i objekton al pli granda:
      <ekz>
        al<tld/>i po&scirc;tmarkon.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">coller<klr tip="ind"> (tr. dir.)</klr></trd>
  <trd lng="nl">opkleven</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="glu.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Forte ali&gcirc;i, kuni&gcirc;i per si mem kun alia objekto:
      <ekz>
        la birdo al<tld/>i&gcirc;is al la kaptilo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia lango al<tld/>i&gcirc;is al mia palato
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 22, 15</lok>)</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;iaj okuloj al<tld/>i&gcirc;is al li<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iu homo ricevos la rajton alglui&gcirc;i per sia
        tuta koro al tiu peco da tero, kiu lin naskis
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo en la Guildhall,
        Londono, 1907</vrk>)</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia animo al<tld/>i&gcirc;is al Vi
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 63, 8</lok>)</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la malnova kutimo al<tld/>i&gcirc;is al li.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="forgx.al0i">alfor&gcirc;i</ref>,
      <ref cel="lig">alligi&gcirc;i</ref>,
      <ref cel="fiks">alfiksi&gcirc;i</ref>,
      <ref cel="firm">alfirmi&gcirc;i</ref>,
      <ref cel="krocx.al0igxi">alkro&ccirc;i&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">sich <ind>heften</ind> an</trd>
  <trdgrp lng="fr">
    <trd>coller<klr tip="ind"> (s'attacher &agrave;)</klr></trd>,
    <trd>s'<ind>attacher</ind></trd>,
    <trd>se <ind>coller</ind></trd>
  </trdgrp>
  <trd lng="nl">kleven<klr tip="ind"> (onov.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="glu.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="G"/>i kune du objektojn:
      <ekz>
        kun<tld/>i dis&scirc;iritajn pecojn de letero.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="ten.kun0i">kunteni</ref>
  </snc>
  <trd lng="de">zusammenkleben</trd>
  <trdgrp lng="fr">
    <trd>coller<klr tip="ind"> (ensemble)</klr></trd>,
    <trd>maintenir ensemble</trd>
  </trdgrp>
  <trd lng="nl">samenhangen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="glu.kun0igxemo">
  <kap>kun<tld/>i&gcirc;emo</kap>
  <snc>
    <dif>
      Kunteni&gcirc;emo.
    </dif>
  </snc>
  <trd lng="fr">coh&eacute;sion</trd>
  <trd lng="nl">cohesie</trd>
</drv>

<drv mrk="glu.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi du <tld/>itajn objektojn.
    </dif>
  </snc>
  <adm>
    Ekz-oj bonvenaj. Cxu eblas diri &leftquot;mal~i koverton&rightquot;?
    <aut>MB</aut>
  </adm>
  <trd lng="de">trennen</trd>
  <trd lng="fr">d&eacute;coller<klr tip="ind"> (enlever la colle)</klr></trd>
  <trd lng="nl">losweken</trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
