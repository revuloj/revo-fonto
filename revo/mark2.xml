<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mark</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mark2.0o">
  <kap><tld/>o</kap>
  <snc mrk="mark2.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iama <ref tip="super" cel="unu.mon0o.EKON">monunuo</ref> de 
      Estonio, Finlando, Germanujo kaj aliaj:
      <ekz>
        en Bosnio kaj Hercegovino la monunuo estas bosna <tld/>o, 
        oficiale nomita <ctl>Konvertibilna marka (KM)</ctl>
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperanto.ba/ijk/KostojSarajevo.htm">
          62-a internacia junulara kongreso</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_k;&c_a; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>  
  <trd lng="br">mark</trd>
  <trd lng="de">Mark</trd>
  <trd lng="fr">mark<klr> (monnaie)</klr></trd>
  <trd lng="hu">m&aacute;rka</trd>
  <trd lng="nl">mark</trd>
  <trd lng="pt">marco</trd>
  <trd lng="vo">mak</trd>
</drv>
</art>

