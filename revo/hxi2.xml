<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
   <art mrk="$Id: hxi2.xml,v 1.11 2022/03/20 17:24:56 revo Exp $">
     <kap><rad>&Hcirc;i</rad>/o</kap>

<drv mrk="hxi2.Hx0o">
  <kap><tld/>o</kap>
  <snc>
     <uzo tip="fak">GEOG</uzo>
     <dif>
       Grekaj <ref tip="lst" cel="insul.0o" lst="voko:insuloj">insulo</ref>
       kaj <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref>, 
       apud <ref cel="anatoli.0o">Anatolio</ref>:
       <ekz>
         kaj de la Metileno &gcirc;is <tld/>o estas 100 verstoj;
         tie ku&scirc;as sankta martiro Izidoro; sur tiu insulo 
         naski&gcirc;as mastiko kaj bona vino kaj diversaj fruktoj
         <fnt>
           <aut>S. Pokrovskij</aut>:
           <vrk>Voja&gcirc;o en la Sanktan Landon</vrk>.
           <lok>LOdE, 1999, n-ro 61:11</lok>
         </fnt>.
       </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="hxio.0o">&hcirc;io (greka litero)</ref>  
   <trd lng="bg">&c_H;&c_i;&c_o;&c_s;</trd>
   <trd lng="de">Chios</trd>
   <trd lng="el">&Chi;&jota_ton;&omikron;&sigma_fina;</trd>
   <trd lng="en">Chios</trd>
   <trdgrp lng="fi">
	 <trd>Khios</trd>,
	 <trd>Hios</trd>
   </trdgrp>
   <trd lng="fr">Chios</trd>
   <trd lng="id">Khios</trd>
   <trd lng="pt">Khios</trd>
   <trd lng="ru">&c_H;&c_i;&c_o;&c_s;</trd>
   <trdgrp lng="it">
	 <trd>Chios</trd>, 
	 <trd>Chio</trd>
   </trdgrp>
  <trd lng="uk">&#x41A;&#x435;&#x44F;</trd>
</drv>

</art>

