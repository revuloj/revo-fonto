<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap><rad>&Hcirc;i</rad>/o</kap>

     <drv mrk="hxi2.Hx0o">
       <kap><tld/>o</kap>
       <snc>
  <uzo tip="fak">GEOG</uzo>
  <dif>
	   Grekaj insulo kaj urbo, apud Anatolio:
	   <ekz>
	     kaj de la Metileno &gcirc;is <tld/>o estas 100 verstoj;
	     tie ku&scirc;as sankta martiro Izidoro; sur tiu insulo
naski&gcirc;as
	     mastiko kaj bona vino kaj diversaj fruktoj
	     <fnt>
	       <aut>S. Pokrovskij</aut>:
	       <vrk>Voja&gcirc;o en la Sanktan Landon</vrk>.
	       <lok>LOdE, 1999, n-ro 61:11</lok>
	     </fnt>.
	   </ekz>
  </dif>
       </snc>
       <ref tip="hom" cel="hxi1.0">&hcirc;i</ref>
       <trd lng="bg">&c_H;&c_i;&c_o;&c_s;</trd>
       <trd lng="de">Chios</trd>
       <trd lng="el">&Chi;&jota_ton;&omikron;&sigma_fina;</trd>
       <trd lng="en">Chios</trd>
       <trdgrp lng="fi">
	 <trd>Khios</trd>,
	 <trd>Hios</trd>
       </trdgrp>
       <trd lng="pt">Khios</trd>
       <trd lng="ru">&c_H;&c_i;&c_o;&c_s;</trd>
     </drv>

   </art>

