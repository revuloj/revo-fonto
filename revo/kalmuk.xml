<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalmuk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kalmuk.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalmuk.0o.ETN">
    <dif>
      Ano de mongola popolo vivanta en Rusujo, Mongolujo kaj &ccirc;ina provinco
      Xinjiang:
      <ekz>
        anka&ubreve; la sova&gcirc;a <tld/>o kaj mongolo posedas forton
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 10a, p. 55a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Kalmouk<klr> (subst.)</klr></trd>
</drv>

<drv mrk="kalmuk.0a">
  <kap><tld/>a</kap>
  <snc mrk="kalmuk.0a.ETN">
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">kalmouk<klr> (adj.)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>