<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>kalmuk</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="kalmuk.0o">
      <kap><tld/>o</kap>
      <snc mrk="kalmuk.0o.ETN">
        <dif>
          Ano de <ref tip="super" cel="mongoled.0a">mongoleda</ref>
          popolo vivanta en Ruslando, Mongolujo kaj &Ccirc;inujo:
          <ekz>
            la Ruslandaj <tld/>oj estas la sola tradicie
            <ref cel="buda.0ismo">budaisma</ref> popolo de E&ubreve;ropo;
          </ekz><ekz>
            anka&ubreve; la sova&gcirc;a <tld/>o kaj mongolo posedas forton
            <fnt><bib>PatrojFiloj</bib>,
              <lok>&ccirc;apitro 10a, p. 55a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_l;&c_m;&c_y;&c_k;</trd>
      <trd lng="fr">Kalmouk<klr tip="ind"> (subst.)</klr></trd>
      <trd lng="ru">&c_k;&c_a;&c_l;&c_m;&c_y;&c_k;</trd>
    </drv>

    <drv mrk="kalmuk.0a">
      <kap><tld/>a</kap>
      <snc mrk="kalmuk.0a.ETN">
        <dif>
          Rilata al la <tld/>oj.
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_l;&c_m;&c_y;&c_c;&c_k;&c_ib;</trd>
      <trd lng="fr">kalmouk<klr tip="ind"> (adj.)</klr></trd>
      <trd lng="ru">&c_k;&c_a;&c_l;&c_m;&c_y;&c_c;&c_k;&c_i;&c_j;</trd>
    </drv>
  </art>

