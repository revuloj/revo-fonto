<drv mrk="salt.0o">
  <kap><tld/>o</kap>
  <snc mrk="salt.0o.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        sin prepari por <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Dano Holger</lok></fnt>;
      </ekz>
      <ekz>
        Ale&ccirc;jo per kelke da <tld/>oj transpasis la &scirc;tuparon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro raviteco li faris transkapi&gcirc;an <tld/>on
        <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        la &ccirc;evalo ektrotis rapide, post kelkaj <tld/>oj de&jcirc;etis
        sian rajdanton en foson
        <fnt><bib>ElektFab</bib>, <lok>feli&ccirc;a Jo&ccirc;jo</lok></fnt>;
      </ekz>
      <ekz>
        la pa&scirc;o estas tiel facila, <klr>[&#8230;]</klr> kiel la <tld/>o de publikulino al
        bigotulino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas mirinde, kiajn pensajn <tld/>ojn oni povas fari
        <fnt><bib>Fab2</bib>, <lok>Malnova strata
        lanterno</lok></fnt>;
      </ekz>
      <ekz>
        ne diru hop anta&ubreve; la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="saltomortal.0o">saltomortalo</ref>
  </snc>
  <trd lng="cs">skok</trd>
  <trdgrp lng="de">
    <trd>Sprung</trd>,
    <trd>Satz <klr>(Sprung)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jump</trd>,
    <trd>leap</trd>,
    <trd>spring</trd>
  </trdgrp>
  <trd lng="es">salto</trd>
  <trdgrp lng="fr">
    <trd>bond</trd>,
    <trd>saut</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>lompat</ind>an</trd>,
    <trd><ind>loncat</ind>an</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>salto</trd>,
    <trd>balzo</trd>
  </trdgrp>
  <trd lng="sk">go to - pr&iacute;kaz k nepodmiene&ccaron;n&eacute;mu skoku</trd>
  <trd lng="sv">hopp</trd>
</drv>

<drv mrk="salt.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.de0i.KOMUNE">
    <dif>
      <tld lit="S"/>i de io, de ie:
      <ekz>
        <klr>[&scirc;i]</klr> desaltis de la kamelo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:64</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> transrampis la muron, de<tld/>is sur la dua flanko
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li kuris tiel rapide, ke unu pantoflo de<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite de<tld/>is la kovrilo de la flartabakujo
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="las.for0i">forlasi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_s;&c_k;&c_o;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>seskakovat</trd>,
    <trd>sesko&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">abspringen</trd>
  <trdgrp lng="en">
    <trd>jump down</trd>,
    <trd>dismount</trd>
  </trdgrp>
  <trd lng="fr">sauter en bas</trd>
  <trd lng="hu">leugrik</trd>
  <trdgrp lng="it">
    <trd>saltare<klr> gi&ugrave;</klr></trd>,
    <trd>balzare<klr> gi&ugrave;</klr></trd>
  </trdgrp>
  <trd lng="nl">van iets <ind>afspringen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_k;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zosko&ccaron;i&tcaron;</trd>
  <trd lng="sv">hoppa ner</trd>
</drv>

<drv mrk="salt.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.dis0i.KOMUNE">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Rapide disrompi&gcirc;i:
      <ekz>
        kelkaj fenestrovitroj dis<tld/>is
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2003/007991.html">Espero</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> de&ccirc;enigis eksplodon, kiu dis<tld/>igis la supran protektoplaton
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2015/011522.php">La akcidento de &Ccirc;ernobilo 30 jarojn poste</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        dis<tld/>os la obstinaj baroj 
        <klr>[inter la popoloj]</klr>
        <fnt>(<aut>Zamenhof</aut>: 
        <vrk>La Espero</vrk>)</fnt>;
      </ekz>
      <ekz>
        dis<tld/>is eternaj montoj
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;abakuk 3:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frakas.0igxi">frakasi&gcirc;i</ref>,
      <ref cel="krev.0i">krevi</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_l;&c_je;&c_t;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_v; &c_p;&c_r;&c_a;&c_h;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ja;&c_c;&c_je;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;hle se odd&ecaron;lit</trd>,
    <trd>rozlomit se</trd>,
    <trd>rozpadnout se</trd>,
    <trd>rozsko&ccaron;it se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zerspringen</trd>,
    <trd>bersten</trd>
  </trdgrp>
  <trd lng="en">fly asunder</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;tugrik</trd>,
    <trd>sz&eacute;trep&uuml;l</trd>,
    <trd>sz&eacute;tt&ouml;rik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>saltare<klr> (rompersi)</klr></trd>,
    <trd>spezzarsi<klr> (con proiezione dei pezzi)</klr></trd>
  </trdgrp>
  <trd lng="nl">uit elkaar <ind>springen</ind></trd>
  <trdgrp lng="sk">
    <trd>rozbi&tcaron; sa</trd>,
    <trd>rozpadn&uacute;&tcaron; sa</trd>,
    <trd>rozsko&ccaron;i&tcaron;</trd>,
    <trd>roztrieska&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">flyga is&auml;r</trd>
</drv>

<drv mrk="salt.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.ek0i.KOMUNE">
    <dif>
      Subite <tld/>i:
      <ekz>
        ek<tld/>i de &gcirc;ojo <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a
        re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        ek<tld/>i de surprizo  <fnt><bib>F</bib>, <lok>Ekzercaro, &para;
        40</lok></fnt>;
      </ekz>
      <ekz>
        muelejo haltas, muelisto ek<tld/>as <fnt><bib>PrV</bib></fnt>
        <klr>(subite skui&gcirc;as pro forta emocio)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_k;&c_o;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vysko&ccaron;it z lehu</trd>,
    <trd>vysko&ccaron;it ze sedu</trd>
  </trdgrp>
  <trd lng="de">aufspringen</trd>
  <trd lng="en">start <klr>(jump from surprise)</klr></trd>
  <trdgrp lng="fr">
    <trd>faire un <ind>saut</ind></trd>,
    <trd>sursauter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felugrik</trd>,
    <trd>megugrik</trd>,
    <trd>ugrik egyet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>saltare</trd>,
    <trd>balzare</trd>, 
    <trd>sobbalzare</trd>, 
    <trd>schizzare<klr> (fig - saltare)</klr></trd>
  </trdgrp>
  <trd lng="nl">opspringen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_k;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vysko&ccaron;i&tcaron; zo sedu</trd>
  <trd lng="sv">hoppa till</trd>
</drv>

<drv mrk="salt.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.el0i.KOMUNE">
    <dif>
      Subite kaj rapide eliri a&ubreve; eli&gcirc;i:
      <ekz>
        hoko el<tld/>is, afero haltis<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kio el la dentoj el<tld/>as, en la lipoj ne haltas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ni ne povas el<tld/>i el nia propra ha&ubreve;to
        <klr>(naturo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_k;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vyskakovat</trd>,
    <trd>vysko&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">rausspringen</trd>
  <trd lng="en">jump out</trd>
  <trd lng="fr">sauter<klr> (hors de sa position)</klr></trd>
  <trd lng="hu">kiugrik</trd>
  <trdgrp lng="it">
    <trd>saltare<klr> via</klr></trd>, 
    <trd>schizzare<klr> via (fig.)</klr></trd>, 
    <trd>saltare<klr> fuori</klr></trd>
  </trdgrp>
  <trd lng="nl">uitspringen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_k;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyskakova&tcaron;</trd>,
    <trd>vysko&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">hoppa ut</trd>
</drv>

<drv mrk="salt.el0ulo">
  <kap>el<tld/>ulo</kap>
  <snc mrk="salt.el0ulo.novricxulo">
    <dif>
      Homo, kiu rapide ri&ccirc;i&gcirc;is a&ubreve; range plialti&gcirc;is;
      <ref tip="sin" cel="ricx.nov0ulo">novri&ccirc;ulo</ref>,
      <ref tip="sin" cel="parven.0o">parvenuo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_v;&c_e;&c_n;&c_ju;</trd>
  </trdgrp>
  <trd lng="cs">n&aacute;hl&yacute; zbohatl&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Empork&ouml;mmling</trd>,
    <trd>Parven&uuml;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>parvenu</trd>,
    <trd>upstart</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>parvenu</trd>,
    <trd>nouveau riche</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felkapaszkodott</trd>,
    <trd>parven&uuml;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>parvenu</trd>, 
    <trd>arrampicatore<klr> sociale</klr></trd>,
    <trd>arricchito<klr> (nuovo ricco)</klr></trd>
  </trdgrp>
  <trd lng="nl">parvenu</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_k;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_v;&c_je;&c_n;&c_ju;</trd>
  </trdgrp>
  <trd lng="sk">n&aacute;hly zbohatl&iacute;k</trd>
  <trdgrp lng="sv">
    <trd>parveny</trd>,
    <trd>uppkomling</trd>,
    <trd>nyrik</trd>
  </trdgrp>
</drv>

<drv mrk="salt.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="salt.en0i.KOMUNE">
    <dif>
      <tld lit="S"/>i al la interno de...:
      <ekz>
        tiu viro, kiu en<tld/>is la kanalon por savi infanon,
        ne unue demandis sin, al kies familio &scirc;i apartenas
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sauter dans</trd>
</drv>

<drv mrk="salt.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.for0i.KOMUNE">
    <dif>
      Rapide deklini&gcirc;i:
      <ekz>
        ne for<tld/>u de mia komisio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">usko&ccaron;it</trd>
  <trd lng="de">wegspringen</trd>
  <trdgrp lng="en">
    <trd>jump away</trd>,
    <trd>bound off</trd>
  </trdgrp>
  <trd lng="hu">elugrik</trd>
  <trdgrp lng="it">
   <trd>saltare<klr> via</klr></trd>, 
   <trd>saltare<klr> (essere rimosso dall'impiego)</klr></trd>
  </trdgrp>
  <trd lng="nl">wegspringen</trd>
  <trd lng="ru">&c_u;&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">odsko&ccaron;i&tcaron;</trd>
</drv>

<drv mrk="salt.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.re0i.KOMUNE">
    <dif>
      <tld lit="S"/>i a&ubreve; esti rapide resendita
      de iu loko al alia:
      <ekz>
        mi estas nur la centra punkto, en kiu kolekti&gcirc;as &ccirc;iuj
        gratuloj por
        re<tld/>i de tie al &ccirc;iuj flankoj de la mondo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zur&uuml;ckspringen</trd>
  <trdgrp lng="en">
    <trd>bounce</trd>,
    <trd>rebound</trd>,
    <trd>ricochet</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rebondir</trd>,
    <trd>ricocher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszaugrik</trd>,
    <trd>visszapattan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rimbalzare</trd>, 
    <trd>rimpallare<klr> (fig.)</klr></trd>,
  </trdgrp>
  <trd lng="nl">terugspringen</trd>
</drv>

<drv mrk="salt.trans0i">
  <kap>trans<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="salt.trans0i.KOMUNE">
    <dif>
      Transpasi <tld/>ante:
      <ekz>
        trans<tld/>i muron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trans<tld/>e senhalte
        <klr>(nur pasante)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ni povas trans<tld/>i <klr>(preterlasi)</klr> la sekvantan
        tagon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_k;&c_o;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;eskakovat</trd>,
    <trd>p&rcaron;esko&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">&uuml;berspringen</trd>
  <trdgrp lng="en">
    <trd>jump across</trd>,
    <trd>jump over</trd>
  </trdgrp>
  <trd lng="fr">sauter par-dessus</trd>
  <trd lng="hu">&aacute;tugrik</trd>
  <trdgrp lng="it">
    <trd>saltare<klr> oltre</klr></trd>,
    <trd>balzare<klr> oltre</klr></trd>, 
    <trd>scavalcare</trd>, 
  </trdgrp>
  <trdgrp lng="nl">
    <trd>overspringen</trd>,
    <trd>overslaan</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_r;&c_y;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_k;&c_a;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_k;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">presko&ccaron;i&tcaron;</trd>
</drv>
