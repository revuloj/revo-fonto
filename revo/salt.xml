<drv mrk="salt.0o">
  <kap><tld/>o</kap>
  <snc mrk="salt.0o.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        sin prepari por <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Dano Holger</lok></fnt>;
      </ekz>
      <ekz>
        Ale&ccirc;jo per kelke da <tld/>oj transpasis la &scirc;tuparon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro raviteco li faris transkapi&gcirc;an <tld/>on
        <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        la &ccirc;evalo ektrotis rapide, post kelkaj <tld/>oj de&jcirc;etis
        sian rajdanton en foson
        <fnt><bib>ElektFab</bib>, <lok>feli&ccirc;a Jo&ccirc;jo</lok></fnt>;
      </ekz>
      <ekz>
        la pa&scirc;o estas tiel facila, <klr>[&#x2026;]</klr> kiel la <tld/>o de publikulino al
        bigotulino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas mirinde, kiajn pensajn <tld/>ojn oni povas fari
        <fnt><bib>Fab2</bib>, <lok>Malnova strata
        lanterno</lok></fnt>;
      </ekz>
      <ekz>
        ne diru hop anta&ubreve; la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="saltomortal.0o">saltomortalo</ref>
  </snc>
  <trd lng="cs">skok</trd>
