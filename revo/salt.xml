<drv mrk="salt.trans0i">
  <kap>trans<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="salt.trans0i.KOMUNE">
    <dif>
      Transpasi <tld/>ante:
      <ekz>
        mi trans<tld/>as muron
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi punos <klr>[&#x2026;]</klr> &ccirc;iujn, kiuj trans<tld/>as la sojlon
        <fnt>
          <bib>MT</bib>
          <lok>Cefanja 1:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grandaj nigraj okuloj de la knabineto trans<tld/>adis jen sur la lumitan
        profundon de la kameno, jen sur la pecon da pano
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nu, la sekvantan tagon ni povas trans<tld/>i
        <klr>(<ref cel="las.preter0i">preterlasi</ref>)</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        trans<tld/>e senhalte
        <klr>(nur pasante)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_k;&c_o;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;eskakovat</trd>,
    <trd>p&rcaron;esko&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">&uuml;berspringen</trd>
  <trdgrp lng="en">
    <trd>jump across</trd>,
    <trd>jump over</trd>
  </trdgrp>
