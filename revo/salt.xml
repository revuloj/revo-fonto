<drv mrk="salt.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.re0i.KOMUNE">
    <dif>
      <tld lit="S"/>i a&ubreve; esti rapide resendita
      de iu loko al alia:
      <ekz>
        bastono batas, bastono re<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sago &ccirc;i tiun fojon ekbatis la akvon kaj re<tld/>inte kelke da fojoj de &gcirc;ia
        supra&jcirc;o, malaperis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la potenca metala sono trakuris la kortojn, re<tld/>is de la dikaj muroj de l'
        templo kaj forflugis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas nur la centra punkto, en kiu kolekti&gcirc;as &ccirc;iuj
        gratuloj por
        re<tld/>i de tie al &ccirc;iuj flankoj de la mondo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="reflek.0i">reflekti</ref>&gcirc;i,
       <ref cel="pusx.re0i">repu&scirc;i</ref>&gcirc;i
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_s;&c_k;&c_o;&c_k;&c_v;&c_a;&c_c;&c_mol;
  <klr>(&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;
  &c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>zur&uuml;ck <ind>springen</ind></trd>,
    <trd>abprallen</trd>,
    <trd><ind>zur&uuml;ck</ind> geworfen werden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bounce</trd>,
    <trd>rebound</trd>,
    <trd>ricochet</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rebondir</trd>,
    <trd>ricocher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszaugrik</trd>,
    <trd>visszapattan</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>ambul</baz>mengambul</trd>,
    <trd><baz>anjal</baz>menganjal</trd>,
    <trd><baz>lenting</baz>melenting</trd>,
    <trd>membal</trd>,
    <trd><baz>pantul</baz>memantul</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rimbalzare</trd>,
    <trd>rimpallare<klr> (fig.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36339;&#12397;&#36820;&#12427;  <pr>&#12399;&#12397;&#12363;&#12360;&#12427;</pr></trd>,
    <trd>&#12392;&#12403;&#30452;&#12377;  <pr>&#12392;&#12403;&#12394;&#12362;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">terugspringen</trd>
