<drv mrk="salt.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.el0i.KOMUNE">
    <dif>
      Subite kaj rapide eliri a&ubreve; eli&gcirc;i:
      <ekz>
        Niko el<tld/>is tra la pordo
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li el<tld/>is el la lito
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;oj kun ar&gcirc;entaj kaj oraj skvamoj <klr>[&#x2026;]</klr> el<tld/>is super la akvon
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la korko brue el<tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        se el<tld/>as la okazo, &gcirc;i rompi&gcirc;as kiel vazo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kio el la dentoj el<tld/>as, en la lipoj ne haltas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vizitantoj preska&ubreve; el<tld/>as el sia ha&ubreve;to, kiam ni faras tiun bruon
        <fnt>
          <bib>KKl</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ir.el0i">eliri</ref>,
      <ref cel="las.for0i">forlasi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_k;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vyskakovat</trd>,
    <trd>vysko&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">heraus <ind>springen</ind></trd>
  <trd lng="en">jump out</trd>
  <trd lng="fr">sauter<klr> (hors de sa position)</klr></trd>
  <trd lng="hu">kiugrik</trd>
  <trdgrp lng="id">
    <trd><baz>lompat</baz>melompat keluar</trd>,
