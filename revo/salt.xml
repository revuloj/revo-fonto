    <trd>wyrywa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_k;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyskakova&tcaron;</trd>,
    <trd>vysko&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">hoppa ut</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43F;&#x43B;&c_i;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
