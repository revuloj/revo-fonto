<drv mrk="salt.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="salt.en0i.KOMUNE">
    <dif>
      <tld lit="S"/>i al la interno de...:
      <ekz>
        tiu viro, kiu en<tld/>is la kanalon por savi infanon,
        ne unue demandis sin, al kies familio &scirc;i apartenas
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sauter dans</trd>
</drv>

<drv mrk="salt.for0i">
