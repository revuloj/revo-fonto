    <trd>&#x43F;&c_je;&c_r;&c_je;&c_s;&c_t;&c_r;&c_i;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43F;&#x43B;&c_i;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_s;&#x43A;&c_a;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
