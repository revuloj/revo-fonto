  <snc mrk="salt.en0i.KOMUNE">
    <dif>
      <tld lit="S"/>i al la interno de...:
      <ekz>
        tra la fenestro <klr>[&#x2026;]</klr> en<tld/>is malbela bufo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj en<tld/>is en la boaton
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        pla&ubreve;! &scirc;i en<tld/>is en la akvon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekkonu anta&ubreve;e la profundecon de la abismo, anta&ubreve; ol vi en<tld/>as en &gcirc;in
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>!
      </ekz>
      <ekz>
        tiu viro, kiu en<tld/>is la kanalon por savi infanon,
        ne unue demandis sin, al kies familio &scirc;i apartenas
        <fnt>
          <aut>J. Francis</aut>,
          <vrk><url ref="http://novajhoj.weebly.com/uploads/8/2/9/5/8295099/la_granda_kaldrono_-_john_francis.pdf">La
          Granda Kaldrono</url></vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">hinein <ind>springen</ind></trd>
  <trd lng="fr">sauter dans</trd>
  <trdgrp lng="id">
    <trd><baz>lompat</baz>melompat masuk</trd>,
    <trd><baz>loncat</baz>meloncat masuk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wskakiwa&cacute;</trd>,
    <trd>wskoczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_s;&#x43A;&c_a;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_r;&c_i;&c_b;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_r;&c_i;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_d;&#x43E; &c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&#x43D;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8DF3;&#x8FDB; <pr>ti&#xE0;oj&#xEC;n</pr></trd>,
    <trd>&#x8DF3;&#x9032; <pr>ti&#xE0;oj&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="salt.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="salt.for0i.KOMUNE">
    <dif>
      Rapide <ref tip="sin" cel="fugx.0i">fu&gcirc;i</ref>:
      <ekz>
        hu!, li ekkriis kaj for<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la leono nur unu fojon ekblekis, kaj &ccirc;iuj for<tld/>is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        laboro ne estas leporo &dash; &gcirc;i haltos, ne for<tld/>os
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        metu kelkan ordon en vian paroladon kaj ne for<tld/>u
        <klr>(rapide <ref cel="devi.0i">deviu</ref>)</klr>
        de mia komisio
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">usko&ccaron;it</trd>
  <trd lng="de">wegspringen</trd>
