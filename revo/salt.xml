  <snc mrk="salt.for0i.KOMUNE">
    <dif>
      Rapide deklini&gcirc;i:
      <ekz>
        ne for<tld/>u de mia komisio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">usko&ccaron;it</trd>
  <trd lng="de">wegspringen</trd>
  <trdgrp lng="en">
    <trd>jump away</trd>,
    <trd>bound off</trd>
  </trdgrp>
  <trd lng="hu">elugrik</trd>
