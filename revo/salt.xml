  <trd lng="hu">&aacute;tugrik</trd>
  <trdgrp lng="id">
    <trd><baz>lompat</baz>melompat ke seberang</trd>,
    <trd><baz>loncat</baz>meloncat ke seberang</trd>,
    <trd><baz>lompat</baz>melompati</trd>,
    <trd><baz>loncat</baz>meloncati</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>saltare<klr> oltre</klr></trd>,
    <trd>balzare<klr> oltre</klr></trd>,
    <trd>scavalcare</trd>,
  </trdgrp>
  <trd lng="ja">&#x8DF3;&#x3073;&#x8D8A;&#x3059;  <pr>&#x3068;&#x3073;&#x3053;&#x3059;</pr></trd>
  <trdgrp lng="nl">
    <trd>overspringen</trd>,
    <trd>overslaan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przeskakiwa&cacute;</trd>,
    <trd>pomija&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_r;&c_y;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_k;&c_a;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_k;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">presko&ccaron;i&tcaron;</trd>
