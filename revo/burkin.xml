<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>burkin</rad>/o
</kap>

<drv mrk="burkin.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="burkin.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt" 
      cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Vagaduguo.
      Iam nomata Supra Volto.
      Teritorio de Respubliko Burkino.
      ISO-3166 kodo: BF.
      &Gcirc;i limas kun
      <ref tip="vid" cel="benin.B0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="ebur.E0Bordo.GEOG">Ebur-Bordo</ref>,
      <ref tip="vid" cel="gana.G0o.GEOG">Ganao</ref>,
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref>,
      <ref tip="vid" cel="nigxer.N0o.GEOG">Ni&gcirc;ero</ref>
      kaj <ref tip="vid" cel="togo.T0olando.GEOG">Togolando</ref>.
    </dif>
    <trd lng="bg">&c_B;&c_u;&c_r;&c_k;&c_i;&c_n;&c_a; &c_F;&c_a;&c_s;&c_o;</trd>
    <trd lng="de">Burkina Faso</trd>
    <trd lng="en">Burkina Faso</trd>
    <trdgrp lng="pl">
      <trd>Burkina Faso</trd>
    </trdgrp>
  </snc>
</drv>

</art>

