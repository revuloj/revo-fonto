<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>burkin</rad>/o
</kap>

<drv mrk="burkin.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="burkin.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt" 
      cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Vagaduguo.
      Iam nomata Supra Volto.
      Teritorio de Respubliko Burkino.
      ISO-3166 kodo: BF.
      &Gcirc;i limas kun
      <ref tip="vid" cel="benin.0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="ebur.0bordo.GEOG">Ebur-Bordo</ref>,
      <ref tip="vid" cel="gana.0o.GEOG">Ganao</ref>,
      <ref tip="vid" cel="mali.0o.GEOG">Malio</ref>,
      <ref tip="vid" cel="nigxer.0o.GEOG">Ni&gcirc;ero</ref>
      kaj <ref tip="vid" cel="togo.0olando.GEOG">Togolando</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Burkina Faso</trd>
    </trdgrp>
  </snc>
</drv>

</art>

