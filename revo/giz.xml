<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: giz.xml,v 1.4 2023/11/11 11:36:35 afido Exp $">
    <kap>
      <rad>Giz</rad>/o
    </kap>

<drv mrk="giz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="egipt.E0ujo">Egiptujo</ref>,
      situanta &ccirc;e la okcidenta bordo de <ref cel="nil.N0o">Nilo</ref>,
      &ccirc; 20&#x202F;km de la centro de <ref cel="kair.K0o">Kairo</ref>:
      <ekz>
        homoj ofte miras anta&ubreve; projektoj kiel la <ref tip="malprt" cel="pirami.0o.ARKI">Piramidoj</ref>
        de <tld/>o
        <fnt>
          <vrk><url ref="https://priioajn.wordpress.com/2019/10/28/mi-miras-pro-la-usona-sosea-sistemo/">Mi miras pro la usona &scirc;osea sistemo &dash; Pri io ajn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Piramidoj de <tld/>o, parto de <ref tip="prt" cel="nekrop.0o">Nekropolo</ref>
        de <tld/>o, estas inter la plej grandaj kaj antikvaj konstrua&jcirc;oj el historio
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Nekropolo_de_Gizo">Nekropolo de Gizo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_ib;&c_z;&c_a;</trd>
    <trd lng="ar">&a_A;&a_l;&a_j;&a_y;&a_z;&a_t_marbuta;</trd>
    <trd lng="de">Gizeh</trd>
    <trd lng="en">Giza</trd>
    <trd lng="fr">Gizeh</trd>
  </snc>
  <trd lng="zh">&#x5409;&#x8428; <pr>j&#xED;s&#xE0;</pr></trd>
</drv>

  </art>

