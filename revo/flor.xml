<drv mrk="flor.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flor.ek0i.komenci">
    <dif>
      Komenci <tld/>i:
      <ekz>
        ek<tld/>os la migdalarbo
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 12:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arboj ek<tld/>is duan fojon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; &gcirc;i ek<tld/>is, tuj aperis sur &gcirc;i beraroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 40:10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flor.ek0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Montri&gcirc;i, ekesti:
      <ekz>
        nova generacio ek<tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fiero ek<tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 7:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas infekta&jcirc;o de lepro, &gcirc;i ek<tld/>is en la absceso
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia afero &ccirc;i tie ek<tld/>os ne malpli potence, ol en la aliaj
        grandaj landoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden
          en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">rozkv&eacute;st</trd>
  <trdgrp lng="de">
    <trd>erbl&uuml;hen</trd>,
    <trd>aufbl&uuml;hen</trd>
  </trdgrp>
  <trd lng="es">florecer<klr tip="ind"> (intr., empezar a
  florecer)</klr></trd>
  <trd lng="fr">fleurir<klr tip="ind"> (intr., se mettre &agrave;
