      <tld/>oj, kies ses egallongaj elipsaj petaloj havas flavan a&ubreve;
      verdan makulon &ccirc;e la pinto, <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">specio</ref> el familio
      <ref tip="super" cel="amaril1.0oj">amarilidacoj</ref>
      (<trd lng="la">Leucoium vernum</trd>).
    </dif>
  </snc>
  <bld lok="../bld/martflor.jpg">
    marto<tld/>oj
  </bld>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_a;</trd>
  <trdgrp lng="de">
    <trd>M&auml;rzenbecher</trd>,
    <trd>Fr&uuml;hlingsknotenblume</trd>.
  </trdgrp>
  <trd lng="en">spring snowflake</trd>
  <trd lng="fr">niv&eacute;ole</trd>
  <trd lng="hu">tavaszi <ind>t&odblac;zike</ind></trd>
