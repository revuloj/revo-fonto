  </trdgrp>
  <trd lng="zh">&#x96EA;&#29255; <pr>xu&#x11B;pi&#xE0;n</pr></trd>
</drv>

<drv mrk="flor.negxo0o">
  <kap>ne&gcirc;o<tld/>o, <var><kap>ne&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="flor.negxo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      de <ref tip="super" cel="edelve.0o">edelvejso</ref>
      (<trd lng="la">Leontopodium alpinum</trd>).
    </dif>
    <rim mrk="flor.negxo0o.RIM_SNC">
      La sama popola nomo aperas en naciaj lingvoj por diversaj
      vintrofinaj floroj, ekzemple anka&ubreve; por <ref tip="vid" cel="galant.0o">galanto</ref> (normale
      <em>ne&gcirc;borulo</em>):
        <ekz>
          ne&gcirc;<tld/>o, kiu timeme montris sin tra la ne&gcirc;a vualo
          <fnt>
            <aut>Julian Modest</aut>
            <vrk>En la taksio</vrk>
          </fnt>.
        </ekz>
    </rim>
    <trd lng="be">&c_e;&c_d;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_j;&c_s;</trd>
    <trd lng="de">Edelwei&szlig;</trd>
    <trd lng="fr">edelweiss</trd>
    <trdgrp lng="es">
      <trd>flor de las nieves</trd>,
      <trd>edelweiss</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&lamed;&bet;&nun;&bet;&fnun;<klr tip="ind">
      (&alef;&dalet;&lamed;&vav;&vav;&yod;&samekh;)</klr></trd>,
      <trd>&alef;&dalet;&lamed;&vav;&vav;&yod;&samekh;</trd>
    </trdgrp>
    <trd lng="id">edelweis</trd>
    <trd lng="hu">gyop&aacute;r</trd>
    <trd lng="nl">sneeuwklokje</trd>
    <trd lng="ru">&c_e;&c_d;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_j;&c_s;</trd>
    <trdgrp lng="zh">
    <trd>&#x706B;&#x7D68;&#33609; <pr>hu&#x1D2;r&#xF3;ngc&#x1CE;o</pr></trd>,
    <trd>&#x9AD8;&#x5C71;&#x706B;&#x7D68;&#33609; <pr>g&amacron;osh&amacron;nhu&#x1D2;r&#xF3;ngc&#x1CE;o</pr></trd>,
    <trd>&#x96EA;&#x7D68;&#x82B1; <pr>xu&#x11B;r&#xF3;nghu&amacron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="flor.negxo0o.prujno">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="prujn.0o">Prujno</ref> florsimila,
      kian lasas frosti&gcirc;inta pluvo a&ubreve; ne&gcirc;o sur fenestra vitro:
      <ekz>
        la homoj sidas hejme kaj &gcirc;uas la varmon kaj rigardas la ne&gcirc;o<tld/>ojn sur la
        fenestro
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne estas multe da prujno, <klr>[&#8230;]</klr> herbo <klr>[&#8230;]</klr> preska&ubreve; kiel
        virinhararo, tiel mola kaj kun ne&gcirc;<tld/>o sur si
        <fnt>
          <bib>VRA</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_n;&c_y; &c_w;&c_z;&c_o;&c_r; <klr>(&c_n;&c_a; &c_v;&c_a;&c_k;&c_n;&c_je; &c_ib; &c_d;&c_a; &c_g;. &c_p;.)</klr></trd>
    <trd lng="de">Eisblume</trd>
    <trd lng="en">window frost</trd>
    <trd lng="ru">&c_i;&c_n;&c_je;&c_je;&c_v;&c_y;&c_je; &c_c;&c_v;&c_je;&c_t;&c_y;</trd>
  </snc>
  <trd lng="en">edelweiss</trd>
  <trd lng="id">edelweis</trd>
  <trd lng="ja">&#x30A8;&#x30FC;&#x30C7;&#x30EB;&#x30EF;&#x30A4;&#x30B9;</trd>
  <trd lng="uk">&c_je;&c_d;&c_je;&#x43B;&#x44C;&c_v;&c_je;&c_j;&c_s;</trd>
</drv>

<drv mrk="flor.sun0o">
  <kap><ofc>*</ofc>sun<tld/>o</kap>
  <snc mrk="flor.sun0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta herba vegeta&jcirc;o kun granda flava floraro,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Helianthus</trd>):
      <ekz>
        <tld/>oj, havantaj la grandecon de sun<tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sun<tld/>oj havis tute &scirc;velintajn kapojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        maturaj sun<tld/>oj ne turnas sin al la suno, nur la nematuraj
        <fnt>
          <aut>Ste&ccirc;jo</aut>,
          <vrk>Sunfloroj</vrk>,
          <lok><url ref="http://www.esperanto.ca/files/halifax/inter_ni_2006_07.pdf">
          Inter Ni, 2006-07</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="helian.0o.BOT">helianto</ref>
    <bld lok="&WCU;/thumb/a/a9/A_sunflower.jpg/360px-A_sunflower.jpg" prm="Public
    domain"><ind>sun<tld/>o</ind> <klr>(Helianthus
    annuus)</klr>
         <fnt>
           <lok><url ref="&WCW;/File:A_sunflower.jpg">&WCW;/File:A_sunflower.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_n;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_s;&c_l;&c_malmol;&c_n;&c_ch;&c_o;&c_g;&c_l;&c_je;&c_d;</trd>
  <trd lng="de">Sonnenblume</trd>
  <trd lng="en">sunflower</trd>
  <trd lng="es">girasol</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;lianthe</trd>,
    <trd>tournesol</trd>
  </trdgrp>
  <trd lng="he">&het;&mem;&nun;&yod;&tav;</trd>
  <trd lng="hu">napraforg&oacute;</trd>
  <trd lng="id"><ind>bunga</ind> matahari</trd>
  <trd lng="ja">&#x30D2;&#x30DE;&#x30EF;&#x30EA;</trd>
  <trd lng="nl">zonnebloem</trd>
  <trd lng="pl">s&lstroke;onecznik</trd>
  <trd lng="ro">floarea-soarelui</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_s;&c_o;&c_l;&c_n;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">solros</trd>
  <trd lng="tr">ay&ccedil;i&ccedil;e&gbreve;i</trd>
  <trd lng="uk">&c_s;&#x43E;&#x43D;&#x44F;&c_sh;&#x43D;&c_i;&#x43A;</trd>
  <trd lng="vo">solaflol</trd>
  <trd lng="sk">kamzi&ccaron;n&iacute;k</trd>
  <trdgrp lng="cs">
    <trd>heliant</trd>,
    <trd>slune&ccaron;nice</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33909;&#x82B1; <pr>ku&#xED;hu&amacron;</pr></trd>,
    <trd>&#33909; <pr>ku&#xED;</pr></trd>,
    <trd>&#x8F49;&#x65E5;&#x84EE; <pr>zhu&#x1CE;nr&#xEC;li&#xE1;n</pr></trd>,
    <trd>&#21521;&#38451;&#x82B1; <pr>xi&#xE0;ngy&#xE1;nghu&amacron;</pr></trd>,
    <trd>&#x592A;&#38451;&#x82B1; <pr>t&#xE0;iy&#xE1;nghu&amacron;</pr></trd>,
