<drv mrk="flor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flor.0i.naski">
    <dif>
      Naski <tld/>ojn <sncref ref="flor.0o.BOT"/>:
      <ekz>
        ekfloros kiel lilio
        <fnt><bib>MT</bib>, <lok>&Jes; 34:17</lok></fnt>;
      </ekz>
      <ekz>
        la vegeta&jcirc;oj <tld/>as en printempo.
      </ekz>
    </dif>
    <trdgrp lng="id">
      <trd><baz>bunga</baz>berbunga</trd>,
      <trd>mekar</trd>
    </trdgrp>
  </snc>
  <snc mrk="flor.0i.prosperi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brile prosperi:
      <ekz>
        &gcirc;i forte ekfloros kaj triumfos, triumfos kaj kantos
        <fnt><bib>MT</bib>, <lok>&Jes; 35:2</lok></fnt>;
      </ekz>
      <ekz>
        vivu, kresku kaj <tld/>u nia afero
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen
          en la 21a de a&ubreve;gusto 1911</vrk>
        </fnt>!
      </ekz>
      <ekz>
        <ind><tld/>anta</ind> sano, a&gcirc;o, viza&gcirc;o, knabina kapo;
        <trd lng="fr">florissant</trd>
        <trd lng="es">floreciente</trd>
        <trd lng="hu">vir&aacute;gz&oacute;</trd>
        <trd lng="nl">florissant</trd>
        <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_u;&c_shch;&c_i;&c_j;
        </trd>
      </ekz>
      <ekz>
        en la <tld/>antaj tagoj de Romo;
      </ekz>
      <ekz>
        la artoj longe <tld/>adis en Grekujo;
      </ekz>
      <ekz>
        la komerco <tld/>as;
      </ekz>
      <ekz>
        la gajeco de l' vivo en mi neniam plu re<tld/>os
        <fnt>
          <aut>J. W. von Goethe, trad. L. L. Zamenhof</aut>
          <vrk><url
          ref="http://karavelo.net/html/pdf/zamenhofo_trad_goethe_ifigenio_en_taurido.pdf">Ifigenio
          en Ta&ubreve;rido</url></vrk>
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flor.0i.disvolvigxi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disvolvi&gcirc;i simile al <tld/>oj:
      <ekz>
        se la lepro disfloros sur la ha&ubreve;to kaj la lepro
        kovros la tutan ha&ubreve;ton
        <fnt><bib>MT</bib>, <lok>&Lev; 13:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_c;&c_malmol;&c_f;&c_t;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_c;&c_malmol;&c_f;&c_t;&c_ja;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kv&eacute;st</trd>,
    <trd>vzkv&eacute;tat</trd>
  </trdgrp>
  <trd lng="de">bl&uuml;hen</trd>
  <trdgrp lng="en">
    <trd>flower</trd>,
    <trd>bloom</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>florear</trd>,
    <trd>florecer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fleurir<klr tip="ind"> (&ecirc;tre en fleurs)</klr></trd>,
    <trd>&ecirc;tre en <ind>fleurs</ind></trd>,
    <trd>cro&icirc;tre</trd>,
    <trd>s'<ind>&eacute;panouir</ind><klr tip="ind"> (fleurir)</klr>
    </trd>,
    <trd>prosp&eacute;rer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vir&aacute;gzik</trd>,
    <trd>kivir&aacute;gzik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33457;&#12364;&#21682;&#12367;  <pr>&#12399;&#12394;&#12364;&#12373;&#12367;</pr></trd>,
    <trd>&#38283;&#33457;&#12377;&#12427;  <pr>&#12363;&#12356;&#12363;&#12377;&#12427;</pr></trd>,
    <trd>&#33457;&#38283;&#12367;  <pr>&#12399;&#12394;&#12402;&#12425;&#12367;</pr></trd>,
    <trd>&#26628;&#12360;&#12427;  <pr>&#12399;&#12360;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">bloeien</trd>
  <trdgrp lng="pl">
