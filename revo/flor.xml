  </trdgrp>
</drv>

<drv mrk="flor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flor.0o.planto">
    <dif>
      Iu el <ref tip="malprt" cel="flor.0oplant"><tld/>oplantoj
      </ref>:
      <ekz>
        <ind><tld/>poto</ind>;
        <trd lng="fr">pot de fleurs</trd>
        <trd lng="es">maceta</trd>
        <trd lng="hu">vir&aacute;gcser&eacute;p</trd>
        <trd lng="nl">bloempot</trd>
        <trd lng="pl">wazon</trd>
        <trd lng="ru">
        &c_c;&c_v;&c_je;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;
        &c_g;&c_o;&c_r;&c_sh;&c_o;&c_k;</trd>
      </ekz>
      <ekz>
        anta&ubreve; la pordego de la granda palaco <klr>[&#8230;]</klr>
        trovi&gcirc;as la bela &gcirc;ardeno kun la multo da <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Floroj de la malgranda Ida</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Blume</trd>
    <trd lng="nl">bloem</trd>
    <trd lng="pt">flor</trd>
  </snc>
  <snc mrk="flor.0o.plantparto">
    <dif>
      Okulfrapa, kutime kolora, parto de planto,
      kun <tld/>oj <sncref ref="flor.0o.BOT"/>:
      <ekz>
        bukedo da <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="fak">BELA</uzo>
        <ind><tld/>desegna</ind> &scirc;tofo.
        <trd lng="fr">fleuri<klr tip="ind"> (avec des motifs
        floraux)</klr></trd>
        <trd lng="hu">vir&aacute;gmint&aacute;s</trd>
        <trd lng="nl">gebloemd</trd>
        <trd lng="pt">flor</trd>
      </ekz>
    </dif>
    <trd lng="de">Blume</trd>
    <trd lng="nl">bloem</trd>
  </snc>
  <snc mrk="flor.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="flor.0oplant">
      <tld/>oplantoj</ref>
      kun la reproduktaj organoj:
      <ekz>
        kaj lia floro defalos kiel &ccirc;e olivarbo
        <fnt><bib>MT</bib>, <lok>&Ijo; 15:33</lok></fnt>;
      </ekz>
      <ekz>
        kiam la florado fini&gcirc;is kaj la floro fari&gcirc;is
        matura bero, Li detran&ccirc;os la bran&ccirc;ojn per
        tran&ccirc;iloj kaj deprenos la vinberbran&ccirc;ojn
        <fnt><bib>MT</bib>, <lok>&Jes; 18:5</lok></fnt>;
      </ekz>
      <ekz>
        ni rigardu, &ccirc;u la vinbertrunko jam bur&gcirc;onis,
        &ccirc;u &gcirc;iaj floroj jam malfermi&gcirc;is
        <fnt><bib>MT</bib>, <lok>&Alt; 7:12</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;i <klr>[&#8230;]</klr> donadis <klr>(naskis)</klr> siajn ru&gcirc;ajn
        bonodorajn <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/flor.png">
      floropartoj
    </bld>
    <refgrp tip="prt">
      1. <ref cel="stilus.0o">stiluso</ref>,
      2. <ref cel="ovari.0o.BOT">ovario</ref>,
      3. florfundo,
      4. <ref cel="stamen.0o">stameno</ref>,
      5. <ref cel="korol.0afolio">korola folio</ref> (<ref cel="petal.0o">petalo</ref>),
      6. <ref cel="stigm.0o">stigmo</ref>,
      7. <ref cel="pistil.0o">pistilo</ref>
      8. <ref cel="kalik.0afolio">kalika folio</ref> (<ref cel="sepal.0o">sepalo</ref>)
      9. flortigo (<ref cel="pedunkl.0o">pedunklo</ref>, pedicelo)
    </refgrp>
    <trd lng="bg">&c_c;&c_malmol;&c_f;&c_t;&c_je;&c_zh;</trd>
    <trd lng="de">Bl&uuml;te</trd>
    <trd lng="nl">bloem</trd>
    <trd lng="pt">flor</trd>
    <trdgrp lng="zh">
    <trd>&#24320;&#33457;&#26399; <pr>k&amacron;ihu&amacron;q&imacron;</pr></trd>,
    <trd>&#20852;&#26106;&#26102;&#26399; <pr>x&imacron;ngw&agrave;ngsh&iacute;q&imacron;</pr></trd>,
    <trd>&#33457;&#26399; <pr>hu&amacron;q&imacron;</pr></trd>,
    <trd>&#20840;&#30427;&#26102;&#26399; <pr>qu&aacute;nch&eacute;ngsh&iacute;q&imacron;</pr></trd>,
    <trd>&#33457;&#26421; <pr>hu&amacron;du&#466;</pr></trd>,
    <trd>&#33457; <pr>hu&amacron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="flor.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="F"/>ado, la plej bela kaj brila periodo de io:
      <ekz>
        en la <tld/>o de la juneco, de beleco, de siaj
        jaroj;
      </ekz>
      <ekz>
        la amo bezonas liberecon kaj tempon por doni siajn plej belajn
        <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bl&uuml;te</trd>
    <trdgrp lng="nl">
      <trd>fleur</trd>,
      <trd>bloei</trd>
      <trd lng="pt">flor</trd>
    </trdgrp>
    <trd lng="ru">&c_c;&c_v;&c_je;&c_t;</trd>
  </snc>
  <trd lng="af">blom</trd>
  <trd lng="am">&#4768;&#4704;&#4707;</trd>
  <trd lng="ar">&a_z;&a_h;&a_r;&a_t_marbuta;</trd>
