<drv mrk="flor.maj0o">
  <kap>maj<tld/>o</kap>
  <snc mrk="flor.maj0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herba vegeta&jcirc;o kun bonodoraj sonoriloformaj floretoj, venena,
