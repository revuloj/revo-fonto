<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>flor</rad>/o
</kap>
<drv mrk="flor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flor.0o.planto">
    <dif>
      Iu el <ref tip="malprt" cel="flor.0oplantoj"><tld/>oplantoj</ref>:
      <ekz>
        <ind>maj<tld/>o</ind>;
        <trd lng="fr">muguet<klr tip="ind"> (fleur)</klr></trd>
        <trd lng="hu">gy&ouml;ngyvir&aacute;g</trd>
        <trd lng="nl">meiklokje</trd>
      </ekz>
      <ekz>
        <ind>sun<tld/>o</ind>;
        <trd lng="es">girasol</trd>
        <trd lng="fr">tournesol</trd>
        <trd lng="hu">napraforg&oacute;</trd>
        <trd lng="nl">zonnebloem</trd>
      </ekz>
      <ekz>
        <ind><tld/>poto</ind>;
        <trd lng="fr">pot de fleurs</trd>
        <trd lng="hu">vir&aacute;gcser&eacute;p</trd>
        <trd lng="nl">bloempot</trd>
      </ekz>
      <ekz>
        anta&ubreve; la pordego de la granda palaco kie trovi&gcirc;as
        la bela &gcirc;ardeno kun la multo da floroj
        <fnt><bib>Fab1</bib>, <lok>Floroj de la malgranda Ida</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Blume</trd>
    <trd lng="nl">bloem</trd>
  </snc>
  <snc mrk="flor.0o.plantparto">
    <dif>
      Okulfrapa, kutime kolora, parto de planto,
      kun <tld/>o(j) <sncref ref="flor.0o.BOT"/>:
      <ekz>
        bukedo da <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="fak">BELA</uzo>
        <ind><tld/>desegna</ind> &scirc;tofo.
        <trd lng="fr">fleuri<klr tip="ind"> (avec des motifs
        floraux)</klr></trd>
        <trd lng="hu">vir&aacute;gmint&aacute;s</trd>
        <trd lng="nl">gebloemd</trd>
      </ekz>
    </dif>
    <trd lng="de">Blume</trd>
    <trd lng="nl">bloem</trd>
  </snc>
  <snc mrk="flor.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="flor.0oplantoj"><tld/>oplantoj</ref>
      kun la reproduktaj organoj:
      <ekz>
        kaj lia floro defalos kiel &ccirc;e olivarbo
        <fnt><bib>MT</bib>, <lok>&Ijo; 15:33</lok></fnt>;
      </ekz>
      <ekz>
        kiam la florado fini&gcirc;is kaj la floro fari&gcirc;is matura bero,
        Li detran&ccirc;os la bran&ccirc;ojn per tran&ccirc;iloj kaj deprenos
        la vinberbran&ccirc;ojn
        <fnt><bib>MT</bib>, <lok>&Jes; 18:5</lok></fnt>;
      </ekz>
      <ekz>
        ni rigardu, &ccirc;u la vinbertrunko jam bur&gcirc;onis,
        &ccirc;u &gcirc;iaj floroj jam malfermi&gcirc;is
        <fnt><bib>MT</bib>, <lok>&Alt; 7:12</lok></fnt>;
      </ekz>
      <ekz>
        doni <klr>(naski)</klr> <tld/>ojn<fnt>Z</fnt>;
      </ekz>
    </dif>
    <bld lok="../bld/flor.png">
      floropartoj<klr>: 1. stiluso, 2. ovario, 3. florfundo, 4. stameno,
      5. korola folio (petalo), 6. stigmo, 7. pistilo,
      8. kalika folio (sepalo), 9. flortigo (pedunklo, pedicelo)</klr>
    </bld>
    <trd lng="de">Bl&uuml;te</trd>
    <trd lng="nl">bloem</trd>
  </snc>
  <snc mrk="flor.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brila prospero:
      <ekz>
        en la <tld/>o de la juneco, de beleco, de siaj
        jaroj.
      </ekz>
    </dif>
    <trd lng="de">Bl&uuml;te</trd>
    <trdgrp lng="nl">
      <trd>fleur</trd>,
      <trd>bloei</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kv&ecaron;t</trd>,
    <trd>kv&ecaron;tina</trd>
  </trdgrp>
  <trd lng="en">flower</trd>
  <trd lng="es">flor</trd>
  <trd lng="fr">fleur</trd>
  <trd lng="hu">vir&aacute;g</trd>
  <trd lng="nl">bloem</trd>
  <trd lng="ro">floare</trd>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="flor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flor.0i.naski">
    <dif>
      Naski <tld/>ojn <sncref ref="flor.0o.BOT"/>:
      <ekz>
        ekfloros kiel lilio
        <fnt><bib>MT</bib>, <lok>&Jes; 34:17</lok></fnt>;
      </ekz>
      <ekz>
        la vegeta&jcirc;oj <tld/>as en printempo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flor.0i.prosperi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brile prosperi:
      <ekz>
        &gcirc;i forte ekfloros kaj triumfos, triumfos kaj kantos
        <fnt><bib>MT</bib>, <lok>&Jes; 35:2</lok></fnt>;
      </ekz>
      <ekz>
        vivu kaj <tld/>u<fnt>B</fnt>!
      </ekz>
      <ekz>
        <ind><tld/>anta</ind> sano, a&gcirc;o, viza&gcirc;o, knabina
        kapo<fnt>Z</fnt>;
        <trd lng="fr">florissant</trd>
        <trd lng="hu">vir&aacute;gz&oacute;</trd>
        <trd lng="nl">florissant</trd>
      </ekz>
      <ekz>
        en la <tld/>antaj tagoj de Romo;
      </ekz>
      <ekz>
        la artoj longe <tld/>adis en Grekujo;
      </ekz>
      <ekz>
        la komerco <tld/>as;
      </ekz>
      <ekz>
        la gajeco de l' vivo en mi neniam plu re<tld/>os<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flor.0i.disvolvigxi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disvolvi&gcirc;i simile al <tld/>oj:
      <ekz>
        se la lepro disfloros sur la ha&ubreve;to kaj la lepro
        kovros la tutan ha&ubreve;ton
        <fnt><bib>MT</bib>, <lok>&Lev; 13:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">bl&uuml;hen</trd>
  <trd lng="es">florear</trd>
  <trdgrp lng="fr">
    <trd>fleurir<klr tip="ind"> (&ecirc;tre en fleurs)</klr></trd>,
    <trd>&ecirc;tre en <ind>fleurs</ind></trd>,
    <trd>cro&icirc;tre</trd>,
    <trd>s'<ind>&eacute;panouir</ind><klr tip="ind"> (fleurir)</klr></trd>,
    <trd>prosp&eacute;rer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vir&aacute;gzik</trd>,
    <trd>kivir&aacute;gzik</trd>
  </trdgrp>
  <trd lng="nl">bloeien</trd>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
</drv>

<drv mrk="flor.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="flor.0ado.naskigxado">
    <dif>
      Stato de tio, kio naskas <tld/>ojn; tempo en kiu naski&gcirc;as
      <tld/>oj:
      <ekz>
        tiu vegeta&jcirc;o havas malfruan <tld/>adon.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flor.0ado.prosperado">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tempo de brila prospero.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Bl&uuml;te</trd>
  <trdgrp lng="fr">
    <trd>floraison<klr tip="ind"> (&eacute;panouissement, &eacute;poque)</klr></trd>,
    <trd>&eacute;panouissement</trd>,
    <trd>prosp&eacute;rit&eacute;</trd>
  </trdgrp>
  <trd lng="hu">vir&aacute;gz&aacute;s</trd>
  <trd lng="nl">bloei</trd>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="flor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o <sncref ref="flor.0o.BOT"/> a&ubreve;
      <tld/>aro:
      <ekz>
        en la alia fino de la &gcirc;ardeno staris la&ubreve;bo el belaj
        arbeta&jcirc;oj de kaprifolioj <klr>[kies]</klr> flora&jcirc;oj estis
        similaj al grandaj pentritaj kornoj
        <fnt><bib>Fab2</bib>, <lok>Elfo de la rozo</lok></fnt>;
      </ekz>
      <ekz>
        la pomarboj staris tute kovritaj de flora&jcirc;o
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        grandaj blankaj sambukaj flora&jcirc;oj
        <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu folio estis fre&scirc;a kaj suke verda kaj &ccirc;iu
        flora&jcirc;o estis bela knabina kapo
        <fnt><bib>Fab3</bib>, <lok>Koboldo &ccirc;e la butikisto</lok></fnt>;
      </ekz>
      <ekz>
        ekstere oni povis vidi blankru&gcirc;an pizan flora&jcirc;on plene
        elvolvi&gcirc;intan
        <fnt><bib>Fab3</bib>, <lok>Kvin en unu silikvo</lok></fnt>;
      </ekz>
      <ekz>
        la lino neniam povis levi sian malgrandan bluan flora&jcirc;on
        <fnt><bib>Fab3</bib>, <lok>Lino</lok></fnt>;
      </ekz>
      <ekz>
        tie disvolvi&gcirc;as la fajroru&gcirc;aj granataj flora&jcirc;oj
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La radiko <em><tld/></em> havis similan evoluon kiel
    <em>plant</em> de verba al substantiva. Jam Zamenhofo uzis
    jen <em><tld/>a&jcirc;o</em> jen <em><tld/>o</em> samsence.
  </rim>
</drv>

<drv mrk="flor.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <ref tip="dif" cel="inflor.0o">infloresko</ref>
  </snc>
  <adm>
    Supozeble ekzistas ankaux nefaka senco (vd en la difino de ~ajxo).
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="fr">inflorescence</trd>
  <trd lng="hu">vir&aacute;gzat</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;</trd>
</drv>

<drv mrk="flor.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Komenci <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erbl&uuml;hen</trd>,
    <trd>aufbl&uuml;hen</trd>
  </trdgrp>
  <trd lng="fr">fleurir<klr tip="ind"> (intr., se mettre &agrave;
  fleurir)</klr></trd>
  <trdgrp lng="hu">
    <trd>kivir&aacute;gzik</trd>,
    <trd>vir&aacute;gzoni kezd</trd>
  </trdgrp>
  <trd lng="nl">openbloeien</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_c;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="flor.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Perdi siajn florojn; velki.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">verbl&uuml;hen</trd>
  <trd lng="fr">d&eacute;fleurir<klr tip="ind"> (intr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>elny&iacute;lik</trd>,
    <trd>elvir&aacute;gzik</trd>
  </trdgrp>
  <trd lng="nl">verwelken</trd>
  <trd lng="ru">&c_o;&c_t;&c_c;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
</drv>

<drv mrk="flor.0folio">
  <kap><tld/>folio</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="sub" cel="kalik.0afolio">Kalika</ref> a&ubreve;
      <ref tip="sub" cel="korol.0afolio">korola</ref> folio
      de la <ref tip="malprt" cel="flor.0o.BOT"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_l;&c_jo;&c_s;&c_t;&c_a;&c_k;</trd>
  <trd lng="es">p&eacute;talo</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;tale</trd>,
    <trd>s&eacute;pale</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vir&aacute;glev&eacute;l</trd>,
    <trd>szirom</trd>
  </trdgrp>
  <trd lng="nl">bloemblad</trd>
  <trd lng="ru">&c_l;&c_je;&c_p;&c_je;&c_s;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="flor.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de la <ref tip="super" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Spermatophyta</trd>), reprodukti&gcirc;antaj
      per semgrajnoj, esti&gcirc;antaj el fekundi&gcirc;o kaj
      enhavantaj la &gcirc;ermon.
    </dif>
    <ref tip="sin" cel="sem.0oplantoj">semoplantoj</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;&c_v;&c_y;&c_ja; <ind>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;</ind></trd>,
    <trd>&c_p;&c_a;&c_k;&c_r;&c_y;&c_t;&c_a;&c_n;&c_a;&c_s;&c_je;&c_n;&c_n;&c_y;&c_ja; <ind>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bl&uuml;tenpflanzen</trd>,
    <trd>Samenpflanzen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>spermaphytes</trd>,
    <trd>spermatophytes</trd>
  </trdgrp>
  <trd lng="hu">magvas n&ouml;v&eacute;nyek</trd>
  <trd lng="nl">zaadplanten</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_v;&c_je;&c_t;&c_k;&c_o;&c_v;&c_y;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_o;&c_s;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
