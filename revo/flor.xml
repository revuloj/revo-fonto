    <trd>&c_p;&c_a;&c_k;&c_r;&c_y;&c_t;&c_a;&c_n;&c_a;&c_s;&c_je;&c_n;&c_n;&c_y; &c_ja;
    <ind>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bl&uuml;tenpflanzen</trd>,
    <trd>Samenpflanzen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>spermaphytes</trd>,
    <trd>spermatophytes</trd>
  </trdgrp>
  <trd lng="hu">magvas n&ouml;v&eacute;nyek</trd>
  <trd lng="id">spermatofita</trd>
  <trd lng="nl">zaadplanten</trd>
  <trd lng="pl">ro&sacute;liny nasienne</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_v;&c_je;&c_t;&c_k;&c_o;&c_v;&c_y;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_o;&c_s;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y; &c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
