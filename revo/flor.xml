<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flor</rad>/o
</kap>
<drv mrk="flor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flor.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="flor.0oplantoj"><tld/>oplantoj</ref>
      kun la reproduktaj organoj:
      <ekz>
        doni <klr>(naski)</klr> <tld/>ojn<fnt>Z</fnt>;
      </ekz>
    </dif>
    <trd lng="de">Bl&uuml;te</trd>
  </snc>
  <snc>
    <dif>
      Okulfrapa, kutime kolora, parto de vegeta&jcirc;o
      kun <ref tip="prt" cel="flor.0o.BOT"><tld/>o(j)</ref>.
      <ekz>
        bukedo da <tld/>oj;
      </ekz>
      <ekz>
        <tld/>desegna &scirc;tofo.
      </ekz>
    </dif>
    <trd lng="de">Blume</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="flor.0oplanto"><tld lit="F"/>oplanto</ref>:
      <ekz>
        maj<tld/>o;
      </ekz>
      <ekz>
        sun<tld/>o;
      </ekz>
      <ekz>
        <tld/>poto.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      Brila prospero:
      <ekz>
        en la <tld/>o de la juneco, de beleco, de siaj
        jaroj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kv&ecaron;t</trd>,
    <trd>kv&ecaron;tina</trd>
  </trdgrp>
  <trd lng="de">Bl&uuml;te</trd>
  <trd lng="en">flower</trd>
</drv>
<drv mrk="flor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Naski <tld/>ojn 1:
      <ekz>
        la vegeta&jcirc;oj <tld/>as en printempo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      Brile prosperi:
      <ekz>
        vivu kaj <tld/>u<fnt>B</fnt>!
      </ekz>
      <ekz>
        <tld/>anta sano, a&gcirc;o, viza&gcirc;o, knabina
        kapo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en la <tld/>antaj tagoj de Romo;
      </ekz>
      <ekz>
        la artoj longe <tld/>adis en Grekujo;
      </ekz>
      <ekz>
        la komerco <tld/>as;
      </ekz>
      <ekz>
        la gajeco de l' vivo en mi neniam plu re<tld/>os<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bl&uuml;hen</trd>
</drv>
<drv mrk="flor.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Stato de tio, kio naskas <tld/>ojn; tempo en kiu naski&gcirc;as <tld/>oj:
      <ekz>
        tiu vegeta&jcirc;o havas malfruan <tld/>adon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      Tempo de brila prospero.
    </dif>
  </snc>
  <trd lng="de">Bl&uuml;te</trd>
</drv>
<drv mrk="flor.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Komenci <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>erbl&uuml;hen</trd>,
    <trd>aufbl&uuml;hen</trd>
  </trdgrp>
</drv>
<drv mrk="flor.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Perdi siajn florojn; velki.
    </dif>
  </snc>
  <trd lng="de">verbl&uuml;hen</trd>
</drv>
<drv mrk="flor.0folieto">
  <kap><tld/>folieto</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="petal.0o">petalo</ref>.
    </dif>
  </snc>
  <trd lng="de">Bl&uuml;tenblatt</trd>
</drv>
<drv mrk="flor.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de la <ref tip="super" cel="plant.oj">plantoj</ref>
      (<trd lng="la">Spermatophyta</trd>), produkti&gcirc;antaj
      per semgrajnoj, esti&gcirc;antaj el fekundi&gcirc;o kaj
      enhavantaj la &gcirc;ermon.
    </dif>
    <ref tip="sin" cel="plant.semo0oj">semoplantoj</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Bl&uuml;tenpflanzen</trd>,
    <trd>Samenpflanzen</trd>
  </trdgrp>                       
</drv>
<drv mrk="flor.negxo0o">
  <kap>ne&gcirc;o<tld/>o</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="edelve.0o">edelvejso</ref>
      (<trd lng="la">Leontopodium alpinum</trd>).
    </dif>
  </snc>
  <trd lng="de">Edelwei&szlig;</trd>
</drv>
<drv mrk="flor.sun0o">
  <kap>sun<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="helian.0o">helianto</ref>
    <bld lok="../bld/heliant.jpg">
      sun<tld/>oj (Helianthus annuus)
    </bld>
  </snc>
  <trd lng="de">Sonnenblume</trd>
</drv>
</art>
