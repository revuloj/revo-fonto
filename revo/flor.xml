    <trd>zakwita&cacute;</trd>,
    <trd>kwitn&aogonek;&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  <trd lng="sk">kvitn&uacute;&tcaron;</trd>
  <trd lng="sv">att <ind>blomma</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_v;&c_ib;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_v;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
