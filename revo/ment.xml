<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ment</rad>/o
</kap>
<drv mrk="ment.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lami.0acoj">lamiacoj</ref>
      (<trd lng="la">Mentha</trd>).
    </dif>
  </snc>
  <trd lng="de">Minze</trd>
  <trd lng="en">mint</trd>
  <trd lng="tr">nane</trd>
</drv>
<drv mrk="ment.pipro0o">
  <kap>pipro<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ment.0o"><tld/>o</ref>
      (<trd lng="la">Mentha piperita</trd>), aroma herbo uzata
      en tizanoj kaj farmacio.
    </dif>
  </snc>
  <trd lng="de">Pfefferminze</trd>
  <trd lng="de">peppermint</trd>
</drv>
</art>
