<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ulm</rad>/o
</kap>
<drv mrk="ulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ulm.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Ulmus</trd>), kies fibreca, elasta ligno estas uzata por
      fari objektojn, kies strukturo a&ubreve; uzado postulas fortikecon.
    </dif>
  </snc>
  <trd lng="de">Ulme</trd>
  <trd lng="en">elm</trd>
  <trd lng="fr">orme</trd>
  <trdgrp lng="nl">
      <trd>olm</trd>,
      <trd>iep</trd>,
      <trd>iepenboom</trd></trdgrp>
  <trd lng="ru">&c_v;&c_ja;&c_z;</trd>
</drv>
<drv mrk="ulm.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      lignaj plantoj (<trd lng="la">Ulmaceae</trd>),
      plej multaj specioj kreskantaj en tropikaj regionoj de Azio kaj
      Ameriko.
    </dif>
  </snc>
  <trd lng="de">Ulmengew&auml;chse</trd>
  <trd lng="fr">ulmac&eacute;es</trd>
</drv>
</art>
