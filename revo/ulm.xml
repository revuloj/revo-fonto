<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ulm</rad>/o
</kap>
<drv mrk="ulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ulmac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Ulmus</trd>), kies fibreca, elasta ligno estas uzata
por
      fari objektojn, kies strukturo a&ubreve; uzado postulas fortikecon.
    </dif>
  </snc>
  <trd lng="de">Ulme</trd>
  <trd lng="el">&phi;&tau;&epsilon;&lambda;&jota;&alfa_ton;</trd>
  <trd lng="en">elm</trd>
  <trd lng="es">olmo</trd>
  <trd lng="fr">orme</trd>
  <trd lng="hu">szil(fa)</trd>
  <trdgrp lng="nl">
      <trd>olm</trd>,
      <trd>iep</trd>,
      <trd>iepenboom</trd></trdgrp>
  <trdgrp lng="pt">
    <trd>olmo</trd>,
    <trd>ulmo </trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_ja;&c_z;</trd>
</drv>

</art>
