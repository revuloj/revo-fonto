<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antidot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="antido.0o">
  <kap><tld/>o</kap>
  <snc mrk="antido.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="venen.kontraux0o.MED">kontra&ubreve;veneno</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      ni deziras krei organismon, kies esenca celo estos produkti
      <tld/>on kontra&ubreve; la nacieco
      <fnt><bib>Lanti</bib></fnt>.      
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_ja;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_ja;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antidotum</trd>,
    <trd>protijed</trd>
  </trdgrp>
  <trd lng="de">Gegengift</trd>
  <trd lng="en">antidote</trd>
  <trd lng="es">ant&iacute;doto</trd>
  <trd lng="fr">antidote</trd>
  <trdgrp lng="pl">
    <trd>odtrutka</trd>,
    <trd>antidotum</trd>
  </trdgrp>
  <trd lng="ro">antidot</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_ja;&c_d;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>antidotum</trd>,
    <trd>protijed</trd>,
    <trd>protil&aacute;tka</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>motgift</trd>,
    <trd>antidot</trd>
  </trdgrp>
</drv>
</art>
