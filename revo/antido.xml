<art mrk="$Id: antido.xml,v 1.23 2023/10/28 08:33:07 revo Exp $">
<kap>
  <rad>antidot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="antido.0o">
  <kap><tld/>o</kap>
  <snc mrk="antido.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="venen.kontraux0o.MED">Kontra&ubreve;veneno</ref>:
      <ekz>
        maltrankviligaj raportoj pri grava morbo <klr>[&#x2026;]</klr>, neniu kuracilo a&ubreve;
        <tld/>o estas efika kontra&ubreve; &gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008875.php">Mallonga historio de la evonima milito</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni deziras krei organismon, kies esenca celo estos produkti
        <tld/>on kontra&ubreve; la nacieco
        <fnt><bib>Lanti</bib></fnt>.      
      </ekz>
   </dif>
    <trdgrp lng="zh">
    <trd>&#x53CD;&#x4F5C;&#x7528;&#x5242; <pr>f&#x1CE;nzu&#xF2;y&#xF2;ngj&#xEC;</pr></trd>,
    <trd>&#x53CD;&#x4F5C;&#x7528;&#x5291; <pr>f&#x1CE;nzu&#xF2;y&#xF2;ngj&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_ja;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_ja;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antidotum</trd>,
    <trd>protijed</trd>
  </trdgrp>
  <trd lng="de">Gegengift</trd>
  <trd lng="en">antidote</trd>
  <trd lng="es">ant&iacute;doto</trd>
  <trd lng="fr">antidote</trd>
  <trd lng="he">&#x5EA;&#x5E8;&#x5D5;&#x5E4;&#x5EA; &#x5E0;&#x5D2;&#x5D3;</trd>
  <trd lng="ja">&#x89E3;&#x6BD2;&#x5264;  <pr>&#x3052;&#x3069;&#x304F;&#x3056;&#x3044;</pr></trd>
  <trdgrp lng="pl">
    <trd>odtrutka</trd>,
    <trd>antidotum</trd>
  </trdgrp>
  <trd lng="ro">antidot</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_ja;&c_d;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>antidotum</trd>,
    <trd>protijed</trd>,
    <trd>protil&aacute;tka</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>motgift</trd>,
    <trd>antidot</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x43D;&#x442;&#x438;&#x434;&#x43E;&#x442;</trd>
</drv>
</art>
