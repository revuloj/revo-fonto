<art mrk="$Id$">
<kap>
  <rad>mot</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frazo pruntita el iu a&ubreve;toro kaj metita anta&ubreve; libro a&ubreve; artikolo
      por montri &gcirc;ian &ccirc;efan ideon.
    </dif>
    <refgrp tip="sin">
      <ref cel="deviz.0o">devizo</ref>,
      <ref cel="maksim.0o">maksimo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">moto</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_t;&c_t;&c_o;</trd>,
    <trd>&c_e;&c_p;&c_i;&c_g;&c_r;&c_a;&c_f;</trd>
  </trdgrp>
</drv>
</art>
