<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>volumen</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="volume.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parto de la spaco okupata de iu korpo:
      <ekz>
        kesto kun granda <tld/>o, sed malgranda pezo;
      </ekz>
      <ekz>
        &scirc;ian atenton kaptis la luksa <tld/>ego de oketa&gcirc;a, evidente
        &jcirc;us konstruita domo
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 21</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amplek.0o">amplekso</ref>,
      <ref cel="hav.en0o">enhavo</ref>.
    </refgrp>
  </snc>
  <snc mrk="volume.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Mezuro de <ref cel="solid.0o.geometrio">solido</ref>:
      <ekz>
        la <tld/>o de <ref cel="kub.0o.figuro">kubo</ref> egalas
        al la <ref cel="kub.0o.algebra">kubo</ref> de &gcirc;ia e&gcirc;o;
      </ekz>
      <ekz>
        la <tld/>o de <ref cel="sfer.0o.solido">sfero</ref> estas
        proporcia al la <ref cel="kub.0o.algebra">kubo</ref> de
        &gcirc;ia radiuso;
      </ekz>
      <ekz>
        el &ccirc;iuj solidoj havantaj saman <ref cel="are.0o.MAT">areon</ref> 
        la <ref cel="sfer.0o.solido">sfero</ref> havas la plej grandan <tld/>on.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volume.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      esprimanta la mezuron de la spaco okupata de iu korpo:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>M<sup>0</sup>L<sup>3</sup>T<sup>0</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="metr.kuba0o">kuba metro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Volumen</trd>
  <trd lng="en">volume</trd>
  <trd lng="es">volumen</trd>
  <trd lng="fr">volume<klr tip="ind"> (dimension)</klr></trd>
  <trdgrp lng="nl">
    <trd>volume<klr tip="ind"> (inhoud)</klr></trd>,
    <trd>inhoud</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_jo;&c_m;</trd>
</drv>
</art>
