<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sanita.xml,v 1.24 2024/03/24 19:59:59 revo Exp $">
<kap>
  <rad>sanitar</rad>/a,
  <var><kap><rad var="i">sanitari</rad>/a</kap></var>
</kap>
<drv mrk="sanita.0a">
  <kap><tld/>a, <var><kap><tld var="i"/>a</kap></var></kap>
  <snc>
    <dif>
      Koncernanta la publikan <ref cel="san.0o">sano</ref>n:
      <ekz>
        grupo da bonvolemaj poloj <klr>[&#x2026;]</klr> ricevis la rajton eniri la geton por
        kontroli tie la <tld/>an staton
        <fnt>
          <bib>Monato</bib>
          <aut>Zofia Banet-Fornalowa</aut>
          <vrk><url ref="&Monato;/2004/008391.html">Listo de Sendler</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        milionoj da homoj mortas pro tabako-rilataj malsanoj &ccirc;iujare <klr>[&#x2026;]</klr>
        diras <tld/>aj ekspertoj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2012/010706.php">Premio alfrontas premon</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        komencas veni <klr>[&#x2026;]</klr> <tld var="i"/>aj ta&ccirc;mentoj, ru&gcirc;aj flegistinoj en
        blankaj anta&ubreve;tukoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni devis ur&gcirc;e <klr>[&#x2026;]</klr> konstrui &gcirc;is la komenco de la vintro <tld/>ejon
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2004/008350.html">Tiu tago</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="higien.0o">higieno</ref>,
      <ref cel="humanitar.0a">humanitara</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_ib;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="de">sanit&auml;r</trd>
  <trdgrp lng="en">
   <trd>sanitary</trd>,
   <trd>concerning health</trd>
  </trdgrp>
  <trd lng="fr">sanitaire<klr> (adj.)</klr></trd>
  <trd lng="hu">eg&eacute;szs&eacute;g&uuml;gyi</trd>
  <trd lng="it">sanitario</trd>
  <trdgrp lng="ja">
    <trd>&#x4FDD;&#x5065;&#x885B;&#x751F;&#x306E;  <pr>&#x307B;&#x3051;&#x3093;&#x3048;&#x3044;&#x305B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x516C;&#x8846;&#x885B;&#x751F;&#x306E;  <pr>&#x3053;&#x3046;&#x3057;&#x3085;&#x3046;&#x3048;&#x3044;&#x305B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">sanitair</trd>
  <trd lng="pl">sanitarny</trd>
  <trdgrp lng="pt">
   <trd>sanit&aacute;rio</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_n;&c_i;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&c_s;&c_a;&#x43D;&c_ib;&c_t;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="sanita.0isto">
  <kap><tld/>isto, <var><kap><tld var="i"/>isto</kap></var></kap>
  <snc mrk="sanita.0isto.ofica">
    <dif>
      <ref tip="super" cel="profes.0isto">Profesiulo</ref>, oficisto, kiu
      okupi&gcirc;as pri la publika sano:
    </dif>
    <refgrp tip="vid">
      <ref cel="epidemiologi.0isto">epidemiologiisto</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_a;&c_n;&c_ib;&c_t;&c_a;&c_r;&c_n;&c_y; &c_w;&c_r;&c_a;&c_ch;</trd>
    <trdgrp lng="de">
      <trd>Amtsarzt</trd>,
      <trd>Sozialmediziner</trd>
    </trdgrp>
  </snc>
  <snc mrk="sanita.0isto.sukuristo">
    <dif>
      <ref tip="dif" cel="sukur.0isto">Sukuristo</ref>:
      <ekz>
        for de &ccirc;i tie! nenian <tld var="i"/>an helpon! &dash; la ektimigitaj <tld var="i"/>istoj
        malaperas en la strateto
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
       <ekz>
         vunditojn provizore banda&gcirc;as <tld/>istoj
         <fnt>
         <aut>Stefan Maul</aut>,
         <vrk>El verva vivo jurnalista</vrk>,
         <lok>p. 31</lok>
         </fnt>;
       </ekz>
      <ekz>
        la magistrato de la &ccirc;efurbo Prago postulas subvencion <klr>[&#x2026;]</klr> por kovri
        la kromajn kostojn de de&jcirc;oroj de la urbaj policanoj, fajrobrigadanoj kaj
        <tld/>istoj
        <fnt>
          <aut>jpa</aut>
            <vrk><url ref="&Monato;/2000/007093.php">Internacia kunsido de financistoj en
            Prago</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 9</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fleg.0isto">flegisto</ref>,
      <ref cel="sav.0o.liberigo">savokuracisto</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_a;&c_n;&c_ib;&c_t;&c_a;&c_r;</trd>
    <trdgrp lng="cs">
      <trd>zdravotnick&yacute; pracovn&iacute;k</trd>,
      <trd>zdravotn&iacute;k</trd>
    </trdgrp>
    <trd lng="de">Sanit&auml;ter</trd>
    <trd lng="nl">verpleger</trd>
    <trd lng="pl">sanitariusz</trd>
    <trd lng="sk">zdravotn&iacute;k</trd>
    <trdgrp lng="zh">
    <trd>&#x6551;&#x6025;&#x6551;&#x547D;&#x58EB; <pr>ji&#xF9;j&#xED;ji&#xF9;m&#xEC;ngsh&#xEC;</pr></trd>,
    <trd>&#x6551;&#x63F4;&#x4EBA;&#x5458; <pr>ji&#xF9;yu&#xE1;nr&#xE9;nyu&#xE1;n</pr></trd>,
    <trd>&#x6551;&#x63F4;&#x4EBA;&#x54E1; <pr>ji&#xF9;yu&#xE1;nr&#xE9;nyu&#xE1;n</pr></trd>,
    <trd>&#x6551;&#x62A4;&#x5458; <pr>ji&#xF9;h&#xF9;yu&#xE1;n</pr></trd>,
    <trd>&#x6551;&#x8B77;&#x54E1; <pr>ji&#xF9;h&#xF9;yu&#xE1;n</pr></trd>,
    <trd>&#x767D;&#x8863;&#x6218;&#x58EB; <pr>b&#xE1;iy&#x12B;zh&#xE0;nsh&#xEC;</pr></trd>,
    <trd>&#x767D;&#x8863;&#x6230;&#x58EB; <pr>b&#xE1;iy&#x12B;zh&#xE0;nsh&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ja">
    <trd>&#x4FDD;&#x5065;&#x5E2B;  <pr>&#x307B;&#x3051;&#x3093;&#x3057;</pr></trd>,
    <trd>&#x4FDD;&#x5065;&#x58EB;  <pr>&#x307B;&#x3051;&#x3093;&#x3057;</pr></trd>,
    <trd>&#x885B;&#x751F;&#x6280;&#x5E2B;  <pr>&#x3048;&#x3044;&#x305B;&#x3044;&#x304E;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_a;&#x43D;&c_ib;&c_t;&c_a;&c_r;</trd>
</drv>
</art>
