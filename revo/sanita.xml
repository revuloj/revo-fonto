<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sanitar</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="sanita.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta la publikan sanon:
      <ekz>
        <tld/>ejo;
      </ekz>
      <ekz>
        <ind><tld/>a kordono</ind>.
        <trd lng="nl">cordon sanitaire</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_ib;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="de">sanit&auml;r</trd>
  <trdgrp lng="en">
   <trd>sanitary</trd>,
   <trd>concerning health</trd>
  </trdgrp>
  <trd lng="hu">eg&eacute;szs&eacute;g&uuml;gyi</trd>
  <trd lng="it">sanitario</trd>
  <trd lng="nl">sanitair</trd>
  <trdgrp lng="pt">
   <trd>sanit&aacute;rio</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_n;&c_i;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sanita.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
       <ref tip="super" cel="profes.0isto">Profesiulo</ref>, kiu
       okupi&gcirc;as
       pri la publika sano: 
       <ekz>
         vunditojn provizore banda&gcirc;as <tld/>istoj
         <fnt>
         <aut>Stefan Maul</aut>,
         <vrk>El verva vivo jurnalista</vrk>,
         <lok>p. 31</lok>
         </fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="de">Sanit&auml;ter</trd>
  <trd lng="nl">verpleger</trd>
</drv>
</art>
