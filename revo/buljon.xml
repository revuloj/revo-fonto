<kap>
<ofc>*</ofc>
<rad>buljon</rad>/o
</kap>
<drv mrk="buljon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Nutra flua&jcirc;o farata el boligitaj  legomoj kun a&ubreve; sen viando.
    </dif>
    <refgrp tip="vid">
      <ref cel="brog.0ajxo">broga&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de"><klr tip="amb">(Gem&uuml;se-)</klr>Br&uuml;he</trd>
  <trd lng="fr">bouillon</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">buj&oacute;n</trd>
</drv>
</art>
