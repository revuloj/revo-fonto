<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buljon.xml,v 1.28 2023/10/29 19:11:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>buljon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="buljon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="buljon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Nutra flua&jcirc;o farita el boligitaj legomoj a&ubreve; viando:
      <ekz>
        la kuracistoj nutris lin per pecetigita viando, donis al li por
        trinki
        fortan <tld/>on, lakton kaj maljunan vinon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 23a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x6C64; <pr>t&#x101;ng</pr></trd>
</snc>
  <ref tip="super" cel="dekokt.0ajxo">dekokta&jcirc;o</ref>
  <refgrp tip="vid">
    <ref cel="brog.0ajxo">broga&jcirc;o</ref>,
    <ref cel="infuz.0ajxo">infuza&jcirc;o</ref>,
    <ref cel="sup.0o.KUI">supo</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_u;&c_l;&c_jo;&c_n;</trd>
  <trd lng="bg">&c_b;&c_u;&c_l;&c_mol;&c_o;&c_n;</trd>
  <trd lng="br">bouilho&ntilde;s</trd>
  <trdgrp lng="de">
    <trd>Br&uuml;he <klr tip="ind">(Suppe)</klr></trd>,
    <trd>Bouillon</trd>
  </trdgrp>  
  <trdgrp lng="en">
    <trd>bouillon</trd>,
    <trd>broth</trd>
  </trdgrp>  
  <trd lng="fr">bouillon</trd>
  <trd lng="he">&#x5DE;&#x5E8;&#x5E7; &#x5D1;&#x5E9;&#x5E8;</trd>
  <trdgrp lng="hu">
    <trd>h&uacute;sleves</trd>,
    <trd>er&odblac;leves</trd>
  </trdgrp>
  <trd lng="ja">&#x30D6;&#x30A4;&#x30E8;&#x30F3;</trd>
  <trd lng="nl">bouillon</trd>
  <trdgrp lng="pl">
    <trd>bulion</trd>,
    <trd>ros&oacute;&lstroke;</trd>
  </trdgrp>
  <trd lng="pt">caldo</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">buj&oacute;n</trd>
  <trdgrp lng="sk">
    <trd>buj&oacute;n</trd>,
    <trd>zeleninov&yacute; odvar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>buj&oacute;n</trd>,
    <trd>masov&aacute; pol&eacute;vka</trd>,
    <trd>v&yacute;var</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x43B;&#x44C;&#x439;&#x43E;&#x43D;</trd>,
    <trd>&#x440;&#x43E;&#x441;&#x456;&#x43B;</trd>
  </trdgrp>
</drv>
</art>
