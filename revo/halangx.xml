<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: halangx.xml,v 1.3 2024/02/08 06:28:54 revo Exp $">
<kap>
  <rad>halan&gcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="halangx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="halangx.0o.KOMUNE">
    <dif>
      Nekomprenebla lingva&jcirc;o,
      <ref tip="vid" cel="galima.0o">galimatio</ref>,
      <ref tip="vid" cel="volapuk.0ajxo">volapuka&jcirc;o</ref>:
      <ekz>
        tiu &ccirc;i viro inventis la <tld/>o-lingvon; li miksas
        sensencajn vortojn en siajn frazojn
        <fnt>
          <aut>F. Karinthy</aut>,
          <vrk><url ref="donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5704/karinthy.html">
          Halan&gcirc;o</url></vrk>,
          <lok>Norda Prismo, 1957:4, p. 186a-187a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_g;&c_l;&c_u;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_b;&c_a;&c_t;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="fr">charabia</trd>
  <trd lng="it">sproloquio</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&#x43B;&#x44F;&c_s;&c_i;</trd>,
    <trd>&c_b;&c_a;&#x43B;&#x44F;&#x43D;&c_d;&c_r;&#x44F;&c_s;&c_i;</trd>,
    <trd>&#x43B;&#x44F;&c_s;&c_i;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_je;&c_v;&c_je;&#x43D;&c_ib;</trd>
  </trdgrp>
</drv>
</art>

