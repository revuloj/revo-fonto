<vortaro>
<art mrk="$Id: perine.xml,v 1.19 2018/12/01 10:10:16 revo Exp $">
<kap>
   <rad>perine</rad>/o
</kap>
<drv mrk="perine.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ANA</uzo>
   <snc mrk="perine.0o.ANA">
     <dif>
       La regiono inter la <ref cel="anus.0o">anuso</ref> kaj la <ref
       cel="testik.0ujo">testikujo</ref> a&ubreve; inter la <ref
       cel="anus.0o">anuso</ref> kaj la <ref cel="vulv.0o">vulvo</ref>:
      <ekz>
        la homa anuso trovi&gcirc;as inter gluteoj, malanta&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32609">Anuso</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="ca">perineu</trd>
  <trdgrp lng="cs">
    <trd>hr&aacute;z &rcaron;itn&iacute;</trd>,
    <trd>hr&aacute;zka &rcaron;itn&iacute;</trd>,
    <trd>perineum</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Perineum</trd>,
     <trd>Damm</trd>,
     <trd>Schamleiste</trd>
   </trdgrp>
  <trd lng="en">perineum</trd>
  <trd lng="es">perineo</trd>
  <trd lng="fr">perin&eacute;e</trd>
  <trdgrp lng="hu">
     <trd>g&aacute;t<klr tip="ind"> (perineum)</klr></trd>,
     <trd>perineum</trd>
   </trdgrp>
  <trd lng="io">perineo</trd>
  <trd lng="it">perineo</trd>
  <trd lng="iu">&#5503;&#5354;&#5335;&#5130;&#5509;</trd>
  <trd lng="la">perineum</trd>
  <trd lng="nl">perineum</trd>
  <trd lng="pl">krocze</trd>
  <trd lng="pt">per&iacute;neo</trd>
  <trdgrp lng="ru">
     <trd>&c_p;&c_je;&c_r;&c_i;&c_n;&c_je;&c_j;</trd>,
     <trd>&c_p;&c_r;&c_o;&c_m;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">hr&aacute;dza</trd>
</drv>

<drv mrk="perine.0a">
  <kap><tld/>a</kap>
  <snc mrk="perine.0a.ANA">
    <dif>
      Rilata al perineo.
    </dif>
  </snc>
  <trd lng="cs">perine&aacute;ln&iacute;</trd>
  <trd lng="en">perineal</trd>
  <trd lng="es">perineal</trd>
  <trd lng="fr">p&eacute;rin&eacute;al</trd>
  <trd lng="io">perineala</trd>
  <trd lng="it">perineale</trd>
  <trd lng="pt">perineal</trd>
  <trd lng="ro">perineal</trd>
  <trd lng="sl">perinealen</trd>
</drv>
</art>
