<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sald.xml,v 1.24 2022/05/05 05:09:55 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sald</rad>/o
</kap>
<drv mrk="sald.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="difere.0o.MAT">Diferenco</ref>
      inter <ref cel="debet.0o">debeto</ref>
      kaj <ref cel="kredit.0o.konto">kredito</ref>:
      <ekz>
        debet<tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de kuranta konto
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/006821.html">E&ubreve;ropa Unio: E&ubreve;ro unujara</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        de multaj jaroj la komerca <tld/>o de Pollando kun EU estas deficita
        <fnt>
          <bib>Monato</bib>
          <aut>Stanis&lstroke;aw &Sacute;migielski</aut>
          <vrk><url ref="&Monato;/2003/007947.html">Eksporto nur al Okcidento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la jaro 2005 la pagobilanco havis negativan <tld/>on de 9,8 miliardoj da
        usonaj dolaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Hin&scaron;t</aut>
          <vrk><url ref="&Monato;/2007/009212.php">Kiel oni (trans)vivas en Kroatio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se bilanci la tuton, la fina <tld/>o estas tre favora <klr>[&#x2026;]</klr>, la
        recenzita verko sendube alportas signifan kontribuon
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2007/009078.php">Interlingvistiko por &ccirc;iuj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bilanc.0o">bilanco</ref>,
      <ref cel="kont.0o.KOME">konto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_mol;&c_d;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_b;&c_a;&c_l;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_d;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>saldo</trd>,
    <trd>vyrovn&aacute;n&iacute;</trd>,
    <trd>z&uring;statek &uacute;&ccaron;tu</trd>
  </trdgrp>
  <trd lng="de">Saldo</trd>
  <trd lng="en">balance<klr tip="amb"> (account)</klr></trd>
  <trd lng="es">saldo<klr tip="amb"> (contable)</klr></trd>
  <trd lng="fr">solde</trd>
  <trd lng="he">&mem;&alef;&zayin;&fnun;</trd>
  <trd lng="hu">egyenleg</trd>
  <trd lng="it">saldo<klr> (econ.)</klr></trd>
  <trd lng="nl">saldo</trd>
  <trdgrp lng="pt">
   <trd>saldo</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_mol;&c_d;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>saldo</trd>,
    <trd>zostatok &uacute;&ccaron;tu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43C;. &#x441;&#x430;&#x43B;&#x44C;&#x434;&#x43E;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x441;&#x443;&#x43C;&#x43E;&#x43A;</trd>,
    <trd>&#x437;&#x430;&#x43B;&#x438;&#x448;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="sald.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Starigi la <tld/>on; rezulti je <tld/>o:
      <ekz>
        la libroservo de UEA <tld/>is siajn vendojn en 1998 per la impona sumo de
        297&#x202F;165 gld
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 4 (54)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi povos trovi tiujn kanajlojn <klr>[&#x2026;]</klr> por <tld/>i mian kalkulon kun ili
        <klr>(vd <ref cel="regul.0igi.konformigi">reguligi</ref> 
        <ref cel="kont.0o.KOME">konton</ref>)</klr>
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_l;&c_ib;&c_ch;&c_v;&c_a;&c_c;&c_mol; <ind>&c_s;&c_a;&c_l;&c_mol;&c_d;&c_a;</ind></trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_a;&c_n;&c_s;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>saldovat</trd>,
    <trd>vyrovnat z&uring;statek</trd>,
    <trd>z&uacute;&ccaron;tovat</trd>
  </trdgrp>
  <trd lng="de">saldieren</trd>
  <trd lng="en">balance<klr tip="amb"> (account)</klr></trd>
  <trd lng="es">saldar</trd>
  <trd lng="fr">solder</trd>
  <trd lng="he">&lamed;<ind>&alef;&zayin;&fnun;</ind></trd>
  <trd lng="hu">egyenleget von</trd>
  <trd lng="nl">het <ind>saldo</ind> opmaken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_s;&c_ch;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_a;&c_l;&c_mol;&c_d;&c_o;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_i;&c_t;&c_o;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>saldova&tcaron;</trd>,
    <trd>z&uacute;&ccaron;tova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x456;&#x434;&#x431;&#x438;&#x432;&#x430;&#x442;&#x438; &#x43F;&#x456;&#x434;&#x441;&#x443;&#x43C;&#x43E;&#x43A;</trd>
</drv>
</art>
