<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sald</rad>/o
</kap>
<drv mrk="sald.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Diferenco inter debeto kaj kredito.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_mol;&c_d;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_b;&c_a;&c_l;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_d;&c_o;</trd>
  </trdgrp>
  <trd lng="de">Saldo</trd>
  <trd lng="en">balance<klr tip="amb"> (account)</klr></trd>
  <trd lng="es">saldo<klr tip="amb"> (contable)</klr></trd>
  <trd lng="fr">solde</trd>
  <trd lng="he">&mem;&alef;&zayin;&fnun;</trd>
  <trd lng="hu">egyenleg</trd>
  <trd lng="it">saldo<klr> (econ.)</klr></trd>
  <trd lng="nl">saldo</trd>
  <trdgrp lng="pt">
   <trd>saldo</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_mol;&c_d;&c_o;</trd>
</drv>

<drv mrk="sald.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Starigi la <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_l;&c_ib;&c_ch;&c_v;&c_a;&c_c;&c_mol; <ind>&c_s;&c_a;&c_l;&c_mol;&c_d;&c_a;</ind></trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_a;&c_n;&c_s;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">saldieren</trd>
  <trd lng="en">balance<klr tip="amb"> (account)</klr></trd>
  <trd lng="es">saldar</trd>
  <trd lng="fr">solder</trd>
  <trd lng="he">&lamed;<ind>&alef;&zayin;&fnun;</ind></trd>
  <trd lng="hu">egyenleget von</trd>
  <trd lng="nl">het <ind>saldo</ind> opmaken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_s;&c_ch;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_a;&c_l;&c_mol;&c_d;&c_o;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_i;&c_t;&c_o;&c_g;</trd>
  </trdgrp>
</drv>
</art>
