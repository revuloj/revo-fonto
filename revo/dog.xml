<kap>
  <ofc>1</ofc>
  <rad>dog</rad>/o
</kap>
<drv mrk="dog.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Speco de forta <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      kun fortikaj makzeloj kaj pendantaj lipoj:
      <ekz>
        kelkaj rasoj pli mordemas, ekzemple rotvejla hundo, terhundo, mastifo kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2010/010026.php">Mordaj demandoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="buldog.0o">buldogo</ref>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_g;</trd>
  <trd lng="cs">doga <klr>(ps&iacute; rasa)</klr></trd>
  <trd lng="de">Dogge</trd>
  <trdgrp lng="en">
    <trd>mastiff</trd>,
    <trd><klr>(English) </klr>bulldog</trd>
  </trdgrp>
  <trd lng="es">dogo<klr tip="amb"> (raza de perro)</klr></trd>
  <trd lng="fr">dogue</trd>
  <trd lng="hu">dog</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30B9;&#x30C1;&#x30D5;</trd>,
    <trd>&#x5927;&#x578B;&#x306E;&#x756A;&#x72AC;  <pr>&#x304A;&#x304A;&#x304C;&#x305F;&#x306E;&#x3070;&#x3093;&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">dog</trd>
  <trdgrp lng="pl">
    <trd>dog</trd>,
    <trd>molos</trd>
  </trdgrp>
  <trd lng="pt">dogue</trd>
  <trd lng="ru">&c_d;&c_o;&c_g;</trd>
  <trd lng="sk">doga</trd>
  <trd lng="uk">&#x434;&#x43E;&#x491;</trd>
  <trdgrp lng="zh">
    <trd>&#x7352; <pr>&#xE1;o</pr></trd>,
    <trd>&#x725B;&#x5934;&#x72AC; <pr>ni&#xFA;t&#xF3;uqu&#x1CE;n</pr></trd>,
    <trd>&#x6597;&#x725B;&#x72D7; <pr>d&#xF2;uni&#xFA;g&#x1D2;u</pr></trd>
  </trdgrp>
</drv>
