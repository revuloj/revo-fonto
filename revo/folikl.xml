<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: folikl.xml,v 1.23 2025/04/26 20:27:41 revo Exp $">
<kap><rad>folikl</rad>/o<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="folikl.0o">
  <kap><tld/>o</kap>
  <snc mrk="folikl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Unufaka <ref tip="super" cel="frukt.malferm0o">malfermfrukto</ref>
      esti&gcirc;anta el unuopa fruktofolio kaj havanta nur unu fendon
      malfermi&gcirc;anta &ccirc;e maturi&gcirc;o, ekz-e &ccirc;e kelkaj
      ranunkolacoj.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="gusx.0o">gu&scirc;o</ref>.
    </rim>
    <trd lng="de">Balgfrucht</trd>
    <trd lng="en">follicle <klr>(botany)</klr></trd>
    <trdgrp lng="nl">
      <trd>zaadvlies</trd>,
      <trd>zaadhulsel</trd>
    </trdgrp>
  </snc>
  <snc mrk="folikl.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Nomo de diversaj saketformaj organoj, haroj, glandoj:
      <ekz>
        <ind>dento<tld/>o</ind>,
        <trd lng="fr">follicule dentaire</trd>
        <trd lng="nl">tandzakje</trd>
      </ekz>
      <ekz>
        <ind>har<tld/>o</ind>,
        <trd lng="fr">follicule pileux</trd>
        <trd lng="it">follicolo pilifero</trd>
        <trdgrp lng="nl">
          <trd>haarzakje</trd>,
          <trd>haarfollikel</trd>
        </trdgrp>
      </ekz>
      <ekz>
        vezikecaj <ind><tld/>oj de l' ovario</ind>,
        <trd lng="fr">follicule ovarien</trd>
        <trd lng="it">follicoli ovarici</trd>
        <trdgrp lng="nl">
          <trd>eifollikel</trd>,
          <trd>follikel van De Graaf</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>limfa <tld/>o</ind>,
          <trd lng="it">follicolo linfatico</trd>
      </ekz>
      <ekz>
         intestaj <tld/>oj.
      </ekz>
    </dif>
    <trd lng="de">Follikel</trd>
    <trd lng="en">follicle <klr>(anatomy)</klr></trd>
    <trd lng="nl">follikel</trd>
    <trdgrp lng="zh">
    <trd>&#39592;&#31361; <pr>g&#468;t&umacron;</pr></trd>,
    <trd>&#28388;&#27873; <pr>l&uuml;p&agrave;o</pr></trd>,
    <trd>&#33991; <pr>g&#468;</pr></trd>,
    <trd>&#23567;&#22218; <pr>xi&#462;on&aacute;ng</pr></trd>,
    <trd>&#33991;&#33878;&#26524; <pr>g&#468;t&uacute;gu&#466;</pr></trd>,
    <trd>&#33878; <pr>t&uacute;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_ib;&c_k;&c_u;&c_l;</trd>,
    <trd>&c_m;&c_ja;&c_sh;&c_e;&c_ch;&c_a;&c_k; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;., &c_b;&c_a;&c_t;.)</klr></trd>,
    <trd>&c_t;&c_o;&c_r;&c_b;&c_a;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;., &c_b;&c_a;&c_t;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>folikul</trd>,
    <trd>m&iacute;&scaron;ek</trd>,
    <trd>m&ecaron;ch&yacute;&rcaron;ek</trd>,
    <trd>v&aacute;&ccaron;ek</trd>
  </trdgrp>
  <trd lng="fr">follicule</trd>
  <trd lng="hu">t&uuml;sz&odblac;</trd>
  <trd lng="it">follicolo</trd>
  <trdgrp lng="ja">
    <trd>&#34955;&#26524;  <pr>&#12383;&#12356;&#12363;</pr></trd>,
    <trd>&#23567;&#32990;  <pr>&#12375;&#12423;&#12358;&#12411;&#12358;</pr></trd>,
    <trd>&#28670;&#32990;  <pr>&#12429;&#12411;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">torebka</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_l;&c_l;&c_i;&c_k;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_sh;&c_o;&c_ch;&c_je;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>folikul</trd>,
    <trd>mie&scaron;ok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a; (&c_r;&c_ib;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d; &c_s;&c_u;&c_h;&c_o;&c_g;&c_o;
    &c_p;&c_l;&c_o;&c_d;&c_u;)</trd>,
    <trd>&c_m;&c_ib;&c_sh;&c_je;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_u;&c_z;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_f;&c_o;&c_l;&c_ib;&c_k;&c_u;&c_l;</trd>
  </trdgrp>
</drv>
</art>
