<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>folikl</rad>/o
</kap>
<drv mrk="folikl.0o">
  <kap><tld/>o</kap>
  <snc mrk="folikl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Unufaka <ref tip="super" cel="frukt.malferm0o">malfermfrukto</ref>
      esti&gcirc;anta el unuopa fruktofolio kaj havanta nur unu fendon
      malfermi&gcirc;anta &ccirc;e maturi&gcirc;o, ekz. &ccirc;e kelkaj
      ranunkolacoj.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="gusx.0o">gu&scirc;o</ref>.
    </rim>
  </snc>
  <snc mrk="folikl.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Nomo de diversaj saketformaj organoj, haroj, glandoj:
      <ekz>
        dento<tld/>o, har<tld/>o, vizikecaj <tld/>oj de l' ovario, limfa
        <tld/>o, intestaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
