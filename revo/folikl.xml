<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>folikl</rad>/o</kap>
<drv mrk="folikl.0o">
  <kap><tld/>o</kap>
  <snc mrk="folikl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Unufaka <ref tip="super" cel="frukt.malferm0o">malfermfrukto</ref>
      esti&gcirc;anta el unuopa fruktofolio kaj havanta nur unu fendon
      malfermi&gcirc;anta &ccirc;e maturi&gcirc;o, ekz-e &ccirc;e kelkaj
      ranunkolacoj.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="gusx.0o">gu&scirc;o</ref>.
    </rim>
    <trd lng="de">Fruchtbalg</trd>
    <trdgrp lng="nl">
      <trd>zaadvlies</trd>,
      <trd>zaadhulsel</trd>
    </trdgrp>
  </snc>
  <snc mrk="folikl.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Nomo de diversaj saketformaj organoj, haroj, glandoj:
      <ekz>
        <ind>dento<tld/>o</ind>,
        <trd lng="fr">follicule dentaire</trd>
        <trd lng="nl">tandzakje</trd>
      </ekz>
      <ekz>
        <ind>har<tld/>o</ind>,
        <trd lng="fr">follicule pileux</trd>
        <trdgrp lng="nl">
          <trd>haarzakje</trd>,
          <trd>haarfollikel</trd>
        </trdgrp>
      </ekz>
      <ekz>
        vezikecaj <ind><tld/>oj de l' ovario</ind>,
        <trd lng="fr">follicule ovarien</trd>
        <trdgrp lng="nl">
          <trd>eifollikel</trd>,
          <trd>follikel van De Graaf</trd>
        </trdgrp>
      </ekz>
      <ekz>
        limfa
        <tld/>o, intestaj <tld/>oj.
      </ekz>
    </dif>
    <trd lng="de">Follikel</trd>
    <trd lng="nl">follikel</trd>
  </snc>
  <trd lng="fr">follicule</trd>
  <trd lng="hu">t&uuml;sz&odblac;</trd>
  <trd lng="pl">torebka</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_l;&c_l;&c_i;&c_k;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_sh;&c_o;&c_ch;&c_je;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>
  </trdgrp>
</drv>
</art>
