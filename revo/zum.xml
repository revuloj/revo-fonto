<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>zum</rad>/i
</kap>
<drv mrk="zum.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <dif>
    A&ubreve;digi kontinuan, malakutan kaj obtuzan bruon:
  </dif>
  <snc>
    <dif>
      pp insektoj, kies flugado faras zo-sonon:
      <ekz>
        <tld/>as abelo, vespo, grilo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mu&scirc;o <tld/>as al li en la kapo
        <klr>(li havas plej strangajn ideojn)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dancario <tld/>is en lia cerbo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en konfuza miksamaso <tld/>as la pensoj en mia kapo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        surdiga <ind><tld/>ado</ind> de la kugloj.
        <trdgrp lng="be">
          <trd>&c_zh;&c_u;&c_zh;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
          <trd>&c_g;&c_u;&c_l;</trd>,
          <trd>&c_sh;&c_u;&c_m;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_zh;&c_u;&c_zh;&c_zh;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_g;&c_u;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_u;&c_s;&c_mol;&c_c;&c_ib;
      <klr>(&c_a;&c_b; &c_n;&c_a;&c_s;&c_ja;&c_k;&c_o;&c_m;&c_y;&c_h;,
      &c_a;&c_b; &c_m;&c_a;&c_t;&c_o;&c_r;&c_y;)</klr></trd>
    <trd lng="de">summen</trd>
    <trd lng="el">&beta;&omikron;&ypsilon;&jota_ton;&zeta;&omega;</trd>
    <trd lng="es">zumbar</trd>
    <trdgrp lng="fr">
      <trd>bourdonner</trd>
    </trdgrp>
    <trd lng="it">ronzare</trd>
    <trd lng="nl">zoemen</trd>
    <trdgrp lng="pt">
      <trd>zumbir</trd>,
      <trd>zunir</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_u;&c_zh;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_g;&c_u;&c_d;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">surra</trd>
  </snc>
  <snc>
    <dif>
      pp persono murmuranta a&ubreve; kantanta kun preska&ubreve; fermita
bu&scirc;o:
      <ekz>
        <tld/>i kanton.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_r;&c_m;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;
        &c_p;&c_r;&c_a; &c_s;&c_ja;&c_b;&c_je;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;
        &c_n;&c_a;&c_p;&c_a;&c_w;&c_g;&c_o;&c_l;&c_a;&c_s;&c_a;</trd>
    </trdgrp>
    <trd lng="el">&beta;&omikron;&ypsilon;&jota_ton;&zeta;&omega;</trd>
    <trd lng="es">canturrear</trd>
    <trdgrp lng="fr">
      <trd>fredonner</trd>
    </trdgrp>
    <trd lng="it">ronzare</trd>
    <trd lng="nl">neuri&euml;n</trd>
    <trd lng="pt">zumbir</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_o;&c_r;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_g;&c_u;&c_d;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>mumla</trd>,
      <trd>gnola</trd>,
      <trd>nynna</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      pp konfuza bruo de parolantaj personoj:
      <ekz>
        sen&ccirc;esa <tld/>ado de la langoj;
      </ekz>
      <ekz>
        la malla&ubreve;ta <tld/>ado de la konversacioj.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_u;&c_m;&c_je;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>bourdonner</trd>
    </trdgrp>
    <trd lng="nl">gonzen</trd>
    <trd lng="ru">&c_sh;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
    <trd lng="sv">sorl</trd>
  </snc>
  <trd lng="he">&lamed;&zayin;&mem;&zayin;&fmem;</trd>
</drv>
</art>
