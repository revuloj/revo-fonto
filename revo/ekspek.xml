<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekspek.xml,v 1.14 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekspekt</rad>/o
</kap>
<drv mrk="ekspek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="variab.hazarda0o">hazarda variablo</ref>
      <frm am="X">X</frm>,
      kadre de la <ref cel="spac.probablo0o">probablospaco</ref>
      <frm am="(bb Omega,bb A,P)">(<g>&Omega;</g>,<g>A</g>,P)</frm>)</klr>
      La <ref cel="integr2.lebega0o">lebega integralo</ref> de
      <frm am="X">X</frm> 
      sur <frm am="bb Omega"><g>&Omega;</g></frm> rilate al mezuro 
      <frm am="P">P</frm>;
      simb. <frm am="E(X)=int XdP = int x dP_X(x)">E(X) = &#x222B;XdP = &#x222B;<k>x</k>dP<sub>X</sub>(<k>x</k>)</frm>:
      <ekz>
        la <tld/>o estas la <ref cel="momant.noa0o">unua momanto</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="ca">
    <trd>esperan&ccedil;a</trd>,
    <trd>valor esperat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>matematick&aacute; nad&ecaron;je</trd>,
    <trd>st&rcaron;edn&iacute; hodnota <klr>(n&aacute;hodn&ecaron; prom&ecaron;nn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="de">Erwartungswert</trd>
  <trd lng="en">expectation<klr tip="ind"> (of a random
  variable)</klr></trd>
  <trd lng="es">esperanza matem&aacute;tica</trd>
  <trd lng="fr">esp&eacute;rance math&eacute;matique</trd>
  <trd lng="hu">v&aacute;rhat&oacute; &eacute;rt&eacute;k</trd>
  <trd lng="pl">warto&sacute;&cacute; oczekiwana<klr tip="ind"> (zmiennej
  losowej)</klr></trd>
  <trd lng="pt">esperan&ccedil;a matem&aacute;tica</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_i;&c_je;</ind></trd>
  <trdgrp lng="sk">
    <trd>matematick&aacute; n&aacute;dej</trd>,
    <trd>stredn&aacute; hodnota</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9810;&#x671F;&#x503C; <pr>y&#xF9;q&#x12B;zh&#xED;</pr></trd>,
    <trd>&#x9810;&#x8A08;&#x503C; <pr>y&#xF9;j&#xEC;zh&#xED;</pr></trd>,
    <trd>&#x6240;&#x6599; <pr>su&#x1D2;li&#xE0;o</pr></trd>,
    <trd>&#x6570;&#x5B66;&#x671F;&#x671B; <pr>sh&#xF9;xu&#xE9;q&#x12B;w&#xE0;ng</pr></trd>,
    <trd>&#x6570;&#x5B66;&#x671F;&#x671B;&#x503C; <pr>sh&#xF9;xu&#xE9;q&#x12B;w&#xE0;ngzh&#xED;</pr></trd>,
    <trd>&#x671F;&#x671B;&#x503C; <pr>q&#x12B;w&#xE0;ngzh&#xED;</pr></trd>,
    <trd>&#x671F;&#x5F85;&#x503C; <pr>q&#x12B;d&#xE0;izh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>

