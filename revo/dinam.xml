    <trd lng="en">dynamo</trd>
    <trd lng="es">d&iacute;namo</trd>
    <trd lng="fr">dynamo</trd>
    <trdgrp lng="hu">
      <trd>dinam&oacute;</trd>,
      <trd>&aacute;ramfejleszt&odblac;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dynamo</trd>,
      <trd>pr&aogonek;dnica</trd>,
      <trd>alternator</trd>,
      <trd>generator<klr tip="amb"> pr&aogonek;du</klr></trd>
    </trdgrp>
    <trd lng="pt">d&iacute;namo</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_n;&c_a;&c_m;&c_o;-&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
    </trdgrp>
  </drv>
</art>
