<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>prim</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="prim.0o">
<kap><tld/>o</kap>
<uzo tip="fak">MAT</uzo>
<snc mrk="prim.0o.entjero">
<dif>
<ref cel="entjer.natura0o">Natura entjero</ref>,
dividebla nur per si mem kaj per la nombro unu:
<ekz>
   sep, dek tri,... estas <tld/>oj.
</ekz>
</dif>
<trd lng="en">prime number</trd>
<trd lng="de">Primzahl</trd>
<trd lng="fr">nombre <ind>premier</ind></trd>
<trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_je; <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
</snc>
       
       <snc mrk="prim.0o.ringo">
         <dif>
           Tia <ref cel="invers.ne0igebla">neinversigebla</ref> elemento p
           de <ref cel="komut.0eca.ringo">komuteca</ref>, 
           <ref cel="unu.0hava">unuhava</ref> 
           <ref cel="ring.0o.MAT">ringo</ref>, ke &gcirc;i 
           <ref cel="nask.0i.MAT">naskas</ref> 
           <ref cel="prim.0a.idealo"><tld/>an</ref> 
           <ref cel="ideal.0o.MAT">idealon</ref>; alidire p estas tia, ke se
           &gcirc;i estas <ref cel="divizo.0o.ringo">divizoro</ref> de la 
           <ref cel="produt.0o">produto</ref> x.y, tiam &gcirc;i nepre
           estas divizoro de x a&ubreve; de y:
           <ekz>
             en <ref cel="integr.0a.MAT">integra</ref> ringo, <tld/>o ne havas
             <ref cel="divizo.propra0o">proprajn divizorojn</ref>.
           </ekz>
         </dif>
         <trd lng="en">prime<klr tip="amb"> (element)</klr></trd>
         <trd lng="de">Primelement</trd>
         <trd lng="fr">&eacute;l&eacute;ment <ind>premier</ind></trd>
         <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_j; &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
       </snc>

</drv>

     <drv mrk="prim.0a">
       <kap><tld/>a</kap>
       <uzo tip="fak">MAT</uzo>
       <snc mrk="prim.0a.elemento">
         <dif>
           Estanta <tld/>o:
           <ekz>
             <tld/>a faktoro.
           </ekz>
         </dif>
         <refgrp tip="vid">
           <ref cel="redukt.ne0ebla">nereduktebla</ref>.
         </refgrp>
         <trd lng="fr">premier <klr tip="ind">(math.)</klr></trd>
         <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_j;</trd>
       </snc>
       <snc mrk="prim.0a.idealo">
         <dif>
           <klr>(p.p. <ref cel="ideal.0o.MAT">idealo</ref>)</klr>
           Tia, ke se produto apartenas al &gcirc;i, tiam nepre
           apartenas al &gcirc;i almena&ubreve; unu el la faktoroj:
           <ekz>
             la idealo de &ccirc;iuj obloj de 4 ne estas <tld/>a, &ccirc;ar
             6&#xd7;10 apartenas al &gcirc;i, sed nek 6, nek 10 estas
             obloj de 4.
           </ekz>
         </dif>
       </snc>
       <snc mrk="prim.0a.intersi">
         <dif>
           <klr>(&ccirc;iam plurale, kun aldono "inter si"
           a&ubreve; "relative", p.p. <ref cel="entjer.0o">entjeroj</ref> 
           a&ubreve; elementoj de komuteca unuhava 
           <ref cel="ring.0o.MAT">ringo</ref>)</klr>
           
           Tiaj, ke iliaj komunaj <ref cel="divizo.0o">divizoroj</ref>
           &ccirc;iuj estas <ref cel="invers.0igebla.MAT">inversigeblaj</ref>;
           alidire, p.p. entjeroj, ilia plej granda komuna divizoro estas
           la nombro unu:
           <ekz>
             dek kaj dudek unu ne estas <tld/>aj, sed ili estas
             <tld/>aj inter si.
           </ekz>
         </dif>
         <trd lng="en">relatively <ind>prime</ind><klr tip="amb"> (elements)</klr></trd>
         <trd lng="de">relativ <ind>prime</ind><klr tip="amb"> (Elemente)</klr></trd>
         <trd lng="fr"><klr tip="amb">(&eacute;l&eacute;ments) </klr><ind>premiers</ind> entre eux</trd>
         <trd lng="ru">&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o;-&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;&c_je;<klr tip="amb"> (&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;&c_y;)</klr></trd>
       </snc>

     </drv>

</art>

