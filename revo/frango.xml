<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>frangol</rad>/o</kap>
<drv mrk="frango.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_listoj">Genro</ref> el familio <ref
      tip="malprt" cel="ramnac.0oj">ramnacoj</ref>
      (<trd lng="la">Frangula</trd>), arbeto, kies beroj kaj
      &scirc;elo estas uzataj kiel laksigilo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Faulbaum</trd>
  <trd lng="fr">bourdaine</trd>
  <trd lng="hu">kutyabenge</trd>
  <trdgrp lng="nl">
    <trd>vuilboom</trd>,
    <trd>sporkeboom</trd>,
    <trd>zwarte <ind>els</ind></trd>
  </trdgrp>
  <trd lng="pt">amieiro-negro</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_sh;&c_i;&c_n;&c_a; &c_l;&c_o;&c_m;&c_k;&c_a;&c_ja;</trd>
</drv>
</art>
