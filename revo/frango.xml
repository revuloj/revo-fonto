<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frangol</rad>/o
</kap>
<drv mrk="frango.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ramnac.0oj">ramnacoj</ref> 
      (<trd lng="la">Frangula</trd>), arbeto, kies beroj k
      &scirc;elo estas uzataj kiel laksigilo.  
    </dif>
  </snc>
  <trd lng="de">Faulbaum</trd>
  <trd lng="fr">bourdaine</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_sh;&c_i;&c_n;&c_a; &c_l;&c_o;&c_m;&c_k;&c_a;&c_ja;</trd>
</drv>
</art>
