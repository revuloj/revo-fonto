<drv mrk="scen.0o">
  <kap><ofc>*</ofc><tld/>o<fnt><bib>F</bib>, <lok>&FE; 2 k
  4</lok></fnt></kap>
  <snc mrk="scen.0o.KOMUNE">
    <dif>
      Emociiga a&ubreve; interesa vidata <ref tip="super" cel="okaz.0o.travivajxo">okazo</ref>:
      <ekz>
        instrua, neforgesebla, na&ubreve;za <tld/>o;
      </ekz>
      <ekz>
        unu <tld/>o fiksi&gcirc;is en la memoro de Ramzes: sur la deklivo de
        monteto
        laboris tri gruoj: unu ver&scirc;is akvon el la rivero en la plej
        malaltan puton, la dua &ccirc;erpis el &gcirc;i <klr>[&#8230;]</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 14a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sola efektive erotika <tld/>o de la libro
        <fnt>
          <aut>S. Johansson</aut>
          <vrk><url ref="&Monato;/1996/005079.php">Hamburgero ne tre erotika</url></vrk>
          <bib>Monato</bib>
          <lok>1996/07, p. 22</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="scen.0o.TEA">
      <uzo tip="fak">TEA</uzo>
      <dif>
      Senrompa agado de samaj rolantoj,
      subparto de teatra <ref tip="malprt" cel="akt.0o.TEA">akto</ref>:
      <ekz>
        la tuta <tld/>o estas parolata per malla&ubreve;tigata vo&ccirc;o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        muta <klr>(senparola)</klr> <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rakontis kelke da komikaj <tld/>oj el la komedioj de Holberg
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <klr>[...]</klr> re&gcirc;isoro sukcese kreas suspenson ekde
        la unua <tld/>o
        <fnt>
          <aut>M. Ecott</aut>
          <vrk><url ref="&Monato;/2001/006838.html">Inkubo en Kubo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="id">
      <trd>adegan</trd>,
      <trd>babak</trd>
    </trdgrp>
    </subsnc>
    <subsnc mrk="scen.0o.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Figurado de homoj en tipa situacio:
        <ekz>
          &ccirc;iuj muroj estis kovritaj per pentra&jcirc;oj, prezentantaj
          <tld/>ojn el la vivo de l' anta&ubreve;uloj de Sofra: militojn, marajn voja&gcirc;ojn kaj
          &ccirc;asojn
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro 24a</lok>
          </fnt>;
        </ekz>
        <ekz>
           Desegnoj kaj fotoj <klr>[&#8230;]</klr> &leftquot;tradukas&rightquot; <tld/>on
           tridimensian en dudimensian surfacon
          <fnt>
          <aut>G. Cazzaro</aut>
          <vrk><url ref="&Monato;/1995/A950101.php">La modo de fiksokuloj</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 12</lok>
        </fnt>;
        </ekz>
        <ekz>
          vi tie trovas vere altvaloran vazaron kun la &scirc;atataj
          &ccirc;aso<tld/>oj, kiajn oni vidas sur malnovaj gobelenoj
          a&ubreve; sur anglaj gravura&jcirc;oj
          <fnt>
            <aut>-</aut>,
            <vrk><url
            ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/arto.html">
            Arto sur la tablo</url></vrk>,
            <lok>Norda Prismo, 1958:2</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="de">Szene</trd>
    <trd lng="pl">scena</trd>
    <trd lng="pt">cena</trd>
  </snc>
  <snc mrk="scen.0o.scenejo">
    <dif>
      <ref tip="dif" cel="scen.0ejo"><tld lit="S"/>ejo</ref>:
      <ekz>
        oni a&ubreve;das pafojn post la <tld/>o
          <fnt>
            <bib>Hamlet</bib>
          </fnt>;
      </ekz>
      <ekz>
        &gcirc;i neniam estis donata sur la <tld/>o
          <fnt>
            <bib>Hamlet</bib>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia persono tute foriras de la <tld/>o
          <fnt>
            <bib>DL</bib>
          </fnt>
        <klr>(rezignas je publika vidateco)</klr>.
      </ekz>
    </dif>
    <trd lng="de">B&uuml;hne</trd>
    <trdgrp lng="id">
      <trd>layar</trd>,
      <trd>panggung</trd>,
      <trd>pentas</trd>
    </trdgrp>
    <trd lng="nl">toneel</trd>
    <trd lng="pl">scena</trd>
    <trdgrp lng="pt">
      <trd>cen&aacute;rio</trd>,
      <trd>palco</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_c;&c_e;&c_n;&c_a;</trd>
  <trd lng="bg">&c_s;&c_c;&c_je;&c_n;&c_a;</trd>
  <trd lng="ca">escena</trd>
  <trdgrp lng="cs">
    <trd>sc&eacute;na</trd>,
    <trd>v&yacute;jev</trd>,
    <trd>v&yacute;stup</trd>
  </trdgrp>
  <trd lng="en">scene</trd>
  <trd lng="es">escena</trd>
  <trd lng="fr">sc&egrave;ne</trd>
  <trd lng="he">&samekh;&tsadi;&yod;&nun;&he;</trd>
  <trdgrp lng="hu">
    <trd>jelenet</trd>,
    <trd>sz&iacute;n</trd>
  </trdgrp>
  <trd lng="id"><baz>pandang</baz>pemandangan</trd>
  <trd lng="it">scena</trd>
  <trdgrp lng="ja">
    <trd>&#22580;  <pr>&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#22580;&#38754;  <pr>&#12400;&#12417;&#12435;</pr></trd>,
    <trd>&#20809;&#26223;  <pr>&#12371;&#12358;&#12369;&#12356;</pr></trd>,
    <trd>&#12471;&#12540;&#12531;  <pr>&#12375;&#12540;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">sc&egrave;ne</trd>
  <trd lng="ru">&c_s;&c_c;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>sc&eacute;na</trd>,
    <trd>v&yacute;jav</trd>,
    <trd>v&yacute;stup</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_c;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_ja;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20809;&#26223; <pr>gu&amacron;ngj&#464;ng</pr></trd>,
    <trd>&#22330;&#38754; <pr>ch&#462;ngmi&agrave;n</pr></trd>,
    <trd>&#22580;&#38754; <pr>ch&#462;ngmi&agrave;n</pr></trd>,
    <trd>&#24773;&#26223; <pr>q&iacute;ngj&#464;ng</pr></trd>,
    <trd>&#30011;&#38754; <pr>hu&agrave;mi&agrave;n</pr></trd>,
    <trd>&#30059;&#38754; <pr>hu&agrave;mi&agrave;n</pr></trd>,
    <trd>&#22330;&#26223; <pr>ch&#462;ngj&#464;ng</pr></trd>,
    <trd>&#22580;&#26223; <pr>ch&#462;ngj&#464;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="scen.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="scen.0aro.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Skizo de teatra&jcirc;o a&ubreve; kina&jcirc;o, prezentanta la
      rolulojn, lokojn, situaciojn, &ccirc;efajn scenojn:
      <ekz>
        vi estas verkanto de via propra viv-<tld/>aro
        <fnt>
          <aut>R. Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        surbaze de kolektita materialo <klr>[ili]</klr> skribis
        <tld/>aron kaj sekve preparis spektaklon
        <fnt>
          <aut>Z. Banet-Fornalowa</aut>
          <vrk><url ref="&Monato;/2004/008391.html">Listo de
          Sendler</url></vrk>
          <bib>Monato</bib>
          <lok>2004/11, p. 18</lok>
        </fnt>.
      </ekz>
      <trd lng="ca">gui&oacute;</trd>
    </dif>
    <trdgrp lng="de">
      <trd>Szenenfolge</trd>,
      <trd>Handlung</trd>
    </trdgrp>
    <trd lng="pt">roteiro</trd>
  </snc>
  <snc mrk="scen.0aro.klarigo">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Klarigo de natura, socia a&ubreve; te&hcirc;nika evoluo per elpensita, hipoteza
      sinsekvo kvaza&ubreve; en <tld/>olibro:
      <ekz>
        advokato <klr>[...]</klr> deklaris, ke la saman <tld/>aron uzis
        la polico anta&ubreve; du jaroj
        <fnt>
          <aut>G. van Damme</aut>
          <vrk>Kiu faris kion?</vrk>
          <bib>Monato</bib>
          <lok>1996/11, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu el tiuj du <tld/>aroj <klr>(por klarigi ekeston de
        Venuso)</klr>
        estas vere pravigebla, certe ne la&ubreve; aktualaj scioj
        <fnt>
        <aut>M. Halvelik</aut>
          <vrk><url ref="&Monato;/1993/A931102.php">Kaj la suno haltis ...</url></vrk>
          <bib>Monato</bib>
          <lok>1993/11, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        povus esti, ke timoj kaj teruradaj <tld/>aroj pri eksplodonta
        monda lo&gcirc;antaro, priskribitaj anta&ubreve; 50 jaroj, ne reali&gcirc;os
        <fnt>
          <aut>S. Maul</aut>
          <vrk><url ref="&Monato;/2003/008117.html">Grizi&gcirc;anta
          mondo</url></vrk>
          <bib>Monato</bib>
          <lok>2003/05, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom realisma estas tiu <tld/>aro
        <fnt>
          <aut>M. Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008735.php">&Ccirc;u la
          tero fari&gcirc;os sa&ubreve;no?</url></vrk>
          <bib>Monato</bib>
          <lok>2005/11, p. 17</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="pt">cen&aacute;rio</trd>
  </snc>
  <trd lng="be">&c_s;&c_c;&c_e;&c_n;&c_a;&c_r;</trd>
  <trd lng="cs">sc&eacute;n&aacute;&rcaron;</trd>
  <trdgrp lng="de">
    <trd>Szenario</trd>,
    <trd>Ablauf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scenario</trd>,
    <trd>screenplay</trd>
  </trdgrp>
  <trd lng="es">guion</trd>
  <trd lng="fr">sc&eacute;nario</trd>
  <trdgrp lng="he">
    <trd>&tav;&samekh;&resh;&yod;&tet;</trd>,
    <trd>&tav;&resh;&het;&yod;&shin;</trd>
  </trdgrp>
  <trd lng="hu">forgat&oacute;k&ouml;nyv</trd>
  <trdgrp lng="id">
    <trd>lakon</trd>,
    <trd>skenario</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>scenario<klr> (sequenza di scene)</klr></trd>,
    <trd>sceneggiatura</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12471;&#12490;&#12522;&#12458;  <pr>&#12375;&#12394;&#12426;&#12362;</pr></trd>,
    <trd>&#33050;&#26412;  <pr>&#12365;&#12419;&#12367;&#12411;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>draaiboek</trd>,
    <trd>scenario</trd>
  </trdgrp>
  <trd lng="pl">scenariusz</trd>
  <trd lng="ru">&c_s;&c_c;&c_je;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">scen&aacute;r</trd>
  <trd lng="uk">&c_s;&c_c;&c_je;&c_n;&c_a;&c_r;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#21095;&#26412; <pr>j&ugrave;b&ecaron;n</pr></trd>,
    <trd>&#21127;&#26412; <pr>j&ugrave;b&ecaron;n</pr></trd>,
    <trd>&#21095;&#24773; <pr>j&ugrave;q&iacute;ng</pr></trd>,
    <trd>&#21127;&#24773; <pr>j&ugrave;q&iacute;ng</pr></trd>,
    <trd>&#30005;&#24433;&#21095;&#26412; <pr>di&agrave;ny&#464;ngj&ugrave;b&ecaron;n</pr></trd>,
    <trd>&#38651;&#24433;&#21127;&#26412; <pr>di&agrave;ny&#464;ngj&ugrave;b&ecaron;n</pr></trd>,
    <trd>&#24773;&#31680; <pr>q&iacute;ngji&eacute;</pr></trd>,
    <trd>&#24773;&#33410; <pr>q&iacute;ngji&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="scen.0aristo">
  <kap><tld/>aristo</kap>
  <snc mrk="scen.0aristo.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Verkisto de <tld/>aroj:
      <ekz>
        la generaloj &ndash; rekte enhaki&gcirc;is en la
        disputon la <tld/>aristo Glu&hcirc;arjov
        <fnt><bib>MkM</bib> <lok>&ccirc;apitro 5a, p. 49a</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>aristo estas Genc Permeti el Albanio, kaj la re&gcirc;isoro
        Goran Pakalevi&cacute; el Serbio
        <fnt>
          <aut>B. Selimi</aut>
          <vrk><url ref="&Monato;/2008/009618.php">Kunlaboro
          mielmonata</url></vrk>
           <bib>Monato</bib>
          <lok>2008/11, p. 09</lok>
        </fnt>.
