<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sibl.xml,v 1.20 2021/05/15 16:02:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sibl</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sibl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sibl.0i.SON">
    <dif>
      Eligi sonon similan al tirata /s/.
    </dif>
    <subsnc mrk="sibl.0i.iu">
      <uzo tip="klr">(iu)</uzo>
      <dif>
        Senintence eligi tian sonon:
        <ekz>
          vi iom <tld/>as, vi havas en la bu&scirc;o unu denton, kiu
          fajfas<fnt>Z</fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sibl.0i.io">
      <uzo tip="klr">(besto a&ubreve; a&jcirc;o)</uzo>
      <dif>
        Simile <ref tip="lst" cel="blek.0o"
        lst="voko:blekoj">bleki</ref>
        a&ubreve; brui:
        <ekz>
          <tld/>as serpento, merlo, sago, vento<fnt>Z</fnt>, kuglo.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="fajf.0i.SON">fajfi</ref>,
      <ref cel="sxusx.0i">&scirc;u&scirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_s;&c_ib;&c_p;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">xiular <klr>(agut)</klr></trd>
  <trdgrp lng="de">
    <trd>pfeifen</trd>,
    <trd>zischen</trd>
  </trdgrp>
  <trd lng="en">hiss</trd>
  <trd lng="es">silbar</trd>
  <trd lng="fr">siffler<klr> (tr&egrave;s aigu)</klr></trd>
  <trdgrp lng="nl">
    <trd>sissen</trd>,
    <trd>fluiten</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sziszeg</trd>,
    <trd>s&uuml;v&iacute;t</trd>,
    <trd>s&uuml;v&ouml;lt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_v;&c_i;&c_s;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;<klr> (&c_i;&c_z;-&c_z;&c_a; &c_d;&c_je;&c_f;&c_je;&c_k;&c_t;&c_a; &c_r;&c_je;&c_ch;&c_i;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>svi&scaron;&tcaron;a&tcaron;</trd>,
    <trd>sy&ccaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>svi&scaron;t&ecaron;t</trd>,
    <trd>sy&ccaron;et</trd>
  </trdgrp>
</drv>

<drv mrk="sibl.0a">
  <kap><tld/>a<fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="sibl.0a.KOMUNE">
    <dif>
      Tia, ke &gcirc;i <tld/>as:
      <ekz>
        Dorina trankvile levis la a&ubreve;skultilon, sed <tld/>a vira vo&ccirc;o
        glaciigis &scirc;in
        <fnt>
          <aut>J. Modest</aut>
          <vrk><url
              ref="http://www.esperanto.be/fel/mon/005343.html"
              >En la ombro de la silento</url></vrk>.
          <bib>Monato</bib>
          <lok>1997:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">FON</uzo>
        <ref cel="sibl.0anto.FON"><tld/>a konsonanto</ref>;
      </ekz>
      <ekz>
        <uzo tip="fak">MED</uzo>
        <tld/>a spirado.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sifflant</trd>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;</trd>
</drv>

<drv mrk="sibl.0o">
  <kap><tld/>o,
  <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="sibl.0o.KOMUNE">
    <dif>
      Ago <tld/>i;
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>
      de io a&ubreve; iu <tld/>anta:
      <ekz>
        la du lastajn vortojn &scirc;i elparolis kun akcento, kun
        <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sifflement</trd>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="sibl.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="sibl.0anto.KOMUNE">
    <dif>
      Iu kiu <tld/>as.
    </dif>
  </snc>
  <snc mrk="sibl.0anto.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="dent.lang0akonsonanto">Lang-denta</ref>
      <ref tip="super" cel="frot.0sono.FON">frota konsonanto</ref>,
      kiel la esperantaj /s/ kaj /z/, a&ubreve;
      <ref cel="frot.ek0akonsonanto">ekfrota konsonanto</ref>,
      entenanta tian parolsonon (kiel la esperanta <ctl>c</ctl>).
    </dif>
    <ref tip="vid" cel="sxusx.0a_konsonanto">&scirc;u&scirc;a konsonanto</ref>
    <trd lng="en">hissing sibilant</trd>
    <trd lng="fr">sifflante</trd>
    <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>sibilanta</trd>,
    <trd>sykavka</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;<klr> (&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">sykavka</trd>
</drv>


</art>
