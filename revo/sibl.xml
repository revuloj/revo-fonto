<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sibl</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sibl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sibl.0i.SON">
    <dif>
      Tre akute <ref tip="super" cel="fajf.0i.SON">fajfi</ref>.
    </dif>
    <subsnc mrk="sibl.0i.iu">
      <uzo tip="klr">(iu)</uzo>
      <dif>
        Senintence eligi tre akutan, ne muzikan sonon:
        <ekz>
          vi iom <tld/>as, vi havas en la bu&scirc;o unu denton, kiu
          fajfas<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la du lastajn vortojn &scirc;i elparolis kun akcento, kun
          <tld/>o<fnt>B</fnt>;
        </ekz>
        <ekz>
          <tld/>a litero;
        </ekz>
        <ekz>
          <tld/>a spirado.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sibl.0i.io">
      <uzo tip="klr">(besto a&ubreve; a&jcirc;o)</uzo>
      <dif>
        Eligi plej akutan sonon:
        <ekz>
          <tld/>as serpento, merlo, vento<fnt>Z</fnt>,
        </ekz>
        <ekz>
          kuglo, abelaro<fnt>B</fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_s;&c_ib;&c_p;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pfeifen</trd>,
    <trd>zischen</trd>
  </trdgrp>
  <trd lng="en">hiss</trd>
  <trd lng="es">silbar</trd>
  <trd lng="fr">siffler<klr> (tr&egrave;s aigu)</klr></trd>
  <trdgrp lng="nl">
    <trd>sissen</trd>,
    <trd>fluiten</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sziszeg</trd>,
    <trd>s&uuml;v&iacute;t</trd>,
    <trd>s&uuml;v&ouml;lt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
