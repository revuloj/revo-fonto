<vortaro>
<art mrk="$Id: sibl.xml,v 1.27 2024/03/24 20:21:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sibl</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sibl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sibl.0i.SON">
    <dif>
      Eligi sonon similan al tirata /s/.
    </dif>
    <subsnc mrk="sibl.0i.parole">
      <uzo tip="klr">(iu)</uzo>
      <dif>
        Senintence eligi tian sonon, <ref tip="sub" cel="lisp.0i">lispi</ref>:
        <ekz>
          vi iom <tld/>as, vi havas <klr>[&#x2026;]</klr> en la bu&scirc;o unu denton, kiu
          fajfas
          <fnt>
            <bib>Revizoro</bib>
            <lok>Akto unua</lok>
          </fnt>;
        </ekz>
        <ekz>
          la vortoj <tld/>is en &scirc;ia bu&scirc;o, kiu estis preska&ubreve; tiel blanka kiel la papero
          <fnt>
            <bib>Marta</bib>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>lispeln</trd>,
      </trdgrp>
    </subsnc>
    <subsnc mrk="sibl.0i.brue">
      <uzo tip="klr">(besto a&ubreve; a&jcirc;o)</uzo>
      <dif>
        Simile <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleki</ref>
        a&ubreve; brui:
        <ekz>
          la serpento ek<tld/>is kaj forrampis
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          la vento <klr>[&#x2026;]</klr> <tld/>as en la kamentubon kaj en la kamenon
          <fnt>
            <bib>Fab4</bib>
            <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          la aero resonadis <klr>[&#x2026;]</klr> de <tld/>antaj sagoj
          <fnt>
            <bib>Fab4</bib>
            <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;irka&ubreve;<tld/>ata de aspidoj
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Tria</lok>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;iuj malgrandaj musfra&ubreve;lino <klr>[&#x2026;]</klr> <tld/>adis kaj paroletadis, kvaza&ubreve;
          dezirante moki unu la alian
          <fnt>
            <bib>Fab2</bib>
            <lok>Dio de dormo</lok>
          </fnt>;
        </ekz>
        <ekz>
          eksonis malla&ubreve;ta <tld/>ado kaj la tor&ccirc;o ekflamis
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XIX</lok>
          </fnt>;
        </ekz>
        <ekz>
          la akvo ek<tld/>is <klr>(ek<ref cel="bol.0i">bolis</ref>)</klr> en la kaldrono
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro XI</lok>
          </fnt>;
        </ekz>
        <ekz>
          la kla&ccirc;ado <klr>[&#x2026;]</klr> <tld/>is tra &ccirc;iuj domoj, stratoj kaj stratetoj
          <fnt>
            <bib>Fab3</bib>
            <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la profundo <tld/>is la malvarma vento, kriis la lupoj
          <fnt>
            <bib>Fab2</bib>
            <lok>Ne&gcirc;a re&gcirc;ino</lok>
          </fnt>.
        </ekz>
      </dif>        
    </subsnc>
    <refgrp tip="vid">
      <ref cel="fajf.0i.SON">fajfi</ref>,
      <ref cel="flustr.0i">flustri</ref>,
      <ref cel="susur.0i">susuri</ref>,
      <ref cel="sxusx.0i">&scirc;u&scirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zischen</trd>,
      <trd>zischeln</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_s;&c_ib;&c_p;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">xiular <klr>(agut)</klr></trd>  
  <trd lng="en">hiss</trd>
  <trd lng="es">silbar</trd>
  <trd lng="fr">siffler<klr> (tr&egrave;s aigu)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E5;&#x30FC;&#x30B7;&#x30E5;&#x30FC;&#x97F3;&#x3092;&#x7ACB;&#x3066;&#x308B;  <pr>&#x30B7;&#x30E5;&#x30FC;&#x30B7;&#x30E5;&#x30FC;&#x304A;&#x3068;&#x3092;&#x305F;&#x3066;&#x308B;</pr></trd>,
    <trd>&#x30D2;&#x30E5;&#x30FC;&#x30D2;&#x30E5;&#x30FC;&#x9CF4;&#x308B;  <pr>&#x30D2;&#x30E5;&#x30FC;&#x30D2;&#x30E5;&#x30FC;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sissen</trd>,
    <trd>fluiten</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sziszeg</trd>,
    <trd>s&uuml;v&iacute;t</trd>,
    <trd>s&uuml;v&ouml;lt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_v;&c_i;&c_s;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;<klr> (&c_i;&c_z;-&c_z;&c_a; &c_d;&c_je;&c_f;&c_je;&c_k;&c_t;&c_a; &c_r;&c_je;&c_ch;&c_i;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>svi&scaron;&tcaron;a&tcaron;</trd>,
    <trd>sy&ccaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>svi&scaron;t&ecaron;t</trd>,
    <trd>sy&ccaron;et</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_i;&#x43F;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_u;&#x43C;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_s;&c_t;&c_ib;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &c_v;&c_ib;&c_t;&c_je;&c_r;)</trd>,
    <trd>&c_h;&c_r;&c_i;&#x43F;&#x43B;&#x43E; &c_d;&c_i;&c_h;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5636; <pr>s&#x12B;</pr></trd>,
    <trd>&#x5636;&#x5636;&#x58F0; <pr>s&#x12B;s&#x12B;sh&#x113;ng</pr></trd>,
    <trd>&#x5636;&#x5636;&#x8072; <pr>s&#x12B;s&#x12B;sh&#x113;ng</pr></trd>,
    <trd>&#x5618; <pr>x&#x16B;</pr></trd>,
    <trd>&#x5653; <pr>x&#x16B;</pr></trd>,
    <trd>&#x5618;&#x58F0; <pr>x&#x16B;sh&#x113;ng</pr></trd>,
    <trd>&#x5653;&#x8072; <pr>x&#x16B;sh&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sibl.0a">
  <kap><tld/>a<fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="sibl.0a.KOMUNE">
    <dif>
      Tia, ke &gcirc;i <tld/>as:
      <ekz>
        <nom>Dorina</nom> trankvile levis la a&ubreve;skultilon, sed <tld/>a vira vo&ccirc;o
        glaciigis &scirc;in
        <fnt>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2001/005343.html">En la ombro de la silento</url></vrk>.
          <bib>Monato</bib>
          <lok>1997/03, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sukcesis aperigi nur ra&ubreve;kajn, <tld/>ajn tonojn, kiuj trapenetris &gcirc;is la ostoj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ampleksa matrono <klr>[&#x2026;]</klr> rapidis al la ban&ccirc;ambro, farante <tld/>ajn
        bruojn
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kvar preterpasantoj <klr>[&#x2026;]</klr> duonsensis la fulman traflugon de io
        <tld/>a
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">FON</uzo>
        <ref cel="sibl.0anto.FON"><tld/>a konsonanto</ref>;
      </ekz>
      <ekz>
        <uzo tip="fak">MED</uzo>
        <tld/>a spirado.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_ja;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_y;&c_ch;&c_a;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_ib;&c_p;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="de">zischend</trd>
  <trd lng="fr">sifflant</trd>
  <trd lng="ja">&#x30B7;&#x30E5;&#x30FC;&#x30C3;&#x3068;&#x97F3;&#x3092;&#x7ACB;&#x3066;&#x308B;  <pr>&#x30B7;&#x30E5;&#x30FC;&#x30C3;&#x3068;&#x304A;&#x3068;&#x3092;&#x305F;&#x3066;&#x308B;</pr></trd>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;</trd>
</drv>

<drv mrk="sibl.0o">
  <kap><tld/>o,
  <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="sibl.0o.KOMUNE">
    <dif>
      Ago <tld/>i;
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>
      de io a&ubreve; iu <tld/>anta:
      <ekz>
        la tor&ccirc;o brulas rapide kun <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> rapide sekvis &scirc;in, ne levante la okulojn kaj nur kaptante per la
        oreloj la <tld/>on kaj murmureton de la silka vesto
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la spektaklo malsukcesos, tie &ccirc;i kaj tie eksonis <tld/>oj, oni komencis voki
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;dis nur la <tld/>adon de la flamoj en la lampoj
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ado de vento
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        ie tute proksime a&ubreve;di&gcirc;as a&ubreve;to<tld/>ado
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun densa <tld/>ado <klr>[&#x2026;]</klr> preterpasis sovetiaj kiras&scirc;ipoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_ib;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_y;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Zischen</trd>
  <trd lng="fr">sifflement</trd>
  <trd lng="ja">&#x30B7;&#x30E5;&#x30FC;&#x30C3;&#x3068;&#x3044;&#x3046;&#x97F3;  <pr>&#x30B7;&#x30E5;&#x30FC;&#x30C3;&#x3068;&#x3044;&#x3046;&#x304A;&#x3068;</pr></trd>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x98C0; <pr>li&#xFA;</pr></trd>,
    <trd>&#x55D6; <pr>s&#x14D;u</pr></trd>,
    <trd>&#x549D; <pr>s&#x12B;</pr></trd>,
    <trd>&#x565D; <pr>s&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sibl.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="sibl.0anto.KOMUNE">
    <dif>
      Iu kiu <tld/>as.
    </dif>
  </snc>
  <snc mrk="sibl.0anto.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="dent.lang0akonsonanto">Lang-denta</ref>
      <ref tip="super" cel="frot.0sono.FON">frota konsonanto</ref>,
      kiel la esperantaj /s/ kaj /z/, a&ubreve;
      <ref cel="frot.ek0akonsonanto">ekfrota konsonanto</ref>,
      entenanta tian parolsonon (kiel la esperanta <ctl>c</ctl>):
      <ekz>
        la etruskoj <klr>[&#x2026;]</klr> havis kvar specojn de /k/ kaj almena&ubreve; tri <tld/>antojn
        <fnt>
          <bib>LOdE</bib>
          <vrk>La Etruska Alfabeto</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxusx.0a_konsonanto">&scirc;u&scirc;a konsonanto</ref>
    <trd lng="de">Zischlaut</trd>
    <trd lng="en">hissing sibilant</trd>
    <trd lng="fr">sifflante</trd>
    <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
    <trdgrp lng="zh">
    <trd>&#x9F52;&#x64E6;&#x97F3; <pr>ch&#x1D0;c&#x101;y&#x12B;n</pr></trd>,
    <trd>&#x9F7F;&#x64E6;&#x97F3; <pr>ch&#x1D0;c&#x101;y&#x12B;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_ja;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_y;&c_ch;&c_a;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sibilanta</trd>,
    <trd>sykavka</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;<klr> (&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">sykavka</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_b;&c_ib;&#x43B;&#x44F;&#x43D;&c_t;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_s;&c_t;&#x44F;&c_ch;&c_i;&c_j; &#x43F;&c_r;&c_i;&c_g;&#x43E;&#x43B;&#x43E;&c_s;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>


</art>
