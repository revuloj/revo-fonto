<drv mrk="port.el0i">
  <kap>el<tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5167">el'port'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.el0i.eksteren">
    <dif>
      Preni kun si kaj iri por demeti eksteren:
      <ekz>
        <ind>balaa&jcirc;on el korto eksteren ne el<tld/>u</ind>
        <trd lng="fr">il faut <ind>laver son linge sale en
        famille</ind></trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi el<tld/>as el via urbo la plej bonan rememoron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekspor.0i">eksporti</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>emporter<klr> (&agrave; l'ext&eacute;rieur)</klr></trd>
    </trdgrp>
    <trd lng="nl">naar buiten <ind>brengen</ind></trd>
    <trdgrp lng="hu">
      <trd>kivisz</trd>,
      <trd>kihoz</trd>,
      <trd>kihord</trd>
    </trdgrp>
  </snc>
  <snc mrk="port.el0i.toleri">
    <dif>
      Toleri &gcirc;isfine,
      <ref tip="vid" cel="ten.el0i">elteni</ref>:
      <ekz>
        el<tld/>i sola tiom da feli&ccirc;o kaj tiom da suferoj mi ne
        povas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ne povis plu el<tld/>i la vidon de tiu mal&gcirc;ojo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne povi el<tld/>i la kostojn;
      </ekz>
      <ekz>
        neel<tld/>ebla doloro;
      </ekz>
      <ekz>
        ni vane el<tld/>is tiujn terurajn oferojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="es">soportar</trd>
    <trdgrp lng="fr">
      <trd>supporter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kib&iacute;r</trd>,
      <trd>elvisel</trd>
    </trdgrp>
    <trd lng="nl">uithouden</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">vyn&eacute;st</trd>
  <trd lng="pl">wynosi&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vynies&tcaron;</trd>,
    <trd>vyn&aacute;&scaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="port.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.en0i.KOMUNE">
    <dif>
      Internen <tld/>i:
      <ekz>
        en la kelon estis en<tld/>ita fasko da vakskandeloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en<tld/>i koton hejmen;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en<tld/>i konfuzon en nian mezon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trans la sojlo vi en<tld/>os denove &gcirc;ojon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per tio &ccirc;i <klr>[reguloj por vortofarado]</klr> mi en<tld/>is
        grandegan ekonomion rilate la nombron de la vortoj ellernotaj
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, el la unua
        libro de la lingvo esperanto, 1</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="import.0i">importi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vn&aacute;&scaron;et</trd>,
    <trd>vn&eacute;st</trd>,
    <trd>zan&eacute;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hineintragen</trd>,
    <trd>hereintragen</trd>,
    <trd>hineinbringen</trd>,
    <trd>hereinbringen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>emporter<klr> (&agrave; l'int&eacute;rieur)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bevisz</trd>,
    <trd>behoz</trd>,
    <trd>behord</trd>
  </trdgrp>
  <trd lng="nl">binnen <ind>brengen</ind></trd>
  <trd lng="pl">wnosi&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vnies&tcaron;</trd>
  <trd lng="tr">ithal etmek</trd>
</drv>

<drv mrk="port.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
