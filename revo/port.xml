  </trdgrp>
  <trd lng="de">Brieftr&auml;ger</trd>
  <trd lng="es">cartero</trd>
  <trdgrp lng="fr">
    <trd>facteur<klr> (postes)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;zbes&iacute;t&odblac;</trd>,
    <trd>lev&eacute;lpost&aacute;s</trd>
  </trdgrp>
  <trd lng="pl">listonosz</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_a;&c_l;&c_mol;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;&c_n;&c_o;&c_s;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>list&aacute;r</trd>,
    <trd>po&scaron;t&aacute;r</trd>
