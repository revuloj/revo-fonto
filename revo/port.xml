<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>port</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="port.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.0i.kun_si">
    <dif>
      Havi sur si a&ubreve; kun si, kutime a&ubreve; ne,
      <ref tip="vid" cel="port.kun0i.kun_si">kun<tld/>i<sncref/></ref>:
      <ekz>
        en la po&scirc;o de mia pantalono mi <tld/>as monujon
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>.
      </ekz>
      <ekz>
        en la mano li <tld/>is bastonegon
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        hundo bojas, vento <tld/>as la bojon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se via spir' en si venenon <tld/>us<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi <tld/>as la nomon Petro.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bei sich haben</trd>,
      <trd>tragen</trd>
    </trdgrp>
    <trd lng="es">llevar <klr tip="amb">(consigo o puesto)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <subsnc mrk="port.0i.subteni">
      <dif>
        Subteni pezan objekton:
        <ekz>
          <tld/>i infanon en la brakoj;
        </ekz>
        <ekz>
          <tld/>i valizon sur sia &scirc;ultro;
        </ekz>
        <ekz>
          plenigu la sakojn de tiuj homoj per greno tiom, kiom ili nur
          povas <tld/>i
          <fnt><bib>MT</bib>, <lok>&Gen; 44:1</lok></fnt>;
        </ekz>
        <ekz>
          arbo <tld/>anta fruktojn;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          li konsentas <tld/>i &ccirc;iujn elspezojn.
        </ekz>
      </dif>
      <trd lng="de">tragen</trd>
      <trdgrp lng="es">
        <trd>llevar</trd>,
        <trd>cargar con <klr tip="amb">(peso)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
        <trd>&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="port.0i.veston">
      <dif>
        Veste <ref tip="dif" cel="hav.sur0i.veston">surhavi</ref>:
      <ekz>
        li <tld/>as rozokoloran superveston
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu volas <tld/>i grandegajn perukojn
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 9a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i la verdan stelon<fnt>Z</fnt>.
      </ekz>
      </dif>
    </subsnc>
  </snc>
  <snc mrk="port.0i.translokigi">
    <dif>
      Translokigi,
      <ref tip="dif"
      cel="port.al0i.translokigi">al<tld/>i<sncref/></ref>,
      <ref tip="dif" cel="port.trans0i.KOMUNE">trans<tld/>i</ref>:
      <ekz>
        <tld/>i leterojn &ccirc;e la po&scirc;tejon, infanon hejmen;
      </ekz>
      <ekz>
        li ordonis, ke oni <tld/>u tion en la grandan
        salonon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i al iu sciigon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &gcirc;is maro fluu mia sang', <tld/>ante malbenon al la bordo de l'
        barbaroj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bringen</trd>,
      <trd>herbeibringen</trd>,
      <trd>herbeitragen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>llevar</trd>,
      <trd>traer</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_s;&c_t;&c_i;</trd>
  </snc>
  <snc mrk="port.0i.senton">
    <dif>
      Havi ian senton en sia koro:
      <ekz>
        en mia brusto <tld/>as mi deziron malnovan<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>as por vi amon varmegan en mia koro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estas tre ekkolerema <klr>...</klr>; tamen li <klr>...</klr>
        ne
        <tld/>as longe la koleron
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>tragen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <subsnc mrk="port.0i.morale">
      <dif>
        Senti sur sian konsciencon ian moralan pezon:
        <ekz>
          vi <tld/>os la sekvojn de via senatento;
        </ekz>
        <ekz>
          <tld/>i grandan respondecon;
        </ekz>
        <ekz>
          la tuta gento <tld/>as sur si la koleron de la
          dioj<fnt>Z</fnt>;
        </ekz>
        <ekz>
          li <tld/>as sur si la kulpon de parencmortigo<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>tragen</trd>
      </trdgrp>
      <trd lng="es">cargar con <klr tip="amb">(culpas,
      responsabilidad...)</klr></trd>
      <trd lng="ru">&c_n;&c_je;&c_s;&c_t;&c_i;</trd>
    </subsnc>
    <subsnc mrk="port.0i.toleri">
      <dif>
        Havi sufi&ccirc;an forton por ne esti premegata de morala pezo;
        <ref tip="vid" cel="toler.0i.KOMUNE">toleri</ref>:
        <ekz>
          <tld/>i sian malfeli&ccirc;on<fnt>B</fnt>;
        </ekz>
        <ekz>
          sorton malsa&gcirc;uloj timas, sa&gcirc;uloj <tld/>as;
        </ekz>
        <ekz>
          <tld/>i la &scirc;ar&gcirc;ojn de la homa vivo<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_p;&c_je;&c_r;&c_a;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>ertragen</trd>,
        <trd>aushalten</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>soportar</trd>,
        <trd>sobrellevar</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
        <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t na sob&ecaron;</trd>,
    <trd>n&eacute;st</trd>,
    <trd>nosit</trd>,
    <trd>p&rcaron;in&aacute;&scaron;et</trd>
  </trdgrp>
  <trd lng="en">carry</trd>
  <trdgrp lng="fr">
    <trd>porter</trd>,
    <trd>supporter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hoz</trd>,
    <trd>visz</trd>,
    <trd>hord</trd>,
    <trd>visel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dragen</trd>,
    <trd>brengen</trd>
  </trdgrp>
  <trd lng="pl">nosi&cacute;</trd>
  <trdgrp lng="pt">
    <trd>carregar</trd>,
    <trd>levar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nies&tcaron;</trd>,
    <trd>nosi&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">b&auml;ra</trd>
  <trd lng="tr">ta&scedil;&inodot;mak</trd>
</drv>

<drv mrk="port.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="port.0ajxo.KOMUNE">
    <dif>
      Tio, kion oni <tld/>as;
      <ref tip="vid" cel="sxargx.0o.KOMUNE">&scirc;ar&gcirc;o<sncref/></ref>:
      <ekz>
        li havis grandegan <tld/>a&jcirc;on sur la brako
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">charge<klr> (port&eacute;e)</klr></trd>
</drv>

<drv mrk="port.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="port.0igxi.KOMUNE">
    <dif>
      Flugpendi, &scirc;vebi:
      <ekz>
        grizaj nuboj <tld/>i&gcirc;is en la aero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dramo el la vivo preter<tld/>i&gcirc;as <klr>(&scirc;vebas, flugas)</klr>
        anta&ubreve; &scirc;ia spirito
        <fnt><bib>Fab2</bib>, <lok>el fenestro en Vartou</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schweben</trd>,
    <trd>getragen werden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;river</trd>,
    <trd>flotter</trd>
  </trdgrp>
  <trd lng="hu">sodr&oacute;dik</trd>  
  <trd lng="nl">zweven</trd>
  <trd lng="pl">unosi&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_t;&c_i;&c_s;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="port.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="port.0ilo.KOMUNE">
    <dif>
      Aparato por <tld/>i objektojn a&ubreve; personojn:
      <ekz>
        anta&ubreve;iris du kuristoj kun bastonoj kaj du kun tor&ccirc;oj; post la
        <tld/>ilo iris kvar servistoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_ib;&c_l;&c_k;&c_ib;</trd>
  <trd lng="cs">nosi&ccaron; <klr>(za&rcaron;&iacute;zen&iacute;)</klr></trd>
  <trdgrp lng="es">
    <trd>andas</trd>,
    <trd>utensilio para transportar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appareil de <ind>levage</ind></trd>,
    <trd>chariot</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hordoz&oacute;(eszk&ouml;z)</trd>,
    <trd>hordsz&eacute;k</trd>,
    <trd>gyaloghint&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>berrie</trd>,
    <trd>draagberrie</trd>,
    <trd>baar</trd>
  </trdgrp>
  <trd lng="pl">nosid&lstroke;o</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_i;&c_l;&c_k;&c_i;</trd>
  <trd lng="sk">nosidlo</trd>
</drv>

<drv mrk="port.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="port.0isto.KOMUNE">
    <dif>
      Homo, kiu metie <tld/>as paka&jcirc;ojn a&ubreve; &scirc;ar&gcirc;ojn:
      <ekz>
        &ccirc;esis &ccirc;iuj laboroj, e&ccirc; la <tld/>istoj ne <tld/>is la
        &scirc;ar&gcirc;ojn
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
      </ekz>
      <ekz>
        ili fari&gcirc;as <tld/>istoj, lavistoj de veturiloj, &scirc;uistoj kaj
        etvendistoj
        <fnt>
        <aut>Z. Ntomono Zola</aut>,
        <vrk>Surstrata skandalo</vrk>,
        <bib>Monato</bib>,
        <lok>2001:3, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_ib;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>nositel</trd>,
    <trd>nosi&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tr&auml;ger</trd>
  </trdgrp>
  <trd lng="es">porteador</trd>
  <trdgrp lng="fr">
    <trd>porteur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hord&aacute;r</trd>,
    <trd>sz&aacute;ll&iacute;t&oacute;munk&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kruier</trd>,
    <trd>sjouwer</trd>,
    <trd>drager</trd>
  </trdgrp>
  <trd lng="pl">tragarz</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
  <trd lng="sk">nosi&ccaron;</trd>
  <trd lng="sv">b&auml;rare</trd>
  <trd lng="tr">ta&scedil;&inodot;y&inodot;c&inodot;</trd>
</drv>

<drv mrk="port.al0i">
  <kap><ofc>*</ofc>al<tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5166">al'port'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.al0i.translokigi">
    <dif>
      Preni kun si kaj iri por demeti en alian lokon;
      <ref tip="sin" cel="port.0i.translokigi"><tld/>i<sncref/></ref>:
    </dif>
    <ekz>
      &ccirc;u oni jam al<tld/>is la leterojn?
    </ekz>
    <ekz>
      <ind>pano estas al<tld/>ita, korbo estu for&jcirc;etita</ind>
      <fnt><bib>PrV</bib></fnt>;
      <trd lng="hu">a m&oacute;r megtette k&ouml;teless&eacute;g&eacute;t, a m&oacute;r mehet</trd>
    </ekz>
    <ekz>
      al<tld/>u al mi metron da nigra drapo
      <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>;
    </ekz>
    <ekz>
      la &scirc;ipo, kiu al ni al<tld/>is la fremdulojn<fnt>Z</fnt>;
    </ekz>
    <ekz>
      al<tld/>i oferon<fnt>Z</fnt>;
    </ekz>
    <ekz>
      al<tld/>i sciigon<fnt>Z</fnt>;
    </ekz>
    <ekz>
      al<tld/>i konsilojn<fnt>Z</fnt> al;
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      <ind>unu hirundo printempon ne al<tld/>as</ind>
      <trd lng="fr">une hirondelle ne fait pas le printemps</trd>
      <trd lng="hu">egy fecske nem csin&aacute;l nyarat</trd>
      <trd lng="tr">bir &ccedil;i&ccedil;ekle bahar gelmez</trd>
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trdgrp lng="de">
      <trd>bringen</trd>,
      <trd>herbeibringen</trd>,
      <trd>herbeitragen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>llevar</trd>,
      <trd>traer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>idehoz</trd>,
      <trd>odavisz</trd>
    </trdgrp>
    <trd lng="sv">h&auml;mta</trd>
  </snc>
  <snc mrk="port.al0i.havigi">
    <dif>
      Havigi al iu; produkti, ka&ubreve;zi:
      <ekz>
        al<tld/>i profiton, perdon, rezultaton, kontribuon,
        helpon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al<tld/>i subtenon al iu;
      </ekz>
      <ekz>
        kelka libereco al<tld/>as al ni nenian malutilon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        senlaboreco dormon al<tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la acida man&gcirc;a&jcirc;' al li al<tld/>as nur la morton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        muziko al<tld/>as al mi multe da plezuro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.0i">doni</ref>,
      <ref cel="est.0igi">estigi</ref>,
      <ref cel="liver.0i">liveri</ref>,
      <ref cel="nask.0i">naski</ref>,
      <ref cel="rezult.0igi">rezultigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beischaffen</trd>,
      <trd>beibringen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>traer<klr tip="ind"> (producir)</klr></trd>,
      <trd>conllevar</trd>,
	  <trd>acarrear</trd>,
	  <trd>provocar</trd>
    </trdgrp>
    <trd lng="hu">hoz <klr tip="ind">(&aacute;tv.)</klr></trd>    
    <trd lng="sv">f&ouml;ra med sig</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;in&eacute;st</trd>
  <trdgrp lng="fr">
    <trd>apporter</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>brengen</trd>,
    <trd>aandragen</trd>,
    <trd>aanbrengen</trd>
  </trdgrp>
  <trd lng="pl">zanosi&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>donies&tcaron;</trd>,
    <trd>prinies&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">getirmek</trd>
</drv>

<drv mrk="port.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="P"/>i &ccirc;iuflanken:
      <ekz>
        <klr>[oni]</klr> kuris tra tiu tuta regiono, kaj komencis
        &ccirc;irka&ubreve;<tld/>i la malsanulojn sur iliaj litoj
        tien, kie ili a&ubreve;dis, ke <klr>[Jesuo]</klr> estas
        <fnt><bib>NT</bib>, <lok>&Mar; 5:55</lok></fnt>;
      </ekz>
      <ekz>
        apud la tablo oni multe man&gcirc;is kaj multe trinkis: Manjo
        &ccirc;irka&ubreve;<tld/>is anseron, poste malvarman porkidon
        kun kreno, poste sturgon
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>k&ouml;rbevisz</trd>,
    <trd>k&ouml;rbehordoz</trd>
  </trdgrp>
  <trd lng="pl">obnosi&cacute;</trd>
</drv>

