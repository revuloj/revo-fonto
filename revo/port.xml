  <trdgrp lng="sk">
    <trd>podoprie&tcaron; <klr>(stav.)</klr></trd>,
    <trd>podporova&tcaron;</trd>,
    <trd>udr&zcaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">desteklemek<klr>(fiziksel olarak)</klr></trd>
</drv>

<drv mrk="port.trans0i">
  <kap>trans<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.trans0i.KOMUNE">
    <dif>
      <tld lit="P"/>i de unu loko en alian,
      <tld/>i transen:
      <ekz>
        trans<tld/>i meblojn de domo en alian;
      </ekz>
      <ekz>
        molekuloj trans<tld/>as la
        informojn el la DNA <klr>...</klr> al la ribosomoj
        <fnt>
        <aut>S. Norvell</aut>,
        <vrk>&Ccirc;u anka&ubreve; genoj responsas pri melanomo?</vrk>,
        <bib>Monato</bib>,
        <lok>1997:7, p. 14a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ebrieta &scirc;telisto decidis, ke estas tro lacige piedire
        trans<tld/>i la &scirc;tela&jcirc;ojn
        <fnt>
        <aut>Pejno S.</aut>,
        <vrk>Vendredon, la dek-trian</vrk>,
        <bib>Monato</bib>,
        <lok>1998:5, p. 4a</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trans<tld/>is lignon per fervojo kaj kamionoj
        <fnt>
        <aut>W. Klag</aut>,
        <vrk>Vakeroj de Alpoj</vrk>,
        <bib>Monato</bib>,
        <lok>2001:1, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_o;&c_z;&c_ib;&c_c;&c_mol;</trd>?
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dopravovat</trd>,
    <trd>p&rcaron;en&aacute;&scaron;et</trd>,
    <trd>p&rcaron;en&eacute;st</trd>,
    <trd>p&rcaron;ev&aacute;&zcaron;et</trd>,
    <trd>transportovat</trd>
  </trdgrp>
  <trd lng="de">transportieren</trd>
  <trd lng="es">transportar</trd>
  <trdgrp lng="fr">
    <trd>transporter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;tvisz</trd>,
    <trd>&aacute;thoz</trd>,
    <trd>sz&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>overbrengen</trd>,
    <trd>verplaatsen</trd>,
    <trd>transporteren</trd>
  </trdgrp>
  <trd lng="pl">transportowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dopravova&tcaron;</trd>,
    <trd>prenies&tcaron;</trd>,
    <trd>prev&aacute;&zcaron;a&tcaron;</trd>,
    <trd>transportova&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">bir yerden bir yere ta&scedil;&inodot;mak</trd>
</drv>

<drv mrk="port.trans0ilo">
  <kap>trans<tld/>ilo</kap>
  <snc mrk="port.trans0ilo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Veturilo a&ubreve; ajna rimedo, kiu helpas trans<tld/>i a&jcirc;ojn a&ubreve;
      homojn:
      <ekz>
        la publiko ne sufi&ccirc;e konsideras la eksterajn kostojn de
        trans<tld/>ilo: polucion, bruon, klimat&scirc;an&gcirc;on kaj
        akcidentojn
        <fnt>
        <aut>R. Rotsaert</aut>,
        <vrk>Medio-indulgo de la &ccirc;efaj ekonomiaj sektoroj</vrk>,
        <bib>Monato</bib>,
        <lok>2001:3, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dopravn&iacute; prost&rcaron;edek</trd>,
    <trd>dopravn&iacute; za&rcaron;&iacute;zen&iacute;</trd>,
    <trd>dopravn&iacute;k</trd>,
    <trd>transport&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>moyen de <ind>transport</ind></trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;ll&iacute;t&oacute;eszk&ouml;z</trd>
  <trd lng="pl">&sacute;rodek transportu</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dopravn&yacute; prostriedok</trd>,
    <trd>transport&eacute;r</trd>
  </trdgrp>
  <trd lng="tr">ula&scedil;&inodot;m ara&ccedil;lar&inodot;</trd>
</drv>

<drv mrk="port.trans0isto">
  <kap>trans<tld/>isto</kap>
  <snc mrk="port.trans0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>
      kiu diversrimede vojigas komerca&jcirc;ojn al
      malproksimaj a&ccirc;etintoj:
      <ekz>
        tiu teksto estas regularo uzata en la tuta mondo de komercistoj,
        juristoj, trans<tld/>istoj kaj asekuristoj
        <fnt><aut>-</aut>, <vrk>Inkotermoj 2000</vrk>,
        <bib>Monato</bib>, <lok>2001:3, p. 29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_e;&c_d;&c_y;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dopravce</trd>,
    <trd>p&rcaron;epravce <klr>(z povol&aacute;n&iacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Transporteur</trd>,
    <trd>Spediteur</trd>
  </trdgrp>
  <trd lng="es">transportista</trd>
  <trdgrp lng="fr">
    <trd>transporteur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;ll&iacute;t&oacute;</trd>,
    <trd>fuvaroz&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przewo&zacute;nik</trd>,
    <trd>spedytor</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_je;&c_d;&c_i;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">dopravca</trd>
  <trd lng="tr">ta&scedil;&inodot;y&inodot;c&inodot;</trd>
</drv>

<drv mrk="port.aertrans0o">
  <kap>aertrans<tld/>o</kap>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="port.aertrans0o.EKON">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Sistemo de trans<tld/>ado de pasa&gcirc;eroj kaj komerca&jcirc;oj
      per avioj.
    </dif>
  </snc>
  <trd lng="cs">leteck&aacute; doprava</trd>
  <trdgrp lng="de">
    <trd>Lufttransport</trd>,
    <trd>Luftverkehr</trd>
  </trdgrp>
  <trd lng="en">air transport</trd>
  <trd lng="es">transporte a&eacute;reo</trd>
  <trd lng="fr">transport a&eacute;rien</trd>
  <trd lng="hu">l&eacute;gi <ind>sz&aacute;ll&iacute;t&aacute;s</ind></trd>
  <trd lng="nl">luchttransport</trd>
  <trd lng="pl">transport lotniczy</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_v;&c_i;&c_a;&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; &c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">leteck&aacute; doprava</trd>
</drv>

<drv mrk="port.leter0isto">
  <kap>leter<tld/>isto</kap>
  <snc mrk="port.leter0isto.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Oficisto, kiu <tld/>as po&scirc;ta&jcirc;ojn al la ricevontoj:
      <ekz>
        subite aliris al mi la leter<tld/>isto kaj transdonis al mi
