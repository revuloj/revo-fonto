  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odnies&tcaron;</trd>
  <trd lng="tr">g&ouml;t&uuml;rmek</trd>
</drv>

<drv mrk="port.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.kun0i.kun_si">
    <dif>
      <tld lit="P"/>i kun si, sur si unu a&ubreve; plurajn objektojn:
      <ekz>
        kun<tld/>u vian fluton;
      </ekz>
      <ekz>
        li ser&ccirc;is en la tuta domo ion, kion li povus kun<tld/>i, sed
        trovis nur froma&gcirc;on kaj metis &gcirc;in en la po&scirc;on
        <fnt><bib>ElektFab</bib>, <lok>la kura&gcirc;a tajloreto</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; <klr tip="amb">(&c_z;
    &c_s;&c_a;&c_b;&c_o;&c_j;)</klr></trd>
    <trdgrp lng="fr">
      <trd>avoir sur soi</trd>,
      <trd>porter avec soi</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>mitbringen</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>mag&aacute;val <ind>visz</ind></trd>,
    <trd>mag&aacute;val <ind>hoz</ind></trd>
  </trdgrp>
    <trdgrp lng="nl">
      <trd>meebrengen</trd>
    </trdgrp>
    <trd lng="pl">nosi&cacute; ze sob&aogonek;</trd>
    <trd lng="ru">&c_n;&c_je;&c_s;&c_t;&c_i; &c_s; &c_s;&c_o;&c_b;&c_o;&c_j;</trd>
    <trd lng="tr">beraberinde ta&scedil;&inodot;mak</trd>
  </snc>
  <snc mrk="port.kun0i.kunigi">
    <dif>
      <tld lit="P"/>i kunen, kunigi, amasigi plurajn objektojn:
      <ekz>
        Arne kaj la knabinoj <klr>...</klr> kun<tld/>is lignon al la
        &scirc;tona
        forneto
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, 1</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_r;&c_a;&c_z;&c_a;&c_m;)</klr></trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_r;&c_a;&c_z;&c_a;&c_m;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zusammentragen</trd>,
      <trd>zusammenstellen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>collecter</trd>
    </trdgrp>
    <trd lng="hu">&ouml;sszehord</trd>    
    <trdgrp lng="nl">
      <trd>bijeenbrengen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol; &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;</trd>,
      <trd>&c_s;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="port.kun0i.kune">
    <dif>
      <tld lit="P"/>i kune kun aliaj personoj:
      <ekz>
        bonvolu kun<tld/>i kun mi la tablon supren.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; <klr tip="amb">(&c_r;&c_a;&c_z;&c_a;&c_m; &c_z;
    &c_k;&c_ib;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    <trdgrp lng="de">
      <trd>zusammen tragen</trd>,
      <trd>gemeinsam tragen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>aider &agrave; <ind>porter</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>egy&uuml;tt <ind>visz</ind></trd>,
      <trd>egy&uuml;tt <ind>hoz</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>samen <ind>dragen</ind></trd>
    </trdgrp>
    <trd lng="pl">nosi&cacute; ze sob&aogonek;</trd>
    <trd lng="ru">&c_n;&c_je;&c_s;&c_t;&c_i; &c_v;&c_m;&c_je;&c_s;&c_t;&c_je; <klr tip="amb">(&c_s;
    &c_k;&c_je;&c_m;-&c_l;.)</klr></trd>
  </snc>
  <trd lng="cs">br&aacute;t s sebou</trd>
  <trdgrp lng="sk">
    <trd>nies&tcaron; so sebou</trd>,
    <trd>pozn&aacute;&scaron;a&tcaron;</trd>,
    <trd>znies&tcaron; dovedna</trd>
  </trdgrp>
</drv>

<drv mrk="port.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.re0i.KOMUNE">
    <dif>
      Denove al<tld/>i tion, kion oni jam for<tld/>is:
      <ekz>
        la kompanio enkondukos novan botelon, por kiu &gcirc;i kalkulos
        kromprezon, poste repagotan se oni re<tld/>as la botelon
        <fnt>
        <aut>G. Van Damme</aut>,
        <vrk>Trinka&jcirc;oj/Uzi la le&gcirc;on por reklami</vrk>,
        <bib>Monato</bib>,
        <lok>1995:12, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; &c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trd lng="de">zur&uuml;ckbringen</trd>
  <trdgrp lng="es">
    <trd>retornar<klr tip="ind"> (devolver)</klr></trd>,
	<trd>devolver</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rapporter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszahoz</trd>,
    <trd>visszavisz</trd>
  </trdgrp>
  <trd lng="nl">terugbrengen</trd>
  <trd lng="pl">przynosi&cacute; z powrotem</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_je;&c_s;&c_t;&c_i; &c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;</trd>
</drv>

<drv mrk="port.sub0i">
  <kap>sub<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="port.sub0i.KOMUNE">
    <dif>
      <tld lit="P"/>i estante sub io.
    </dif>
  </snc>
