<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: munken.xml,v 1.6 2022/09/19 19:31:23 revo Exp $">
    <kap>
      <rad>Munken</rad>/o,
      <var><kap><rad var="&hcirc;">Mun&hcirc;en</rad>/o <fnt><bib>FK</bib></fnt></kap></var>
    </kap>

<drv mrk="munken.0o">
  <kap><tld/>o, <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <uzo tip="fak">POL</uzo><uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      &Ccirc;ef<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="bavar.B0ujo">Bavarujo</ref>:
      <ekz>
        la sekreta ministra&jcirc;o de interna&jcirc;oj en <tld var="&hcirc;"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        formi&gcirc;is en <tld var="&hcirc;"/>o centra societo por pazigrafio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni veturos <klr>[&#8230;]</klr> tri horojn al <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007112.html">Virbovo-tirpovo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto-klubo de <tld/>o &ccirc;iujare donacas konsiderindan sumon al nia Abonhelpa
        Fonduso
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.net/2003/008057.html">Po&scirc;to kaj kosto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la urbaj servoj en <tld/>o, Germanio, decidis anstata&ubreve;igi Vindozon per Linukso
        por oficejaj taskoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="https://www.monato.net/2004/007797.html">Mikrosofto ektimas
          Linukson</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la projekto celas mal&scirc;ar&gcirc;i la a&ubreve;tovojon inter <tld/>o kaj la flughaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard F&ouml;ssmeier</aut>
          <vrk><url ref="https://www.monato.net/2004/008200.html">&Ccirc;u iam rapidos
          Transrapid?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_ju;&c_n;&c_h;&c_je;&c_n;</trd>
    <trd lng="de">M&uuml;nchen</trd>
    <trd lng="en">Munich</trd>
    <trd lng="fr">Munich</trd>
    <trd lng="pl">Monachium</trd>
    <trd lng="ro">M&uuml;nchen</trd>
  </snc>
  <trd lng="uk">&#x41C;&#x44E;&#x43D;&c_h;&c_je;&#x43D;</trd>
</drv>

  </art>

