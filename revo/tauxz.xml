<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ta&ubreve;z</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="tauxz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tauxz.0i.malordige">
    <dif>
      Tirskue a&ubreve; &ccirc;ife <ref tip="super" cel="ord.mal0igi">malordigi</ref>:
      <ekz>
        la uragano <tld/>as la salikarojn kaj igos la rigidajn piceojn
        riverenci;
      </ekz>
      <ekz>
        la vento <tld/>as la flagon flirtantan sur la citadelo;
      </ekz>
      <ekz>
        ordigu viajn dis<tld/>itajn harojn;
      </ekz>
      <ekz>
        la <tld/>ita pajla&jcirc;o de la tegmento;
      </ekz>
      <ekz>
        li forkuris kun mieno <tld/>ita de teruro;
      </ekz>
      <ekz>
        li ekridis amare kaj liaj trajtoj <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rimo ne devas <tld/>i la sencon.
      </ekz>
    </dif>
    <trd lng="nl">verfomfaaien</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;coiffer</trd>,
      <trd>d&eacute;ranger</trd>,
      <trd>&eacute;bouriffer</trd>,
      <trd>&eacute;cheveler</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v; &c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>,
      <trd>&c_m;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_o;&c_m;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tauxz.0i.homogenige">
    <dif>
      Tirskue malkonfuzi harojn:
      <ekz>
        <tld/>i lanon, &ccirc;evalharojn, matracon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>agiter<klr> (p. ex. cheveux)</klr></trd>,
      <trd>secouer<klr> (p. ex. cheveux)</klr></trd>
    </trdgrp>
    <trd lng="nl">in de <ind>war</ind> brengen<klr tip="ind">
    (v.haar)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_je;&c_r;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_z;&c_malmol;&c_je;&c_r;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tauxz.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tirskue <ref tip="super" cel="trakt.mis0i">mistrakti</ref> iun:
      <ekz>
        la patro <tld/>is la tro petoleman filon;
      </ekz>
      <ekz>
        la policanoj <tld/>is la &scirc;teliston;
      </ekz>
      <ekz>
        la plora infana vo&ccirc;o <tld/>is lian koron;
      </ekz>
      <ekz>
        li vivis <tld/>itan vivon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>secouer<klr> (quelqu'un)</klr></trd>
    </trdgrp>
    <trd lng="nl">ruw <ind>behandelen</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_r;&c_m;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_r;&c_ja;&c_s;&c_t;&c_i;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="cxarpi">&ccirc;arpiigi</ref>,
    <ref cel="pincx.0i">pin&ccirc;i</ref>,
    <ref cel="sxir.0i">&scirc;iri</ref>
  </refgrp>
  <trd lng="en">tousle</trd>
</drv>
</art>
