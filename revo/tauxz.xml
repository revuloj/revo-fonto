<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tauxz.xml,v 1.19 2022/05/05 17:08:11 revo Exp $">
<kap>
  <rad>ta&ubreve;z</rad>/i 
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tauxz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tauxz.0i.KOMUNE">
    <dif>
      Tiri, skui ion pli-malpli molan,
      &scirc;an&gcirc;ante &gcirc;ian formon.
    </dif>
    <subsnc mrk="tauxz.0i.malordige">
      <dif>
        Tirskue a&ubreve; &ccirc;ife disigi, <ref cel="tord.0i">tordi</ref>,
        <ref cel="form.mis0i">misformi</ref>:
        <ekz>
          &scirc;i embarase <tld/>as la pajlo&ccirc;apelon en la manoj
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          li tusetadis kaj senbezone <tld/>is la manuskripton
          <fnt>
            <bib>MkM</bib>
            <lok>13</lok>
          </fnt>;
        </ekz>
        <ekz>
          rompante la ungojn mi dis&scirc;iradis la kajerojn <klr>[&#x2026;]</klr> kaj per la fajrohoko
          <tld/>is la foliojn
          <fnt>
            <bib>MkM</bib>
            <lok>13</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="pluk.0i">pluki</ref>
      </refgrp>
      <trd lng="bg">&c_r;&c_a;&c_z;&c_b;&c_malmol;&c_r;&c_k;&c_v;&c_a;&c_m;</trd>      
      <trdgrp lng="de">
        <trd>zerzausen</trd>,
        <trd>verdrehen</trd>,
        <trd>zerw&uuml;hlen</trd>
      </trdgrp>      
      <trd lng="nl">verfromfaaien</trd>
      <trdgrp lng="fr">
        <trd>d&eacute;coiffer</trd>,
        <trd>d&eacute;ranger</trd>,
        <trd>&eacute;bouriffer</trd>,
        <trd>&eacute;cheveler</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kusz&aacute;l</trd>,
        <trd>borzol</trd>,
        <trd>zil&aacute;l</trd>,
        <trd>k&oacute;col</trd>
      </trdgrp>    
      <trdgrp lng="it">
        <trd>strapazzare</trd>,
        <trd>spettinare</trd>,
        <trd>disordinare</trd>,
        <trd>rabbuffare</trd>,
        <trd>arruffare</trd>,
        <trd>scompigliare</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>szarpa&cacute;</trd>,
        <trd>tarmosi&cacute;</trd>,
        <trd>targa&cacute;</trd>,
        <trd>potarga&cacute;</trd>
      </trdgrp>
      <trdgrp lng="pt">
       <trd>desalinhar</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
        &c_v; &c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>,
        <trd>&c_m;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_k;&c_o;&c_m;&c_k;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="tauxz.0i.homogenige">
      <dif>
        Tirskue mal<ref cel="implik.0i">impliki</ref> harojn, 
        fadenojn a&ubreve; similan konfuzita&jcirc;on:
        <ekz>
          <tld/>i lanon, &ccirc;evalharojn, matracon;
        </ekz>
        <ekz>
          &scirc;iaj etaj <klr>[&#x2026;]</klr> manetoj, kiel tenerige ili iras al la haroj, kiel
          am-veke ili <tld/>as la senkomparajn buklojn
          <fnt>
            <bib>ChV</bib>
            <lok>11</lok>
          </fnt>!
        </ekz>
        <ekz>
          la fakulo <tld/>is sian mentonon, dum liaj lipoj elstaris iom pa&ubreve;te kaj lia tuta
          viza&gcirc;o esprimis pripensadon
          <fnt>
            <bib>ChV</bib>
            <lok>19</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="friz.0i">frizi</ref>,
        <ref cel="hirt.0igi">hirtigi</ref>,
        <ref cel="komb.0i">kombi</ref>,
        <ref cel="krisp.0igi">krispigi</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>zausen</trd>
      </trdgrp>      
      <trdgrp lng="fr">
        <trd>agiter<klr> (p. ex. cheveux)</klr></trd>,
        <trd>secouer<klr> (p. ex. cheveux)</klr></trd>
      </trdgrp>
      <trd lng="hu">tilol</trd>   
      <trdgrp lng="it">
        <trd>strigliare</trd>,
        <trd>sgrumare</trd>,
        <trd>pettinare<klr> (sgrumare con le dita)</klr></trd>,
        <trd>ravvivare<klr> (sgrumare)</klr></trd>,
        <trd>sprimacciare</trd>
      </trdgrp> 
      <trd lng="nl">in de <ind>war</ind> brengen<klr tip="ind">
      (v.haar)</klr></trd>
      <trdgrp lng="pl">
        <trd>czochra&cacute;</trd>,
        <trd>rozczochra&cacute;</trd>,
        <trd>targa&cacute;</trd>,
        <trd>potarga&cacute;</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_je;&c_r;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_z;&c_malmol;&c_je;&c_r;&c_o;&c_sh;&c_i;&c_t;&c_mol;
        </trd>,
        <trd>&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="tauxz.0i.FIG">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Kvaza&ubreve; tirskue <ref tip="super" cel="trakt.mis0i">mistrakti</ref> iun:
        <ekz>
          tiu &gcirc;enas min, li <tld/>as mian kvieton, &ccirc;u li ne povas morta&ccirc;i silente sen &gcirc;eni
          la aliajn
          <fnt>
            <bib>Monato</bib>
            <aut>Roland Platteau</aut>
            <vrk><url ref="&Monato;/2004/008329.html">Demandoj</url></vrk>
            <lok>2004</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">strapazieren</trd>
      <trdgrp lng="fr">
        <trd>secouer<klr> (quelqu'un)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>r&aacute;ngat</trd>,
        <trd>cib&aacute;l</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>strigliare</trd>,
        <trd>strapazzare</trd>,
        <trd>scompigliare</trd>
      </trdgrp> 
      <trd lng="nl">ruw <ind>behandelen</ind></trd>
      <trdgrp lng="pl">
        <trd>szarpa&cacute;</trd>,
        <trd>tarmosi&cacute;</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_t;&c_o;&c_r;&c_m;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_t;&c_r;&c_ja;&c_s;&c_t;&c_i;</trd>
      </trdgrp>
    </subsnc>  
  </snc>
  <refgrp tip="vid">
    <ref cel="agit.0i">agiti</ref>,
    <ref cel="hirt.0igi">hirtigi</ref>,
    <ref cel="konfuz.0i">konfuzi</ref>,
    <ref cel="sxir.0i">&scirc;iri</ref>,
    <ref cel="vil.0a">vila</ref>
  </refgrp>
  <trd lng="en">tousle</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x432;&#x43E;&#x434;&#x438;&#x442;&#x438; &#x432; &#x431;&#x435;&#x437;&#x43B;&#x430;&#x434;&#x434;&#x44F;</trd>,
    <trd>&#x43C;&#x2019;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x440;&#x456;&#x43F;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="tauxz.0ilanervojn">
  <kap><tld/>i la nervojn</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="tauxz.0ilanervojn.PSI">
    <dif>
      Da&ubreve;re <ref tip="vid" cel="incit.0i.kolerigi">inciti</ref>:
      <ekz>
        eterna boja&ccirc;ado ne povos ne <tld/>i kolegajn nervojn
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekci.0i.PSI">afekcii<sncref/></ref>,
      <ref cel="frenez.0igi">frenezigi</ref>,
      <ref cel="konste1.0i">konsterni</ref>,
      <ref cel="ted.0i">tedi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>nerven</trd>,
    <trd>die <ind>Nerven</ind> strapazieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mettre en boule</trd>,
    <trd>taper sur les nerfs</trd>
  </trdgrp>
</drv>
</art>
