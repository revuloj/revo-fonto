    <trd>Ocean Arktyczny</trd>,
    <trd>Morze Arktyczne</trd>,
    <trd>Ocean Lodowaty P&oacute;&lstroke;nocny</trd>
  </trdgrp>
  <trd lng="pt">Oceano &Aacute;rtico</trd>
  <trd lng="ru">&c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;
