<drv mrk="ocean.mond0o">
  <kap>mond<tld/>o,
  <var><kap>monda <tld/>o<fnt>Z</fnt></kap></var></kap>
  <tezrad fak="GEOG"/>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La tuta kuno da sala akvo kovranta tri kvinonojn de la <ref tip="malprt" cel="ter.0o.planedo">tersurfaco</ref> kaj
      apartiganta la kontinentojn:
      <ekz>
        post jarmiloj ili alflugos sur la flugiloj
        de la vaporo tra la aero trans la monda <tld/>o
        <fnt><bib>Fab3</bib>, <lok>Post jarmiloj</lok></fnt>;
      </ekz>
      <ekz>
        la nomon <ctl>golfo</ctl> portas kelkaj partoj de
        mond<tld/>o, kiuj per siaj hidrologiaj re&gcirc;imoj estas
        maroj, ekz. Meksika Golfo
        <fnt><bib>Viki</bib>, <lok>art. <url ref="http://wapedia.mobi/eo/Golfo">golfo</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y;
  <ind>&c_a;&c_k;&c_ib;&c_ja;&c_n;</ind></trd>
  <trd lng="bg">
  &c_s;&c_v;&c_je;&c_t;&c_o;&c_v;&c_je;&c_n; &c_o;&c_k;&c_je;&c_a;&c_n;
  </trd>
  <trd lng="cs">sv&ecaron;tov&yacute; oce&aacute;n</trd>
  <trd lng="de">Weltmeer</trd>
  <trdgrp lng="en">
    <trd>world ocean</trd>, <trd>global ocean</trd>
  </trdgrp>
  <trd lng="nl">wereldzee</trd>
  <trd lng="pl">Wszechocean</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_o;&c_v;&c_o;&c_j;
  <ind>&c_o;&c_k;&c_je;&c_a;&c_n;</ind></trd>
</drv>

<drv mrk="ocean.AntarktaO0o">
  <kap>Antarkta <tld lit="O"/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o.tuto"><tld/>o<sncref/></ref>
      &ccirc;irka&ubreve; <ref cel="antark.A0o.kontinento">Antarkto <sncref/></ref>:
      <ekz>
        Japanio nepre &ccirc;esigu baleno-kaptadon en Antarkta <tld lit="O"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011296.php">Kortuma Ne al
          baleno-kaptado</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_a;&c_r;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y;
  &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="de">Antarktischer Ozean</trd>
  <trdgrp lng="en">
    <trd>Antarctic Ocean</trd>,
    <trd>Southern Ocean</trd>
  </trdgrp>
  <trd lng="fr">Oc&eacute;an Antarctique</trd>
  <trd lng="hu">D&eacute;li-Jeges-tenger</trd>
  <trd lng="nl">Antarctische <ind>Oceaan</ind></trd>
  <trdgrp lng="pl">
  <trd>Ocean Po&lstroke;udniowy</trd>,
