<drv mrk="ocean.ArktaO0o">
  <kap>Arkta <tld lit="O"/>o, <var><kap>Norda <tld lit="O"/>o</kap></var></kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o.tuto">oceano<sncref/></ref>
      situanta norde de Ameriko kaj E&ubreve;razio:
      <ekz>
        ekspozicio de &gcirc;is nun ne elmontritaj ikonoj de ortodoksaj mona&hcirc;ejoj ekde la
        rusia bordregiono de Arkta <tld lit="O"/>o &gcirc;is Monto Sinajo
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2000/007012.php">Jarmila festego ortodoksa</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        La kaptado de baleno / En la Norda <tld lit="O"/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>En Kinematografejo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_y; &c_L;&c_je;&c_d;&c_a;&c_v;&c_ib;&c_t;&c_y;
