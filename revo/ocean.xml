<drv mrk="ocean.mond0o">
  <kap>mond<tld/>o,
  <var><kap>monda <tld/>o<fnt>Z</fnt></kap></var></kap>
  <tezrad fak="GEOG"/>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La tuta kuno da sala akvo kovranta tri kvinonojn de la <ref tip="malprt" cel="ter.0o.planedo">tersurfaco</ref> kaj
      apartiganta la kontinentojn:
      <ekz>
        post jarmiloj ili alflugos sur la flugiloj
        de la vaporo tra la aero trans la monda <tld/>o
        <fnt><bib>Fab3</bib>, <lok>Post jarmiloj</lok></fnt>;
      </ekz>
      <ekz>
        la nomon <ctl>golfo</ctl> portas kelkaj partoj de
        mond<tld/>o, kiuj per siaj hidrologiaj re&gcirc;imoj estas
        maroj, ekz. Meksika Golfo
        <fnt><bib>Viki</bib>, <lok>art. <url ref="http://wapedia.mobi/eo/Golfo">golfo</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y; <ind>&c_a;&c_k;&c_ib;&c_ja;&c_n;</ind></trd>
  <trd lng="bg">
  &c_s;&c_v;&c_je;&c_t;&c_o;&c_v;&c_je;&c_n; &c_o;&c_k;&c_je;&c_a;&c_n;
  </trd>
  <trd lng="cs">sv&ecaron;tov&yacute; oce&aacute;n</trd>
  <trd lng="de">Weltmeer</trd>
  <trdgrp lng="en">
    <trd>world ocean</trd>, <trd>global ocean</trd>
  </trdgrp>
  <trd lng="nl">wereldzee</trd>
  <trd lng="pl">Wszechocean</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_o;&c_v;&c_o;&c_j;
  <ind>&c_o;&c_k;&c_je;&c_a;&c_n;</ind></trd>
</drv>

<drv mrk="ocean.AntarktaO0o">
  <kap>Antarkta <tld lit="O"/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o.tuto"><tld/>o<sncref/></ref>
      &ccirc;irka&ubreve; <ref cel="antark.A0o.kontinento">Antarkto <sncref/></ref>:
      <ekz>
        Japanio nepre &ccirc;esigu baleno-kaptadon en Antarkta <tld lit="O"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2014/011296.php">Kortuma Ne al
          baleno-kaptado</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_a;&c_r;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="de">Antarktischer Ozean</trd>
  <trdgrp lng="en">
    <trd>Antarctic Ocean</trd>,
    <trd>Southern Ocean</trd>
  </trdgrp>
  <trd lng="fr">Oc&eacute;an Antarctique</trd>
  <trd lng="hu">D&eacute;li-Jeges-tenger</trd>
  <trd lng="nl">Antarctische <ind>Oceaan</ind></trd>
  <trdgrp lng="pl">
  <trd>Ocean Po&lstroke;udniowy</trd>,
  <trd>Ocean Antarktyczny</trd>,
  <trd>Po&lstroke;udniowy Ocean Lodowaty</trd>
</trdgrp>
  <trd lng="sk">Antarktick&yacute; oce&aacute;n</trd>
</drv>

<drv mrk="ocean.ArktaO0o">
  <kap>Arkta <tld lit="O"/>o, <var><kap>Norda <tld lit="O"/>o</kap></var></kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o.tuto">oceano<sncref/></ref>
      situanta norde de Ameriko kaj E&ubreve;razio:
      <ekz>
        ekspozicio de &gcirc;is nun ne elmontritaj ikonoj de ortodoksaj mona&hcirc;ejoj ekde la
        rusia bordregiono de Arkta <tld lit="O"/>o &gcirc;is Monto Sinajo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007012.html">Grekio: Jarmila festego
          ortodoksa</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        La kaptado de baleno / En la Norda <tld lit="O"/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
          <lok>En Kinematografejo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_y; &c_L;&c_je;&c_d;&c_a;&c_v;&c_ib;&c_t;&c_y; &c_A;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="cs">Severn&iacute; ledov&yacute; oce&aacute;n</trd>
  <trd lng="de">Arktischer Ozean</trd>
  <trd lng="en">Arctic Ocean</trd>
  <trd lng="fr">Oc&eacute;an Arctique</trd>
  <trd lng="hu">&Eacute;szaki-Jeges-tenger</trd>
  <trd lng="nl">Arctische <ind>Oceaan</ind></trd>
  <trdgrp lng="pl">
    <trd>Ocean Arktyczny</trd>,
    <trd>Morze Arktyczne</trd>,
    <trd>Ocean Lodowaty P&oacute;&lstroke;nocny</trd>
  </trdgrp>
  <trd lng="pt">Oceano &Aacute;rtico</trd>
  <trd lng="ru">&c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j; &c_L;&c_je;&c_d;&c_o;&c_v;&c_i;&c_t;&c_y;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>Severn&yacute; &Lcaron;adov&yacute; Oce&aacute;n</trd>,
    <trd>Severn&yacute; &lcaron;adov&yacute; oce&aacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="ocean.Atlanta_O0o">
  <kap>
    Atlanta <tld lit="O"/>o, 
    <var><kap>Atlantika <tld lit="O"/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="atlant.A0o">Atlantiko</ref>:
      <ekz>
        transveturigita tra la Atlantan <tld lit="O"/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lago <nom>Chautauque</nom>, havanta longon de 20 anglaj mejloj, ku&scirc;as sur la alteco de
        1400 futoj super la nivelo de la Atlanta <tld lit="O"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Gerando estas &ccirc;arma urbo en nord-okcidenta Francio, kiu ku&scirc;as sur samnoma
        duoninsulo &ccirc;e la Atlantika <tld lit="O"/>o kaj estas parto de suda Bretonio
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="https://www.monato.be/2009/009817.php">Fortika&jcirc;urbo Gerando</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_t;&c_l;&c_ja;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="de">Atlantischer Ozean</trd>
</drv>


<drv mrk="ocean.Hinda0o">
  <kap>Hinda <tld lit="O"/>o</kap>
  <snc mrk="ocean.Hinda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o.tuto">oceano<sncref/></ref> inter
      Afriko, Azio kaj <ref cel="oceani.O0o">Oceanio</ref>:
      <ekz>
        inter 1855 kaj 1870 Bombajo estis la bazo de la lukto kontra&ubreve; la sklavokomerco
        en la Hinda <tld lit="O"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2008/009357.php">La forgesitaj helpantoj de la
          esploristoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katastrofan cunamon de la 26a de decembro 2004 kun minimume 170&#x202F;000 mortintoj
        <klr>[&#x2026;]</klr> en aro da landoj &ccirc;irka&ubreve; Hinda <tld lit="O"/>o sekvis ondego da
        helpemo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2005/008551.php">Ondego da helpemo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="prt" cel="maldiv.0oj.GEOG">Maldivoj</ref>, 
        insularo en Hinda <tld lit="O"/>o, malaperos sub la maro, se la mondo
        nenion faros
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Lal Shrestha</aut>
          <vrk><url ref="https://www.monato.be/2010/010121.php">Insulo malaperinta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="ca">oce&agrave; &Iacute;ndic</trd>
  <trd lng="de">Indischer Ozean</trd>
  <trd lng="es">Oceano <ind>&Iacute;ndico</ind></trd>
  <trd lng="fr">Oc&eacute;an Indien</trd>
  <trd lng="hu">Indiai-&oacute;ce&aacute;n</trd>
  <trd lng="it">Oceano <ind>Indiano</ind></trd>
  <trd lng="nl">Indian Ocean</trd>
  <trd lng="nl">Indische Oceaan</trd>
  <trd lng="pl">Ocean <ind>Indyjski</ind></trd>
  <trd lng="pt">Oceano &Iacute;ndico</trd>
  <trd lng="ru">&c_I;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
</drv>

<drv mrk="ocean.Silenta_O0o">
  <kap>
    Silenta <tld lit="O"/>o, 
    <var><kap>Pacifika <tld lit="O"/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="pacifik.P0o">Pacifiko</ref>:
      <ekz>
        esploroj pri la floro kaj fa&ubreve;no de la Silenta <tld lit="O"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        unika fervojlinio <klr>[&#x2026;]</klr> supreniras &gcirc;is 4250 metroj, trapasas Andojn kaj
        atingas fine la &ccirc;ilian havenurbon <nom>Antofagasta</nom> &ccirc;e la Pacifika <tld lit="O"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="https://www.monato.be/2005/008399.php">Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_C;&c_ib;&c_h;&c_ib; &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="de">Stiller Ozean</trd>
</drv>


