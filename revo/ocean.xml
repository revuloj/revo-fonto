<drv mrk="ocean.Atlanta_O0o">
  <kap>
    Atlanta <tld lit="O"/>o,
    <var><kap>Atlantika <tld lit="O"/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="atlant.A0o">Atlantiko</ref>:
      <ekz>
        transveturigita tra la Atlantan <tld lit="O"/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lago <nom>Chautauque</nom>, havanta longon de 20 anglaj mejloj, ku&scirc;as sur la alteco de
        1400 futoj super la nivelo de la Atlanta <tld lit="O"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Gerando estas &ccirc;arma urbo en nord-okcidenta Francio, kiu ku&scirc;as sur samnoma
        duoninsulo &ccirc;e la Atlantika <tld lit="O"/>o kaj estas parto de suda Bretonio
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2009/009817.php">Fortika&jcirc;urbo Gerando</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_t;&c_l;&c_ja;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_y;
  &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="de">Atlantischer Ozean</trd>
  <trd lng="en">Atlantic Ocean</trd>
  <trd lng="zh">&#x5927;&#x897F;&#x6D0B; <pr>D&#xE0;x&#x12B;y&#xE1;ng</pr></trd>
</drv>

<drv mrk="ocean.Hinda0o">
  <kap>Hinda <tld lit="O"/>o</kap>
  <snc mrk="ocean.Hinda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o.tuto">oceano<sncref/></ref> inter
      Afriko, Azio kaj <ref cel="oceani.O0o">Oceanio</ref>:
      <ekz>
        inter 1855 kaj 1870 Bombajo estis la bazo de la lukto kontra&ubreve; la sklavokomerco
        en la Hinda <tld lit="O"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2008/009357.php">La forgesitaj helpantoj de la
          esploristoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katastrofan cunamon de la 26a de decembro 2004 kun minimume 170&#x202F;000 mortintoj
        <klr>[&#x2026;]</klr> en aro da landoj &ccirc;irka&ubreve; Hinda <tld lit="O"/>o sekvis ondego da
        helpemo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008551.php">Ondego da helpemo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="prt" cel="maldiv.0oj.GEOG">Maldivoj</ref>,
        insularo en Hinda <tld lit="O"/>o, malaperos sub la maro, se la mondo
        nenion faros
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Lal Shrestha</aut>
          <vrk><url ref="&Monato;/2010/010121.php">Insulo malaperinta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="ca">oce&agrave; &Iacute;ndic</trd>
  <trd lng="de">Indischer Ozean</trd>
  <trd lng="en">Indian Ocean</trd>
  <trd lng="es">Oceano <ind>&Iacute;ndico</ind></trd>
  <trd lng="fr">Oc&eacute;an Indien</trd>
  <trd lng="hu">Indiai-&oacute;ce&aacute;n</trd>
  <trd lng="it">Oceano <ind>Indiano</ind></trd>
