<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: metanol.xml,v 1.4 2022/05/01 12:05:12 revo Exp $">
    <kap>
      <rad>metanol</rad>/o
    </kap>

<drv mrk="metanol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="metanol.0o.KEM">
    <dif>
      <frm>CH<sub>3</sub>OH</frm>, la plej simpla 
      <ref tip="vid" cel="alkoho.0o.KEM">alkoholo</ref> kies molekulo 
      enhavas nur unu atomon de karbono:
      <ekz>
        la malpermeso sekvas la enhospitaligon en septembro de dekoj da homoj en &Ccirc;e&hcirc;io,
        kiuj konsumis alkohola&jcirc;ojn diluitajn per <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010873.php">Je via malsano</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        falsantoj miksis porindustrian alkoholon, enhavantan <tld/>on, nocan al homo,
        kun ordinara brando
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2004/008368.html">Maltrankvili&gcirc;o pro a&ccirc;aj man&gcirc;a&jcirc;oj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_n;&c_o;&c_l;</trd>
    <trd lng="de">Methanol</trd>
    <trd lng="en">methanol</trd>
    <trd lng="fr">m&eacute;thanol</trd>
  </snc>
  <trd lng="uk">&#x43C;&#x435;&#x442;&#x430;&#x43D;&#x43E;&#x43B;</trd>
</drv>

  </art>

