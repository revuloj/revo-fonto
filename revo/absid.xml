<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>absid</rad>/o
</kap>
<drv mrk="absid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Duonronda funda parto de pre&gcirc;ejo, formanta ekstremon de la navo kaj 
      enhavanta la &hcirc;orejon kaj altaron.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>absis</trd>,
    <trd>apsis</trd>,
    <trd>abside</trd>,
    <trd>overwelfde <ind>koornis</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Absis</trd>,
    <trd>Apsis</trd>,
    <trd>Apside</trd>,
    <trd>Chornische</trd>
  </trdgrp>
  <trd lng="en">apse</trd>
  <trd lng="es">a&aacute;bside</trd>
  <trd lng="fr">abside</trd>
  <trd lng="hu">apszis</trd>
  <trd lng="pt">&aacute;bside</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_s;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_s;&c_i;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="absid.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">ARKI</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Ccirc;iu el la akcesoraj <tld/>formaj kapeletoj, kiuj
      kronas la <tld/>on en iuj pre&gcirc;ejoj.
    </dif>
  </snc>
  <trd lng="fr">absidiole</trd>
  <trd lng="hu">apszidiola</trd>
</drv>
</art>
