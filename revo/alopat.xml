<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alopati</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="alopat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Medicina doktrino, kontra&ubreve;a al la
      <ref tip="ant" cel="homeop.0o">homeopatio</ref>,
      kaj kuracanta per  kontra&ubreve;a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_a;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="ca">al&middot;lopatia</trd>
  <trd lng="de">Allopathie</trd>
  <trd lng="es">alopat&iacute;a</trd>
  <trd lng="en">allopathy</trd>
  <trd lng="fr">allopathie</trd>
  <trd lng="nl">allopathie</trd>
  <trd lng="pt">alopatia</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_o;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trd lng="sk">alopatia</trd>
</drv>

<drv mrk="alopat.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_a;&c_p;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">al&middot;lop&agrave;tic</trd>
  <trd lng="de">allopathisch</trd>
  <trd lng="en">allopathic</trd>
  <trd lng="nl">allopathisch</trd>
  <trd lng="pt">alop&aacute;tico</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_o;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">alopatick&yacute;</trd>
</drv>
</art>
