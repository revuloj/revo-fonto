<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>orl</rad>/o</kap>
<drv mrk="orl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Tia kudrita faldo de &scirc;tofpeco, ke la rando
      de la peco ne plu estas videbla kaj ne riskas
      diseri&gcirc;i:
      <ekz>
        po&scirc;tuko kun lar&gcirc;a <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Saum</trd>
  <trd lng="en">hem</trd>
  <trd lng="fr">ourlet</trd>
  <trd lng="hu">szeg&eacute;ly</trd>
  <trd lng="nl">zoom</trd>
  <trdgrp lng="pl">
    <trd>obr&eogonek;bek</trd>,
    <trd>obr&aogonek;bek</trd>,
    <trd>mere&zdot;ka</trd>,
    <trd>wywini&eogonek;cie<klr> skraju materia&lstroke;u</klr></trd>
  </trdgrp>
  <trd lng="pt">bainha</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_o;&c_v;</trd>,
    <trd>&c_r;&c_u;&c_b;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="orl.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEKS</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="border.0i.sxtofon">borderi</ref>,
      <ref cel="kudr.cxirkaux0i">&ccirc;irka&ubreve;kudri</ref>
    </refgrp>
  </snc>
  <trd lng="de">s&auml;umen</trd>
  <trd lng="en">hem</trd>
  <trd lng="fr">ourler</trd>
  <trd lng="hu">beszeg</trd>
  <trd lng="nl">zomen</trd>
  <trdgrp lng="pl">
    <trd>obr&eogonek;bia&cacute;</trd>,
    <trd>mere&zdot;kowa&cacute;</trd>,
    <trd>wywija&cacute; <klr>skraj materia&lstroke;u</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_r;&c_u;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <klr>(&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ju;)</klr></trd>
  </trdgrp>
</drv>
</art>
