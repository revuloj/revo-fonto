<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>orl</rad>/o</kap>
<drv mrk="orl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Refaldita bordero &ccirc;e tuko a&ubreve; vesto:
      <ekz>
        po&scirc;tuko kun lar&gcirc;a <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Saum</trd>
  <trd lng="nl">zoom</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_o;&c_v;</trd>,
    <trd>&c_r;&c_u;&c_b;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="orl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="de">s&auml;umen</trd>
  <trd lng="nl">zomen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_r;&c_u;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ju;)</klr></trd>
  </trdgrp>
</drv>
</art>
