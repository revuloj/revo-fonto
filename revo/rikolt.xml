    <trd>&#x5208;&#x308A;&#x5165;&#x308C;&#x308B;  <pr>&#x304B;&#x308A;&#x3044;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x63A1;&#x53D6;&#x3059;&#x308B;  <pr>&#x3055;&#x3044;&#x3057;&#x3085;&#x3059;&#x308B;</pr></trd>,
