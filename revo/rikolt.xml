<drv mrk="rikolt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rikolt.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Detran&ccirc;i kaj <ref tip="super" cel="kolekt.0i.pluki">kolekti</ref> grenon
      a&ubreve; iun vegeta&jcirc;on:
      <ekz>
        prisemu vian teron kaj <tld/>u &gcirc;iajn produkta&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 23:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi <tld/>os en via vinberejo, ne forkolektu la resta&jcirc;on post vi: io restu
        por la fremdulo, por la orfo, kaj por la vidvino
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 24:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Malsupra Egipto oni <tld/>is la trifolion kaj seneson
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is terpomojn kaj komencis la a&ubreve;tunan plugadon
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        arbaranoj <tld/>is fojnon sur la herbori&ccirc;a bordo
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Li ordonis al mi <tld/>i la homajn animojn en la nomo de l&#x2019; amo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[tio]</klr> efikas sur la vivtenan kapablon de somaliaj fi&scirc;kaptistoj, &ccirc;ar
        okcidentaj &scirc;ipoj <tld/>as en unu nokto tiom, kiom lokuloj kaptas en unu jaro
        <fnt>
          <bib>MD11</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        printempo semas, a&ubreve;tuno <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- apena&ubreve; uzata tiel kaj konsiderebla kiel figura uzo de snc 1!
  <snc>
    <uzo tip="fak">AGR</uzo>
    <dif>
      Amase kolekti iun ajn naturan produkta&jcirc;on:
      <ekz>
        oni <tld/>as la kaviaron &ccirc;e la bordoj de la Kaspia maro.
      </ekz>
    </dif>
  </snc>
-->
  <snc mrk="rikolt.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="ricev.0i">Ricevi</ref> iajn rezultatojn de sia ago:
      <ekz>
        por ke vi bone komprenu la tutan historion, estas necese reiri por <tld/>i
        diversajn scii&gcirc;ojn
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        kelkfoje li <tld/>is en tiuj noktaj ekskursoj tuberojn kaj kontuzojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni ankora&ubreve; ne sukcesis <tld/>i sufi&ccirc;e da materialo por plenigi tutan numeron
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        valorajn la&ubreve;rojn li <tld/>is en la tradukado kaj en la romanverkado
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kion vi semas, tion vi <tld/>os</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="ru">&c_ch;&c_t;&c_o;
&c_p;&c_o;&c_s;&c_je;&c_je;&c_sh;&c_mol;, &c_t;&c_o; &c_i;
&c_p;&c_o;&c_zh;&c_n;&c_jo;&c_sh;&c_mol;</trd>
      </ekz>
      <ekz>
        <ind>kiu semas venton, <tld/>os fulmotondron</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">wer Wind s&auml;t wird Sturm <ind>ernten</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ricev.0i">ricevi</ref>,
      <ref cel="gajn.0i">gajni</ref>,
      <ref cel="akir.0i">akiri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol;
<ind>&c_u;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_sa;&t_naro;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>sb&iacute;rat</trd>,
