<drv mrk="rikolt.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>as:
      <ekz>
        la resta&jcirc;on de via <tld/>a&jcirc;o ne forkolektu
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        restos tie post<tld/>a&jcirc;o, kiel &ccirc;e la skuado de olivarbo: du, tri olivoj sur la
        supro de alta bran&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 17:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o estas sekigata, sekve prilaborata: spikoj estas forigataj, aldonaj
        pajlaj elementoj kaj fine la pajleroj estas tran&ccirc;ataj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2014/011226.php">Pendantaj artaj pajla&jcirc;oj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_j;</trd>
  <trd lng="de">Erntegut</trd>
  <trd lng="en">harvest</trd>
  <trdgrp lng="id">
    <trd>hasil panen</trd>,
    <trd>ketaman</trd>,
    <trd>panen</trd>,
    <trd>panenan</trd>,
    <trd>tuaian</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21454;&#31339;&#29289;  <pr>&#12375;&#12421;&#12358;&#12363;&#12367;&#12406;&#12388;</pr></trd>,
    <trd>&#21454;&#37327;  <pr>&#12375;&#12421;&#12358;&#12426;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&zdot;niwo</trd>,
    <trd>ziemiop&lstroke;ody {pl}</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&c_o;&c_zh;&c_a;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_a;&c_t;&c_je; &c_z;&c_b;&c_ib;&c_zh;&c_zh;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#25910;&#25104; <pr>sh&omacron;uch&eacute;ng</pr></trd>
</drv>

<drv mrk="rikolt.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5184">rikolt'il'</url></lok></fnt></kap>
  <snc mrk="rikolt.0ilo.falcxileto">
    <dif>
      Malgranda <ref tip="super" cel="falcx.0ilo">fal&ccirc;ilo</ref>,
      kun mallonga tenilo kaj lunarka klingo:
      <ekz>
        akrigi sian <klr>[&#8230;]</klr> <tld/>ilon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 13:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        svingu la rikoltilon, &ccirc;ar la <tld/>a&jcirc;o estas matura
        <fnt><bib>MT</bib></fnt>;
      </ekz>
      <ekz>
        ili for&gcirc;os el siaj glavoj plugilojn kaj el siaj lancoj <tld/>ilojn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 2:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la briliantoj, aspektantaj kiel <tld/>ilo, povas figuri kvaronlunon
        <fnt>
          <bib>I&scirc;tar</bib>
