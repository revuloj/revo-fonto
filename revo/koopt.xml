<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koopt.xml,v 1.5 2024/03/01 09:26:58 revo Exp $">
<kap>
  <rad>koopt</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="koopt.0i">
  <kap><tld/>i</kap>
  <snc mrk="koopt.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="vid" cel="elekt.al0i">Alelekti</ref>
      per komuna decido de jamaj anoj:
      <ekz>
        la Estraro <klr>...</klr> &ccirc;e rezigno de estrarano dum
        balotperiodo rajtas <tld/>i novan membron
        <fnt>
         <aut>-</aut>,
         <vrk><url
         ref="http://w3.forst.tu-muenchen.de/~quednau/klerigofico/statutoj/sk.txt">
         Statuto de la Slovakia AIS</url></vrk>,
         <lok>[2006?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_p;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kooptovat</trd>,
    <trd>p&rcaron;ibrat za &ccaron;lena</trd>
  </trdgrp>
  <trd lng="fr">coopter</trd>
  <trdgrp lng="pl">
    <trd>dobiera&cacute;</trd>,
    <trd>dobra&cacute;</trd>
  </trdgrp>
  <trd lng="ro">&icirc;mperechea</trd>
  <trdgrp lng="sk">
    <trd>kooptova&tcaron;</trd>,
    <trd>pribra&tcaron; za &ccaron;lena</trd>
  </trdgrp>
</drv>
</art>

