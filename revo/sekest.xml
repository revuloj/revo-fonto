<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sekestr</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekest.0i.JUR">
    <dif>
      <ref tip="sin" cel="sekvestr.0i.JUR">Sekvestri</ref>:
      <ekz>
        polico <tld/>is kaj fermis la studion
        <klr>[&#8230;]</klr> pro iu nebula afero
        <fnt>
          <aut>F. Martorell, M. Eaton</aut>,
          <vrk>Esperanto Desperado sekestrita</vrk>,
          <lok><url ref="http://artista.ikso.net/eurokka/novajx/junio04_3.php">
          Informletero de Vinilkosmo, 2004-06 (7)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        jen, vi timas nenion &mdash; vian cerbon neniu povas <tld/>i
        <fnt>
          <aut>Georgi Stamatov, trad. Ivan H. Krestanoff</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Nuntempaj_rakontoj/Du_talentoj">Du talentoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>obstavit</trd>,
    <trd>zabavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beschlagnahmen</trd>,
    <trd>zwangsverwalten</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sekvestrova&tcaron;</trd>,
    <trd>s&uacute;dne zhaba&tcaron;</trd>
  </trdgrp>
</drv>
</art>
