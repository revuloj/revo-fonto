<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: advent.xml,v 1.26 2023/08/30 18:23:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>Advent</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="advent.0o">
  <kap><tld/>o</kap>
  <snc mrk="advent.0o.KRI">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Kvarsemajna tempo anta&ubreve;
      <ref tip="vid" cel="krist.0nasko">Kristnasko</ref>:
      <ekz>
        la vetero &scirc;an&gcirc;i&gcirc;is post meza <tld lit="a"/>o, komencis ne&gcirc;i tagon post tago
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_d;&c_n;&c_y; <ind>&c_p;&c_o;&c_s;&c_t;</ind></trd>
  <trd lng="ca">Advent</trd>
  <trd lng="cs">advent</trd>
  <trd lng="de">Advent</trd>
  <trd lng="en">Advent</trd>
  <trd lng="es">Adviento</trd>
  <trd lng="fr">avent</trd>
  <trd lng="hu">advent</trd>
  <trd lng="it">avvento<klr> (festa rel.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5F85;&#x964D;&#x7BC0;  [<ind>&#x305F;&#x3044;&#x3053;&#x3046;&#x305B;&#x3064;</ind>]</trd>,
    <trd>&#x964D;&#x81E8;&#x7BC0;  [<ind>&#x3053;&#x3046;&#x308A;&#x3093;&#x305B;&#x3064;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">advent</trd>
  <trd lng="pl">adwent</trd>
  <trd lng="pt">Advento</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_zh;&c_d;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j; &c_p;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_v;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">advent</trd>
  <trd lng="sv">advent</trd>
  <trdgrp lng="uk">
    <trd>&#x41F;&#x438;&#x43B;&#x438;&#x43F;&#x456;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x440;&#x456;&#x437;&#x434;&#x432;&#x44F;&#x43D;&#x438;&#x439; &#x43F;&#x456;&#x441;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="advent.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="advent.0ismo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Doktrino kaj praktiko de
      <ref tip="vid" cel="advent.0isto.KRI"><tld lit="a"/>istoj</ref>;
      bran&ccirc;o de
      <ref tip="malprt" cel="protes1.0ismo.KRI">protestantismo</ref>
      esti&gcirc;inta en la 19a jarcento,
      predikanta balda&ubreve;an revenon de Jesuo Kristo:
      <ekz>
        la&ubreve; <tld/>ismo kaj Jehovismo la animo ne estas nature senmorta
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Neniigeco">
        neniigeco</url>, 2010-06-22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_e;&c_n;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">adventisme</trd>
  <trd lng="cs">adventizmus</trd>
  <trd lng="fr">adventisme</trd>
  <trd lng="it">avventismo</trd>
  <trd lng="pl">adwentyzm</trd>
  <trd lng="sk">advedntizmus</trd>
</drv>

<drv mrk="advent.0isto">
  <kap><tld lit="a"/>isto</kap>
  <snc mrk="advent.0isto.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Ano de <ref tip="vid" cel="advent.0ismo.KRI"><tld/>ismo</ref>:
      <ekz>
        <nom>De Kock</nom> estas profunda kristano, <ind><tld lit="a"/>isto de la sepa
        tago</ind>, kun granda atento pri la Malnova Testamento
        <fnt>
          <aut>C. Minnaja</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/lf/konfliktrec.html">
          La konflikto de la epokoj [recenzo]</url></vrk>,
          <lok>Literatura foiro, 1988-04 (112)</lok>
        </fnt>.
        <trd lng="ca">adventista del set&eacute; dia</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_e;&c_n;&c_t;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">adventista</trd>
  <trd lng="cs">adventista</trd>
  <trd lng="fr">adventiste<klr> (subst.)</klr></trd>
  <trd lng="it">avventista</trd>
  <trd lng="pl">adwentysta</trd>
  <trd lng="sk">adventista</trd>
</drv>
</art>
