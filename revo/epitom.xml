<art mrk="$Id: epitom.xml,v 1.28 2025/03/29 15:36:47 revo Exp $">
<kap>
  <rad>epitom</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="epitom.0o">
  <kap><tld/>o</kap>
  <snc mrk="epitom.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="verk.0o.propra">Verko</ref>,
      kiu <ref cel="resum.0i">resumas</ref>
       pli ampleksan verkon por studa celo a&ubreve; lerneja uzo:
      <ekz>
        <ctl><tld lit="E"/>o de esperantologio</ctl>
        <fnt>
        <aut>N. Gudskov</aut>,
        <vrk>Epitomo de esperantologio</vrk>
        <lok>REU-Impeto, 2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        kompleta <tld/>o de la eduka scenaro en nia reala mondo
        <fnt>
          <vrk><url ref="https://www.coursehero.com/file/p4oqmp5/Li-akceptis-metilernadon-de-la-butiko-de-sia-patro-kaj-post-sia-morto-li-povis/">Li
          akceptis metilernadon de la butiko de sia patro kaj ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malvarma kafo vere estas la <tld/>o de &ccirc;io, kion elstara taso da kafo devus esti
        <fnt>
          <vrk><url ref="https://eo.haymancoffee.com/Blogoj/kafo-blogo/kvin-surprizaj-avanta%C4%9Doj-de-malvarma-kafo">Kvin
          Surprizaj Avanta&gcirc;oj de Malvarma Bierkafo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la urbo <nom>Baden-Baden</nom> <klr>[&#8230;]</klr> ne estas granda kosmopolita urbo kiel
        Berlino, &gcirc;i estas la <tld/>o de bela vivado
        <fnt>
          <vrk><url ref="https://www.saveatrain.com/blog/europe-visit-senior-travelers/?lang=eo">7
          Plej Bonaj Urboj En E&ubreve;ropo Viziti Por Maljunaj ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>  
    <ref tip="sin" cel="digxest.0o">di&gcirc;esto</ref>
    <refgrp tip="vid">
      <ref cel="elekt.0o">elekto</ref>,
      <ref cel="esenc.0o">esenco</ref>,
      <ref cel="ekstra.0o.TIP">ekstrakto<sncref/></ref>,
      <ref cel="kompen.0o">kompendio</ref>,
      <ref cel="kompil.0ajxo">kompila&jcirc;o</ref>,
      <ref cel="kvodli.0o">kvodlibeto</ref>,
      <ref cel="resum.0o">resumo</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>resumo</ke>, <ke>resuma lernolibro</ke>, <ke>eltira&#309;o</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_s;&c_p;&c_e;&c_k;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_r;&c_o;&c_ch;&c_a;&c_n;&c_y;
    &c_p;&c_je;&c_r;&c_a;&c_k;&c_a;&c_z;</trd>,
    <trd>&c_r;&c_e;&c_f;&c_e;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">ep&iacute;tom</trd>
  <trdgrp lng="de">
    <trd>Abriss</trd>,
    <trd>Kurzfassung</trd>,
    <trd>Auszug</trd>,
    <trd>Epitome</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>epitome <klr tip="amb">(abstract summary)</klr></trd>,
    <trd>digest</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pitom&eacute;</trd>
  <trdgrp lng="hu">
    <trd>epitom&eacute;</trd>,
    <trd>kivonatos m&udblac;</trd>,
    <trd>r&ouml;vid <ind>kivonat</ind></trd>,
    <trd>foglalat<klr tip="ind"> (kivonat)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25244;&#31883;  <pr>&#12400;&#12387;&#12377;&#12356;</pr></trd>,
    <trd>&#35201;&#32004;  <pr>&#12424;&#12358;&#12420;&#12367;</pr></trd>,
    <trd>&#26775;&#27010;  <pr>&#12371;&#12358;&#12364;&#12356;</pr></trd>,
    <trd>&#27010;&#35201;  <pr>&#12364;&#12356;&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>streszczenie<klr tip="amb"> (ksi&aogonek;&zdot;ki)</klr></trd>,
    <trd>skr&oacute;t<klr tip="amb"> (ksi&aogonek;&zdot;ki)</klr></trd>,
    <trd>bryk</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ep&iacute;tome</trd>,
    <trd>comp&ecirc;ndio</trd>,
    <trd>sinopse</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_k;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_je;
    &c_i;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_s;&c_p;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_d;&c_a;&c_j;&c_d;&c_zh;&c_je;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>epitom&eacute;</trd>,
    <trd>v&yacute;&tcaron;ah z diela</trd>
  </trdgrp>
  <trd lng="cs">epitom&eacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&#1103;&c_g;</trd>,
    <trd>&c_s;&#1082;&#1086;&c_r;&#1086;&c_ch;&c_je;&#1085;&c_i;&c_j; &c_v;&c_i;&#1082;&#1083;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25688;&#35201; <pr>zh&#257;iy&#224;o</pr></trd>,
    <trd>&#31680;&#26412; <pr>ji&#233;b&#283;n</pr></trd>,
    <trd>&#31680;&#30053; <pr>ji&#233;l&#252;&#232;</pr></trd>,
    <trd>&#31616;&#26412; <pr>ji&#462;nb&#283;n</pr></trd>
  </trdgrp>
</drv>
