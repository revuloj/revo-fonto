<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>epitom</rad>/o <fnt>Z</fnt></kap>
<drv mrk="epitom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref> kiu resumas pli ampleksan verkon por studa celo a&ubreve; lerneja uzo.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="kompen.0o">kompendio</ref>,
    <ref cel="kompil.0ajxo">kompila&jcirc;o</ref>
  </refgrp>
  <trdgrp lng="de">
     <trd>Abriss</trd>,
     <trd>Kurzfassung</trd>,
     <trd>Auszug</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pitom&eacute;</trd>
  <trdgrp lng="hu">
     <trd>epitom&eacute;</trd>,
     <trd>kivonatos m&udblac;</trd>,
     <trd>r&ouml;vid <ind>kivonat</ind></trd>,
     <trd>foglalat<klr tip="ind"> (kivonat)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_k;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_je; &c_i;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_s;&c_p;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
</drv>
</art>
