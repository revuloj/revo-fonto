<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>boved</rad>/oj <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="boved.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="boved.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Grandaj kvarpiedaj bestoj plejofte kornoportantaj 
      &ccirc;e amba&ubreve; seksoj, familio  el la ordo <ref tip="super" 
      cel="huf.par0uloj">parhufuloj</ref> de <ref tip="super" 
      cel="macx.re0uloj">rema&ccirc;antaj</ref> (<trd 
      lng="la">Bovidae</trd>):
      <ekz>
        Antilopoj, &scirc;afoj, kaproj, kaj bovoj estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_a;&c_r;&c_o;&c_g;&c_ib;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Horntiere</trd>
  <trd lng="en">bovids</trd>
  <trd lng="fr">bovid&eacute;s</trd>
  <trd lng="hu">tulokf&eacute;l&eacute;k</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_r;&c_o;&c_g;&c_i;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_p;&c_a;&c_r;&c_n;&c_o;&c_k;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_h;)</klr></trd>
</drv>
</art>

