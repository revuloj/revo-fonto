<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: boved.xml,v 1.13 2022/03/13 11:32:50 revo Exp $">
<kap>
  <rad>boved</rad>/oj <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="boved.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="boved.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Grandaj kvarpiedaj bestoj plejofte kornoportantaj 
      &ccirc;e amba&ubreve; seksoj, <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">familio</ref>  el la ordo <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref> de <ref tip="malprt" cel="macx.re0uloj">rema&ccirc;antaj</ref> (<trd lng="la">Bovidae</trd>):
      <ekz>
        Antilopoj, &scirc;afoj, kaproj, kaj bovoj estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_a;&c_r;&c_o;&c_g;&c_ib;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_u;&c_h;&c_o;&c_r;&c_o;&c_g;&c_i;</trd>
  <trd lng="ca">b&ograve;vids</trd>
  <trd lng="cs">turovit&iacute;</trd>
  <trd lng="de">Horntiere</trd>
  <trd lng="en">bovids</trd>
  <trd lng="fr">bovid&eacute;s</trd>
  <trd lng="hu">tulokf&eacute;l&eacute;k</trd>
  <trd lng="nl">holhoornigen</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_r;&c_o;&c_g;&c_i;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;
  &c_p;&c_a;&c_r;&c_n;&c_o;&c_k;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_h;)</klr></trd>
  <trd lng="sk">turovit&iacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x440;&#x43E;&#x436;&#x43D;&#x438;&#x441;&#x442;&#x43E;&#x440;&#x43E;&#x433;&#x456;</trd>,
    <trd>&#x431;&#x438;&#x447;&#x430;&#x447;&#x456;</trd>
  </trdgrp>
</drv>
</art>

