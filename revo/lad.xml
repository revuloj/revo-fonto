<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lad.xml,v 1.22 2016/09/29 09:10:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lad</rad>/o
</kap>

<drv mrk="lad.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Plato el iu ajn metalo:
      <ekz>
        fer<tld/>o, stanita <tld/>o;
      </ekz>
      <ekz>
        eksonis trumpetoj kaj bronzaj <tld/>oj
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 4</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="krisp.0a">krispa</ref>,
      <ref cel="tavol.0o">tavolo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="bg">&c_t;&c_je;&c_n;&c_je;&c_k;&c_i;&c_ja;</trd>
  <trd lng="de">Blech</trd>
  <trd lng="en">sheet metal</trd>
  <trd lng="es">l&aacute;mina</trd>
  <trdgrp lng="fr">
    <trd>t&ocirc;le</trd>,
    <trd>fer-blanc</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;mlemez</trd>,
    <trd>pl&eacute;h</trd>
    <trd>b&aacute;dog</trd>
  </trdgrp> 
  <trd lng="nl">dunne <ind>metaalplaat</ind></trd>
  <trd lng="pl">blacha</trd>
  <trdgrp lng="pt">
    <trd>lata</trd>,
    <trd>folha de flandres</trd>,
    <trd>folha <klr tip="amb">(folha de flandres)</klr></trd>,
    <trd>l&acirc;mina <klr tip="amb">(de metal)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_je; 
&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="lad.onda0o">
  <kap>
    onda <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="L"/>o el &scirc;talo, plej ofte zinkita, prezentanta
      paralelajn kurbi&gcirc;ojn alterne konveksajn kaj
      konkavajn, uzata por tegmentoj, eksteraj muroj.
    </dif>
  </snc>
  <trd lng="de">Wellblech</trd>
  <trdgrp lng="en">
    <trd>corrugated iron</trd>,
    <trd>corrugated sheet metal</trd>
  </trdgrp>
  <trd lng="es"><ind>l&aacute;mina</ind> corrugada</trd>
  <trd lng="fr">t&ocirc;le ondul&eacute;e</trd>
  <trd lng="hu">hull&aacute;mlemez</trd> 
  <trd lng="nl">golfplaat</trd>
  <trd lng="pl">blacha falista</trd>
  <trdgrp lng="ru">    
    <trd>&c_g;&c_o;&c_f;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="lad.0skatolo">
  <kap>
    <tld/>skatolo
  </kap>
  <snc>
    <dif>
      Hermetike fermita skatolo el stanita <tld/>o, &ccirc;efe uzata por 
konservi nutra&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_sh;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">(&c_t;&c_je;&c_n;&c_je;&c_k;&c_i;&c_je;&c_n;&c_a;) <ind>&c_k;&c_u;&c_t;&c_i;&c_ja;</ind></trd>
  <trdgrp lng="de">
     <trd>Blechb&uuml;chse</trd>,
     <trd>Blechdose</trd>,
     <trd>Dose</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tin<klr> (brite)</klr></trd>,
    <trd>(tin) <ind>can</ind><klr> (usone)</klr></trd>
  </trdgrp>
  <trd lng="es">lata</trd>
  <trd lng="fr">bo&icirc;te de conserve</trd>
  <trd lng="hu">hull&aacute;mlemez</trd> 
  <trd lng="nl">blikje</trd>
  <trd lng="pl">puszka</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_n;&c_k;&c_a; <klr>(&c_zh;&c_je;&c_s;&c_t;&c_ja;&c_n;&c_a;&c_ja;)</klr></trd>
</drv>
</art>
