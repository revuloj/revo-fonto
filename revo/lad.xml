<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lad.xml,v 1.34 2024/11/14 15:30:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lad</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="lad.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="lad.0o.plato">
    <dif>
      <ref tip="super" cel="plat.0o">Plato</ref> el iu ajn <ref cel="metal.0o">metalo</ref>:
      <ekz>
        zorge fiksita tubo el <tld/>o
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi eka&ubreve;dos tri batojn en la bronzajn <tld/>ojn, suriru la terason
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        terura asiria muziko: trumpetoj, tamburoj, <tld/>oj kaj flutoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        tegmenta <tld/>o
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        zinkita <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2015/011449.php">Trep&ccedil;a, pomo de malkonkordo inter Kosovo kaj
          Serbio</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        stranga&jcirc;oj stancitaj el stanita <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        galvanizita <tld/>o
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        rustinta <tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        flava <tld/>o ekbrilis sur la brusto de homo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lamen.0o">lameno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="bg">&c_t;&c_je;&c_n;&c_je;&c_k;&c_i;&c_ja;</trd>
  <trd lng="de">Blech</trd>
  <trd lng="en">sheet metal</trd>
  <trd lng="es">l&aacute;mina</trd>
  <trdgrp lng="fr">
    <trd>t&ocirc;le</trd>,
    <trd>fer-blanc</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;mlemez</trd>,
    <trd>pl&eacute;h</trd>
    <trd>b&aacute;dog</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#37329;&#23646;&#12398;&#34180;&#26495; <pr>&#12365;&#12435;&#12382;&#12367;&#12398;&#12358;&#12377;&#65381;&#12356;&#12383;</pr></trd>,
    <trd>&#37329;&#23646;&#26495; <pr>&#12365;&#12435;&#12382;&#12367;&#12400;&#12435;</pr></trd>,
    <trd>&#12502;&#12522;&#12461;</trd>,
    <trd>&#12488;&#12479;&#12531; <pr>&#12392;&#12383;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">dunne <ind>metaalplaat</ind></trd>
  <trd lng="pl">blacha</trd>
  <trdgrp lng="pt">
    <trd>lata</trd>,
    <trd>folha de flandres</trd>,
    <trd>folha <klr tip="amb">(folha de flandres)</klr></trd>,
    <trd>l&acirc;mina <klr tip="amb">(de metal)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_je;
&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_je; &c_z;&c_a;&c_l;&c_ib;&c_z;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34180;&#26495; <pr>b&aacute;ob&#462;n</pr></trd>,
    <trd>&#37941;&#30382; <pr>ti&ecaron;p&iacute;</pr></trd>,
    <trd>&#38081;&#30382; <pr>ti&ecaron;p&iacute;</pr></trd>,
    <trd>&#37393; <pr>b&#462;n</pr></trd>,
    <trd>&#38051; <pr>b&#462;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lad.fer0o">
  <kap>fer<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="L"/>o el fero, eventuale protektitaj de rusto per stano a&ubreve; zinko ks:
      <ekz>
        &Jcirc;akemaro eltiris el sia po&scirc;o skatoleton el fer<tld/>o
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>stanita fer<tld/>o</ind>
        <fnt>
          <bib>EeP</bib>
        </fnt>.
        <trd lng="de">Wei&szlig;blech</trd>
        <trd lng="en">tinplate</trd>
        <trdgrp lng="fr">
          <trd>fer &eacute;tam&eacute;</trd>,
          <trd>fer-blanc</trd>
        </trdgrp>
        <trd lng="hu">b&aacute;dog</trd>
        <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="br">tolenn houarn</trd>
  <trd lng="cs">&zcaron;elezn&yacute; plech</trd>
  <trd lng="de">Eisenblech</trd>
  <trd lng="es">chapa</trd>
  <trd lng="fr">t&ocirc;le<klr tip="ind"> (de fer, de fer-blanc)</klr></trd>
  <trd lng="hu">vaslemez</trd>
  <trd lng="ja">&#34180;&#37444;&#26495;  <pr>&#12358;&#12377;&#12390;&#12387;&#12401;&#12435;</pr></trd>
  <trd lng="pl">blacha</trd>
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">&zcaron;elezn&yacute; plech</trd>
  <trd lng="uk">&c_n;&c_je;&c_o;&c_c;&c_i;&c_n;&c_k;&c_o;&c_v;&c_a;&c_n;&c_a; &c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
</drv>

<drv mrk="lad.onda0o">
  <kap>
    onda <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="L"/>o fortikigita per
      paralelaj kurbi&gcirc;oj alterne konveksaj kaj
      konkavaj, uzata por tegmentoj ks:
      <ekz>
        la hala tegmento estis el onda <tld/>o, kiu ree&hcirc;is la bruon
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_f;&c_r;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; <ind>&c_b;&c_l;&c_ja;&c_h;&c_a;</ind></trd>,
    <trd>&c_g;&c_a;&c_f;&c_r;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je; <ind>&c_zh;&c_a;&c_l;&c_je;&c_z;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Wellblech</trd>
  <trdgrp lng="en">
    <trd>corrugated iron</trd>,
    <trd>corrugated sheet metal</trd>
  </trdgrp>
  <trd lng="es"><ind>l&aacute;mina</ind> corrugada</trd>
  <trd lng="fr">t&ocirc;le ondul&eacute;e</trd>
  <trd lng="hu">hull&aacute;mlemez</trd>
  <trdgrp lng="ja">
    <trd>&#27874;&#26495;  <pr>&#12394;&#12415;&#12356;&#12383;</pr></trd>,
    <trd>&#12394;&#12414;&#12371;&#26495;  <pr>&#12394;&#12414;&#12371;&#12356;&#12383;</pr></trd>,
    <trd>&#12488;&#12479;&#12531;&#26495;  <pr>&#12488;&#12479;&#12531;&#12356;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="nl">golfplaat</trd>
  <trd lng="pl">blacha falista</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_f;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29926;&#26974;&#37941;&#30382; <pr>w&#462;l&eacute;ngti&ecaron;p&iacute;</pr></trd>,
    <trd>&#29926;&#26974;&#38081;&#30382; <pr>w&#462;l&eacute;ngti&ecaron;p&iacute;</pr></trd>
  </trdgrp>
</drv>

</art>
