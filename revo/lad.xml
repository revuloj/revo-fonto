<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lad</rad>/o
</kap>
<drv mrk="lad.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Plato el iu ajn metalo:
      <ekz>
        fer<tld/>o, stanita <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="krisp.0a">krispa</ref>,
      <ref cel="tavol.0o">tavolo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">sheet metal</trd>
  <trd lng="nl">metaalplaat</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="lad.onda0o">
  <kap>
    onda <tld/>o
  </kap>
  <snc>
    <dif>
      Fer<tld/>o prezentanta paralelajn kurbi&gcirc;ojn alterne konveksajn kaj
      konkavajn.
    </dif>
  </snc>
  <trd lng="en">corrugated iron</trd>
  <trd lng="nl">golfplaat</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_f;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="lad.0skatolo">
  <kap>
    <tld/>skatolo
  </kap>
  <snc>
    <dif>
      Hermetike fermita skatolo, &ccirc;efe uzata por konservi nutra&jcirc;ojn.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>tin</trd>,
    <trd>tin can<klr> (Usone)</klr></trd>
  </trdgrp>
  <trd lng="nl">blikje</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_n;&c_k;&c_a; <klr>(&c_zh;&c_je;&c_s;&c_t;&c_ja;&c_n;&c_a;&c_ja;)</klr></trd>
</drv>
</art>
