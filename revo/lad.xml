<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lad.xml,v 1.28 2022/05/01 12:04:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lad</rad>/o
</kap>

<drv mrk="lad.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="lad.0o.plato">
    <dif>
      <ref tip="super" cel="plat.0o">Plato</ref> el iu ajn <ref cel="metal.0o">metalo</ref>:
      <ekz>
        zorge fiksita tubo el <tld/>o
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi eka&ubreve;dos tri batojn en la bronzajn <tld/>ojn, suriru la terason
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        terura asiria muziko: trumpetoj, tamburoj, <tld/>oj kaj flutoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        tegmenta <tld/>o
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        zinkita <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2015/011449.php">Trep&ccedil;a, pomo de malkonkordo inter Kosovo kaj Serbio</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        stranga&jcirc;oj stancitaj el stanita <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        galvanizita <tld/>o
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        rustinta <tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        flava <tld/>o ekbrilis sur la brusto de homo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="krisp.0a">krispa</ref>,
      <ref cel="tavol.0o">tavolo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="bg">&c_t;&c_je;&c_n;&c_je;&c_k;&c_i;&c_ja;</trd>
  <trd lng="de">Blech</trd>
  <trd lng="en">sheet metal</trd>
  <trd lng="es">l&aacute;mina</trd>
  <trdgrp lng="fr">
    <trd>t&ocirc;le</trd>,
    <trd>fer-blanc</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;mlemez</trd>,
    <trd>pl&eacute;h</trd>
    <trd>b&aacute;dog</trd>
  </trdgrp> 
  <trd lng="nl">dunne <ind>metaalplaat</ind></trd>
  <trd lng="pl">blacha</trd>
  <trdgrp lng="pt">
    <trd>lata</trd>,
    <trd>folha de flandres</trd>,
    <trd>folha <klr tip="amb">(folha de flandres)</klr></trd>,
    <trd>l&acirc;mina <klr tip="amb">(de metal)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_je; 
&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x43B;&#x44F;&#x445;&#x430;</trd>,
    <trd>&#x436;&#x435;&#x440;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43B;&#x438;&#x441;&#x442;&#x43E;&#x432;&#x435; &#x437;&#x430;&#x43B;&#x456;&#x437;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="lad.fer0o">
  <kap>fer<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="L"/>o el fero, eventuale protektitaj de rusto per stano a&ubreve; zinko ks:
      <ekz>
        &Jcirc;akemaro eltiris el sia po&scirc;o skatoleton el fer<tld/>o
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>stanita fer<tld/>o</ind>
        <fnt>
          <bib>EeP</bib>
        </fnt>.
        <trd lng="de">Wei&szlig;blech</trd>
        <trd lng="en">tinplate</trd>
        <trdgrp lng="fr">
          <trd>fer &eacute;tam&eacute;</trd>,
          <trd>fer-blanc</trd>
        </trdgrp>
        <trd lng="hu">b&aacute;dog</trd>
        <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="br">tolenn houarn</trd>
  <trd lng="cs">&zcaron;elezn&yacute; plech</trd>
  <trd lng="de">Eisenblech</trd>
  <trd lng="es">chapa</trd>
  <trd lng="fr">t&ocirc;le<klr tip="ind"> (de fer, de fer-blanc)</klr></trd>
  <trd lng="hu">vaslemez</trd>
  <trd lng="pl">blacha</trd>
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">&zcaron;elezn&yacute; plech</trd>
  <trd lng="uk">&#x43D;&#x435;&#x43E;&#x446;&#x438;&#x43D;&#x43A;&#x43E;&#x432;&#x430;&#x43D;&#x430; &#x431;&#x43B;&#x44F;&#x445;&#x430;</trd>
</drv>

<drv mrk="lad.0skatolo">
  <kap>
    <tld/>skatolo
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="skatol.0o">Skatolo</ref> el lado, ekz-e hermetike fermita
      el stanita <tld/>o por konservi nutra&jcirc;ojn ks:
      <ekz>
        malplenaj boteloj kaj <tld/>skatoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2004/008236.html">La rikolto de la nova tempo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        negoci per buntaj <tld/>skatoloj plenaj je aero
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/009845.php">Mono el aero</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="konser.0ujo">konservujo</ref>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_sh;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">(&c_t;&c_je;&c_n;&c_je;&c_k;&c_i;&c_je;&c_n;&c_a;) <ind>&c_k;&c_u;&c_t;&c_i;&c_ja;</ind></trd>
  <trdgrp lng="de">
    <trd>Blechb&uuml;chse</trd>,
    <trd>Blechdose</trd>,
    <trd>Dose</trd>,
    <trd>Konserve</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tin<klr> (brite)</klr></trd>,
    <trd>(tin) <ind>can</ind><klr> (usone)</klr></trd>
  </trdgrp>
  <trd lng="es">lata</trd>
  <trd lng="fr">bo&icirc;te de conserve</trd>
  <trd lng="hu">hull&aacute;mlemez</trd> 
  <trd lng="nl">blikje</trd>
  <trd lng="pl">puszka</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_n;&c_k;&c_a; <klr>(&c_zh;&c_je;&c_s;&c_t;&c_ja;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="uk">&#x431;&#x43B;&#x44F;&#x448;&#x430;&#x43D;&#x43A;&#x430;</trd>
</drv>


<drv mrk="lad.onda0o">
  <kap>
    onda <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="L"/>o fortikigita per 
      paralelaj kurbi&gcirc;oj alterne konveksaj kaj
      konkavaj, uzata por tegmentoj ks:
      <ekz>
        la hala tegmento estis el onda <tld/>o, kiu ree&hcirc;is la bruon
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wellblech</trd>
  <trdgrp lng="en">
    <trd>corrugated iron</trd>,
    <trd>corrugated sheet metal</trd>
  </trdgrp>
  <trd lng="es"><ind>l&aacute;mina</ind> corrugada</trd>
  <trd lng="fr">t&ocirc;le ondul&eacute;e</trd>
  <trd lng="hu">hull&aacute;mlemez</trd> 
  <trd lng="nl">golfplaat</trd>
  <trd lng="pl">blacha falista</trd>
  <trdgrp lng="ru">    
    <trd>&c_g;&c_o;&c_f;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_o;&c_je; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  </trdgrp>
</drv>

</art>
