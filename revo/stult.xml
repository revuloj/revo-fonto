<drv mrk="stult.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="stult.0umi.KOMUNE">
    <dif>
      &Scirc;ajnigi <tld/>on, obstine agi kvaza&ubreve; naive pro ne
      eldirita kialo:
      <ekz>
        kelkaj inter ni elektis la duan vojon, nome maksimume <tld/>umi,
        preferante propagandi mortnaskitajn reformalfabetajn projektojn
        ol
        serioze okupi&gcirc;i pri konvenaj komputilaj solvoj por nia
        lingvo
        <fnt>
        <aut>Pejno S.</aut>,
        <vrk>Sidi&gcirc;u kaj tajpu!</vrk>,
        <bib>Monato</bib>,
        <lok>2001:4, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="infan.0umi.PSI">infanumi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_u;&c_r;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire l'<ind>idiot</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&uuml;ly&eacute;skedik</trd>,
    <trd>id&eacute;tlenkedik</trd>
  </trdgrp>
  <trd lng="it">agire <ind>stupidamente</ind></trd>
  <trdgrp lng="pl">
    <trd>wyg&lstroke;upia&cacute; si&eogonek;</trd>,
    <trd>wydurnia&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_d;&c_u;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8CBD;&#x7B11; <pr>y&#xED;xi&#xE0;o</pr></trd>,
    <trd>&#x8D3B;&#x7B11; <pr>y&#xED;xi&#xE0;o</pr></trd>
