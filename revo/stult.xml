<drv mrk="stult.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stult.0igi.KOMUNE">
    <dif>
      Igi a&ubreve; &scirc;ajnigi <tld/>a:
      <ekz>
        ne permesu <klr>...</klr> ke kiso
        <tld/>igu vin
        <fnt>
          <aut>trad. R. F&ouml;ssmeier</aut>,
          <vrk><url ref="http://foessmeier.de/reinhard/cetero/fortune/sprit">
          sprit</url></vrk>,
          <lok>[2000]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_r;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>ohlupovat</trd>,
    <trd>zpitomit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ab&ecirc;tir</trd>,
    <trd>abrutir</trd>
  </trdgrp>
  <trd lng="pl">og&lstroke;upia&cacute;</trd>
  <trdgrp lng="sk">
    <trd>ohl&uacute;pnu&tcaron;</trd>,
    <trd>osprostie&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="stult.0ulo">
