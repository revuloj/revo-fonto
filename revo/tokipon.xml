<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Tokipon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tokipon.0o">
  <kap><tld/>o</kap>
  <snc mrk="tokipon.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="lst" cel="lingv.0o.homa" lst="voko:lingvoj">Lingvo</ref>
      internacia, kreita de la kanadano Sonja Elen Kisa 
      en 2001:
      <ekz>
        <nom>Sonja Lang</nom> estas profesia tradukisto (la angla, la franca kaj Esperanto)
        <klr>[kaj]</klr> inventis la planlingvon <tld lit="T"/>o, kiu estas lingvo
        simpla, la&ubreve; la pensoj de Taoismo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=763"
              >Sonja Lang</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="da">Tokipono</trd>
  <trd lng="de">Toki Pona</trd>
  <trd lng="en">Toki Pona</trd>
  <trd lng="es">toki pona</trd>
  <trd lng="fr">Toki pona</trd>
  <trd lng="nl">Toki Pona</trd>
  <trd lng="no">Toki pona</trd>
  <trd lng="sv">Toki pona</trd>
</drv>
</art>

