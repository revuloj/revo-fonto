<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Tokipon</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="tokipon.0o">
  <kap><tld/>o</kap>
  <snc mrk="tokipon.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="lst" lst="voko:lingvoj" cel="lingv.plan0o.LIN">Planlingvo</ref>
      minimumisma, celanta simplecon kaj pozitivan pensadon:
      <ekz>
        <tld/>o estas kreita de kanada tradukistino <nom>Sonja LANG</nom>,
        kiu publikigis &gcirc;in en 2001
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://tokipona.info/PriTokipono"
              >toki pona en 76 ilustritaj lecionoj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="da">Tokipono</trd>
  <trd lng="de">Toki Pona</trd>
  <trd lng="en">Toki Pona</trd>
  <trd lng="es">toki pona</trd>
  <trd lng="fr">Toki pona</trd>
  <trd lng="nl">Toki Pona</trd>
  <trd lng="no">Toki pona</trd>
  <trd lng="sv">Toki pona</trd>
</drv>
</art>

