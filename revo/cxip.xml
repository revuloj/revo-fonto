<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxip.xml,v 1.8 2024/05/30 22:29:39 revo Exp $">
<kap>
  <rad>&ccirc;ip</rad>/a <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="cxip.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="cxip.0a.EKON">
    <dif>
      <ref tip="dif" cel="kost.malmulte0a.EKON">Malmultekosta</ref>:
      <ekz>
        &gcirc;i e&ccirc; deproksime similas <tld/>an spickukan &ccirc;evaleton
        <fnt>
          <aut>J. Mura&scirc;kovskij kaj I. Mura&scirc;kovska</aut>,
          <vrk>Kien fluas roj' Castalie?</vrk>
          <lok><url ref="&Tekstaro;?nomo=kien-fluas-roj-castalie">Tekstaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; ilia monhelpo <klr>[...]</klr> estis nenio alia ol <tld/>a reklamkampanjo
        <fnt>
          <aut>V. Melnikov</aut>,
          <vrk>&Ccirc;u sincera naivo?</vrk>,
          <bib>LOdE</bib>,
          <lok>jaro 2000a, numero 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu el la e&ubreve;ropaj elituloj deziras havi abundan kaj <tld/>an gason el Rusio
        <fnt>
          <aut>S. Belov</aut>,
          <vrk>Greta Thunberg kaj politikaj ludoj</vrk>,
          <lok>&leftquot;Kontakto&rightquot;, n-ro 292 (2019:04), p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kar.mal0a">malkara</ref>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_n;&c_n;&c_y;</trd>
  <trd lng="de">billig</trd>
  <trdgrp lng="en">
    <trd>cheap</trd>,
    <trd>inexpensive</trd>,
    <trd>low-priced</trd>
  </trdgrp>
  <trd lng="es">barato</trd>
  <trd lng="fy">goedkeap</trd>
  <trd lng="nl">goedkoop</trd>
  <trd lng="pl">niedrogi</trd>
  <trd lng="uk">&c_d;&c_je;&c_sh;&c_je;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EF7;&#x683C;&#x4FBF;&#x5B9C; <pr>ji&#xE0;g&#xE9;pi&#xE1;nyi</pr></trd>,
    <trd>&#x4F4E;&#25104;&#x672C; <pr>d&#x12B;ch&#xE9;ngb&#x11B;n</pr></trd>,
    <trd>&#x8CE4; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x5EC9; <pr>li&#xE1;n</pr></trd>,
    <trd>&#x4FBF;&#x5B9C; <pr>pi&#xE1;nyi</pr></trd>
  </trdgrp>
</drv>
</art>

