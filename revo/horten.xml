<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hortensi</rad>/o
</kap>
<drv mrk="horten.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
cel="saksifrag.0oj">saksifragacoj</ref>,
      (<trd lng="la">Hydrangea</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_t;&c_e;&c_n;&c_z;&c_ib;&c_ja;</trd>
  <trdgrp lng="br">        
    <trd>hortensia</trd>,
    <trd>kaouled<klr tip="ind"> (hortensia)</klr></trd>    
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hortensie</trd>,
    <trd>Wasserstrauch</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&tau;&alfa;&ny;&sigma;&jota_ton;&alfa;</trd>
  <trd lng="fr">hortensia</trd>
  <trd lng="hu">hortenzia</trd>
  <trd lng="nl">hortensia</trd>
  <trd lng="pt">hort&ecirc;nsia</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_t;&c_je;&c_n;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">hortenzia</trd>
</drv>
</art>
