<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: decime.xml,v 1.21 2021/03/08 14:29:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>decimetr</rad>/o
</kap>
<drv mrk="decime.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">dm</mlg>
  <snc>
    <dif>
    <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo, dekono de 
      <ref tip="malprt" cel="metr.0o.unuo">metro</ref>:
      <ekz>
        <klr>[li]</klr> demetas la ujon en etan truon malmultajn <tld/>ojn sub la tersurfaco
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011167.php">&Ccirc;erko&ccirc;are mono&scirc;pare</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="kilome.0o">kilometro</ref>,
      <ref cel="hektom.0o">hektometro</ref>,
      <ref cel="dekame.0o">dekametro</ref>,
      <ref cel="metr.0o">metro</ref>,
    </refgrp>
    <refgrp tip="prt">
      <ref cel="centim1.0o">centimetro</ref>,
      <ref cel="milime.0o">milimetro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_c;&c_y;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">decimetr</trd>
  <trd lng="de">Dezimeter</trd>
  <trdgrp lng="en">
    <trd>decimeter</trd>,
    <trd>decimetre</trd>
  </trdgrp>
  <trd lng="es">dec&iacute;metro</trd>
  <trd lng="fr">d&eacute;cim&egrave;tre</trd>
  <trd lng="he">&#x5D3;&#x5E6;&#x5D9;&#x5DE;&#x5D8;&#x5E8;</trd>
  <trd lng="hu">decim&eacute;ter</trd>
  <trd lng="nl">decimeter</trd>
  <trd lng="pl">decymetr</trd>
  <trd lng="pt">dec&iacute;metro</trd>
  <trd lng="ru">&c_d;&c_je;&c_c;&c_i;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">decimeter</trd>
  <trd lng="sv">decimeter</trd>
</drv>
</art>
