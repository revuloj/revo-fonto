<drv mrk="bang.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="afrik.CentrA0o">Centr-Afriko</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> fondi&gcirc;is en 1889 kaj en 1960 i&gcirc;is la &ccirc;efurbo de la
        sendependi&gcirc;anta lando
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Bango"
              >Bango - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alianco de antiregistaraj armitaj fortoj, plu proksimi&gcirc;is al <tld/>o, &ccirc;efurbo de
        Centra-Afrika Respubliko
        <fnt>
          <vrk><url ref="http://esperanto.cri.cn/1341/2012/12/31/170s144426.htm"
              >Seleka proksimi&gcirc;is al centrafrika &ccirc;efurbo Bango</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bangui</trd>
    <trd lng="en">Bangui</trd>
    <trd lng="fr">Bangui</trd>
  </snc>
</drv>

