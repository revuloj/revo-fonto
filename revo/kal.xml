<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kal.xml,v 1.31 2023/12/02 21:20:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kal</rad>/o
</kap>
<drv mrk="kal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kal.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loko en la ha&ubreve;to, precipe &ccirc;e piedoj a&ubreve; manoj, malmoli&gcirc;inta
      pro frotado:
      <ekz>
        la for&gcirc;isto estas nigra, malpura, havas fingrojn kovritajn de
        <tld/>oj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        mi certe tretas sur kelkaj <ind>piedkalo</ind>j &ccirc;e la profesia
        instruistaro
        <trd lng="ca">ull de poll</trd>
        <trd lng="de">H&uuml;hnerauge</trd>
        <trd lng="fr">&oelig;il-de-perdrix</trd>
        <trdgrp lng="nl">
          <trd>eksteroog</trd>,
          <trd>likdoorn</trd>
        </trdgrp>
        <fnt>
      <aut>KaGu:-}</aut>,
      <vrk>nova pedagogio</vrk>,
      <lok><url ref="&SCE;">soc.culture.esperanto</url>,
      2007-03-18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_o;&c_l;&c_mol;</trd>
  <trd lng="br">kaledenn<klr tip="ind"> (kroc'hen)</klr></trd>
  <trdgrp lng="ca">
    <trd>call</trd>,
    <trd>dur&iacute;cia</trd>
  </trdgrp>
  <trd lng="cs">mozol</trd>
  <trd lng="de">Schwiele</trd>
  <trdgrp lng="en">
    <trd>callus</trd>,
    <trd>corn</trd>
  </trdgrp>
  <trd lng="es">callo</trd>
  <trd lng="fr">cal</trd>
  <trdgrp lng="hu">
    <trd>b&odblac;rkem&eacute;nyed&eacute;s</trd>,
    <trd>ty&uacute;kszem</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305F;&#x3053;</trd>,
    <trd>&#x3046;&#x304A;&#x306E;&#x3081;</trd>
  </trdgrp>
  <trdgrp lng="la">
    <trd>callus</trd>,
    <trd>clavus</trd>
  </trdgrp>
  <trd lng="nl">eelt</trd>
  <trdgrp lng="pl">
    <trd>odcisk</trd>,
    <trd>nagniotek</trd>
    <trd>modzel</trd>,
  </trdgrp>
  <trd lng="pt">calo</trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>mozo&#x13E;</trd>,
    <trd>otlak</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>liktorn</trd>,
    <trd>valk</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x437;&#x43E;&#x43B;&#x44F;</trd>,
    <trd>&#x43C;&#x43E;&#x437;&#x43E;&#x43B;&#x44C;</trd>,
    <trd>&#x43A;&#x430;&#x43B;&#x44E;&#x441;</trd>
  </trdgrp>
  <trd lng="zh">&#x96DE;&#x773C; <pr>j&#x12B;y&#x1CE;n</pr></trd>
</drv>

<drv mrk="kal.0a">
  <kap><tld/>a</kap>
  <snc mrk="kal.0a.MED">
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        <tld/>aj manoj de laboristoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_o;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="br">kaledet<klr tip="ind"> (kroc'hen)</klr></trd>
  <trd lng="ca">call&oacute;s</trd>
  <trd lng="cs">mozolnat&#xFD;</trd>
  <trdgrp lng="de">
    <trd>schwielig</trd>,
    <trd>verhornt</trd>
  </trdgrp>
  <trd lng="en">callous</trd>
  <trd lng="fr">calleux</trd>
  <trdgrp lng="hu">
    <trd>b&uuml;tyk&ouml;s</trd>,
    <trd>k&eacute;rges</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305F;&#x3053;&#x306E;&#x3067;&#x304D;&#x305F;</trd>,
    <trd>&#x3054;&#x3064;&#x3054;&#x3064;&#x3057;&#x305F;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eeltig</trd>,
    <trd>vereelt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zrogowacia&lstroke;y</trd>,
    <trd>stwardnia&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">caloso</trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_o;&c_l;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">mozo&#x13E;nat&#xFD;</trd>
  <trd lng="sv">valkig</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x437;&#x43E;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43C;&#x43E;&#x437;&#x43E;&#x43B;&#x438;&#x441;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x43C;&#x43E;&#x437;&#x43E;&#x43B;&#x44F;&#x441;&#x442;&#x438;&#x439;</trd>
  </trdgrp>
  <trd lng="zh">&#x80DD; <pr>zh&#x12B;</pr></trd>
</drv>
</art>
