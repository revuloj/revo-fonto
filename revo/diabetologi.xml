  <trd lng="be">&c_d;&c_y;&c_ja;&c_b;&c_e;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="en">diabetology</trd>
  <trd lng="fr">diab&eacute;tologie</trd>
</drv>
