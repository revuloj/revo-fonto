<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>trigonometri</rad>/o
</kap>
<drv mrk="trigon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="matema.0o">matematiko</ref>,
      kiu studas la rilatojn inter anguloj kaj lateroj de triangulo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Trigonometrie</trd>,
    <trd>Dreiecksmessung</trd>
  </trdgrp>
  <trd lng="en">trigonometry</trd>
  <trd lng="fr">trigonom&eacute;trie</trd>
  <trdgrp lng="nl">
    <trd>trigonometrie</trd>,
    <trd>driehoeksmeting</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_g;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="trigon.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="trigon.0a.gxenerale">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a egala&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="trigon.0a.cirklo">
    <dif>
      <klr>(p.p. <ref cel="cirkl.0o.linio">cirklo</ref>)</klr>
      Tia, ke &gcirc;ia centro koincidas kun la origino de la
      kartezia <ref cel="koordi.0sistemo">koordinatsistemo</ref>
      kaj ke &gcirc;ia radiuso egalas unu.
    </dif>
  </snc>
  <snc mrk="trigon.0a.funkcio">
    <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="funkci.0o.MAT">funkcio</ref>)</klr>
      Apartenanta al la sekva vico:
      <refgrp tip="vid">
        <ref cel="sinus.0o.funkcio">sinuso</ref>,
        <ref cel="kosinu.0o.funkcio">kosinuso</ref>,
        <ref cel="tangen.0o.funkcio">tangento</ref>,
        <ref cel="kotang.0o.funkcio">kotangento</ref>,
        <ref cel="sekant.0o.funkcio">sekanto</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="de">trigonometrisch</trd>
  <trd lng="en">trigonometric</trd>
  <trd lng="fr">trigonom&eacute;trique</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_g;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
