<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sterk</rad>/o
</kap>
<drv mrk="sterk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sterk.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Gcirc;enerala nomo de &ccirc;iuj produkta&jcirc;oj, organikaj a&ubreve; mineralaj, uzataj
      por plibonigi la kulturgrundojn:
      <ekz>
        kiam la akvoj malaperas, <klr>...</klr> restas fruktoporta &scirc;limo, kiu anstata&ubreve;as        plej bonajn <tld/>ojn
        <fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_jo;&c_t;</trd>,
    <trd>&c_u;&c_g;&c_n;&c_a;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">D&uuml;nger</trd>
  <trd lng="en">manure</trd>
  <trdgrp lng="es">
    <trd>abono</trd>,
    <trd>fertilizante</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>engrais</trd>,
    <trd>fumier</trd>
  </trdgrp>
  <trd lng="hu">tr&aacute;gya</trd>
  <trd lng="nl">mest</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_o;&c_b;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_z;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="sterk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sterk.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Plibonigi grundon per <tld/>o:
      <ekz>
        fosante ni trovis mar&ccirc;grundon <tld/>itan de post jarmiloj per
        putri&gcirc;intaj herboj kaj folioj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_g;&c_n;&c_a;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_n;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">d&uuml;ngen</trd>
  <trdgrp lng="fr">
    <trd>mettre de l'<ind>engrais</ind></trd>,
    <trd>fumer<klr> (un terrain)</klr></trd>
  </trdgrp>
  <trd lng="hu">tr&aacute;gy&aacute;z</trd>
  <trd lng="nl">bemesten</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_n;&c_a;&c_v;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
