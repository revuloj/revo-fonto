<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>tiroid</rad>/o
</kap>
<drv mrk="tiroid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Hormona glando &ccirc;e vertebruloj. &Ccirc;e mamuloj &gcirc;i trovi&gcirc;as je la kolo
      sub la laringo. La greka nomo devenas de &gcirc;ia &scirc;ilda formo.
      &Gcirc;i konservas jodon kaj produktas jodhajvan hormonojn, kiuj regulas
      la metabolisman rapidon de kreskado.
    </dif>
  </snc>
  <trd lng="af">skildklier</trd>
  <trd lng="ar">&a_gh;&a_d;&a_t_marbuta; &a_d;&a_r;&a_q;&a_y;&a_t_marbuta;</trd>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_t;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_o;&c_z;&c_a;</trd>
  <trd lng="bg">&c_shch;&c_i;&c_t;&c_o;&c_v;&c_i;&c_d;&c_n;&c_a; &c_zh;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="ca">tiroide</trd>
  <trd lng="cs">&scaron;t&iacute;tn&aacute; &zcaron;l&aacute;za</trd>
  <trd lng="de">Schilddr&uuml;se</trd>
  <trd lng="el">&theta;&ypsilon;&rho;&epsilon;&omikron;&epsilon;&jota;&delta;&eta_ton;&sigma_fina; &alfa;&delta;&epsilon_ton;&ny;&alfa;&sigma_fina;</trd>
  <trd lng="en">thyroid</trd>
  <trd lng="es">gl&aacute;ndula tiroides</trd>
  <trd lng="fa">&a_gh;&a_d;&a_h; &a_t;&f_y;&a_r;&a_w;&a_y_hamza;&f_y;&a_d;</trd>
  <trd lng="fr">thyro&iuml;de</trd>
  <trd lng="hu">pajzsmirigy</trd>
  <trd lng="it">tiroide</trd>
  <trdgrp lng="nl">
    <trd>schildklier</trd>,
    <trd>thyro&iuml;de</trd>
  </trdgrp>
  <trd lng="pl">tarczyca</trd>
  <trd lng="pt">tiroide</trd>
  <trd lng="ru">&c_shch;&c_i;&c_t;&c_o;&c_v;&c_i;&c_d;&c_n;&c_a;&c_ja; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
</drv>
</art>

