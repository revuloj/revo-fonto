<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>paletr</rad>/o</kap>
<drv mrk="paletr.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <uzo tip="fak">BELA</uzo>
    <snc>
      <dif>
        Maldika tabuleto, sur kiu la pentristoj orde pretigas kaj miksas la
        kolorilojn.
      </dif>
    </snc>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_r;&c_a;</trd>
    <trd lng="de">Palette</trd>
    <trd lng="hu">paletta</trd>
    <trd lng="nl">palet</trd>
    <trd lng="fr">palette<klr tip="ind"> (de peintre)</klr></trd>
    <trd lng="ru">&c_p;&c_a;&c_l;&c_i;&c_t;&c_r;&c_a;</trd>
</drv>
</art>
