</kap>
<drv mrk="bru.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bru.0i.soni">
    <dif>
      A&ubreve;digi konfuzajn sonojn sen harmonia eco:
      <ekz>
        la vento <tld/>is &ccirc;irka&ubreve; liaj oreloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la arbarpintoj <klr>[&#8230;]</klr> &ccirc;iuj bran&ccirc;oj kaj folioj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>is forta pluvego
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>is flugilo de cigno
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        infanoj kuris tumulte, ridis kaj <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        bala muziko <tld/>is tra la festaj salonoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la la&ubreve;tega muziko <ind>super<tld/>i</ind>s lian krion
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
        <trd lng="de">&uuml;bert&ouml;nen</trd>
      </ekz>
      <ekz>
        la stanaj soldatoj frapo<tld/>is en sia skatolo
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en lia koro kaj kapo &ccirc;io <tld/>is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;uldo ne <tld/>as, tamen dormon detruas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="silent.0i.malbrui">silenti</ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="cxirp.0i">&ccirc;irpi</ref>,
      <ref cel="klak.0i.KOMUNE">klaki</ref>,
      <ref cel="knal.0i">knali</ref>,
      <ref cel="krak.0i">kraki</ref>,
      <ref cel="mugx.0i">mu&gcirc;i</ref>,
      <ref cel="sibl.0i">sibli</ref>,
      <ref cel="tint.0i.SON">tinti</ref>,
      <ref cel="tondr.0i">tondri</ref>,
      <ref cel="tumult.0i">tumulti</ref>,
      <ref cel="zum.0i">zumi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>l&auml;rmen</trd>,
      <trd>tosen</trd>
    </trdgrp>
    <trd lng="es">hacer ruido</trd>
    <trdgrp lng="fr">
      <trd>faire du bruit</trd>,
      <trd>bruire</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>berisik</trd>,
      <trd>bising</trd>,
      <trd>ingar</trd>,
      <trd>ramai</trd>
    </trdgrp>
    <trd lng="pl">ha&lstroke;asowa&cacute;</trd>
    <trd lng="ro">face zgomot</trd>
    <trdgrp lng="zh">
    <trd>&#x54D7;&#x54D7; <pr>hu&amacron;hu&amacron;</pr></trd>,
    <trd>&#x55A7;&#x95F9; <pr>xu&amacron;nn&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="bru.0i.krii">
    <dif>
      <tld lit="B"/>e krii pro <ref cel="koler.0i">kolero</ref>, <ref
      cel="protes.0i">protesto</ref>:
      <ekz>
        malpiuloj <klr>[&#8230;]</klr> <tld/>is, &jcirc;etis koton sur la murojn de l' temploj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel vi ne hontas, granda&gcirc;a viro, tiel <tld/>i pro kelkaj mankantaj butonoj
        <fnt>
          <bib>FK</bib>
        </fnt>!
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><ind>L&auml;rm</ind> machen</trd>,
    </trdgrp>
    <trdgrp lng="es">
      <trd>protestar</trd>,
      <trd>montar un <ind>esc&aacute;ndalo</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>gueuler <klr tip="ind">(protester)</klr></trd>,
      <trd>faire du <ind>foin</ind> <klr tip="ind">(protester)</klr></trd>,
      <trd>faire du <ind>scandale</ind></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>gaduh</trd>,
      <trd>ribut</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>awanturowa&cacute; si&eogonek;</trd>,
      <trd>szumie&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>certa</trd>,
      <trd>zgomot</trd>
    </trdgrp>
    <trd lng="zh">&#22823;&#21561;&#22823;&#x64C2; <pr>d&#xE0;chu&#x12B;d&#xE0;l&#xE9;i</pr></trd>
</snc>
  <snc mrk="bru.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klopodi por atentigi la publikon:
      <ekz>
        ne &ccirc;iu, kiu la&ubreve;te <tld/>as pri esperanto, estas vera spertulo pri &gcirc;i
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        se e&ccirc; la tuta mondo <tld/>os pri nia afero
        <fnt>
          <aut>L. L. Zamenhof</aut>,
          <vrk><url ref="http://www.gazetoteko.com/hef/bol017.pdf">&Ccirc;u
          ni progresas?</url></vrk>,
          <lok>La esperantiso, 1892</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fanfar1.0i">fanfaroni</ref>,
       <ref cel="klacx.0i">kla&ccirc;i</ref>,
       <ref cel="preten.0i.parole">pretendi<sncref/></ref>,
       <ref cel="reklam.0i">reklami</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd><ind>Aufsehen</ind> erregen</trd>,
      <trd>viel <ind>Aufheben</ind> verursachen</trd>,
      <trd><ind>Aufmerksamkeit</ind> auf sich ziehen</trd>,
      <trd>einen <ind>Rummel</ind> veranstalten</trd>
    </trdgrp>
    <trd lng="es">llamar la atenci&oacute;n (de)</trd>
    <trd lng="fr">faire grand <ind>bruit</ind> (de)</trd>
    <trdgrp lng="pl">
      <trd>robi&cacute; wiele <ind>ha&lstroke;asu</ind><klr> (o
      co&sacute;)</klr></trd>
      <trd>robi&cacute; wiele <ind>szumu</ind><klr> (dooko&lstroke;a
      czego&sacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>face mult  <ind>zgomot</ind>  despre ceva</trd>,
      <trd>faci mult  <ind>zgomot</ind>  &icirc;n jurul unui lucru</trd>
    </trdgrp>
    <trd lng="zh">&#x62DB;&#25622; <pr>zh&amacron;oy&#xE1;o</pr></trd>
</snc>
  <trd lng="be">&c_sh;&c_u;&c_m;&c_je;&c_c;&c_mol;</trd>
  <trd lng="br">ober <ind>trouz</ind></trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;lat povyk</trd>,
    <trd>hlu&ccaron;et</trd>,
    <trd>lomozit</trd>
  </trdgrp>
  <trd lng="en">noisy</trd>
  <trd lng="he">&lamed;&he;&resh;&ayin;&yod;&shin;</trd>
  <trdgrp lng="hu">
    <trd>z&uacute;g</trd>,
    <trd>zajong</trd>,
    <trd>l&aacute;rm&aacute;zik</trd>,
    <trd>zajt csap<klr tip="amb"> (&aacute;tv. is.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9A12;&#x97F3;&#12434;&#x51FA;&#12377;  <pr>&#x305D;&#12358;&#x304A;&#12435;&#12434;&#12384;&#12377;</pr></trd>,
    <trd>&#x9A12;&#x304E;&#x7ACB;&#12390;&#x308B;  <pr>&#12373;&#x308F;&#x304E;&#x305F;&#12390;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pt">fazer barulho</trd>
  <trd lng="ru">&c_sh;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>robi&tcaron; hluk</trd>,
    <trd>vr&ccaron;a&tcaron; <klr>(motor)</klr></trd>
