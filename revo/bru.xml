<drv mrk="bru.0a">
  <kap><tld/>a</kap>
  <snc mrk="bru.0a.lauxta">
    <dif>
      <tld lit="B"/>anta, <ref tip="super" cel="lauxt.0a.MUZ">la&ubreve;tega</ref>:
      <ekz>
        <tld/>a urbo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 32:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;as <tld/>a vo&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 66:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a interparolado
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eka&ubreve;dis <tld/>an tonon, kiu povis ektimigi la koron e&ccirc; de plej kura&gcirc;a viro
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de la <tld/>a galopado <klr>[&#8230;]</klr> li turnis la vipon en sia mano
        <fnt>
          <bib>Fab3</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>l&auml;rmig</trd>, <trd>laut</trd>
    </trdgrp>
    <trd lng="es">ruidoso</trd>
    <trdgrp lng="hu">
      <trd>zajos</trd>,
      <trd>l&aacute;rm&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>berisik</trd>,
      <trd>bising</trd>,
      <trd>ingar</trd>,
      <trd>ramai</trd>
    </trdgrp>
    <trd lng="pl">ha&lstroke;a&sacute;liwy</trd>
    <trd lng="ro">zgomotos</trd>
    <trd lng="zh">&#x566A;&#26434; <pr>z&#xE0;oz&#xE1;</pr></trd>
</snc>
  <snc mrk="bru.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekscitiga, tumulta, altiranta la atenton:
      <ekz>
        &scirc;i elpa&scirc;is en la <tld/>an, tumultan, strangan mondon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>a vivo de l' homoj oni trovas tiom da sa&gcirc;o kiom da malsa&gcirc;o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i, manke de pruvo, ne povis tion publikigi, <klr>[&#8230;]</klr> time ke oni lan&ccirc;us
        kontra&ubreve; &scirc;i la tutan justican aparaton en <tld/>a proceso pri misfamigo
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>a vento subita silento
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kapt.atento0a">atentokapta</ref>,
       <ref cel="ekstra1.0a">ekstravaganca</ref>
    </refgrp>
    <trd lng="de">aufsehenerregend</trd>
    <trd lng="es">llamativo</trd>
    <trdgrp lng="id">
      <trd>gaduh</trd>,
      <trd>ribut</trd>
    </trdgrp>
    <trd lng="pl">szumny</trd>
    <trdgrp lng="zh">
    <trd>&#x5F15;&#x8D77;&#x8F5F;&#x52D5; <pr>y&#x1D0;nq&#x1D0;h&#x14D;ngd&#xF2;ng</pr></trd>,
    <trd>&#32824;&#x4EBA;&#x542C;&#x95FB; <pr>s&#x1D2;ngr&#xE9;nt&#x12B;ngw&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_u;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_u;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
