    <trd>&c_sh;&c_u;&c_m;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="bru.0o">
  <kap><tld/>o</kap>
  <snc mrk="bru.0o.sonaro">
    <dif>
      <ref tip="super" cel="son.0o.SON">Sono</ref> a&ubreve;
      sonoj konfuzaj, sen difinita alteco:
      <ekz>
        <tld/>o de tendaro
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        Josuo a&ubreve;dis la <tld/>on de la popolo &gcirc;ojkrianta
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 32:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        dancado, muziko kaj <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ondo da <tld/>o, konsistanta el homaj vo&ccirc;oj, tu&scirc;is &scirc;iajn orelojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kaldronoj de la &scirc;ipo kun mu&gcirc;o kaj <tld/>o ellasas vaporon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro la resti&gcirc;o senaga: soifis li <tld/>on, batalon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>o oni malfermis la pordegon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 38</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ebrii&gcirc;is kaj faris tian <tld/>on, ke la policisto admonis la
        pordiston
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        klak<tld/>o de la hakiloj
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">L&auml;rm</trd>
    <trd lng="es">ruido</trd>
    <trdgrp lng="hu">
      <trd>zaj</trd>,
      <trd>l&aacute;rma</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>bising</baz>kebisingan</trd>,
      <trd><baz>ramai</baz>keramaian</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ha&lstroke;as</trd>,
      <trd>szum</trd>
    </trdgrp>
    <trd lng="ro">zgomot</trd>
    <trd lng="ru">&c_sh;&c_u;&c_m;</trd>
  </snc>
  <snc mrk="bru.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="ekscit.0igxo">Eksciti&gcirc;o</ref>, <ref cel="tumult.0o">tumulto</ref>:
      <ekz>
        la afero jam faris multon da <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        li vivas en &gcirc;uo kaj <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o potenca, nula esenco
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
        pli da <tld/>o <klr>(fanfaronado)</klr> ol da
        faro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frakas.0o.FIG">frakaso</ref>,
      <ref cel="rumor.0o">rumoro</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Aufsehen</trd>,
      <trd>Aufhebens</trd>,
      <trd>Rummel <klr>(Aufsehen)</klr></trd>,
      <trd>Unruhe</trd>
    </trdgrp>
    <trd lng="es">alboroto</trd>
    <trd lng="hu">zaj</trd>
    <trdgrp lng="id">
      <trd><baz>gaduh</baz>kegaduhan</trd>,
      <trd><baz>ribut</baz>keributan</trd>
    </trdgrp>
    <trd lng="pl">szum</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_u;&c_m;</trd>,
      <trd>&c_sh;&c_u;&c_m;&c_i;&c_h;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="af">geraas</trd>
  <trd lng="am">&#x132B;&#x132B;&#4723;</trd>
