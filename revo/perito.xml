   <ofc>*</ofc>
   <rad>peritone</rad>/o
</kap>
<drv mrk="perito.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="membra.0o.ANA">Membrano</ref>
      entenanta la intestojn:
      <ekz>
        apendicito <klr>[&#x2026;]</klr> povas ka&ubreve;zi la dan&gcirc;eran inflamon de la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17276">Apendicito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">peritoneum</trd>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_je;&c_m;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
     <trd>rouedenn ar bouzello&ugrave;</trd>,
     <trd>rouedenn-gof</trd>
   </trdgrp>
  <trd lng="ca">peritoneu</trd>
  <trdgrp lng="cs">
    <trd>peritoneum</trd>,
    <trd>pob&rcaron;i&scaron;nice</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Bauchfell</trd>,
     <trd>Peritoneum</trd>
   </trdgrp>
  <trd lng="es">peritoneo</trd>
  <trdgrp lng="fa">
     <trd>&a_S;&a_f;&a_A;&a_q;</trd>,
     <trd>&f_p;&a_r;&a_d;&a_h;&a_hamza_sure; &a_S;&a_f;&a_A;&a_q;</trd>
   </trdgrp>
  <trd lng="fr">p&eacute;ritoine</trd>
  <trdgrp lng="hu">
     <trd>hash&aacute;rtya</trd>,
     <trd>peritoneum</trd>
