<vortaro>
<art mrk="$Id: pseuxdopod.xml,v 1.7 2022/06/26 22:28:56 revo Exp $">
<kap>
  <rad>pse&ubreve;dopod</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pseuxdopod.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pseuxdopod.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      <ref tip="dif" cel="pied.sxajn0o.BIO">&Scirc;ajnpiedo</ref>:
      <ekz>
        scienc-fikciaj na&ubreve;zodonaj protoplasmoj,
        kun iaj tiam kaj jene el&scirc;ovataj kaptokapablaj <tld/>oj
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>oj estas malkonstantaj citoplasmaj elkreskoj &ccirc;e unu&ccirc;elaj organismoj kaj &ccirc;e
        iuj &ccirc;eloj de plur&ccirc;elaj viva&jcirc;oj
        <fnt>
          <vrk><url ref="&Viki;/Pse%C5%ADdopodo">&Scirc;ajnpiedo - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">pseudopode</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Pseudopodium</trd>,
    <trd>Scheinf&uuml;&szlig;chen</trd>
  </trdgrp>
  <trd lng="pl">nibyn&oacute;&zdot;ki {pl}</trd>
  <trd lng="ro">pseudopod</trd>
  <trd lng="uk">&#x43F;&c_s;&c_je;&c_v;&c_d;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&#x44F;</trd>
</drv>
</art>

