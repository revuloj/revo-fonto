<drv mrk="gener.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="gener.0ado.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Ago <tld/>i<sncref ref="gener.0i.BIO"/>:
      <ekz>
        homa, besta, vegeta <tld/>ado;
      </ekz>
      <ekz>
        spontanea <tld/>ado <klr>(produktado de vivo, okazanta la&ubreve; kelkaj
        scienculoj sen anta&ubreve;a &gcirc;ermo)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>generaci&oacute;</trd>,
      <trd>engendrament</trd>
    </trdgrp>
    <trd lng="fr">g&eacute;n&eacute;ration<klr tip="ind"> (fait d'engendrer)</klr></trd>
    <trd lng="hu">nemz&eacute;s</trd>
    <trd lng="nl">verwekking</trd>
    <trd lng="pl">p&lstroke;odzenie</trd>
    <trd lng="pt">gera&ccedil;&atilde;o<klr tip="amb"> (ato de gerar)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_z;&c_a;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gener.0ado.produktado">
    <dif>
      Ago <tld/>i<sncref ref="gener.0i.produkti"/>:
      <ekz>
        la <tld/>ado kaj akcelado de partikloj (ekz. protonoj)
        per elektronaj rimedoj
        <fnt>
        <bib>Viki</bib>,
        <lok><url ref="eo.wikipedia.org/wiki/Joniga_radiado">Joniga radiado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu tamen bezonas dukolumnan tekston, <klr>[&#x2026;]</klr>
        a&ubreve;tomatan <tld/>adon de indekso ktp, tiu ne trovos
        tiujn funkciojn en la aktuala versio
        <fnt>
        <aut>PEJNO Simono</aut>,
        <vrk><url
        ref="http://ourworld.compuserve.com/homepages/profcon/e_reklam.htm">&Ccirc;apelilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">generaci&oacute; <klr tip="amb">(acci&oacute; de generar)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_n;&c_a;&c_p;&c_r;.
    &c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ib;)</klr></trd>,
    <trd>&c_u;&c_t;&c_v;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;
    <klr>(&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;)</klr></trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b; <klr>(&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>plozen&iacute;</trd>,
    <trd>rozmno&zcaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
