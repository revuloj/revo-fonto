<drv mrk="gener.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gener.re0i.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Rekreskigi perditan organon a&ubreve; ties parton:
      <ekz>
        marstelo re<tld/>is la perditan brakon;
      </ekz>
      <ekz>
        la homo &ccirc;iutage re<tld/>as multmilionojn da &ccirc;eloj
        de la ha&ubreve;to, sango ktp
        <fnt>
        <vrk><url ref="http://www.fw.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm">Enkonduko
        en la biologion</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_r;&c_je;&c_g;&c_je;&c_n;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="gener.re0i.produkti">
    <dif>
      Ree produkti ion foruzitan:
      <ekz>
        oni povas uzi vegeta&jcirc;ojn por re<tld/>i oksigenon
        konsumatan de la homoj;
      </ekz>
      <ekz>
        anta&ubreve;tagmeze &ccirc;iu havos sufi&ccirc;e da tempo por re<tld/>i
        sian sanon dum aktiva ripozado: promenado, biciklado,
        na&gcirc;ado, sunumado
        <fnt>
        <vrk><url ref="http://www.esperanto.koszalin.pl/28cebalta.html">La 28a &Ccirc;ebalta esperantista
        printempo (2006)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_je;&c_g;&c_je;&c_n;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_o;&c_sh;&c_ch;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_n;&c_a;&c_n;&c_o;&c_v;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ca">regenerar</trd>
  <trdgrp lng="cs">
    <trd>obnovit</trd>,
    <trd>obrodit</trd>,
    <trd>regenerovat</trd>
  </trdgrp>
  <trd lng="de">regenerieren</trd>
  <trdgrp lng="ja">
    <trd>&#x518D;&#x751F;&#x3055;&#x305B;&#x308B;  <pr>&#x3055;&#x3044;&#x305B;&#x3044;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5237;&#x65B0;&#x3059;&#x308B;  <pr>&#x3055;&#x3063;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x66F4;&#x751F;&#x3055;&#x305B;&#x308B;  <pr>&#x3053;&#x3046;&#x305B;&#x3044;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obnovi&tcaron;</trd>,
    <trd>regenerova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_gu;&c_je;&c_n;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_n;&c_o;&c_v;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
