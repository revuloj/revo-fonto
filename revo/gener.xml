<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gener</rad>/i
<fnt>Z</fnt></kap>
<drv mrk="gener.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(p.p. virseksulo)</uzo>
    <dif>
      Produkti vivantan samspecan kiel si esta&jcirc;on; naskigi:
      <ekz>
        Abraham <tld/>is Jakobon;
      </ekz>
      <ekz>
        de li devenis la tuta <tld/>o<fnt>Z</fnt> <klr>(idaro)</klr>.
      </ekz>
    </dif>
    <trd lng="en">generate</trd>
    <trd lng="hu">nemz</trd>    
    <trdgrp lng="pl">
      <trd>p&lstroke;odzi&cacute;</trd>,
      <trd>rozmna&zdot;a&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gener.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <ref tip="dif" cel="nask.0i.MAT">naski.</ref>
     <trdgrp lng="de">
      <trd>erzeugen</trd>,
      <trd>bestimmen</trd>,
      <trd>aufspannen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>generate</trd>,
      <trd>span</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>engendrer<klr tip="ind"> (math.)</klr></trd>,
      <trd>g&eacute;n&eacute;rer</trd>,
      <trd>sous-tendre</trd>
    </trdgrp>
    <trd lng="hu">gener&aacute;l</trd>    
    <trd lng="pl">generowa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
 </snc>
  <trd lng="fr">engendrer</trd>
  <trd lng="nl">verwekken<klr tip="ind"> (een kind)</klr></trd>
  <trdgrp lng="pt">
   <trd>gerar</trd>, 
   <trd>procriar</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_p;&c_o;&c_t;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gener.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        homa, besta, vegeta <tld/>ado;
      </ekz>
      <ekz>
        spontanea <tld/>ado <klr>(produktado de vivo, okazanta la&ubreve; kelkaj
        scienculoj sen anta&ubreve;a &gcirc;ermo)</klr>.
      </ekz>
    </dif>
  </snc>
    <trd lng="en">generation</trd>
  <trd lng="fr">g&eacute;n&eacute;ration<klr tip="ind"> (fait d'engendrer)</klr></trd>
  <trd lng="hu">nemz&eacute;s</trd>  
  <trd lng="nl">verwekking</trd>
  <trd lng="pl">p&lstroke;odzenie</trd>
  <trd lng="pt">gera&ccedil;&atilde;o<klr tip="amb"> (ato de gerar)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="gener.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">ANA</uzo>
        <ind><tld/>aj organoj</ind>.
        <trd lng="fr">organes g&eacute;nitaux</trd>
        <trd lng="hu">nemi szervek</trd>
        <trdgrp lng="nl">
          <trd>genitali&euml;n</trd>,
          <trd>geslachtsdelen</trd>
        </trdgrp>
        <trd lng="pl">narz&aogonek;dy p&lstroke;ciowe</trd>
        <trd lng="ru">&c_d;&c_je;&c_t;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_je; &c_o;&c_r;&c_g;&c_a;&c_n;&c_y;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>genital</trd>,
    <trd>generative</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&eacute;n&eacute;sique</trd>,
    <trd>g&eacute;nital</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nemz&odblac;</trd>,
    <trd>nemi</trd>
  </trdgrp>
  <trd lng="nl">genitaal</trd>
  <trd lng="pt">genital</trd>
  <trd lng="pl">p&lstroke;ciowy</trd>
</drv>
</art>
