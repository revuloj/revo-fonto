<drv mrk="edelve.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Leontopodium</trd>), kun blanka laneca floro,
      kreskanta sur kalka grundo en montaro.
