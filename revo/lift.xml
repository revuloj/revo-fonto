<drv mrk="lift.0o">
  <kap><tld/>o</kap>
  <snc mrk="lift.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por suprenlevi personojn a&ubreve; objektojn &gcirc;is
      la 
      diversaj eta&gcirc;oj de domo a&ubreve; konstruo:
      <ekz>
        <nom>Siemens</nom> <klr>[&#x2026;]</klr> konstruis <klr>[&#x2026;]</klr> en 1880 la unuan elektran
        <tld/>on
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007112.html">Virbovo-tirpovo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la geedzoj venis al la <tld/>o, sed rimarkinte, ke &gcirc;i estas okupita
        <klr>[&#x2026;]</klr> ili malsupreniris la &scirc;tuparon perpiede
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2008/009339.php">En granda urbo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabeto en la <tld/>o kun sia ru&gcirc;a &ccirc;apo
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 10a, p. 
        34a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="argan.0o.TEK">argano<sncref/></ref>,
       <ref cel="elevat.0o">elevatoro</ref>,
       <ref cel="gru.0o.TEK">gruo<sncref/></ref>,
       <ref cel="lev.0ilo.aparato">levilo<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_l;&c_ib;&c_f;&c_t;</trd>
    <trd lng="bg">&c_a;&c_s;&c_a;&c_n;&c_s;&c_mol;&c_o;&c_r;</trd>
    <trd lng="br">pignerez</trd>
    <trdgrp lng="cs">
      <trd>lift <klr>(hovor.)</klr></trd>,
      <trd>v&yacute;tah</trd>,
      <trd>zdvi&zcaron;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Aufzug</trd>,
      <trd>Fahrstuhl</trd>
    </trdgrp>
    <trd lng="el">&alfa;&sigma;&epsilon;&ny;&sigma;&epsilon_ton;&rho;</trd>
    <trdgrp lng="en">
      <trd>lift (Brite)</trd>,
      <trd>elevator (Usone)</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>ascensor</trd>,
      <trd>elevador</trd>
    </trdgrp>
    <trd lng="fr">ascenseur</trd>
    <trdgrp lng="hu">
      <trd>lift</trd>,
      <trd>felvon&oacute;</trd>
    </trdgrp>
    <trd lng="nl">lift</trd>
    <trd lng="pl">winda</trd>
    <trdgrp lng="pt">
      <trd>elevador <klr>(subst.)</klr></trd>,
      <trd>ascensor <klr>(subst.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_i;&c_f;&c_t;</trd>
    <trd lng="sk">v&yacute;&tcaron;ah</trd>
    <trd lng="sv">hiss</trd>
  </snc>
  <snc mrk="lift.0o.telfero">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="telfer.0o">Telfero</ref>:
      <ekz>
        somera pluso estas la senpagaj utiligado de <tld/>oj al la montaj alta&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj
          progreso</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        per kablo-tramo kaj <ind>ski<tld/>o</ind>j ili supreniras por profiti la skieblajn deklivojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008395.php">Vizito al la nacia parko
          Vanoise</url></vrk>
          <lok>2005</lok>
        </fnt>;
        <trd lng="de">Skilift</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="funikular.0o">funikularo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_d;&#x2019;&c_jo;&c_m;&c_n;&c_ib;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Lift <klr>(Gebirge)</klr></trd>,
      <trd>Seilbahn</trd>
    </trdgrp>
  </snc>
