<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kojon.xml,v 1.12 2022/03/22 15:44:11 revo Exp $">
<kap>
  <rad>kojon</rad>/o
</kap>
<drv mrk="kojon.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="kojon.0o.testiko">
    <dif>
      <ref tip="dif" cel="testik.0o">Testiko</ref>:
      <ekz>
        vi ne povus e&ccirc; vendi <tld/>ojn al e&ubreve;nuko
        <fnt><aut>Cezaro Rossetti</aut><vrk>Kredu Min, Sinjorino!</vrk>
        <lok>jaro 1990a, &ccirc;apitro 3a, p. 28a</lok></fnt>;
      </ekz>
      <ekz>
        Rikardo, per fusilo sin gratadis &ccirc;e l' kojono
        <fnt>
          <vrk><url ref="http://www.literaturo.org/HARLOW-Don/192.220.96.203/don/esplit/Eskimino.html">La Eskimino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF62;&#xFB3;&#xF72;&#xF42;&#xF0B;&#xF62;&#xF72;&#xF63;&#xF0B;</trd>
  <trd lng="ca">coll&oacute;</trd>
  <trdgrp lng="cs">
    <trd>koule</trd>,
    <trd>varle</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hoden</trd>,
    <trd>Ei <klr>(Hoden)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>testicle</trd>,
    <trd>ball</trd>,
    <trd>nut</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>couille<klr tip="ind"> (testicule)</klr></trd>,
    <trd>burne</trd>
    <trd>coucougnette</trd>
    <trd>valseuse<klr tip="ind"> (testicule)</klr></trd>
  </trdgrp>
  <trd lng="it">coglione<klr> (volg.)</klr></trd>
  <trdgrp lng="nl">
    <trd>kloot</trd>,
    <trd>testikel</trd>
  </trdgrp>
  <trd lng="pl">jajo <klr>(wulgarnie)</klr></trd>
  <trd lng="pt">ovo</trd>
  <trdgrp lng="sk">
    <trd>gule</trd>,
    <trd>semen&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&#x44F;&#x439;&#x446;&#x435;</trd>
</drv>
</art>
