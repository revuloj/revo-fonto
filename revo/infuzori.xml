<vortaro>
<art mrk="$Id$">
<kap>
  <rad>infuzori</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="infuzori.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="infuzori.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      <ref tip="super" cel="mikrovivajx.0o.BIO">Mikroviva&jcirc;o</ref>
      kiu kapablas movi&gcirc;i cilie a&ubreve; alirimede:
      <ekz>
        li volis vidi, kiel diafana <tld/>o glutas verdan polveron
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 23a, p.
        147a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">infusoria</trd>
  <trd lng="fr">infusoire</trd>
</drv>
</art>

