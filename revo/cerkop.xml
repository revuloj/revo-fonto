<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cerkopitek</rad>/o
</kap>
<drv mrk="cerkop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cerkop.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cercopithecus</trd>).
    </dif>
  </snc>
  <trd lng="de">Meerkatze</trd>
  <trd lng="fr">cercopith&egrave;que</trd>
</drv>
<drv mrk="cerkop.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="simi.0oj">simioj</ref>
      (<trd lng="la">Cercopithecidae</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Meerkatzen</trd>,
    <trd>Hundsaffen</trd>
  </trdgrp>
  <trd lng="en">old world <ind>monkeys</ind></trd>
</drv>
</art>

