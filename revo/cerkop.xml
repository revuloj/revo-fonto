    <trd>meerkat</trd>,
    <trd>makako</trd>
  </trdgrp>
  <trd lng="pl">koczkodan</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
</drv>
