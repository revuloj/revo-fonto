<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cerkop.xml,v 1.20 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>cerkopitek</rad>/o
</kap>
<drv mrk="cerkop.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Mezgranda <ref tip="lst" cel="simi.0o" lst="voko:simioj">simio</ref>,
      kun longaj kruroj kaj vosto, diverskolora felo kaj hela
      viza&gcirc;desegno, vivanta en Afriko sude de Saharo sur arboj nutranta sin precipe de
      fruktoj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="cerkop1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cercopithecus</trd>).
      <ekz>
        la bonobo estas endem<klr>[i]</klr>a de la humidaj arbaroj de la regiono, same
        kiel aliaj simbolaj specioj kiaj la nigraverda <tld/>o, la
        drjasa <tld/>o, la fi&scirc;oman&gcirc;a genoto, la okapio kaj la konga pavo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22806">Kongo (rivero)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/72/Blue_monkey_%28Cercopithecus_mitis_stuhlmanni%29_pair.jpg/640px-Blue_monkey_%28Cercopithecus_mitis_stuhlmanni%29_pair.jpg" prm="CC BY-SA 4.0"><tld/>oj (Cercopithecus mitis stuhlmanni), Arbaro Kakamega, Kenio
        <fnt>
          <aut>Charles J Sharp, propra verko, from Sharp Photography, sharpphotography</aut>
          <lok><url ref="&WCW;/File:Blue_monkey_(Cercopithecus_mitis_stuhlmanni)_pair.jpg">&WCW;/File:Blue_monkey_(Cercopithecus_mitis_stuhlmanni)_pair.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;</trd>
    <trd lng="ca">cercopitec</trd>
    <trd lng="de">Meerkatze</trd>
    <trdgrp lng="en">
      <trd>Cercopithecus</trd>,
      <trd>guenon</trd>
    </trdgrp>
    <trd lng="fr">cercopith&egrave;que</trd>
    <trd lng="hu">cerk&oacute;f</trd>
    <trd lng="it">cercopiteco</trd>
    <trdgrp lng="nl">
      <trd>meerkat</trd>,
      <trd>makako</trd>
    </trdgrp>
    <trd lng="pl">koczkodan</trd>
    <trd lng="ru">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="he">
    <trd>&#x5D2;&#x5D5;&#x5E0;&#x5D5;&#x5DF;</trd>,
    <trd>&#x5E1;&#x5D5;&#x5D2; &#x5E7;&#x5D5;&#x5E3;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_r;&c_t;&c_i;&c_sh;&#x43A;&c_a;</trd>,
    <trd>&#x43C;&c_a;&c_v;&#x43F;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x732B; <pr>h&#x1CE;im&#x101;o</pr></trd>,
    <trd>&#x9577;&#x5C3E;&#x7334;&#x5C6C; <pr>ch&#xE1;ngw&#x11B;ih&#xF3;ush&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
