<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pred.xml,v 1.13 2024/02/17 07:52:35 revo Exp $">
<kap>
  <rad>pred</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pred.0o">
  <kap><tld/>o</kap>
  <snc mrk="pred.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="cxas.0ajxo.CXAS">&Ccirc;asa&jcirc;o</ref>:
      <ekz>
        &gcirc;i observas sian <tld/>on, kaj subite malsupren&scirc;vebas rapide
        al &gcirc;i
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;turfalko&rightquot;,
        2006-07-03</lok></fnt>;
      </ekz>
      <ekz>
        piratoj ser&ccirc;as <tld/>ojn surmare kaj surborde
        <fnt><aut>Baldur Ragnarsson</aut>, <vrk><url ref="http://www.esperanto.net/literaturo/uea/arnecefidrec.html">Rakonto
        kun humanisma mesa&gcirc;o</url></vrk>,
        <bib>Esperanto</bib>, <lok>jaro 2006a</lok></fnt>;
      </ekz>
      <ekz>
        tiuj du specioj estas la &ccirc;efaj <tld/>oj de la Komodo-varanoj
        <fnt>
          <aut>Wim de Smet</aut>
          <vrk><url ref="&Monato;/2000/006900.php">La &leftquot;drakoj&#x201D; de Komodo:
          Gigantaj bestoj sur izola insulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hungara forinto estis facila <tld/>o por la internacia
        fikapitalo
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2003/008056.html">Atako kontra&ubreve; la forinto</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_ch;&c_y;&c_n;&c_a; <klr>((&c_n;&c_a; &c_ja;&c_k;&c_u;&c_ju; &c_p;&c_a;&c_l;&c_ju;&c_ju;&c_c;&c_mol;))</klr></trd>
  <trd lng="cs">ko&rcaron;ist</trd>
  <trd lng="de">Beute</trd>
  <trd lng="en">prey</trd>  
  <trd lng="es">presa <klr>(de caza)</klr></trd>
  <trd lng="fr">proie</trd>
  <trd lng="nl">prooi</trd>
  <trd lng="pt">presa<klr> (ca&ccedil;a)</klr></trd>
  <trd lng="sk">koris&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_d;&#x43E;&c_b;&c_i;&c_ch;</trd>,
    <trd>&c_t;&c_r;&#x43E;&c_f;&c_je;&c_j;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x730E;&#x7269; <pr>li&#xE8;w&#xF9;</pr></trd>,
    <trd>&#x7375;&#x7269; <pr>li&#xE8;w&#xF9;</pr></trd>,
    <trd>&#x6355;&#x7372;&#x7269; <pr>b&#x1D4;hu&#xF2;w&#xF9;</pr></trd>,
    <trd>&#x6355;&#x83B7;&#x7269; <pr>b&#x1D4;hu&#xF2;w&#xF9;</pr></trd>,
    <trd>&#x63A0;&#x593A;&#x54C1; <pr>l&#xFC;&#xE8;du&#xF3;p&#x1D0;n</pr></trd>,
    <trd>&#x63A0;&#x596A;&#x54C1; <pr>l&#xFC;&#xE8;du&#xF3;p&#x1D0;n</pr></trd>,
    <trd>&#x6728;&#x6876; <pr>m&#xF9;t&#x1D2;ng</pr></trd>,
    <trd>&#x727A;&#x7272;&#x54C1; <pr>x&#x12B;sh&#x113;ngp&#x1D0;n</pr></trd>,
    <trd>&#x72A7;&#x7272;&#x54C1; <pr>x&#x12B;sh&#x113;ngp&#x1D0;n</pr></trd>,
    <trd>&#x990C;&#x98DF; <pr>&#x11B;rsh&#xED;</pr></trd>,
    <trd>&#x9975;&#x98DF; <pr>&#x11B;rsh&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pred.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pred.0i.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="akir.cxas0i.CXAS">&Ccirc;asakiri</ref>:
      <ekz>
        ridkokaburo <klr>[&#x2026;]</klr> <tld/>as anka&ubreve; reptiliojn, e&ccirc;
        serpentojn
        <fnt>
          <vrk><url ref="">Best&gcirc;ardeno veszpr&eacute;m n.o.</url></vrk>,
          <lok>2006-04-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        afidoj adapti&gcirc;as pli rapide ol iliaj naturaj <tld/>antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Joyce Bunting</aut>
          <vrk><url ref="&Monato;/2013/010846.php">Taktiko por noveca tritiko</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        mamutino, &ccirc;irka&ubreve; 50- a&ubreve; 60-jara&gcirc;a, parte man&gcirc;ita de <tld/>antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011214.php">2013: dek donacoj al la posteuloj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lovit</trd>,
    <trd>ulovit</trd>
  </trdgrp>
  <trd lng="de">erbeuten</trd>
  <trd lng="en">prey on</trd>
  <trd lng="fr">faire sa <ind>proie</ind> de</trd>
  <trd lng="nl">tot <ind>prooi</ind> nemen</trd>
  <trd lng="pt">predar</trd>
  <trd lng="sk">lovi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&#x43F;&c_ib;&c_j;&#x43C;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_h;&#x43E;&#x43F;&c_i;&c_t;&c_i; (&#x44F;&#x43A; &c_z;&c_d;&#x43E;&c_b;&c_i;&c_ch;)</trd>,
    <trd>&c_z;&c_d;&#x43E;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; &c_t;&c_r;&#x43E;&c_f;&c_je;&c_ji;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; (&c_s;&c_v;&#x43E;&c_jeu;&#x44E;) &c_zh;&c_je;&c_r;&c_t;&c_v;&#x43E;&#x44E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62A2;&#x6765; <pr>qi&#x1CE;ngl&#xE1;i</pr></trd>,
    <trd>&#x6436;&#x4F86; <pr>qi&#x1CE;ngl&#xE1;i</pr></trd>,
    <trd>&#x6355;&#x98DF; <pr>b&#x1D4;sh&#xED;</pr></trd>,
    <trd>&#x63A0;&#x98DF; <pr>lu:&#xE8;sh&#xED;</pr></trd>,
    <trd>&#x6355; <pr>b&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>

