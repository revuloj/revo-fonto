<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pred.xml,v 1.8 2020/03/22 23:10:48 revo Exp $">
<kap>
  <rad>pred</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pred.0o">
  <kap><tld/>o</kap>
  <snc mrk="pred.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="cxas.0ajxo.CXAS">&Ccirc;asa&jcirc;o</ref>:
      <ekz>
        &gcirc;i observas sian <tld/>on, kaj subite malsupren&scirc;vebas rapide
        al &gcirc;i
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;turfalko&rightquot;,
        2006-07-03</lok></fnt>;
      </ekz>
      <ekz>
        piratoj ser&ccirc;as <tld/>ojn surmare kaj surborde
        <fnt><aut>Baldur Ragnarsson</aut>, <vrk><url
        ref="http://www.esperanto.net/literaturo/uea/arnecefidrec.html">Rakonto
        kun humanisma mesa&gcirc;o</url></vrk>,
        <bib>Esperanto</bib>, <lok>jaro 2006a</lok></fnt>;
      </ekz>
      <ekz>
        tiuj du specioj estas la &ccirc;efaj <tld/>oj de la Komodo-varanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Izabel C. O. Santiago</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006900.html"
              >Fa&ubreve;no: La &leftquot;drakoj&rightquot; de Komodo: Gigantaj bestoj sur izola
              insulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la hungara forinto estis facila <tld/>o por la internacia
        fikapitalo
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="https://www.monato.be/2003/008056.html"
              >Atako kontra&ubreve; la forinto</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">ko&rcaron;ist</trd>
  <trd lng="de">Beute</trd>
  <trd lng="en">prey</trd>  
  <trd lng="es">presa <klr>(de caza)</klr></trd>
  <trd lng="fr">proie</trd>
  <trd lng="nl">prooi</trd>
  <trd lng="pt">presa<klr> (ca&ccedil;a)</klr></trd>
  <trd lng="sk">koris&tcaron;</trd>
</drv>

<drv mrk="pred.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pred.0i.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="akir.cxas0i.CXAS">&Ccirc;asakiri</ref>:
      <ekz>
        ridkokaburo <klr>[&#8230;]</klr> <tld/>as anka&ubreve; reptiliojn, e&ccirc;
        serpentojn
        <fnt>
          <vrk><url ref="">Best&gcirc;ardeno veszpr&eacute;m n.o.</url></vrk>,
          <lok>2006-04-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        afidoj adapti&gcirc;as pli rapide ol iliaj naturaj <tld/>antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Joyce Bunting</aut>
          <vrk><url ref="https://www.monato.be/2013/010846.php"
              >Taktiko por noveca tritiko</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        mamutino, &ccirc;irka&ubreve; 50- a&ubreve; 60-jara&gcirc;a, parte man&gcirc;ita de <tld/>antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011214.php"
              >2013: dek donacoj al la posteuloj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lovit</trd>,
    <trd>ulovit</trd>
  </trdgrp>
  <trd lng="de">erbeuten</trd>
  <trd lng="en">prey on</trd>
  <trd lng="fr">faire sa <ind>proie</ind> de</trd>
  <trd lng="nl">tot <ind>prooi</ind> nemen</trd>
  <trd lng="pt">predar</trd>
  <trd lng="sk">lovi&tcaron;</trd>
</drv>
</art>

