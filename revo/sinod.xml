<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinod.xml,v 1.15 2019/06/30 18:21:49 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sinod</rad>/o
</kap>
<drv mrk="sinod.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="ven.kun0o">Kunveno</ref>
      de pastroj por konsili&gcirc;i kaj decidi pri ekleziaj aferoj:
      <ekz>
        okazis en novembro 2014 <tld/>o de la evangelia eklezio
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011438.php"
              >Anka&ubreve; per armiloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>shrom&aacute;&zcaron;d&ecaron;n&iacute;</trd>,
    <trd>sn&ecaron;m</trd>,
    <trd>synod</trd>,
    <trd>synoda</trd>
  </trdgrp>
  <trd lng="de">Synode</trd>
  <trd lng="en">synod</trd>
  <trd lng="fr">synode</trd>
  <trdgrp lng="hu">
    <trd>zsinat</trd>,
    <trd>szin&oacute;dus</trd></trdgrp>
  <trd lng="nl">synode</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>cirkevn&yacute; snem</trd>,
    <trd>synoda</trd>
  </trdgrp>
</drv>
</art>
