<kap>
  <ofc>1</ofc>
  <rad>sinod</rad>/o
</kap>
<drv mrk="sinod.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="ven.kun0o">Kunveno</ref>
      de pastroj por konsili&gcirc;i kaj decidi pri ekleziaj aferoj:
      <ekz>
        okazis en novembro 2014 <tld/>o de la evangelia eklezio
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011438.php">Anka&ubreve; per armiloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>shrom&aacute;&zcaron;d&ecaron;n&iacute;</trd>,
    <trd>sn&ecaron;m</trd>,
    <trd>synod</trd>,
    <trd>synoda</trd>
  </trdgrp>
  <trd lng="de">Synode</trd>
  <trd lng="en">synod</trd>
  <trd lng="fr">synode</trd>
  <trdgrp lng="hu">
    <trd>zsinat</trd>,
    <trd>szin&oacute;dus</trd></trdgrp>
  <trd lng="ja">&#x6559;&#x4F1A;&#x4F1A;&#x8B70;  <pr>&#x304D;&#x3087;&#x3046;&#x304B;&#x3044;&#x304B;&#x3044;&#x304E;</pr></trd>
  <trd lng="nl">synode</trd>
  <trd lng="pl">synod</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>cirkevn&yacute; snem</trd>,
    <trd>synoda</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_i;&#x43D;&#x43E;&c_d;</trd>
</drv>
