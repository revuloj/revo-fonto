<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kaprifoli</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="kaprif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="lonice.0o">lonicero</ref>
      (<trd lng="la">Lonicera caprifolium</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Wohlriechendes Gei&szlig;blatt</trd>,
    <trd>Jel&auml;ngerjelieber</trd>
  </trdgrp>
</drv>
<drv mrk="kaprif.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Caprifoliaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Gei&szlig;blattgew&auml;chse</trd>
</drv>
</art>
