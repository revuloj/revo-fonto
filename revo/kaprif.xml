<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kaprifoli</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="kaprif.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="lonice.0o">lonicero</ref>
      (<trd lng="la">Lonicera kaprifolium</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gei&szlig;blatt</trd>,
    <trd>Jel&auml;ngerjelieber</trd>
  </trdgrp>
</drv>
<drv mrk="kaprif.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Caprifoliaceae</trd>).
    </dif>
  </snc>
</drv>
</art>
