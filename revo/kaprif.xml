<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaprif.xml,v 1.24 2023/09/01 13:58:55 revo Exp $">
<kap>
  <rad>kaprifoli</rad>/o
  <fnt><bib>Fab2</bib></fnt>
</kap>
<drv mrk="kaprif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="lonice.0o">lonicero</ref>
      (<trd lng="la">Lonicera caprifolium</trd>):
      <ekz>
        en la alia fino de la &gcirc;ardeno staris, kiel li sciis, la&ubreve;bo el
        belaj arbeta&jcirc;oj de
        <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam penetris al &gcirc;i la odoro de la asperulo kaj balda&ubreve; ankora&ubreve;
        pli forte la
        odoro de la <tld/>oj kaj de la violoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/00/Lonicera_caprifolium_RF.jpg/320px-Lonicera_caprifolium_RF.jpg" prm="CC BY 3.0">floroj de <tld/>o (Lonicera caprifolium), Kernberge &ccirc;e Jena,
      Germanujo
        <fnt>
          <aut>Robert Flogaus-Faust, propra verko</aut>
          <lok><url ref="&WCW;/File:Lonicera_caprifolium_RF.jpg">&WCW;/File:Lonicera_caprifolium_RF.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/f/f3/Lonicera_caprifolium_1672.jpg/640px-Lonicera_caprifolium_1672.jpg" prm="CC BY-SA 4.0">folioj kaj fruktoj de <tld/>o (Lonicera caprifolium)
        <fnt>
          <aut>Buendia22, propra verko</aut>
          <lok><url ref="&WCW;/File:Lonicera_caprifolium_1672.jpg">&WCW;/File:Lonicera_caprifolium_1672.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_p;&c_r;&c_y;&c_f;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_zh;&c_m;&c_je;&c_l;&c_mol; &c_p;&c_a;&c_h;&c_u;&c_ch;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_zh;&c_m;&c_je;&c_l;&c_mol; &c_d;&c_u;&c_h;&c_m;&c_ja;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">lligabosc de jard&iacute;</trd>
  <trd lng="cs">zimolez koz&iacute; list</trd>
  <trdgrp lng="de">
    <trd>Wohlriechendes Gei&szlig;blatt</trd>,
    <trd>Jel&auml;ngerjelieber</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&f_y;&f_ch; &a_A;&a_m;&f_y;&a_n;&#x200C;&a_A;&a_l;&a_d;&a_w;&a_l;&a_h;</trd>
  <trd lng="fr">ch&egrave;vre-feuille</trd>
  <trd lng="hu">jerik&oacute;i lonc</trd>
  <trd lng="ja">&#x30B9;&#x30A4;&#x30AB;&#x30BA;&#x30E9;</trd>
  <trd lng="nl">kamperfoelie</trd>
  <trd lng="pl">przewiercie&nacute;</trd>     
  <trd lng="ro">urechea-iepurelui</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_p;&c_r;&c_i;&c_f;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_zh;&c_i;&c_m;&c_o;&c_l;&c_o;&c_s;&c_t;&c_mol;
    &c_d;&c_u;&c_sh;&c_i;&c_s;&c_t;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">koz&iacute; list</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43F;&c_r;&c_i;&c_f;&#x43E;&#x43B;&c_ib;&c_j;</trd>,
    <trd>&c_zh;&c_i;&#x43C;&#x43E;&#x43B;&#x43E;&c_s;&c_t;&#x44C; &#x43A;&#x43E;&c_z;&#x43E;&#x43B;&c_i;&c_s;&c_t;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&c_z;&#x43E;&#x43B;&c_i;&c_s;&c_t;</trd>,
    <trd>&#x43A;&#x43E;&c_z;&#x44F;&c_ch;&c_i;&c_j; &#x43B;&c_i;&c_s;&c_t;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
