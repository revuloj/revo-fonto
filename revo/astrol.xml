<art mrk="$Id: astrol.xml,v 1.22 2023/08/30 18:23:37 revo Exp $">
<kap>
  <rad>astrolab</rad>/o
</kap>
<drv mrk="astrol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref>
      por determini horon a&ubreve; pozicion de la observanto
      per mezuro de la angula pozicio de astro super la horizonto:
      <ekz>
        de la 16-a jarcento al la 18-a jarcento (kiam la sekstanto estis
        inventita kaj anstata&ubreve;is la <tld/>on), la <tld/>o estis la &ccirc;efa mara navigilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7753">Astrolabo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Geoffrey Chaucer</nom> <klr>[&#x2026;]</klr> fami&gcirc;is anka&ubreve; kiel
        alkemiisto kaj astronomo kaj komponis traktaton pri <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=640">Geoffrey Chaucer</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_l;&c_ja;&c_b;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_a;&c_b;&c_i;&c_ja;</trd>
  <trd lng="cs">astrol&aacute;b</trd>
  <trd lng="de">Astrolabium</trd>
  <trd lng="en">astrolabe</trd>
  <trd lng="es">astrolabio</trd>
  <trd lng="fa">&a_A;&a_s;&a_T;&a_r;&a_l;&a_A;&a_b;</trd>
  <trd lng="fr">astrolabe</trd>
  <trd lng="he">&alef;&samekh;&tet;&resh;&vav;&lamed;&bet;&he;</trd>
  <trd lng="hu">asztrol&aacute;bium</trd>
  <trd lng="it">astrolabio</trd>
  <trd lng="ja">&#x30A2;&#x30B9;&#x30C8;&#x30ED;&#x30E9;&#x30FC;&#x30D9;</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_ja;&c_b;&c_i;&c_ja;</trd>
  <trd lng="sk">hvezd&aacute;rsky uhlomer</trd>
  <trd lng="uk">&#x430;&#x441;&#x442;&#x440;&#x43E;&#x43B;&#x44F;&#x431;&#x456;&#x44F;</trd>
</drv>
    <bld lok="&WCU;/thumb/0/02/MHS_42680_Astrolabe.jpg/576px-MHS_42680_Astrolabe.jpg" prm="CC BY-SA 4.0"><tld/>o, kolektero 42680 en Muzeo de Historio kaj Scienco,
        Oksfordo
