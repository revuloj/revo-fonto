<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>konoid</rad>/o
  </kap>
<drv mrk="konoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, naskita de
      &ccirc;iuj rektoj paralelaj al iu ebeno <klr>(&gcirc;iaj
      <ref tip="prt" cel="nask.0anto.rekto">naskantoj</ref>)</klr>,
      kiuj sekcas fiksan linion <klr>(&gcirc;ian
      <ref tip="prt" cel="direkt.0anto.linio">direktanton</ref>)</klr>.
    </dif>
    <ref tip="vid" cel="rekt.0ara.surfaco">rektara.</ref>
  </snc>
  <trd lng="de">Konoid</trd>
  <trd lng="en">conoid</trd>
  <trd lng="fr">cono&iuml;de</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_i;&c_d;</trd>
</drv>
</art>

