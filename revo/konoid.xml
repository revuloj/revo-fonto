<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konoid.xml,v 1.7 2022/05/01 08:09:25 revo Exp $">
  <kap>
    <rad>konoid</rad>/o
  </kap>
<drv mrk="konoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, naskita de
      &ccirc;iuj rektoj paralelaj al iu ebeno <klr>(&gcirc;iaj
      <ref tip="prt" cel="nask.0anto.rekto">naskantoj</ref>)</klr>,
      kiuj sekcas fiksan kurbon <klr>(&gcirc;ian
      <ref tip="prt" cel="direkt.0anto.linio">direktanton</ref>)</klr>
      kaj fiksan rekton:
      <ekz>
        <tld/>a kapitelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24182"
              >Kapitelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Ar&hcirc;imedo: &leftquot;Pri Konoidoj kaj Sferoidoj&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=622498"
              >Grandaj Libroj de la Okcidenta Mondo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rekt.0ara.surfaco">rektara</ref>
    <bld
      lok="&WCU;/2/22/Conoide.svg"
      prm="CC-BY-SA-3.0"
      lrg="360"
      >cirkla <tld/>o
        <fnt>
          <aut>HB</aut>
          <lok><url ref="&WCW;/File:Conoide.svg"
              >&WCW;/File:Conoide.svg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">conoide</trd>
  <trd lng="de">Konoid</trd>
  <trd lng="en">conoid</trd>
  <trd lng="es">conoide</trd>
  <trd lng="fr">cono&iuml;de</trd>
  <trd lng="pl">konoida</trd>
  <trd lng="pt">con&oacute;ide</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_i;&c_d;</trd>
</drv>
</art>

