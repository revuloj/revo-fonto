<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grenat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="grenat.0o">
  <kap><tld/>o</kap>
  <snc mrk="grenat.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Malhele ru&gcirc;a juvel&scirc;tono, kapabla gravuri la kvarcon.
    </dif>
    <refgrp tip="vid">
      <ref cel="ruben.0o">rubeno</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;</trd>
  <trd lng="ca">granat<klr> (subst.)</klr></trd>
  <trd lng="de">Granat</trd>
  <trd lng="en">garnet</trd>
  <trd lng="es">granate</trd> 
  <trd lng="fr">grenat<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">gr&aacute;n&aacute;t<klr tip="ind"> (&aacute;sv&aacute;ny)</klr></trd>
  <trdgrp lng="pt">
   <trd>granada<klr tip="amb"> (j&oacute;ia)</klr></trd>,
   <trd>granato</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;<klr tip="amb"> (&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
</drv>

<drv mrk="grenat.0kolora">
  <kap><tld/>kolora, <var><kap><tld/>a</kap></var></kap>
  <snc mrk="grenat.0kolora.KOL">
    <dif>
      Vinru&gcirc;a:
        <ekz>
          <tld/>kolora robo.
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_y; <klr>(&c_k;&c_o;&c_l;&c_je;&c_r;&c_u;
  &c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;)</klr></trd>
  <trd lng="ca">granat<klr> (adj.)</klr></trd>
  <trd lng="es">granate</trd> 
  <trd lng="fr">grenat<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>gr&aacute;n&aacute;tv&ouml;r&ouml;s</trd>,
    <trd>gr&aacute;n&aacute;tsz&iacute;n&udblac;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gren&aacute;</trd>,
   <trd>cor de <ind>granada</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_y;&c_j;<klr> (&c_c;&c_v;&c_je;&c_t;&c_a;
  &c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;)</klr></trd>
</drv>
</art>
