<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grenat</rad>/o
</kap>
<drv mrk="grenat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Malhele ru&gcirc;a juvel&scirc;tono, kapabla gravuri la kvarcon.
    </dif>
    <refgrp tip="vid">
      <ref cel="ruben.0o">rubeno</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Granat</trd>
  <trd lng="fr">grenat<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
</drv>
<drv mrk="grenat.0kolora">
  <kap><tld/>(kolor)a</kap>
  <snc>
    <dif>
      Vinru&gcirc;a: <ekz><tld/>(kolor)a robo.</ekz>
    </dif>
  </snc>
  <trd lng="fr">grenat<klr tip="ind"> (adj.)</klr></trd>
</drv>
</art>
