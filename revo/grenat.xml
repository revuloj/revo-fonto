<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grenat.xml,v 1.25 2024/03/13 15:24:39 revo Exp $">
<kap>
  <rad>grenat</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="grenat.0o">
  <kap><tld/>o</kap>
  <snc mrk="grenat.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Malhele ru&gcirc;a <ref tip="super" cel="sxton.juvel0o">juvel&scirc;tono</ref>, kapabla gravuri la
      kvarcon:
      <ekz>
        oni sukcesis datigi la esti&gcirc;-a&gcirc;on de diamantoj <klr>[&#8230;]</klr>, helpis bon&scirc;anco:
        maloftaj <tld/>-inkludoj enhavas radoaktivajn substancojn
        <fnt>
          <vrk><url ref="https://scienca-revuo.info/files/196-1-334-1-10-20140426.pdf"
              >Diamantoj: a&gcirc;aj pli ol 3 miliardojn da jaroj</url></vrk>
          <lok>Scienca Revuo vol 62, 3/2011, n-ro 226, p. 204</lok>
        </fnt>;
      </ekz>
      <ekz>
        koliero el fajre ru&gcirc;a grenato (piropo a&ubreve; nofeko) &mdash; la &scirc;tono de la printempa
        ekvinokso &mdash; &ccirc;irka&ubreve;is la altan kolon de la atenanino
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Efremov/tais_05.html"
              >Tais el Ateno</url></vrk>
          <lok>&Ccirc;apitro V, Muzo de la templo de Nejto</lok>
        </fnt>;
      </ekz>
      <ekz>
        antologieto el bohema poezio aperis en 1920: Bohemaj Grenatoj, de <nom>St. Schulhof</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ruben.0o">rubeno</ref>
    </refgrp>
  </snc>
    <rim>
      Ne konfuzu kun <ref tip="hom" cel="granat.0o">granato</ref> (frukto).
    </rim>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;</trd>
  <trd lng="ca">granat<klr> (subst.)</klr></trd>
  <trd lng="cs">gran&aacute;t</trd>
  <trd lng="de">Granat</trd>
  <trd lng="en">garnet</trd>
  <trd lng="es">granate</trd>
  <trd lng="fr">grenat<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">gr&aacute;n&aacute;t<klr tip="ind"> (&aacute;sv&aacute;ny)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12374;&#12367;&#12429;&#30707;  <pr>&#12374;&#12367;&#12429;&#12356;&#12375;</pr></trd>,
    <trd>&#12460;&#12540;&#12493;&#12483;&#12488;</trd>
  </trdgrp>
  <trd lng="pl">granat <klr>(kamie&nacute; szlachetny)</klr></trd>
  <trdgrp lng="pt">
   <trd>granada<klr tip="amb"> (j&oacute;ia)</klr></trd>,
   <trd>granato</trd>
  </trdgrp>
  <trd lng="ro">granat</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;<klr tip="amb"> (&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">gran&aacute;t</trd>
  <trd lng="uk">&c_gu;&c_r;&c_a;&c_n;&c_a;&c_t;</trd>
  <trd lng="zh">&#30707;&#27060;&#30707; <pr>sh&iacute;li&uacute;sh&iacute;</pr></trd>
</drv>

<drv mrk="grenat.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o a&ubreve; <tld/>kolora:
      <ekz>
        <tld/>a braceleto
        <fnt>
          <bib>LOdE</bib>
          <aut>Aleksander Kor&jcirc;enkov</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Ondo/Lo-312demo.pdf"
              >Aleksandr Kuprin</url></vrk>
          <lok>2022, n-ro2, p. 56</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fajenca&jcirc;o <klr>[&#8230;]</klr> fami&gcirc;is en <nom>Ming</nom>-dinastio pro siaj ri&ccirc;koloraj
        glazuroj, kiuj estas safira, viola, <tld/>a
        <fnt>
          <aut>Xu Senrong kaj Niu Xueqin</aut>
          <vrk><url ref="http://www.espero.com.cn/se/txt/2012-10/31/content_495045.htm"
              >Ceramiko</url></vrk>
          <lok>Espero (&Ccirc;inujo), 2012-10-31 </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_y; <klr>(&c_d;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_k;&c_a;&c_m;&c_je;&c_n;&c_ja; &c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_u;)</klr></trd>
<trd lng="de">granaten</trd>
</drv>

<drv mrk="grenat.0kolora">
  <kap><tld/>kolora</kap>
  <snc mrk="grenat.0kolora.KOL">
    <dif>
      Vin<ref tip="super" cel="rugx.0a">ru&gcirc;a</ref>:
      <ekz>
        <tld/>kolora robo;
      </ekz>
      <ekz>
        la Skvamnuka kolombo estas <klr>[&#8230;]</klr> ardezgriza kolombo <klr>[kun]</klr>
        <tld/>kolora plumaro &ccirc;irka&ubreve; la kolo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=222995"
              >Skvamnuka kolombo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_y; <klr>(&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; &c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_u;)</klr></trd>
  <trd lng="ca">granat<klr> (adj.)</klr></trd>
  <trd lng="de">weinrot</trd>
  <trd lng="es">granate</trd>
  <trd lng="fr">grenat<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>gr&aacute;n&aacute;tv&ouml;r&ouml;s</trd>,
    <trd>gr&aacute;n&aacute;tsz&iacute;n&udblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26263;&#36196;&#33394;&#12398;  <pr>&#12354;&#12435;&#12354;&#12363;&#12356;&#12429;&#12398;</pr></trd>,
    <trd>&#12460;&#12540;&#12493;&#12483;&#12488;&#33394;&#12398;  <pr>&#12460;&#12540;&#12493;&#12483;&#12488;&#12356;&#12429;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">granatowy</trd>
  <trdgrp lng="pt">
   <trd>gren&aacute;</trd>,
   <trd>cor de <ind>granada</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_y;&c_j;<klr> (&c_c;&c_v;&c_je;&c_t;&c_a;
  &c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;)</klr></trd>
  <trd lng="uk">&c_gu;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_o;&c_g;&c_o; &c_k;&c_o;&c_l;&c_mol;&c_o;&c_r;&c_u;</trd>
</drv>
</art>
