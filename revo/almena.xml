   <trd lng="so">ugu yaraan</trd>
   <trd lng="st">bonyane</trd>
   <trd lng="su">saeutikna</trd>
   <trd lng="sw">angalau</trd>
   <trd lng="ta">&#2965;&#3009;&#2993;&#3016;&#2984;&#3021;&#2980;&#2980;&#3009;</trd>
   <trd lng="tg">&c_k;&c_a;&c_m;&c_t;&c_a;&c_r;&c_i;&c_n;</trd>
   <trd lng="th">&#3629;&#3618;&#3656;&#3634;&#3591;&#3609;&#3657;&#3629;&#3618;</trd>
   <trd lng="tt">&c_k;&c_i;&c_m; &c_d;&c_i;&c_g;&#1241;&c_n;&c_d;&#1241;</trd>
   <trd lng="uk">&c_m;&c_ib;&c_r;&c_o;&c_ju;</trd>
   <trd lng="ur">&f_k;&a_m; &a_A;&a_z; &f_k;&a_m;</trd>
   <trd lng="uz">kamida</trd>
   <trd lng="vi">nh&#7845;t</trd>
   <trd lng="xh">ubuncinane</trd>
   <trd lng="yi">&qof;&lamed;&ayin;&nun;&samekh;&tet;&ayin;&resh;</trd>
   <trd lng="yo">o kere</trd>
   <trd lng="zu">okungenani</trd>
</drv>
