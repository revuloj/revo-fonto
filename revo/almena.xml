   <trd lng="lt">ma&zcaron;iau</trd>
   <trd lng="lv">vismaz</trd>
   <trd lng="mg">kely indrindra</trd>
   <trd lng="mi">te iti rawa</trd>
   <trd lng="mk">&c_n;&c_a;&#1112;&c_m;&c_a;&c_l;&c_k;&c_u;</trd>
   <trd lng="ml">&#3349;&#3393;&#3377;&#3358;&#3405;&#3358;&#3364;&#3405;</trd>
   <trd lng="mn">&c_n;&c_a;&c_a;&c_d; &c_z;&c_a;&c_h; &c_n;&c_mol;</trd>
   <trd lng="mr">&#2325;&#2367;&#2350;&#2366;&#2344;</trd>
   <trd lng="ms">kurangnya</trd>
   <trd lng="mt">inqas</trd>
   <trd lng="my">&#4129;&#4116;&#4106;&#4154;&#4152;&#4102;&#4143;&#4150;&#4152;</trd>
   <trd lng="no">i det minste</trd>
   <trd lng="ny">osachepera</trd>
   <trd lng="pa">&#2584;&#2673;&#2591;</trd>
   <trdgrp lng="pl">
     <trd>przynajmniej</trd>,
     <trd>co najmniej</trd>
   </trdgrp>
   <trd lng="oc">al <ind>mens</ind></trd>
   <trd lng="ps">&a_l;&#1686; &a_t;&a_r; &a_l;&#1686;&a_h;</trd>
   <trdgrp lng="pt">
     <trd>ao menos</trd>,
     <trd>pelo menos</trd>,
     <trd>aldemenos <klr>(ark.)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_o; &c_k;&c_r;&c_a;&c_j;&c_n;&c_je;&c_j;
   &c_m;&c_je;&c_r;&c_je;</trd>
   <trd lng="rw">byibuze</trd>
   <trd lng="sd">&f_g;&a_h;&#1661;</trd>
   <trd lng="si">&#3461;&#3520;&#3512; &#3520;&#3521;&#3514;&#3545;&#3505;&#3530;</trd>
