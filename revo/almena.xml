   <trd lng="my">&#x1021;&#x1014;&#x100A;&#x103A;&#x1038;&#x1006;&#x102F;&#x1036;&#x1038;</trd>
   <trd lng="no">i det minste</trd>
   <trd lng="ny">osachepera</trd>
   <trd lng="pa">&#xA18;&#xA71;&#xA1F;</trd>
   <trdgrp lng="pl">
