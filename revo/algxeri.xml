<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>al&gcirc;eri</rad>/o
</kap>

<drv mrk="algxeri.0o">
  <kap>
    <tld lit="A"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="algxeri.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la norda Afriko kun &ccirc;efurbo Al&gcirc;ero.
      Teritorio de <em>Demokrata Popolrespubliko Al&gcirc;erio</em>.
      ISO-3166 kodo: DZ.
      &Gcirc;i limas kun
      <ref tip="vid" cel="libi.0o.GEOG">Libio</ref>,
      <ref tip="vid" cel="mali.0o.GEOG">Malio</ref>,
      <ref tip="vid" cel="marok.0o.GEOG">Maroko</ref>,
      <ref tip="vid" cel="mauxrit.0o.GEOG">Ma&ubreve;ritanio</ref>,
      <ref tip="vid" cel="nigxer.0o.GEOG">Ni&gcirc;ero</ref>,
      Okcidenta Saharo,
      <ref tip="vid" cel="tunizi.0o.GEOG">Tunizio</ref>
      kaj <ref tip="vid" cel="mediterane.0o.GEOG">Mediteraneo</ref>.
    </dif>
    <trdgrp lng="ar">
      <trd>&alif;&lam;&jim;&zin;&alif;&ya_hamza;&ra;</trd>
      <trd>&alif;&lam;&jim;&mim;&ha;&waw;&ra;&ya;&ta_marbuta; &alif;&lam;&jim;&zin;&alif;&ya_hamza;&ra;&ya;&ta_marbuta; &alif;&lam;&dal;&ya;&mim;&qaf;&ra;&alif;&Ta;&ya;&ta_marbuta;
      &alif;&lam;&shin1;&ayn;&ba;&ya;&ta_marbuta;</trd>
    </trdgrp>
    <trd lng="bg">&c_A;&c_l;&c_zh;&c_i;&c_r;</trd>
    <trd lng="de">Algerien</trd>
    <trd lng="en">Algeria</trd>
    <trd lng="it">algeria</trd>
    <trdgrp lng="pl">
      <trd>Algieria</trd>,
      <trd>Algierska Republika Ludowo-Demokratyczna</trd>
    </trdgrp>
  </snc>
</drv>

</art>

