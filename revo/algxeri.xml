<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: algxeri.xml,v 1.20 2023/10/12 18:05:09 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Al&gcirc;eri</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="algxeri.A0o">
  <kap>
    <tld lit="A"/>o
  </kap>
  <mlg kod="ISO-3166">DZ</mlg>
  <snc mrk="algxeri.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o.POL" lst="voko:&scirc;tatoj">Lando</ref> en
      la norda <ref tip="malprt" cel="afrik.A0o.mondparto">Afriko</ref>
      kun &ccirc;efurbo Al&gcirc;ero; teritorio de Demokrata Popolrespubliko <tld/>o,
      limanta kun
      <ref tip="vid" cel="libi.L0o.GEOG">Libio</ref>,
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref>,
      <ref tip="vid" cel="marok.M0o.GEOG">Maroko</ref>,
      <ref tip="vid" cel="mauxrit.M0o.GEOG">Ma&ubreve;ritanio</ref>,
      <ref tip="vid" cel="nigxer.N0o.lando">Ni&gcirc;ero</ref>,
      Okcidenta Saharo,
      <ref tip="vid" cel="tunizi.T0o.GEOG">Tunizio</ref>
      kaj <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteraneo</ref>:
      <ekz>
        la du belegaj arab&ccirc;evaloj, kiujn li rajdis en Al&gcirc;erio, devige
        estis lasitaj en la soldatejo
        <fnt>
          <aut>Henri Vallienne</aut>
          <vrk>Kastelo de Prelongo</vrk>
          <lok>&ccirc;apitro 1a</lok>
        </fnt>
      </ekz>
      <ekz>
        kunlaborantoj de Kuracistoj Sen Limoj trovis pli ol 500 homojn 30&#x202F;km for de la
        urbo <nom>Ajn a&scirc;-&Scirc;uatir</nom>, en dezerta limregiono kun <tld/>o la 7an de oktobro
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008814.php">E&ubreve;ropo plialtigas sian drataron</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiun le&gcirc;on starigis la franca registaro de <nom>Edgar Faure</nom> en <tld/>o, kiam &gcirc;i
        kontra&ubreve;batalis la manifestaciojn anta&ubreve; la sendependi&gcirc;o de la lando
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Pinard</aut>
          <vrk><url ref="&Monato;/2006/008840.php">Signalo ankora&ubreve; ne revolucio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ar">
      <trd>&a_A;&a_l;&a_j;&a_z;&a_A;&a_y_hamza;&a_r;</trd>
      <trd>&a_A;&a_l;&a_j;&a_m;&a_h;&a_w;&a_r;&a_y;&a_t_marbuta; &a_A;&a_l;&a_j;&a_z;&a_A;&a_y_hamza;&a_r;&a_y;&a_t_marbuta; &a_A;&a_l;&a_d;&a_y;&a_m;&a_q;&a_r;&a_A;&a_T;&a_y;&a_t_marbuta;
      &a_A;&a_l;&a_sh;&a_ayn;&a_b;&a_y;&a_t_marbuta;</trd>
    </trdgrp>
    <trd lng="be">&c_A;&c_l;&c_mol;&c_zh;&c_y;&c_r;</trd>
    <trd lng="bg">&c_A;&c_l;&c_zh;&c_i;&c_r;</trd>
    <trd lng="de">Algerien</trd>
    <trd lng="en">Algeria</trd>
    <trd lng="fr">Alg&eacute;rie</trd>
    <trd lng="fa">&a_A;&a_l;&a_j;&a_z;&a_A;&f_y;&a_r;</trd>
    <trd lng="id">Aljazair</trd>
    <trd lng="it">algeria</trd>
    <trd lng="nl">Algerije</trd>
    <trdgrp lng="pl">
      <trd>Algieria</trd>,
      <trd>Algierska Republika Ludowo-Demokratyczna</trd>
    </trdgrp>
    <trd lng="pt">Arg&eacute;lia</trd>
  </snc>
  <trd lng="he">&#x5D0;&#x5DC;&#x5D2;'&#x5D9;&#x5E8;&#x5D9;&#x5D4;</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30B8;&#x30A7;&#x30EA;&#x30A2; 
  <pr>&#12354;&#x308B;&#12376;&#12359;&#x308A;&#12354;</pr></trd>
</drv>

</art>

