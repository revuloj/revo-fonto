<art mrk="$Id: antipi.xml,v 1.15 2023/08/30 18:23:35 revo Exp $">
<kap>
  <rad>antipirin</rad>/o
</kap>
<drv mrk="antipi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Blanka amara <ref tip="super" cel="pulvor.0o">pulvoro</ref>
      eltirata el terkarbo kaj uzata kontra&ubreve; febro kaj
      ne&ubreve;ralgio:
      <ekz>
        kontra&ubreve;febraj medikamentoj estas inter aliaj kinino kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40261">Febro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_p;&c_ib;&c_r;&c_y;&c_n;</trd>
  <trd lng="ca">antipirina</trd>
  <trd lng="cs">antipyrin</trd>
  <trd lng="de">Antipyrin</trd>
  <trd lng="en">antipyrine</trd>
  <trd lng="es">antipirina</trd>
  <trd lng="fr">antipyrine</trd>
  <trd lng="he">&#x5EA;&#x5E8;&#x5D5;&#x5E4;&#x5D4; &#x5DC;&#x5E9;&#x5D9;&#x5DB;&#x5D5;&#x5DA; &#x5DB;&#x5D0;&#x5D1;&#x5D9;&#x5DD;</trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C1;&#x30D4;&#x30EA;&#x30F3;</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
  <trd lng="sk">antipyrin</trd>
  <trd lng="uk">&#x430;&#x43D;&#x442;&#x438;&#x43F;&#x456;&#x440;&#x438;&#x43D;</trd>
</drv>
</art>
