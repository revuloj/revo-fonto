<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antipirin</rad>/o
</kap>
<drv mrk="antipi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Blanka amara pulvoro eltirata el terkarbo kaj uzata kontra&ubreve; febro kaj
      ne&ubreve;ralgio.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_p;&c_ib;&c_r;&c_y;&c_n;</trd>
  <trd lng="de">Antipyrin</trd>
  <trd lng="en">antipyrine</trd>
  <trd lng="fr">antipyrine</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
</drv>
</art>
