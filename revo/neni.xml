      <trd>oblivion</trd>
    </trdgrp>
    <trd lng="hu">a <ind>semmi</ind></trd>
    <trd lng="nl">het <ind>niets</ind></trd>
  </snc>
  <snc mrk="neni.0ajxo.absoluta">
    <dif>
      Absoluta senvalora&jcirc;o:
      <ekz>
        vi estas <tld/>a&jcirc;o kaj via agado estas
        <tld/>a&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 41:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen ili &ccirc;iuj estas vanta&jcirc;o, <tld/>a&jcirc;o estas ilia faro, vento kaj senenhava&jcirc;o
        estas iliaj <klr>[&#x2026;]</klr> idoloj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 41:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu havas <tld/>on estas <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        nur tute malgranda, <tld/>a&jcirc;o granda kiom ovo de mar&ccirc;kokino
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
