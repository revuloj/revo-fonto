<drv mrk="neni.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="neni.0eco.absoluta">
    <dif>
      Absoluta neesto a&ubreve; <ref tip="vid" cel="vant.0o">vanteco</ref>:
      <ekz>
        la <tld/>eco de la homa gloro;
      </ekz>
      <ekz>
        tian senton de senespero, senforteco kaj <tld/>eco li havis ankora&ubreve; neniam
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i etigis preska&ubreve; &gcirc;is plena <tld/>eco
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
  <trd>&c_n;&c_ib;&c_k;&c_ch;&c_e;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neexistence</trd>,
    <trd>nicota</trd>
  </trdgrp>
  <trd lng="de">Nichtigkeit</trd>
  <trdgrp lng="en">
    <trd>vacuousness</trd>,
    <trd>nothingness</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inanit&eacute;</trd>,
    <trd>nullit&eacute;</trd>
  </trdgrp>
  <trd lng="hu">semmiss&eacute;g</trd>
  <trd lng="nl">nietigheid</trd>
  <trdgrp lng="pl">
    <trd>nico&sacute;&cacute;</trd>,
    <trd>pr&oacute;&zdot;no&sacute;&cacute;</trd>
  </trdgrp>  
  <trd lng="ru">&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ni&ccaron;ota</trd>,
    <trd>ni&ccaron;otnos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="neni.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="neni.0igi.detrui">
    <dif>
      Tute forigi, detrui, pereigi:
      <ekz>
        la tertremo <tld/>igis ok vila&gcirc;ojn;
      </ekz>
      <ekz>
        la barbaraj invadoj <tld/>igis la romanan potencon;
      </ekz>
      <ekz>
        ne sufi&ccirc;as <tld/>igi
        <klr>(kp <ref tip="sin" cel="negati.0a">negativa</ref>)</klr>,
        necesas plenigi la malplenon;
      </ekz>
      <ekz>
        &ccirc;iujn &gcirc;iajn palacojn ili forbruligis per fajro, kaj &ccirc;iujn &gcirc;iajn plej karajn
        objektojn ili <tld/>igis
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 36:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        Mi <tld/>igos vin, kaj vi ne plu ekzistos
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 26:21</lok>
        </fnt>;
      </ekz>
     <ekz>
        kreska&jcirc;oj <klr>[&#8230;]</klr>, kiuj havis la eblon <klr>[&#8230;]</klr> prokrasti la
        morton, sed ne <tld/>igi &gcirc;in
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
     <trd lng="de">vernichten</trd>
    <trd lng="en">to annihilate</trd>
  </snc>
  <snc mrk="neni.0igi.senefikigi">
    <dif>
      Tute senvalorigi, nuligi, senefikigi:
      <ekz>
        <tld/>igi decidon, regulon;
      </ekz>
      <ekz>
        niaj klopodoj <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        li <tld/>igas &scirc;ian promeson
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 30:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        per memmortigo mi <tld/>igos la kontrakton, kiu rilatas nur al vivanto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ruza&jcirc;on <tld/>igas singardeco
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="fum.0o">fumo</ref>,
      <ref cel="sxauxm.0o">&scirc;a&ubreve;mo</ref>,
      <ref cel="van.0a">vana</ref>
    </refgrp>
    <trd lng="de">entwerten</trd>
    <trdgrp lng="en">
      <trd>to annul</trd>,
      <trd>invalidate</trd>, <trd>void</trd>
    </trdgrp>
  </snc>
  <rim>
      Z atentigas, ke &leftquot;&ccirc;ar la vortoj &#8218;<tld/>igi&#8216; kaj
      &#8218;<tld/>i&gcirc;i&#8216; estas jam de longa tempo tre bone konataj kaj
      uzataj de &ccirc;iuj Esperantistoj, tial mi opinias, ke malpermesi la
      uzadon de tiu &ccirc;i formo estus tute senbezone&rightquot;<fnt><bib>LR</bib></fnt>.
      Vidu la rimarkon &ccirc;e <ref cel="neni.0o.pronomo">nenio</ref>.
    </rim>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_u;&c_n;&c_i;&c_shch;&c_o;&c_zh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="de">
     <trd>aufheben</trd>,
     <trd>annullieren</trd>,
     <trd>aufl&ouml;sen</trd>,
     <trd>negieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>an&eacute;antir</trd>,
    <trd>annihiler</trd>
  </trdgrp>
  <trd lng="hu">megsemmis&iacute;t</trd>
  <trd lng="nl">vernietigen</trd>
  <trdgrp lng="pl">
    <trd>anihilowa&cacute;</trd>,
    <trd>niweczy&cacute;</trd>,
    <trd>unicestwi&cacute;</trd>,
    <trd>wniwecz obr&oacute;ci&cacute;</trd>,
    <trd>zniszczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>anihila</trd>,
    <trd>distruge</trd>,
    <trd>epav&abreve;</trd>,
    <trd> &icirc;ntoarce-te</trd>,
    <trd>strica</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kyno&zcaron;i&tcaron;</trd>,
    <trd>nivo&ccaron;i&tcaron;</trd>,
    <trd>zmari&tcaron;</trd>,
    <trd>zni&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ni&ccaron;it</trd>,
    <trd>zni&ccaron;it</trd>
  </trdgrp>
</drv>
</art>
