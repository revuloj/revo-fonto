<drv mrk="neni.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="neni.0eco.absoluta">
    <dif>
      Absoluta neesto a&ubreve; <ref tip="vid" cel="vant.0o">vanteco</ref>:
      <ekz>
        la <tld/>eco de la homa gloro;
      </ekz>
      <ekz>
        tian senton de senespero, senforteco kaj <tld/>eco li havis ankora&ubreve; neniam
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i etigis preska&ubreve; &gcirc;is plena <tld/>eco
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
