<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zip.xml,v 1.37 2024/07/26 01:42:17 revo Exp $">
<kap>
  <ofc>9</ofc><rad>zip</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="zip.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="zip.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Fermilo de <ref tip="malprt" cel="vest.0o">vesto</ref> a&ubreve;
      alia &scirc;tofa&jcirc;o
      konsistanta el du rubandoj provizitaj per vico da metalaj dentoj
      kaj
      impliki&gcirc;antaj a&ubreve; malimpliki&gcirc;antaj unu en la
      alian per la inter&scirc;ovo de
      movpeco.
    </dif>
    <ref tip="sin" cel="ferm.fulmo0ilo">fulmofermilo</ref>
    <ref tip="vid" cel="ferm.lap0ilo">lapfermilo</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a; <klr tip="amb">(&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_c;&c_i;&c_p;</trd>
  <trd lng="ca">cremallera</trd>
  <trdgrp lng="cs">
    <trd>zip</trd>,
    <trd>zips</trd>,
    <trd>zdrhovadlo</trd>
  </trdgrp>
  <trd lng="de">Rei&szlig;verschluss</trd>
  <trdgrp lng="en">
    <trd>zip<klr> (Britujo)</klr></trd>,
    <trd>zipper<klr> (&ccirc;efe Usono)</klr></trd>,
    <trd>zip fastener</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cremallera</trd>,
    <trd>cierre</trd>
  </trdgrp>
  <trd lng="fa">&a_z;&f_y;&f_p;</trd>
  <trdgrp lng="fr">
    <trd>fermeture &agrave; glissi&egrave;re</trd>,
    <trd>fermeture &eacute;clair</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cipz&aacute;r</trd>,
    <trd>vill&aacute;mz&aacute;r</trd>
  </trdgrp>
  <trd lng="ia">serratura fulmine</trd>
  <trd lng="io">zipo</trd>
  <trd lng="id">resleting</trd>
  <trdgrp lng="it">
    <trd>chiusura lampo</trd>,
    <trd>zip</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30A1;&#x30B9;&#x30CA;&#x30FC; <pr>&#12405;&#12353;&#12377;&#x306A;&#x30FC;</pr></trd>
    <trd>&#x30B8;&#x30C3;&#x30D1;&#x30FC; <pr>&#12376;&#12387;&#12401;&#x30FC;</pr></trd>
    <trd>&#x30C1;&#x30E3;&#x30C3;&#x30AF; <pr>&#12385;&#12419;&#12387;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="ko">&#xC9C0;&#xD37C;</trd>
  <trdgrp lng="nl">
    <trd>rits</trd>,
    <trd>ritssluiting</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zamek b&lstroke;yskawiczny</trd>,
    <trd>suwak</trd>
  </trdgrp>
  <trd lng="pt">z&iacute;per</trd>
  <trd lng="ro">fermoar</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;
  <klr>(&c_z;&c_a;&c_s;&c_t;&c_jo;&c_zh;&c_k;&c_a;)</klr></trd>
  <trd lng="sk">zips</trd>
  <trdgrp lng="sv">
    <trd>blixtl&aring;s</trd>,
    <trd>dragkedja</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&#x43C;&#x43E;&#x43A;-&c_b;&#x43B;&c_i;&c_s;&#x43A;&c_a;&c_v;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x62C9;&#x93C8; <pr>l&amacron;li&#xE0;n</pr></trd>,
    <trd>&#x62C9;&#x94FE; <pr>l&amacron;li&#xE0;n</pr></trd>,
    <trd>&#x62C9;&#37782; <pr>l&amacron;su&#x1D2;</pr></trd>,
    <trd>&#x62C9;&#38145; <pr>l&amacron;su&#x1D2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="zip.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="zip.0i.TEKS">
    <dif>
      Fermi per tirado de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_u;-&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_u;)</klr></trd>
  <trd lng="ca">tancar la <ind>cremallera</ind></trd>
  <trdgrp lng="cs">
    <trd>zapnout <klr>(si)</klr> zip</trd>,
    <trd>zapnout na zip</trd>
  </trdgrp>
  <trd lng="de">Rei&szlig;verschluss schlie&szlig;en</trd>
  <trd lng="en">zip</trd>
  <trd lng="es">cerrar con cremallera</trd>
  <trd lng="fr">fermer<klr tip="amb"> (une fermeture
  &eacute;clair)</klr></trd>
  <trdgrp lng="hu">
    <trd>beh&uacute;zza a <ind>cipz&aacute;rt</ind><klr tip="amb">
    (vmin)</klr></trd>,
    <trd>becipz&aacute;roz</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30A1;&#x30B9;&#x30CA;&#x30FC;&#12391;&#38281;&#12376;&#x308B; 
  <pr>&#12405;&#12353;&#12377;&#x306A;&#x30FC;&#12391;&#12392;&#12376;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>ritsen</trd>,
    <trd>dichtritsen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zapina&cacute; <klr>zamek b&lstroke;yskawiczny</klr></trd>,
    <trd>zasuwa&cacute; <klr>zamek b&lstroke;yskawiczny</klr></trd>
  </trdgrp>
  <trd lng="pt">fechar<klr tip="amb"> (um z&iacute;per)</klr></trd>
  <trd lng="sk">zapn&uacute;&tcaron; na zips</trd>
  <trd lng="uk">&c_z;&c_a;&c_s;&c_t;&c_ib;&c_b;&c_a;&c_t;&c_i; (&#x43D;&c_a; &c_z;&c_a;&#x43C;&#x43E;&#x43A;)</trd>
</drv>

<drv mrk="zip.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="zip.mal0i.TEKS">
    <dif>
      Malfermi per tirado de <tld/>o:
      <ekz>
        diris fama esperantisto: Se Esperanto estus <tld/>o nuntempe jam
        la tuta mondo <tld/>us kaj mal<tld/>us sian pantalonon per
        &gcirc;i
        <fnt><aut>E. Fabian</aut>, <vrk>vivrajto</vrk>,
        <lok>soc.culture.esperanto, 1998-09-12 </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_u;-&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_u;)</klr></trd>
  <trd lng="ca">obrir la <ind>cremallera</ind></trd>
  <trd lng="cs">rozepnout <klr>(si)</klr> zip</trd>
  <trd lng="de">Rei&szlig;verschluss &ouml;ffnen</trd>
  <trd lng="en">unzip</trd>
  <trd lng="es">abrir la <ind>cremallera</ind></trd>
  <trd lng="fr">ouvrir<klr tip="amb"> (une fermeture
  &eacute;clair)</klr></trd>
  <trdgrp lng="hu">
    <trd>kinyitja a <ind>cipz&aacute;rt</ind><klr tip="amb">
    (vmin)</klr></trd>,
    <trd>kicipz&aacute;roz</trd>
  </trdgrp>
  <trd lng="nl">openritsen</trd>
  <trdgrp lng="pl">
    <trd>rozpina&cacute; <klr>zamek b&lstroke;yskawiczny</klr></trd>
  </trdgrp>
  <trd lng="pt">abrir<klr tip="amb"> (um z&iacute;per)</klr></trd>
  <trd lng="sk">rozopn&uacute;&tcaron; zips</trd>
  <trd lng="uk">&c_r;&#x43E;&c_z;&c_s;&c_t;&c_ib;&c_b;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
