<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zip</rad>/o
</kap>
<drv mrk="zip.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Fermilo de <ref tip="malprt" cel="vest.0o">vesto</ref> a&ubreve; alia
&scirc;tofa&jcirc;o
      konsistanta el du rubandoj provizitaj per vico da
      metalaj dentoj kaj impliki&gcirc;antaj a&ubreve;
malimpliki&gcirc;antaj unu en
      la alian per la inter&scirc;ovo de movpeco.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="ferm.fulmo0ilo">fulmofermilo</ref>
  </refgrp>
    <trdgrp lng="en">
    <trd>zip</trd>,
    <trd>zipper</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fermeture &agrave; glissi&egrave;re</trd>,
    <trd>fermeture &eacute;clair</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cipz&aacute;r</trd>,
    <trd>vill&aacute;mz&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">ritssluiting</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;
<klr>(&c_z;&c_a;&c_s;&c_t;&c_jo;&c_zh;&c_k;&c_a;)</klr></trd>
</drv>
</art>
