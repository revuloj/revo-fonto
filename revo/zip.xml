    <trd>zip</trd>,
    <trd>zipper</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja; <klr>(&c_z;&c_a;&c_s;&c_t;&c_jo;&c_zh;&c_k;&c_a;)</klr></trd>
