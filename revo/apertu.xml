<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apertu.xml,v 1.25 2023/10/13 16:46:48 revo Exp $">
<kap>
   <ofc>6</ofc>
   <rad>apertur</rad>/o
   <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="apertu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ferm.mal0ajxo.KOMUNE">Malferma&jcirc;o</ref>:
      <ekz>
        la fenestra <tld/>o denove fermi&gcirc;is
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
         alrulu grandajn &scirc;tonojn al la <tld/>o de la kaverno
         <fnt><bib>MT</bib>, <lok>&Jos; 10:18</lok></fnt>;
      </ekz>
      <ekz>
         gardu la <tld/>on de via bu&scirc;o
         <fnt><bib>MT</bib>, <lok>&Mih; 7:5</lok></fnt>;
      </ekz>
      <ekz>
         <tld/>o de sako
         <fnt><bib>MT</bib>, <lok>&Gen; 43:21</lok></fnt>;
      </ekz>
      <ekz>
        granda &scirc;tono estis sur la <tld/>o de la puto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:2</lok>
        </fnt>;
      </ekz>
      <ekz>
         <tld/>o por la kapo en mezo de tuniko
         <fnt><bib>MT</bib>, <lok>&Eli; 28:32</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> almetis la bu&scirc;on al la <tld/>o de tubo, kondukanta al la supraj
        eta&gcirc;oj la vo&ccirc;on, parolantan malsupre
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lasta en la vico metis la grenfaskon supren al la <tld/>a tablo
        <fnt>
          <bib>VRA</bib>
          <lok>Ivar Lo-johansson</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="apert.0ajxo">aperta&jcirc;o</ref>
    <refgrp tip="vid">
      <ref cel="brecx.0o.KOMUNE">bre&ccirc;o</ref>,
      <ref cel="fauxk.0o.malfermajxo">fa&ubreve;ko<sncref/></ref>,
      <ref cel="fend.0o.KOMUNE">fendo</ref>,
      <ref cel="mank.0loko">mankloko</ref>,
      <ref cel="tru.0o.bestahejmo">truo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>,
     <trd>&c_d;&c_z;&c_ib;&c_r;&c_k;&c_a;</trd>,
     <trd>&c_zh;&c_a;&c_r;&c_a;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>apertura</trd>,
    <trd>d&iacute;ra</trd>,
    <trd>j&iacute;cen</trd>,
    <trd>otvor</trd>,
    <trd>pr&uring;duch</trd>,
    <trd>ventil</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&Ouml;ffnung</trd>,
    <trd>Lochweite</trd>,
    <trd>Apertur</trd>,
    <trd>Ausschnitt <klr>(&Ouml;ffnung)</klr></trd>,
    <trd>Aussparung</trd>
  </trdgrp>
  <trd lng="en">aperture</trd>
  <trd lng="es">abertura</trd>
  <trdgrp lng="fr">
    <trd>ouverture</trd>,
    <trd>entr&eacute;e</trd>
    <trd>orifice</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tsadi;&vav;&#x5B9;&he;&#x5B7;&resh;</trd>,
    <trd>&pe;&tav;&het;</trd>
  </trdgrp>
  <trd lng="hu">ny&iacute;l&aacute;s</trd>
  <trd lng="it">apertura</trd>
  <trdgrp lng="ja">
    <trd>&#x958B;&#x53E3;&#x90E8;  <pr>&#x304B;&#x3044;&#x3053;&#x3046;&#x3076;</pr></trd>,
    <trd>&#x53E3;  <pr>&#x304F;</pr></trd>,
    <trd>&#x7A74;  <pr>&#x3081;&#x3069;</pr></trd>,
    <trd>&#x51FA;&#x5165;&#x308A;&#x53E3;  <pr>&#x3067;&#x3044;&#x308A;&#x3050;&#x3061;</pr></trd>,
    <trd>&#x53E3;&#x5F84;  <pr>&#x3053;&#x3046;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">opening</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_je;&c_r;&c_s;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">stupe&ncaron; otvorenia clony</trd>
  <trd lng="sv">&ouml;ppning</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_t;&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_a;&c_p;&c_je;&c_r;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
