<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balanit.xml,v 1.7 2024/04/20 21:20:30 revo Exp $">
<kap>
  <rad>balanit</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="balanit.0o">
  <kap><tld/>o</kap>
  <snc mrk="balanit.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="glan.0o.penisa">Penisglana<sncref/></ref>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>:
      <ekz>
        tiuj afekciitaj <klr>[de fimozo]</klr> havas pli grandan riskon je <tld/>o kaj
        aliaj komplika&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=597013">Fimozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_a;&c_n;&c_ib;&c_t;</trd>
  <trd lng="ca">balanitis</trd>
  <trdgrp lng="de">
    <trd>Eichelentz&uuml;ndung</trd>,
    <trd>Balanitis</trd>
  </trdgrp>
  <trd lng="en">balanitis</trd>
  <trd lng="es">balanitis</trd>
  <trd lng="fr">balanite</trd>
  <trd lng="it">balanite</trd>
  <trd lng="nl">balanitis</trd>
  <trd lng="pl">zapalenie <ind>&zdot;o&lstroke;&eogonek;dzi</ind></trd>
  <trd lng="pt">balanite</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_a;&c_n;&c_i;&c_t;</trd>
  <trd lng="sv">balanit</trd>
  <trd lng="uk">&c_b;&c_a;&#x43B;&c_a;&#x43D;&c_ib;&c_t;</trd>
</drv>
</art>

