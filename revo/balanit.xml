<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>balanit</rad>/o
  </kap>
  <drv mrk="balanit.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <snc>
      <dif>
        <ref tip="vid" cel="glan.0o.penisa">Penisglana<sncref/></ref>
        <ref tip="lst" cel="inflam.0o"
        lst="voko:malsanoj">inflamo</ref>.
      </dif>
    </snc>
    <trd lng="be">&c_b;&c_a;&c_l;&c_a;&c_n;&c_ib;&c_t;</trd>
    <trd lng="ca">balanitis</trd>
    <trdgrp lng="de">
      <trd>Eichelentz&uuml;ndung</trd>,
      <trd>Balanitis</trd>
    </trdgrp>
    <trd lng="en">balanitis</trd>
    <trd lng="es">balanitis</trd>
    <trd lng="fr">balanite</trd>
    <trd lng="it">balanite</trd>
    <trd lng="nl">balanitis</trd>
    <trd lng="pl">zapalenie <ind>&zdot;o&lstroke;&eogonek;dzi</ind></trd>
    <trd lng="pt">balanite</trd>
    <trd lng="ru">&c_b;&c_a;&c_l;&c_a;&c_n;&c_i;&c_t;</trd>
    <trd lng="sv">balanit</trd>
  </drv>
</art>

