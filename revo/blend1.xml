<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>blend</rad>/o
</kap>
<drv mrk="blend1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <ref tip="dif" cel="kiras.0o.de_veturilo">kiraso <sncref/></ref>
    <trd lng="ca">blindatge</trd>
    <trd lng="es">blindaje</trd>
    <trd lng="fr">blindage</trd>
    <trd lng="hu">p&aacute;nc&eacute;lzat</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
  </snc>
  <refgrp tip="hom">
    Ne miksu kun <ref cel="blend.0o">blendo</ref> (mineralo).
  </refgrp>
</drv>

<drv mrk="blend1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">blindar</trd>
  <trd lng="es">blindar</trd>
  <trd lng="fr">blinder</trd>
  <trd lng="hu">p&aacute;nc&eacute;loz</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>

