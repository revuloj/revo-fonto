<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blend1.xml,v 1.18 2025/04/11 02:02:21 revo Exp $">
<kap>
  <rad>blend</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="blend1.0o">
  <kap><tld/>o</kap>
  <snc mrk="blend1.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="kiras.0o.de_veturilo">Kiraso<sncref/></ref>:
      <ekz>
        kiu aletendas manon al kiu ajn maro, kolizios kun <tld/>oj de
        niaj &scirc;ipoj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 2a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dikeco de la <tld/>o &scirc;an&gcirc;i&gcirc;as la&ubreve; tio, kia estas ver&scirc;ajneco, ke &gcirc;i estos eksponita al
        malamika pafado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20114">Tanko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ZOO</uzo> la hipokampo <klr>[havas]</klr> korpo<klr>[n]</klr>, kiu estas
        kovrita per ostaj tabuletoj kiel <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=81006">Hipokampo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#37628;&#30002; <pr>g&amacron;ngji&#462;</pr></trd>,
    <trd>&#35013;&#30002; <pr>zhu&amacron;ngji&#462;</pr></trd>
  </trdgrp>
</snc>
  <refgrp tip="hom">
    Ne konfuzu kun la mineralo <ref cel="blend.0o">blendo</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">blindatge</trd>
  <trd lng="de">Panzerung</trd>
  <trd lng="es">blindaje</trd>
  <trd lng="fr">blindage</trd>
  <trd lng="he">&mem;&shin;&vav;&resh;&yod;&yod;&fnun;</trd>
  <trd lng="hu">p&aacute;nc&eacute;lzat</trd>
  <trdgrp lng="it">
    <trd>blindatura</trd>,
    <trd>corazza<klr> (blindatura)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38275;&#20124;&#37467;&#37489;  <pr>&#12379;&#12435;&#12354;&#12360;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#30827;&#21270;&#37489;&#29289;  <pr>&#12426;&#12421;&#12358;&#12363;&#12371;&#12358;&#12406;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="pl">pancerz</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>sfalerit</trd>,
    <trd>sulfid zino&ccaron;nat&yacute;</trd>
  </trdgrp>
  <trd lng="cs">panc&eacute;&rcaron;ov&yacute; kryt</trd>
  <trd lng="en"><ind>armor</ind> plating</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_je;&c_n;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_f;&c_a;&c_l;&c_je;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_c;&c_i;&c_n;&c_k;&c_o;&c_v;&c_a; &c_o;&c_b;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_j;&c_s;&c_mol;&c_k;.&nbsp;&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_c;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="blend1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blend1.0i.MIL">
    <dif>
      Provizi per <tld/>o:
      <ekz>
        li respondas <klr>[&#8230;]</klr> rifuzas respondi, kaj nur petas pri tio, ke oni lin
        ka&scirc;u en blenditan kameron sub armitan gardon
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni <klr>[...]</klr> likvidis 93 tankojn, 153 <tld/>itajn transportilojn kaj 339
        militveturilojn
        <fnt>

          <aut>Evgeni Georgiev</aut>
          <vrk>Jugoslavio: Atenco ne
          preteratentita</vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="fortik.0igi">fortikigi</ref>,
       <ref cel="kiras.0i">kirasi</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#25259;&#30002; <pr>p&imacron;ji&#462;</pr></trd>,
    <trd>&#26377;&#30002; <pr>y&#466;uji&#462;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">blindar</trd>
  <trd lng="cs">opanc&eacute;&rcaron;ovat</trd>
  <trd lng="de">panzern</trd>
  <trd lng="en"><ind>armor</ind> plate</trd>
  <trd lng="es">blindar</trd>
  <trd lng="fr">blinder</trd>
  <trd lng="he">&lamed;&shin;&resh;&yod;&yod;&fnun;</trd>
  <trd lng="hu">p&aacute;nc&eacute;loz</trd>
  <trdgrp lng="it">
    <trd>blindare</trd>, <trd>corazzare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pancerzy&cacute;</trd>,
    <trd>opancerza&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">opancierova&tcaron;</trd>
  <trd lng="uk">&c_b;&c_r;&c_o;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>

