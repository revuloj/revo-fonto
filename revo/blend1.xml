<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>blend</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="blend1.0o">
  <kap><tld/>o</kap>
  <snc mrk="blend1.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <ref tip="dif" cel="kiras.0o.de_veturilo">kiraso <sncref/></ref>
    <ekz>
      kiu aletendas manon al kiu ajn maro, kolizios kun <tld/>oj de 
      niaj &scirc;ipoj 
      <fnt>
      <bib>Salamandroj</bib>,
      <lok>libro 3a, &ccirc;apitro 2a</lok>
      </fnt>.
    </ekz>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun la mineralo <ref cel="blend.0o">blendo</ref>.
  </refgrp>
    <trd lng="ca">blindatge</trd>
    <trd lng="es">blindaje</trd>
    <trd lng="fr">blindage</trd>
    <trd lng="hu">p&aacute;nc&eacute;lzat</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
</drv>

<drv mrk="blend1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blend1.0i.MIL">
    <dif>
      Provizi per <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">blindar</trd>
  <trd lng="es">blindar</trd>
  <trd lng="fr">blinder</trd>
  <trd lng="hu">p&aacute;nc&eacute;loz</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>

