<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: peterburg.xml,v 1.16 2023/09/03 12:45:07 revo Exp $">
<kap>
  <rad>Peterburg</rad>/o  <fnt><bib>FK</bib></fnt>, 
  <var><kap><rad var="St">Sankt-Peterburg</rad>/o <fnt><bib>Revizoro</bib></fnt></kap></var></kap>
<drv mrk="peterburg.0o">
  <kap><tld/>o, <var><kap><tld var="St"/>o</kap></var></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref> en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref> 
      inter Finnlanda Golfo kaj Ladoga Lago
      (<frm>30&#x2DA;18'E, 59&#x2DA;55'N</frm>),
      fondita en 1703 de la caro Petro Unua
      kaj de 1712 &gcirc;is 1918 la &ccirc;efurbo de Rusujo:
      <ekz>
        de 1914 &gcirc;is 1924 <tld/>o nomi&gcirc;is Petrogrado, kaj
        de 1924 &gcirc;is 1991 Leningrado;
      </ekz>
      <ekz>
        sendi ion el Parizo <tld/>on per &ccirc;evaloj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on alveturis unu instruita mimikisto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld var="St"/>o, strato Po&scirc;toficeja, domo numero na&ubreve;dek-sep, en la korto, tria
        eta&gcirc;o, dekstre
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la plej grandaj kaj plej bonaj &scirc;ipoj <klr>[&#x2026;]</klr> devis en tiu jaro
        &gcirc;uste iri al Stokholmo kaj veturigi al <tld/>o la karajn infanojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moskv.0o">Moskvo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_P;&c_je;&c_c;&c_ja;&c_r;&c_b;&c_u;&c_r;&c_g;</trd>,
    <trd>&c_S;&c_a;&c_n;&c_k;&c_t;-&c_P;&c_je;&c_c;&c_ja;&c_r;&c_b;&c_u;&c_r;&c_g;</trd>
  </trdgrp>
  <trd lng="ca">Sant Petersburg</trd>
  <trd lng="de">Sankt Petersburg</trd>
  <trd lng="en">Saint-Petersburg</trd>
  <trd lng="fr">Saint-P&eacute;tersbourg</trd>
  <trdgrp lng="hu">
    <trd>P&eacute;terv&aacute;r</trd>,
    <trd>Szentp&eacute;terv&aacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#x30DA;&#x30C6;&#x30EB;&#x30D6;&#x30EB;&#x30B0;</trd>
  <trd lng="nl">Sint-Petersburg</trd>
  <trd lng="pl">Petersburg</trd>
  <trd lng="ro">Sankt Petersburg</trd>
  <trdgrp lng="ru">
    <trd>&c_P;&c_je;&c_t;&c_je;&c_r;&c_b;&c_u;&c_r;&c_g;</trd>,
    <trd>&c_S;&c_a;&c_n;&c_k;&c_t;-&c_P;&c_je;&c_t;&c_je;&c_r;&c_b;&c_u;&c_r;&c_g;</trd>
  </trdgrp>
  <trd lng="uk">&#x41F;&#x435;&#x442;&#x435;&#x440;&#x431;&#x443;&#x440;&#x491;</trd>
</drv>
</art>

