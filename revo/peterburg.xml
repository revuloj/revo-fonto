<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Peterburg</rad>/o, Sankt-Peterburgo
</kap>
<drv mrk="peterburg.0o">
  <kap><tld/>o, Sankt-Peterburgo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref> (30&ring;18'E, 59&ring;55'N),
      fondita en 1703 fare de caro Petro I;
      ekde 1712 &gcirc;is 1918 la &ccirc;efurbo de Rusujo;
      ekde 1914 &gcirc;is 1925 <tld/>o nomi&gcirc;is Petrogrado, kaj
      ekde 1924 &gcirc;is 1991 Leningrado:
      <ekz>
       Mi ekskribos pri &ccirc;io al Trjapi&ccirc;kin en Peterburgo.
       <fnt>
         <aut>N. V. Gogol, trad. L. Zamenhof</aut>:
         <vrk>La Revizoro</vrk>.
       </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moskv.0o">Moskvo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sankt Petersburg</trd>
  <trd lng="en">Saint-Petersburg</trd>
  <trdgrp lng="ru">
    <trd>&c_P;&c_je;&c_t;&c_je;&c_r;&c_b;&c_u;&c_r;&c_g;</trd>,
    <trd>&c_S;&c_a;&c_n;&c_k;&c_t;-&c_P;&c_je;&c_t;&c_je;&c_r;&c_b;&c_u;&c_r;&c_g;</trd>
  </trdgrp>
</drv>
</art>

