<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hug.xml,v 1.10 2024/03/19 09:23:23 revo Exp $">
<kap>
  <rad>Hug</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hug.0o">
  <kap><tld/>o</kap>
  <snc mrk="hug.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        li &scirc;ovis al onklo <tld/>o la partibileton
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_ju;&c_g;&c_o;</trd>,
    <trd>&c_H;&#8217;&c_ju;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Hugo</trd>
  <trd lng="es">Hugo</trd>
  <trdgrp lng="fr">
    <trd>Hugo</trd>,
    <trd>Hugues</trd>
  </trdgrp>
  <trd lng="it">Ugo</trd>
  <trd lng="ja">&#x30E6;&#x30B4;&#x30FC;</trd>
  <trd lng="pt">Hugo</trd>
  <trdgrp lng="ru">
    <trd>&c_U;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_G;&c_u;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_H;&c_mol;&c_ju;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_G;&#x44E;&c_g;&#x43E; (&c_f;&c_r;&c_a;&#x43D;&c_c;&c_u;&c_z;&#x44C;&#x43A;&c_i;&c_j;
  &#x43F;&c_i;&c_s;&#x44C;&#x43C;&c_je;&#x43D;&#x43D;&c_i;&#x43A;)</trd>
</drv>
</art>

