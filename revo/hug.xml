<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hug.xml,v 1.9 2023/08/31 15:46:19 revo Exp $">
<kap>
  <rad>Hug</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hug.0o">
  <kap><tld/>o</kap>
  <snc mrk="hug.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        li &scirc;ovis al onklo <tld/>o la partibileton
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hugo</trd>
  <trd lng="es">Hugo</trd>
  <trdgrp lng="fr">
    <trd>Hugo</trd>,
    <trd>Hugues</trd>
  </trdgrp>
  <trd lng="it">Ugo</trd>
  <trd lng="ja">&#x30E6;&#x30B4;&#x30FC;</trd>
  <trd lng="pt">Hugo</trd>
  <trdgrp lng="ru">
    <trd>&c_U;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_G;&c_u;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_H;&c_mol;&c_ju;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x413;&#x44E;&#x433;&#x43E; (&#x444;&#x440;&#x430;&#x43D;&#x446;&#x443;&#x437;&#x44C;&#x43A;&#x438;&#x439; &#x43F;&#x438;&#x441;&#x44C;&#x43C;&#x435;&#x43D;&#x43D;&#x438;&#x43A;)</trd>
</drv>
</art>

