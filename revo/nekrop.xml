<art mrk="$Id$">
<kap>
   <rad>nekropol</rad>/o
</kap>
<drv mrk="nekrop.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Granda, subtera tombejo &ccirc;e antikvaj popoloj.
     </dif>
   </snc>
   <trd lng="be">&c_n;&c_e;&c_k;&c_r;&c_o;&c_p;&c_a;&c_l;&c_mol;</trd>
   <trd lng="ca">catacumbes</trd>
   <trdgrp lng="de">
     <trd>Nekropole</trd>,
     <trd>Totenstadt</trd>
   </trdgrp>
   <trd 
lng="el">&kappa;&alfa;&tau;&alfa;&kappa;&omikron_ton;&my;&beta;&epsilon;&sigma_fina;</trd>
   <trd lng="en">catacomb</trd>
   <trd lng="es">catacumbas</trd>
   <trd lng="hu">nekropolisz</trd>
   <trd lng="nl">dodenstad</trd>
   <trd lng="fr">n&eacute;cropole</trd>
   <trdgrp lng="pt">
     <trd>necr&oacute;pole</trd>,
     <trd>catacumba</trd>
   </trdgrp>
   <trd lng="ru">&c_n;&c_je;&c_k;&c_r;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
</drv>
</art>
