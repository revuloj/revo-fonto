<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spontan</rad>/a
  <fnt>V</fnt>
</kap>
<drv mrk="sponta.0a">
  <kap><tld/>a</kap>
  <snc>
   <dif>
      <ref tip="dif" cel="sponta1.0a">Spontanea</ref>:
      <ekz>
        <tld/>a alparolo
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mia respondo estis <tld/>a: &leftquot;mi ne scias&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2006/009046.php">Mia religio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        neniam mi planas ion anta&ubreve;e, la ideoj &ccirc;iam venas <tld/>e en mian kapon
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2004/008445.html">Kolekti antikva&jcirc;ojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        infana <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009307.php">La neforgesebla Ada</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">spontaneous</trd>
  <trd lng="be">&c_s;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;</trd>  
  <trd lng="de">spontan</trd>
  <trd lng="fr">spontan&eacute;</trd>
  <trdgrp lng="hu">
    <trd>spont&aacute;n</trd>,
    <trd>&ouml;nk&eacute;nytelen</trd>
  </trdgrp>
  <trd lng="nl">spontaan</trd>
  <trd lng="pl">spontaniczny</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
