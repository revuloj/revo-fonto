<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: bristol.xml,v 1.6 2024/10/10 15:31:02 afido Exp $">
    <kap>
      <rad>Bristol</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="bristol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="bristol.0o.urbo">
    <dif>
      Angla haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>,
      &ccirc;. 170&#x202F;km okcidente de Londono:
      <ekz>
        du amatoraj trupoj: unu el Londono, la alia el <tld/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la fino de la <tld/>a foiro ni adia&ubreve;is kaj disiris
        <fnt>
          <bib>KrM</bib>
          <lok>17. Fripono &Ccirc;ie Sian Nomon Skribas.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_B;&c_r;&c_y;&c_s;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_B;&c_r;&c_y;&c_s;&c_t;&c_a;&c_l;</trd>
    </trdgrp>
    <trd lng="de">Bristol</trd>
    <trd lng="en">Bristol</trd>
    <trd lng="fr">Bristol</trd>
    <trd lng="nl">Bristol</trd>
    <trd lng="pl">Bristol</trd>
    <trdgrp lng="zh">
    <trd>&#x5E03;&#x91CC;&#x65AF;&#25176; <pr>b&#xF9;l&#x1D0;s&#x12B;tu&#x14D;</pr></trd>,
    <trd>&#x5E03;&#x88E1;&#x65AF;&#x8A17;&#x723E;
    <pr>b&#xF9;l&#x1D0;s&#x12B;tu&#x14D;&#x11B;r</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30D6;&#x30EA;&#x30B9;&#x30C8;&#x30EB;</trd>
</drv>
  </art>

