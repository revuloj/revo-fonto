<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>helic</rad>/o
</kap>
<drv mrk="helic.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="helic.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
    <dif>
      Tia neebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, ke la
      angulo inter &gcirc;ia <ref cel="tangx.0anto">tan&gcirc;anto</ref>
      &ccirc;e punkto <frm>M</frm> kaj la rekto kun difinita direkto 
      trairanta punkton <frm>M</frm> ne dependas de <frm>M</frm>:
      <ekz>
        cirkla <tld/>o <klr>(situanta sur cirkla
        <ref cel="cilind.0o.surfaco">cilindro</ref>)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="spiral.0o.MAT">spiralo</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pasx.0o.de_helico">pa&scirc;o</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Schraubenlinie</trd>,
      <trd>Wendel</trd>
    </trdgrp>
    <trd lng="en">helix</trd>
    <trd lng="nl">schroefdraad</trd>
    <trd lng="pl">linia &sacute;rubowa</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_p;&c_i;&c_r;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_a;&c_ja; <ind>&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;</ind></trd></trdgrp>
  </snc>
  <snc mrk="helic.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      &Scirc;ra&ubreve;bforma aparato, kiu irigas &scirc;ipon en akvo a&ubreve; flugma&scirc;inon en aero.
    </dif>
    <trdgrp lng="de">
      <trd>Schiffsschraube</trd>,
      <trd>Luftschraube</trd>,
      <trd>Propeller</trd>
    </trdgrp>
    <trd lng="en">propeller</trd>
    <trd lng="nl">propeller</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_n;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_p;&c_je;&c_l;&c_l;&c_je;&c_r;</trd>
    </trdgrp>
  </snc>
  <trd lng="el">&epsilon_ton;&lambda;&jota;&kappa;&alfa;&sigma_fina;</trd>
  <trd lng="es">h&eacute;lice</trd>
  <trd lng="fr">h&eacute;lice</trd>
  <trd lng="pt">h&eacute;lice</trd>
</drv>
</art>
