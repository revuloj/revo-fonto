<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fort</rad>/a
</kap>
<drv mrk="fort.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="fort.0a.mano">
    <dif>
      Kapabla venki kontra&ubreve;staron kaj produkti grandajn efikojn materiajn
      a&ubreve; moralajn:
      <ekz>
        <tld/>a homo, luktisto, nacio, motoro, registaro;
      </ekz>
      <ekz>
        <tld/>a helpo, volo<fnt>B</fnt>, esprimo;
      </ekz>
      <ekz>
        kontra&ubreve; <tld/>a mano la le&gcirc;o estas vana
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li en &ccirc;io estas <tld/>a<fnt>Z</fnt> <klr>(multkapabla)</klr>;
      </ekz>
      <ekz>
        <tld/>korulo
        <fnt><bib>MT</bib>, <lok>&Psa; 76:5</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="potenc.0a">potenca</ref>.
    </refgrp>
  </snc>
  <snc mrk="fort.0a.intensa">
    <dif>
      Altgrada, <ref tip="sin"
      cel="intens.0a.impresa">intensa<sncref/></ref>;
      posedanta en alta grado siajn karakterizajn ecojn:
      <ekz>
        <tld/>a sento, kolero, intereso, bezono, dozo, fulmotondro, streko,
        profito, utilo, diferenco;
      </ekz>
      <ekz>
        en plej <tld/>a grado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a malsano, febro, varmo, tuso, koloro, lumo, adoro, vo&ccirc;o;
      </ekz>
      <ekz>
        <tld/>a kafo, biero, vino, cigaro.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fort.0a.fortika">
    <uzo tip="stl">ARK</uzo>
    <dif>
      = <ref tip="dif" cel="fortik.0a.materialo">Fortika<sncref/></ref> a&ubreve;
      <ref tip="sin" cel="firm.0a.fiksita">firma<sncref/></ref>:
      <ekz>
        <tld/>a muro, fortika&jcirc;o, &scirc;nuro, teksa&jcirc;o, papero;
      </ekz>
      <ekz>
        <tld/>a sano, pozicio.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fort.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Kvalifikas la konsonantojn a&ubreve; kvaza&ubreve;vokalojn prononcatajn
      pli intense.
    </dif>
  </snc>
  <snc mrk="fort.0a.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Kvalifikas la silabojn prononcatajn pli intense ol la
      najbaraj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_u;&c_zh;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">siln&yacute;</trd>
  <trdgrp lng="de">
    <trd>stark</trd>,
    <trd>kr&auml;ftig</trd>,
    <trd>heftig</trd>
  </trdgrp>
  <trd lng="en">strong</trd>
  <trd lng="es">fuerte</trd>
  <trd lng="fr">fort<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">sterk</trd>
  <trd lng="pl">silny</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="fort.0e">
  <kap><tld/>e</kap>
  <snc mrk="fort.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e bati.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fort.0e.multe">
    <dif>
      Multe, tre:
      <ekz>
        <tld/>e malpura<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e &gcirc;oji, dubi<fnt>Z</fnt>,
        deziri<fnt>Z</fnt>, voli<fnt>Z</fnt>,
        sin amuzi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>e volus scii&gcirc;i, kiel<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_ib;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="de">sehr</trd>
    <trd lng="ru">&c_o;&c_ch;&c_je;&c_n;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_c;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fortement</trd>,
    <trd>tr&egrave;s</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_o;</trd>
</drv>

<drv mrk="fort.0o">
  <kap><tld/>o</kap>
  <snc mrk="fort.0o.kapablo">
    <dif>
      Kapablo venki kontra&ubreve;staron kaj produkti grandajn efikojn:
      <ekz>
        <tld/>o de vivo, volo<fnt>B</fnt>, spirito;
      </ekz>
      <ekz>
        <tld/>o de la esperanta movado;
      </ekz>
      <ekz>
        kie regas la <tld/>o, tie rajto silentas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>oj de la naturo<fnt>B</fnt>;
      </ekz>
      <ekz>
        per &ccirc;iuj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Sa2; 6:14</lok></fnt>,
      </ekz>
      <ekz>
        per la tuta <tld/>o
        <fnt><bib>MT</bib>, <lok>&Kr1; 13:8</lok></fnt>;
      </ekz>
      <ekz>
        per propraj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        perdi, stre&ccirc;i la <tld/>ojn;
      </ekz>
      <ekz>
        kiu havas <tld/>on, havas rajton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        larmoj kun neretenebla <tld/>o ver&scirc;i&gcirc;is sur
        &scirc;iaj vangoj;
      </ekz>
      <ekz>
        la malnova dialekto restas en sia plena <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        militaj, maraj <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kraft</trd>,
      <trd>St&auml;rke</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_i;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="fort.0o.solideco">
    <dif>
      Solideco, fortikeco, malfacila rompebleco:
      <ekz>
        <tld/>o de &scirc;nuro, de sano;
      </ekz>
      <ekz>
        hodia&ubreve; <tld/>o, morga&ubreve; morto <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Festigkeit</trd>,
      <trd>Stabilit&auml;t</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
  <snc mrk="fort.0o.intenseco">
    <dif>
      Alta grado, intenseco:
      <ekz>
        <tld/>o de febro<fnt>B</fnt>, de vino.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>H&ouml;he</trd>,
      <trd>St&auml;rke</trd>,
      <trd>Intensit&auml;t</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
      <trd>&c_s;&c_i;&c_l;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fort.0o.FIZ">
    <tezrad fak="FIZ"/>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      esprimanta &ccirc;ian ka&ubreve;zon kapablan produkti,
      modifi a&ubreve; nuligi movon:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>MLT<sup>-2</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neuxto.n0o">ne&ubreve;tono</ref>.
    </refgrp>
    <trd lng="de">Kraft</trd>
    <trd lng="ru">&c_s;&c_i;&c_l;&c_a;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_c;</trd>
  </trdgrp>
  <trd lng="es">fuerza</trd>
  <trd lng="fr">force</trd>
  <trdgrp lng="nl">
    <trd>sterkte</trd>,
    <trd>kracht</trd>
  </trdgrp>
</drv>

<drv mrk="fort.0ega">
  <kap><tld/>ega</kap>
  <snc>
    <dif>
      Malmodere, senbride <tld/>a.
    </dif>
  </snc>
</drv>

<drv mrk="fort.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Kontra&ubreve;o de <tld/>a en &ccirc;iuj sencoj:
      <ekz>
        &ccirc;iuj vortoj estas mal<tld/>aj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en mal<tld/>a grado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a en la kalkulado<fnt>Z</fnt>, en la
        muziko<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="sxance">&scirc;anceli&gcirc;a</ref>,
      <ref cel="radik">senradika</ref>,
      <ref cel="pal.0a">pala</ref>,
      <ref cel="san.mal0a">malsana</ref>,
      <ref cel="velk.0a">velka</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_b;&c_y;</trd>
  <trd lng="de">schwach</trd>
  <trd lng="es">d&eacute;bil</trd>
  <trd lng="fr">faible</trd>
  <trd lng="nl">zwak</trd>
  <trd lng="pl">s&lstroke;aby</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_b;&c_y;&c_j;</trd>
</drv>

<drv mrk="fort.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Mal<tld/>a flanko (precipe f., pri la karaktero):
      <ekz>
        li havis la strangan mal<tld/>a&jcirc;on ke li ne povis...
        <fnt><bib>Fab1</bib>, <lok>Malgranda Niko kaj granda
        Niko</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tikl.0a">tikla</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_b;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_o;&c_je; &c_m;&c_je;&c_s;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de"><klr tip="ind">(Charakter-)</klr> Schw&auml;che</trd>
  <trd lng="es">debilidad</trd>
  <trd lng="fr">faiblesse</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_b;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="fort.mal0igi">
  <kap>mal<tld/>igi</kap>
  <snc>
    <dif>
      Igi mal<tld/>a:
      <ekz>
        malsano mal<tld/>igis lin;
      </ekz>
      <ekz>
        fremdaj elementoj mal<tld/>igis la unuecon de la
        socio<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fos.sub0i">subfosi</ref>,
      <ref cel="frost.0igi">frostigi</ref>,
      <ref cel="sang.sen0igi">sensangigi</ref>,
      <ref cel="konsum.0i">konsumi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_l;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_s;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schw&auml;chen</trd>,
    <trd>entkr&auml;ften</trd>
  </trdgrp>
  <trd lng="es">debilitar</trd>
  <trd lng="fr">affaiblir</trd>
  <trd lng="ru">&c_o;&c_s;&c_l;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fort.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i mal<tld/>a:
      <ekz>
        la vento, la entuziasmo mal<tld/>i&gcirc;as
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="lac">laci&gcirc;i</ref>,
      <ref cel="kviet">kvieti&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_l;&c_a;&c_b;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_s;&c_ib;&c_l;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_m;&c_a;&c_g;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="es">debilitarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>affaiblir</ind></trd>,
    <trd>baisser<klr tip="ind"> (faiblir)</klr></trd>,
    <trd>faiblir</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_l;&c_a;&c_b;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fort.per0i">
  <kap>per<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Per <tld/>o devigi, trudi, akiri:
      <ekz>
        vi per<tld/>is virinon dum &scirc;ia monata&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Jeh; 22:10</lok></fnt>;
      </ekz>
      <ekz>
        per<tld/>i infanon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_l;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>gewaltt&auml;tig</ind> sein gegen</trd>,
    <trd>vergewaltigen</trd>
  </trdgrp>
  <trd lng="es">forzar</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>violence</ind> (&agrave;)</trd>,
    <trd>violenter</trd>,
    <trd>violer</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_i;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fort.per0a">
  <kap>per<tld/>a</kap>
  <snc>
    <dif>
      Trudita per <tld/>o:
      <ekz>
        per<tld/>a morto;
      </ekz>
      <ekz>
        per<tld/>a amo estas plej <tld/>a malamo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        akiri per<tld/>e la regadon;
      </ekz>
      <ekz>
        altrudi per<tld/>e sian opinion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i per<tld/>e sin kvietigis kaj aperigis rideton sur
        siaj lipoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kion Dio ne donis, tion per<tld/>e ne ser&ccirc;u
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        nur al per<tld/>o cedas per<tld/>o;
      </ekz>
      <ekz>
        fari per<tld/>a&jcirc;on al<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxtat.0a">&scirc;tata</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mit <ind>Gewalt</ind></trd>,
    <trd>gewaltt&auml;tig</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>met <ind>geweld</ind></trd>,
    <trd>gewelddadig</trd>
  </trdgrp>
  <trd lng="fr">violent</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_i;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fort.per0eta">
  <kap>per<tld/>eta<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malmilda.
    </dif>
  </snc>
</drv>

<drv mrk="fort.seksper0i">
  <kap>seksper<tld/>i</kap>
  <snc>
    <dif>
      Per <tld/>o trudi sekskunigon:
      <ekz>
        iun kristnaskvesperon &dash; nigrigita aro kun lanternoj <klr>...</klr>
        partigis la grandbienulon <klr>...</klr> dum ebriuloj
        seksper<tld/>is lian
        edzinon
        <fnt>
        <aut>H. Pontoppidan, tr. L.B. Storm</aut>:
        <vrk>La vaganto</vrk>, <lok>Norda Prismo, 57:2</lok>.
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kaj kiel perdis dignon nun la morto, ||
        la am' aspektas same io besta: ||
        la ambrakumo &scirc;ajnas seksper<tld/>o
        <fnt>
        <aut>K. Kalocsay</aut>: <vrk>En &ccirc;i murdepoko</vrk>.
        <lok>Tra la &scirc;tormo, 1939&ndash;1945</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_l;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">violer<klr tip="ind"> (sexuellement)</klr></trd>
  <trd lng="ru">&c_i;&c_z;&c_n;&c_a;&c_s;&c_i;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fort.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti pli <tld/>a ol.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>battre<klr tip="ind"> (l'emporter sur)</klr></trd>,
    <trd>l'<ind>emporter</ind> (sur)</trd>,
    <trd>vaincre</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_s;&c_i;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fort.elektromova0o">
  <kap>elektromova <tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PV</bib><lok>artikolo: volto</lok></fnt>
  <fnt><bib>PIV1</bib><lok>artikolo: forto</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      esprimanta &ccirc;ian ka&ubreve;zon a&ubreve; procedon, kapablan subteni
      <ref cel="potenc1.0adiferenco">potencialan diferencon</ref>
      inter du punktoj de malfermita cirkvito, a&ubreve;
      <ref cel="kurent.0o">kurenton</ref> en fermita cirkvito:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de
        elektromova <tld/>o estas
        <frm>M<sup>1</sup>L<sup>2</sup>T<sup>-3</sup>I<sup>-1</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tensi.0o.ELE">tensio</ref>,
      <ref cel="volt.v0o">volto</ref>.
    </refgrp>
    <adm>
      Mi apenaux adaptis tiun difinacxon, prenitan de PIV1 kaj
      de francaj vortaroj, sed necesus io multe pli klara
      kaj science rigora, klariganta kial tiu grando havas
      la samajn dimensiojn kaj mezurunuojn, kiel "tensio".
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_r;&c_u;&c_h;&c_a;&c_ju;&c_ch;&c_a;&c_ja; <ind>&c_s;&c_ib;&c_l;&c_a;</ind></trd>
  <trd lng="fr">force &eacute;lectromotrice</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_a;&c_ja; &c_s;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_E;&c_D;&c_S;</trd>
  </trdgrp>
</drv>

<drv mrk="fort.inerto0o">
  <kap>inerto<tld/>o <fnt><bib>PV</bib></fnt>, inercia <tld/>o
  <fnt><bib>PIV2</bib></fnt></kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref cel="inert.0eca">Inerteca</ref> forto.
    </dif>
    <trd lng="be">&c_s;&c_ib;&c_l;&c_a; &c_ib;&c_n;&c_e;&c_r;&c_c;&c_y;&c_ib;</trd>
    <trd lng="de">Tr&auml;gheitskraft</trd>
    <trd lng="es">fuerza de <ind>inercia</ind></trd>
    <trd lng="fr">force d'inertie</trd>
    <trd lng="nl">inertie</trd>
    <trd lng="ru">&c_s;&c_i;&c_l;&c_a; &c_i;&c_n;&c_je;&c_r;&c_c;&c_i;&c_i;</trd>
  </snc>
</drv>
