    <trd>mno&zcaron;stvo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_n;&c_o;&c_zh;&c_i;&c_n;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
