<drv mrk="plur.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="plur.0eco.KOMUNE">
    <dif>
      Ekzisto de pluraj uloj, a&jcirc;oj, opinioj...
      <ref tip="vid" cel="dis.0eco">diseco</ref>
      <ref tip="vid" cel="unu.mal0eco.KOMUNE">malunueco</ref>:
      <ekz>
        <tld/>eco de religiaj konvinkoj, de mondkonceptoj kaj 
        vivomanieroj fari&gcirc;is trajto kiu karakterizas la e&ubreve;ropan 
        kulturon
        <fnt>
          <aut>KEK, KEEK, trad. -</aut>,
          <vrk><url ref="http://www.ikue.org/ekumcarto.html">
          Ekumena &Ccirc;arto</url></vrk>,
          <lok>[2006?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mnohost</trd>,
    <trd>mno&zcaron;stv&iacute;</trd>
  </trdgrp>
  <trd lng="en">plurality</trd>
  <trd lng="fr">pluralit&eacute;</trd>
  <trdgrp lng="sk">
