<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mauxric.xml,v 1.6 2024/04/18 06:29:44 revo Exp $">
<kap>
  <rad>Ma&ubreve;ric</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mauxric.0o">
  <kap><tld/>o</kap>
  <snc mrk="mauxric.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <ctl>Iom pri <tld/>o Maeterlinck</ctl>
        <fnt>
          <aut>G. Waringhien</aut>,
          <vrk><url ref="">
          Iom pri Ma&ubreve;rico Maeterlinck</url></vrk>,
          <lok>Nica Literatura Revuo, 1961-09 &gcirc;is 10, numero 7/1,
          p.
          27-30a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Maurici</trd>
    <trd lng="fr">Maurice</trd>
    <trd lng="pl">Maurycy</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_M;&c_a;&c_r;&c_y;&c_s;</trd>,
    <trd>&c_M;&c_a;&c_w;&c_r;&c_y;&c_c;&c_y;&c_jo;</trd>
  </trdgrp>
  <trd lng="uk">&#x41C;&#x43E;&c_r;&c_ib;&c_s;</trd>
</drv>
</art>

