<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: detonaci.xml,v 1.15 2024/01/03 08:30:40 revo Exp $">
<kap>
  <rad>detonaci</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="detonaci.0i">
  <kap><tld/>i</kap>
  <snc mrk="detonaci.0i.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="eksplo.0i">Eksplodi</ref> kun premondo de supersona rapido:
      <ekz>
        la tero <tld/>is sub niaj piedoj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la polico trovas po&scirc;telefonon, sep <ind><tld/>igilo</ind>jn, kelkajn vesta&jcirc;ojn kaj kasedon
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos I Font</aut>
          <vrk><url ref="&Monato;/2004/008300.html">Kronologio de kanajla mistifikado</url></vrk>
          <lok>2004</lok>
        </fnt>.
        <trd lng="de">Z&uuml;nder</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_t;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_b;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>detonovat</trd>,
    <trd>vybouchnout</trd>
  </trdgrp>
  <trd lng="de">detonieren</trd>
  <trd lng="en">detonate</trd>
  <trd lng="fr">d&eacute;toner<klr> (explosion)</klr></trd>
  <trd lng="he">&lamed;&nun;&pe;&ftsadi;</trd>
  <trd lng="ja">&#29190;&#x8F5F;&#12373;&#x305B;&#x308B;  <pr>&#12400;&#x304F;&#12372;&#12358;&#12373;&#x305B;&#x308B;</pr></trd>
  <trd lng="pl">detonowa&cacute;</trd>
  <trd lng="pt">detonar</trd>
  <trdgrp lng="sk">
    <trd>detonova&tcaron;</trd>,
    <trd>vybuchn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="detonaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="detonaci.0o.FIZ">
    <dif>
      <ref tip="super" cel="eksplo.0o">Eksplodo</ref> kun supersona premondo:
      <ekz>
        iom poste, serio da <tld/>oj estis a&ubreve;ditaj en Bagdado
        <fnt>
        <aut>&Gcirc;angalo</aut>,
        <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1780">Serio da atakoj
        per bomba&ubreve;toj...</url></vrk>,
        <lok>2004-10-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_t;&c_a;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_b;&c_u;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>detonace</trd>,
    <trd>v&yacute;buch</trd>
  </trdgrp>
  <trd lng="de">Detonation</trd>
  <trd lng="en">detonation</trd>
  <trd lng="fr">d&eacute;tonation</trd>
  <trd lng="he">&he;&#x5B4;&tav;&#x5B0;&pe;&#x5BC;&vav;&#x5B9;&tsadi;&#x5B0;&tsadi;&vav;&#x5BC;&tav;</trd>
  <trd lng="ja">&#29190;&#x8F5F;  <pr>&#12400;&#x304F;&#12372;&#12358;</pr></trd>
  <trd lng="pl">detonacja</trd>
  <trd lng="pt">detona&ccedil;&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>deton&aacute;cia</trd>,
    <trd>v&yacute;buch</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_t;&c_o;&c_n;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_b;&c_u;&c_h;</trd>
  </trdgrp>
  <trd lng="zh">&#x5F15;&#29190; <pr>y&#x1D0;nb&#xE0;o</pr></trd>
</drv>
</art>

