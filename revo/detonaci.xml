<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: detonaci.xml,v 1.11 2023/08/31 10:43:59 revo Exp $">
<kap>
  <rad>detonaci</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="detonaci.0i">
  <kap><tld/>i</kap>
  <snc mrk="detonaci.0i.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Eksplodi kun premondo de supersona rapido:
      <ekz>
        la tero <tld/>is sub niaj piedoj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>detonovat</trd>,
    <trd>vybouchnout</trd>
  </trdgrp>
  <trd lng="en">detonate</trd>
  <trd lng="fr">d&eacute;toner<klr> (explosion)</klr></trd>
  <trd lng="he">&lamed;&nun;&pe;&ftsadi;</trd>
  <trd lng="ja">&#x7206;&#x8F5F;&#x3055;&#x305B;&#x308B;  [<ind>&#x3070;&#x304F;&#x3054;&#x3046;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  <trd lng="pl">detonowa&cacute;</trd>
  <trd lng="pt">detonar</trd>
  <trdgrp lng="sk">
    <trd>detonova&tcaron;</trd>,
    <trd>vybuchn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="detonaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="detonaci.0o.FIZ">
    <dif>
      Eksplodo kun supersona premondo:
      <ekz>
        iom poste, serio da <tld/>oj estis a&ubreve;ditaj en Bagdado
        <fnt>
        <aut>&Gcirc;angalo</aut>,
        <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1780">Serio da
atakoj per bomba&ubreve;toj...</url></vrk>,
        <lok>2004-10-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>detonace</trd>,
    <trd>v&yacute;buch</trd>
  </trdgrp>
  <trd lng="en">detonation</trd>
  <trd lng="fr">d&eacute;tonation</trd>
  <trd lng="he">&he;&#x5B4;&tav;&#x5B0;&pe;&#x5BC;&vav;&#x5B9;&tsadi;&#x5B0;&tsadi;&vav;&#x5BC;&tav;</trd>
  <trd lng="ja">&#x7206;&#x8F5F;  [<ind>&#x3070;&#x304F;&#x3054;&#x3046;</ind>]</trd>
  <trd lng="pl">detonacja</trd>
  <trd lng="pt">detona&ccedil;&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>deton&aacute;cia</trd>,
    <trd>v&yacute;buch</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x435;&#x442;&#x43E;&#x43D;&#x430;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x431;&#x443;&#x445;</trd>
  </trdgrp>
</drv>
</art>

