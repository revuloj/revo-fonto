<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>detonaci</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="detonaci.0i">
  <kap><tld/>i</kap>
  <snc mrk="detonaci.0i.SON">
    <ref tip="dif" cel="knal.0i.SON">knali</ref>
      <ekz>
        la tero <tld/>is sub niaj piedoj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="fr">d&eacute;toner<klr> (explosion)</klr></trd>
</drv>

<drv mrk="detonaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="detonaci.0o.SON">
    <ref tip="dif" cel="knal.0o.SON">knalo</ref>
      <ekz>
        iom poste, serio da <tld/>oj estis a&ubreve;ditaj en Bagdado
        <fnt>
        <aut>&Gcirc;angalo</aut>,
        <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1780">Serio da atakoj per bomba&ubreve;toj...</url></vrk>,
        <lok>2004-10-24</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="fr">d&eacute;tonation</trd>
</drv>
</art>

