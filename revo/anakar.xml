<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anakar.xml,v 1.12 2024/04/16 07:24:17 revo Exp $">
<kap>
  <rad>anakardi</rad>/o
</kap>
<drv mrk="anakar.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="anakar1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Anacardium</trd>), kies unu specio estas
      la <ref tip="sub" cel="akajxu.0ujo">aka&jcirc;uujo</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> originas en tropikaj regionoj de Ameriko,
        unu tre konata ekzemplo estas la aka&jcirc;uarbo a&ubreve; okcidenta <tld/>o
        (<nom>Anacardium occidentale</nom>) de nordorienta Brazilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128362">Anakardio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/c/c7/Ripe_Cashew_apple_with_Nuts_Anacardium_occidentale_Goa_IMG_7907_%281%29.JPG/640px-Ripe_Cashew_apple_with_Nuts_Anacardium_occidentale_Goa_IMG_7907_%281%29.JPG"
    prm="CC BY-SA 3.0">frukto kaj folioj de aka&jcirc;uujo (Anacardium occidentale)
        <fnt>
          <aut>Dr. Raju Kasambe, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Ripe_Cashew_apple_with_Nuts_Anacardium_occidentale_Goa_IMG_7907_(1).JPG">&WCW;/File:Ripe_Cashew_apple_with_Nuts_Anacardium_occidentale_Goa_IMG_7907_(1).JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_k;&c_a;&c_r;&c_d;&c_y;&c_ju;&c_m;</trd>
  <trdgrp lng="hu">
    <trd>kesu</trd>,
    <trd>kasu</trd>,
    <trd>akazsu</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>orzech nerkowca</trd>,
    <trd>nanercz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_k;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_k;&c_je;&c_sh;&c_mol;&c_ju;</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#x43D;&c_a;&#x43A;&c_a;&c_r;&c_d;&c_ib;&#x44F;</trd>
</drv>
</art>
