    <trd>s&#xE1;lik</trd>
  </trdgrp>
  <trd lng="de">Fakir</trd>
  <trd lng="en">fakir</trd>
  <trd lng="es">faquir</trd>
  <trd lng="fr">fakir</trd>
  <trd lng="hu">fak&iacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#x6258;&#x9262;&#x50E7;  <pr>&#x305F;&#x304F;&#x306F;&#x3064;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x884C;&#x8005;  <pr>&#x3042;&#x3093;&#x3058;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">fakir</trd>
  <trd lng="pl">fakir</trd>
  <trd lng="pt">faquir</trd>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_i;&c_r;</trd>
  <trd lng="sk">fak&#xED;r</trd>
  <trd lng="sv">fakir</trd>
