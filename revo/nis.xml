    <kap>
      <rad>Nis</rad>/o
    </kap>

<drv mrk="nis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="nis.0o.rivero">
    <dif>
      Maldekstre alflua <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">rivero</ref>
      de <ref cel="odr.O0o">Odro</ref> fluanta la&ubreve;longe de la Germana-Pola landlimo:
      <ekz>
        la historie signifa limo Odro-<tld/>o ne plu ekzistas: la germana kanceliero
        <nom>Angela Merkel</nom> &gcirc;in solene malaperigis
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="https://www.monato.be/2008/009423.php"
              >Historia momento</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la urbon]</klr> <nom>Liberec</nom> <klr>[&#8230;]</klr> trafluas Luzacia <tld/>o kaj ties
        alriveroj, ekzemple Nigra Niso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12337"
              >Liberec</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Nei&szlig;e</trd>
    <trd lng="en">Neisse</trd>
    <trd lng="fr">Neisse</trd>
  </snc>
</drv>

