<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kampec.xml,v 1.15 2019/03/07 23:10:16 revo Exp $">
<kap>
  <rad>kampe&ccirc;</rad>/o  <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="kampec.0o">
  <kap><tld/>o</kap>
  <snc mrk="kampec.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el Suda Ameriko, kies ligno liveras ru&gcirc;an tinkturon:
      <ekz>
        &scirc;iaj manoj kaj piedoj, e&ccirc; la haroj estis la&ubreve; cigana
        kutimo <tld/>e ru&gcirc;igitaj
        <fnt>
          <aut>G. G&aacute;rdonyi, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/stelspec.pdf">
          Steloj de Eger</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz> 
    </dif>
    <trd lng="de">Kampesche</trd>
    <trdgrp lng="en">
      <trd>campeachy</trd>,
      <trd>logwood tree</trd>
    </trdgrp>
    <trd lng="nl">camp&ecirc;cheboom</trd>
    <trd lng="pl">modrzejec kampechia&nacute;ski</trd>
  </snc>
  <snc mrk="kampec.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Peza kaj malmola ligno de 
      <ref tip="vid" cel="kampec.0o.BOT"><tld/>o<sncref/></ref>.  
    </dif>
    <trd lng="de">Kampescheholz</trd>
    <trd lng="en">logwood</trd>
    <trd lng="nl">camp&ecirc;chehout</trd>
    <trdgrp lng="pl">
      <trd>kampeszyn</trd>,
      <trd>kampesz</trd>
    </trdgrp>   
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_e;&c_sh;</trd>
  <trd lng="fr">camp&ecirc;che</trd>
  <trdgrp lng="hu">
    <trd>b&ouml;rzs&ouml;nyfa</trd>,
    <trd>k&eacute;kfa</trd>,
    <trd>kampisfa</trd>
  </trdgrp>
  <trd lng="ro">b&abreve;can</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_p;&c_je;&c_sh;</trd>
</drv>
</art>
