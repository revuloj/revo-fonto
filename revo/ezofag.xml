<art mrk="$Id: ezofag.xml,v 1.28 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ezofag</rad>/o
</kap>
<drv mrk="ezofag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="tub.0o">Tubo</ref>
      kondukanta la nutra&jcirc;on 
      de la <ref tip="vid" cel="gorgx.0o">gor&gcirc;o</ref> 
      &gcirc;is la <ref tip="vid" cel="stomak.0o">stomako</ref>:
      <ekz>
        &ccirc;e aidoso-malsanuloj herpeto konstante recidivas kaj ofte atakas faringon,
        <tld/>on, hepaton, cerebron kaj aliajn organojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php">La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la donita&jcirc;oj ne sufi&ccirc;is por eltiri iajn konkludojn pri 20&#x202F;% de la kancertipoj,
        kiuj trafas la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2014/011193.php">Malsama a&gcirc;o, malsamaj riskoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="cs">j&iacute;cen</trd>
  <trd lng="de">Speiser&ouml;hre</trd>
  <trd lng="en">esophagus</trd>
  <trd lng="es">es&oacute;fago</trd>
  <trd lng="fa">&a_m;&a_kasra;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&oelig;sophage</trd>,
    <trd>oesophage</trd>
  </trdgrp>
  <trd lng="hu">nyel&odblac;cs&odblac;</trd>
  <trdgrp lng="id">
    <trd>faring</trd>,
    <trd>kerongkongan</trd>
  </trdgrp>
  <trd lng="it">esofago</trd>
  <trd lng="ja">&#x98DF;&#x9053;  <pr>&#x3057;&#x3087;&#x304F;&#x3069;&#x3046;</pr></trd>
  <trd lng="la">oesophagus</trd>
  <trd lng="pl">prze&lstroke;yk</trd>
  <trd lng="pt">es&ocirc;fago</trd>
  <trd lng="ru">&c_p;&c_i;&c_shch;&c_je;&c_v;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>hltan</trd>,
    <trd>pa&zcaron;er&aacute;k</trd>
  </trdgrp>
  <trd lng="sv">matstrupe</trd>
  <trd lng="uk">&#x441;&#x442;&#x440;&#x430;&#x432;&#x43E;&#x445;&#x456;&#x434;</trd>
  <trdgrp lng="zh">
    <trd>&#x98DF;&#x9053; <pr>sh&#xED;d&#xE0;o</pr></trd>,
    <trd>&#x98DF;&#x7BA1; <pr>sh&#xED;gu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
</art>
