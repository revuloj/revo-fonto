<art mrk="$Id: evikci.xml,v 1.18 2023/10/14 15:49:05 revo Exp $">
<kap>
<rad>evikci</rad>/i
</kap>
<drv mrk="evikci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sen.0igi">Senigi</ref> je la posedo de havo akirita en bona fido:
      <ekz>
        en la jaro 1171 herezuloj <klr>[&#x2026;]</klr> estis <tld/>itaj de la urbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=161658">Beaune</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        1284: &Gcirc;enovo venkas Pizon dum la &scirc;ipbatalo de <nom>Meloria</nom> <klr>[&#x2026;]</klr> Pizo estas
        <tld/>ita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2163">Korsiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la 1870-aj jaroj ka&scirc;ubaj kaj germanaj enmigrintoj kreis fi&scirc;istan vila&gcirc;on sur
        Insulo <nom>Jones</nom> en Milvokio, sed ili ne posedis tiun terenon, kaj la registaro
        de Milvokio <tld/>is ilin en la 1940-aj jaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=125686">Ka&scirc;uboj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
  <trd>&c_p;&c_a;&c_z;&c_b;&c_a;&c_v;&c_ib;&c_c;&c_mol; <ind>&c_m;&c_a;&c_jo;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind> <klr tip="ind">(&c_p;&c_r;&c_a;&c_z;
  &c_p;&c_r;&c_y;&c_s;&c_u;&c_d;)</klr></trd>,
  <trd>&c_k;&c_a;&c_n;&c_f;&c_ib;&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a;&c_z; &c_p;&c_r;&c_y;&c_s;&c_u;&c_d;)</klr></trd>
</trdgrp>
  <trd lng="ca">desposseir</trd>
  <trd lng="cs">prov&eacute;st evikci</trd>
  <trdgrp lng="de">
     <trd>evinzieren</trd>,
     <trd>exmittieren</trd>,
     <trd>entziehen</trd>
  </trdgrp>
  <trd lng="en">evict</trd>
  <trd lng="fr">&eacute;vincer<klr tip="ind"> (d&eacute;poss&eacute;der
juridiquement)</klr></trd>
  <trdgrp lng="hu">
   <trd>kisemmiz</trd>,
   <trd>megfoszt<klr tip="ind"> (vagyont&oacute;l)</klr></trd>
</trdgrp>
  <trd lng="ja">&#x53D6;&#x308A;&#x623B;&#x3059;  <pr>&#x3068;&#x308A;&#x3082;&#x3069;&#x3059;</pr></trd>
  <trdgrp lng="pt">
    <trd>evencer</trd>,
    <trd>desapossar</trd>,
    <trd>destituir</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_sh;&c_i;&c_t;&c_mol; &c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_a; &c_p;&c_o; &c_s;&c_u;&c_d;&c_u;</trd>
  <trd lng="sk">previes&tcaron; evikciu</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x437;&#x431;&#x430;&#x432;&#x438;&#x442;&#x438; &#x43C;&#x430;&#x439;&#x43D;&#x430; &#x441;&#x443;&#x434;&#x43E;&#x43C;</trd>
</drv>
</art>
