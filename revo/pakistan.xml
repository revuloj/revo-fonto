<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pakistan.xml,v 1.17 2020/02/27 16:10:22 revo Exp $">
<kap>
  <ofc>8</ofc><rad>Pakistan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="pakistan.P0o">
  <kap>
    <ofc>8</ofc><tld lit="P"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">PK</mlg>
  <snc mrk="pakistan.P0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la sudokcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>,
      teritorio de <em>Islama Respubliko de Pakistano</em>,
      limanta kun <ref tip="vid" cel="afgan.A0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="barat.B0o">Barato</ref>,
      <ref tip="vid" cel="iran.I0o.GEOG">Irano</ref>,
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      kaj kun Araba Maro;
      kun &ccirc;efurbo <ref tip="prt" cel="islamabad.0o">Islamabado</ref>:
      <ekz>
        grupo el <tld/>o &#8208; &ccirc;i-foje komercistoj kun validaj vizoj &#8208; estis
        malliberigita dum
        tuta semajno, &ccirc;ar oni senbaze kredis ilin kontra&ubreve;le&gcirc;aj
        enmigrantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007319.html">Ne tiom
          negative ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        amasdetruajn armilojn <esc>tutcerte</esc> posedas <klr>[&#8230;]</klr>
        ekzemple Nord-Koreio,
        same <tld/>o kaj Hindio, Rusio kaj Israelo <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2003/008114.html">Papo
          kontra&ubreve; kristano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_k;&c_ib;&c_s;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>el Pakistan</trd>,
    <trd>Rep&uacute;blica Isl&agrave;mica del Pakistan</trd>
  </trdgrp>
  <trd lng="de">Pakistan</trd>
  <trd lng="en">Pakistan</trd>
  <trd lng="fa">&f_p;&a_A;&f_k;&a_s;&a_t;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>Pakistan</trd>,
    <trd>R&eacute;publique islamique du Pakistan</trd>
  </trdgrp>
  <trd lng="id">Pakistan</trd>
  <trd lng="it">Pakistan</trd>
  <trd lng="nl">Pakistan</trd>
  <trdgrp lng="pl">
    <trd>Pakistan</trd>,
    <trd>Islamska Republika <ind>Pakistanu</ind></trd>
  </trdgrp>
  <trd lng="pt">Paquist&atilde;o</trd>
  <trd lng="vo">Pakist&auml;n</trd>
</drv> 

<drv mrk="pakistan.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld lit="p"/>aj kuracistoj, laborantaj en urbo
        <nom>Nenagh</nom>, 
        estas insultitaj en trinkejo: la nazo de unu estis rompita
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007319.html">Ne tiom
          negative ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">pakistanisch</trd>
  <trd lng="en">Pakistani</trd>
  <trdgrp lng="fr">
    <trd>pakistanais</trd>,
    <trd>pakistanaise<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Pakistaans</trd>
  <trd lng="vo">Pakist&auml;nik</trd>
</drv>

<drv mrk="pakistan.0ano">
  <kap><tld lit="p"/>ano</kap>
  <snc>
    <dif>
      Civitano de <tld/>o:
      <ekz>
        hindo<tld lit="p"/>oj ri&ccirc;i&gcirc;as rapide pro vendado de novaj, sed
        tamen fu&scirc;aj a&ubreve;topecaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2007/009054.php">A&ubreve;tokomerco pli
          strikte regulita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pakistaner</trd>,
    <trd>Pakistanerin</trd>,
    <trd>Pakistani</trd>
  </trdgrp>
  <trd lng="en">Pakistani</trd>
  <trdgrp lng="fr">
    <trd>Pakistanais</trd>,
    <trd>Pakistanaise<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Pakistaan</trd>,
    <trd>Pakistani</trd>
  </trdgrp>
  <trd lng="vo">Pakist&auml;nan</trd>
</drv>

</art>

