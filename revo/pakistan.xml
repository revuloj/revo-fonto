<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc><rad>Pakistan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="pakistan.P0o">
  <kap>
    <ofc>8</ofc><tld lit="P"/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Pakistano">Pakistano</url></lok></fnt>
  </kap>
  <snc mrk="pakistan.P0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>PK</mlg>
    <dif>
      &Scirc;tato en la sudokcidenta Azio kun &ccirc;efurbo Islamabado.
      Oficiala nomo: <em>Islama Respubliko de Pakistano</em>.
      &Gcirc;i limas kun <ref tip="vid"
      cel="afgan.A0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="barat.B0o">Barato</ref>,
      <ref tip="vid" cel="iran.I0o.GEOG">Irano</ref>,
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      kaj kun Araba Maro.
    </dif>
    </snc>
    <trd lng="be">&c_P;&c_a;&c_k;&c_ib;&c_s;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>el Pakistan</trd>,
      <trd>Rep&uacute;blica Isl&agrave;mica del Pakistan</trd>
    </trdgrp>
    <trd lng="fa">&f_p;&a_A;&f_k;&a_s;&a_t;&a_A;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>Pakistan</trd>,
      <trd>R&eacute;publique islamique du Pakistan</trd>
    </trdgrp>
    <trd lng="id">Pakistan</trd>
    <trd lng="it">Pakistan</trd>
    <trd lng="nl">Pakistan</trd>
    <trdgrp lng="pl">
      <trd>Pakistan</trd>,
      <trd>Islamska Republika <ind>Pakistanu</ind></trd>
    </trdgrp>
    <trd lng="pt">Paquist&atilde;o</trd>
</drv> 

<drv mrk="pakistan.0ano">
  <kap><tld lit="p"/>ano,
  <var><kap><tld lit="p"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Pakistano.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Pakistanais</trd>
    <trd>Pakistanaise<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="pakistan.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Pakistano.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>pakistanais</trd>
    <trd>pakistanaise<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

