<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>asparag</rad>/o
</kap>
<drv mrk="aspara.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Asparagus</trd>), kies tigojn ankora&ubreve; subterajn
      oni man&gcirc;as kiel legomo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_p;&c_a;&c_r;&c_a;&c_g;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_zh;&c_a;</trd>
  </trdgrp>                    
  <trd lng="br">asperjezenn</trd>
  <trd lng="en">asparagus</trd>
  <trd lng="de">Spargel</trd>
  <trd lng="es">esp&aacute;rrago</trd>
  <trd lng="fr">asperge</trd>
  <trd lng="hu">sp&aacute;rga <klr
  tip="ind">(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="nl">asperge</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_g;&c_u;&c_s;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_zh;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">sparris</trd>
</drv>
</art>
