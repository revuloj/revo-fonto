<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aspara.xml,v 1.33 2026/02/22 20:40:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>asparag</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="aspara.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="aspara.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Asparagus</trd>), la junajn tigojn de la specio
      <nom>Asparagus officinalis</nom> oni man&gcirc;as kiel
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="legom.0o">legomo</ref>,
      blankajn anta&ubreve; kiam ili eli&gcirc;as el la tero, a&ubreve; verdajn:
      <ekz>
        &scirc;i man&gcirc;is nek <klr>[&#x2026;]</klr> <tld/>ojn, nek heliantajn tuberojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        januare oni preparas la sablon por la <tld/>o-bedoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/009930.php"
              >Ni fosu nian konton!</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel &scirc;ildojn ili uzis fungojn, kaj tigojn de <tld/>oj kiel
        lancojn
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url ref="https://web.archive.org/web/20110718052119if_/http://www.esperanto.gr/lit/els/rakontoj.pdf"
              >veraj rakontoj</url>. &ndash; 2005</vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/d8/Gr&uuml;ner_Spargel_IMG_20220508_121719.jpg/389px-Gr&uuml;ner_Spargel_IMG_20220508_121719.jpg"
      prm="CC BY-SA 4.0"
      >verda <tld/>o
        <fnt>
          <aut>IgorCalzone1, propra verko</aut>
          <lok><url ref="&WCW;/File:Gr%C3%BCner_Spargel_IMG_20220508_121719.jpg"
              >https://commons.wikimedia.org/wiki/File:Gr&uuml;ner_Spargel_IMG_20220508_121719.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="zh">&#x6587;&#x7AF9; <pr>w&eacute;nzh&uacute;</pr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_p;&c_a;&c_r;&c_a;&c_g;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_zh;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_s;&c_p;&c_je;&c_r;&c_zh;&c_a;</trd>
  <trd lng="br">asperjezenn</trd>
  <trd lng="ca">esp&agrave;rrec</trd>
  <trdgrp lng="cs">
    <trd>aspar&aacute;gus</trd>,
    <trd>ch&rcaron;est</trd>,
    <trd>&scaron;pargl</trd>
  </trdgrp>
  <trd lng="de">Spargel</trd>
  <trd lng="en">asparagus</trd>
  <trd lng="es">esp&aacute;rrago</trd>
  <trd lng="fa">&a_m;&a_A;&a_r;&f_ch;&a_w;&a_b;&a_h;</trd>
  <trd lng="fr">asperge</trd>
  <trd lng="he">&alef;&samekh;&pe;&resh;&gimel;&vav;&samekh;</trd>
  <trd lng="hu">sp&aacute;rga <klr tip="ind">(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="it">asparago</trd>
  <trd lng="ja">&#x30A2;&#x30B9;&#x30D1;&#x30E9;&#x30AC;&#x30B9;</trd>
  <trd lng="nl">asperge</trd>
  <trdgrp lng="pl">
    <trd>szparag</trd>,
    <trd>asparagus<klr> (jako ozdoba do bukiet&oacute;w)</klr></trd>
  </trdgrp>
  <trd lng="pt">aspargo</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_g;&c_u;&c_s;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>aspar&aacute;gus</trd>,
    <trd>&scaron;parg&lcaron;a</trd>
  </trdgrp>
  <trd lng="sv">sparris</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_o;&c_l;&c_o;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_ja;&c_ch;&c_i;&c_j; &c_h;&c_o;&c_l;&c_o;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_d;&c_i;&c_k;&c_a; &c_s;&c_p;&c_a;&c_r;&c_zh;&c_a;</trd>
  </trdgrp>
</drv>
</art>
