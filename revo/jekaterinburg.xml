<vortaro>
  <art mrk="$Id: jekaterinburg.xml,v 1.5 2023/12/13 12:45:03 afido Exp $">
    <kap>
      <rad>Jekaterinburg</rad>/o
    </kap>

<drv mrk="jekaterinburg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Universitata <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> &ccirc;e Uralo,
      1400&#x202F;km
      oriente de Moskvo (inter 1924-1991 nomata Sverdlovsk):
       <ekz>
        lo&gcirc;antoj de <tld/>o nun scias kiel atingi la plej
        proksiman superbazaron, sed ne
        la memorejon al la 45&#x202F;000 Stalin-viktimoj, kiuj
        konstruis en teruraj kondi&ccirc;oj
        la a&ubreve;to&scirc;oseon al Moskvo
        <fnt>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2011/010390.php">Kvarlingva muzeo</url></vrk>
              <bib>Monato</bib>
          <lok>2011/07, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        okazis renkonti&gcirc;o de uralaj kaj siberiaj esperantistoj
        kun 33 partoprenantoj,
        plejparte el Tjumeno kaj <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 6 (104)</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi volas scii, kian veteron ni havos post 2-3 tagoj,
        mi informi&gcirc;as pri la
        hodia&ubreve;a vetero en Omsko, se min interesas la vetero
        sekvasemajna, mi rigardas
        al <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Jekaterinburg</trd>
    <trd lng="en">Yekaterinburg</trd>
    <trd lng="fr">Iekaterinbourg</trd>
    <trd lng="nl">Jekaterinburg</trd>
  <trd lng="ru">&c_Je;&c_k;&c_a;&c_t;&c_je;&c_r;&c_i;&c_n;&c_b;&c_u;&c_r;&c_g;</trd>
  </snc>
  <trd lng="be">&c_Je;&c_k;&c_a;&c_c;&c_ja;&c_r;&c_y;&c_n;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="ja">&#x30A8;&#x30AB;&#x30C6;&#x30EA;&#x30F3;&#x30D6;&#x30EB;&#x30AF;</trd>
  <trd lng="zh">&#x53F6;&#21345;&#25463;&#29747;&#22561; <pr>y&#xE8;k&#x1CE;ji&#xE9;l&#xED;nb&#x1CE;o</pr></trd>
</drv>

