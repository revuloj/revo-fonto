<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>dinasti</rad>/o
</kap>
<drv mrk="dinast.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Sinsekvo da regnestroj el sama familio, heredantaj la tronon unu de
      alia:
      <ekz>
        Liu Bang fondis Han-<tld/>on (206 a.K.&ndash;220 p.K.)
        <fnt><bib>EncCxi</bib> <lok>&ccirc;ap.. &leftquot;Historio&rightquot;, p.
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_s;&c_t;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Dynastie</trd>,
    <trd>Herrscherhaus</trd>,
    <trd>Herrschergeschlecht</trd>
  </trdgrp>
  <trd lng="es">dinast&iacute;a</trd>
  <trd lng="fr">dynastie</trd>
  <trdgrp lng="hu">
    <trd>uralkod&oacute;h&aacute;z</trd>,
    <trd>dinasztia</trd>
  </trdgrp>
  <trd lng="pt">dinastia</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_s;&c_t;&c_i;&c_ja;</trd>
</drv>
</art>
