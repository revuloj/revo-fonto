<vortaro>
<art mrk="$Id: pterosauxr.xml,v 1.3 2022/03/26 15:53:39 afido Exp $">
<kap>
  <rad>pterosa&ubreve;r</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="pterosauxr.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="pterosauxr.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo><uzo tip="fak">PAL</uzo>
    <dif>
      <ref tip="dif" cel="sauxr.flug0oj.ZOO">Flugsa&ubreve;roj</ref>:
      <ekz>
        <tld/>oj ne estis dinosa&ubreve;roj, sed ili ja estis proksimaj parencoj
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2019/08/14/la-vivo-malantau-fosilioj/">La Vivo malanta&ubreve; Fosilioj</url></vrk>
          <lok>Scivolemo, 2019-08-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pterosaurus</trd>
  <trd lng="fr">pt&eacute;rosaures</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x442;&#x435;&#x440;&#x43E;&#x437;&#x430;&#x432;&#x440;&#x438;</trd>,
    <trd>&#x43B;&#x456;&#x442;&#x430;&#x44E;&#x447;&#x456; &#x44F;&#x449;&#x435;&#x440;&#x438;</trd>
  </trdgrp>
</drv>
</art>

