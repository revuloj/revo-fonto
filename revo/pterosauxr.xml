<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pterosauxr.xml,v 1.6 2024/02/17 11:11:10 afido Exp $">
<kap>
  <rad>pterosa&ubreve;r</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="pterosauxr.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="pterosauxr.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo><uzo tip="fak">PAL</uzo>
    <dif>
      <ref tip="dif" cel="sauxr.flug0oj.ZOO">Flugsa&ubreve;roj</ref>:
      <ekz>
        <tld/>oj ne estis dinosa&ubreve;roj, sed ili ja estis proksimaj parencoj
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2019/08/14/la-vivo-malantau-fosilioj/">La
          Vivo malanta&ubreve; Fosilioj</url></vrk>
          <lok>Scivolemo, 2019-08-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_e;&c_r;&c_a;&c_z;&c_a;&c_w;&c_r;&c_y;</trd>
  <trd lng="de">Pterosaurus</trd>
  <trd lng="fr">pt&eacute;rosaures</trd>
  <trd lng="pl">pterozaury</trd>
  <trd lng="ro">pterozaurii</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_t;&c_je;&c_r;&#x43E;&c_z;&c_a;&c_v;&c_r;&c_i;</trd>,
    <trd>&#x43B;&c_ib;&c_t;&c_a;&#x44E;&c_ch;&c_ib; &#x44F;&c_shch;&c_je;&c_r;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7FFC;&#x9F8D;&#x76EE; <pr>y&#xEC;l&#xF3;ngm&#xF9;</pr></trd>,
    <trd>&#x7FFC;&#x9F99;&#x76EE; <pr>y&#xEC;l&#xF3;ngm&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>

