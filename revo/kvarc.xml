<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kvarc</rad>/o
</kap>
<drv mrk="kvarc.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Pura siliko.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_c;</trd>
  <trdgrp lng="de">
     <trd>Quarz</trd>,
     <trd>Kiesels&auml;ure</trd>
  </trdgrp>
  <trd lng="en">quartz</trd>
  <trd lng="fr">quartz</trd>
  <trd lng="hu">kvarc</trd>
  <trd lng="nl">kwarts</trd>
  <trd lng="pl">kwarc</trd>
  <trd lng="pt">quartzo</trd>
  <trd lng="ro">cuar&#539;</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_c;</trd>
</drv>
<drv mrk="kvarc.vitroida0o">
  <kap>vitroida <tld/>o</kap>
  <snc>
    <dif>
      Plej pura formo de nature trovita kvarco.
    </dif>
  </snc>
  <trd lng="cs">horsk&yacute; k&rcaron;i&scaron;&tcaron;&aacute;l</trd>
  <trd lng="de">Bergkristall</trd>
  <trdgrp lng="en">
    <trd>vitreous quartz</trd>,
    <trd>fused quartz</trd>
  </trdgrp>
  <trd lng="hu">hegyikrist&aacute;ly</trd>
  <trdgrp lng="nl">
    <trd>bergkristal</trd>,
    <trd>dragoniet</trd>
  </trdgrp>
  <trd lng="pl">kryszta&lstroke; g&oacute;rski </trd>
  <trd lng="pt">quartzo v&iacute;treo</trd>
  <trd lng="ro">cristal de st&acirc;nc&abreve;</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_y;&c_j; <ind>&c_h;&c_r;&c_u;&c_s;&c_t;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="sk">horsk&yacute; kri&scaron;t&aacute;&lcaron;</trd>
</drv>
</art>
