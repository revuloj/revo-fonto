<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvarc.xml,v 1.27 2023/12/03 09:59:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kvarc</rad>/o
</kap>
<drv mrk="kvarc.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="minera.0o">Mineralo</ref>, dua plej ofta
      en la ter&scirc;elo post feldspato, el silicia oksido kun trinombra
      kristala strukturo:
      <ekz>
        oni turnas sablon kun akvo kaj la centrifuga forto koncentras
        sablerojn pli pezajn ol <tld/>o en unu lokon
        <fnt>
          <bib>Monato</bib> <aut>last</aut>
          <vrk><url ref="&Monato;/2010/009963.php">Ora febro en Litovio</url></vrk>
        </fnt>;
      </ekz><ekz>
        granito estas <klr>[&#x2026;]</klr> miksa&jcirc;o de glimo, feldspato kaj <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23403">Granito</url></lok>
        </fnt>;
      </ekz><ekz>
        modernaj horlo&gcirc;oj uzas osciladojn de kristala <tld/>o, kies
        re<klr>[s]</klr>onanca frekvenco estas konstanta
        <fnt>
          <bib>Kon11</bib> <aut>Cecilia Prudkin</aut>
          <vrk>Atom-horlo&gcirc;oj: modernaj temp-mezuriloj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
       <ref cel="silik.0o">siliko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_c;</trd>
  <trdgrp lng="de">
     <trd>Quarz</trd>,
     <trd>Kiesels&auml;ure</trd>
  </trdgrp>
  <trd lng="en">quartz</trd>
  <trd lng="fr">quartz</trd>
  <trd lng="hu">kvarc</trd>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x82F1;  <pr>&#x305B;&#x304D;&#x3048;&#x3044;</pr></trd>,
    <trd>&#x6C34;&#x6676;  <pr>&#x3059;&#x3044;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">kwarts</trd>
  <trd lng="pl">kwarc</trd>
  <trd lng="pt">quartzo</trd>
  <trd lng="ro">cuar&#x21B;</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_c;</trd>
  <trd lng="uk">&#x43A;&#x432;&#x430;&#x440;&#x446;</trd>
  <trd lng="zh">&#x77F3;&#x82F1; <pr>sh&#xED;y&#x12B;ng</pr></trd>
</drv>
<drv mrk="kvarc.vitroida0o">
  <kap>vitroida <tld/>o <fnt><bib>PIV1</bib></fnt>,
    <var><kap>vitreca <tld/>o <fnt><bib>PIV2</bib></fnt></kap></var>
  </kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Plej pura formo de nature trovita
      <ref tip="super" cel="kvarc.0o">kvarco</ref>:
      <ekz>
        homoj konas valorajn &scirc;tonojn jam dum almena&ubreve; 7000 jaroj, la
        unuaj estis ametisto, vitroida <tld/>o, sukceno, grenato,
        jado, jaspo, koraloj, lazurito, perloj, serpenteno, smeraldo
        kaj turkiso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50124">Gemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/bc/Quartz%2C_Bergkristall.jpg/219px-Quartz%2C_Bergkristall.jpg" prm="CC BY-SA 2.0">vitroida <tld/>o
        <fnt>
          <aut>Peter Forster, originalo el Flickr: Quartz, Bergkristall</aut>
          <lok><url ref="&WCW;/File:Quartz,_Bergkristall.jpg">&WCW;/File:Quartz,_Bergkristall.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_n;&c_y; <ind>&c_k;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="cs">horsk&yacute; k&rcaron;i&scaron;&tcaron;&aacute;l</trd>
  <trd lng="de">Bergkristall</trd>
  <trdgrp lng="en">
    <trd>vitreous quartz</trd>,
    <trd>fused quartz</trd>
  </trdgrp>
  <trd lng="hu">hegyikrist&aacute;ly</trd>
  <trdgrp lng="nl">
    <trd>bergkristal</trd>,
    <trd>dragoniet</trd>
  </trdgrp>
  <trd lng="pl">kryszta&lstroke; g&oacute;rski </trd>
  <trd lng="pt">quartzo v&iacute;treo</trd>
  <trd lng="ro">cristal de st&acirc;nc&abreve;</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_y;&c_j; <ind>&c_h;&c_r;&c_u;&c_s;&c_t;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="sk">horsk&yacute; kri&scaron;t&aacute;&lcaron;</trd>
  <trd lng="uk">&#x433;&#x456;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43A;&#x440;&#x438;&#x448;&#x442;&#x430;&#x43B;&#x44C;</trd>
  <trd lng="zh">&#x6C34;&#x6676; <pr>shu&#x1D0;j&#x12B;ng</pr></trd>
</drv>
</art>
