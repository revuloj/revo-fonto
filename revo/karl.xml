<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karl.xml,v 1.6 2022/06/03 06:36:33 revo Exp $">
<kap>
    <rad>Karl</rad>/o <fnt>Z</fnt> <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="karl.0o">
  <kap><tld/>o</kap>
  <snc mrk="karl.0o.SNC">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Varia&jcirc;o de <ref tip="sin" cel="karol1.0o">Karolo</ref>:
      <ekz>
        <tld/>o estis tre pensema knabeto kaj jam havis
        siajn precizajn ideojn pri multaj aferoj
        <fnt>
          <aut>E. Privat</aut>,
          <vrk>Karlo</vrk>,
          <lok>1926</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_a;&c_r;&c_l;</trd>,
    <trd>&c_K;&c_a;&c_r;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Karl</trd>
  <trdgrp lng="en">
    <trd>Carl</trd>,
    <trd>Carlo</trd>, <trd>Carlos</trd>
  </trdgrp>
  <trd lng="fr">Charles</trd>
  <trd lng="pl">Karol</trd>
  <trd lng="uk">&#x41A;&c_a;&c_r;&#x43B;</trd>
</drv>
</art>

