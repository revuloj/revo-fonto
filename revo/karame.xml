<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karame.xml,v 1.26 2023/12/03 08:00:23 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>karamel</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="karame.0o">
  <kap><tld/>o</kap>
  <snc mrk="karame.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Rostita bruni&gcirc;inta <ref tip="super" cel="suker.0o">sukero</ref>:
      <ekz>
        <tld/>a siropo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;atis brunajn kafajn volva&jcirc;ojn kun <tld/>a kovrilo
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kand.0o">kando</ref>
  </snc>
  <snc mrk="karame.0o.bombono">
    <dif>
      <ref tip="dif" cel="karame.0bombono"><tld lit="K"/>bombono</ref>:
      <ekz>
        li neniam venis al Novponto sen kunporti saketon da <tld/>oj
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la granda <tld/>o en &scirc;ia bu&scirc;o, &scirc;i jam a&ubreve;tas en la kontra&ubreve;an direkton
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Karamelle</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_m;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;</trd>
  <trd lng="ca">caramel</trd>
  <trdgrp lng="cs">
    <trd>cukrov&eacute; barvivo</trd>,
    <trd>karamel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Karamell</trd>,
    <trd>Karamellzucker</trd>
  </trdgrp>
  <trd lng="en">caramel</trd>
  <trd lng="es">caramelo</trd>
  <trd lng="fr">caramel</trd>
  <trd lng="hu">karamella</trd>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30E1;&#x30EB;</trd>
  <trd lng="nl">caramel</trd>
  <trd lng="pl">karmel</trd>
  <trd lng="ro">caramel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cukrov&eacute; farbivo</trd>,
    <trd>karamel</trd>
  </trdgrp>
  <trd lng="tr">karamela</trd>
  <trd lng="uk">&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;&c_mol;</trd>
  <trd lng="zh">&#x7126;&#x7CD6; <pr>ji&#x101;ot&#xE1;ng</pr></trd>
</drv>

<drv mrk="karame.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Rosti sukersiropon al <tld/>o; kovri per <tld/>o:
      <ekz>
        deserto <klr>[&#x2026;]</klr> kovrita de <tld/>igita sukero
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php">Sidante en pudingo, mi alvokas miajn savantojn!</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="kand.0i">kandi</ref>
  </snc>
  <trd lng="de">karamellisieren</trd>
</drv>

<drv mrk="karame.0bombono">
  <kap>
    <tld/>bombono
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="bombon.0o">Bombono</ref> el <tld/>o,
      eventuale kun aldonitaj aromoj:
      <ekz>
        Jo&ccirc;jo ricevis <tld/>bonbonon
        <fnt>
          <bib>ChC</bib>
          <lok>17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Karamellbonbon</trd>
  <trd lng="zh">&#x592A;&#x5983;&#x7CD6; <pr>t&#xE0;if&#x113;it&#xE1;ng</pr></trd>
</drv>

</art>
