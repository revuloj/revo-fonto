<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karame.xml,v 1.20 2021/01/27 17:19:10 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>karamel</rad>/o
</kap>
<drv mrk="karame.0o">
  <kap><tld/>o</kap>
  <snc mrk="karame.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Rostita bruni&gcirc;inta <ref tip="super" cel="suker.0o">sukero</ref>:

      <ekz>
        li neniam venis al Novponto sen kunporti saketon da <tld/>oj a&ubreve; alian bongusta&jcirc;on
        <fnt>
          <aut>Stellan Engholm</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=homoj-sur-la-tero"
              >Homoj sur la tero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li eltiris el la po&scirc;o <tld/>ojn
        <klr>(<tld/>bombonojn)</klr>
        <fnt><aut>Lina Gabrielli</aut>, <vrk>Karnavalo. &dash; Romo:
        International Center of Arts, 1973</vrk>, <lok>p.
        11a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a siropo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/010973.php"
              >Bombo-bankroto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        deserto <klr>[&#8230;]</klr> kovrita de <tld/>igita sukero
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="https://www.monato.be/2008/009292.php"
              >Sidante en pudingo, mi alvokas miajn savantojn!</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kand.0o">kando</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_m;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;</trd>
  <trd lng="ca">caramel</trd>
  <trdgrp lng="cs">
    <trd>cukrov&eacute; barvivo</trd>,
    <trd>karamel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Karamell</trd>,
    <trd>Karamellzucker</trd>
  </trdgrp>
  <trd lng="en">caramel</trd>
  <trd lng="es">caramelo</trd>
  <trd lng="fr">caramel</trd>
  <trd lng="hu">karamella</trd>
  <trd lng="nl">caramel</trd>
  <trd lng="pl">karmel</trd>
  <trd lng="ro">caramel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cukrov&eacute; farbivo</trd>,
    <trd>karamel</trd>
  </trdgrp>
  <trd lng="tr">karamela</trd>
</drv>
</art>
