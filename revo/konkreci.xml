<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konkreci.xml,v 1.9 2023/12/03 09:58:56 revo Exp $">
<kap>
  <rad>konkreci</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="konkreci.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkreci.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Poioma kunsolidi&gcirc;o de substancoj:
      <ekz>
        vi vidos mirindajn <tld/>ojn en formo de gigantaj meduzoj 
        (stalagmitajn fluojn)
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://grottesdesaudides.free.fr/esperanto.htm">
          Groto de la Audides</url></vrk>,
          <lok>[2009?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akreci.0o">akrecio</ref>,
      <ref cel="konkrement.0o.FIZL">konkremento</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_r;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">concreci&oacute;<klr> (patol.)</klr></trd>
  <trdgrp lng="cs">
    <trd>konkrece</trd>,
    <trd>konkr&eacute;cie</trd>
  </trdgrp>
  <trd lng="de">Konkretion</trd>
  <trd lng="en">concretion</trd>
  <trd lng="fr">concr&eacute;tion</trd>
  <trd lng="pl">konkrecja</trd>     
  <trd lng="ro">concre&#x21B;iune</trd>
  <trd lng="sk">konkr&eacute;cia</trd>
  <trd lng="zh">&#x5177;&#x4F53;&#x5316; <pr>j&#xF9;t&#x1D0;hu&#xE0;</pr></trd>
</drv>
</art>

