<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>har</rad>/o
</kap>
<drv mrk="har.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Filamento kreskanta el la ha&ubreve;to de mamulo,  pli malpli dense
      kovranta la korpon de bestoj k de homo:
      <ekz>
        tondi <tld/>ojn;
      </ekz>
      <ekz>
        kombi al si la <tld/>ojn;
      </ekz>
      <ekz>
        maldika kiel <tld/>o;
      </ekz>
      <ekz>
        la <tld/>oj de negroj similas stupon;
      </ekz>
      <ekz>
        blonda, griza <tld/>aro;
      </ekz>
      <ekz>
        la <tld/>aran&gcirc;o de la virinoj multe
        &scirc;an&gcirc;i&gcirc;is de kelkaj jaroj <klr>(f)</klr>;
      </ekz>
      <ekz>
        disfendi, dissplitigi  harojn
        <klr>(subtila&ccirc;e diskuti)</klr>;
      </ekz>
      <ekz>
        lia vivo estas sur <tld/>eto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pendas de, el <tld/>eto<fnt>Z</fnt>
        <klr>(estas en ekstrema dan&gcirc;ero)</klr>;
      </ekz>
      <ekz>
        li estas <tld/>plena kiel urso;
      </ekz>
      <ekz>
        <tld/>stariga eraro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="faden.0o">fadeno</ref>,
      <ref cel="fibr.0o">fibro</ref>,
      <ref cel="filame.0o">filamento</ref>,
      <ref cel="peruk.0o">peruko</ref>,
      <ref cel="tuber.har0o">hartubero</ref>,
      <ref cel="bukl.0o">buklo</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Fibra elkreska&jcirc;o el la epidermo de planto
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>vlas</trd>,
    <trd>chlup</trd>
  </trdgrp>
  <trd lng="de">Haar</trd>
  <trd lng="en">hair</trd>
</drv>
<drv mrk="har.0aro">
  <kap><tld/>aro</kap>
  <snc num="1">
    <fnt>Z</fnt>
    <dif>
      Peruko. 
    </dif>
    <trd lng="de">Per&uuml;cke</trd>
  </snc>
  <snc num="2">
    <dif>
      Kap<tld/>oj. 
    </dif>
  </snc>
  <trd lng="de">Haar</trd>
</drv>
<drv mrk="har.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Dika <tld/>o de iuj bestoj:
      <ekz>
        <tld/>egoj de apro, de &ccirc;evala vosto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Borste</trd>
  <trd lng="en">bristle</trd>
</drv>
<drv mrk="har.0ligo">
  <kap><tld/>ligo</kap>
  <snc>
    <dif>
      Kunplektitaj tutoj da longaj kap<tld/>oj. 
    </dif>
    <trd lng="de">Zopf</trd>
    <trd lng="en">plait</trd>
  </snc>
</drv>
<drv mrk="har.kol0aro">
  <kap>kol<tld/>aro</kap>
  <snc>
    <dif>
      Longaj pendantaj <tld/>oj &ccirc;e kolo de iuj bestoj (leono,
      &ccirc;evalo).
    </dif>
  </snc>
  <trd lng="de">M&auml;hne</trd>
  <trd lng="en">mane</trd>
</drv>
<drv mrk="har.lip0oj">
  <kap>lip<tld/>oj</kap>
  <snc>
    <dif>
      <tld/>oj &ccirc;e la supera lipo:
      <ekz>
        bruligi al si la lip<tld/>ojn<fnt>Z</fnt>
        <klr>(agi nesufi&ccirc;e singarde k devi penti pri tio)</klr>.
      </ekz> 
    </dif>
    <trdgrp lng="de">
      <trd>Schnauzer</trd>,
      <trd>Bart<klr>(haare)</klr></trd>
    </trdgrp>
    <trd lng="en">moustache</trd>
  </snc>
</drv>
<drv mrk="har.okul0oj">
  <kap>okul<tld/>oj</kap>
  <snc>
    <dif>
      <tld/>oj &ccirc;e la rando de palpebroj. 
    </dif>
  </snc>
  <trd lng="de">Wimpern</trd>
  <trd lng="en">eyebrow</trd>
</drv>
<drv mrk="har.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Ne havanta <tld/>ojn, perdinta la <tld/>ojn:
      <ekz>
        sen<tld/>a kapo, maljunulo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kalv.0a">kalva</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de"><trd>haarlos</trd>, <trd>glatzk&ouml;pfig</trd></trdgrp>
  <trd lng="en">bald</trd>
</drv>
<drv mrk="har.vang0oj">
  <kap>vang<tld/>oj</kap>
  <snc>
    <dif>
      <tld/>oj kreskantaj &ccirc;e la vangoj.  
    </dif>
  </snc>
  <trd lng="de">Bart<klr>(haare)</klr></trd>
  <trd lng="en">side-whiskers</trd>
</drv>
</art>
