<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>har</rad>/o
</kap>

<drv mrk="har.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="har.0o.mamulo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="filame.0o">Filamento</ref> kreskanta el la
      ha&ubreve;to de <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      kaj pli-malpli dense kovranta ilian korpon:
      <ekz>
        <tld/>o estas tre maldika
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        tondi <tld/>ojn;
      </ekz>
      <ekz>
        kombi al si la <tld/>ojn;
      </ekz>
      <ekz>
        la <tld/>oj de negroj similas stupon;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>disfendi, dissplitigi harojn</ind>
        <klr>(subtila&ccirc;e diskuti)</klr>;
        <trd lng="br">spazha&ntilde; laou</trd>
        <trd lng="de">Haare <ind>spalten</ind></trd>
        <trd lng="fr">couper les <ind>cheveux</ind> en quatre</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        lia vivo estas sur <tld/>eto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pendas de, el <tld/>eto<fnt>Z</fnt>
        <klr>(estas en ekstrema dan&gcirc;ero)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        li estas <ind><tld/>plena</ind> kiel urso;
        <trd lng="br">blevek</trd>
        <trd lng="fr">poilu</trd>
      </ekz>
      <ekz>
        <ind><tld/>stariga</ind> eraro.  
        <trd lng="br">hirisus</trd>
        <trd lng="de">haarstr&auml;ubend</trd>
        <trdgrp lng="fr">
          <trd>h&eacute;rissant</trd>,
          <trd>horripilant</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>hajsz&aacute;l</trd>,
      <trd>sz&odblac;rsz&aacute;l</trd>
    </trdgrp>
    <refgrp tip="vid">
      <ref cel="bukl.0o">buklo</ref>,
      <ref cel="faden.0o">fadeno</ref>,
      <ref cel="fibr.0o">fibro</ref>,
      <ref cel="peruk.0o">peruko</ref>
    </refgrp>
    <trd lng="pt">cabelo</trd>
    <trd lng="sv">h&aring;rstr&aring;</trd>
  </snc>
  <snc mrk="har.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Fibra elkreska&jcirc;o el la epidermo de
      <ref tip="malprt" cel="veget.0ajxo">vegeta&jcirc;o</ref>.
    </dif>
    <trd lng="hu">sz&odblac;r</trd>
    <trd lng="pt">p&ecirc;lo<klr tip="amb"> (de vegetal)</klr></trd>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_malmol;&c_m;</trd>
  <trd lng="br">blevenn</trd>
  <trdgrp lng="cs">
    <trd>vlas</trd>,
    <trd>chlup</trd>
  </trdgrp>
  <trd lng="de">Haar</trd>
  <trd lng="en">hair</trd>
  <trdgrp lng="es">
    <trd>cabello</trd>,
    <trd>pelo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cheveu</trd>,
    <trd>poil</trd>
  </trdgrp>
  <trd lng="nl">haar</trd>
  <trd lng="pl">w&lstroke;os</trd>
  <trdgrp lng="pt">
    <trd>cabelo</trd>,
    <trd>p&ecirc;lo</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_s;</trd>
  <trd lng="tp">linja</trd>
</drv>

<drv mrk="har.0aro">
  <kap><ofc>*</ofc><tld/>aro</kap>
  <snc mrk="har.0aro.FRIZ">
    <dif>
      Tuto de la kapaj <tld/>oj de iu homo,
      de la <tld/>oj de iu besto:
      <ekz>
        lia korve nigra <tld/>aro
        <fnt><bib>Kiso</bib>, <lok>poetoj kaj homoj</lok></fnt>;
      </ekz>
      <ekz>
        blonda, griza <tld/>aro.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>blev</trd>,
      <trd>blevenn<klr> [bestoj]</klr>
      <klr tip="ind"> (hollad blev ar bronneged)</klr></trd>,
      <trd>pennad blev</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hair <klr tip="amb">(person)</klr></trd>,
      <trd>coat <klr tip="amb">(animal)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cabellera</trd>,
      <trd>pelaje<klr tip="ind"> (animal)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>chevelure</trd>,
      <trd>cheveux</trd>,
      <trd>pelage<klr tip="ind"> (d'un animal)</klr></trd>
    </trdgrp>
    <trd lng="hu">haj</trd>
    <trdgrp lng="nl">
      <trd>haardos</trd>,
      <trd>haar</trd>
    </trdgrp>
    <trd lng="pl">ow&lstroke;osienie</trd>
    <trd lng="ru">&c_sh;&c_je;&c_v;&c_je;&c_l;&c_ju;&c_r;&c_a;</trd>
    <trd lng="sv">h&aring;r</trd>
  </snc>
  <snc mrk="har.0aro.peruko">
    <uzo tip="stl">ARK</uzo>
    <fnt>Z</fnt>
    <ref tip="dif" cel="peruk.0o">peruko</ref>
    <trd lng="br">perukenn</trd>
    <trd lng="de">Per&uuml;cke</trd>
    <trd lng="es">peluca</trd>
    <trd lng="fr">perruque</trd>
    <trd lng="pl">peruka</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_k;</trd>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_a;</trd>
  <trd lng="de">Haar</trd>
  <trdgrp lng="hu">
    <trd>par&oacute;ka</trd>,
    <trd>vend&eacute;ghaj</trd>
  </trdgrp>
  <trd lng="pt">cabeleira</trd>
</drv>

<drv mrk="har.H0o">
  <kap>Berenica <tld lit="H"/>o</kap>
  <mlg>Com</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="super" cel="stel.0figuro">stelfiguro
      </ref> (<trd lng="la">Coma Berenices</trd>).
    </dif>
  </snc>
  <trd lng="de">Haar der Berenike</trd>
  <trd lng="en">Coma Berenices</trd>
  <trd lng="es">Cabellera de Berenices</trd>
  <trd lng="pl">Warkocz Bereniki</trd>
</drv>

<drv mrk="har.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc mrk="har.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Dika <tld/>o de iuj bestoj:
      <ekz>
        persistemaj &ccirc;evaloj kun longaj <tld/>egoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>egoj de apro.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">reun</trd>
  <trd lng="de">Borste</trd>
  <trd lng="en">bristle</trd>
  <trd lng="es">cerda</trd>
  <trdgrp lng="fr">
    <trd>crin</trd>,
    <trd>soie<klr tip="ind"> (poil)</klr></trd>
  </trdgrp>
  <trd lng="hu">s&ouml;rte</trd>
  <trd lng="nl">borstel</trd>
  <trd lng="pl">sier&sacute;&cacute;</trd>
  <trd lng="pt">crina</trd>
  <trd lng="ru">&c_shch;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="har.0ligo">
  <kap><ofc>*</ofc> <tld/>ligo</kap>
  <snc>
    <dif>
      Kunplektitaj tufoj da longaj kap<tld/>oj:
      <ekz>
        dika plekta&jcirc;o de nigraj <tld/>ligoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;<klr tip="amb">
  (&c_k;&c_o;&c_s;&c_a;)</klr></trd>
  <trd lng="br">plezhenn<klr tip="ind"> (blev)</klr></trd>
  <trd lng="de">Zopf</trd>
  <trd lng="en">plait</trd>
  <trd lng="es">trenza</trd>
  <trd lng="fr">tresse<klr tip="ind"> (de cheveux)</klr></trd>
  <trdgrp lng="hu">
    <trd>copf</trd>,
    <trd>hajfonat</trd>
  </trdgrp>
  <trd lng="nl">vlecht</trd>
  <trd lng="pl">warkocz</trd>
  <trd lng="pt">tran&ccedil;a<klr tip="amb"> (de cabelo)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a;
  <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
</drv>

<drv mrk="har.kol0aro">
  <kap>kol<tld/>aro, <ofc>*</ofc>kol<tld/>oj</kap>
  <snc mrk="har.kol0aro.ANA">
    <dif>
      Longaj pendantaj <tld/>oj &ccirc;e kolo de iuj bestoj: leono,
      &ccirc;evalo...
    </dif>
  </snc>
  <trd lng="br">moue</trd>
  <trd lng="de">M&auml;hne</trd>
  <trd lng="en">mane</trd>
  <trdgrp lng="es">
    <trd>melena</trd>,
    <trd>crin</trd>
  </trdgrp>
  <trd lng="fr">crini&egrave;re</trd>
  <trd lng="hu">s&ouml;r&eacute;ny</trd>
  <trd lng="nl">manen</trd>
  <trd lng="pl">grzywa</trd>
  <trdgrp lng="pt">
   <trd>crina</trd>,
   <trd>juba</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_v;&c_a;</trd>
  <trd lng="sv">man</trd>
</drv>

<drv mrk="har.lip0oj">
  <kap><ofc>*</ofc>lip<tld/>oj</kap>
  <snc mrk="har.lip0oj.ANA">
    <dif>
      <tld lit="H"/>oj kreskantaj &ccirc;e la supera lipo:
      <ekz>
        liaj lip<tld/>oj estas pli grizaj, ol liaj vangharoj
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        bruligi al si la lip<tld/>ojn
        <klr>(agi nesufi&ccirc;e singarde kaj ne atingi la esperatan
        rezulton)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">mourro&ugrave;</trd>
  <trd lng="be">&c_v;&c_u;&c_s;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Oberlippenbarthaare</trd>,
    <trd>Schnauzer</trd>,
    <trd>Bart<klr>(haare)</klr></trd>
  </trdgrp>
  <trd 
lng="el">&my;&omikron;&ypsilon;&sigma;&tau;&alfa_ton;&kappa;&jota;</trd>
  <trd lng="en">moustache</trd>
  <trd lng="es">bigote</trd>
  <trd lng="fr">moustache</trd>
  <trd lng="he">&shin;&pe;&fmem;</trd>
  <trd lng="hu">bajusz</trd>
  <trd lng="nl">snor</trd>
  <trd lng="pl">w&aogonek;sy</trd>
  <trd lng="pt">bigode</trd>
  <trd lng="ru">&c_u;&c_s;&c_y;</trd>
  <trd lng="sv">mustasch</trd>
</drv>

<drv mrk="har.okul0oj">
  <kap><ofc>*</ofc>okul<tld/>oj</kap>
  <snc mrk="har.okul0oj.ANA">
    <dif>
      <tld lit="H"/>oj &ccirc;e la rando de palpebroj:
      <ekz>
        li <klr>...</klr> vidis larmojn, kiuj tremis inter &scirc;iaj
        okul<tld/>oj
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">malgudenno&ugrave;</trd>
  <trd lng="de">Wimpern</trd>
  <trd lng="en">eyebrows</trd>
  <trd lng="es">cejas</trd>
  <trd lng="fr">cils</trd>
  <trd lng="hu">szempilla</trd>
  <trd lng="nl">wimpers</trd>
  <trd lng="pl">rz&eogonek;sy</trd>
  <trdgrp lng="pt">
   <trd>c&iacute;lio</trd>,
   <trd>pestana</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_n;&c_i;&c_c;&c_y;</trd>
</drv>

<drv mrk="har.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="har.sen0a.ANA">
    <dif>
      Ne havanta <tld/>ojn, perdinta la <tld/>ojn:
      <ekz>
        la kruela patro elektis por &scirc;i malbelegan, maljunan,
        sen<tld/>an baronon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
        Devjatnin), 2</lok></fnt>.
      </ekz>
      <ekz>
        sen<tld/>a kapo.
      </ekz>
    </dif>
    <ref tip="sin" cel="kalv.0a">kalva</ref>
  </snc>
  <trd lng="br">moal</trd>
  <trdgrp lng="de">
    <trd>haarlos</trd>,
    <trd>glatzk&ouml;pfig</trd>
  </trdgrp>
  <trd lng="en">bald</trd>
  <trd lng="es">calvo</trd>
  <trdgrp lng="fr">
    <trd>chauve</trd>,
    <trd>glabre</trd>
  </trdgrp>
  <trd lng="hu">kopasz</trd> 
  <trd lng="nl">kaal</trd>
  <trd lng="pl">&lstroke;ysy</trd>
  <trdgrp lng="pt">
   <trd>careca<klr tip="ind"> (adj.)</klr></trd>,
   <trd>calvo<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_o;&c_l;&c_o;&c_s;&c_y;&c_j;
  </trd>
</drv>

<drv mrk="har.vang0oj">
  <kap><ofc>*</ofc>vang<tld/>oj</kap>
  <snc mrk="har.vang0oj.ANA">
    <dif>
      <tld lit="H"/>oj kreskantaj &ccirc;e la vangoj.
    </dif>
  </snc>
  <trd lng="br">blev kostez ar skouarn</trd>
  <trd lng="de">Bart<klr>(haare)</klr></trd>
  <trdgrp lng="en">
    <trd>sideburns</trd>,
    <trd>side-whiskers</trd>
  </trdgrp>
  <trd lng="es">patillas</trd>
  <trd lng="fr">favoris<klr tip="ind"> (barbe)</klr></trd>
  <trd lng="hu">szak&aacute;ll</trd>
  <trd lng="nl">bakkebaard</trd>
  <trdgrp lng="pl">
    <trd>bokobrody</trd>,
    <trd>baki</trd>,
    <trd>pejsy</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>su&iacute;&ccedil;as</trd>,
   <trd>barbas</trd>
  </trdgrp>                     
  <trd lng="ru">&c_b;&c_a;&c_k;&c_je;&c_n;&c_b;&c_a;&c_r;&c_d;&c_y;
  </trd>
</drv>
<adm>
  Enkonduki ankaux la singularan formon por:
  kol~aro, okul~o. <aut>MB</aut>
</adm>
</art>
