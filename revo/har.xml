<drv mrk="har.0aro">
  <kap><ofc>*</ofc><tld/>aro</kap>
  <snc mrk="har.0aro.FRIZ">
    <dif>
      Tuto de la kapaj <tld/>oj de iu homo,
      de la <tld/>oj de iu besto:
      <ekz>
        lia korve nigra <tld/>aro
        <fnt><bib>Kiso</bib>, <lok>poetoj kaj homoj</lok></fnt>;
      </ekz><ekz>
        blonda, griza <tld/>aro;
      </ekz>
      <ekz>
        &ccirc;io &ccirc;e li, lia vesto, lia viza&gcirc;o, lia <tld/>aro estis griza kiel &scirc;tono
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Minejo de l' Bildoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ludema buklo de &scirc;ia ka&scirc;tankolora <tld/>aro falis sur &scirc;ian glatan frunton
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>blev</trd>,
      <trd>blevenn<klr> [bestoj]</klr>
        <klr tip="ind"> (hollad blev ar bronneged)</klr></trd>,
      <trd>pennad blev</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hair <klr tip="amb">(person)</klr></trd>,
      <trd>coat <klr tip="amb">(animal)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cabellera</trd>,
      <trd>pelaje<klr tip="ind"> (animal)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>chevelure</trd>,
      <trd>cheveux</trd>,
      <trd>pelage<klr tip="ind"> (d'un animal)</klr></trd>
    </trdgrp>
    <trd lng="hu">haj</trd>
    <trdgrp lng="nl">
      <trd>haardos</trd>,
      <trd>haar</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>chioma</trd>, 
      <trd>capelli</trd>,
      <trd>capigliatura</trd>, 
      <trd>manto<klr> (di animali)</klr></trd>, 
      <trd>pelame<klr> (di animali)</klr></trd>
    </trdgrp>
    <trd lng="pl">ow&lstroke;osienie</trd>
    <trd lng="ru">&c_sh;&c_je;&c_v;&c_je;&c_l;&c_ju;&c_r;&c_a;</trd>
    <trd lng="sv">h&aring;r</trd>
  </snc>
  <snc mrk="har.0aro.peruko">
    <uzo tip="stl">ARK</uzo>
    <fnt>Z</fnt>
    <ref tip="dif" cel="peruk.0o">peruko</ref>
    <trd lng="br">perukenn</trd>
    <trd lng="de">Per&uuml;cke</trd>
    <trd lng="es">peluca</trd>
    <trd lng="fr">perruque</trd>
    <trd lng="it">parrucca</trd>
    <trd lng="pl">peruka</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_k;</trd>
  </snc>
  <ref tip="hom" cel="harar.0o">Hararo (&ccirc;efurbo)</ref>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xFA4;&#xF74;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>k&scaron;tice</trd>,
    <trd>srst</trd>,
    <trd>vlasy</trd>
  </trdgrp>
  <trd lng="de">Haar</trd>
  <trdgrp lng="hu">
    <trd>par&oacute;ka</trd>,
    <trd>vend&eacute;ghaj</trd>
  </trdgrp>
  <trd lng="pt">cabeleira</trd>
  <trdgrp lng="sk">
    <trd>srs&tcaron;</trd>,
    <trd>vlasy</trd>,
    <trd>&scaron;tica</trd>
  </trdgrp>
</drv>

<drv mrk="har.berenicaH0aro">
  <kap>Berenica <tld lit="H"/>aro
    <fnt><vrk><url ref="http://at.komputilo.org">Astronomia
          Terminaro</url></vrk></fnt>
  </kap>
  <mlg>Com</mlg>
  <snc mrk="har.berenicaH0aro.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-duonsfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Coma Berenices</trd>):
      <ekz>
        NGC 4042 estas galaksio en konstelacio Berenica <tld lit="H"/>aro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=116784">NGC 4042</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_K;&c_o;&c_s;&c_i;&c_t;&c_je; &c_n;&c_a; &c_V;&c_je;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="de">Haar der Berenike</trd>
  <trd lng="en">Coma Berenices</trd>
  <trd lng="es">Cabellera de Berenices</trd>
  <trd lng="fr">Chevelure de B&eacute;r&eacute;nice</trd>
  <trd lng="it">Chioma di Berenice</trd>
  <trd lng="pl">Warkocz Bereniki</trd>
  <trd lng="ru">&c_V;&c_o;&c_l;&c_o;c&c_y;
  &c_V;&c_je;&c_r;&c_o;&c_n;&c_i;&c_k;&c_i;</trd>
</drv>

<drv mrk="har.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc mrk="har.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Dika <tld/>o de iuj bestoj:
      <ekz>
        persistemaj &ccirc;evaloj kun longaj <tld/>egoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>egoj de apro;
      </ekz>
      <ekz>
        Ili &scirc;iris al [la urso] la <tld/>egojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="br">reun</trd>
  <trdgrp lng="cs">
    <trd>&scaron;t&ecaron;tina</trd>,
    <trd>&zcaron;&iacute;n&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Borste</trd>
  <trd lng="en">bristle</trd>
  <trd lng="es">cerda</trd>
  <trdgrp lng="fr">
    <trd>crin</trd>,
    <trd>soie<klr tip="ind"> (poil)</klr></trd>
  </trdgrp>
  <trd lng="hu">s&ouml;rte</trd>
  <trdgrp lng="it">
    <trd>setola</trd>, 
    <trd>crine</trd>
  </trdgrp>
  <trd lng="nl">borstel</trd>
  <trd lng="pl">sier&sacute;&cacute;</trd>
  <trd lng="pt">crina</trd>
  <trd lng="ru">&c_shch;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">&scaron;tetina</trd>
</drv>

<drv mrk="har.0plektajxo">
  <kap><tld/>plekta&jcirc;o</kap>
  <snc mrk="har.0plektajxo.FRIZ">
    <dif>
      Kelkaj (plej ofte, tri) &scirc;nure kunplektitaj tufoj da
      kap<tld/>oj:
      <ekz>
        kiam li vidis min, li &ccirc;iam tiris
        miajn longajn ru&gcirc;ajn <tld/>plekta&jcirc;ojn kaj
        &scirc;ajnigis, ke li
        bruligis sian manon
        <fnt>
          <aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        junulinoj en Kroatio amase aran&gcirc;as por si
        <tld/>plekta&jcirc;ojn
        kiel Janica
        <fnt>
          <aut>Z. Ti&scaron;ljar</aut>,
          <vrk>Janica Kosteli&cacute; - skia miraklo</vrk>,
          <bib>Monato</bib>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;<klr tip="amb">
  (&c_k;&c_o;&c_s;&c_a;)</klr></trd>
  <trd lng="br">plezhenn<klr tip="ind"> (blev)</klr></trd>
  <trd lng="de">Zopf</trd>
  <trd lng="en">plait</trd>
  <trd lng="es">trenza</trd>
  <trd lng="fr">tresse<klr tip="ind"> (de cheveux)</klr></trd>
  <trdgrp lng="hu">
    <trd>copf</trd>,
    <trd>hajfonat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>treccia<klr tip="amb"> (acconciatura)</klr></trd>, 
    <trd>codino<klr tip="amb"> (acconciatura)</klr></trd>, 
    <trd><ind>coda</ind> di cavallo<klr tip="amb">
        (acconciatura)</klr></trd>
  </trdgrp>
  <trd lng="nl">vlecht</trd>
  <trd lng="pl">warkocz</trd>
  <trd lng="pt">tran&ccedil;a<klr tip="amb"> (de cabelo)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a;
  <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
</drv>

<drv mrk="har.0ligo">
  <kap><ofc>*</ofc> <tld/>ligo</kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="har.0plektajxo"><tld lit="H"/>plekta&jcirc;o</ref>:
      <ekz>
        &scirc;ia korpo estis admirinde gracia; &scirc;ia <tld/>ligo de ora
        koloro
        kaj peza kiel oro, falis &gcirc;is la genuoj, sed neniu povus
        &scirc;in nomi
        belulino
        <fnt><bib>PatrojFiloj</bib>, <lok>&Ccirc;ap. 7a.</lok></fnt>;
      </ekz>
      <ekz>
        dika plekta&jcirc;o de nigraj <tld/>ligoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;<klr tip="amb">
        (&c_k;&c_o;&c_s;&c_a;)</klr></trd>
    <trd lng="br">plezhenn<klr tip="ind"> (blev)</klr></trd>
    <trd lng="de">Zopf</trd>
    <trd lng="en">plait</trd>
    <trd lng="es">trenza</trd>
    <trd lng="fr">tresse<klr tip="ind"> (de cheveux)</klr></trd>
    <trdgrp lng="hu">
      <trd>copf</trd>,
      <trd>hajfonat</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>treccia<klr tip="amb"> (acconciatura)</klr></trd>, 
      <trd>codino<klr tip="amb"> (acconciatura)</klr></trd>, 
      <trd><ind>coda</ind> di cavallo<klr tip="amb">
          (acconciatura)</klr></trd>
    </trdgrp>
    <trd lng="nl">vlecht</trd>
    <trd lng="pl">warkocz</trd>
    <trd lng="pt">tran&ccedil;a<klr tip="amb"> (de cabelo)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_a;
    <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </snc>
</drv>

<drv mrk="har.kol0aro">
  <kap>kol<tld/>aro,
  <var><kap><ofc>*</ofc>kol<tld/>oj</kap></var></kap>
  <snc mrk="har.kol0aro.ZOO">
    <dif>
      Longaj pendantaj <tld/>oj &ccirc;e kolo de iuj bestoj: leono,
      &ccirc;evalo...:
      <ekz>
        leono venis kaj ekskuis la kol<tld/>aron
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        laca &gcirc;i falis sur la delikatan, molan, longan kol<tld/>aron de la imperiestra &ccirc;evalo
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_i;&c_v;&c_a;</trd>
  <trd lng="br">moue</trd>
  <trd lng="cs">h&rcaron;&iacute;va <klr>(lva| kon&ecaron;)</klr></trd>
  <trd lng="de">M&auml;hne</trd>
  <trd lng="en">mane</trd>
