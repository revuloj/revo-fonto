  </trdgrp>
  <trdgrp lng="it">
    <trd>setola</trd>,
    <trd>crine</trd>
  </trdgrp>
  <trd lng="ja">&#x525B;&#x6BDB;  <pr>&#12372;&#12358;&#12418;&#12358;</pr></trd>
  <trd lng="nl">borstel</trd>
  <trd lng="pl">sier&sacute;&cacute;</trd>
  <trd lng="pt">crina</trd>
  <trd lng="ro">blan&abreve;</trd>
  <trd lng="ru">&c_shch;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">&scaron;tetina</trd>
  <trd lng="uk">&c_shch;&c_je;&c_t;&c_i;&#x43D;&c_a;</trd>
