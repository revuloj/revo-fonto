<drv mrk="har.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="har.0o.mamulo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="filame.0o">Filamento</ref> kreskanta el
      la
      ha&ubreve;to de <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      kaj pli-malpli dense kovranta ilian korpon:
      <ekz>
        <tld/>o estas tre maldika
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        tondi <tld/>ojn;
      </ekz><ekz>
        kombi al si la <tld/>ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>disfendi, dissplitigi harojn</ind>
        <klr>(subtila&ccirc;e diskuti)</klr>;
        <trd lng="br">spazha&ntilde; laou</trd>
        <trd lng="de">Haare <ind>spalten</ind></trd>
        <trd lng="fr">couper les <ind>cheveux</ind> en quatre</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_v;
          <ind>&c_m;&c_je;&c_l;&c_o;&c_ch;&c_a;&c_h;</ind></trd>,
          <trd>&c_b;&c_u;&c_k;&c_v;&c_o;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_z;&c_a;&c_n;&c_u;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        mia vivo estas sur <tld/>eto
        <klr>(estas en ekstrema dan&gcirc;ero)</klr>;
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        li estas <ind><tld/>plena</ind> kiel urso.
        <trd lng="br">blevek</trd>
        <trd lng="fr">poilu</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bukl.0o">buklo</ref>,
      <ref cel="faden.0o">fadeno</ref>,
      <ref cel="fibr.0o">fibro</ref>,
      <ref cel="peruk.0o">peruko</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>hajsz&aacute;l</trd>,
      <trd>sz&odblac;rsz&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>pelo</trd>,
      <trd>capello</trd>
    </trdgrp>
    <trd lng="pt">cabelo</trd>
    <trd lng="sv">h&aring;rstr&aring;</trd>
  </snc>
  <snc mrk="har.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Fibra elkreska&jcirc;o el la epidermo de
      <ref tip="malprt" cel="veget.0ajxo">vegeta&jcirc;o</ref>:
      <ekz>
        brul<tld/>o estas tiu eta esta&jcirc;o de kelkaj plantoj kiu ka&ubreve;zas jukadon (urtiko) a&ubreve; malofte
        inflamon (tropikaj plantoj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29629">Brulharo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        glanda <tld/>o &ccirc;e la kruci&gcirc;o de la infloreska akso kaj la petaloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51396">Lupolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">sz&odblac;r</trd>
    <trd lng="it">tricoma</trd>
    <trd lng="pt">p&ecirc;lo<klr tip="amb"> (de vegetal)</klr></trd>
  </snc>
  <trd lng="af">hare</trd>
  <trd lng="am">&#x1340;&#x1309;&#x122D;</trd>
