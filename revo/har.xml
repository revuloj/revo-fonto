<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>har</rad>/o
</kap>
<drv mrk="har.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="har.0o.mamulo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="filame.0o">Filamento</ref>
      kreskanta el la ha&ubreve;to de
      <ref tip="malprt" cel="mam.0uloj">mamuloj</ref> kaj pli
      malpli dense kovranta ilian korpon:
      <ekz>
        tondi <tld/>ojn;
      </ekz>
      <ekz>
        kombi al si la <tld/>ojn;
      </ekz>
      <ekz>
        maldika kiel <tld/>o;
      </ekz>
      <ekz>
        la <tld/>oj de negroj similas stupon;
      </ekz>
      <ekz>
        blonda, griza <tld/>aro;
      </ekz>
      <ekz>
        la <tld/>aran&gcirc;o de la virinoj multe
        &scirc;an&gcirc;i&gcirc;is de kelkaj jaroj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        disfendi, dissplitigi  harojn
        <klr>(subtila&ccirc;e diskuti)</klr>;
      </ekz>
      <ekz>
        lia vivo estas sur <tld/>eto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pendas de, el <tld/>eto<fnt>Z</fnt>
        <klr>(estas en ekstrema dan&gcirc;ero)</klr>;
      </ekz>
      <ekz>
        li estas <tld/>plena kiel urso;
      </ekz>
      <ekz>
        <tld/>stariga eraro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="faden.0o">fadeno</ref>,
      <ref cel="fibr.0o">fibro</ref>,
      <ref cel="peruk.0o">peruko</ref>,
      <ref cel="tuber.har0o">hartubero</ref>,
      <ref cel="bukl.0o">buklo</ref>.
    </refgrp>
    <trd lng="sv">h&aring;rstr&aring;</trd>
  </snc>
  <snc mrk="har.0o.planto">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Fibra elkreska&jcirc;o el la epidermo de
      <ref tip="malprt" cel="plant.0o">planto</ref>.
    </dif>
  </snc>
  <trd lng="nl">haar</trd>
  <trdgrp lng="cs">
    <trd>vlas</trd>,
    <trd>chlup</trd>
  </trdgrp>
  <trd lng="de">Haar</trd>
  <trd lng="en">hair</trd>
  <trd lng="fr">poil</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_s;</trd>
</drv>
<drv mrk="har.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="har.0aro.peruko">
    <fnt>Z</fnt>
    <dif>
      Peruko.
    </dif>
    <trd lng="de">Per&uuml;cke</trd>
    <trd lng="fr">perruque</trd>
  </snc>
  <snc mrk="har.0aro.kapo">
    <dif>
      Kap<tld/>oj.
    </dif>
    <trdgrp lng="nl">
      <trd>haardos</trd>,
      <trd>haar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>chevelure</trd>,
      <trd>cheveux</trd>
    </trdgrp>
    <trd lng="sv">h&aring;r</trd>
  </snc>
  <trd lng="de">Haar</trd>
  <trd lng="ru">&c_sh;&c_je;&c_v;&c_je;&c_l;&c_ju;&c_r;&c_a;</trd>
</drv>
<drv mrk="har.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Dika <tld/>o de iuj bestoj:
      <ekz>
        <tld/>egoj de apro, de &ccirc;evala vosto.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">borstel</trd>
  <trd lng="de">Borste</trd>
  <trd lng="en">bristle</trd>
  <trd lng="fr">soie<klr> (poil)</klr></trd>
  <trd lng="ru">&c_shch;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>
</drv>
<drv mrk="har.0ligo">
  <kap><tld/>ligo</kap>
  <snc>
    <dif>
      Kunplektitaj tufoj da longaj kap<tld/>oj.
    </dif>
  </snc>
  <trd lng="nl">vlecht</trd>
  <trd lng="de">Zopf</trd>
  <trd lng="en">plait</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a; <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
</drv>
<drv mrk="har.kol0aro">
  <kap>kol<tld/>aro</kap>
  <snc>
    <dif>
      Longaj pendantaj <tld/>oj &ccirc;e kolo de iuj bestoj (leono,
      &ccirc;evalo).
    </dif>
  </snc>
  <trd lng="nl">manen</trd>
  <trd lng="de">M&auml;hne</trd>
  <trd lng="en">mane</trd>
  <trd lng="fr">crini&egrave;re</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_v;&c_a;</trd>
  <trd lng="sv">man</trd>
</drv>
<drv mrk="har.lip0oj">
  <kap>lip<tld/>oj</kap>
  <snc>
    <dif>
      <tld lit="H"/>oj &ccirc;e la supera lipo:
      <ekz>
        bruligi al si la lip<tld/>ojn
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(agi nesufi&ccirc;e singarde kaj devi penti pri tio)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schnauzer</trd>,
    <trd>Bart<klr>(haare)</klr></trd>
  </trdgrp>
  <trd lng="en">moustache</trd>
  <trd lng="fr">moustache</trd>
  <trd lng="nl">snor</trd>
  <trd lng="ru">&c_u;&c_s;&c_y;</trd>
</drv>
<drv mrk="har.okul0oj">
  <kap>okul<tld/>oj</kap>
  <snc>
    <dif>
      <tld/>oj &ccirc;e la rando de palpebroj.
    </dif>
  </snc>
  <trd lng="nl">wimpers</trd>
  <trd lng="de">Wimpern</trd>
  <trd lng="en">eyebrow</trd>
  <trd lng="fr">cil</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_n;&c_i;&c_c;&c_y;</trd>
</drv>
<drv mrk="har.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Ne havanta <tld/>ojn, perdinta la <tld/>ojn:
      <ekz>
        sen<tld/>a kapo, maljunulo
      </ekz>
    </dif>
    <ref tip="sin" cel="kalv.0a">kalva</ref>
  </snc>
  <trd lng="nl">kaal</trd>
  <trdgrp lng="de">
    <trd>haarlos</trd>,
    <trd>glatzk&ouml;pfig</trd>
  </trdgrp>
  <trd lng="en">bald</trd>
  <trdgrp lng="fr">
    <trd>chauve</trd>,
    <trd>glabre</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_o;&c_l;&c_o;&c_s;&c_y;&c_j;</trd>
</drv>
<drv mrk="har.vang0oj">
  <kap>vang<tld/>oj</kap>
  <snc>
    <dif>
      <tld lit="H"/>oj kreskantaj &ccirc;e la vangoj.
    </dif>
  </snc>
  <trd lng="nl">bakkebaard</trd>
  <trd lng="de">Bart<klr>(haare)</klr></trd>
  <trd lng="en">side-whiskers</trd>
  <trd lng="fr">favoris<klr> (barbe)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_je;&c_n;&c_b;&c_a;&c_r;&c_d;&c_y;</trd>
</drv>
</art>
