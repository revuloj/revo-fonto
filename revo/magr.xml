<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>magr</rad>/a
</kap>
<drv mrk="magr.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="magr.0a.maldika">
      <ref tip="dif" cel="gras.mal0a">malgrasa</ref>
      <ekz>
        li konstatis, ke Jaspa fari&gcirc;is ankora&ubreve; pli kompatinde <tld/>a
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 283a</lok></fnt>.
      </ekz>
      <trd lng="hu">sov&aacute;ny</trd>
  </snc>
  <snc mrk="magr.0a.malabunda">
    <ref tip="dif" cel="abund.mal0a">malabunda</ref>
    <ekz>
      la komforto de la lo&gcirc;ejoj estas <tld/>a
      <fnt><aut>Bardhyl Selimi</aut>: <vrk>Albanio, la homa disvolvi&gcirc;o dum
      la lastaj dek jaroj</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero
      3a, p. 12-14a</lok></fnt>.
    </ekz>
    <trd lng="hu">sz&udblac;k&ouml;s</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>maigre</trd>
  </trdgrp>
</drv>
</art>

