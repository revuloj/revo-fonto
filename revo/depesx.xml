<vortaro>
<art mrk="$Id: depesx.xml,v 1.21 2023/10/14 11:26:53 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>depe&scirc;</rad>/o
</kap>

<drv mrk="depesx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ur&gcirc;a <ref tip="super" cel="sci.0igo">sciigo</ref>, sendita per rapida vojo, ekz-e per telegrafo:
      <ekz>
        <nom>Mentezufis</nom> sendis <tld/>on al <nom>Herhor</nom> kaj en la da&ubreve;ro de unu tago ricevis
        respondon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o &jcirc;us informis <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam je la na&ubreve;a matene venis radio<tld/>o de <nom>Caronia</nom>,
        &scirc;ipo de <nom>Cunard</nom>: &leftquot;al la
        kapitano de <nom>Titanic</nom>: vapor&scirc;ipoj celantaj okcidenten anoncas la 12-an de
        aprilo grandajn kaj malgrandajn glacimontojn kaj glaciajn kampojn en regiono
        de 42&deg; de la norda latitudo kaj 49&deg; &gcirc;is 51&deg; de la okcidenta longitudo
        <klr>[&#x2026;]</klr>&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4259">RMS Titanic</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="telegr1.0o">telegramo</ref>,
      <ref cel="mesagx.0o">mesa&gcirc;o</ref>,
      <ref cel="ekspre.0o.POSX">ekspreso</ref>,
      <ref cel="kurier.0o.POSX">kuriero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_p;&c_e;&c_sh;&c_a;</trd>
  <trd lng="bg">&c_d;&c_je;&c_p;&#x450;&c_sh;&c_a;</trd>
  <trd lng="ca">despatx</trd>
  <trd lng="de">Depesche</trd>
  <trdgrp lng="en">
    <trd>dispatch</trd>,
    <trd>urgent written message</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;p&ecirc;che</trd>
  <trd lng="he">&#x5DE;&#x5D1;&#x5E8;&#x5E7;</trd>
  <trd lng="hu">s&uuml;rg&ouml;ny</trd>
  <trdgrp lng="ja">
    <trd>&#x81F3;&#x6025;&#x5831;  <pr>&#x3057;&#x304D;&#x3085;&#x3046;&#x307B;&#x3046;</pr></trd>,
    <trd>&#x6025;&#x9001;&#x516C;&#x6587;&#x66F8;  <pr>&#x304D;&#x3085;&#x3046;&#x304A;&#x304F;&#x3053;&#x3046;&#x3076;&#x3093;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="pl">depesza</trd>
  <trd lng="pt">despacho</trd>
  <trd lng="ru">&c_d;&c_je;&c_p;&c_je;&c_sh;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x435;&#x43F;&#x435;&#x448;&#x430;</trd>,
    <trd>&#x442;&#x435;&#x440;&#x43C;&#x456;&#x43D;&#x43E;&#x432;&#x435; &#x43F;&#x43E;&#x432;&#x456;&#x434;&#x43E;&#x43C;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
