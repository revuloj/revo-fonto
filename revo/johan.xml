<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: johan.xml,v 1.30 2023/12/03 07:09:18 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>Johan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="johan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="johan.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        la mano de <tld/>o estas pura
        <fnt>
          <bib>F</bib>
          <lok>&FE; 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne turnu atenton sur tion, kion diras Petro a&ubreve; <tld/>o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">BIB</uzo>
        <uzo tip="fak">PERS</uzo>
        en tiuj tagoj venis <ind><tld/>o la Baptisto</ind>,
        predikante en la dezerto de Judujo
        <fnt><bib>NT</bib>, <lok>&Mat; 3:1.</lok></fnt>;
        <trd lng="be">&c_Ja;&c_n; &c_H;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>
        <trd lng="bg">&c_J;&c_o;&c_a;&c_n; &c_K;&c_r;&c_malmol;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;</trd>
        <trdgrp lng="br">
          <trd>Yann ar Badezour</trd>,
          <trd>Yann-Vadezour</trd>
        </trdgrp>
        <trd lng="de">Johannes der T&auml;ufer</trd>
        <trd lng="es">Juan el Bautista</trd>
        <trdgrp lng="fr">
          <trd>Jean le Baptiste</trd>,
          <trd>Jean-Baptiste</trd>
        </trdgrp>
        <trd lng="hu">Keresztel&odblac; (Szent) J&aacute;nos</trd>
        <trdgrp lng="it">
          <trd>san <ind>Giovanni</ind> Battista</trd>,
          <trd>(san) <ind>Giovan</ind> Battista</trd>
        </trdgrp>
        <trd lng="nl">Johannes de Doper</trd>
        <trd lng="pt">Jo&atilde;o Batista</trd>
        <trd lng="ru">&c_I;&c_o;&c_a;&c_n;&c_n; &c_K;&c_r;&c_je;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
        <trd lng="sv">Johannes D&ouml;paren</trd>
      </ekz>
      <ekz>
        ne &ccirc;iam estas <klr>(tago de)</klr> sankta <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la malfeli&ccirc;a <tld/>o estis tre mal&gcirc;oja, &ccirc;ar lia patro estis tre malsana kaj
        balda&ubreve; mortonta
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la libro Agoj de <tld/>o Masono
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">PERS</uzo>
        sankta <ind><tld/>o la Orbu&scirc;ulo</ind>
        <klr>(eklezia patro kaj konstantinopola patriarko
        en 398&ndash;404)</klr>.
        <trd lng="be">&c_Ja;&c_n; &c_Z;&c_l;&c_a;&c_t;&c_a;&c_v;&c_u;&c_s;&c_t;</trd>
        <trd lng="br">Yann Grizostomos</trd>
        <trd lng="en">John Chrysostome</trd>
        <trd lng="fr">Jean Chrysostome</trd>
        <trd lng="hu">Aranysz&aacute;j&uacute; Szent J&aacute;nos</trd>
        <trd lng="nl">Johannes <ind>Chrysostomos</ind></trd>
        <trd lng="ru">&c_I;&c_o;&c_a;&c_n;&c_n; &c_Z;&c_l;&c_a;&c_t;&c_o;&c_u;&c_s;&c_t;</trd>
        <trd lng="it">Giovanni Crisostomo</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ja;&c_n;</trd>
  <trd lng="bg">&c_J;&c_o;&c_a;&c_n;</trd>
  <trd lng="br">Yann</trd>
  <trd lng="hu">J&aacute;nos</trd>
  <trd lng="de">Johannes</trd>
  <trd lng="en">John</trd>
  <trd lng="es">Juan</trd>
  <trdgrp lng="fr">
    <trd>Jean</trd>,
    <trd>Jehan</trd>
  </trdgrp>
  <trd lng="it">Giovanni</trd>
  <trdgrp lng="ja">
    <trd>&#x30E8;&#x30CF;&#x30F3;</trd>,
    <trd>&#x30E8;&#x30CF;&#x30CD;</trd>,
    <trd>&#x30B8;&#x30E7;&#x30F3;  <pr>&#x3058;&#x3087;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Jan</trd>,
    <trd>Johan</trd>
  </trdgrp>
  <trd lng="pt">Jo&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_I;&c_o;&c_a;&c_n;&c_n;</trd>,
    <trd>&c_I;&c_v;&c_a;&c_n;</trd>,
    <trd>&c_I;&c_o;&c_g;&c_a;&c_n;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_Zh;&c_a;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
    <trd>&c_D;&c_zh;&c_o;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>    
  </trdgrp>
  <trd lng="sk">J&aacute;n</trd>
  <trdgrp lng="sv">
    <trd>Johan</trd>,
    <trd>Johannes</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_Ib;&c_v;&c_a;&#x43D;</trd>,
    <trd>&c_J;&#x43E;&c_a;&#x43D;</trd>,
    <trd>&c_Ib;&#x43E;&c_a;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D04;&#x7FF0; <pr>Yu&#x113;h&#xE0;n</pr></trd>,
    <trd>&#x82E5;&#x671B; <pr>Ru&#xF2;w&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="johan.0cxjo">
  <kap><ofc>*</ofc> <tld/>&ccirc;jo,
  <var><kap><ofc>*</ofc>Jo&ccirc;jo</kap></var></kap>
  <snc>
   <dif>
    Diminutivo de <tld/>o:
      <ekz>
        <tld/>on, <klr>[&#x2026;]</klr> iliaj gepatroj nomas Johan&ccirc;jo (a&ubreve; Jo&ccirc;jo), ...
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz> 
      <ekz>
        <tld/>&ccirc;jo-malsa&gcirc;ulo
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Jeannot</trd>
    <trd>Johnny</trd>
  </trdgrp>
  <trd lng="nl">Jantje</trd>
  <trdgrp lng="uk">
    <trd>&c_Ib;&c_v;&c_a;&c_s;&#x44C;</trd>,
    <trd>&c_Ib;&c_v;&c_a;&c_s;&c_i;&#x43A;</trd>,
    <trd>&c_Ib;&c_v;&c_a;&#x43D;&#x43A;&#x43E;</trd>,
    <trd>&c_J;&#x43E;&c_s;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&#x42E;&c_z;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="johan.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="johan.0ino.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Ina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        <tld/>ino, la virgulino de Orleano, la sor&ccirc;istino,
        estas forbruligita
        <fnt><bib>Fab3</bib>, <lok>Dorna vojo de la honoro</lok></fnt>;
      </ekz>
      <ekz>
        tri elegantaj knabinoj, Ido, <tld/>ino kaj Anno-Doroteo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Diminutivoj povus esti Johaninjo, Johanjo, Jonjo...
    </rim>
  </snc>
  <trd lng="be">&c_Ja;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_Zh;&c_a;&c_n;&c_a;</trd>
  <trd lng="br">Chann</trd>
  <trd lng="de">Johanna</trd>
  <trd lng="hu">Johanna</trd>
  <trdgrp lng="en">
    <trd>Joanne</trd>,
    <trd>Joan</trd>,
    <trd>Joanna</trd>
  </trdgrp>
  <trd lng="es">Juana</trd>
  <trdgrp lng="fr">
    <trd>Jeanne</trd>,
    <trd>Jehanne</trd>
  </trdgrp>
  <trd lng="it">Giovanna</trd>
  <trd lng="nl">Johanna</trd>
  <trd lng="pt">Joana</trd>
  <trdgrp lng="ru">
    <trd>&c_I;&c_o;&c_a;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_Zh;&c_a;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_Ja;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">Jana</trd>
  <trd lng="sv">Johanna</trd>
  <trdgrp lng="uk">
    <trd>&c_Ib;&c_v;&c_a;&#x43D;&#x43D;&c_a;</trd>,
    <trd>&#x42F;&#x43D;&c_ib;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
    <rim> La 7a OA klarigas la fundamentecon de la radiko &leftquot;Johan&rightquot;
    aperanta en la ekzemploj de la radiko -&ccirc;j' en la Universala Vortaro.
    La kunmeta&jcirc;o Jo'&ccirc;j' estas anka&ubreve; fundamenta el tiu sama ekzemplo. 
    Johan&ccirc;jo estas anka&ubreve; fundamenta el la ekzerco 38 de la Ekzercaro, en
    kiu Jo'&ccirc;j' estas anka&ubreve; ripetita.</rim>
</art>
