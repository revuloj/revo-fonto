<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Johan</rad>/o
</kap>
<drv mrk="johan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira anta&ubreve;nomo hebredevena:
      <ekz>
        <uzo tip="fak">BIB</uzo>
        <uzo tip="fak">PERS</uzo>
        en tiuj tagoj venis <ind>Johano la Baptisto</ind>,
        predikante en la dezerto de Judujo
        <fnt><bib>NT</bib>, <lok>&Mat; 3:1.</lok></fnt>;
        <trd lng="be">&c_Ja;&c_n; &c_H;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>
        <trd lng="es">Juan el Bautista</trd>
        <trdgrp lng="fr">
          <trd>Jean le Baptiste</trd>,
          <trd>Jean-Baptiste</trd>
        </trdgrp>
        <trd lng="hu">Keresztel&odblac; (Szent) J&aacute;nos</trd>
        <trd lng="nl">Johannes de Doper</trd>
        <trd lng="pt">Jo&atilde;o Batista</trd>
        <trd lng="ru">&c_I;&c_o;&c_a;&c_n;&c_n; &c_K;&c_r;&c_je;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
        <trd lng="sv">Johannes D&ouml;paren</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">PERS</uzo>
        sankta <ind><tld/>o la Orbu&scirc;ulo</ind>
        <klr>(eklezia patro kaj konstantinopola patriarko
        en 398&ndash;404)</klr>.
        <trd lng="be">&c_Ja;&c_n; &c_Z;&c_l;&c_a;&c_t;&c_a;&c_v;&c_u;&c_s;&c_t;</trd>
        <trd lng="en">John Chrysostome</trd>
        <trd lng="fr">Jean Chrysostome</trd>
        <trd lng="hu">Aranysz&aacute;j&uacute; Szent J&aacute;nos</trd>
        <trd lng="nl">Johannes <ind>Chrysostomos</ind></trd>
        <trd lng="ru">&c_I;&c_o;&c_a;&c_n;&c_n; &c_Z;&c_l;&c_a;&c_t;&c_o;&c_u;&c_s;&c_t;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ja;&c_n;</trd>
  <trd lng="hu">J&aacute;nos</trd>
  <trd lng="en">John</trd>
  <trd lng="es">Juan</trd>
  <trdgrp lng="fr">
    <trd>Jean</trd>,
    <trd>Jehan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Jan</trd>,
    <trd>Johan</trd>
  </trdgrp>
  <trd lng="pt">Jo&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_I;&c_o;&c_a;&c_n;&c_n;</trd>,
    <trd>&c_I;&c_v;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">J&aacute;n</trd>
  <trdgrp lng="sv">
    <trd>Johan</trd>,
    <trd>Johannes</trd>
  </trdgrp>
</drv>

<drv mrk="johan.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Ina anta&ubreve;nomo:
      <ekz>
        <tld/>ino, la virgulino de Orleano, la sor&ccirc;istino,
        estas forbruligita
        <fnt><bib>Fab3</bib>, <lok>Dorna vojo de la honoro</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ja;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="hu">Johanna</trd>
  <trdgrp lng="en">
    <trd>Joanne</trd>,
    <trd>Joan</trd>,
    <trd>Joanna</trd>
  </trdgrp>
  <trd lng="es">Juana</trd>
  <trdgrp lng="fr">
    <trd>Jeanne</trd>,
    <trd>Jehanne</trd>
  </trdgrp>
  <trd lng="nl">Johanna</trd>
  <trd lng="pt">Joana</trd>
  <trd lng="ru">&c_I;&c_o;&c_a;&c_n;&c_n;&c_a;</trd>
  <trd lng="sk">Jana</trd>
  <trd lng="sv">Johanna</trd>
</drv>
</art>
