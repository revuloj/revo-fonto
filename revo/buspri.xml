<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buspri.xml,v 1.20 2024/05/26 15:25:18 revo Exp $">
<kap>
  <rad>busprit</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="buspri.0o">
  <kap><tld/>o</kap>
  <snc mrk="buspri.0o.MAR">
  <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="mast.0o.MAR">Masto</ref>, kiu elstaras el la &scirc;ipo anta&ubreve;en kaj al kiu oni
      fiksas
      <ref tip="vid" cel="staj.0o.SHI">stajon</ref> por porti
      <ref tip="vid" cel="vel.antaux0o">anta&ubreve;velojn</ref>,
      ekz. <ref tip="vid" cel="jxib.0o">&jcirc;ibon</ref>,
      <ref cel="vel.staj0o">stajvelon</ref>:
      <ekz>
        &gcirc;uste super la <tld/>o staris la bela, gaje trembriletanta Siriuso, &scirc;ajne
        farante al li signon, ke li sekvu &gcirc;in
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen venis ondego, kiu simile al frakasanta roko ruli&gcirc;is trans la <tld/>on, kaj
        &ccirc;i tiu for&scirc;iri&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        longaj brakoj anta&ubreve;etenditaj kiel la <tld/>oj de
        aro de velboatoj
        <fnt><bib>DOz</bib>, <lok>&leftquot;ili renkontas la lignajn
        gargojlojn&rightquot;</lok></fnt>;
      </ekz>
      <ekz>
        graciaj karaveloj, rapidaj brigantinoj, pezaj mallertaj galionoj kaj flugilaj
        galiotoj portis sur siaj <tld/>oj kaj flagmastoj tre diversajn flagojn
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_u;&c_sh;&c_p;&c_r;&c_y;&c_t;</trd>
    <trd lng="bg">&c_b;&c_u;&c_sh;&c_p;&c_r;&c_i;&c_t;</trd>
    <trd lng="ca">baupr&eacute;s</trd>
    <trd lng="de">Bugspriet</trd>
    <trd lng="en">bowsprit</trd>
    <trd lng="it">bompresso</trd>
    <trd lng="fr">beaupr&eacute;</trd>
    <trd lng="hu">orr&aacute;rboc</trd>
    <trd lng="pl">bukszpryt</trd>
    <trd lng="ru">&c_b;&c_u;&c_sh;&c_p;&c_r;&c_i;&c_t;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#31532;&#19968;&#26012;&#27299; <pr>&#12384;&#12356;&#65381;&#12356;&#12385;&#65381;&#12375;&#12419;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#12420;&#12426;&#20986;&#12375; <pr>&#12420;&#12426;&#12384;&#12375;</pr></trd>,
    <trd>&#12496;&#12454;&#12473;&#12503;&#12522;&#12483;&#12488;</trd>
  </trdgrp>
  <trd lng="sk">predn&yacute; s&tcaron;a&zcaron;e&ncaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_sh;&c_p;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_g;&c_sh;&c_p;&c_r;&c_i;&c_t;</trd>
  </trdgrp>
</drv></art>
