<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>armen</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="armen.0ujo">
  <kap>
    <tld lit="A"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="armen.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AM</mlg>
    <dif>
      &Scirc;tato en la centra Azio kun &ccirc;efurbo Erevano.
      Teritorio de <em>Respubliko Armenujo</em>. &Gcirc;is 1991
      <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
      &Gcirc;i limas kun <ref tip="vid"
      cel="azerbajg.0o.GEOG">Azerbaj&gcirc;ano</ref>
      <ref tip="vid" cel="iran.0o.GEOG">Irano</ref>
      <ref tip="vid" cel="kartvel.K0ujo">Kartvelujo</ref>,
      Na&hcirc;i&ccirc;evano
      kaj <ref tip="vid" cel="uzbek.0ujo">Turkujo</ref>.
    </dif>
    <ref tip="sin" cel="armeni.0o.GEOG">Armenio</ref>
    <trd lng="bg">&c_A;&c_r;&c_m;&c_je;&c_n;&c_i;&c_ja;</trd>
    <trd lng="de">Armenien</trd>
    <trd lng="fa">&a_A;&a_r;&a_m;&a_n;&a_s;&a_t;&a_A;&a_n;</trd>
    <trd lng="hu">&Ouml;rm&eacute;nyorsz&aacute;g</trd>
    <trdgrp lng="it">
      <trd>Armenia</trd>,
      <trd>Repubblica di <ind>Armenia</ind></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Armenia</trd>,
      <trd>Republika <ind>Armenii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

