<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>armen</rad>/o
</kap>

<drv mrk="armen.0ujo">
  <kap>
    <tld lit="A"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="armen.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AM</mlg>
    <dif>
      &Scirc;tato en la centra Azio kun &ccirc;efurbo Erevano.
      Oficiala nomo: <em>Respubliko de Armenujo</em>. &Gcirc;is 1991
      <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
      &Gcirc;i limas kun <ref tip="vid"
      cel="azerbajgxan.0ujo.GEOG">Azerbaj&gcirc;ano</ref>
      <ref tip="vid" cel="iran.0o.GEOG">Irano</ref>
      <ref tip="vid" cel="kartvel.K0ujo">Kartvelujo</ref>,
      Na&hcirc;i&ccirc;evano
      kaj <ref tip="vid" cel="uzbek.0ujo">Turkujo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Armenia</trd>,
      <trd>Republika <ind>Armenii</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="armen.0io">
  <kap>
    <tld lit="A"/>io
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="armen.0io.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AM</mlg>
    <dif>
      <ref tip="dif" cel="armen.0ujo.GEOG">Armenujo</ref>
    </dif>
    <trdgrp lng="pl">
      <trd>Armenia</trd>,
      <trd>Republika <ind>Armenii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>