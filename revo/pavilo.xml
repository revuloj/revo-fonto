<drv mrk="pavilo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Granda, luksa tendo.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_jo;&c_n;</trd>
    <trd lng="hu">d&iacute;szs&aacute;tor</trd>
    <trd lng="ru">&c_p;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_o;&c_n;</trd>
  </snc>
  <snc>
    <dif>
      Konstruo alti&gcirc;anta super tegmento de la ceteraj partoj de iu domego.
    </dif>
  </snc>
  <snc>
    <dif>
      Izola konstruo en korto a&ubreve; &gcirc;ardeno.
    </dif>
    <trd lng="be">&c_a;&c_l;&c_mol;&c_t;&c_a;&c_n;&c_k;&c_a;</trd>
    <trd lng="hu">pavilon</trd>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_je;&c_d;&c_k;&c_a;</trd>
  </snc>
  <trd lng="de">Pavillon</trd>
  <trd lng="nl">paviljoen</trd>
</drv>
</art>
