<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pavilo.xml,v 1.21 2024/02/16 19:24:12 revo Exp $">
<kap>
  <ofc>9</ofc><rad>pavilon</rad>/o 
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pavilo.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="pavilo.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref cel="sxirm.0ejo">&Scirc;irmejo</ref> en &gcirc;ardeno a&ubreve; en la naturo.
    </dif>
    <subsnc>
      <dif>
        Granda, luksa <ref tip="super" cel="tend.0o.ARKI">tendo</ref>:
        <ekz>
          la organizantoj starigis aron da tend-<tld/>oj
          <fnt>
            <bib>KrM</bib>
            <lok>39. Ni Forton Pli Fre&scirc;an Akiras.</lok>
          </fnt>;
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_jo;&c_n;</trd>
      <trd lng="hu">d&iacute;szs&aacute;tor</trd>
      <trd lng="ru">&c_p;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_o;&c_n;</trd>
    </subsnc>
    <subsnc>
      <dif>
        <ref tip="super" cel="konstr.0ajxo">Konstrua&jcirc;eto</ref>,
        kun simetrie aran&gcirc;itaj muroj kaj a&ubreve; kolonoj, kun tendosimila a&ubreve; kupola tegmento:
        <ekz>
          mi lo&gcirc;is en flanka <tld/>o, destinita por gastoj
          <fnt>
            <bib>QuV</bib>
            <lok>&Ccirc;apitro II</lok>
          </fnt>;
        </ekz>
        <ekz>
          ili penetras la <tld/>ojn, zigzag-rubande &ccirc;irka&ubreve;volvante la aran&gcirc;itajn tie
          ekspoziciojn
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;e la enirejo al la <tld/>o li subite trovis sin anta&ubreve; knabino
          <fnt>
            <bib>Tor</bib>
            <lok>I.</lok>
          </fnt>;
        </ekz>
        <ekz>
          ronda <tld/>o kun fontano
          <fnt>
            <bib>MkM</bib>
            <lok>2</lok>
          </fnt>;
        </ekz>
        <ekz>
          vasta frukt&gcirc;ardeno kun disfalanta <tld/>o sur monteto
          <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e Lev Tolstoj en Jasnaja
          Poljana</lok></fnt>;
        </ekz>  
        <ekz>
          <tld/>oj kun kvar lo&gcirc;ioj, en kiuj estis konservitaj grandaj pergamenaj
          memorlibroj kun nomoj de falintoj
          <fnt>
            <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2003/008003.html">Amnezio-amnestio</url></vrk>
                <bib>Monato</bib>
            <lok>2003/02, p. 16</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_a;&c_l;&c_mol;&c_t;&c_a;&c_n;&c_k;&c_a;</trd>
      <trd lng="hu">pavilon</trd>
      <trd lng="ru">&c_b;&c_je;&c_s;&c_je;&c_d;&c_k;&c_a;</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="kiosk.0o">kiosko</ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>,
      <ref cel="bud.muzik0o">muzikbudo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/0/06/20091009440DR_Dresden_Schlo&szlig;_Pillnitz_Chinesischer_Pavillon.jpg/626px-20091009440DR_Dresden_Schlo&szlig;_Pillnitz_Chinesischer_Pavillon.jpg" prm="CC BY-SA 4.0">&Ccirc;ina <tld lit="P"/>o en Kastela Parko de Pillnitz
        <fnt>
          <aut>J&ouml;rg Blobelt, propra verko</aut>
          <lok><url ref="&WCW;/File:20091009440DR_Dresden_Schlo%C3%9F_Pillnitz_Chinesischer_Pavillon.jpg">&WCW;/File:20091009440DR_Dresden_Schlo&szlig;_Pillnitz_Chinesischer_Pavillon.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Pavillon</trd>
  <trd lng="en">pavilion</trd>
  <trd lng="fr">pavillon</trd>
  <trd lng="id">paviliun</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30D3;&#x30EA;&#x30AA;&#x30F3;</trd>,
    <trd>&#x5225;&#x68DF;  <pr>&#x3079;&#x3064;&#x3080;&#x306D;</pr></trd>,
    <trd>&#x96E2;&#x308C;  <pr>&#x3070;&#x306A;&#x308C;</pr></trd>,
    <trd>&#x6771;&#x5C4B;  <pr>&#x3042;&#x305A;&#x307E;&#x3084;</pr></trd>,
    <trd>&#x5F35;&#x308A;&#x51FA;&#x3057;  <pr>&#x306F;&#x308A;&#x3060;&#x3057;</pr></trd>,
    <trd>&#x7FFC;  <pr>&#x3064;&#x3070;&#x3055;</pr></trd>,
    <trd>&#x5927;&#x30C6;&#x30F3;&#x30C8;  <pr>&#x3060;&#x3044;&#x3066;&#x3093;&#x3068;</pr></trd>
  </trdgrp>
  <trd lng="nl">paviljoen</trd>
  <trd lng="pl">pawilon</trd>
  <trd lng="ro">pavilion  <klr>(construc&#x21B;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_j;&c_o;&c_n;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_t;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EAD;&#x5B50; <pr>t&#xED;ngzi</pr></trd>,
    <trd>&#x697C;&#x9601; <pr>l&#xF3;ug&#xE9;</pr></trd>,
    <trd>&#x6A13;&#x95A3; <pr>l&#xF3;ug&#xE9;</pr></trd>,
    <trd>&#x4EAD; <pr>t&#xED;ng</pr></trd>,
    <trd>&#x95A3; <pr>g&#xE9;</pr></trd>,
    <trd>&#x9601; <pr>g&#xE9;</pr></trd>,
    <trd>&#x51C9;&#x4EAD; <pr>li&#xE1;ngt&#xED;ng</pr></trd>,
    <trd>&#x6DBC;&#x4EAD; <pr>li&#xE1;ngt&#xED;ng</pr></trd>,
    <trd>&#x4EAD;&#x53F0; <pr>t&#xED;ngt&#xE1;i</pr></trd>,
    <trd>&#x4EAD;&#x81FA; <pr>t&#xED;ngt&#xE1;i</pr></trd>,
    <trd>&#x69AD; <pr>xi&#xE8;</pr></trd>,
    <trd>&#x5C55;&#x9928; <pr>zh&#x1CE;ngu&#x1CE;n</pr></trd>,
    <trd>&#x5C55;&#x9986; <pr>zh&#x1CE;ngu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
