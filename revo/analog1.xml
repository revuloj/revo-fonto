<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc></ofc>
  <rad>analogi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="analog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="analog.0eco">Analogeco</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">analogia</trd>
  <trdgrp lng="cs">
    <trd>analogie</trd>,
    <trd>obdoba</trd>,
    <trd>podobnost</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Entsprechung</trd>,
      <trd>Analogie</trd>
  </trdgrp>
  <trd lng="en">analogy</trd>
  <trd lng="es">analog&iacute;a</trd>
  <trd lng="fr">analogie</trd>
  <trd lng="hu">anal&oacute;gia</trd>
  <trd lng="it">analogia</trd>
  <trd lng="nl">analogie</trd>
  <trd lng="pl">analogia</trd>
  <trd lng="ro">analogie</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>anal&oacute;gia</trd>,
    <trd>anal&oacute;gia</trd>,
    <trd>&uacute;pln&aacute; podoba</trd>
  </trdgrp>
</drv>

<drv mrk="analog1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konforma al <tld/>o; analogeca:
      <ekz>
        <tld/>a rezono konkludas, pro simileco de du
        aferoj la&ubreve; unu rilato, pri ilia simileco anka&ubreve; la&ubreve;
        aliaj rilatoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="ca">an&agrave;leg</trd>
  <trdgrp lng="cs">
    <trd>analogick&yacute;</trd>,
    <trd>obdobn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>entsprechend</trd>,
      <trd>analog</trd>,
      <trd>analogisch</trd>
  </trdgrp>
  <trd lng="en">analogical</trd>
  <trdgrp lng="es">
    <trd>an&aacute;logo</trd>,
    <trd>anal&oacute;gico</trd>
  </trdgrp>
  <trd lng="fr">analogique</trd>
  <trd lng="hu">anal&oacute;g</trd>
  <trd lng="nl">analoog</trd>
  <trd lng="pl">analogiczny</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">analogick&yacute;</trd>
</drv>
</art>
