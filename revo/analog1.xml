<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>analogi</rad>/o
</kap>
<drv mrk="analog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Analogeco.
    </dif>
  </snc>
  <trd lng="de">Analogie</trd>
  <trd lng="en">analogy</trd>
  <trd lng="fr">analogie</trd>
  <trd lng="nl">analogie</trd>
  <trd lng="it">analogia</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">anal&oacute;gia</trd>
</drv>

<drv mrk="analog1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konforma al la <tld/>o:
      <ekz>
        <tld/>a rezono konkludas, pro simileco de du
        aferoj la&ubreve; unu rilato, pri ilia simileco anka&ubreve; la&ubreve;
        aliaj rilatoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">analogical</trd>
  <trd lng="fr">analogique</trd>
  <trd lng="de">analogisch</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
