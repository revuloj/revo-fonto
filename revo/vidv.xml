<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vidv.xml,v 1.20 2022/03/28 11:08:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vidv</rad>/o
</kap>
<drv mrk="vidv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Viro perdinta sian edz(in)on kaj ne reedzi&gcirc;inta.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_v;&c_je;&c_c;</trd>
  <trd lng="ca">viudo</trd>
  <trd lng="cs">vdovec</trd>
  <trd lng="de">Witwer</trd>
  <trd lng="el">&chi;&eta_ton;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">widower</trd>
  <trd lng="es">viudo</trd>
  <trd lng="fa"><klr>(&a_m;&a_r;&a_d;) </klr>&a_b;&f_y;&a_w;&a_h;</trd>
  <trd lng="fr">veuf</trd>
  <trdgrp lng="fy">
    <trd>widner</trd>,
    <trd>wjidner</trd>,
    <trd>widdoman</trd>
  </trdgrp>
  <trd lng="hu">&ouml;zvegy(ember)</trd>  
  <trd lng="id">duda</trd>
  <trd lng="io">vidvulo</trd>
  <trd lng="nl">weduwnaar</trd>
  <trd lng="pt">vi&uacute;vo</trd>
  <trd lng="ru">&c_v;&c_d;&c_o;&c_v;&c_je;&c_c;</trd>
  <trd lng="sk">vdovec</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x434;&#x456;&#x432;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x443;&#x434;&#x456;&#x432;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
  <trd lng="vo">hiviudan</trd>
</drv>

<drv mrk="vidv.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc>
    <dif>
      Virino perdinta sian edz(in)on kaj ne reedzini&gcirc;inta:
      <ekz>
        <tld/>inon kaj orfon ne premu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i estas <tld/>ino post homo, kiun mi konis<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_v;&c_a;</trd>
  <trd lng="ca">viuda</trd>
  <trd lng="cs">vdova <klr>(po n&ecaron;kom)</klr></trd>
  <trd lng="de">Witwe</trd>
  <trd lng="el">&chi;&eta_ton;&rho;&alfa;</trd>
  <trd lng="en">widow</trd>
  <trd lng="es">viuda</trd>
  <trd lng="fa"><klr>(&a_z;&a_n;) </klr>&a_b;&f_y;&a_w;&a_h;</trd>
  <trd lng="fr">veuve</trd>
  <trdgrp lng="fy">
    <trd>widdo</trd>,
    <trd>wjiddo</trd>,
    <trd>widdofrou</trd>
  </trdgrp>
  <trd lng="hu">&ouml;zvegyasszony</trd>
  <trd lng="id">janda</trd>
  <trd lng="io">vidvino</trd>
  <trd lng="nl">weduwe</trd>
  <trd lng="pt">vi&uacute;va</trd>
  <trd lng="ru">&c_v;&c_d;&c_o;&c_v;&c_a;</trd>
  <trd lng="sk">vdova</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x434;&#x43E;&#x432;&#x430;</trd>,
    <trd>&#x443;&#x434;&#x43E;&#x432;&#x430;</trd>
  </trdgrp>
  <trd lng="vo">jiviudan</trd>
</drv>
</art>
