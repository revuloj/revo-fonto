<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vidv.xml,v 1.24 2024/06/25 23:25:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vidv</rad>/o
</kap>
<drv mrk="vidv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Viro perdinta sian edz(in)on kaj ne reedzi&gcirc;inta.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_v;&c_je;&c_c;</trd>
  <trd lng="ca">viudo</trd>
  <trd lng="cs">vdovec</trd>
  <trd lng="de">Witwer</trd>
  <trd lng="el">&chi;&eta_ton;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">widower</trd>
  <trd lng="es">viudo</trd>
  <trd lng="fa"><klr>(&a_m;&a_r;&a_d;) </klr>&a_b;&f_y;&a_w;&a_h;</trd>
  <trd lng="fr">veuf</trd>
  <trdgrp lng="fy">
    <trd>widner</trd>,
    <trd>wjidner</trd>,
    <trd>widdoman</trd>
  </trdgrp>
  <trd lng="hu">&ouml;zvegy(ember)</trd>
  <trd lng="id">duda</trd>
  <trd lng="io">vidvulo</trd>
  <trdgrp lng="ja">
    <trd>&#30007;&#12420;&#12418;&#12417;  <pr>&#x304A;&#12392;&#12371;&#12420;&#12418;&#12417;</pr></trd>,
    <trd>&#x5BE1;&#x592B;  <pr>&#x304B;&#12405;</pr></trd>
  </trdgrp>
  <trd lng="nl">weduwnaar</trd>
  <trd lng="pl">wdowiec</trd>
  <trd lng="pt">vi&uacute;vo</trd>
  <trd lng="ru">&c_v;&c_d;&c_o;&c_v;&c_je;&c_c;</trd>
  <trd lng="sk">vdovec</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_d;&c_ib;&c_v;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_u;&c_d;&c_ib;&c_v;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
  <trd lng="vo">hiviudan</trd>
  <trdgrp lng="zh">
    <trd>&#x5B40; <pr>shu&amacron;ng</pr></trd>,
    <trd>&#x9C25; <pr>gu&amacron;n</pr></trd>,
    <trd>&#x9CCF; <pr>gu&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vidv.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc>
    <dif>
      Virino perdinta sian edz(in)on kaj ne reedzini&gcirc;inta:
      <ekz>
        <tld/>inon kaj orfon ne premu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i estas <tld/>ino post homo, kiun mi konis<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_v;&c_a;</trd>
  <trd lng="ca">viuda</trd>
  <trd lng="cs">vdova <klr>(po n&ecaron;kom)</klr></trd>
  <trd lng="de">Witwe</trd>
  <trd lng="el">&chi;&eta_ton;&rho;&alfa;</trd>
  <trd lng="en">widow</trd>
  <trd lng="es">viuda</trd>
  <trd lng="fa"><klr>(&a_z;&a_n;) </klr>&a_b;&f_y;&a_w;&a_h;</trd>
  <trd lng="fr">veuve</trd>
  <trdgrp lng="fy">
    <trd>widdo</trd>,
    <trd>wjiddo</trd>,
    <trd>widdofrou</trd>
  </trdgrp>
  <trd lng="hu">&ouml;zvegyasszony</trd>
  <trd lng="id">janda</trd>
  <trd lng="io">vidvino</trd>
  <trdgrp lng="ja">
    <trd>&#22899;&#12420;&#12418;&#12417;  <pr>&#x304A;&#12435;&#x306A;&#12420;&#12418;&#12417;</pr></trd>,
    <trd>&#x5BE1;&#x5A66;  <pr>&#x304B;&#12405;</pr></trd>,
    <trd>&#x672A;&#x4EA1;&#x4EBA;  <pr>&#x307F;&#x307C;&#12358;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">weduwe</trd>
  <trd lng="pl">wdowa</trd>
  <trd lng="pt">vi&uacute;va</trd>
  <trd lng="ru">&c_v;&c_d;&c_o;&c_v;&c_a;</trd>
  <trd lng="sk">vdova</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_d;&#x43E;&c_v;&c_a;</trd>,
    <trd>&c_u;&c_d;&#x43E;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">jiviudan</trd>
  <trdgrp lng="zh">
    <trd>&#x5B40; <pr>shu&amacron;ng</pr></trd>,
    <trd>&#x5AE0; <pr>l&#xED;</pr></trd>,
    <trd>&#x5BE1;&#22919; <pr>gu&#x1CE;fu</pr></trd>,
    <trd>&#x5BE1;&#x5A66; <pr>gu&#x1CE;fu</pr></trd>,
    <trd>&#36951;&#x5B40; <pr>y&#xED;shu&amacron;ng</pr></trd>,
    <trd>&#x907A;&#x5B40; <pr>y&#xED;shu&amacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>
