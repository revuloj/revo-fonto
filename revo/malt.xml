<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: malt.xml,v 1.25 2022/03/23 14:01:05 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>malt</rad>/o
</kap>

<drv mrk="malt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Gcirc;erminta sekigita greno, ordinare <ref cel="horde.0o">hordeo</ref>, 
      interalie uzata por fabriki
      <ref tip="malprt" cel="bier.0o">bieron</ref>:
      <ekz>
        helkoloraj <tld/>oj de apudmaraj hordeoj<fnt><aut>Lode Van De
        Velde</aut>: <vrk>Bierlando Belgio</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="malt1.M0o">Malto (insulo)</ref>
  <trd lng="be">&c_s;&c_o;&c_l;&c_a;&c_d;</trd>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_c;</trd>
  <trd lng="br">malt</trd>
  <trd lng="ca">malt</trd>
  <trd lng="cs">slad</trd>
  <trd lng="de">Malz</trd>
  <trd lng="en">malt</trd>
  <trd lng="es">malta</trd>
  <trd lng="fa">&a_m;&a_A;&a_l;&a_t;</trd>
  <trd lng="fr">malt</trd>
  <trd lng="hu">mal&aacute;ta</trd>
  <trd lng="nl">mout</trd>
  <trd lng="pl">s&lstroke;&oacute;d</trd>
  <trd lng="pt">malte</trd>
  <trd lng="ro">mal&#x21B;</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_o;&c_d;</trd>
  <trd lng="sk">slad <klr>(pivn&yacute;)</klr></trd>
  <trd lng="sv">malt</trd>
  <trd lng="uk">&#x441;&#x43E;&#x43B;&#x43E;&#x434;</trd>
</drv>

<drv mrk="malt.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transformi grenon en <tld/>on:
      <ekz>
        en la grandbieno oni faris la sensoifigan blankan bieron el
        &ccirc;irka&ubreve;
        50 procentoj da ne<tld/>igita tritiko<fnt><aut>Lode Van De
        Velde</aut>: <vrk>Bierlando Belgio</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">maltar</trd>
  <trd lng="cs">sladovat</trd>
  <trd lng="de">m&auml;lzen</trd>
  <trd lng="es">maltear</trd>
  <trd lng="fr">malter</trd>
  <trd lng="pl">zmieni&cacute; w <ind/>s&lstroke;&oacute;d</trd>
  <trd lng="ro">schimbare &icirc;n <ind>mal&#x21B;</ind></trd>
  <trd lng="sk">sladova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x442;&#x432;&#x43E;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x432; &#x441;&#x43E;&#x43B;&#x43E;&#x434;</trd>
  </trdgrp>
</drv>
</art>
