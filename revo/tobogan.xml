<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tobogan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tobogan.0o">
  <kap><tld/>o</kap>
  <snc mrk="tobogan.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Infan&gcirc;ardena <ref tip="lst" cel="lud.0ilo"
      lst="voko:ludiloj">ludilo</ref>, dekliva glitvojo, sur kiu oni
      side
      a&ubreve; ku&scirc;e glitigas sin.
    </dif>
    <refgrp tip="sin">
       <ref cel="glit.de0ejo.tobogano">deglitejo</ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="tub.glit0o.tobogano">glittubo</ref>
    </refgrp>
  </snc>
  <trd lng="ca">tobogan</trd>
  <trd lng="cs">tobogán <klr>(skluzavka i sáňky)</klr></trd>
  <trd lng="de">Rutsche</trd>
  <trd lng="fr">tobogan</trd>
  <trdgrp lng="pl">
    <trd>&sacute;lizgawka</trd>,
    <trd>zje&zdot;d&zdot;alnia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>escorregador</trd>,
    <trd>escorrega</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_k;&c_a;<klr tip="amb">(&c_d;&c_l;&c_ja;
    &c_s;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_n;&c_i;&c_ja;)
    </klr></trd>,
    <trd>&c_t;&c_o;&c_b;&c_o;&c_g;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skĺzavka</trd>,
    <trd>tobogan <klr>(aj druh saní)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="tobogan.rel0o">
  <kap>rel<tld/>o</kap>
  <snc mrk="tobogan.rel0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Granda <tld/>o kun reloj en amuzparko, sur kiu supren kaj
      malsupren glitas vagonetoj:
      <ekz>
	ci tiu klasika turni&gcirc;anta ludilo <klr>...</klr> postvivis
	&gcirc;is nun en la jarcenta fino, konkurante kun rel<tld/>o
	<fnt><bib>HomojMan&gcirc;antaj</bib>,
	<lok>&leftquot;man&gcirc;i en vagono de
	spekto-radego&rightquot;, p. 123a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">muntanyes russes</trd>
  <trd lng="de">Sommerrodelbahn</trd>
  <trd lng="fr">montagnes russes</trd>
  <trd lng="pl">kolejka g&oacute;rska</trd>
  <trd lng="pt">montanha-russa</trd>
  <trd lng="ru">
  &c_a;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je;
  &c_g;&c_o;&c_r;&c_k;&c_i;</trd>
</drv>
</art>

