<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inund.xml,v 1.25 2023/11/19 10:22:14 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>inund</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="inund.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="inund.0i.MET">
    <dif>
      <ref tip="sin" cel="akv.super0i.KOMUNE">Superakvi</ref>:
      <ekz>
        la rivero <tld/>is la kampojn, detruante la rikoltojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bord.super0igxi">superbordi&gcirc;i</ref>,
      <ref cel="versx.super0i">superver&scirc;i</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_t;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zaplavit</trd>,
    <trd>zaplavovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&uuml;berschwemmen</trd>,
    <trd>&uuml;berfluten</trd>
  </trdgrp>
  <trd lng="en">flood</trd>
  <trd lng="es">inundar</trd>
  <trdgrp lng="fr">
    <trd>inonder</trd>,
    <trd>noyer</trd>,
    <trd>submerger</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&aacute;raszt</trd>,
    <trd>el&ouml;nt</trd>
  </trdgrp>
  <trd lng="id">mem<ind>banjir</ind>i</trd>
  <trdgrp lng="ja">
    <trd>&#x6C3E;&#x6FEB;&#x3059;&#x308B;  <pr>&#x306F;&#x3093;&#x3089;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6C34;&#x6D78;&#x3057;&#x306B;&#x3059;&#x308B; 
    <pr>&#x307F;&#x305A;&#x3073;&#x305F;&#x3057;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3042;&#x3075;&#x308C;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">overstromen</trd>
  <trdgrp lng="pl">
    <trd>zalewa&cacute;</trd>,
    <trd>zatapia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">inundar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zaplavi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&#x43E;&#x43F;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_v;&#x43E;&c_d;&#x43D;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6DF9; <pr>y&#x101;n</pr></trd>,
    <trd>&#x6C92; <pr>m&#xF2;</pr></trd>,
    <trd>&#x6DF9;&#x6C92; <pr>y&#x101;nm&#xF2;</pr></trd>,
    <trd>&#x6D1A; <pr>ji&#xE0;ng</pr></trd>,
    <trd>&#x6D41;&#x6CFB; <pr>li&#xFA;xi&#xE8;</pr></trd>,
    <trd>&#x5145;&#x65A5; <pr>ch&#x14D;ngch&#xEC;</pr></trd>,
    <trd>&#x6D8C;&#x5411; <pr>y&#x1D2;ngxi&#xE0;ng</pr></trd>,
    <trd>&#x8E8F; <pr>l&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="inund.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="inund.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="sin" cel="akv.super0o.MET">Superakvo</ref>:
      <ekz>
        la <ind><tld/>o</ind> kovris la tutan ebena&jcirc;on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bo">&#xF46;&#xF74;&#xF0B;&#xF63;&#xF7C;&#xF42;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>&Uuml;berschwemmung</trd>,
    <trd>&Uuml;berflutung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crue</trd>,
    <trd>inondation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rad&aacute;s</trd>,
    <trd>&aacute;rv&iacute;z</trd>
  </trdgrp>
  <trd lng="id">banjir</trd>
  <trdgrp lng="ja">
    <trd>&#x6C3E;&#x6FEB;  <pr>&#x306F;&#x3093;&#x3089;&#x3093;</pr></trd>,
    <trd>&#x6D2A;&#x6C34;  <pr>&#x3053;&#x3046;&#x305A;&#x3044;</pr></trd>,
    <trd>&#x5145;&#x6E80;  <pr>&#x3058;&#x3085;&#x3046;&#x307E;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">pow&oacute;d&zacute;</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_v;&c_ib;&#x43D;&#x44C;</trd>,
    <trd>&#x43F;&#x43E;&c_v;&c_ib;&c_d;&#x44C;</trd>,
    <trd>&#x43F;&c_a;&c_v;&#x43E;&c_d;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6D2A; <pr>h&#xF3;ng</pr></trd>,
    <trd>&#x6D2A;&#x6C34; <pr>h&#xF3;ngshu&#x1D0;</pr></trd>,
    <trd>&#x5927;&#x6C34; <pr>d&#xE0;shu&#x1D0;</pr></trd>,
    <trd>&#x6C94; <pr>mi&#x1CE;n</pr></trd>,
    <trd>&#x6D1A; <pr>ji&#xE0;ng</pr></trd>,
    <trd>&#x6D25;&#x6C34; <pr>j&#x12B;nshu&#x1D0;</pr></trd>,
    <trd>&#x6D2A;&#x6D9D; <pr>h&#xF3;ngl&#xE0;o</pr></trd>,
    <trd>&#x6C34;&#x60A3; <pr>shu&#x1D0;hu&#xE0;n</pr></trd>,
    <trd>&#x6C34;&#x707D; <pr>shu&#x1D0;z&#x101;i</pr></trd>
  </trdgrp>
</drv>
</art>
