<art mrk="$Id: inund.xml,v 1.27 2025/03/30 17:18:22 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>inund</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="inund.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="inund.0i.MET">
    <dif>
      <ref tip="sin" cel="akv.super0i.KOMUNE">Superakvi</ref>:
      <ekz>
        la rivero <tld/>is la kampojn, detruante la rikoltojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bord.super0igxi">superbordi&gcirc;i</ref>,
      <ref cel="versx.super0i">superver&scirc;i</ref>,
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>superakvi</ke>, <ke>subakvigi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_t;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zaplavit</trd>,
    <trd>zaplavovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&uuml;berschwemmen</trd>,
    <trd>&uuml;berfluten</trd>
  </trdgrp>
  <trd lng="en">flood</trd>
  <trd lng="es">inundar</trd>
  <trdgrp lng="fr">
    <trd>inonder</trd>,
    <trd>noyer</trd>,
    <trd>submerger</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&aacute;raszt</trd>,
    <trd>el&ouml;nt</trd>
  </trdgrp>
  <trd lng="id"><baz>banjir</baz>membanjiri</trd>
  <trdgrp lng="ja">
    <trd>&#27710;&#28651;&#12377;&#12427;  <pr>&#12399;&#12435;&#12425;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#27700;&#28024;&#12375;&#12395;&#12377;&#12427;
    <pr>&#12415;&#12378;&#12403;&#12383;&#12375;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#12354;&#12405;&#12428;&#12427;</trd>
  </trdgrp>
  <trd lng="nl">overstromen</trd>
  <trdgrp lng="pl">
    <trd>zalewa&cacute;</trd>,
    <trd>zatapia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">inundar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zaplavi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&#1086;&#1087;&#1083;&#1103;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#1083;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1085;&c_a;&c_v;&#1086;&c_d;&#1085;&#1103;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28153; <pr>y&amacron;n</pr></trd>,
    <trd>&#27794; <pr>m&#242;</pr></trd>,
    <trd>&#28153;&#27794; <pr>y&amacron;nm&#242;</pr></trd>,
    <trd>&#27930; <pr>ji&#224;ng</pr></trd>,
    <trd>&#27969;&#27899; <pr>li&#250;xi&#232;</pr></trd>,
    <trd>&#20805;&#26021; <pr>ch&#333;ngch&#236;</pr></trd>,
    <trd>&#28044;&#21521; <pr>y&#466;ngxi&#224;ng</pr></trd>,
    <trd>&#36495; <pr>l&#236;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="inund.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="inund.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="sin" cel="akv.super0o.MET">Superakvo</ref>:
      <ekz>
        la <ind><tld/>o</ind> kovris la tutan ebena&jcirc;on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bo">&#3910;&#3956;&#3851;&#3939;&#3964;&#3906;&#3851;</trd>
  <trdgrp lng="de">
    <trd>&Uuml;berschwemmung</trd>,
    <trd>&Uuml;berflutung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crue</trd>,
    <trd>inondation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rad&aacute;s</trd>,
    <trd>&aacute;rv&iacute;z</trd>
  </trdgrp>
  <trd lng="id">banjir</trd>
  <trdgrp lng="ja">
    <trd>&#27710;&#28651;  <pr>&#12399;&#12435;&#12425;&#12435;</pr></trd>,
    <trd>&#27946;&#27700;  <pr>&#12371;&#12358;&#12378;&#12356;</pr></trd>,
    <trd>&#20805;&#28288;  <pr>&#12376;&#12421;&#12358;&#12414;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">pow&oacute;d&zacute;</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&#1086;&c_v;&c_ib;&#1085;&#1100;</trd>,
    <trd>&#1087;&#1086;&c_v;&c_ib;&c_d;&#1100;</trd>,
    <trd>&#1087;&c_a;&c_v;&#1086;&c_d;&#1086;&#1082;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27946; <pr>h&#243;ng</pr></trd>,
    <trd>&#27946;&#27700; <pr>h&#243;ngshu&#464;</pr></trd>,
    <trd>&#22823;&#27700; <pr>d&#224;shu&#464;</pr></trd>,
    <trd>&#27796; <pr>mi&#462;n</pr></trd>,
    <trd>&#27930; <pr>ji&#224;ng</pr></trd>,
    <trd>&#27941;&#27700; <pr>j&#299;nshu&#464;</pr></trd>,
    <trd>&#27946;&#28061; <pr>h&#243;ngl&#224;o</pr></trd>,
    <trd>&#27700;&#24739; <pr>shu&#464;hu&#224;n</pr></trd>,
    <trd>&#27700;&#28797; <pr>shu&#464;z&amacron;i</pr></trd>
  </trdgrp>
</drv>
</art>
