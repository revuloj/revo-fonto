<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>nitrat</rad>/o</kap>
<drv mrk="nitrat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Salo de <ref cel="nitrat.0a.acido">nitrata acido</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Nitrat</trd>
  <trd lng="en">nitrate</trd>
  <trd lng="pt">nitrato</trd>
  <trd lng="fr">nitrate</trd>
  <trd lng="hu">nitr&aacute;t</trd>
  <trd lng="nl">nitraat</trd>
  <trd lng="pl">nitrat</trd>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_a;&c_t;</trd>
  <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_a;&c_t;</trd>
</drv>
<drv mrk="nitrat.0a.acido">
  <kap><tld/>a acido</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Tre aktiva neorganika <ref tip="super" cel="acid.0o">acido</ref>
      (<frm>HNO<sub>3</sub></frm>), 
      vaste uzata en industrio.
    </dif>
  </snc>
  <ref tip="sin" cel="azot.0acido">azotacido</ref>
  <trd lng="be">&c_a;&c_z;&c_o;&c_t;&c_n;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</ind></trd> 
  <trdgrp lng="de">
	<trd>Salpeters&auml;ure</trd>,
	<trd>Scheidewasser</trd>
  </trdgrp>
  <trd lng="en">azotic acid</trd>
  <trd lng="pt">&aacute;cido n&iacute;trico</trd>
  <trd lng="es">&aacute;cido <ind>n&iacute;trico</ind></trd>
  <trd lng="fr">acide <ind>nitrique</ind></trd>
  <trdgrp lng="hu">
	<trd>sal&eacute;tromsav</trd>,
	<trd>v&aacute;laszt&oacute;v&iacute;z</trd>
  </trdgrp>
  <trd lng="nl">salpeterzuur</trd>
  <trd lng="pl">kwas azotowy</trd>
  <trd lng="ru">&c_a;&c_z;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
  <trd lng="uk">&c_a;&c_z;&c_o;&c_t;&c_n;&c_a; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
</drv>

</art>

