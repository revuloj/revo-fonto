<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mili</rad>/o
</kap>
<drv mrk="mili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Panicum miliaceum</trd>),
      malnova grenplanto, uzata por ka&ccirc;o, pano kaj por nutrado
      de hejmaj birdoj.
    </dif>
  </snc>                    
  <trd lng="be">&c_p;&c_r;&c_o;&c_s;&c_a;</trd>
  <trd lng="br">mell<klr tip="ind"> (plant)</klr></trd>
  <trd lng="de">Hirse</trd>
  <trd lng="en">millet</trd>
  <trd lng="fr">millet</trd>
  <trd lng="hu">k&ouml;les</trd>
  <trd lng="nl">gierst</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_o;</trd>
</drv>
</art>
