<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mili.xml,v 1.26 2024/08/30 21:26:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mili</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="mili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mili.0o.BOT">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Panicum miliaceum</trd>),
      malnova grenplanto, uzata por <ref cel="kacx.0o.KUI">ka&ccirc;o</ref>,
      pano a&ubreve; por nutrado de hejmaj birdoj:
      <ekz>
        prenu al vi tritikon kaj hordeon, fabojn kaj lentojn, <tld/>on kaj spelton, kaj
        <klr>[&#8230;]</klr> faru al vi el tio panon
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 4:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni pagis &ccirc;iujn impostojn: en <tld/>o, tritiko, floroj kaj brutaj
        feloj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 12a</lok></fnt>;
      </ekz>
      <ekz>
        la anoj de <nom>Betamariba</nom> konstruas siajn domojn <klr>[&#8230;]</klr> sur kampoj de sorgo
        kaj <tld/>o &ndash; iliaj &ccirc;efaj nutra&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Adj&eacute; Adj&eacute;vi</aut>
          <vrk><url ref="&Monato;/2012/010618.php"
              >Kutamaku, la lando de Betamariba</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_s;&c_a;</trd>
  <trd lng="br">mell<klr tip="ind"> (plant)</klr></trd>
  <trd lng="ca">mill</trd>
  <trdgrp lng="cs">
    <trd>proso obecn&eacute;</trd>,
    <trd>proso set&eacute;</trd>
  </trdgrp>
  <trd lng="de">Hirse</trd>
  <trd lng="en">millet</trd>
  <trd lng="fr">millet</trd>
  <trd lng="hu">k&ouml;les</trd>
  <trd lng="ja">&#38609;&#31296;  <pr>&#12374;&#12387;&#12371;&#12367;</pr></trd>
  <trd lng="nl">gierst</trd>
  <trd lng="pl">proso</trd>
  <trdgrp lng="pt">
   <trd>pain&ccedil;o</trd>,
   <trd>milho-mi&uacute;do</trd>,
   <trd>milhete</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_o;</trd>
  <trd lng="sk">proso <klr>(rastlina)</klr></trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_s;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#40653; <pr>sh&#468;</pr></trd>,
    <trd>&#31903; <pr>s&ugrave;</pr></trd>,
    <trd>&#31200; <pr>p&imacron;</pr></trd>,
    <trd>&#23567;&#31859; <pr>xi&#462;om&#464;</pr></trd>,
    <trd>&#34364; <pr>m&iacute;</pr></trd>,
    <trd>&#31300; <pr>j&igrave;</pr></trd>,
    <trd>&#31211; <pr>sh&uacute;</pr></trd>
  </trdgrp>
</drv>
</art>
