<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: maka.xml,v 1.6 2023/09/02 06:04:08 afido Exp $">
    <kap>
      <rad>Maka</rad>/o
    </kap>

<drv mrk="maka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="maka.0o.havenurbo">
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en suda <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>,
      50&#x202F;km okcidente de <ref cel="honkong.0o">Honkongo</ref>; &gcirc;is 1999 portugala kolonio:
      <ekz>
        neniu alia lando superos la kreskon de &ccirc;. 14&#x202F;% en <tld/>o, kaj en ties prognozo
        oni e&ccirc; ne kalkulas la sukceson sur la ludotabloj de la lokaj kazinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/011053.php">Ekonomia fato</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is 2020 la kazino-merkato en Azio saturi&gcirc;os kaj tiam akri&gcirc;os konkurenco kun
        <tld/>o, Singapuro, Koreio
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011375.php">Kanibalismo - kaj kazinoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La&ubreve; la urbo oni nomas anka&ubreve; kelkajn hazardludojn &leftquot;makao&rightquot;.
    </rim>
    <trd lng="be">&c_M;&c_a;&c_k;&c_a;&c_w;</trd>
    <trd lng="de">Macau</trd>
    <trd lng="en">Macau</trd>
    <trd lng="fr">Macao</trd>
    <trd lng="pl">Makao</trd> 
    <trd lng="pt">Macau</trd>
    <trd lng="ro">Macao</trd>
    <trd lng="zh">&#x6FB3;&#x9580;</trd>
  </snc>
  <trd lng="ja">&#x30DE;&#x30AB;&#x30AA;</trd>
  <trd lng="uk">&#x41C;&c_a;&#x43A;&c_a;&#x43E;</trd>
</drv>

  </art>

