<kap>
  <ofc>*</ofc>
  <rad>glob</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="glob.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="glob.0o.FORM">
    <dif>
      Fizika objekto, <ref tip="lst" lst="voko:formoj" cel="korp.0o.objekto">korpo<sncref/></ref>,
      kies supra&jcirc;o pli a&ubreve; malpli
      &gcirc;uste formas <ref cel="sfer.0o.surfaco">sferon<sncref/></ref>:
      <ekz>
        la tero estas <tld/>o
        <fnt><bib>EE</bib> <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz><ekz>
        <uzo tip="fak">SPO</uzo>
        bilarda <tld/>o;
      </ekz><ekz>
        <uzo tip="fak">TEK</uzo>
        la <tld/>oj de lagro;
      </ekz><ekz>
        <uzo tip="fak">GEOG</uzo>
        <ref cel="glob.ter0o">ter<tld/>o</ref>;
      </ekz><ekz>
        <uzo tip="fak">GEOG</uzo>
        kartona <ind>geografia <tld/>o</ind>
        <klr>(t.e. <ref cel="globus.0o">globuso</ref>)</klr>;
        <trdgrp lng="es">
          <trd>globo terrestre</trd>,
          <trd>globo terr&aacute;queo</trd>,
          <trd>orbe</trd>
        </trdgrp>
        <trd lng="fr">globe<klr tip="ind">
        (repr&eacute;sentant la Terre)</klr></trd>
        <trd lng="hu">f&ouml;ldg&ouml;mb</trd>
        <trd lng="nl">wereldbol</trd>
      </ekz><ekz>
        <uzo tip="fak">ANA</uzo>
        <ind>okul<tld/>o</ind>;
        <trd lng="es"><ind>globo</ind> ocular</trd>
        <trd lng="fr">globe oculaire</trd>
        <trd lng="hu">szemgoly&oacute;</trd>
        <trdgrp lng="nl">
          <trd>oogbal</trd>,
          <trd>oogbol</trd>
        </trdgrp>
        <trd lng="pl">ga&lstroke;ka oczna</trd>
        <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_n;&c_o;&c_je; &c_ja;&c_b;&c_l;&c_o;&c_k;&c_o;</trd>
      </ekz><ekz>
        <ref cel="krajon.glob0o"><tld/>krajono</ref>;
      </ekz>
      <ekz>
        amikoj de Esperanto, kiuj reprezentas per si preska&ubreve; &ccirc;iujn
        landojn de la tera <tld/>o
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne
          sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni ne havas kristalan <tld/>on <klr>(kiel uzatan de
        a&ubreve;guristoj)</klr>, do ni nur atendu kaj vidu
        <fnt>
         <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2000/006821.php">E&Ubreve;ROPA UNIO:
          E&ubreve;ro unujara</url></vrk>
           <bib>Monato</bib>
          <lok>2000/03, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kugel</trd>
    <trdgrp lng="es">
      <trd>bola</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>globe</trd>,
      <trd>boule</trd>
    </trdgrp>
    <trd lng="hu">bili&aacute;rdgoly&oacute;</trd>
    <trdgrp lng="nl">
      <trd>bal</trd>,
      <trd>bol</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x67AA;&#x5F39; <pr>qi&#x101;ngd&#xE0;n</pr></trd>,
    <trd>&#x5F39; <pr>d&#xE0;n</pr></trd>,
    <trd>&#x73E0; <pr>zh&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="glob.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <subsnc mrk="glob.0o.metrikaspaco">
      <dif>
        <klr>(<tld lit="G"/>o kun centro <frm am="a"><k>a</k></frm>
        kaj radiuso <frm am="rho">&rho;</frm>)</klr>
        Aro da &ccirc;iuj tiaj <ref cel="punkt.0o.spaco">punktoj</ref>
        el <ref cel="spac.metrika0o">metrika spaco</ref>, ke ilia
        <ref cel="distan.0o.inter_punktoj">distanco</ref>
        <frm am="d"><k>d</k></frm> de <frm am="a"><k>a</k></frm> la
        radiuson <frm am="rho">&rho;</frm> malsuperas
        (<frm am="d lt rho"><k>d</k>&lt;&rho;</frm>, por
        &leftquot;<ref cel="ferm.mal0ita.metrikaspaco">malfermita</ref>
        <tld/>o&rightquot;),
        a&ubreve; ne superas (<frm am="d le rho"><k>d</k>&#x2264;&rho;</frm>, por
        &leftquot;<ref cel="ferm.0ita.topologio">fermita</ref> <tld/>o&rightquot;).
      </dif>
      <rim>
        &Ccirc;i-sence troveblas anka&ubreve; <ctl>bulo</ctl>
        <fnt><bib>EKV</bib><lok>&para;53</lok></fnt>.
      </rim>
      <trd lng="de">Kugel</trd>
      <trd lng="en">ball<klr tip="ind"> (in a metric space)</klr></trd>
      <trd lng="es">bola<klr tip="ind"> (de un espacio
      m&eacute;trico)</klr></trd>
      <trd lng="fr">boule<klr tip="ind"> (dans un espace
      m&eacute;trique)</klr></trd>
      <trd lng="pl">kula<klr> (w przestrzeni metrycznej)</klr></trd>
      <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <ind>&c_sh;&c_a;&c_r;</ind></trd>
    </subsnc>
    <subsnc mrk="glob.0o.sfero">
      <fnt><bib>PIV2</bib></fnt>
      <dif>
        (Speciale:) Same difinita (fermita a&ubreve; malfermita)
        <tld/>o<sncref ref="glob.0o.metrikaspaco"/> 
        (<ref tip="super" cel="solid.0o.geometrio">solido<sncref/></ref>) en
        tridimensia e&ubreve;klida spaco:
        <ekz>
          la volumeno <frm am="V">V</frm> de <tld/>o havanta la radiuson
          <frm am="r">r</frm> estas <frm am="V=4pi r^3//3">V = 4&pi;r&cubic;/3</frm>;
        </ekz>
        <ekz>
          <uzo tip="fak">SPO</uzo>
          la <ind>pez<tld/>o</ind> estas fera a&ubreve; latuna <tld/>o kun
          difinitaj pezo kaj diametro
          <fnt>
            <aut>T. Ujlaky-Nagy</aut>:
            <vrk><url ref="http://www.freeweb.hu/eventoj/steb/sportalingvo.rtf">
            La sporta lingvo en Esperanto</url></vrk>.
            <lok>1972</lok>
          </fnt>.
          <trd lng="ru">&c_ja;&c_d;&c_r;&c_o;<klr> (&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je;)</klr></trd>
      </ekz>
      </dif>
      <trd lng="de">Kugel</trd>
      <trdgrp lng="en">
        <trd>sphere</trd>,
        <trd>ball<klr tip="ind"> (solid)</klr></trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>esfera</trd>,
        <trd>bola<klr tip="ind"> (volumen esf&eacute;rico)</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>sph&egrave;re</trd>,
        <trd>boule<klr tip="ind"> (volume sph&eacute;rique)</klr></trd>
      </trdgrp>
      <trd lng="pl">kula<klr> (bry&lstroke;a)</klr></trd>
      <trd lng="ru">&c_sh;&c_a;&c_r;<klr tip="ind"> (&c_t;&c_je;&c_l;&c_o;)</klr></trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="centr.0o.cirklo">centro</ref>,
      <ref cel="diamet.0o.cirklo">diametro</ref>,
      <ref cel="radius.0o.cirklo">radiuso</ref>;
    </refgrp>
    <refgrp tip="prt">
      <ref cel="segmen.0o.sfero">segmento</ref>,
      <ref cel="tavol.glob0o">globtavolo</ref>,
      <ref cel="disk.sfer0o"><tld/>disko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">koule</trd>
  <trd lng="id">globe</trd>
  <trdgrp lng="ja">
    <trd>&#x7403;&#x4F53;  <pr>&#x304D;&#x3085;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x7403;  <pr>&#x305F;&#x307E;</pr></trd>,
    <trd>&#x7389;  <pr>&#x304E;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x92FC;&#x7403;  <pr>&#x3053;&#x3046;&#x304D;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">bol</trd>
  <trdgrp lng="pt">
   <trd>globo</trd>, 
   <trd>bola</trd>, 
   <trd>orbe</trd> 
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_r;</trd>
  <trd lng="sk">gu&lcaron;a</trd>
  <trd lng="uk">&#x43A;&c_u;&#x43B;&#x44F;</trd>
</drv>

<drv mrk="glob.0a">
  <kap><tld/>a</kap>
  <snc mrk="glob.0a.FORM">
    <dif>
      <tld lit="G"/>forma:
      <ekz>
        <ind><tld/>a fulmo</ind>
        <klr>(lumanta <tld/>o &scirc;vebanta en aerfluo)</klr>
	<fnt>
	  <aut>M. Lewin</aut>
          <vrk>Meteologia terminaro</vrk>
          <lok>1961</lok>
	</fnt>;
        <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_a;&c_ja; &c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;</trd>
      </ekz><ekz>
        <tld/>e kunvolvita tuketo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_r;&c_a;&c_a;&c_b;&c_r;&c_y;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_a;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;</trd>,
      <trd>&c_s;&c_f;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">kugelig</trd>
    <trd lng="es">esf&eacute;rico</trd>
    <trdgrp lng="fr">
      <trd>rond<klr tip="ind"> (sph&eacute;rique)</klr></trd>,
      <trd>sph&eacute;rique</trd>
    </trdgrp>
    <trd lng="hu">g&ouml;mb alak&uacute;</trd>
    <trd lng="id">sferis</trd>
    <trd lng="nl">bolvormig</trd>
    <trd lng="pl">kulisty</trd>
    <trd lng="pt">globular</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_a;&c_r;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_sh;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#x7403;&#x5F62; <pr>qi&#xFA;x&#xED;ng</pr></trd>
</snc>
  <snc mrk="glob.0a.tergloba">
    <dif>
      Tuttera, tutter<tld/>a:
      <ekz>
        jam delonge ekzistas GPS (<tld/>a pozicia sistemo), aro da
        satelitoj el kiuj &ccirc;iam tri estas videblaj en preska&ubreve;
        &ccirc;iuj lokoj de la mondo
        <fnt>
          <aut>R. Rotsaert</aut>
          <vrk>Galileo defios Usonon</vrk>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>global</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_l;&c_o;&c_b;&c_a;&c_l;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_s;&c_je;&c_m;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_ra;&t_gigu;&t_la;&t_tsheg;&t_ra;&t_gigu;&t_la;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>kulat&yacute;</trd>,
    <trd>kulov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7403;&#x4F53;&#x306E;  <pr>&#x304D;&#x3085;&#x3046;&#x305F;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x7403;&#x306E;  <pr>&#x305F;&#x307E;&#x306E;</pr></trd>,
    <trd>&#x7403;&#x5F62;&#x306E;  <pr>&#x304D;&#x3085;&#x3046;&#x3051;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x7403;&#x72B6;&#x306E;  <pr>&#x305F;&#x307E;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="sk">gu&lcaron;at&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u;&#x43B;&#x44F;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43A;&c_r;&c_u;&c_g;&#x43B;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glob.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="glob.0eto.FORM">
    <dif>
      Malgranda <tld/>o:
      <ekz>
        aer<tld/>eto;
      </ekz>
      <ekz>
        <uzo tip="fak">BIO</uzo>
        sangaj <tld/>etoj;
      </ekz>
      <ekz>
        <uzo tip="fak">MED</uzo>
        farmaciaj <tld/>etoj;
      </ekz>
      <ekz>
        <uzo tip="fak">SPO</uzo>
        ludi <tld/>etojn;
      </ekz>
      <ekz>
        en la densa barbo li havis vitran globeton, el kiu fluis
        parfumoj
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 13a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Mi ne certas, pri kio temas la farmaciaj ~etoj.
    &Ccirc;u sinonimoj de &leftquot;granolo&rightquot;?
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_sh;&c_a;&c_r;&c_y;&c_k;</trd>
  <trd lng="de">K&uuml;gelchen</trd>
  <trdgrp lng="es">
    <trd>gl&oacute;bulo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>globule</trd>,
    <trd>bulle</trd>,
    <trd>bille</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x7403;&#x4F53;  <pr>&#x3057;&#x3087;&#x3046;&#x304D;&#x3085;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x30D3;&#x30FC;&#x7389;  <pr>&#x30D3;&#x30FC;&#x3060;&#x307E;</pr></trd>,
    <trd>&#x7403;  <pr>&#x305F;&#x307E;</pr></trd>,
    <trd>&#x73E0;  <pr>&#x305F;&#x307E;</pr></trd>
  </trdgrp>
  <trd lng="nl">bolletje</trd>
  <trd lng="pl">kulka</trd>
  <trd lng="pt">gl&oacute;bulo</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_i;&c_k;</trd>
  <trd lng="sk">gu&lcaron;ka</trd>
  <trd lng="cs">kuli&ccaron;ka</trd>
  <trd lng="uk">&#x43A;&c_u;&#x43B;&#x44C;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x73E0; <pr>shu&#x1D0;zh&#x16B;</pr></trd>,
    <trd>&#x5C0F;&#x7403; <pr>xi&#x1CE;oqi&#xFA;</pr></trd>,
    <trd>&#x4E38; <pr>w&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glob.blanka0eto">
  <kap>blanka <tld/>eto</kap>
  <snc mrk="glob.blanka0eto.FIZL">
    <dif>
      <ref tip="dif" cel="cxel.blankasango0o.FIZL">Blanka
      sango&ccirc;elo</ref>:
      <ekz>
        tromulti&gcirc;o de blankaj <tld/>etoj en la sango
        <fnt>
          <bib>PV</bib>,
          <lok>le&ubreve;kemio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">gl&oacute;bulo blanco</trd>
  <trd lng="id"><ind>sel</ind> darah putih</trd>
</drv>

<drv mrk="glob.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="glob.duon0o.FORM">
    <dif>
      Duono de <tld/>o; objekto havanta tian formon:
      <ekz>
        <uzo tip="fak">ANA</uzo>
        la <ind>dekstra duon<tld/>o de la cerbo</ind>;
        <trdgrp lng="de">
          <trd>Halbkugel</trd>,
          <trd>Hemisph&auml;re</trd>
        </trdgrp>
        <trd lng="en">hemisphere</trd>
        <trd lng="es"><ind>hemisfero</ind> derecho</trd>
        <trd lng="hu">jobb <ind>f&eacute;lteke</ind></trd>
        <trd lng="nl">rechter hersenhelft</trd>
        <trd lng="pl">p&oacute;&lstroke;kula</trd>
        <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_o;&c_je; &c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je; &c_m;&c_o;&c_z;&c_g;&c_a;</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <ind>norda duon<tld/>o</ind>;
        <trd lng="en">northern hemisphere</trd>
        <trd lng="es"><ind>hemisferio</ind> norte</trd>
        <trd lng="hu">&eacute;szaki <ind>f&eacute;lg&ouml;mb</ind></trd>
        <trd lng="id"><ind>belah</ind>an bumi utara</trd>
        <trd lng="nl">noordelijk <ind>halfrond</ind></trd>
        <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_je; &c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je; <klr>&c_Z;&c_je;&c_m;&c_l;&c_i;</klr></trd>
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <ind>orienta duon<tld/>o</ind>;
        <trd lng="en">eastern hemisphere</trd>
        <trd lng="es"><ind>hemisferio</ind> oriental</trd>
        <trd lng="hu">keleti <ind>f&eacute;lg&ouml;mb</ind></trd>
        <trd lng="id"><ind>belah</ind>an bumi timur</trd>
        <trd lng="nl">oostelijk <ind>halfrond</ind></trd>
        <trd lng="ru">&c_v;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_o;&c_je; &c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je; <klr>&c_Z;&c_je;&c_m;&c_l;&c_i;</klr></trd>
      </ekz>
      <ekz>
        antipodo <klr>estas</klr> loko de la tero ku&scirc;anta sub niaj
        piedoj sur la alia duon<tld/>o <fnt>PV</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_sh;&c_a;&c_r;'&c_je;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_k;&c_u;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">polokoule Zem&ecaron;</trd>
  <trd lng="en">hemisphere</trd>
  <trd lng="es">hemisferio</trd>
  <trd lng="fr">h&eacute;misph&egrave;re</trd>
  <trd lng="hu">f&eacute;lg&ouml;mb</trd>
  <trd lng="id">hemisfer</trd>
  <trd lng="ja">&#x534A;&#x7403;  <pr>&#x306F;&#x3093;&#x304D;&#x3085;&#x3046;</pr></trd>
  <trdgrp lng="nl">
