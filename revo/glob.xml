<drv mrk="glob.jxet0o">
  <kap>&jcirc;et<tld/>o</kap>
  <snc mrk="glob.jxet0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
       <tld/>o plejofte ligna a&ubreve; metala, tenebla per unu mano,
       uzata en diversaj ludoj en kiuj ludantoj provas ilin
       &jcirc;eti plejeble proksimen al iu celo:
       <ekz>
         diversaj &jcirc;et<tld/>ludoj estas popularaj precipe
         en Kroatio, Italio, Francio;
       </ekz>
      <ekz>
        dum la posttagmezoj diman&ccirc;aj mi iris tien kun
        via patro por ludi je &jcirc;et<tld/>oj.
        <fnt>
          <aut>I. Silone, trad. D. Mistretta</aut>
          <vrk><url
          ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">
          La sekreto de Luka</url></vrk>
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_d;&c_r;&c_o; <klr>(&c_s;&c_p;&c_a;&c_r;&c_t;&c_o;&c_v;&c_a;&c_je;)</klr></trd>
  <trd lng="es">bocha<klr> (del juego de bochas)</klr></trd>
  <trd lng="fr">boule<klr> (de jeu de boule)</klr></trd>
  <trd lng="it">boccia</trd>
  <trd lng="ja">&#x7832;&#x4E38;  <pr>&#x307B;&#x3046;&#x304C;&#x3093;</pr></trd>
  <trd lng="uk">&c_ja;&c_d;&c_r;&c_o;</trd>
</drv>

<drv mrk="glob.kristall0o">
  <kap>
    kristall<tld/>o, <var><kap>vitro<tld/>o</kap></var>
  </kap>
  <snc>
