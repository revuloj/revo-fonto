<drv mrk="glob.kava0o">
  <kap>kava <tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="solid.0o.geometrio">Solido</ref>
      konsistanta el &ccirc;iuj punktoj inter du samcentraj sferoj:
      <ekz>
        la terkrusto kaj la atmosfero formas kavajn <tld/>ojn.
      </ekz>
    </dif>
  </snc>
