        &gcirc;i trapasas &ccirc;iujn meridianojn egalangule kaj rezultigas la saman
        kalkulon kiel per rekta linio desegnita sur plat<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27499">Kiblo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_ib;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
