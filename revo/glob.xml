<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glob</rad>/o
</kap>
<drv mrk="glob.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sferforma, bulforma korpo a&ubreve; objekto:
      <ekz>
        ronda balono estas <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        <ind>ter<tld/>o</ind>;
        <trd lng="es"><ind>globo</ind> terraqueo</trd>
        <trd lng="fr">globe terrestre<klr tip="ind"> (la Terre)</klr></trd>
        <trd lng="hu">f&ouml;ldgoly&oacute;</trd>
        <trd lng="nl">aardbol</trd>
        <trd lng="pl">kula</trd>
        <trd lng="ru">&c_z;&c_je;&c_m;&c_n;&c_o;&c_j; &c_sh;&c_a;&c_r;</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        kartona <ind>geografia <tld/>o</ind>
        <klr>(<ref cel="globus.0o">globuso</ref>)</klr>;
        <trd lng="fr">globe<klr tip="ind">
        (repr&eacute;sentant la Terre)</klr></trd>
        <trd lng="hu">f&ouml;ldg&ouml;mb</trd>
        <trd lng="nl">wereldbol</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">ANA</uzo>
        <ind>okul<tld/>o</ind>;
        <trd lng="es"><ind>globo</ind> ocular</trd>
        <trd lng="fr">globe oculaire</trd>
        <trd lng="hu">szemgoly&oacute;</trd>
        <trdgrp lng="nl">
          <trd>oogbal</trd>,
          <trd>oogbol</trd>
        </trdgrp>
        <trd lng="pl">ga&lstroke;ka oczna</trd>
        <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_n;&c_o;&c_je;
&c_ja;&c_b;&c_l;&c_o;&c_k;&c_o;</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">SPO</uzo>
        <tld/>o de bilardo.
      </ekz>
    </dif>
    <trd lng="de">Kugel</trd>
    <trd lng="es">globo</trd>
    <trdgrp lng="fr">
      <trd>globe</trd>,
      <trd>boule</trd>
    </trdgrp>
    <trd lng="hu">bili&aacute;rdgoly&oacute;</trd>
    <trdgrp lng="nl">
      <trd>bal</trd>,
      <trd>bol</trd>
    </trdgrp>
  </snc>
  <snc mrk="glob.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <subsnc mrk="glob.0o.metrikaspaco">
      <dif>
        <klr>(kun centro <frm><k>a</k></frm> kaj radiuso <frm>&rho;</frm>,
        en <ref cel="spac.metrika0o">metrika spaco</ref>)</klr>
        Subaro konsistanta el &ccirc;iuj
        <ref cel="punkt.0o.spaco">punktoj</ref>, kies
        <ref cel="distan.0o.inter_punktoj">distanco</ref> al 
        <frm><k>a</k></frm> estas malpli granda ol <frm>&rho;</frm>:
        <ekz>
          <tld/>oj estas
          <ref cel="ferm.mal0ita.metrikaspaco">malfermitaj</ref> subaroj.
        </ekz>
      </dif>
      <rim>
        &Ccirc;i-sence troveblas anka&ubreve; <ctl>bulo</ctl>
        <fnt><bib>EKV</bib><lok>&para;53</lok></fnt>.
        Eblas difini anka&ubreve; <ctl>fermitajn <tld/>ojn</ctl>, se oni
        allasas, ke la distanco al la centro egalu la radiuson. Kontrastcele
        oni foje nomas la &ccirc;i-supre difinitajn <tld/>ojn <ctl>malfermitaj
        <tld/>oj</ctl>.
      </rim>
      <trd lng="de">Kugel</trd>
      <trd lng="en">ball<klr tip="ind"> (in a metric space)</klr></trd>
      <trd lng="fr">boule<klr tip="ind"> (dans un espace m&eacute;trique)</klr></trd>
      <trd lng="pl">kula<klr> (w przestrzeni metrycznej)</klr></trd>
      <trd lng="ru">&c_sh;&c_a;&c_r;<klr tip="ind"> (&c_v; &c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_m; &c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_je;)</klr></trd>
    </subsnc>
    <subsnc mrk="glob.0o.sfero">
      <refgrp tip="dif">
        <ref cel="sfer.0o.solido">sfero</ref> (kiel solido).
      </refgrp>
      <trd lng="de">Kugel</trd>
      <trdgrp lng="en">
        <trd>sphere</trd>,
        <trd>ball<klr tip="ind"> (solid)</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>sph&egrave;re</trd>,
        <trd>boule<klr tip="ind"> (volume sph&eacute;rique)</klr></trd>
      </trdgrp>
      <trd lng="pl">kula<klr> (bry&lstroke;a)</klr></trd>
      <trd lng="ru">&c_sh;&c_a;&c_r;<klr tip="ind"> (&c_t;&c_je;&c_l;&c_o;)</klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="nl">bol</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;</trd>
</drv>

<drv mrk="glob.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="G"/>forma:
      <ekz>
        <tld/>e kunvolvita tuketo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_r;&c_a;&c_a;&c_b;&c_r;&c_y;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_f;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">kugelig</trd>
  <trdgrp lng="fr">
    <trd>rond<klr tip="ind"> (sph&eacute;rique)</klr></trd>,
    <trd>sph&eacute;rique</trd>
  </trdgrp>
  <trd lng="hu">g&ouml;mb alak&uacute;</trd>
  <trd lng="nl">bolvormig</trd>
  <trd lng="pl">kulisty</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_r;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glob.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Malgranda <tld/>o:
      <ekz>
        aer<tld/>eto;
      </ekz>
      <ekz>
        <uzo tip="fak">BIO</uzo>
        sangaj <tld/>etoj;
      <trd lng="es">gl&oacute;bulo</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">MED</uzo>
        farmaciaj <tld/>etoj.
      </ekz>
    </dif>
  </snc>
  <adm>
    Mi ne certas, pri kio temas la farmaciaj ~etoj. Cxu sinonimoj
    de &leftquot;granolo&rightquot;? <aut>MB</aut>
  </adm>
  <trd lng="be">&c_sh;&c_a;&c_r;&c_y;&c_k;</trd>
  <trd lng="de">K&uuml;gelchen</trd>
  <trdgrp lng="fr">
    <trd>globule</trd>,
    <trd>bulle</trd>
  </trdgrp>
  <trd lng="nl">bolletje</trd>
  <trd lng="pl">kulka</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_i;&c_k;</trd>
</drv>

<drv mrk="glob.duon0o">
  <kap>duon<tld/>o</kap>
  <snc>
    <dif>
      Duono de <tld/>o; objekto havanta tian formon:
      <ekz>
        <uzo tip="fak">ANA</uzo>
        la <ind>dekstra duon<tld/>o de la cerbo</ind>;
        <trd lng="es"><ind>hemisfero</ind> derecho</trd>
        <trd lng="hu">jobb <ind>f&eacute;lteke</ind></trd>
        <trd lng="nl">rechter hersenhelft</trd>
        <trd lng="pl">p&oacute;&lstroke;kula</trd>
        <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_o;&c_je; &c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je; &c_m;&c_o;&c_z;&c_g;&c_a;</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <ind>norda duon<tld/>o</ind>;
        <trd lng="en">northern hemisphere</trd>
        <trd lng="es"><ind>hemisferio</ind> norte</trd>
        <trd lng="hu">&eacute;szaki <ind>f&eacute;lg&ouml;mb</ind></trd>
        <trd lng="nl">noordelijk <ind>halfrond</ind></trd>
        <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_je; &c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je; <klr>&c_Z;&c_je;&c_m;&c_l;&c_i;</klr></trd>
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <ind>orienta duon<tld/>o</ind>;
        <trd lng="en">eastern hemisphere</trd>
        <trd lng="es"><ind>hemisferio</ind> oriental</trd>
        <trd lng="hu">keleti <ind>f&eacute;lg&ouml;mb</ind></trd>
        <trd lng="nl">oostelijk <ind>halfrond</ind></trd>
        <trd lng="ru">&c_v;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_o;&c_je; &c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je; <klr>&c_Z;&c_je;&c_m;&c_l;&c_i;</klr></trd>
      </ekz>
      <ekz>
        antipodo <klr>estas</klr> loko de la tero ku&scirc;anta sub niaj
        piedoj sur la alia duon<tld/>o <fnt>PV</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_sh;&c_a;&c_r;'&c_je;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_k;&c_u;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="en">hemisphere</trd>
  <trd lng="es">hemisferio</trd>
  <trd lng="fr">h&eacute;misph&egrave;re</trd>
  <trd lng="hu">f&eacute;lg&ouml;mb</trd>
  <trdgrp lng="nl">
    <trd>halfrond</trd>,
    <trd>hemisfeer</trd>
  </trdgrp>
  <trd lng="pl">p&oacute;&lstroke;kula</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
</drv>

<drv mrk="glob.faden0o">
  <kap>faden<tld/>o</kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="bul.faden0o">fadenbulo</ref>
    <adm>
      La PV-a difino estas &leftquot;volva&jcirc;eto da fadeno&rightquot;
kaj
      mi ne vidas esencan diferencon disde &leftquot;fadenbulo&rightquot;.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="de">Garnkn&auml;uel</trd>
  <trd lng="fr">pelote</trd>
  <trd lng="pl">k&lstroke;&eogonek;bek</trd>
</drv>

<drv mrk="glob.plat0o">
  <kap>plat<tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="map.0o">Mapo</ref> de
      la tuta ter<tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_ib;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
  <trd lng="de">Weltkarte</trd>
  <trd lng="es">planisferio</trd>
  <trd lng="fr">planisph&egrave;re</trd>
  <trd lng="hu">vil&aacute;gt&eacute;rk&eacute;p</trd>
  <trd lng="nl">wereldkaart</trd>
</drv>
</art>
