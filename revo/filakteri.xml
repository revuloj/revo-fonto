<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filakteri.xml,v 1.9 2024/04/03 07:21:37 revo Exp $">
<kap>
  <rad>filakteri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="filakteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="filakteri.0o.JUD">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="skatol.0o">Skatoleto</ref> entenanta etan
      pergamenon kun biblia teksto, portata precipe dum pre&gcirc;oj:
      <ekz>
        ili lar&gcirc;igas siajn <tld/>ojn kaj grandigas la fran&gcirc;ojn de siaj
        vestoj
        <fnt><bib>NT</bib>, <lok>&Mat; 23:5</lok></fnt>;
      </ekz>
      <ekz>
        eble estas sur tiuj <tld/>oj a&ubreve; sur la rula&jcirc;o fingrospuroj
        <fnt>
          <bib>CKv</bib>
          <lok>14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amulet.0o">amuleto</ref>,
      <ref cel="relikv.0ujo">relikvujo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_k;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>
  <trd lng="cs">fylakt&#xE9;rie</trd>
  <trd lng="de">Phylakterium</trd>
  <trd lng="en">phylactery</trd>
  <trd lng="es">filacteria</trd>
  <trd lng="fa">&a_t;&a_f;&f_y;&a_l;&f_y;&a_n;</trd>
  <trd lng="fr">phylact&egrave;re</trd>
  <trd lng="he">&tav;&pe;&yod;&lamed;&yod;&fnun;</trd>
  <trd lng="it">filatterio</trd>
  <trd lng="nl">fylakterion</trd>
  <trd lng="pl">filakteria</trd>
  <trd lng="pt">filact&eacute;rio</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">talizman star&#xFD;ch Hebrejov</trd>
  <trd lng="uk">&c_f;&c_ib;&#x43B;&c_a;&#x43A;&c_t;&c_je;&c_r;&c_ib;&#x44F; (&c_jeu;&c_v;&c_r;&c_je;&c_j;&c_s;&#x44C;&#x43A;&c_i;&c_j;
  &c_t;&c_a;&#x43B;&c_ib;&c_s;&#x43C;&c_a;&#x43D;)</trd>
  <trd lng="yi">&tav;&pe;&yod;&lamed;&yod;&fnun;</trd>
</drv>
</art>

