<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>poleks</rad>/o
</kap>
<drv mrk="poleks.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="poleks.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="fingr.dik0o.super">Dikfingro</ref> mana:
      <ekz>
        jen tion &ccirc;i mi komprenas! &ndash; levas la <tld/>on Sa&scirc;ka
        <klr>(signo de aprobo)</klr>
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 21a, p. 122a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib; &c_p;&c_a;&c_l;&c_je;&c_c; &c_r;&c_u;&c_k;&c_ib;</trd>
  <trd lng="de">Daumen</trd>
  <trd lng="en">thumb</trd>
  <trd lng="fr">pouce</trd>
  <trd lng="nl">duim</trd>
  <trd lng="pl">kciuk</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_o;&c_j; &c_p;&c_a;&c_l;&c_je;&c_c; &c_r;&c_u;&c_k;&c_i;</trd>
</drv>
</art>
