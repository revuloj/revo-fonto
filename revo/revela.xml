<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>revelaci</rad>/o
</kap>
<drv mrk="revela.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Inspiro per kiu Dio sciigas al homo pri siaj misteroj a&ubreve; voloj.
    </dif>
  </snc>
  <trd lng="de">Offenbarung</trd>
  <trd lng="en">revelation</trd>
  <trd lng="nl">openbaring</trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="revela.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malka&scirc;i per <tld/>o:
      <ekz>
        ankora&ubreve; ne estis <tld/>ita al Samuel la vorto de la
        Eternulo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">offenbaren</trd>
  <trd lng="nl">openbaren</trd>
</drv>
</art>
