<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>apostol</rad>/o
</kap>
<drv mrk="aposto.0o">
  <kap><tld/>o</kap>
  <snc mrk="aposto.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      &Ccirc;iu el la dis&ccirc;iploj, kiujn
      <ref cel="krist.0o">Kristo</ref> elektis por prediki
      evangelion.
    </dif>
  </snc>
  <snc mrk="aposto.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fervora, sindona propagandisto:
      <ekz>
        <tld/>o de ideo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Zamenhof estis vera <tld/>o de la homamo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="misi.0o">misio</ref>,
      <ref cel="pionir.0o">pioniro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_o;&c_s;&c_t;&c_a;&c_l;</trd>
  <trdgrp lng="de">
    <trd>Apostel</trd>,
    <trd>J&uuml;nger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apostle</trd>,
    <trd>disciple</trd>
  </trdgrp>
  <trd lng="es">ap&oacute;stol</trd>
  <trd lng="fr">ap&ocirc;tre</trd>
  <trdgrp lng="hu">
    <trd>apostol</trd>,
    <trd>tan&iacute;tv&aacute;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_s;&c_t;&c_o;&c_l;</trd>
  <trd lng="sk">apo&scaron;tol</trd>
</drv>
</art>
