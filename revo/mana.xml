<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mana</rad>/o
</kap>
<drv mrk="mana.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      &Ccirc;iela nutra&jcirc;o donita de Dio al la hebreoj en la dezerto.
    </dif>
    <trd lng="de">Manna</trd>
    <trd lng="en">manna</trd>
    <trd lng="es">man&aacute;</trd>
    <trd lng="hu">manna</trd>
    <trd lng="nl">manna</trd>
    <trd lng="ru">&c_m;&c_a;&c_n;&c_n;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Sukera substanco eltirita el fraksen&scirc;elo kaj uzata kiel
laksigilo.
    </dif>
    <trd lng="en">manna</trd>
    <trd lng="hu">manna</trd>
    <trd lng="nl">manna</trd>
  </snc>
  <snc>
    <uzo tip="klr">(En praaj, totemaj socioj)</uzo>
    <dif>
      Mistera, magia forto, kvaza&ubreve; kolektiva animo.
    </dif>
    <trd lng="de">Mana</trd>
    <trd lng="en">mana</trd>
    <trd lng="hu">mana</trd>
    <trd lng="nl">mana</trd>
    <trd lng="ru">&c_m;&c_a;&c_n;&c_a;</trd>
  </snc>
</drv>
</art>
