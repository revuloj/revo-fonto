<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>variete</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="variet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Spektaklo konsistanta el diversaj prezentadoj, kantoj ktp.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;programm</trd>
  </trdgrp>
  <trd lng="es">variedades</trd>
  <trd lng="nl">variet&eacute;</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</trd>
</drv>

<drv mrk="variet.0ateatro">
  <kap><tld/>a teatro</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Muzikkafejo, koncertkafejo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;theater</trd>
  </trdgrp>
  <trd lng="nl">variet&eacute;-theater</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r; <ind>&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</ind></trd>
</drv>
</art>
