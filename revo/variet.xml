<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: variet.xml,v 1.19 2023/09/05 09:43:54 revo Exp $">
<kap>
  <rad>variete</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="variet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="variet.0o.TEA">
    <dif>
      <ref tip="super" cel="spekta.0o.TEA">Spektaklo</ref>
      konsistanta el diversaj prezentadoj, kantoj ktp:
      <ekz>
        mi neniam edzi&gcirc;is, &ccirc;ar mi estas <tld/>a artisto
        <klr>[&#x2026;]</klr> tio estas nomada vivo
        <fnt>
          <aut>trad. M. Blahu&scirc;</aut>,
          <vrk><url ref="http://filmoj.net/kusxejetoj">
          Ku&scirc;ejetoj</url></vrk>,
          <lok>[2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="variet.0o.loko">
    <dif>
      Domo, en kiu prezenti&gcirc;as tiaj spektakloj;
      <ref cel="teatr.varieta0o">varietea teatro</ref>:
      <ekz>
        <nom>Berlin</nom> estas ri&ccirc;ega urbo! &gcirc;i havas
        tiom <klr>[&#x2026;]</klr> da teatroj, kinejoj,
        varieteoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;'&c_ja;&c_t;&c_e;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_je;&c_v;&c_i;&c_l;</trd>
  <trd lng="ca">varietats</trd>
  <trd lng="cs">variet&eacute;</trd>
  <trdgrp lng="de">
    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;programm</trd>
  </trdgrp>
  <trd lng="es">variedades</trd>
  <trdgrp lng="fr">
    <trd><ind>vari&eacute;t&eacute;</ind>s</trd>,
    <trd>spectacle de <ind>vari&eacute;t&eacute;</ind>s</trd>
  </trdgrp>
  <trd lng="hu">variet&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30E9;&#x30A8;&#x30C6;&#x30A3;&#x30FC;  [<ind>&#x3070;&#x3089;&#x3048;&#x3066;&#x3043;&#x30FC;</ind>]</trd>,
    <trd>&#x5BC4;&#x5E2D;&#x82B8;  [<ind>&#x3088;&#x305B;&#x3052;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">variet&eacute;</trd>
  <trdgrp lng="pt">
   <trd>variedades</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</trd>
  <trd lng="sk">variet&eacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_r;&#x2019;&c_jeu;&c_t;&c_je;</trd>,
    <trd>&c_t;&c_je;&c_a;&c_t;&c_r; &c_je;&c_s;&c_t;&c_r;&c_a;&c_d;&c_i;</trd>
  </trdgrp>
</drv>
</art>
