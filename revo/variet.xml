<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>variete</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="variet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Spektaklo konsistanta el diversaj prezentadoj, kantoj ktp.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;'&c_ja;&c_t;&c_e;</trd>
  <trdgrp lng="de">
    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;programm</trd>
  </trdgrp>
  <trd lng="es">variedades</trd>
  <trdgrp lng="fr">
    <trd><ind>vari&eacute;t&eacute;</ind>s</trd>,
    <trd>spectacle de <ind>vari&eacute;t&eacute;</ind>s</trd>
  </trdgrp>
  <trd lng="hu">variet&eacute;</trd>  
  <trd lng="nl">variet&eacute;</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</trd>
</drv>

<drv mrk="variet.0ateatro">
  <kap><tld/>a teatro</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="teatr.0o">Teatro</ref>, en kiu oni
      prezentas <tld/>on.
    </dif>
    <ref tip="vid" cel="kabare.0o.ejo">kabareto</ref>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_r; 
    <ind>&c_v;&c_a;&c_r;'&c_ja;&c_t;&c_e;</ind></trd>
  <trdgrp lng="de">
    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;theater</trd>
  </trdgrp>
  <trd lng="fr">th&eacute;&acirc;tre de <ind>vari&eacute;t&eacute;</ind>s</trd>
  <trdgrp lng="hu">
    <trd>variet&eacute;sz&iacute;nh&aacute;z</trd>,
    <trd>orfeum</trd>,
    <trd>zen&eacute;s kabar&eacute;</trd>
  </trdgrp>
  <trd lng="nl">variet&eacute;-theater</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r; <ind>&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</ind></trd>
</drv>
</art>
