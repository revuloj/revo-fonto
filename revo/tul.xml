<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tul.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tul.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldika kaj malpeza, retosimila, travidebla teksa&jcirc;o:
      <ekz>
        larma nebulo pendas kvaza&ubreve; <tld/>o anta&ubreve; lia
        rigardo
        <fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="ca">tul</trd>
  <trd lng="cs">tyl <klr>(tkanina)</klr></trd>
  <trd lng="de">T&uuml;ll</trd>
  <trd lng="en">tulle</trd>
  <trd lng="fr">tulle</trd>
  <trd lng="hu">t&uuml;ll</trd>
  <trd lng="nl">tule</trd>
  <trd lng="pl">tiul</trd>
  <trd lng="pt">tule</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="sk">tyl <klr>(tkanina)</klr></trd>
</drv>
</art>
