<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tul.xml,v 1.21 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tul.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldika kaj malpeza, retosimila, travidebla 
      <ref tip="super" cel="teks.0ajxo.TEKS">teksa&jcirc;o<sncref/></ref>:
      <ekz>
        <nom>Thubui</nom> forlasis lin, sed tuj revenis en vesto el diafana <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filinoj de la diablo, en <tld/>o kaj silko, ili venas kun disliberigitaj
        haroj kaj belaj membroj, &dash; for&scirc;iru vin, se vi povas
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="punt.0o">punto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="ca">tul</trd>
  <trd lng="cs">tyl <klr>(tkanina)</klr></trd>
  <trd lng="de">T&uuml;ll</trd>
  <trd lng="en">tulle</trd>
  <trd lng="fr">tulle</trd>
  <trd lng="hu">t&uuml;ll</trd>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30E5;&#x30FC;&#x30EB;</trd>,
    <trd>&#x4E80;&#x7532;&#x7D17;  <pr>&#x304B;&#x3081;&#x3053;&#x3046;&#x7D17;</pr></trd>
  </trdgrp>
  <trd lng="nl">tule</trd>
  <trd lng="pl">tiul</trd>
  <trd lng="pt">tule</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="sk">tyl <klr>(tkanina)</klr></trd>
  <trd lng="uk">&#x442;&#x44E;&#x43B;&#x44C;</trd>
</drv>
</art>
