<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kript</rad>/o
</kap>
<drv mrk="kript.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Subtera &ccirc;ambro de pre&gcirc;ejo, kie oni iam enterigis
mortintojn.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_y;&c_p;&c_t;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Krypta</trd>,
      <trd>Grabkapelle</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crypt</trd>,
      <trd>vault</trd>,
      <trd>underground cell</trd>
    </trdgrp>
    <trd lng="es">cripta</trd>
    <trd lng="fr">crypte</trd>
    <trd lng="hu">kripta</trd>
    <trd lng="pl">krypta</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_i;&c_p;&c_t;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
