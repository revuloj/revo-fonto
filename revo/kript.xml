      <trd>Grabkapelle</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crypt</trd>,
      <trd>vault</trd>,
      <trd>underground cell</trd>
    </trdgrp>
    <trd lng="hu">kripta</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_i;&c_p;&c_t;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>
