<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>kopul</rad>/o <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="kopul.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <ref tip="sin" cel="lig.0verbo">ligverbo</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>kopula</trd>,
    <trd>spona</trd>,
    <trd>sponové sloveso</trd>
  </trdgrp>
  <trdgrp lng="fr">
  <trd>copule</trd>,
  <trd>verbe d'&eacute;tat</trd>
</trdgrp>
  <trd lng="sk">sponové sloveso</trd>
</drv>
</art>

