<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adjunkt</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="adjunk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Helpanta oficisto:
      <ekz>
        <tld/>o de urbestro, de direktoro.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">asistente</trd>
  <trdgrp lng="fr">
    <trd>adjoint</trd>,
    <trd>assistant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helyettes</trd>,
    <trd>adjunktus</trd>
  </trdgrp>
  <trd lng="nl">adjunct</trd>
  <trdgrp lng="pt">
    <trd>adjunto</trd>,
    <trd>assistente</trd>,
    <trd>auxiliar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_o;&c_shch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_s;&c_i;&c_s;&c_t;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_malmol;&c_ju;&c_n;&c_k;&c_t;</trd>
  </trdgrp>
</drv>
</art>
