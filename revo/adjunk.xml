  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;'&c_ju;&c_n;&c_k;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_y;&c_s;&c_t;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adjunt</trd>,
    <trd>assistent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>adjunkt</trd>,
    <trd>pomocn&yacute; &uacute;&rcaron;edn&iacute;k</trd>
  </trdgrp>
  <trd lng="en">adjunct</trd>
  <trdgrp lng="es">
    <trd>adjunto</trd>,
    <trd>asistente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adjoint</trd>,
    <trd>assistant</trd>
  </trdgrp>
  <trd lng="he">&#x5EA;&#x5D5;&#x5E1;&#x5E4;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>helyettes</trd>,
    <trd>adjunktus</trd>
  </trdgrp>
  <trd lng="it">aggiunto<klr> (burocr.)</klr></trd>
  <trd lng="nl">adjunct</trd>
  <trd lng="pl">adiunkt</trd>
  <trdgrp lng="pt">
    <trd>adjunto</trd>,
    <trd>assistente</trd>,
    <trd>auxiliar</trd>
  </trdgrp>
  <trd lng="ro">asistent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_o;&c_shch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_s;&c_i;&c_s;&c_t;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_malmol;&c_ju;&c_n;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pomocn&yacute; <ind>&uacute;radn&iacute;k</ind></trd>,
    <trd>pomocn&yacute; &uacute;radn&iacute;k</trd>
  </trdgrp>
</drv>
