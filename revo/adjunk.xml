<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adjunk.xml,v 1.24 2023/10/12 16:57:42 revo Exp $">
<kap>
  <rad>adjunkt</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="adjunk.0o">
  <kap><tld/>o</kap>
  <snc mrk="adjunk.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="help.0anto.KOMUNE">Helpanto</ref>
      en ofica funkcio:
      <ekz>
        <tld/>o de urbestro, de direktoro;
      </ekz>
      <ekz>
        la partoprenantoj unue a&ubreve;skultis prelegon de 
        <nom>Ph. Dr. Veronika He&eacute;</nom>, <tld/>o pri
        teoriaj demandoj de tradukado
        <fnt>
          <bib>LOdE</bib>
          <vrk>Instrui Esperanton</vrk>
          <lok>2001, &#x2116;12</lok>
        </fnt>;
      </ekz>
      <ekz>
        s-ro <nom>Hikmet Habib</nom> <klr>[&#x2026;]</klr>, <tld/>o 
        de s-ino <nom>Ahmed</nom> kaj membro de la
        delegitaro, klarigas <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MD17</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kunvenoj <klr>[&#x2026;]</klr> kunigas la 
        ambasadorojn de la membro-landoj kaj
        iliajn <tld/>ojn
        <fnt>
          <bib>MD11</bib>
          <aut>Christophe Deloire kaj Christophe Dubois </aut>
          <vrk>Oni ne konfuzu</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="asist.0anto">asistanto</ref>,
       <ref cel="adjuta.0o">adjutanto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;'&c_ju;&c_n;&c_k;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_y;&c_s;&c_t;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adjunt</trd>,
    <trd>assistent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>adjunkt</trd>,
    <trd>pomocn&yacute; &uacute;&rcaron;edn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Beigeordneter</trd>
  <trd lng="en">adjunct</trd>
  <trdgrp lng="es">
    <trd>adjunto</trd>,
    <trd>asistente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adjoint</trd>,
    <trd>assistant</trd>
  </trdgrp>
  <trd lng="he">&tav;&vav;&samekh;&pe;&tav;</trd>
  <trdgrp lng="hu">
    <trd>helyettes</trd>,
    <trd>adjunktus</trd>
  </trdgrp>
  <trd lng="it">aggiunto<klr> (burocr.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x52A9;&#x5F79;  <pr>&#x3058;&#x3087;&#x3084;&#x304F;</pr></trd>,
    <trd>&#x88DC;&#x4F50;  <pr>&#x307B;&#x3055;</pr></trd>,
    <trd>&#x52A9;&#x624B;  <pr>&#x3058;&#x3087;&#x3057;&#x3085;</pr></trd>
  </trdgrp>
  <trd lng="nl">adjunct</trd>
  <trd lng="pl">adiunkt</trd>
  <trdgrp lng="pt">
    <trd>adjunto</trd>,
    <trd>assistente</trd>,
    <trd>auxiliar</trd>
  </trdgrp>
  <trd lng="ro">asistent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_o;&c_shch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_s;&c_i;&c_s;&c_t;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_malmol;&c_ju;&c_n;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pomocn&yacute; <ind>&uacute;radn&iacute;k</ind></trd>,
    <trd>pomocn&yacute; &uacute;radn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&#x456;&#x447;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x430;&#x434;&#x2019;&#x44E;&#x43D;&#x43A;&#x442;</trd>
  </trdgrp>
</drv>
</art>
