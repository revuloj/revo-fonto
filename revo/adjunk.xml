  <rad>adjunkt</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="adjunk.0o">
  <kap><tld/>o</kap>
  <snc mrk="adjunk.0o.KOMUNE">
