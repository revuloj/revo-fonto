<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plumb.xml,v 1.32 2024/12/13 08:36:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>plumb</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="plumb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">Pb</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="82">elemento</ref>
      kun
      atomnumero 82, atompezo 207,19; griza mola metalo, tre peza:
      <ekz>
        kiel <tld/>o ili iris al fundo en la akvo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o malaperis en la fajro, vane fandis la fandisto
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>o la dormo nin premas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek&scirc;ajnis al li, ke liaj piedoj estas el <tld/>o kaj ke iu nevidebla mano retenas
        lin per superhoma forto
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_o;&c_l;&c_o;&c_v;&c_o;</trd>
  <trd lng="br">plom<klr tip="ind"> (metal)</klr></trd>
  <trd lng="ca">plom</trd>
  <trdgrp lng="cs">
    <trd>Pb</trd>,
    <trd>olovo</trd>,
    <trd>plumbum</trd>
  </trdgrp>
  <trd lng="de">Blei</trd>
  <trd lng="en">lead</trd>
  <trd lng="es">plomo</trd>
  <trd lng="fa">&a_s;&a_r;&a_b;</trd>
  <trd lng="fr">plomb<klr tip="ind"> (m&eacute;tal)</klr></trd>
  <trd lng="hu">&oacute;lom</trd>
  <trd lng="id">timbal</trd>
  <trd lng="it">piombo</trd>
  <trd lng="ja">&#37467;  <pr>&#12394;&#12414;&#12426;</pr></trd>
  <trd lng="la">Plumbum</trd>
  <trd lng="nl">lood</trd>
  <trd lng="pl">o&lstroke;&oacute;w</trd>
  <trd lng="pt">chumbo</trd>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_n;&c_je;&c_c;</trd>
  <trdgrp lng="sk">
    <trd>Pb</trd>,
    <trd>olovo</trd>
  </trdgrp>
  <trd lng="sv">bly</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_ju;&c_m;&c_b;&c_u;&c_m; (&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_i;&c_j; &c_je;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;)</trd>,
    <trd>&c_s;&c_v;&c_i;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_v;&c_o; (&c_p;&c_r;&c_o;&c_s;&c_t;&c_a; &c_r;&c_je;&c_ch;&c_o;&c_v;&c_i;&c_n;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37467; <pr>qi&amacron;n</pr></trd>,
    <trd>&#38085; <pr>qi&amacron;n</pr></trd>,
    <trd>&#23376;&#24377;&#24635;&#31216; <pr>z&#464;d&agrave;nz&#466;ngch&egrave;n</pr></trd>,
    <trd>&#23376;&#24392;&#32317;&#31281; <pr>z&#464;d&agrave;nz&#466;ngch&egrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="plumb.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="plumb.0a.el_plumbo">
    <dif>
      El <tld/>o:
      <ekz>
        granda <tld/>a pezilo
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la tegmento trovi&gcirc;is <tld/>a defluilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kirasa&ubreve;tomobiloj minace montras ma&scirc;inpafilojn kaj <tld/>a pluvo superver&scirc;as
        la ribelantajn kvartalojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sor&ccirc;istino ne timis la <tld/>ajn kuglojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="plumb.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Peza, prema, griza kvaza&ubreve; <tld/>o:
      <ekz>
        malvarma, griza, kvaza&ubreve; <tld/>a tago nin premas al la tero
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        io nekonata iris peze sur la <tld/>aj akvoj de l' oceano
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a silento haltigis subite la interparoladon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Fine &ccirc;esas &#11005; <tld lit="P"/>a son&gcirc;' de vintra dormo
        <fnt>
          <bib>LSB</bib>
          <lok>Printempa&jcirc;oj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_o;&c_v;&c_y;</trd>
  <trd lng="de">bleiern</trd>
</drv>

<drv mrk="plumb.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      &Ccirc;iu el la <tld/>aj grajnetoj, per kiuj oni plenigas la
      karto&ccirc;ojn de &ccirc;aspafilo:
      <ekz>
        li singardeme portis sian pafilon en la mano, anstata&ubreve;inte per kugloj la
        <tld/>erajn karto&ccirc;ojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> bone konas la &ccirc;asarmilojn, malpezajn mitralojn, fusilojn,
        <tld/>erpafilojn kaj aerpafilojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2013/011065.php"
              >&Ccirc;asado</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_o;&c_t; <klr>(&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_o;&c_v;&c_y;)</klr></trd>
  <trdgrp lng="br">
    <trd>drajezenn<klr tip="ind"> (plomenn)</klr></trd>,
    <trd>plomemm<klr tip="ind"> (drajezenn)</klr></trd>
  </trdgrp>
  <trd lng="cs">brok</trd>
  <trd lng="de">Schrot</trd>
  <trd lng="en">lead shot</trd>
  <trd lng="fa">&a_s;&a_A;&f_ch;&a_m;&a_h;</trd>
  <trd lng="fr">plomb<klr tip="ind"> (grain de plomb)</klr></trd>
  <trd lng="hu">&oacute;loms&ouml;r&eacute;t</trd>
  <trd lng="ja">&#25955;&#24382;  <pr>&#12373;&#12435;&#12384;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>loodkorrel</trd>,
    <trd>hagelkorrel<klr tip="ind"> (v.geweer)</klr></trd>
  </trdgrp>
  <trd lng="pl">&sacute;rut</trd>
  <trd lng="pt">chumbo <klr tip="amb">(para ca&ccedil;a)</klr></trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_b;&c_mol;
    <klr>(&c_s;&c_v;&c_i;&c_n;&c_c;&c_o;&c_v;&c_a;&c_ja;, &c_d;&c_l;&c_ja; &c_s;&c_t;&c_r;&c_je;&c_l;&c_mol;&c_b;&c_y;)</klr></trd>
  <trd lng="sk">brok</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_o;&c_b;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_r;&c_o;&c_t;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37467;&#24392; <pr>qi&amacron;nd&agrave;n</pr></trd>,
    <trd>&#38085;&#24377; <pr>qi&amacron;nd&agrave;n</pr></trd>
  </trdgrp>
</drv>
</art>
