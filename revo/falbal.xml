<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>falbal</rad>/o
</kap>
<drv mrk="falbal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Krispe faldita strio el &scirc;tofo almetita &ccirc;e jupo a&ubreve; robo
      por ornami &gcirc;in:
      <ekz>
        virinon prezentas tiu statuo kaj <tld/>oj de &scirc;ia robo tre mole
        elver&scirc;i&gcirc;as &gcirc;is la plata tomba kovrilo<fnt><aut>Spomenka
        &Scirc;timec</aut>: <vrk>Hejmo en Meze&ubreve;ropo</vrk>,
        <bib>Monato</bib><lok>jaro 1996a, numero 1a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_b;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Falbel</trd>,
    <trd>Behang</trd>,
    <trd>Volant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flounce</trd>,
    <trd>furbelow</trd>,
    <trd>frill</trd>,
    <trd>ruffle</trd>,
    <trd>trimming</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>falbala<klr tip="ind"> (volant)</klr></trd>,
    <trd>volant<klr tip="ind"> (falbala)</klr></trd>
  </trdgrp>
  <trd lng="hu">ruhafodor</trd>
  <trd lng="nl">tierelantijn</trd>
  <trdgrp lng="pt">
    <trd>falbal&aacute;</trd>,
    <trd>babado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
