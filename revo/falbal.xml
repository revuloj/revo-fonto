<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>falbal</rad>/o</kap>
<drv mrk="falbal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Krispe faldita strio el &scirc;tofo almetita &ccirc;e jupo a&ubreve; robo
      por ornami &gcirc;in.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Falbel</trd>,
    <trd>Behang</trd>,
    <trd>Volant</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>falbala<klr tip="ind"> (volant)</klr></trd>,
    <trd>volant<klr tip="ind"> (falbala)</klr></trd>
  </trdgrp>
  <trd lng="nl">tierelantijn</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
