<kap>
  <ofc>*</ofc>
  <rad>falbal</rad>/o
</kap>
<drv mrk="falbal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Krispe faldita strio el &scirc;tofo almetita &ccirc;e jupo
      a&ubreve; robo
      por ornami &gcirc;in:
      <ekz>
        virinon prezentas tiu statuo kaj <tld/>oj de &scirc;ia robo tre
        mole
        elver&scirc;i&gcirc;as &gcirc;is la plata tomba
        kovrilo<fnt><aut>Spomenka
        &Scirc;timec</aut>: <vrk>Hejmo en Meze&ubreve;ropo</vrk>,
        <bib>Monato</bib><lok>jaro 1996a, numero 1a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_b;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kan&#xFD;r</trd>,
    <trd>karn&#xFD;r</trd>,
    <trd>ry&#x161;</trd>,
    <trd>vol&#xE1;n</trd>,
    <trd>vol&#xE1;nek</trd>,
    <trd>z&#xE1;hyb</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Falbel</trd>,
    <trd>Behang</trd>,
    <trd>Volant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flounce</trd>,
    <trd>furbelow</trd>,
    <trd>frill</trd>,
    <trd>ruffle</trd>,
    <trd>trimming</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>falbala<klr tip="ind"> (volant)</klr></trd>,
    <trd>volant<klr tip="ind"> (falbala)</klr></trd>
  </trdgrp>
  <trd lng="hu">ruhafodor</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30A1;&#x30EB;&#x30D0;&#x30E9;</trd>,
    <trd>&#x3072;&#x3060;&#x98FE;&#x308A;  <pr>&#x3072;&#x3060;&#x304B;&#x3056;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="nl">tierelantijn</trd>
  <trd lng="pl">falbana</trd>
  <trdgrp lng="pt">
    <trd>falbal&aacute;</trd>,
    <trd>babado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vol&#xE1;n</trd>,
    <trd>z&#xE1;hyb</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x431;&#x43E;&#x440;&#x43A;&#x430;</trd>,
    <trd>&#x448;&#x43B;&#x44F;&#x440;&#x43A;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x98FE;&#x4EE5;&#x8377;&#x8449;&#x908A; <pr>sh&#xEC;y&#x1D0;h&#xE9;y&#xE8;bi&#x101;n</pr></trd>,
    <trd>&#x7A81;&#x7136;&#x79BB;&#x53BB; <pr>t&#x16B;r&#xE1;nl&#xED;q&#xF9;</pr></trd>,
    <trd>&#x8377;&#x53F6;&#x8FB9; <pr>h&#xE9;y&#xE8;bi&#x101;n</pr></trd>
  </trdgrp>
</drv>
