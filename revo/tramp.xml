<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tramp.xml,v 1.10 2024/02/28 09:23:54 revo Exp $">
<kap>
  <rad>tramp</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tramp.0o">
  <kap><tld/>o</kap>
  <snc mrk="tramp.0o.MOR">
    <dif>
      Mizera vagulo,
      <ref tip="vid" cel="cxifon.0ulo">&ccirc;ifonulo</ref>:
      <ekz>
        tiu trampo <klr>...</klr> konis la vivon de la karceroj kaj de
        la bordeloj
        <fnt>
          <aut>G. Waringhien</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr33/verlaine.html">
          Pa&ubreve;lo Verlaine</url></vrk>,
          <lok>La Nica Literatura Revuo, 1958-01 &gcirc;is 02 (3/3)</lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vagabond.0o.MOR">vagabondo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_d;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>letadlo bez pravideln&eacute; linky</trd>,
    <trd>lo&dcaron; bez pravideln&eacute; linky</trd>
  </trdgrp>
  <trd lng="de">Landstreicher</trd>
    <trdgrp lng="de">
    <trd>Landstreicher</trd>,
    <trd>Nichtsesshafter</trd>
  </trdgrp>
    <trdgrp lng="fr">
    <trd>clochard</trd>,
    <trd>mis&eacute;reux<klr> (subst.)</klr></trd>,
    <trd>trimardeur</trd>,
    <trd>va-nu-pied</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D6E;&#x6D6A;&#32773;  <pr>&#12405;&#x308D;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#x30EB;&#x30F3;&#x30DA;&#x30F3;</trd>,
    <trd/>,
    <trd>&#x4E0D;&#x5B9A;&#x671F;&#x8CA8;&#29289;&#33337;  <pr>&#12405;&#12390;&#12356;&#x304D;&#x304B;&#12418;&#12388;&#x305B;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="sk">lo&dcaron; bez pravidelnej linky</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&#x43C;&#x43F;</trd>,
    <trd>&c_v;&c_a;&#x43D;&c_t;&c_a;&c_zh;&#x43D;&c_je; &c_s;&c_u;&c_d;&#x43D;&#x43E; (&c_b;&c_je;&c_z; &#x43F;&c_je;&c_v;&#x43D;&#x43E;&c_g;&#x43E;
    &#x43C;&c_a;&c_r;&c_sh;&c_r;&c_u;&c_t;&c_u;)</trd>,
    <trd>&c_t;&c_r;&c_a;&#x43D;&c_s;&#x43F;&#x43E;&c_r;&c_t;&#x43D;&c_i;&c_j; &#x43B;&c_ib;&c_t;&c_a;&#x43A; (&c_b;&c_je;&c_z; &#x43F;&c_je;&c_v;&#x43D;&#x43E;&c_g;&#x43E;
    &#x43C;&c_a;&c_r;&c_sh;&c_r;&c_u;&c_t;&c_u;)</trd>,
    <trd>&#x43E;&c_b;&c_ib;&c_d;&c_r;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&#x43E;&c_b;&c_sh;&c_a;&c_r;&#x43F;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_g;&#x43E;&#x43B;&#x43E;&c_d;&c_r;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_b;&c_r;&#x43E;&c_d;&#x44F;&c_g;&c_a;</trd>,
    <trd>&c_b;&#x43E;&#x43C;&c_zh;</trd>
  </trdgrp>
</drv>

<drv mrk="tramp.0i">
  <kap><tld/>i</kap>
  <snc mrk="tramp.0i.MOR">
    <dif>
      Vivi,
      <ref tip="vid" cel="vag.0i">vagi</ref>
      kiel <tld/>o:
      <ekz>
        li havas dignan nomon, donitan de la gepatroj; sed de post kiam
        li <tld/>as, la nomo jam ludas nenian rolon
        <fnt>
          <aut>Mao Dun</aut>,
          <vrk><url ref="&CRI;1/2004/01/15/1@2725.htm">
          Rakonto pri Nazego</url></vrk>,
          <lok>22004</lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="fr">trimarder</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a; <ind>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_zh;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;epravovat dle pot&rcaron;eby</trd>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x6D6A;&#12377;&#x308B;  <pr>&#x307B;&#12358;&#x308D;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6E21;&#x308A;&#x6B69;&#x304F;  <pr>&#x308F;&#x305F;&#x308A;&#12354;&#x308B;&#x304F;</pr></trd>,
    <trd>&#x4E0D;&#x5B9A;&#x671F;&#33337;&#12391;&#x822A;&#x6D77;&#12377;&#x308B; 
    <pr>&#12405;&#12390;&#12356;&#x304D;&#x305B;&#12435;&#12391;&#12371;&#12358;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">prepravova&tcaron; pod&lcaron;a potreby</trd>
</drv>
</art>

