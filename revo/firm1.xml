<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>firm</rad>/o
</kap>
<drv mrk="firm1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="firm1.0o.nomo">
    <dif>
      Nomo de komerca entrepreno, ekz. &leftquot;Centra E-a Librejo&rightquot;.
    </dif>
    <adm>
      Cxu vere nomo? PIV1 inversis la ordon de sencoj. Ekz-oj
      bonvenaj. <aut>MB</aut>
    </adm>
    <trd lng="fr">raison sociale</trd>
  </snc>
  <snc mrk="firm1.0o.entrepreno">
    <dif>
      Komerca entrepreno mem:
      <ekz>
        konata fidinda <tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="firma.0o">firmao.</ref>
    <trd lng="de">Unternehmen</trd>
    <trdgrp lng="fr">
      <trd>firme</trd>,
      <trd>entreprise</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Firma</trd>
  <trd lng="nl">firma</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
