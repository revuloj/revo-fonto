<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>firm</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad>firma</rad>/o</kap></var>
</kap>
<drv mrk="firm1.0o">
  <kap><tld/>o, <var><kap>firmao</kap></var></kap>
  <snc mrk="firm1.0o.entrepreno">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Komerca <ref tip="super" cel="entrep.0o">entrepreno</ref>:
      <ekz>
        la elirintaj kajeroj estas riceveblaj &ccirc;e la <tld/>o 
        <nom>M&uuml;ller</nom> kaj <nom>Borel</nom> en Berlino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la plimulto de la viroj el la barako forestis en la
        urbo por labori &ccirc;e diversaj <tld/>oj
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        komerca <tld/>o petis ke mi eksperimente provu novan aparaton kiu donas aliron
        al ankora&ubreve; aliaj servoj de la kompanio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9401-telefona-labirinto.txt">Will Green kondukas vin tra sia</url></vrk>
          <lok>94/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        litertiparoj de diversaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9506-lingvema.txt">Por lingvemaj komputilemuloj...</url></vrk>
          <lok>95/06</lok>
        </fnt>;
      </ekz>
      <ekz>
        kostoj de la transiro al e&ubreve;roj por firmaoj kaj publikaj
        servoj<fnt><aut>Marc Vanden Bempt</aut>: <vrk>E&ubreve;ro: ecoj
        kaj sekvoj por &ccirc;iuj</vrk>, <bib>Monato</bib>
        <lok>jaro 1998a, numero 9a, p. 17a</lok></fnt>;
      </ekz>
      <ekz>
        sur la flugma&scirc;ino estis reklamataj produkta&jcirc;oj de tabakfirmao
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/007181.html">http://steloj.de/esperanto/monato2/007181.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la bulgaraj firmaoj elspezis pli ol 8 milionojn da e&ubreve;roj por trans&scirc;ar&gcirc;igo de
        varoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006857.html">Trafiko: Danuba &scirc;iptrafiko paralizita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la registaro ne apogas la malgrandajn kaj mezgrandajn firmaojn kiel anta&ubreve;e
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006858.html">Albanio: Diktaturo de impostoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la instalado plej konvene okazu helpe de iu satelite televida fak<tld/>o el via
        najbara&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9504-radio.txt">Radio</url></vrk>
          <lok>95/04</lok>
        </fnt>;
      </ekz>
      <ekz>
        publika akcifirmao
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007132.html">Interreto: Interreto senpage</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plej multaj retservoj simple revendas sub sia aparta nomo la
        servon de pli granda ret- a&ubreve; telefon-firmao<fnt>
        <aut>Edmund Grimley Evans</aut>: <vrk><url ref="http://esperanto.be/fel/mon/007132.html">Interreto
        senpage</url></vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 11a,
        p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="kompan.0o">kompanio</ref>,
       <ref cel="entrep.0o.firmao">entrepreno<sncref/></ref>
    </refgrp>
    <trd lng="de">Unternehmen</trd>
    <trdgrp lng="en">
      <trd>firm</trd>,
      <trd>company</trd>,
      <trd>business</trd>,
      <trd>enterprise</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Empresa<klr> (comercial)</klr></trd>,
      <trd>Firma</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>firme</trd>,
      <trd>entreprise<klr> (soci&eacute;t&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>firma</trd>,
      <trd>przedsi&eogonek;biorstwo</trd>
    </trdgrp>
  </snc>
<!-- ne konvinka ekz-o / snc-o...
  <snc mrk="firm1.0o.nomo">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Nomo de komerca entrepreno, ekzemple <ctl>Centra Esperanta
      Librejo</ctl>:
      <ekz>
        tiuj &ndash; la&ubreve; la kompanio &ndash; kalumniis la
        <tld/>on, kiam fine de la 80aj
        jaroj ili disdonis en Londono flugfolion, en kiu ili akuzis
        <ctl>McDonalds</ctl> pri i.a. ses misfaroj
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Ne tiom ridos la kla&ubreve;no</vrk>, <bib>Monato</bib>
          <lok>jaro 1997a, numero 8a, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      Cxu vere nomo? PIV1 inversis la ordon de sencoj. Ekz-oj
      bonvenaj. <aut>MB</aut>
    </adm>
    <trd lng="fr">raison sociale</trd>
    <trd lng="es">firma comercial<klr> (empresarial)</klr></trd>
  </snc>
-->
  <rim>
     &leftquot;Firmao&rightquot; estas nun pli ofte uzata ol &leftquot;<tld/>o&rightquot;.
  </rim>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>firma</trd>,
    <trd>empresa</trd>,
    <trd>companyia</trd>
  </trdgrp>
  <trd lng="de">Firma</trd>
  <trdgrp lng="nl">
    <trd>firma</trd>,
    <trd>bedrijf</trd>,
    <trd>onderneming</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>firma</trd>,
    <trd>bedrift</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>firma</trd>,
    <trd>casa <klr tip="amb">(comercial)</klr></trd>,
    <trd>casa de neg&oacute;cios</trd>,
    <trd>loja</trd>,
    <trd>empresa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">firma</trd>
  <trd lng="cs">firma</trd>
</drv>

<drv mrk="firm1.0eto">
  <kap><tld/>eto, <var><kap>etfirmao</kap></var></kap>
  <snc mrk="firm1.0eto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malgranda <tld/>o diverse difinita la&ubreve; landoj, ordinare kun
      maksimume kelkdek laborantoj:
      <ekz>
        nepra &gcirc;i estas anka&ubreve; por la <tld/>eto
        &leftquot;<nom>Professional Consulting</nom>&rightquot;,
        kiu fidis je la esperantistaro kreante tiun
        produkton
        <fnt>
          <aut>P. Peeraerts, B. Demeyere</aut>,
          <vrk>Windows/Ni levu la &ccirc;apelon! (&Ccirc;apelilo)</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1995a, numero 5a, p. 16a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la etfirmaoj, kiu fakte faras la laboron<fnt><aut>Paul
        Gubbins</aut>: <vrk><url ref="http://esperanto.be/fel/mon/007260.html">Politiko
        elreligita</url></vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_je;<ind> &c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trd lng="ca">petita empresa</trd>
  <trdgrp lng="de">
    <trd>Kleinunternehmen</trd>,
    <trd>Kleinbetrieb</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>small firm</trd>,
    <trd>small company</trd>,
    <trd>small business</trd>,
    <trd>partnership</trd>,
    <trd>sole proprietorship</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>petite entreprise</trd>,
    <trd>PME</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>firemka</trd>,
    <trd>drobna przedsi&eogonek;biorczo&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pequena <ind>empresa</ind></trd>,
    <trd>microempresa</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_o;&c_je;<ind> &c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</ind>
  </trd>
</drv>


<drv mrk="firm1.ek0o">
  <kap>ek<tld/>o, <var><kap>ekfirmao</kap></var></kap>
  <snc mrk="firm1.ek0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="firm1.0eto">Firmeto</ref> nove kreita kaj tre
      rapide kreskanta:
      <ekz>
        <nom>Uber</nom> sekvas la poststrion de pli modestaj ekfirmaoj;
      </ekz>
      <ekz>
        mi &ccirc;eestas intervjuon kun posedanto de ek<tld/>ao
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=KoDzkKHbGH8">Evildea portas amorvekan smokingon</url></vrk>
          <lok>Jutubo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_t;&c_a;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Startup</trd>,
    <trd>Neugr&uuml;ndung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>startup</trd>,
    <trd>start-up</trd>,
    <trd>jeune pousse</trd>
  </trdgrp>
</drv> 

</art>
