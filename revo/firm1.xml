  </trd>
</drv>


<drv mrk="firm1.ek0o">
  <kap>ek<tld/>o, <var><kap>ek<tld var="a"/>o</kap></var></kap>
  <snc mrk="firm1.ek0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="firm1.0eto">Firmeto</ref> nove kreita kaj tre
      rapide kreskanta:
      <ekz>
        <nom>Uber</nom> sekvas la poststrion de pli modestaj ek<tld var="a"/>oj;
      </ekz>
      <ekz>
        mi &ccirc;eestas intervjuon kun posedanto de ek<tld/>ao
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=KoDzkKHbGH8">Evildea portas amorvekan
          smokingon</url></vrk>
          <lok>Jutubo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_t;&c_a;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Startup</trd>,
    <trd>Neugr&uuml;ndung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>startup</trd>,
    <trd>start-up</trd>,
    <trd>jeune pousse</trd>
  </trdgrp>
</drv> 

