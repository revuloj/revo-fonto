<drv mrk="firm1.0o">
  <kap><tld/>o, <var><kap><tld var="a"/>o</kap></var></kap>
  <snc mrk="firm1.0o.entrepreno">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Komerca <ref tip="super" cel="entrep.0o">entrepreno</ref>:
      <ekz>
        la elirintaj kajeroj estas riceveblaj &ccirc;e la <tld/>o 
        <nom>M&uuml;ller</nom> kaj <nom>Borel</nom> en Berlino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la plimulto de la viroj el la barako forestis en la
        urbo por labori &ccirc;e diversaj <tld/>oj
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        komerca <tld/>o petis ke mi eksperimente provu novan aparaton kiu donas aliron
        al ankora&ubreve; aliaj servoj de la kompanio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9401-telefona-labirinto.txt">Will
          Green kondukas vin tra sia</url></vrk>
          <lok>94/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        litertiparoj de diversaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9506-lingvema.txt">Por lingvemaj
          komputilemuloj...</url></vrk>
          <lok>95/06</lok>
        </fnt>;
      </ekz>
      <ekz>
        kostoj de la transiro al e&ubreve;roj por <tld var="a"/>oj kaj publikaj
        servoj<fnt><aut>Marc Vanden Bempt</aut>: <vrk>E&ubreve;ro: ecoj
        kaj sekvoj por &ccirc;iuj</vrk>, <bib>Monato</bib>
        <lok>jaro 1998a, numero 9a, p. 17a</lok></fnt>;
      </ekz>
      <ekz>
        sur la flugma&scirc;ino estis reklamataj produkta&jcirc;oj de tabak<tld var="a"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/007181.html">http://steloj.de/esperanto/monato2/007181.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la bulgaraj <tld var="a"/>oj elspezis pli ol 8 milionojn da e&ubreve;roj por trans&scirc;ar&gcirc;igo de
        varoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006857.html">Trafiko: Danuba &scirc;iptrafiko
          paralizita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la registaro ne apogas la malgrandajn kaj mezgrandajn <tld var="a"/>ojn kiel anta&ubreve;e
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006858.html">Albanio: Diktaturo de
          impostoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la instalado plej konvene okazu helpe de iu satelite televida fak<tld/>o el via
        najbara&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9504-radio.txt">Radio</url></vrk>
          <lok>95/04</lok>
        </fnt>;
      </ekz>
      <ekz>
        publika akci<tld var="a"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007132.html">Interreto: Interreto
          senpage</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plej multaj retservoj simple revendas sub sia aparta nomo la
        servon de pli granda ret- a&ubreve; telefon-<tld var="a"/>o<fnt>
        <aut>Edmund Grimley Evans</aut>: <vrk><url ref="http://esperanto.be/fel/mon/007132.html">Interreto
        senpage</url></vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 11a,
        p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="kompan.0o">kompanio</ref>,
       <ref cel="entrep.0o.firmao">entrepreno<sncref/></ref>
    </refgrp>
    <trd lng="de">Unternehmen</trd>
    <trdgrp lng="en">
      <trd>firm</trd>,
      <trd>company</trd>,
      <trd>business</trd>,
      <trd>enterprise</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Empresa<klr> (comercial)</klr></trd>,
      <trd>Firma</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>firme</trd>,
      <trd>entreprise<klr> (soci&eacute;t&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>firma</trd>,
      <trd>przedsi&eogonek;biorstwo</trd>
    </trdgrp>
  </snc>
<!-- ne konvinka ekz-o / snc-o...
