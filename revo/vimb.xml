<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: vimb.xml,v 1.3 2022/09/02 10:35:44 revo Exp $">
    <kap>
      <rad>vimb</rad>/o
    </kap>
    <drv mrk="vimb.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Bongusta fi&scirc;o (<trd lng="la">Vimba vimba</trd>) el la
          familio <ref tip="super" cel="ciprin1.0oj">ciprinedoj</ref>,
          trovebla
          en la baseno de la maroj e&ubreve;ropaj kaj kaspia:
          <ekz>
            Gruntin, &gcirc;ibeti&gcirc;inte super <tld/>o, sidis tute sola,
            drinkis glason post glaso, klare konsciante kaj
            agnoskante, ke nenio en lia vivo estas riparebla, ke al li
            restas nur forgeso
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 6a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_r;&c_y;&c_b;&c_je;&c_c;</trd>,
        <trd>&c_c;&c_y;&c_r;&c_t;&c_a;</trd>,
        <trd>&c_p;&c_a;&c_d;&c_u;&c_s;&c_v;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Z&auml;hrte</trd>,
        <trd>Ru&szlig;nase</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>vimba bream</trd>,
        <trd>vimba</trd>,
        <trd>zanthe</trd>,
        <trd>zarte</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>vimbe</trd>,
        <trd>br&egrave;me de la mer Noire</trd>
      </trdgrp>
      <trd lng="pl">certa</trd>
      <trdgrp lng="ru">
        <trd>&c_r;&c_y;&c_b;&c_je;&c_c;</trd>,
        <trd>&c_s;&c_y;&c_r;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>
  </art>

