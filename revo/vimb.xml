<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vimb.xml,v 1.4 2024/05/16 16:29:53 revo Exp $">
<kap>
  <rad>vimb</rad>/o
</kap>
<drv mrk="vimb.0o">
  <kap><tld/>o</kap>
  <snc mrk="vimb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bongusta <ref tip="lst" lst="voko:fi&scirc;oj" cel="fisx.0o.ZOO">fi&scirc;o</ref> (<trd lng="la">Vimba vimba</trd>) el la
      familio <ref tip="super" cel="ciprin1.0oj">ciprinedoj</ref>,
      trovebla en la baseno de la maroj e&ubreve;ropaj kaj kaspia:
      <ekz>
        <nom>Gruntin</nom>, &gcirc;ibeti&gcirc;inte super <tld/>o, sidis tute sola,
        drinkis glason post glaso, klare konsciante kaj
        agnoskante, ke nenio en lia vivo estas riparebla, ke al li
        restas nur forgeso
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 6a</lok></fnt>;
      </ekz>
      <ekz>
        en la rivero trovi&gcirc;as ezokoj, perkoj, plotoj, bramoj, sandroj, lojtoj, <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=753455"
              >Iecava (rivero)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/b6/Vimba_vimba.jpg/800px-Vimba_vimba.jpg"
      prm="CC BY-SA 2.5"
      ><tld/>o, rivero Morava
        <fnt>
          <aut>Tokic, propra verko</aut>
          <lok><url ref="&WCW;/File:Vimba_vimba.jpg"
              >https://commons.wikimedia.org/wiki/File:Vimba_vimba.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_b;&c_je;&c_c;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_u;&c_s;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Z&auml;hrte</trd>,
    <trd>Ru&szlig;nase</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vimba bream</trd>,
    <trd>vimba</trd>,
    <trd>zanthe</trd>,
    <trd>zarte</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vimbe</trd>,
    <trd>br&egrave;me de la mer Noire</trd>
  </trdgrp>
  <trd lng="pl">certa</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_b;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_y;&c_r;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

