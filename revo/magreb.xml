<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Magreb</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="magreb.0o">
      <kap><tld/>o</kap>
      <snc mrk="magreb.0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          La araba okcidento, la nordokcidenta parto de
          <ref tip="malprt" cel="afrik.A0o.kontinento">Afriko</ref>,
          kiun limas Mediteraneo norde kaj Saharo sude:
          <ekz>
            En <tld/>o, Maroko nuntempe preti&gcirc;as la plej granda
            biblioteko de tuta Afriko
            <fnt>
              <aut>-</aut>,
              <vrk><url ref="http://www.esperantoland.org"
                  >Esperantoland.org</url></vrk>,
              <lok>Esperanto-nova&jcirc;oj, Esperanto-librojn..., 2005-01-25</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <adm>
        Fakte, anka&ubreve; la Okcidenta Saharo estas parto de Magrebo.
        <aut>Sergio</aut>
      </adm>
      <trd lng="fr">Maghreb</trd>
      <trd lng="pt">Magreb</trd>
      <trd lng="ru">&c_M;&c_a;&c_g;&c_r;&c_i;&c_b;</trd>
    </drv>

    <drv mrk="magreb.0ano">
      <kap><tld lit="m"/>ano</kap>
      <snc mrk="magreb.0ano.GEOG">
        <dif>
          Lo&gcirc;anto de <ref tip="vid" cel="magreb.0o.GEOG"><tld/>o</ref>:
          <ekz>
            mi estis anka&ubreve; edzino de <tld lit="m"/>ano
            <fnt>
              <aut>mhdesert</aut>,
              <vrk><url
 ref="http://www.liberafolio.org/2005/Komentario/rasismo/view"
                  >rasismo</url></vrk>,
              <lok>libera folio, 2004-08-05</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">maghr&eacute;bin<klr> (subst.)</klr></trd>
      <trd lng="ru">&c_m;&c_a;&c_g;&c_r;&c_i;&c_b;&c_i;&c_n;&c_je;&c_c;</trd>
    </drv>
  </art>

