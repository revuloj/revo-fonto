<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>karat</rad>/o
</kap>
<drv mrk="karat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="karat.0o.de_alojo">
    <dif>
      &Ccirc;iu dudekkvarono da pura oro, entenata en alojo el oro:
      <ekz>
        oro je <frm><k>n</k></frm> <tld/>oj
        <klr>(kies enteno en pura oro estas
        <frm><k>n</k>/24</frm>)</klr>;
      </ekz>
      <ekz>
        oro je <frm>24</frm> <tld/>oj
        <klr>(pura)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>carat</trd>,
      <trd>karat</trd>
    </trdgrp>
    <trd lng="fr">carat<klr tip="ind"> (partie d'or pur)</klr></trd>
  </snc>
  <snc mrk="karat.0o.mezurunuo">
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> 
      de maso, por perloj kaj diamantoj, egala al 200 
      <ref cel="miligr.0o">miligramoj</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>carat</trd>,
      <trd>karat</trd>
    </trdgrp>
    <trd lng="fr">carat<klr tip="ind"> (unit&eacute; de masse)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Karat</trd>
  <trd lng="hu">kar&aacute;t</trd>
  <trd lng="nl">karaat</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_a;&c_t;</trd>
</drv>
</art>
