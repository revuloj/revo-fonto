<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karat.xml,v 1.23 2023/12/03 08:00:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>karat</rad>/o
</kap>
<drv mrk="karat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="karat.0o.de_alojo">
    <dif>
      &Ccirc;iu dudekkvarono da pura oro, entenata en alojo el oro:
      <ekz>
        oro je <frm><k>n</k></frm> <tld/>oj
        <klr>(kies enteno en pura oro estas
        <frm><k>n</k>/24</frm>)</klr>;
      </ekz>
      <ekz>
        oro je <frm>24</frm> <tld/>oj
        <klr>(pura)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>carat</trd>,
      <trd>karat</trd>
    </trdgrp>
    <trd lng="fr">carat<klr tip="ind"> (partie d'or pur)</klr></trd>
  </snc>
  <snc mrk="karat.0o.mezurunuo">
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">Mezurunuo</ref> 
      de maso, por perloj kaj diamantoj, egala al 200 
      <ref cel="miligr.0o">miligramoj</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>carat</trd>,
      <trd>karat</trd>
    </trdgrp>
    <trd lng="fr">carat<klr tip="ind"> (unit&eacute; de
    masse)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="cs">kar&aacute;t <klr>(klenot.)</klr></trd>
  <trd lng="de">Karat</trd>
  <trd lng="es">quilate</trd>
  <trd lng="hu">kar&aacute;t</trd>
  <trd lng="id">karat</trd>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30C3;&#x30C8;</trd>
  <trd lng="nl">karaat</trd>
  <trd lng="pl">karat</trd>     
  <trd lng="ro">carat</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">kar&aacute;t</trd>
  <trd lng="uk">&#x43A;&#x430;&#x440;&#x430;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x514B;&#x62C9; <pr>k&#xE8;l&#x101;</pr></trd>,
    <trd>&#x91CD;&#x91CF;&#x5355;&#x4F4D;&#x4E4B;&#x4E00; <pr>zh&#xF2;ngli&#xE0;ngd&#x101;nw&#xE8;izh&#x12B;y&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
