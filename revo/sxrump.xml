<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>&scirc;rump</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="sxrump.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Kuntiri&gcirc;i kaj sulketi&gcirc;i pro sekeco a&ubreve; maljuneco; mal&scirc;veli, malkreski:
      <ekz>
        <tld/>as mortantaj folioj, maljunula ha&ubreve;to.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_s;&c_k;&c_u;&c_r;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_b;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>  
  <trdgrp lng="de">
    <trd>schrumpfen</trd>,
    <trd>einschrumpfen</trd>,
    <trd>eingehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shrivel <klr>(up)</klr></trd>,
    <trd>crumple</trd>,
    <trd>shrink <klr>(away)</klr></trd>,
    <trd>wilt <klr>(away)</klr></trd>,
    <trd>wrinkle <klr> (up)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verschrompelen</trd>
  </trdgrp>
  <trd lng="pt">enrugar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_malmol;&c_jo;&c_zh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
