<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxrump.xml,v 1.25 2023/09/04 15:14:36 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>&scirc;rump</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxrump.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxrump.0i.FORM">
    <dif>
      <ref tip="sin" cel="tir.kun0igxi.KOMUNE">Kuntiri&gcirc;i</ref>
      kaj sulketi&gcirc;i pro sekeco a&ubreve; maljuneco; 
      <ref tip="sin" cel="sxvel.mal0i">mal&scirc;veli</ref>, 
      <ref tip="sin" cel="kresk.mal0i">malkreski</ref>:
      <ekz>
        la granda folio de lapo <klr>[&#x2026;]</klr> <tld/>is, kaj de &gcirc;i restis nur flava
        seki&gcirc;inta trunketo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trikiloj sen&ccirc;ese zigzagis inter &scirc;iaj ostaj kaj <tld/>intaj fingroj
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta juna oficisto <tld/>is kaj komencis tute eksterordinaran voja&gcirc;an tra la
        koroj de la unuavicaj rigardantoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite &scirc;i <tld/>is, <klr>[&#x2026;]</klr> kaj ekkriis: kvak! <klr>[&#x2026;]</klr> en momento
        &scirc;i fari&gcirc;is rano, kvak! kaj &scirc;i denove fari&gcirc;is la maljuna virino
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sub la premo de tiuj tri mil kronoj liaj fanfaronemo kaj memkonfido treege
        <tld/>is
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viza&gcirc;o <tld/>is kaj havas multajn sulkojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiel <tld/>as la homaj agoj, kiam oni <klr>[&#x2026;]</klr> kontrolas ilin en la lumo
        de eterneco
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as la litova lo&gcirc;antaro: dum la jaroj de la resendependi&gcirc;o elmigris &ccirc;irka&ubreve;
        250&#x202F;000 homoj kaj krome malkreskis la naskokvanto
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2005/008518.php">Mizera statistiko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="kresk.mal0i">malkreski</ref>,
      <ref cel="sxvel.0i.plivolumene">&scirc;veli<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_m;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>, 
      <trd>&c_s;&c_k;&c_u;&c_r;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>, 
      <trd>&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_b;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
    </trdgrp>  
    <trd lng="ca">arrugar-se</trd>
    <trdgrp lng="de">
      <trd>schrumpfen</trd>,
      <trd>einschrumpfen</trd>,
      <trd>eingehen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>se ratatiner</trd>,
      <trd>se racornir</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>arrugarse</trd>,
      <trd>encogerse</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>shrivel <klr>(up)</klr></trd>,
      <trd>crumple</trd>,
      <trd>shrink <klr>(away)</klr></trd>,
      <trd>wilt <klr>(away)</klr></trd>,
      <trd>wrinkle <klr> (up)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;sszezsugorodik</trd>,
      <trd>&ouml;sszeaszal&oacute;dik</trd>,
      <trd>&ouml;sszemegy</trd>
    </trdgrp>
    <trd lng="it">raggrinzirsi</trd>
    <trdgrp lng="nl">
      <trd>verschrompelen</trd>
    </trdgrp>
    <trd lng="pt">enrugar-se</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_malmol;&c_jo;&c_zh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxrump.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <klr>(Pri &scirc;tofo, vesta&jcirc;o)</klr>
      <ref tip="vid" cel="tir.kun0igxi.KOMUNE">Kuntiri&gcirc;i</ref>,
      malgrandi&gcirc;i sekve de lavado:
      <ekz>
        flanelo <tld/>as &ccirc;e lavado.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uz.el0i.foruzi">eluzi&gcirc;i</ref>,
      <ref cel="triv.0i">trivi&gcirc;i</ref>
    </refgrp>
    <trd lng="en">shrink</trd>
    <trd lng="fr">se r&eacute;tr&eacute;cir</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_je;&c_s;&c_t;&c_mol;<klr> (&c_o;&c_b; &c_o;&c_d;&c_je;&c_zh;&c_d;&c_je;, &c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_i;)</klr></trd>,
      <trd>&c_d;&c_a;&c_t;&c_mol; <ind>&c_u;&c_s;&c_a;&c_d;&c_k;&c_u;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x3057;&#x306A;&#x3073;&#x308B;</trd>,
    <trd>&#x3057;&#x308F;&#x304C;&#x5BC4;&#x308B;  [<ind>&#x3057;&#x308F;&#x304C;&#x3088;&#x308B;</ind>]</trd>,
    <trd>&#x3057;&#x304A;&#x308C;&#x308B;</trd>,
    <trd>&#x7E2E;&#x3080;  [<ind>&#x3061;&#x3062;&#x3080;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>(&c_z;)&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_ch;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_o;&c_l;&c_o;&c_b;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_o;&c_r;&c_o;&c_b;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>(&c_z;&c_ib;)&c_b;&c_g;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o; &c_t;&c_k;&c_a;&c_n;&c_i;&c_n;&c_u;)</trd>,
    <trd>(&c_z;&c_ib;)&c_v;&#x2019;&c_ja;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>(&c_z;)&c_s;&c_o;&c_h;&c_n;&c_u;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_l;&c_i;&c_s;&c_t;&c_ja; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>
</art>
