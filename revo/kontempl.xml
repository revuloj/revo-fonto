<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontempl.xml,v 1.14 2024/03/25 13:25:18 revo Exp $">
<kap>
  <rad>kontempl</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kontempl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontempl.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="rigard.0i">Rigardi</ref> longe, pripense,
      absorbite:
      <ekz>
        Jano <tld/>is la foton, &gcirc;i efektive estis admirinda
        <fnt>
          <bib>ChB</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiuj, la&ubreve; vi, estas veraj filozofoj? &ndash; personoj kiuj <tld/>as la veron
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvin</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is <klr>[&scirc;in]</klr> minuteton kvaza&ubreve; mirante, ke tia mizera insekto
        kapablas havi civilizitajn sentojn
        <fnt>
          <bib>ChV</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        mediteme <tld/>ante akvon, &scirc;a&ubreve;mantan la&ubreve; &scirc;tonoj, la afrikano
        subite eksentis sur si ies rigardon
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/andromedo.html">La
          nebulozo de Andromedo</url></vrk>,
          <lok>&ccirc;apitro 11a. - 2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        bona okazo por <tld/>i unurigarde la staton de la studoj pri tiu temo estas nun
        la <nom>Encyclopedia of Witchcraft</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2008/009295.php">La sor&ccirc;istin-persekutoj: rerigardo kaj
          reinterpretoj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admir.0i">admiri</ref>,
      <ref cel="atent.0i">atenti</ref>,
      <ref cel="ekzame.0i.esplori">ekzameni<sncref/></ref>,
      <ref cel="esplor.0i">esplori</ref>,
      <ref cel="konsid.0i">konsideri</ref>,
      <ref cel="medit.0i.KOMUNE">mediti</ref>,
      <ref cel="observ.0i.rigardi">observi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">to contemplate</trd>
  <trdgrp lng="cs">
    <trd>hloubat</trd>,
    <trd>p&rcaron;em&iacute;tat</trd>,
    <trd>rozj&iacute;mat o n&ecaron;&ccaron;em</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>nachdenken</trd>,
    <trd>sinnen</trd>,
    <trd>erw&auml;gen</trd>,
    <trd>betrachten <klr>(gedanklich)</klr></trd>,
    <trd>seinen <ind>Gedanke</ind>n nachh&auml;ngen</trd>,
    <trd>innere <ind>Einkehr</ind> halten</trd>,
    <trd>beschauen</trd>,
    <trd>versinken<klr> (in Gedanken)</klr></trd>
  </trdgrp>
  <trd lng="fr">contempler</trd>
  <trdgrp lng="ja">
    <trd>&#x719F;&#35222;&#12377;&#x308B; <pr>&#12371;&#x306A;&#12375;&#12377;&#x308B;</pr></trd>,
    <trd>&#x89B3;&#29031;&#12377;&#x308B; <pr>&#x304B;&#12435;&#12375;&#12423;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6C88;&#x601D;&#x9ED9;&#32771;&#12377;&#x308B;
    <pr>&#12375;&#x305A;&#x304A;&#12418;&#12384;&#x307E;&#12371;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#30609;&#x60F3;&#12377;&#x308B; <pr>&#12417;&#12356;&#x305D;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mediteren</trd>,
    <trd>contempleren</trd>
  </trdgrp>
  <trd lng="pl">kontemplowa&cacute;</trd>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_je;&c_r;&c_c;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kontemplova&tcaron;</trd>,
    <trd>meditova&tcaron;</trd>
  </trdgrp>
  <trd lng="zh">&#x6C89;&#x601D; <pr>ch&#xE9;ns&#x12B;</pr></trd>
</drv>

<drv mrk="kontempl.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="kontempl.0ado.KOMUNE">
    <dif>
      Observado kaj enpensi&gcirc;o, <ref tip="vid" cel="medit.0ado">meditado</ref>:
      <ekz>
        bruaj vo&ccirc;oj &gcirc;enas &gcirc;uan <tld/>adon
        <fnt>
          <bib>ChB</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> abrupte metis finon al sia <tld/>ado por ekpa&scirc;i
        <fnt>
          <bib>ChR</bib>
          <lok>Spiona rondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pasigis plurajn jarojn de sola vivo, sindonanta al <tld/>ado
        kaj verkado
        <fnt>
          <aut>C. Marvin, trad. rB</aut>,
          <vrk>...Al Ghazali</vrk>,
          <lok><url ref="http://www.geocities.com/hilelista_komunumo/hilelisto32.htm">Hilelisto,
          2003-04-15, numero 32a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        mirinde, <klr>[&#8230;]</klr> ke post <tld/>ado pri kio estas dia, kiam oni revenas al
        la homa mondo oni agas maldece
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Sep</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admir.0o">admiro</ref>,
      <ref cel="atent.0o">atento</ref>,
      <ref cel="fascin.0o">fascino</ref>,
      <ref cel="intere.0o.sento">intereso<sncref/></ref>,
      <ref cel="konsid.0o">konsidero</ref>,
      <ref cel="konste1.0o">konsterno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_z;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="en">contemplation</trd>
  <trdgrp lng="cs">
    <trd>kontemplace</trd>,
    <trd>p&rcaron;em&iacute;t&aacute;n&iacute;</trd>,
    <trd>rozj&iacute;m&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>innere <ind>Einkehr</ind></trd>,
    <trd>gedankliche <ind>Betrachtung</ind></trd>,
    <trd>Kontemplation</trd>,
    <trd>Andacht</trd>,
    <trd>Innenschau</trd>,
    <trd>Versunkenheit</trd>,
    <trd>innere <ind>Versenkung</ind></trd>
  </trdgrp>
  <trd lng="fr">contemplation</trd>
  <trdgrp lng="ja">
    <trd>&#x719F;&#35222;  <pr>&#12376;&#12421;&#x304F;&#12375;</pr></trd>,
    <trd>&#x89B3;&#29031;  <pr>&#x304B;&#12435;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x89B3;&#x60F3;  <pr>&#x304B;&#12435;&#x305D;&#12358;</pr></trd>,
    <trd>&#x6C88;&#x601D;&#x9ED9;&#32771;  <pr>&#12385;&#12435;&#12375;&#12418;&#12387;&#12371;&#12358;</pr></trd>,
    <trd>&#30609;&#x60F3;  <pr>&#12417;&#12356;&#x305D;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>meditatie</trd>,
    <trd>contemplatie</trd>
  </trdgrp>
  <trd lng="pl">kontemplacja</trd>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_je;&c_r;&c_c;&c_a;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>medit&aacute;cia</trd>,
    <trd>rozj&iacute;manie</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38745;&#x4FEE; <pr>j&#xEC;ngxi&#x16B;</pr></trd>,
    <trd>&#x4E13;&#x5FC3; <pr>zhu&amacron;nx&#x12B;n</pr></trd>
  </trdgrp>
</drv>
</art>

