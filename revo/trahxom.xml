  <art mrk="$Id$">
    <kap>
      <rad>tra&hcirc;om</rad>/o
    </kap>

    <drv mrk="trahxom.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">MED</uzo>
        <dif>
          Infekta okulmalsano, &ccirc;e kiu la
          <ref cel="konjun4">konjuktivo</ref> ru&gcirc;i&gcirc;as, en
&gcirc;i aperas
          grizetaj grajnoj.
        </dif>
      </snc>
      <trd lng="en">trachoma</trd>
      <trdgrp lng="he">
        <trd>&tet;&resh;&kaf;&vav;&mem;&he;</trd>,
        <trd>&gimel;&resh;&ayin;&nun;&tav;</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_r;&c_a;&c_h;&c_o;&c_m;&c_a;</trd>
    </drv>
  </art>


