<drv mrk="tre.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ekstre.0a.kvante">Ekstrema</ref>, <ref tip="dif"
      cel="ordina.ekster0a.lau_grado">eksterordinara</ref>:
      <ekz>
        <tld/>a simpleco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105771"
              >Hindaj ciferoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a deklini&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=745124"
              >Milutin Milankovi&cacute;</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_a;&c_j;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">au&szlig;erordentliche</trd>
</drv>

