<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tre</rad>
</kap>
<drv mrk="tre.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo montranta la superlativon kaj signifanta &leftquot;en alta grado,
      forte&rightquot;:
      <ekz>
        lia heroeco <tld/> pla&ccirc;is al mi<fnt><bib>F</bib>, <lok>&FE;
        35</lok></fnt>;
      </ekz>
      <ekz>
        <tld/> granda, a&gcirc;a, malsana;
      </ekz>
      <ekz>
        <tld/> rapide iri;
      </ekz>
      <ekz>
        <tld/> la&ubreve;te paroli;
      </ekz>
      <ekz>
        estus <tld/> bele se;
      </ekz>
      <ekz>
        <tld/> multe da akvo;
      </ekz>
      <ekz>
        mi <tld/> estimas lin;
      </ekz>
      <ekz>
        <tld/> ami<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/> timi<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/> povas esti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mia oficejo <tld/> multe suferus de tio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_l;&c_mol;&c_m;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>velmi</trd>,
    <trd>velice</trd>
  </trdgrp>
  <trd lng="de">sehr</trd>
  <trd lng="en">very</trd>
  <trdgrp lng="es">
    <trd>muy</trd>,
    <trd>mucho</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>beaucoup</trd>,
    <trd>tr&egrave;s</trd>
  </trdgrp>
  <trd lng="he">&mem;&alef;&dalet;</trd>
  <trd lng="hu">nagyon</trd>  
  <trd lng="nl">erg</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_mol;&c_m;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tre.0ege">
  <kap><tld/>ege</kap>
  <snc>
    <dif>
      En ekstrema grado.
    </dif>
    <refgrp tip="vid">
      <ref cel="eg">ege</ref>,
      <ref cel="mult.0e">multe</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_t;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="en">very much</trd>
  <trdgrp lng="es">
    <trd>extremadamente</trd>,
    <trd>much&iacute;simo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;norm&eacute;ment</trd>,
    <trd>extr&ecirc;mement</trd>
  </trdgrp>
  <trd lng="hu">rendk&iacute;v&uuml;li <ind>m&oacute;don</ind></trd>  
  <trd lng="nl">zeer <ind>erg</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_r;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_o;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  </trdgrp>
</drv>
</art>
