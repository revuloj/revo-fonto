<drv mrk="tre.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo montranta la <ref cel="superl.0o.GRA">superlativo</ref>n kaj
      signifanta &leftquot;en alta grado, <ref cel="fort.0e.multe">forte</ref>&rightquot;:
      <ekz>
         lia heroeco <tld/> pla&ccirc;is al mi
         <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        kvardek jaroj ne estas <tld/> alta a&gcirc;o
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        Johano estis <tld/> mal&gcirc;oja, &ccirc;ar lia patro estis <tld/> malsana
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> multe da fi&scirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 47:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa korpuso iris <tld/> rapide
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        deziro <tld/> granda, sed mankas la forto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        apud plena man&gcirc;otablo &ccirc;iu estas <tld/> afabla
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> tre rapide rimarkis, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam Karadelo <tld/> malla&ubreve;te parolis, tamen Valentino a&ubreve;dis
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        estus <tld/> belege, se trovi&gcirc;us ankora&ubreve; unu homo, kiu povus <klr>[&#8230;]</klr>
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ijob efektive <tld/> estimas kaj amas lin
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tre timis, ke mi ne povos akompani mian edzinon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/> povas esti</ind>, ke la plimulto de ni ne &gcirc;isvivos tiun momenton
        <fnt>
          <bib>EE</bib>
          <lok>IV</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>es kann durchaus sein</trd>,
          <trd>es ist sehr wahrscheinlich</trd>
        </trdgrp>
      </ekz>
      <ekz>
         mia oficejo <tld/> multe suferas de tio
         <fnt>
          <bib>Marta</bib>
        </fnt>.
       </ekz>
     </dif>
   </snc>
