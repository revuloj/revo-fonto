<drv mrk="tre.0ege">
   <kap><tld/>ege</kap>
   <snc>
     <dif>
       En ekstrema grado:
       <ekz>
         mi <tld/>ege dankas vin
         <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="eg">ege</ref>,
       <ref cel="mult.0e">multe</ref>
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_d;&c_t;&c_a;</trd>,
     <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>nesm&#xED;rn&#x11B;</trd>,
    <trd>ohromn&#x11B;</trd>,
    <trd>p&#x159;evelice</trd>,
    <trd>tuze</trd>,
    <trd>velmi mnoho</trd>
  </trdgrp>
  <trd lng="en">very much</trd>
  <trdgrp lng="es">
     <trd>extremadamente</trd>,
     <trd>much&iacute;simo</trd>
   </trdgrp>
