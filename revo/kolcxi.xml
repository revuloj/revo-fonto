<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolcxi.xml,v 1.19 2024/03/14 22:26:10 revo Exp $">
<kap>
  <rad>kol&ccirc;ik</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="&hcirc;">kol&hcirc;ik</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="kolcxi.0o">
  <kap><tld/>o,
  <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <snc mrk="kolcxi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Venena vegeta&jcirc;o de mezvarma E&ubreve;ropo kun violkoloraj
      floroj (iom similaj al <ref tip="malprt" cel="krokus.0o">krokusoj</ref>),
      perdanta dum la florado en a&ubreve;tuno la foliojn;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Colchicum</trd>):
      <ekz>
         la folioj de ursa ajlo similas al tiuj de la tre venena a&ubreve;tuna <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18114">Kuracplanto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj partoj de <tld var="&hcirc;"/>o enhavas la tre venenan alkaloidon kol&hcirc;icino
        <fnt>
         <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/A%C5%ADtuna_kol%C4%A5iko">
          A&ubreve;tuna kol&hcirc;iko</url></vrk>
          <lok>Vikipedio, 2023-09-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ib;&c_m;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_je;&c_s;&c_je;&c_n;&c_je;&c_n; &c_m;&c_i;&c_n;&c_z;&c_u;&c_h;&c_a;&c_r;</trd>
  <trd lng="de">Herbstzeitlose</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l; &a_H;&a_s;&a_r;&a_t;</trd>,
    <trd>&a_s;&a_w;&a_r;&a_n;&a_j;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>colchique</trd>,
    <trd>safran des pr&eacute;s</trd>,
    <trd>tue-<ind>chien</ind></trd>
  </trdgrp>
  <trd lng="hu">kikerics</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30EB;&#x30C1;&#x30AB;&#x30E0;</trd>,
    <trd>&#x30A4;&#x30CC;&#x30B5;&#x30D5;&#x30E9;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tijloos</trd>,
    <trd>herfsttijloos</trd>,
    <trd>naakt <ind>begijntje</ind></trd>,
    <trd>kind-voor-de-vader</trd>,
    <trd>kale <ind>juffer</ind></trd>
  </trdgrp>
  <trd lng="pl">zimowit jesienny</trd>
  <trd lng="ro">br&acirc;ndu&#537;&abreve; de toamn&abreve;</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k; &c_o;&c_s;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_z;&#x43D;&#x44C;&#x43E;&c_c;&c_v;&c_ib;&c_t;</trd>,
    <trd>&#x43F;&c_ib;&c_z;&#x43D;&#x43E;&c_c;&c_v;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>
</art>
