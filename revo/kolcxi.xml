<vortaro>
<art mrk="$Id$">
<kap>
<rad>kol&ccirc;ik</rad>/o
</kap>
<drv mrk="kolcxi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Colchicum</trd>), venena vegeta&jcirc;o kun violkoloraj
      floroj, perdanta dum la florado en a&ubreve;tuno la foliojn. 
    </dif>
  </snc>
  <trd lng="de">Herbstzeitlose</trd>
  <trd lng="hu">kikerics</trd>
  <trdgrp lng="fr">
    <trd>colchique</trd>,
    <trd>safran des pr&eacute;s</trd>,
    <trd>tue-<ind>chien</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tijloos</trd>,
    <trd>herfsttijloos</trd>,
    <trd>naakt <ind>begijntje</ind></trd>,
    <trd>kind-voor-de-vader</trd>,
    <trd>kale <ind>juffer</ind></trd>
  </trdgrp>
<trd
lng="ru">&c_b;&c_je;&c_z;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k;
&c_o;&c_s;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
