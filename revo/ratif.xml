<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ratif.xml,v 1.26 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>ratif</rad>/i <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="ik">ratifik</rad>/i <fnt><bib>PIV1</bib></fnt></kap></var>
  <var><kap><rad var="ikaci">ratifikaci</rad>/i </kap><uzo tip="stl">ARK</uzo></var>
</kap>
<drv mrk="ratif.0i">
  <kap><tld/>i,
  <var><kap><tld var="ik"/>i</kap></var>,
   <var><kap><tld var="ikaci"/>i</kap></var>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ratif.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="konfir.0i.JUR">Konfirmi</ref> inter&scirc;tatan kontrakton, subskribitan
      nome de la &scirc;tato de &scirc;tataj rajtigitoj:
      <ekz>
        en multaj landoj la interkonsentojn <tld/>as la parlamento;
      </ekz>
      <ekz>
        la menciitaj konvencioj estas kaj morale kaj jure devigaj por
        &ccirc;iuj &scirc;tatoj, kiuj subskribis kaj <tld var="ik"/>is ilin
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="http://www.ivolapenna.org/historiegravaj/manifesto_100107.htm">
          Por Paco en Libereco - Por Libereco en Paco</url></vrk>,
          <lok>Horizonto, 1977:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ratificar</trd>
  <trd lng="cs">ratifikovat</trd>
  <trd lng="de">ratifizieren</trd>
  <trd lng="en">ratify</trd>
  <trd lng="es">ratificar</trd>
  <trd lng="fr">ratifier</trd>
  <trdgrp lng="hu">
    <trd>ratifik&aacute;l</trd>,
    <trd>j&oacute;v&aacute;hagy</trd>
  </trdgrp>
  <trd lng="ja">&#x6279;&#x51C6;&#x3059;&#x308B;  <pr>&#x3072;&#x3058;&#x3085;&#x3093;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">ratificeren</trd>
  <trdgrp lng="pl">
    <trd>ratyfikowa&cacute;</trd>,
    <trd>zatwierdza&cacute;</trd>
  </trdgrp>
  <trd lng="pt">ratificar</trd>
  <trd lng="ru">&c_r;&c_a;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ratifikova&tcaron;</trd>
  <trd lng="uk">&c_r;&c_a;&c_t;&c_i;&c_f;&c_ib;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x6279;&#x51C6; <pr>p&#x12B;zh&#x1D4;n</pr></trd>
</drv>

<drv mrk="ratif.0o">
  <kap><tld/>o,
  <var><kap><tld var="ik"/>o</kap></var>,
  <var><kap><tld var="ikaci"/>o</kap></var>
  </kap>
  <snc mrk="ratif.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficiala <ref tip="super" cel="konfir.0o.JUR">konfirmo</ref> de inter&scirc;tata kontrakto:
      <ekz>
        tiu &ccirc;i Protokolo estas submetita al <tld/>o de &ccirc;iu &scirc;tato
        <fnt>
          <aut>trad. vl</aut>,
          <vrk><url ref="http://www.mas-eo.org/article.php3?id_article=208">
          UN-Protokolo pri mortpuno</url></vrk>,
          <lok>MAS, [2007?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rapide alproprigis al si la regon kaj balda&ubreve; ricevis la
        <tld var="ik"/>on de la konsilantaro de ulemoj <klr>[&#x2026;]</klr> de la urbo
        <fnt>
          <aut>J. Bousac, trad. M. Leon</aut>,
          <vrk><url ref="&Mondediplo;article841.html">
          Afganio vidate de Herato</url></vrk>,
          <lok>le Monde diplomatique, 2003-12</lok>
        </fnt>;
      </ekz>
    <ekz>
      montri&gcirc;is vantaj la lastaj esperoj de bohemaj ekzilitoj je
      politika helpo, post <tld var="ikaci"/>o de la interkonsentoj de paco
      <nom>Westfala</nom> en <nom>N&uuml;renberg</nom>
      <fnt>
        <aut>J. Nov&aacute;k, trad. Rondeto de esperantistinoj</aut>,
        <vrk><url ref="&Gutenberg;files/21951/21951-0.txt">
        Jan Amos Komensk&yacute;</url></vrk>,
        <lok>1921</lok>
      </fnt>.
    </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>kone&ccaron;n&eacute; schv&aacute;len&iacute;</trd>,
    <trd>ratifikace</trd>
  </trdgrp>
  <trd lng="de">Ratifizierung</trd>
  <trd lng="en">ratification</trd>
  <trd lng="fr">ratification</trd>
  <trd lng="ja">&#x6279;&#x51C6;  <pr>&#x3072;&#x3058;&#x3085;&#x3093;</pr></trd>
  <trd lng="pl">ratyfikacja</trd>
  <trd lng="sk">ratifik&aacute;cia</trd>
  <trd lng="uk">&c_r;&c_a;&c_t;&c_i;&c_f;&c_ib;&#x43A;&c_a;&c_c;&c_ib;&#x44F;</trd>
</drv>
</art>
