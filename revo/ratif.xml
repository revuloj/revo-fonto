<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ratif</rad>/i <fnt><bib>PV</bib></fnt>,
  <var><kap><rad>ratifik</rad>/i <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="ratif.0i">
  <kap><tld/>i, <var><kap>ratifiki</kap></var></kap>
  <snc mrk="ratif.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="konfir.0i.JUR">Konfirmi</ref> inter&scirc;tatan kontrakton, subskribitan en la
      nomo de la &scirc;tato de &scirc;tataj rajtigitoj:
      <ekz>
        en multaj landoj la interkonsentojn <tld/>as la parlamento;
      </ekz>  
      <ekz>
        la menciitaj konvencioj estas kaj morale kaj jure devigaj por 
        &ccirc;iuj &scirc;tatoj, kiuj subskribis kaj ratifikis ilin
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="http://www.ivolapenna.org/historiegravaj/manifesto_100107.htm">
          Por Paco en Libereco - Por Libereco en Paco</url></vrk>,
          <lok>Horizonto, 1977:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ratificar</trd>
  <trd lng="cs">ratifikovat</trd>
  <trd lng="de">ratifizieren</trd>
  <trd lng="en">ratify</trd>
  <trd lng="es">ratificar</trd>
  <trd lng="fr">ratifier</trd>
  <trdgrp lng="hu">
    <trd>ratifik&aacute;l</trd>,
    <trd>j&oacute;v&aacute;hagy</trd>
  </trdgrp>
  <trd lng="nl">ratificeren</trd>
  <trdgrp lng="pl">
    <trd>ratyfikowa&cacute;</trd>,
    <trd>zatwierdza&cacute;</trd>
  </trdgrp>
  <trd lng="pt">ratificar</trd>
  <trd lng="ru">&c_r;&c_a;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ratifikova&tcaron;</trd>
</drv>

<drv mrk="ratif.0o">
  <kap><tld/>o, <var><kap>ratifiko</kap></var></kap>
  <snc mrk="ratif.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficiala <ref tip="super" cel="konfir.0o.JUR">konfirmo</ref> de inter&scirc;tata kontrakto:
      <ekz>
        tiu &ccirc;i Protokolo estas submetita al <tld/>o de &ccirc;iu &scirc;tato
        <fnt>
          <aut>trad. vl</aut>,
          <vrk><url ref="http://www.mas-eo.org/article.php3?id_article=208">
          UN-Protokolo pri mortpuno</url></vrk>,
          <lok>MAS, [2007?]</lok>
        </fnt>;
      </ekz>  
      <ekz>
        li rapide alproprigis al si la regon kaj balda&ubreve; ricevis la 
        ratifikon de la konsilantaro de ulemoj <klr>[&#8230;]</klr> de la urbo
        <fnt>
          <aut>J. Bousac, trad. M. Leon</aut>,
          <vrk><url ref="http://eo.mondediplo.com/article841.html">
          Afganio vidate de Herato</url></vrk>,
          <lok>le Monde diplomatique, 2003-12</lok>
        </fnt>.
      </ekz>
    </dif>   
  </snc>
  <trdgrp lng="cs">
    <trd>kone&ccaron;n&eacute; schv&aacute;len&iacute;</trd>,
    <trd>ratifikace</trd>
  </trdgrp>
  <trd lng="de">Ratifizierung</trd>
  <trd lng="en">ratification</trd>
  <trd lng="fr">ratification</trd>
  <trd lng="pl">ratyfikacja</trd>
  <trd lng="sk">ratifik&aacute;cia</trd>
</drv>
</art>
