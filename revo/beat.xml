<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: beat.xml,v 1.17 2021/03/04 20:23:07 revo Exp $">
<kap>
  <rad>beat</rad>/a
</kap>
<drv mrk="beat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Feli&ccirc;ega 1.
    </dif>
    <trd lng="ca">benaurat</trd>
    <trd lng="es">beato</trd>
    <trd lng="fr">b&eacute;at</trd>
    <trd lng="hu">&uuml;dv&ouml;z&uuml;lt<klr tip="amb"> (boldog)</klr>
    </trd>
    <trd lng="it">beato<klr> (agg.)</klr></trd>
    <trd lng="pl">b&lstroke;ogi</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_a;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_m;&c_ja;&c_t;&c_je;&c_zh;&c_n;&c_y;&c_j;
      </trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">KAT</uzo>
    <dif>
      Solene proklamita de la Eklezio kiel preska&ubreve;sanktulo:
      <ekz>
        la papo deklaris Johanon 23an <tld/>a<fnt><aut>Gerrit
        Berveling</aut>: <vrk>Pri la dokumento &leftquot;Dominus Iesus
        &rightquot; de
        Vatikano</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">beatificat</trd>
    <trd lng="de">selig</trd>
    <trd lng="fr">bienheureux<klr> (adj.)</klr></trd>
    <trd lng="hu">boldog</trd>
    <trdgrp lng="it">
      <trd>beato<klr> (beatificato)</klr></trd>,
      <trd>beatificato</trd>
    </trdgrp>
    <trd lng="pl">b&lstroke;ogos&lstroke;awiony</trd>
    <trd lng="ru">&c_b;&c_l;&c_a;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="ca">beat <klr>(adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>bla&#x17E;en&#xFD;</trd>,
    <trd>p&#x159;e&#x161;&#x165;astn&#xFD;</trd>
  </trdgrp>
  <trd lng="en">beatified</trd>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5D1;&#x5D5;&#x5E8;&#x5DA;</trd>,
    <trd>&#x5DE;&#x5D0;&#x5D5;&#x5E9;&#x5E8;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>blahoslaven&#xFD;</trd>,
    <trd>bla&#x17E;en&#xFD;</trd>
  </trdgrp>
</drv>

<drv mrk="beat.0igo">
  <kap><tld/>igo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Validigo kaj proklamo de ies <tld/>eco:
      <ekz>
        proceso de <tld/>igo &ccirc;e Vatikano<fnt><aut>Augusto Casquero
        </aut>:
        <vrk>2002 - Internacia jaro de Antoni Gaud&iacute;</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 6a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">beatificaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>beatifikace</trd>,
    <trd>blaho&#x159;e&#x10D;en&#xED;</trd>
  </trdgrp>
  <trd lng="en">beatification</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;atification</trd>
  </trdgrp>
  <trd lng="it">beatificazione</trd>
  <trd lng="pl">beatyfikacja</trd>
  <trd lng="ru">
  &c_b;&c_je;&c_a;&c_t;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">beatifik&#xE1;cia</trd>
</drv>

<drv mrk="beat.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Agnoskita preska&ubreve;sanktulo.
    </dif>
  </snc>
  <trd lng="ca">beat <klr>(subst.)</klr></trd>
  <trd lng="en">beatus</trd>
  <trdgrp lng="fr">
    <trd>bienheureux<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="it">beato<klr> (sost.)</klr></trd>
  <trd lng="pl">b&lstroke;ogos&lstroke;awiony</trd>
  <trd lng="ru">&c_b;&c_l;&c_a;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
