<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lan.xml,v 1.30 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lan</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="lan.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="lan.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Densa mola <ref tip="super" cel="har.0aro.FRIZ">hararo</ref> kovranta la korpon de kelkaj
      bestoj,
      precipe de <ref tip="malprt" cel="sxaf.0o.ZOO">&scirc;afo</ref>, kaj uzata por teksi &scirc;tofojn:
      <ekz>
        bona &scirc;afo, sana kaj abunde kovrita de <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        blanka <tld/>o kiel ne&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 1:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru tapi&scirc;ojn el kapra <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> alportis <ref cel="sxpin.0i.TEKS">&scirc;pini</ref>tan <tld/>on bluan kaj
        purpuran
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 35:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mallevis la <tld/>ajn tukojn de la tendo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne surmetu sur vin miksospecan veston el <tld/>o kaj lino kune
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 22:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        li portis dis&scirc;iritajn <tld/>kitelon kaj &ccirc;emizon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;afo donas sian <tld/>on, por ke mastro havu panon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kardi <tld/>on
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kelke da disaj, <tld/>osimilaj nubetoj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="lanug.0o.birdo">lanugo<sncref/></ref>,
       <ref cel="vat.0o">vato</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_e;&c_r;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_w;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_w;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_la;&t_tsheg;</trd>
  <trd lng="br">gloan</trd>
  <trd lng="ca">llana</trd>
  <trd lng="cs">vlna</trd>
  <trdgrp lng="de">
   <trd>Wolle</trd>,
   <trd>Vlies</trd>
  </trdgrp>
  <trd lng="el">&my;&alfa;&lambda;&lambda;&jota_ton;</trd>
  <trd lng="en">wool</trd>
  <trd lng="es">lana</trd>
  <trd lng="fr">laine</trd>
  <trd lng="hu">gyapj&uacute;</trd>
  <trd lng="id">wol</trd>
  <trdgrp lng="ja">
    <trd>&#x7F8A;&#x6BDB;  <pr>&#x3088;&#x3046;&#x3082;&#x3046;</pr></trd>,
    <trd>&#x30A6;&#x30FC;&#x30EB;</trd>,
    <trd>&#x67D4;&#x6BDB;  <pr>&#x3058;&#x3085;&#x3046;&#x3082;&#x3046;</pr></trd>,
    <trd>&#x6BDB;&#x7CF8;  <pr>&#x3051;&#x3044;&#x3068;</pr></trd>,
    <trd>&#x7F8A;&#x6BDB;&#x72B6;&#x306E;&#x3082;&#x306E;  <pr>&#x3088;&#x3046;&#x3082;&#x3046;&#x3058;&#x3087;&#x3046;&#x306E;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">wol</trd>
  <trd lng="pl">we&lstroke;na</trd>
  <trd lng="pt">l&atilde;</trd>
  <trd lng="ru">&c_sh;&c_je;&c_r;&c_s;&c_t;&c_mol;</trd>
  <trd lng="tr">y&uuml;n</trd>
  <trd lng="uk">&c_v;&c_o;&c_v;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7F8A;&#x6BDB; <pr>y&#xE1;ngm&#xE1;o</pr></trd>,
    <trd>&#x7D68;&#x7DDA; <pr>r&#xF3;ngxi&#xE0;n</pr></trd>,
    <trd>&#x7ED2;&#x7EBF; <pr>r&#xF3;ngxi&#xE0;n</pr></trd>,
    <trd>&#x6BDB; <pr>m&#xE1;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lan.0ero">
  <kap>
    <tld/>ero
  </kap>
  <snc mrk="lan.0ero.floko">
    <dif>
      <ref tip="super" cel="flok.0o.tufeto">Floko</ref> de lano:
      <ekz>
        preska&ubreve; en la mezo de la lensa disko de la grandega Galaksio, <klr>[&#x2026;]</klr>
        debran&ccirc;i&gcirc;is sistemo da steloj, &scirc;ajnanta eta <tld/>ero, &ndash; sendube, giganta
        bran&ccirc;o
        <fnt>
          <aut>Ivan Efremov</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Efremov/andromedo_15.html">La nebulozo de Andromedo</url></vrk>
          <lok>&Ccirc;apitro 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        de &scirc;afo senlana e&ccirc; <tld/>ero ta&ubreve;gas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bukl.0o.hara">buklo</ref>,
      <ref cel="fask.har0o">harfasko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Woll<ind>flocke</ind></trd>,
    <trd>Str&auml;hne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>strand</trd>,
    <trd>wool <ind>flake</ind></trd>
  </trdgrp>
  <trd lng="fr"><ind>flocon</ind> de laine</trd>
</drv>

<drv mrk="lan.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lan.sen0igi.TEKS">
    <dif>
      Depreni, <ref tip="super" cel="tond.de0i">detondi</ref> la <tld/>on de...:
      <ekz>
        proksime, kelkaj liaj servutuloj sen<tld/>igas &scirc;afojn
        <fnt>
          <aut>G. G&aacute;rdonyi, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/stelspec.pdf">
          Steloj de Eger</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>igu, sed ne senfeligu
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_y;&c_g;&c_ch;&c_y; <klr>(&c_z;&c_d;&c_y;&c_m;&c_a;&c_ju;&c_ch;&c_y; &c_v;&c_o;&c_w;&c_n;&c_u;)</klr></trd>
  <trd lng="cs">ost&rcaron;&iacute;hat vlnu</trd>
  <trd lng="de">scheren <klr>(Schaf)</klr></trd>
  <trd lng="fr">tondre<klr> (moutons)</klr></trd>
  <trd lng="ja">&#x6BDB;&#x3092;&#x5208;&#x308A;&#x53D6;&#x308B;  <pr>&#x3051;&#x3092;&#x304B;&#x308A;&#x3068;&#x308B;</pr></trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_i;&c_g;&c_t;&c_i; (&c_o;&c_v;&c_je;&c_c;&c_mol;)</trd>
</drv>
</art>
