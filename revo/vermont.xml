<kap>
  <rad>Vermont</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vermont.0o">
  <kap><tld/>o</kap>
  <snc mrk="vermont.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la nordorienta parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        restoracio <ctl>Esperanto</ctl> en
        Burlington, <tld/>o
        <fnt>
          <aut>&Jcirc;. Amis</aut>,
          <vrk>Vermonta restoracio fami&gcirc;as danke al Esperanto</vrk>,
          <lok>en <url ref="http://bulteno.esperanto-usa.org/2012/6/20-loke.html">
          Usona Esperantisto, 2012:6</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_e;&c_r;&c_m;&c_o;&c_n;&c_t;</trd>
  <trd lng="cs">Vermont</trd>
  <trd lng="en">Vermont</trd>
  <trd lng="fr">Vermont</trd>
  <trd lng="ja">&#x30D0;&#x30FC;&#x30E2;&#x30F3;&#x30C8;&#x5DDE;  <pr>&#x3070;&#x30FC;&#x3082;&#x3093;&#x3068;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="pl">Vermont</trd>
  <trd lng="sk">Vermont</trd>
  <trd lng="uk">&c_V;&c_je;&c_r;&#x43C;&#x43E;&#x43D;&c_t; (&#x43E;&c_d;&c_i;&#x43D; &c_ib;&c_z;
  &c_a;&#x43C;&c_je;&c_r;&c_i;&#x43A;&c_a;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_h; &c_sh;&c_t;&c_a;&c_t;&c_ib;&c_v;)</trd>
  <trdgrp lng="zh">
    <trd>&#x4F5B;&#x8499;&#x7279; <pr>F&#xF3;m&#xE9;ngt&#xE8;</pr></trd>,
    <trd>&#x4F5B;&#x8499;&#x7279;&#x5DDE; <pr>F&#xF3;m&#xE9;ngt&#xE8;zh&#x14D;u</pr></trd>
  </trdgrp>
</drv>
