<art mrk="$Id: vermont.xml,v 1.4 2022/03/28 11:08:54 revo Exp $">
<kap>
  <rad>Vermont</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vermont.0o">
  <kap><tld/>o</kap>
  <snc mrk="vermont.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la nordorienta parto de 
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        restoracio <ctl>Esperanto</ctl> en 
        Burlington, <tld/>o
        <fnt>
          <aut>&Jcirc;. Amis</aut>,
          <vrk>Vermonta restoracio fami&gcirc;as danke al Esperanto</vrk>,
          <lok>en <url ref="http://bulteno.esperanto-usa.org/2012/6/20-loke.html">
          Usona Esperantisto, 2012:6</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Vermont</trd>
  <trd lng="en">Vermont</trd>
  <trd lng="fr">Vermont</trd>
  <trd lng="sk">Vermont</trd>
  <trd lng="uk">&#x412;&#x435;&#x440;&#x43C;&#x43E;&#x43D;&#x442; (&#x43E;&#x434;&#x438;&#x43D; &#x456;&#x437; &#x430;&#x43C;&#x435;&#x440;&#x438;&#x43A;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x445; &#x448;&#x442;&#x430;&#x442;&#x456;&#x432;)</trd>
</drv>
</art>

