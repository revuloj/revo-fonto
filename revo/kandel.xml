<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kandel</rad>/o
</kap>
<drv mrk="kandel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Lumigilo, konsistanta el cilindra seba, stearina a&ubreve; vaksa maso,
      &ccirc;irka&ubreve;anta me&ccirc;on:
      <ekz>vaks<tld/>o<fnt>Z</fnt>;</ekz>
      <ekz>seba <tld/>o<fnt>Z</fnt>;</ekz>
      <ekz>gardu <tld/>on por la nokto
        <klr>(&scirc;paru, &ccirc;ar eble vi bezonos pli poste)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        al &ccirc;iu sanktulo sian <tld/>on<fnt>Z</fnt>
        <klr>(respektu &ccirc;iujn respektindulojn)</klr>;
      </ekz>
      <ekz>
        fari rabon kaj &scirc;telon por oferi al Dio <tld/>on<fnt>Z</fnt>
        <klr>(esti nur &scirc;ajne kaj ekstere pia)</klr>;
      </ekz>
      <ekz><ind>la tuta ludo ne valoras <tld/>on</ind>
        <klr>(la rezultato ne valoras la penon)</klr>;
        <trd lng="ru">&c_i;&c_g;&c_r;&c_a; &c_n;&c_je; &c_s;&c_t;&c_o;&c_i;&c_t; &c_s;&c_v;&c_je;&c_ch;</trd><fnt><bib>PrV</bib></fnt></ekz>
      <ekz><ind>de malgranda <tld/>o forbrulis granda kastelo</ind>
        <klr>(malgranda ka&ubreve;zo, granda efiko)</klr>;
        <trd lng="ru">&c_o;&c_t; &c_k;&c_o;&c_p;&c_je;&c_je;&c_ch;&c_n;&c_o;&c_j; &c_s;&c_v;&c_je;&c_ch;&c_i; &c_M;&c_o;&c_s;&c_k;&c_v;&c_a; &c_s;&c_g;&c_o;&c_r;&c_je;&c_l;&c_a;</trd><fnt><bib>PrV</bib></fnt></ekz>
      <ekz>perdi la <tld/>on (la sa&gcirc;on) el la kapo<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
  </snc>
  <trd lng="cs">sv&iacute;ce</trd>
  <trd lng="en">candle</trd>
  <trd lng="es">vela</trd>
  <trd lng="de">Kerze</trd>
  <trd lng="fr">chandelle</trd>
  <trd lng="nl">kaars</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_ch;&c_a;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="kandel.0ingo">
  <kap><tld/>ingo</kap>
  <snc>
    <dif>
      Ilo por fiksi <tld/>ojn dum ilia brulado kaj lumigado.
    </dif>
  </snc>
  <trd lng="de">Kerzenhalter</trd>
  <trdgrp lng="en">
    <trd>candle stick</trd>,
    <trd>sconce</trd>
  </trdgrp>
  <trd lng="fr">chandelier</trd>
  <trd lng="es">candelero</trd>
  <trd lng="nl">kandelaar</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_v;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
