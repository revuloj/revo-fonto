<art mrk="$Id: hidrot.xml,v 1.24 2025/03/30 11:03:08 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>hidroterapi</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="hidrot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Medicina sistemo de <ref tip="super" cel="kurac.0o">kuracado</ref>
      per <ref cel="akv.0o">akvo</ref>:
      <ekz>
        la kuracmetodo de <nom>Kneipp</nom> bazi&gcirc;as sur kvin kolonoj: <tld/>o (kuraci per akvo),
        fitoterapio (kuraci per plantoj), movoterapio (korpa aktivado), diet<klr>[&#8230;]</klr>o
        (racia nutrado) kaj ordoterapio (ordo kaj ekvilibro en la vivo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16467">Sebastian Kneipp</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>akvokuracado</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_d;&c_a;&c_l;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;&c_e;&c_r;&c_a;&c_p;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">vodol&eacute;&ccaron;ba</trd>
  <trdgrp lng="de">
     <trd>Hydrotherapie</trd>,
     <trd>Wasserheilkunde</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&theta;&epsilon;&rho;&alfa;&pi;&epsilon;&jota_ton;&alfa;</trd>
  <trd lng="en">hydrotherapy</trd>
  <trd lng="es">hidroterapia</trd>
  <trd lng="fr">hydroth&eacute;rapie</trd>
  <trdgrp lng="hu">
    <trd>v&iacute;zgy&oacute;gy&aacute;szat</trd>,
    <trd>hidroter&aacute;pia</trd>
  </trdgrp>
  <trd lng="it">idroterapia</trd>
  <trd lng="ja">&#27700;&#27835;&#30274;&#27861;  <pr>&#12377;&#12356;&#12385;&#12426;&#12423;&#12358;&#12411;&#12358;</pr></trd>
  <trd lng="nl">hydrotherapie</trd>
  <trdgrp lng="pl">
    <trd>hydroterapia</trd>,
    <trd>wodolecznictwo</trd>
  </trdgrp>
  <trd lng="pt">hidroterapia</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">vodolie&ccaron;ba</trd>
  <trdgrp lng="uk">
    <trd>&#1074;&#1086;&#1076;&#1086;&#1083;&#1110;&#1082;&#1091;&#1074;&#1072;&#1085;&#1085;&#1103;</trd>,
    <trd>&#1075;&#1110;&#1076;&#1088;&#1086;&#1090;&#1077;&#1088;&#1072;&#1087;&#1110;&#1103;</trd>
  </trdgrp>
  <trd lng="zh">&#27700;&#30103; <pr>shu&#464;li&#225;o</pr></trd>
</drv>
</art>
