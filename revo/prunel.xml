<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prunel.xml,v 1.31 2024/02/17 07:52:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prunel</rad>/o
</kap>
<drv mrk="prunel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="prunel.0o.BOT">
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref> de <ref tip="malprt" cel="prunel.0arbo"><tld/>arbo</ref>:
      <ekz>
        preteriris sova&gcirc;a besto <klr>[&#x2026;]</klr> kaj dispremis per la
        piedoj
        la <tld/>on
        <fnt><bib>MT</bib> <lok>&Kr2; 25:18</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schlehenfrucht</trd>
  </snc>
  <snc mrk="prunel.0o.BOT_specio">
    <dif>
      <ref cel="prunel.0arbo.BOT"><tld lit="P"/>arbo</ref>:
      <ekz>
        la <tld/>oj malbonigas la kampon kaj kreskas &ccirc;ie
        <fnt>
          <aut>&Scirc;. Alej&hcirc;em, trad. I. Mu&ccirc;nik</aut>
          <vrk>Aforismoj</vrk>
          <lok>en: Hebreaj rakontoj, 1923</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d; &c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y; &c_k;&c_a;&c_l;&c_ju;&c_ch;&c_a;&c_j;)</klr></trd>
  <trdgrp lng="cs">
    <trd>slivo&ncaron; trnit&aacute;</trd>,
    <trd>trnka</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>blackthorn</trd>,
    <trd>sloe</trd>
  </trdgrp>
  <trd lng="fr">prunelle</trd>
  <trd lng="hu">k&ouml;k&eacute;ny</trd>
  <trd lng="ja">&#x30B9;&#x30D4;&#x30CE;&#x30B5;&#x30B9;&#x30E2;&#x30E2;&#x306E;&#x5B9F;  <pr>&#x30B9;&#x30D4;&#x30CE;&#x30B5;&#x30B9;&#x30E2;&#x30E2;&#x306E;&#x307F;</pr></trd>
  <trd lng="nl">sleedoorn</trd>
  <trd lng="pl">tarnina</trd>
  <trdgrp lng="pt">
    <trd>abrunho</trd>,
    <trd>ameixa</trd>,
    <trd>brunho</trd>
  </trdgrp>
  <trd lng="ro">porumbar</trd>
  <trd lng="ru">&c_t;&c_jo;&c_r;&c_n; <klr tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">trnka</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_n; (&c_p;&c_l;&c_ib;&c_d;)</trd>,
    <trd>&c_p;&c_r;&c_ju;&c_n;&c_je;&c_l;&c_mol; (&c_t;&c_k;&c_a;&c_n;&c_i;&c_n;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="prunel.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="prunel.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus spinosa</trd>) kun nigre bluaj
      &ccirc;erizgrandaj fruktoj:
      <ekz>
        la <tld/>arbo havis sur si ankora&ubreve; fruktojn, kiuj cetere estis
        sufi&ccirc;e akragustaj
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino,
        3a rakonto, la flor&gcirc;ardeno de la virino kiu povosciis
        sor&ccirc;i</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_jo;&c_r;&c_n; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a; &c_k;&c_a;&c_l;&c_ju;&c_ch;&c_a;&c_ja;</trd>,
    <trd>&c_zh;&c_a;&c_s;&c_mol;&c_c;&c_jo;&c_r;</trd>,
    <trd>&c_c;&c_ja;&c_r;&c_n;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>slivo&ncaron; trnit&aacute;</trd>,
    <trd>trnka</trd>
  </trdgrp>
  <trd lng="de">Schlehe</trd>
  <trdgrp lng="de">
    <trd>Schlehdorn</trd>,
    <trd>Schlehendorn</trd>,
    <trd>Schlehe</trd>,
    <trd>Heckendorn</trd>,
    <trd>Sauerpflaume</trd>,
    <trd>Schwarzdorn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>blackthorn</trd>,
    <trd>sloe</trd>
  </trdgrp>
  <trd lng="fr">prunellier</trd>
  <trd lng="hu">k&ouml;k&eacute;nybokor</trd>
  <trd lng="ja">&#x30B9;&#x30D4;&#x30CE;&#x30B5;&#x30B9;&#x30E2;&#x30E2;</trd>
  <trd lng="nl">sleedoorn</trd>
  <trd lng="pl">&sacute;liwa tarnina</trd>
  <trdgrp lng="pt">
    <trd>abrunheiro</trd>,
    <trd>abrunheiro-bravo</trd>,
    <trd>abrunho</trd>,
    <trd>ac&aacute;cia-dos-alem&atilde;es</trd>,
    <trd>ameixeira-brava</trd>
  </trdgrp>
  <trd lng="ro">porumbel</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_r;&c_n; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">trnka <klr>(kr&iacute;k)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_n;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x674E;&#x6728; <pr>l&#x1D0;m&#xF9;</pr></trd>
</drv>
</art>
