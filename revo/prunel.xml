<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prunel</rad>/o
</kap>
<uzo tip="fak">BOT</uzo>
<drv mrk="prunel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="prunel.0ujo0arbo"><tld/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="nl">sleedoorn</trd>
  <trd lng="ru">&c_t;&c_jo;&c_r;&c_n; <klr tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
</drv>

<drv mrk="prunel.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus spinosa</trd>) kun nigre bluaj
      &ccirc;erizgrandaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">Schlehe</trd>
  <trd lng="fr">prunellier</trd>
  <trd lng="nl">sleedoorn</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_r;&c_n; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_t;&c_je;&c_r;&c_n;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
