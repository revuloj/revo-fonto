<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kasav.xml,v 1.9 2023/12/03 08:00:24 revo Exp $">
<kap>
  <rad>kasav</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kasav.0o">
  <kap><tld/>o</kap>
  <snc mrk="kasav.0o.KUI_faruno">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Maniok<ref tip="super" cel="farun.0o.KUI">faruno</ref>:
      <ekz>
        estas primitiva <tld/>o-farejo <klr>...</klr> ekster la vila&gcirc;o
        <fnt>
          <aut>R. da Silva Ribeiro</aut>,
          <vrk>Voja&gcirc;taglibro al Amazonio</vrk>,
          <lok><url ref="http://www.lakaravelo.com/LK_000/LK_018.pdf">
          La Karavelo, 2009-09 (18)</url>, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kasav.0o.KUI_pano">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Patkuko el maniokfaruno.
      <ekz>
        en la unuaj dudek kvar horoj Aureliano Segundo konsumis unu 
        bovidon kun <tld/>o, ignamo kaj kuiritaj bananoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;&c_v;&c_a; <klr>(&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_m;&c_a;&c_n;&c_i;&c_o;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kasava</trd>,
    <trd>kasave</trd>,
    <trd>maniok</trd>,
    <trd>manioka</trd>,
    <trd>maniokov&#xE1; mouka</trd>
  </trdgrp>
  <trd lng="de">Maniok</trd>
  <trd lng="en">cassava</trd>
  <trd lng="fr">cassave</trd>
  <trd lng="nl">cassave</trd>
  <trdgrp lng="pl">
    <trd>kassawa</trd>,
    <trd>tapioka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kasava</trd>,
    <trd>maniok</trd>,
    <trd>maniokov&#xE1; m&#xFA;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_s;&c_a;&c_v;&c_a;</trd>,
    <trd>&#x43C;&c_a;&#x43D;&c_ib;&#x43E;&#x43A;&#x43E;&c_v;&c_je; &c_b;&#x43E;&c_r;&#x43E;&c_sh;&#x43D;&#x43E;</trd>,
    <trd>&c_h;&#x43B;&c_ib;&c_b; &c_z; &#x43C;&c_a;&#x43D;&c_ib;&#x43E;&#x43A;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x85AF; <pr>m&#xF9;sh&#x1D4;</pr></trd>,
    <trd>&#x6728;&#x85AF;&#x6DC0;&#x7C89; <pr>m&#xF9;sh&#x1D4;di&#xE0;nf&#x11B;n</pr></trd>
  </trdgrp>
</drv>
</art>

