<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>solanac</rad>/oj
</kap>
<drv mrk="solana.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      unupetalaj plantoj (<trd lng="la">Solanaceae</trd>), al kiu
      apartenas multaj kuracaj, man&gcirc;eblaj kaj venenaj plantoj,
      ekzemple
      <refgrp tip="sub">
        <ref cel="solan.0o">solano</ref>,
        <ref cel="tomat.0o">tomato</ref>,
        <ref cel="kapsik.0o">kapsiko</ref>,
        <ref cel="nikoti.0o">nikotiano</ref>,
        <ref cel="belado.0o">beladono</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd
  lng="be">&c_p;&c_a;&c_s;&c_mol;&c_l;&c_jo;&c_n;&c_a;&c_v;&c_y;&c_ja;</trd>
  <trd lng="ca">solan&agrave;cies</trd>
  <trd lng="de">Nachtschattengew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>burgonyaf&eacute;l&eacute;k</trd>,
    <trd>csucsorf&eacute;l&eacute;k</trd>,
    <trd>ebsz&odblac;l&odblac;f&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">nachtschadenfamilie</trd>
  <trd lng="pl">psiankowate</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_l;&c_jo;&c_n;&c_o;&c_v;&c_y;&c_je;
  <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
