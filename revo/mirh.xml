<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mirh</rad>/o
</kap>
<drv mrk="mirh.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bonodora rezino el kelkaj arabaj kaj afrikaj arboj uzata en
      fabrikado de parfumoj:
      <ekz>
        mi parfumis mian ku&scirc;ejon per <tld/>o, aloo kaj cinamo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="incens.0o">incenso</ref>,
      <ref cel="balzam">balzamo</ref>,
      <ref cel="hisop.0o">hisopo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">mirre</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_i;&c_r;&c_r;&c_o;</trd>
  </trdgrp>
</drv>
</art>
