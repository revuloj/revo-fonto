<art mrk="$Id: mirh.xml,v 1.25 2025/09/27 12:52:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mirh</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="mirh.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bonodora <ref tip="super" cel="rezin.0o">rezino</ref> el
      kelkaj arabaj kaj afrikaj arboj uzata en
      fabrikado de parfumoj:
      <ekz>
        mi parfumis mian ku&scirc;ejon per <tld/>o, aloo kaj
        cinamo
        <fnt><bib>MT</bib>, <lok>Sentencoj 7:17</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="incens.0o">incenso</ref>,
      <ref cel="balzam">balzamo</ref>,
      <ref cel="hisop.0o">hisopo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_a;</trd>
  <trd lng="br">mir<klr tip="ind"> (gom-rousin)</klr></trd>
  <trd lng="ca">mirra</trd>
  <trd lng="cs">myrha</trd>
  <trd lng="de">Myrrhe</trd>
  <trd lng="en">myrrh</trd>
  <trd lng="es">mirra</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_damma;&a_r;</trd>,
    <trd>&a_m;&a_damma;&a_r; &a_m;&f_k;&f_y;</trd>,
    <trd>&a_S;&a_m;&a_gh; &a_s;&a_q;&a_z;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">myrrhe</trd>
  <trd lng="hu">mirha</trd>
  <trd lng="id">mur<klr tip="amb"> (resin)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30EB;&#x30E9;</trd>,
    <trd>&#x6CA1;&#x85AC;  <pr>&#12418;&#12388;&#12420;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">mirre</trd>
  <trd lng="pl">mirra</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_i;&c_r;&c_r;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">myrha <klr>(&zcaron;ivica)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_i;&c_r;&c_r;&c_a;</trd>,
    <trd>&c_z;&c_a;&#x43F;&c_a;&c_sh;&#x43D;&c_a; &c_s;&#x43C;&#x43E;&#x43B;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C92;&#x85E5; <pr>m&#xE9;iy&#xE0;o</pr></trd>,
    <trd>&#x6CA1;&#x836F; <pr>m&#xE9;iy&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>
