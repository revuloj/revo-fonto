<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trinidad.xml,v 1.18 2024/09/29 21:26:45 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Trinidad</rad>/o
</kap>

<drv mrk="trinidad.T0o">
  <kap>
    <ofc>10</ofc> <tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc mrk="trinidad.T0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      en <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>
      &ccirc;e nordorienta bordo de <ref tip="vid" cel="amerik.Sud0o">Sudameriko</ref>,
      apartentanta al la teritorio de <ref tip="malprt" cel="trinidad.T0otobago.sxtato">Trinidado
      kaj Tobago</ref>:
      <ekz>
        ne malproksime de la Venezuela marbordo trovi&gcirc;as anka&ubreve; la Nederlandaj Antiloj:
        Kuracao, Bonero kaj Arubo (Nederlando) kaj <tld/>o kaj Tobago
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4453">Venezuelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tobag.T0o.GEOG">Tobago</ref>
    </snc>
    <trd lng="be">&c_T;&c_r;&c_y;&c_n;&c_ib;&c_d;&c_a;&c_d;</trd>
    <trd lng="bg">&c_T;&c_r;&c_i;&c_n;&c_i;&c_d;&c_a;&c_d;</trd>
    <trd lng="ca">Trinitat</trd>
    <trd lng="de">Trinidad<klr tip="amb"> (Insel)</klr></trd>
    <trd lng="en">Trinidad</trd>
    <trd lng="id">Trinidad</trd>
    <trd lng="ja">&#x30C8;&#x30EA;&#x30CB;&#x30C0;&#x30FC;&#x30C9;&#x5CF6; 
    <pr>&#x30C8;&#x30EA;&#x30CB;&#x30C0;&#x30FC;&#x30C9;&#12375;&#x307E;</pr></trd>
  <trdgrp lng="pl">
      <trd>Trynidad</trd>
    </trdgrp>
    <trd lng="fr">Trinit&eacute;</trd>
    <trd lng="pt">Trinidad e Tobago</trd>
  <trdgrp lng="zh">
    <trd>&#29305;&#x7ACB;&#x5C3C;&#x8FBE; <pr>T&#xE8;l&#xEC;n&#xED;d&#xE1;</pr></trd>,
    <trd>&#29305;&#x7ACB;&#x5C3C;&#36948; <pr>T&#xE8;l&#xEC;n&#xED;d&#xE1;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="trinidad.T0otobago">
  <kap>
    <ofc>10</ofc> <tld/>o kaj Tobago
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg>TT</mlg>
  <snc mrk="trinidad.T0otobago.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      en la insuloj <ref tip="prt" cel="trinidad.T0o.GEOG">Trinidado</ref>,
      <ref tip="prt" cel="tobag.T0o.GEOG">Tobago</ref> kaj kelkaj aliaj
      kun &ccirc;efurbo Portospeno.
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_r;&c_y;&c_n;&c_ib;&c_d;&c_a;&c_d; &c_ib; &c_T;&c_a;&c_b;&c_a;&c_g;&c_a;</trd>
  <trd lng="bg">&c_T;&c_r;&c_i;&c_n;&c_i;&c_d;&c_a;&c_d; &c_i; &c_T;&c_o;&c_b;&c_a;&c_g;&c_o;</trd>
  <trdgrp lng="ca">
    <trd>Trinitat i Tobago</trd>,
    <trd>Rep&uacute;blica de Trinitat i Tobago</trd>
  </trdgrp>
  <trd lng="de">Trinidad und Tobago</trd>
  <trdgrp lng="en">
    <trd>Trinidad and Tobago</trd>,
    <trd>Republic of <ind>Trinidad and Tobago</ind></trd>
  </trdgrp>
  <trd lng="fr">Trinit&eacute; et Tobago</trd>
  <trd lng="id">Trinidad dan Tobago</trd>
  <trd lng="ja">&#x30C8;&#x30EA;&#x30CB;&#x30C0;&#x30FC;&#x30C9;&#x30FB;&#x30C8;&#x30D0;&#x30B4; 
  <pr>&#12392;&#x308A;&#x306B;&#12384;&#x30FC;&#12393;&#x30FB;&#12392;&#12400;&#12372;</pr></trd>
  <trdgrp lng="pl">
    <trd>Trynidad i Tobago</trd>
  </trdgrp>
  <trd lng="uk">&c_T;&c_r;&c_i;&#x43D;&c_ib;&c_d;&c_a;&c_d; &c_ib; &c_T;&#x43E;&c_b;&c_a;&c_gu;&#x43E;</trd>
  <trdgrp lng="zh">
    <trd>&#21315;&#x91CC;&#x8FBE;&#x548C;&#x591A;&#x5DF4;&#x54E5;
    <pr>Qi&amacron;nl&#x1D0;d&#xE1;h&#xE9;Du&#x14D;b&amacron;g&emacron;</pr></trd>,
    <trd>&#21315;&#x91CC;&#36948;&#x548C;&#x591A;&#x5DF4;&#x54E5;
    <pr>Qi&amacron;nl&#x1D0;d&#xE1;h&#xE9;Du&#x14D;b&amacron;g&emacron;</pr></trd>,
    <trd>&#29305;&#x7ACB;&#x5C3C;&#x8FBE;&#x548C;&#x591A;&#x5DF4;&#x54E5;
    <pr>T&#xE8;l&#xEC;n&#xED;d&#xE1;h&#xE9;Du&#x14D;b&amacron;g&emacron;</pr></trd>,
    <trd>&#29305;&#x7ACB;&#x5C3C;&#36948;&#x548C;&#x591A;&#x5DF4;&#x54E5;
    <pr>T&#xE8;l&#xEC;n&#xED;d&#xE1;h&#xE9;Du&#x14D;b&amacron;g&emacron;</pr></trd>,
    <trd>&#21315;&#x91CC;&#x8FBE;&#x53CA;&#25176;&#x5DF4;&#x54E5;
    <pr>Qi&amacron;nl&#x1D0;d&#xE1;j&#xED;Tu&#x14D;b&amacron;g&emacron;</pr></trd>,
    <trd>&#21315;&#x91CC;&#36948;&#x53CA;&#25176;&#x5DF4;&#x54E5; <pr>Qi&amacron;nl&#x1D0;d&#xE1;j&#xED;Tu&#x14D;b&amacron;g&emacron;</pr></trd>
  </trdgrp>
</drv>

</art>

