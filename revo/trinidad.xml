<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trinidad</rad>/o
</kap>

<drv mrk="trinidad.0o">
  <kap>
    <tld lit="T"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="trinidad.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insulo en la <ref tip="vid" cel="atlant.0o">Atlantiko</ref>
      &ccirc;e nordorienta bordo de <ref tip="vid"
      cel="amerik.Sud0o">Sudameriko</ref>.
      Teritorio de <ref tip="malprt"
      cel="trinidad.0otobago.sxtato">Trinidado kaj Tobago</ref>.
    </dif>
    <ref tip="vid" cel="tobag.0o.GEOG">Tobago</ref>
    <trdgrp lng="pl">
      <trd>Trynidad</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="trinidad.0otobago">
  <kap>
    <tld lit="T"/>o kaj Tobago
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="trinidad.0otobago.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la insuloj <ref tip="prt"
      cel="trinidad.0o.GEOG">Trinidado</ref>,
      <ref tip="prt" cel="tobag.0o.GEOG">Tobago</ref> kaj kelkaj aliaj
      kun &ccirc;efurbo Portospeno.
      ISO-3166 kodo: TT.
    </dif>
    <trdgrp lng="pl">
      <trd>Trynidad i Tobago</trd>
    </trdgrp>
  </snc>
</drv>

</art>

