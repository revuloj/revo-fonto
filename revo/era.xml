<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>era</rad>/o
</kap>
<drv mrk="era.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Difinita epoko, de kiam oni komencas la kalkuladon de la jaroj;
      tempokalkulo, jarkalkulo:
      <ekz>
        <tld/>o de la Olimpiadoj, de la fondo de Romo;
      </ekz>
      <ekz>
        kristana <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de l' He&gcirc;iro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>&Auml;ra</trd>,
    <trd>Zeitrechnung</trd>,
    <trd>Zeitalter</trd></trdgrp>
  <trd lng="fr">&egrave;re</trd>
  <trdgrp lng="nl"><trd>era</trd>,
     <trd>tijdrekening</trd></trdgrp>
  <trd lng="ru">&c_e;&c_r;&c_a;</trd>
</drv>
</art>

