<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>spektakl</rad>/o
</kap>
<drv mrk="spekta.0o">
  <kap><tld/>o</kap>
  <snc mrk="spekta.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <ref tip="dif" cel="spekt.0ajxo">spekta&jcirc;o</ref>
    <trdgrp lng="de">
      <trd>Auff&uuml;hrung</trd>,
      <trd>Vorf&uuml;hrung</trd>
    </trdgrp>
  </snc>
  <snc mrk="spekta.0o.evento">
    <dif>
      &Ccirc;ia vida&jcirc;o, kiu altiras atenton kaj intereson.
    </dif>
    <trdgrp lng="de">
      <trd>Spektakel</trd>,
      <trd>&ouml;ffentliches Ereignis</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pod&iacute;van&aacute;</trd>
  <trdgrp lng="fr">
    <trd>spectacle</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schouwspel</trd>,
    <trd>spektakel</trd>
  </trdgrp>
</drv>
</art>
