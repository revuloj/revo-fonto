</kap>
<drv mrk="kares.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kares.0i.tusxi">
    <dif>
      Delikate kaj ame <ref tip="super" cel="tusx.0eti">tu&scirc;eti</ref>:
      <ekz>
        &scirc;ia patro  per mola mano <tld/>as &scirc;iajn longajn harojn
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la suno <tld/>as la teron per somere varmaj kaj tamen printempe mildaj fingroj
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la maro kun sia eterna mu&gcirc;o <tld/>is la bordon kaj sur la rokoj, dividi&gcirc;inte en
        mil perletojn, refalis, kiel fea vualo
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        selante &ccirc;evalon, oni &gcirc;in <tld/>as
        <klr>(oni devas flati tiun, de kiu oni postulas ion al li
        malagrablan)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i al iu la barbon</ind>
        <klr>(flategi; <ref tip="vid" cel="lek.0i">leki</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="ca">
          <trd>fer la barbeta</trd>,
          <trd>afalagar</trd>
        </trdgrp>
        <trd lng="fr">flatter</trd>
        <trd lng="es">halagar</trd>
        <trd lng="en">flatter</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i kontra&ubreve; la haroj</ind>
        <klr>(malflati, malpla&ccirc;i)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="ca">
          <trd>venir a rep&egrave;l</trd>,
          <trd>actuar a contracor</trd>
        </trdgrp>
        <trd lng="fr">prendre &agrave; <ind>rebrousse-poil</ind></trd>
        <trd lng="es">acariciar a <ind>contrapelo</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>liebkosen</trd>,
      <trd>herzen</trd>,
      <trd>streicheln</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>liefkozen</trd>,
      <trd>aaien</trd>
    </trdgrp>
  </snc>
  <snc mrk="kares.0i.felicxigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="plezur.0igi">Plezurigi</ref>, <ref cel="felicx.0igi">feli&ccirc;igi</ref>,
      <ref tip="vid" cel="dorlot.0i">dorloti</ref>:
      <ekz>
        viaj konsoloj <tld/>as mian animon
        <fnt><bib>MT</bib> <lok>&Psa; 94:19</lok></fnt>;
      </ekz>
      <ekz>
        brila lumo <klr>[&#8230;]</klr> <tld/>as iliajn okulojn
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        belkoloraj tapi&scirc;oj <klr>[&#8230;]</klr> <tld/>is la rigardon per ru&gcirc;o de rozoj kaj
        verdo de herbo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiun la sorto <tld/>as, al tiu &ccirc;io sukcesas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schmeicheln</trd>,
      <trd><ind>gef&auml;llig</ind> sein</trd>
    </trdgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>choura&ntilde;<klr tip="ind"> (floura&ntilde;)</klr></trd>,
    <trd>floura&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acariciar</trd>,
    <trd>amanyagar</trd>,
    <trd>acaronar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laskat se <klr>(s n&ecaron;k&yacute;m)</klr></trd>,
    <trd>mazlit se</trd>
  </trdgrp>
  <trd lng="en">caress</trd>
  <trd lng="es">acariciar</trd>
  <trdgrp lng="fr">
    <trd>caresser</trd>,
    <trd>cajoler</trd>,
    <trd>c&acirc;liner</trd>,
    <trd>dorloter</trd>,
    <trd>effleurer</trd>,
    <trd>enj&ocirc;ler</trd>
    <trd>nourrir<klr tip="ind"> (un r&ecirc;ve, un espoir)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat</trd>,
    <trd>cir&oacute;gat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24859;&#25771;&#12377;&#12427;  <pr>&#12354;&#12356;&#12406;&#12377;&#12427;</pr></trd>,
    <trd>&#12420;&#12373;&#12375;&#12367;&#12394;&#12391;&#12427;</trd>,
    <trd>&#12381;&#12387;&#12392;&#35302;&#12428;&#12427;  <pr>&#12381;&#12387;&#12392;&#12405;&#12428;&#12427;</pr></trd>,
    <trd>&#24944;&#12417;&#12427;  <pr>&#12394;&#12368;&#12373;&#12417;&#12427;</pr></trd>,
    <trd>&#21916;&#12400;&#12379;&#12427;  <pr>&#12424;&#12429;&#12371;&#12400;&#12379;&#12427;</pr></trd>,
    <trd>&#22909;&#24847;&#12434;&#31034;&#12377;  <pr>&#12371;&#12358;&#12356;&#12434;&#12375;&#12417;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cacka&cacute;</trd>,
    <trd>g&lstroke;aska&cacute;</trd>,
    <trd>pie&sacute;ci&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>m&acirc;ng&acirc;ia</trd>,
    <trd>dezmierdare</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hladka&tcaron; sa <klr>(s niek&yacute;m)</klr></trd>,
    <trd>l&aacute;ska&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25242;&#25720; <pr>f&#468;m&omacron;</pr></trd>,
    <trd>&#25705;&#25394; <pr>m&oacute;su&omacron;</pr></trd>,
    <trd>&#25242;&#25705; <pr>f&#468;m&oacute;</pr></trd>,
    <trd>&#21561;&#25282; <pr>chu&imacron;f&uacute;</pr></trd>,
    <trd>&#25282; <pr>f&uacute;</pr></trd>,
    <trd>&#25614; <pr>s&umacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kares.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        &scirc;i deziris, ke almena&ubreve; dum la <tld/>oj li ne agu kun &scirc;i kiel sinjoro kaj estro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mirtaj arbetaroj ka&scirc;os niajn <tld/>ojn, kaj dioj, pli favoraj por la amantoj
        gardos nian feli&ccirc;on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sola infano de tiuj gepatroj kreskis meze <klr>[&#8230;]</klr> de <tld/>oj de
        bonstato
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ccirc;eti la <tld/>ojn de publikulino
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistas <tld/>o sen intereso
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu ursan <klr>(mallertan, vundan)</klr> <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>allazig</trd>,
    <trd>chouradenn</trd>,
    <trd>flouradenn</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>car&iacute;cia</trd>,
    <trd>manyaga</trd>,
    <trd>moixaina</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lask&aacute;n&iacute;</trd>,
    <trd>mazlen&iacute;</trd>,
    <trd>pohlazen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Streicheln</trd>,
    <trd>Liebkosung</trd>
  </trdgrp>
  <trd lng="es">caricia</trd>
  <trdgrp lng="fr">
    <trd>caresse</trd>,
    <trd>cajolerie</trd>,
    <trd>c&acirc;lin<klr tip="ind"> (subst.)</klr></trd>,
    <trd>effleurement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat&aacute;s</trd>,
    <trd>cir&oacute;gat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24859;&#25771;  <pr>&#12354;&#12356;&#12406;</pr></trd>,
    <trd>&#24944;&#12417;  <pr>&#12394;&#12368;&#12373;&#12417;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozing</trd>,
    <trd>aai</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cacanka</trd>,
    <trd>pieszczota</trd>
  </trdgrp>
  <trd lng="ro">dezmierdare</trd>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">maznanie</trd>
  <trd lng="uk">&c_l;&c_a;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="kares.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>anta:
      <ekz>
        li allogis min per <klr>[&#8230;]</klr> <tld/>aj vortoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sentis la <tld/>an tu&scirc;eton de la vento
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ricevas <tld/>ajn rigardojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kaj submetita vo&ccirc;tono
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Brita</nom> <klr>[&#8230;]</klr> rigardis lin kun <tld/>a kompato en la okuloj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dolcx.0a.milda">dol&ccirc;a<sncref/></ref>,
      <ref cel="mild.0a">milda</ref>,
      <ref cel="tener.0a">tenera</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>chourus</trd>,
    <trd>flourus</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acariciador</trd>,
    <trd>amanyagador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laskav&yacute;</trd>,
    <trd>vl&iacute;dn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cari&ntilde;oso</trd>,
    <trd>acariciador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caressant</trd>,
    <trd>cajoleur<klr tip="ind"> (adj.)</klr></trd>,
    <trd>c&acirc;lin<klr tip="ind"> (adj.)</klr></trd>,
    <trd>enjoleur<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat&oacute;</trd>,
    <trd>cir&oacute;gat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24859;&#25771;&#12377;&#12427;&#12424;&#12358;&#12394;  <pr>&#12354;&#12356;&#12406;&#12377;&#12427;&#12424;&#12358;&#12394;</pr></trd>,
    <trd>&#20778;&#12375;&#12356;  <pr>&#12420;&#12373;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozend</trd>,
    <trd>aaiend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czu&lstroke;y</trd>,
    <trd>pieszczotliwy</trd>
  </trdgrp>
  <trd lng="ro">afectuos</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&aacute;skav&yacute;</trd>,
    <trd>v&lcaron;&uacute;dny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#24859;&#25771; <pr>&agrave;if&#468;</pr></trd>
</drv>

<drv mrk="kares.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Malmilde trakti:
      <ekz>
        &leftquot;lia <tld/>a nomo estas <nom>Tomek</nom>!&rightquot;, demandas la hispano: &leftquot;kaj la mal<tld/>a?&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Werner KLAG</aut>
          <vrk>Karesado</vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni batas, mal<tld/>as, kaj e&ccirc; plori ne permesas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_a; &c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">gwallgas</trd>
  <trdgrp lng="ca">
    <trd>maltractar</trd>,
    <trd>tractar rudament</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsn&ecaron; zach&aacute;zet</trd>,
    <trd>nevl&iacute;dn&ecaron; zach&aacute;zet</trd>
  </trdgrp>
  <trd lng="de">hart <ind>angehen</ind></trd>
  <trd lng="es">maltratar</trd>
  <trdgrp lng="fr">
    <trd>rudoyer</trd>,
    <trd>brutaliser</trd>,
    <trd>maltraiter</trd>,
    <trd>molester</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helybenhagy</trd>,
    <trd>elt&aacute;ng&aacute;l</trd>,
    <trd>megruh&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25163;&#33618;&#12367;&#25201;&#12358;  <pr>&#12390;&#12354;&#12425;&#12367;&#12354;&#12388;&#12363;&#12358;</pr></trd>,
    <trd>&#37034;&#38522;&#12395;&#25201;&#12358;  <pr>&#12376;&#12419;&#12369;&#12435;&#12395;&#12354;&#12388;&#12363;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">ruw behandelen</trd>
  <trdgrp lng="pl">
    <trd>n&eogonek;ka&cacute;</trd>,
    <trd>brutalizowa&cacute;</trd>
    <trd>maltretowa&cacute;</trd>,
    <trd>molestowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>abuz</trd>,
    <trd>h&abreve;r&tcomma;uire</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o; &c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_y;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">drsne zaobch&aacute;dza&tcaron;</trd>
  <trd lng="uk">&c_g;&c_r;&c_u;&c_b;&c_o; &c_p;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja; (&c_z; &c_k;&c_i;&c_m;-&c_n;.)</trd>
  <trdgrp lng="zh">
    <trd>&#27450;&#36000; <pr>q&imacron;fu</pr></trd>,
    <trd>&#34384;&#24453; <pr>nu:&egrave;d&agrave;i</pr></trd>
