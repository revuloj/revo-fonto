    <trd>amanyagador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laskav&yacute;</trd>,
    <trd>vl&iacute;dn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
