<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kares</rad>/i
</kap>
<drv mrk="kares.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Delikate kaj ame tu&scirc;eti:
      <ekz>
        selante &ccirc;evalon, oni &gcirc;in <tld/>as<fnt>Z</fnt>
        <klr>(oni devas flati tiun, de kiu oni postulas ion al li
        malagrablan)</klr>;
      </ekz>
      <ekz>
        <tld/>i siajn lipharojn<fnt>Z</fnt>;
      </ekz>
      (f)
      <ekz>
        <tld/>i al iu la barbon<fnt>Z</fnt>
        <klr>(flategi. <ref tip="sin" cel="lek.0i">leki</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>i kontra&ubreve; la haroj<fnt>Z</fnt> <klr>(malflati,
        malpla&ccirc;i)</klr>;
      </ekz>
      <ekz>
        <klr>(f)</klr> <tld/>i iluzion <klr>(dorloti 4)</klr>;
      </ekz>
      <ekz>
        <klr>(f)</klr> la lumo <tld/>as la murojn;
      </ekz>
      <ekz>
        la ondoj <tld/>is la dezertan bordon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>liebkosen</trd>,
      <trd>herzen</trd>
    </trdgrp>
    <trdgrp lng="nl"><trd>liefkozen</trd>.
       <trd>aaien</trd></trdgrp>
  </snc>
  <snc>
    <dif>
      Plezurigi, feli&ccirc;igi:
      <ekz>
        tiu koloro <tld/>as la rigardon, la okulojn;
      </ekz>
      <ekz>
        min mono neniam <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viaj konsoloj <tld/>as mian vunditan animon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schmeicheln</trd>,
      <trd>gef&auml;llig sein</trd>
    </trdgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <snc>
    <dif>
      Favori, protekti:
      <ekz>
        kiun la sorto <tld/>as, al tiu &ccirc;io sukcesas<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dorlot.0i">dorloti</ref>.
    </refgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <trd lng="en">caress</trd>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kares.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        ne ekzistas <tld/>o sen intereso<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ursa (mallerta) <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="nl"><trd>liefkozing</trd>,
    <trd>aai</trd></trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="kares.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>anta:
      <ekz>
        <tld/>a tu&scirc;o, kiso;
      </ekz>
      <ekz>
        <tld/>aj vortoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="nl"><trd>liefkozend</trd>,
    <trd>aaiend</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kares.mal0i">
  <kap>
    mal<tld/>i<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      Malmilde trakti.
    </dif>
  </snc>
  <trd lng="nl">ruw behandelen</trd>
</drv>
</art>
