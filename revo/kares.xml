    <trd>&#x512A;&#x3057;&#x3044;  <pr>&#x3084;&#x3055;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozend</trd>,
    <trd>aaiend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czu&lstroke;y</trd>,
    <trd>pieszczotliwy</trd>
  </trdgrp>
  <trd lng="ro">afectuos</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&aacute;skav&yacute;</trd>,
    <trd>v&lcaron;&uacute;dny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_a;&c_s;&#x43A;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_s;&c_t;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x611B;&#x64AB; <pr>&#xE0;if&#x1D4;</pr></trd>
</drv>

<drv mrk="kares.mal0i">
  <kap>mal<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malmilde trakti.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_a; &c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">gwallgas</trd>
  <trdgrp lng="ca">
    <trd>maltractar</trd>,
    <trd>tractar rudament</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsn&ecaron; zach&aacute;zet</trd>,
    <trd>nevl&iacute;dn&ecaron; zach&aacute;zet</trd>
  </trdgrp>
  <trd lng="es">maltratar</trd>
  <trdgrp lng="fr">
    <trd>rudoyer</trd>,
    <trd>brutaliser</trd>,
    <trd>maltraiter</trd>,
    <trd>molester</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helybenhagy</trd>,
    <trd>elt&aacute;ng&aacute;l</trd>,
    <trd>megruh&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x624B;&#x8352;&#x304F;&#x6271;&#x3046;  <pr>&#x3066;&#x3042;&#x3089;&#x304F;&#x3042;&#x3064;&#x304B;&#x3046;</pr></trd>,
    <trd>&#x90AA;&#x967A;&#x306B;&#x6271;&#x3046;  <pr>&#x3058;&#x3083;&#x3051;&#x3093;&#x306B;&#x3042;&#x3064;&#x304B;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">ruw behandelen</trd>
  <trdgrp lng="pl">
    <trd>n&eogonek;ka&cacute;</trd>,
    <trd>brutalizowa&cacute;</trd>
    <trd>maltretowa&cacute;</trd>,
    <trd>molestowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>abuz</trd>,
    <trd>h&abreve;r&#x21B;uire</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o; &c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_y;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">drsne zaobch&aacute;dza&tcaron;</trd>
  <trd lng="uk">&c_g;&c_r;&c_u;&c_b;&#x43E; &#x43F;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i;&c_s;&#x44F; (&c_z; &#x43A;&c_i;&#x43C;-&#x43D;.)</trd>
  <trdgrp lng="zh">
    <trd>&#x6B3A;&#x8CA0; <pr>q&#x12B;fu</pr></trd>,
    <trd>&#x8650;&#x5F85; <pr>nu:&#xE8;d&#xE0;i</pr></trd>
  </trdgrp>
</drv>
