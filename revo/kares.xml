<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kares</rad>/i
</kap>
<drv mrk="kares.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Delikate kaj ame tu&scirc;eti:
      <ekz>
        selante &ccirc;evalon, oni &gcirc;in <tld/>as
        <klr>(oni devas flati tiun, de kiu oni postulas ion al li
        malagrablan)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i siajn lipharojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i al iu la barbon</ind>
        <klr>(flategi; <ref tip="vid" cel="lek.0i">leki</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">flatter</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i kontra&ubreve; la haroj</ind> 
        <klr>(malflati, malpla&ccirc;i)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">prendre &agrave; <ind>rebrousse-poil</ind></trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo> 
        <tld/>i iluzion <klr>(dorloti)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo> 
        la lumo <tld/>as la murojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ondoj <tld/>is la dezertan bordon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>liebkosen</trd>,
      <trd>herzen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>liefkozen</trd>,
      <trd>aaien</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Plezurigi, feli&ccirc;igi:
      <ekz>
        tiu koloro <tld/>as la rigardon, la okulojn;
      </ekz>
      <ekz>
        min mono neniam <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viaj konsoloj <tld/>as mian vunditan animon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schmeicheln</trd>,
      <trd>gef&auml;llig sein</trd>
    </trdgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <snc>
    <dif>
      Favori, protekti:
      <ekz>
        kiun la sorto <tld/>as, al tiu &ccirc;io sukcesas
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dorlot.0i">dorloti</ref>.
    </refgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">caress</trd>
  <trdgrp lng="fr">
    <trd>caresser</trd>,
    <trd>cajoler</trd>,
    <trd>c&acirc;liner</trd>,
    <trd>dorloter</trd>,
    <trd>effleurer</trd>,
    <trd>enj&ocirc;ler</trd>
    <trd>nourrir<klr tip="ind"> (un r&ecirc;ve, un espoir)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat</trd>,
    <trd>cir&oacute;gat</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kares.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        ne ekzistas <tld/>o sen intereso
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ursa (mallerta) <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caresse</trd>,
    <trd>cajolerie</trd>,
    <trd>c&acirc;lin<klr tip="ind"> (subst.)</klr></trd>,
    <trd>effleurement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat&aacute;s</trd>,
    <trd>cir&oacute;gat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozing</trd>,
    <trd>aai</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="kares.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>anta:
      <ekz>
        <tld/>a tu&scirc;o, kiso;
      </ekz>
      <ekz>
        <tld/>aj vortoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat&oacute;</trd>,
    <trd>cir&oacute;gat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caressant</trd>,
    <trd>cajoleur<klr tip="ind"> (adj.)</klr></trd>,
    <trd>c&acirc;lin<klr tip="ind"> (adj.)</klr></trd>,
    <trd>enjoleur<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozend</trd>,
    <trd>aaiend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kares.mal0i">
  <kap>mal<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malmilde trakti.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>rudoyer</trd>,
    <trd>brutaliser</trd>,
    <trd>maltraiter</trd>,
    <trd>molester</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helybenhagy</trd>,
    <trd>elt&aacute;ng&aacute;l</trd>,
    <trd>megruh&aacute;z</trd>
  </trdgrp>
  <trd lng="nl">ruw behandelen</trd>
</drv>
</art>
