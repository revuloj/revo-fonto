<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sen1.xml,v 1.9 2023/08/05 17:26:39 revo Exp $">
<kap>
  <rad>sen</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sen1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sen1.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Eksa <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">mona
      subunuo</ref> en
      japanujo egala al centono de <ref tip="malprt" cel="en1.0o.EKON">eno</ref>:
      <ekz>
        &leftquot;nur du <tld/>ojn? Donu tri <tld/>ojn, se ne, mi ne...&rightquot;
        <fnt><aut>Ueyama M.</aut>, <vrk>Pardonon!</vrk>, <lok>l'
        omnibuso, 1970</lok></fnt>;
      </ekz>
      <ekz>
        oni devos pagi 30 a&ubreve; 50 <tld/>ojn por a&ccirc;eti maldikan
        gvidlibreton
        <fnt>
          <aut>Kooga S., trad. &Scirc;imizu J.</aut>,
          <vrk><url ref="http://www.geocities.jp/akvoju/traduko/viroenbluavesto.html">
          Viro en Blua Vesto</url></vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_n; <klr>(&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>sena</trd>,
    <trd>senes</trd>
  </trdgrp>
  <trd lng="fr">sen</trd>
  <trd lng="it">sen</trd>
  <trd lng="pl">sen  <klr>(moneta japo&nacute;ska)</klr></trd>
  <trd lng="sk">sen</trd>
  <trd lng="uk">&c_s;&c_je;&#x43D; (&c_r;&#x43E;&c_z;&#x43C;&c_ib;&#x43D;&#x43D;&c_a; &#x43C;&#x43E;&#x43D;&c_je;&c_t;&c_a;)</trd>
</drv>
</art>

