<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>porcelan</rad>/o
</kap>
<drv mrk="porcel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="porcel.0o.substanco">
    <dif>
      <ref tip="vid" cel="cerami.0o">Ceramika</ref> substanco delikata,
      diafaneta, bakita el kaolino:
      <ekz>
        statuetoj, teleroj el <tld/>o;
      </ekz>
      <ekz>
        materialo de la tenilo: diversaj lignospecoj, korno de antilopo,
        plasto, <tld/>o<fnt><aut>Araneo</aut>: <vrk>Forko</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="porcel.0o.porcelanajxo">
    <ref tip="dif" cel="porcel.0ajxo"><tld/>a&jcirc;o</ref>
    <ekz>
      &hcirc;inaj <tld/>oj.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_c;&c_a;&c_l;&c_ja;&c_n;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Porzellan</trd>
  <trdgrp lng="fr">
    <trd>porcelaine<klr> (mat&eacute;riau)</klr></trd>
  </trdgrp>
  <trd lng="hu">porcel&aacute;n</trd>
  <trd lng="nl">porselein</trd>
  <trd lng="pl">porcelana</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;</trd>
  <trd lng="sv">porslin</trd>
</drv>

<drv mrk="porcel.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_c;&c_a;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en <ind>porcelaine</ind></trd>
  </trdgrp>
  <trd lng="pl">porcelanowy</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="porcel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Objekto el <tld/>o:
      <ekz>
        <tld/>a&jcirc;o, farita el kaolino post bakado en medio de pli ol 1
        200&deg;C, estas kompakta, nesorba kaj metale sonanta je
        frapado<fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Porcelana&jcirc;oj&rightquot;, p.
        212a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_c;&c_a;&c_l;&c_ja;&c_n;&c_a;
    <klr>(&c_v;&c_y;&c_r;&c_a;&c_b;)</klr></trd>
  <trdgrp lng="fr">
    <trd>porcelaine<klr> (objet)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z; <ind>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_a;</ind></trd>
</drv>
</art>
