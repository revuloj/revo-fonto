<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>porcelan</rad>/o
</kap>
<drv mrk="porcel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tre delikata, blanka argilo, kiun oni bakas por fari diversajn
      duondiafanajn objektojn:
      <ekz>
        statuetoj, teleroj el <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="P"/>a&jcirc;o:
      <ekz>
        &hcirc;inaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Porzellan</trd>
  <trd lng="hu">porcel&aacute;n</trd>
  <trd lng="nl">porselein</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;</trd>
  <trd lng="sv">porslin</trd>
</drv>
</art>
