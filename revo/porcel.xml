<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: porcel.xml,v 1.26 2024/02/17 07:52:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>porcelan</rad>/o
</kap>
<drv mrk="porcel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="porcel.0o.CER">
    <dif>
      <ref tip="vid" cel="cerami.0o">Ceramika</ref> substanco delikata,
      diafaneta, bakita el <ref cel="kaolin.0o">kaolino</ref>:
      <ekz>
        statuetoj, teleroj el <tld/>o;
      </ekz>
      <ekz>
        delikata <tld/>o, tiel multekosta, sed anka&ubreve; tiel rompebla
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rigardadis al la tualeta tablo, &ccirc;ar tie staris &ccirc;arma malgranda pa&scirc;tistino el <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="porcel.0o.porcelanajxo">
    <dif>
      <ref tip="dif" cel="porcel.0ajxo"><tld lit="P"/>a&jcirc;o</ref>:
      <ekz>
        &ccirc;inaj <tld/>oj;
      </ekz>
      <ekz>
        la tablo estis kovrita per blindige blanka tablotuko kaj delikata
        <tld/>o kaj sur &gcirc;i plej bonguste vaporis rostita ansero, far&ccirc;ita per prunoj kaj pomoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_c;&c_a;&c_l;&c_ja;&c_n;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>porcel&aacute;n</trd>,
    <trd>porcul&aacute;n</trd>
  </trdgrp>
  <trd lng="de">Porzellan</trd>
  <trd lng="en">porcelain</trd>
  <trd lng="fr">porcelaine<klr> (mat&eacute;riau)</klr></trd>
  <trd lng="hu">porcel&aacute;n</trd>
  <trd lng="ja">&#x78C1;&#x5668;  <pr>&#x3058;&#x304D;</pr></trd>
  <trd lng="nl">porselein</trd>
  <trd lng="pl">porcelana</trd>
  <trd lng="pt">porcelana</trd>
  <trd lng="ro">por&#x21B;elan</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;</trd>
  <trd lng="sk">porcel&aacute;n</trd>
  <trd lng="sv">porslin</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_r;&c_c;&c_je;&#x43B;&#x44F;&#x43D;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&#x43E;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x74F7;&#x5668; <pr>c&#xED;q&#xEC;</pr></trd>,
    <trd>&#x74F7; <pr>c&#xED;</pr></trd>,
    <trd>&#x9676;&#x74F7; <pr>t&#xE1;oc&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="porcel.0a">
  <kap><tld/>a</kap>
  <snc mrk="porcel.0a.CER">
    <dif>
      El <tld/>o:
      <ekz>
        la sinjoro trovis <klr>[&#x2026;]</klr> plenmanon da rizo, teajn foliojn,
        <tld/>ajn kalikojn ornamitajn per pentra&jcirc;oj 
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        tablo kun <tld/>a servico kaj kun resta&jcirc;oj de videble &jcirc;us man&gcirc;itaj franda&jcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la juna sinjoro en la ri&ccirc;a pelto elektadis tapi&scirc;ojn, ar&gcirc;entajn korbojn, <tld/>ajn vazojn kaj
        marmorajn statuetojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        panjo vizitis nin alportante al mi belan pupon
        kun kapo <tld/>a
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_c;&c_a;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="en">porcelain</trd>
  <trd lng="fr">en <ind>porcelaine</ind></trd>
  <trd lng="ja">&#x78C1;&#x5668;&#x306E;  <pr>&#x3058;&#x304D;&#x306E;</pr></trd>
  <trd lng="nl">porceleinen</trd>
  <trd lng="pl">porcelanowy</trd>
  <trd lng="ro">de  <ind>por&#x21B;elan</ind></trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_r;&c_c;&c_je;&#x43B;&#x44F;&#x43D;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_f;&#x43E;&c_r;&#x43E;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="porcel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Objekto el <tld/>o:
      <ekz>
        fabrikisto de <tld/>a&jcirc;oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o, farita el kaolino post bakado en medio de pli ol
        1&nbsp;200&deg;C, estas kompakta, nesorba kaj metale sonanta je
        frapado
        <fnt>
          <bib>EncCxi</bib>,
          <lok>&ccirc;ap. &leftquot;Porcelana&jcirc;oj&rightquot;, p. 212a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_c;&c_a;&c_l;&c_ja;&c_n;&c_a;
    <klr>(&c_v;&c_y;&c_r;&c_a;&c_b;)</klr></trd>
  <trd lng="en">porcelain</trd>
  <trd lng="fr">porcelaine<klr> (objet)</klr></trd>
  <trd lng="ja">&#x78C1;&#x5668;&#x88FD;&#x54C1;  <pr>&#x3058;&#x304D;&#x305B;&#x3044;&#x3072;&#x3093;</pr></trd>
  <trd lng="nl">voorwerp uit <ind>porcelein</ind></trd>
  <trd lng="pl">porcelana  <klr>(wyr&oacute;b)</klr></trd>
  <trd lng="ro">por&#x21B;elan  <klr>(produs)</klr></trd>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z;
<ind>&c_f;&c_a;&c_r;&c_f;&c_o;&c_r;&c_a;</ind></trd>
  <trd lng="uk">&c_v;&c_i;&c_r;&c_ib;&c_b; &c_z; &#x43F;&#x43E;&c_r;&c_c;&c_je;&#x43B;&#x44F;&#x43D;&c_i;/&c_f;&c_a;&c_r;&c_f;&#x43E;&c_r;&c_u;</trd>
</drv>
</art>
