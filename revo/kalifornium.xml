<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalifornium.xml,v 1.14 2023/12/02 21:20:04 revo Exp $">
<kap>
  <rad>kalifornium</rad>/o,
  <var><kap><rad var="i">kaliforni</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="kalifornium.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var></kap>
  <mlg kod="IUPAC">Cf</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="kalifornium.0o.KEM">
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="98">elemento</ref> 
      kun atomnumero 98, atompezo 251,0796;  
      <ref tip="super" cel="urani.trans0o">transuranio</ref>, kies 
      &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj, aktinoido:
      <ekz>
        <tld/>o estas transurania radioaktivega metala elemento, kiun oni povas 
        fari per bombardado de kuriumo <klr>[per]</klr> alfaradioj, &gcirc;i nomi&gcirc;is 
        <klr>[la&ubreve;]</klr> <nom>Berkeley</nom>, Kalifornio, kie &gcirc;i estis unue farita 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34029">Kaliforniumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="kaliforni.0o">Kalifornio (usona &scirc;ato)</ref>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_i;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
  <trd lng="br">kaliforniom</trd>
  <trd lng="ca">californi</trd>
  <trd lng="cs">kalifornium</trd>
  <trd lng="de">Californium</trd>
  <trd lng="en">californium</trd>
  <trd lng="es">californio</trd>
  <trd lng="fr">californium</trd>
  <trd lng="hu">kalifornium</trd>
  <trd lng="id">kalifornium</trd>
  <trd lng="it">californio</trd>
  <trd lng="ja">&#x30AB;&#x30EA;&#x30D5;&#x30A9;&#x30EB;&#x30CB;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Californium</trd>
  <trd lng="nl">californium</trd>
  <trd lng="pl">kaliforn</trd>
  <trd lng="pt">calif&oacute;rnio</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">kalifornium</trd>
  <trd lng="sv">californium</trd>
  <trdgrp lng="zh">
    <trd>&#x9272; <pr>k&#x1CE;</pr></trd>,
    <trd>&#x9426; <pr>k&#x101;i</pr></trd>
  </trdgrp>
</drv>
</art>
