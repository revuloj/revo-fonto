<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalifornium</rad>/o <fnt>kalifornio en ReVo</fnt>
</kap>
<drv mrk="kalifornium.0o">
  <kap><tld/>o</kap>
  <mlg>Cf</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="kalifornium.0o.KEM">
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> 
      kun atomnumero 98, atompezo 251,0796, simbolo <frm>Cf</frm>.  
      <ref tip="super" cel="urani.trans0o">Transuranio</ref>, kies 
      &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj. Aktinoido.
    </dif>
  </snc>
  <trd
lng="be">&c_k;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_j;</trd>    
  <trd lng="br">kaliforniom</trd>
  <trd lng="ca">californi</trd>
  <trd lng="de">Californium</trd>
  <trd lng="en">californium</trd>
  <trd lng="es">californio</trd>
  <trd lng="fr">californium</trd>
  <trd lng="hu">kalifornium</trd>
  <trd lng="it">californio</trd>
  <trd lng="la">Californium</trd>
  <trd lng="nl">californium</trd>
  <trd lng="pl">kaliforn</trd>
  <trd lng="pt">calif&oacute;rnio</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
  <trd lng="sv">californium</trd>
</drv>
</art>
