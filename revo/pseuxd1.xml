<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pseuxd1.xml,v 1.25 2024/02/17 11:11:10 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>pse&ubreve;donim</rad>/o <fnt><bib>PV</bib></fnt>,
   <var><kap><rad var="o">pse&ubreve;donom</rad>/o</kap></var>
</kap>
<drv mrk="pseuxd1.0o">
   <kap><tld/>o, <var><kap><tld var="o"/>o</kap></var></kap>
   <snc mrk="pseuxd1.0o.KOMUNE">
     <dif>
       <ref tip="dif" cel="nom.kasx0o.KOMUNE">Ka&scirc;nomo</ref>:
       <ekz>
         vidante, ke mia komenca <tld/>o transiris al la afero mem kaj ke
         &gcirc;i estas tre oportuna, mi balda&ubreve; tute &ccirc;esis uzadi mian <tld/>on,
         kaj &gcirc;i restis nur kiel nomo por la afero mem
         <fnt>
           <bib>LR</bib>,
           <lok>La Esperantisto, 1890, p.60</lok>
         </fnt>;
       </ekz>
       <ekz>
         <nom>Kabe</nom> estas la <tld/>o de D-ro <nom>K. Bein</nom>;
       </ekz>
       <ekz>
        &leftquot;vi ne vidos mian nomon&rightquot;, la libristo respondis 
        kun petola brilo en la okulvitroj, &leftquot;mi uzas 
        <tld var="o"/>on&rightquot;
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li bremsis 
        sufi&ccirc;e?</vrk>, <lok>1979</lok></fnt>.
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;&c_n;&c_ib;&c_m;</trd>
   <trd lng="bg">&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;&c_n;&c_i;&c_m;</trd>
   <trd lng="ca">pseud&ograve;nim</trd>
   <trdgrp lng="de">
     <trd>K&uuml;nstlername</trd>,
     <trd>Pseudonym</trd>
   </trdgrp>
   <trd lng="en">pseudonym</trd>
   <trd lng="es">seud&oacute;nimo</trd>
   <trdgrp lng="fa">
     <trd>&a_n;&a_A;&a_m; &a_m;&a_s;&a_t;&a_ayn;&a_A;&a_r;</trd>,
     <trd>&a_t;&a_kh;&a_l;&a_S;</trd>
   </trdgrp>
   <trd lng="fr">pseudonyme</trd>
   <trd lng="hu">&aacute;ln&eacute;v</trd>
   <trdgrp lng="nl">
     <trd>schuilnaam</trd>,
     <trd>pseudoniem</trd>
   </trdgrp>
   <trd lng="pl">pseudonim</trd>
   <trd lng="pt">pseud&ocirc;nimo</trd>
   <trd lng="ro">pseudonim</trd>
   <trd lng="ru">&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;&c_n;&c_i;&c_m;</trd>
   <trd lng="sv">pseudonym</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_s;&c_je;&c_v;&c_d;&#x43E;&#x43D;&c_ib;&#x43C;</trd>,
    <trd>&#x43F;&c_s;&c_je;&c_v;&c_d;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5316;&#x540D; <pr>hu&#xE0;m&#xED;ng</pr></trd>,
    <trd>&#x7B14;&#x540D; <pr>b&#x1D0;m&#xED;ng</pr></trd>,
    <trd>&#x7B46;&#x540D; <pr>b&#x1D0;m&#xED;ng</pr></trd>
  </trdgrp>
</drv>
