<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: herbicid.xml,v 1.7 2023/07/02 06:31:19 revo Exp $">
<kap>
  <rad>herbicid</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="herbicid.0o">
   <kap><tld/>o</kap>
  <snc mrk="herbicid.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="pesticid.0o.AGR">Pesticido</ref>
      per kiu oni mortigas fiherbojn kaj aliajn trudvegeta&jcirc;ojn,
      ordinare en formo de veneno...:
      <ekz>
        la tria &ccirc;apitro okupi&gcirc;as pri historio kaj efiko de pesticidoj
        kaj <tld/>oj.
        <fnt><vrk><url ref="&Viki;/Silenta_printempo">
          Pri la la libro "Silent Spring" de Rachel
          Carson</url></vrk></fnt>;
      </ekz>
      <ekz>
        ili provizas &ndash; kredite &ndash; la kultivnecesa&jcirc;ojn, insekticidojn kaj
        <tld/>ojn, kaj kiam rikolto fini&gcirc;is, ili venas kolekti &gcirc;in en la
        kampoj  kaj porti &gcirc;in al la disgrajniga fabriko.
        <fnt><vrk><url ref="&Mondediplo;article1459.html">
         Artikolo de  Fran&ccedil;oise G&Eacute;RARD dans le Monde Diplomatique:
         Monsanto konkeranta Burkinon </url></vrk></fnt>.
      </ekz>
    </dif>
  <trd lng="de">Herbizid</trd>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_b;&c_ib;&c_c;&c_y;&c_d;</trd>
  <trd lng="cs">herbicid</trd>
  <trd lng="fr">herbicide</trd>
  <trd lng="sk">herbic&iacute;d</trd>
  <trd lng="uk">&c_g;&c_je;&c_r;&c_b;&c_ib;&c_c;&c_i;&c_d;</trd>
</drv>
</art>

