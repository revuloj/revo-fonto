<drv mrk="epilog.0o">
<kap><tld/>o</kap>
<uzo tip="fak">TEA</uzo>
<snc>
<dif>
Fina parto de verko, malo de prologo.  
</dif>
<trd lng="be">&c_e;&c_p;&c_ib;&c_l;&c_jo;&c_g;</trd>
  <trdgrp lng="de">
     <trd>Nachrede</trd>,
     <trd>Nachspiel</trd>,
     <trd>Schluss</trd>,
     <trd>Epilog</trd>
  </trdgrp>
  <trd lng="en">epilog(ue)</trd>
  <trd lng="es">ep&iacute;logo</trd>
  <trd lng="fr">&eacute;pilogue</trd>
  <trdgrp lng="hu">
     <trd>epil&oacute;gus</trd>,
     <trd>ut&oacute;sz&oacute;</trd>,
     <trd>ut&oacute;j&aacute;t&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>epiloog</trd>,
    <trd>slot<klr tip="ind"> (epiloog)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_l;&c_o;&c_g;</trd>
</snc>
</drv>
</art>
