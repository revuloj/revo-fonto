<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>anka&ubreve;</rad>
</kap>
<drv mrk="ankaux.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <gra><vspec>adv</vspec></gra>
     <dif>
       Kaj plie, same, egale kiel aliaj:
       <ekz>
         <tld/> mi estas esperantisto;
       </ekz>
       <ekz>
         ne nur... sed <tld/>;
       </ekz>
       <ekz>
         amiko de amiko estas <tld/> amiko<fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/> dum ridado povas dolori la koro
         <fnt><bib>MT</bib>, <lok>&Sen; 14:13</lok></fnt>;
       </ekz>
       <ekz>
         <tld/> diablo tondron suferos<fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <rim>
       <em>Anka&ubreve;</em> devas &ccirc;iam esti metata senpere
       anta&ubreve; la vorto, kiun oni volas emfazi.
     </rim>
   </snc>
   <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_a;&c_m;&c_a;</trd>
   <trd lng="br">ivez</trd>
   <trd lng="cs">tak&eacute;</trd>
   <trdgrp lng="de">
     <trd>auch</trd>, <trd>ebenfalls</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>also</trd>, <trd>too</trd>, <trd>moreover</trd>
   </trdgrp>
   <trd lng="es">tambi&eacute;n</trd>
   <trdgrp lng="fi">
     <trd>my&ouml;s</trd>,
     <trd>-<ind>kin</ind></trd>;
     <trd><klr>[en nea kunteksto:] </klr>my&ouml;sk&auml;&auml;n</trd>,
     <trd>-<ind>kaan</ind></trd>,
     <trd>-<ind>k&auml;&auml;n</ind></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>aussi</trd>,
     <trd>pareillement</trd>,
     <trd>de <ind>plus</ind></trd>
   </trdgrp>
   <trd lng="he">&gimel;&fmem;</trd>
   <trd lng="hu">is</trd>
   <trd lng="it">anche</trd>
   <trd lng="nl">ook</trd>
   <trdgrp lng="pl">
     <trd>tak&zdot;e</trd>,
     <trd>te&zdot;</trd>,
     <trd>r&oacute;wnie&zdot;</trd>
   </trdgrp>
   <trdgrp lng="pt">
    <trd>tamb&eacute;m</trd>,
    <trd>ainda</trd>,
    <trd>idem</trd>,
    <trd>outrossim</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_t;&c_a;&c_k;&c_zh;&c_je;</trd>,
     <trd>&c_t;&c_o;&c_zh;&c_je;</trd>
   </trdgrp>
   <trdgrp lng="sk">
     <trd>aj</trd>,
     <trd>tie&zcaron;</trd>
   </trdgrp>
   <trdgrp lng="sv">
     <trd>ocks&aring;</trd>,
     <trd>&auml;ven</trd>
   </trdgrp>
   <trd lng="tp">kin</trd>
   <trd lng="vo">i</trd>
</drv>
</art>
