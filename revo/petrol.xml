<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: petrol.xml,v 1.28 2022/05/01 17:51:19 revo Exp $">
<kap><ofc>*</ofc>
  <rad>petrol</rad>/o</kap>

<drv mrk="petrol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="petrol.0o.rafinita">
   <uzo tip="fak">MIN</uzo>    
   <dif>
     Minerala <ref tip="super" cel="ole.0o">oleo</ref>,
     ekstraktata el <ref cel="naft.0o">nafto</ref> kun
     boltemperaturo inter 175&#x202F;&deg;C &gcirc;is 325&#x202F;&deg;C
     kaj uzata precipe kiel brula&jcirc;o por lampoj, hejtado, en eksplodaj
     kaj
     reakciaj motoroj ktp:
      <ekz>
        &scirc;i a&ccirc;etis en la butiko <klr>[&#x2026;]</klr> malgrandan lampeton kaj
        iom da <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum vintro oni havos tro malmulte da hejt<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009177.php">Bone nutrataj a&ubreve;toj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="benzin.0o">benzino</ref>,
     <ref cel="kerosen.0o">keroseno</ref>,
     <ref cel="vazeli.0o">vazelino</ref>
   </refgrp>
   <trd lng="be">&c_n;&c_a;&c_f;&c_t;&c_a;&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_t;</trd>
   <trd lng="br">eoul-maen</trd>
   <trd lng="de">Petroleum</trd>
   <trd lng="en">petroleum</trd>
   <trd lng="es">nafta</trd>
   <trd lng="fr">p&eacute;trole<klr> (lampant)</klr></trd>
   <trdgrp lng="ru">
     <trd><klr tip="amb">(&c_o;&c_ch;&c_i;&c_shch;&c_je;&c_n;&c_n;&c_a;&c_ja;)
     </klr>&c_n;&c_je;&c_f;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_je;&c_f;&c_t;&c_je;&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;</trd>
   </trdgrp>
  </snc>
  <snc mrk="petrol.0o.kruda">
    <uzo tip="stl">EVI</uzo>
    <dif>
     <ref tip="dif" cel="naft.0o">Nafto</ref>:
      <ekz>
        la homoj militis <klr>[&#x2026;]</klr> pro <tld/>o, karbo, pro ercoj
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="&Steloj;monato2/007018.html">&Steloj;monato2/007018.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Klubo de Romo en la jaro 1972 prognozis ke la rezervoj de
        <tld/>o sufi&ccirc;os por
        20 &gcirc;is 50 jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007184.html">Petrolo: Nehaltigeblaj prezalti&gcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        de pluraj monatoj la prezo de kruda <tld/>o, kaj sekve de &gcirc;iaj
        derivatoj kiel
        benzino kaj dizeloleo, da&ubreve;re kreskas
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007184.html">Petrolo: Nehaltigeblaj prezalti&gcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="petrol.Org0oEkspLandoj.EKON">Organiza&jcirc;o de <tld lit="P"/>-Eksportaj Landoj</ref>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ole.sxton0o">&scirc;tonoleo</ref>
   <trd lng="be">&c_n;&c_a;&c_f;&c_t;&c_a;</trd>
   <trd lng="de">Erd&ouml;l</trd>
   <trd lng="fr">p&eacute;trole<klr> (brut)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>miner&aacute;ln&iacute; olej</trd>,
    <trd>nafta</trd>,
    <trd>petrolej</trd>,
    <trd>ropa</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;trole</trd>
  <trd lng="hu">k&odblac;olaj</trd>
  <trdgrp lng="nl">
    <trd>olie</trd>,
    <trd>petroleum</trd>
  </trdgrp>
  <trd lng="pl">ropa naftowa</trd>
  <trd lng="ro">petrol</trd>
  <trdgrp lng="sk">
    <trd>nafta</trd>,
    <trd>petrolej</trd>,
    <trd>ropa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x444;&#x442;&#x430;</trd>,
    <trd>&#x433;&#x430;&#x441;</trd>,
    <trd>&#x43D;&#x430;&#x444;&#x442;&#x43E;&#x43F;&#x440;&#x43E;&#x434;&#x443;&#x43A;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="petrol.Org0oEkspLandoj">
  <kap>Organiza&jcirc;o de <tld lit="P"/>-Eksportaj Landoj
  <fnt><bib>ManPol</bib></fnt></kap>
  <mlg>OPEL</mlg>
  <snc mrk="petrol.Org0oEkspLandoj.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
     <ref tip="lst" cel="organi2.0ajxo" lst="voko:organiza&jcirc;oj">Organiza&jcirc;o</ref>, 
     kiu celas kunordigi la politikon pri nafto en siaj
     <ref tip="vid" cel="sxtat.membro0o">membro-&scirc;tato</ref>j:
     <ekz>
       je la 10a de septembro 2000 la Organiza&jcirc;o de <tld lit="P"/>-Eksportaj
       Landoj kunvenis en Vieno kaj decidis nur etkvante
       plialtigi la produktadon
       <fnt><aut>Roland Rotsaert</aut>:
         <vrk><url ref="&Monato;/007379.html">Nehaltigeblaj
         prezalti&gcirc;oj</url></vrk>,
         <bib>Monato</bib>,
         <lok>n-ro 11/2000</lok>
       </fnt>;
     </ekz>
      <ekz>
        Ni&gcirc;erio estas la plej granda nafto-produktanto en Afriko, kaj la
        sepa plej forta
        membro de la Organiza&jcirc;o de <tld lit="P"/>-Eksportaj Landoj
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2012/010616.php">Malfeli&ccirc;an novjaron</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;
      &c_k;&c_r;&c_a;&c_ib;&c_n;-&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_e;&c_r;&c_a;&c_w;
     <ind>&c_n;&c_a;&c_f;&c_t;&c_y;</ind></trd>,
    <trd>&c_A;&c_P;&c_E;&c_K;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Organisation <ind>Erd&ouml;l</ind> exportierender L&auml;nder</trd>,
    <trd>OPEC</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>Organisation of <ind>Petroleum</ind> Exporting
     Countries</trd>,
     <trd>OPEC</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>Organisation des Pays Exportateurs de
     <ind>P&eacute;trole</ind></trd>,
     <trd>OPEP</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>K&odblac;olajexport&aacute;l&oacute; Orsz&aacute;gok Szervezete</trd>,
     <trd>OPEC</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Organisatie van <ind>Olie</ind>-Exporterende Landen</trd>,
    <trd>OPEC</trd>
  </trdgrp>
  <trd lng="pl">Organizacja Kraj&oacute;w Eksportuj&aogonek;cych Rop&eogonek; Naftow&aogonek;  <klr>(OPEC)</klr></trd>
  <trd lng="ro">Organiza&#x21B;ia &#x21A;&abreve;rilor Exportatoare de Petrol  <klr>(OPEC)</klr></trd>
  <trdgrp lng="ru">
     <trd>&c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;
    &c_s;&c_t;&c_r;&c_a;&c_n;-&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_jo;&c_r;&c_o;&c_v;
    &c_n;&c_je;&c_f;&c_t;&c_i;</trd>,
     <trd>&c_O;&c_P;&c_Je;&c_K;</trd>
  </trdgrp>
</drv>
