<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>petrol</rad>/o</kap>
<drv mrk="petrol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Minerala oleo, eltirata el naturaj fontoj kaj uzata por lumigado kaj
      hejtado.
    </dif>
    <refgrp tip="sin">
      <ref cel="benzin.0o">benzino</ref>,
      <ref cel="naft.0o">nafto</ref>,
      <ref cel="ole.sxton0o">&scirc;tonoleo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_f;&c_t;&c_a;</trd>
  <trd lng="de">Erd&ouml;l</trd>
  <trd lng="hu">k&odblac;olaj</trd>
  <trdgrp lng="nl">
    <trd>olie</trd>,
    <trd>petroleum</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_f;&c_t;&c_mol;</trd>
</drv>

<drv mrk="petrol.0o.OPEL">
  <kap>Organiza&jcirc;o de <tld lit="P"/>-Eksportaj Landoj</kap>
  <mlg>OPEL</mlg>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>ManPol</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="organi2.0ajxo">Organiza&jcirc;o</ref>, kiu celas
      kunordigi la politikon pri petrolo en siaj <ref tip="vid"
      cel="sxtat.membro0o">membro-&scirc;tato</ref>j:
      <ekz>
        je la 10a de septembro 2000 la Organiza&jcirc;o de <tld
        lit="P"/>-Eksportaj Landoj kunvenis en Vieno kaj decidis nur
        etkvante
        plialtigi la produktadon.
        <fnt><aut>Roland ROTSAERT</aut>:
        <vrk><url ref="&Monato;/007379.html">Nehaltigeblaj
        prezalti&gcirc;oj</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 11/2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; &c_k;&c_r;&c_a;&c_ib;&c_n;-&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_e;&c_r;&c_a;&c_w; <ind>&c_n;&c_a;&c_f;&c_t;&c_y;</ind></trd>,
    <trd>&c_A;&c_P;&c_E;&c_K;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Organisation of <ind>Petroleum</ind> Exporting Countries</trd>,
    <trd>OPEC</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation des Pays Exportateurs de <ind>P&eacute;trole</ind></trd>,
    <trd>OPEP</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>K&odblac;olajexport&aacute;l&oacute; Orsz&aacute;gok Szervezete</trd>,
    <trd>OPEC</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Organisatie van <ind>Olie</ind>-Exporterende Landen</trd>,
    <trd>OPEC</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja; &c_s;&c_t;&c_r;&c_a;&c_n;-&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_jo;&c_r;&c_o;&c_v; &c_n;&c_je;&c_f;&c_t;&c_i;</trd>,
    <trd>&c_O;&c_P;&c_Je;&c_K;</trd>
  </trdgrp>
</drv>
</art>
