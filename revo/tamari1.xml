<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>tamarik</rad>/o
</kap>
<drv mrk="tamari1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="tamari1.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Tamarix</trd>), arbeto kun malgrandaj folietoj kaj
      floretoj, kreskanta en la mediteraneaj regionoj.
    </dif>
  </snc>
  <trd lng="de">Tamariske</trd>
</drv>
<drv mrk="tamari1.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de malgrandaj arboj kaj arbedoj
      (<trd lng="la">Tamaricaceae</trd>), kun skvamaj folioj eligantaj
      salon, precipe kreskantaj en stepoj kaj dezertoj.
    </dif>
  </snc>
  <trd lng="de">Tamariskengew&auml;chse</trd>
</drv>
</art>
