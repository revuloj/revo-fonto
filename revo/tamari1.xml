<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tamari1.xml,v 1.28 2023/09/04 17:49:39 revo Exp $">
<kap>
  <rad>tamarik</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="s">tamarisk</rad>/o <fnt><bib>MT</bib></fnt></kap></var>
</kap>
<drv mrk="tamari1.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="tamari2.0oj"><tld/>acoj</ref>
      (<trd lng="la">Tamarix</trd>), arbeto kun malgrandaj folietoj kaj
      floretoj, kreskanta en la mediteraneaj regionoj:
      <ekz>
       riveraj arbaroj formitaj de blanka poplo, fraks<klr>[e]</klr>no, blanka saliko kaj komuna
       ulmo,
       krom<klr>[e]</klr> estas <tld/>o, rubuso, rozfrukto, klematido <klr>[&#x2026;]</klr>
       <fnt>
        <bib>Viki</bib>
        <lok><url ref="https://eo.wikipedia.org/?curid=39280">Gvadalaharo (Hispanio)</url></lok>
       </fnt>;
      </ekz>
      <ekz>
        <nom>Abraham</nom> plantis <tld var="s"/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 21:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Saul</nom> tiam sidis en <nom>Gibea</nom> sub la <tld var="s"/>o sur la alta&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 22:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e Vergilio &ndash; Eklogo 4, verso 2: &leftquot;Pla&ccirc;as ne &ccirc;iujn arbustoj kaj &ccirc;i <tld var="s"/>' humila&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2013/010730.php">Brile versform-fidela traduko &gcirc;uinda</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d6/Tamarix_canariensis.JPG/640px-Tamarix_canariensis.JPG" prm="CC BY 3.0">floranta bran&ccirc;o de <tld/>o, Ceuta, Hispanujo
        <fnt>
          <aut>Xemenendura, propra verko</aut>
          <lok><url ref="&WCW;/File:Tamarix_canariensis.JPG">&WCW;/File:Tamarix_canariensis.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_m;&c_a;&c_r;&c_y;&c_s;&c_k;</trd>
  <trd lng="ca">tamariu</trd>
  <trd lng="cs">tamary&scaron;ek</trd>
  <trd lng="de">Tamariske</trd>
  <trdgrp lng="fr">
    <trd>tamarix</trd>,
    <trd>tamaris</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tamariska</trd>,
    <trd>tamariszkusz</trd>
  </trdgrp>
  <trd lng="ja">&#x30AE;&#x30E7;&#x30EA;&#x30E5;&#x30A6;</trd>
  <trd lng="nl">tamarisk</trd>
  <trd lng="pl">tamaryszek</trd>
  <trdgrp lng="pt">
   <trd>tamargueira</trd>,
   <trd>tamariz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_k;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">tamari&scaron;ka</trd>
  <trd lng="sv">kl&aring;dris</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x430;&#x43C;&#x430;&#x440;&#x438;&#x43A;&#x441;</trd>,
    <trd>&#x442;&#x430;&#x43C;&#x430;&#x440;&#x438;&#x441;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
