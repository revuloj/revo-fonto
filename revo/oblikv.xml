<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
<ofc>1</ofc>
      <rad>oblikv</rad>/a
    </kap>
    <drv mrk="oblikv.0a">
      <kap><tld/>a</kap>
      <fnt><bib>PIV1</bib></fnt>
      <fnt><bib>KrED</bib></fnt>
      <snc>
        <dif>
          Klini&gcirc;anta for de la vertikala direkto:
          <ekz>
            la ortanto estas pli mallonga ol &ccirc;iuj <tld/>aj linioj
            kondukitaj de la sama punkto al la sama rekto;
          </ekz> <ekz>
            <tld/>e staranta turo.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_n;&c_a;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
          <trd>&c_n;&c_a;&c_h;&c_ib;&c_l;&c_je;&c_n;&c_y;</trd>,
          <trd>&c_s;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_t;&c_y;</trd>
        </trdgrp>
        <trd lng="de">schief</trd>
        <trd lng="nl">schuin</trd>
        <trdgrp lng="ru"><trd>&c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_p;&c_o;&c_k;&c_a;&c_t;&c_y;&c_j;</trd></trdgrp>
       </snc>
      <snc>
        <dif>
          Klini&gcirc;anta for de la normala direkto: <ekz><tld/>a vojo;
          </ekz> <ekz><tld/>e teksita drapo;
          </ekz> <ekz><tld/>a rigardo<fnt>Z</fnt>
            <klr>(= ne direktata al la okuloj)</klr>;
          </ekz> <ekz>li multe rigardis &scirc;in <tld/>e kaj avide.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="rekt">malrekta</ref>,
          <ref cel="sxultr">tra&scirc;ultra</ref>,
          <ref cel="transv.0a">transversa</ref>.
        </refgrp>
        <trdgrp lng="be">   
          <trd>&c_k;&c_o;&c_s;&c_y;</trd>,
          <trd>&c_k;&c_a;&c_s;&c_y;</trd>
          <trd>&c_k;&c_r;&c_y;&c_v;&c_y;</trd>
        </trdgrp>
        <trd lng="ru">&c_k;&c_o;&c_s;&c_o;&c_j;</trd>
      </snc>
      <trd lng="fr">oblique</trd>
      <trd lng="hu">ferde</trd>
      <trd lng="pt">obl&iacute;quo</trd>
    </drv>

    <drv mrk="oblikv.0i">
      <kap><tld/>(ir)i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Iri la&ubreve; <tld/>a direkto.
        </dif>
        <trd lng="fr">obliquer</trd>
        <trd lng="ru">&c_k;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
      </snc>
      <trdgrp lng="hu">
        <trd>ferd&eacute;n megy</trd>,
        <trd>ferd&eacute;n halad</trd>
      </trdgrp>
    </drv>

    <drv mrk="oblikv.0o">
      <kap><tld/>o</kap>
      <snc mrk="oblikv.0o.signo">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Nomo de <ref tip="malprt" cel="aski.0o">askia</ref> signo
          iam destinita por prezenti oblikvan
          <ref tip="sin" cel="frakci.0streko">frakcistrekon</ref>, kaj
          nun ricevinta la funkcion de disigilo, i.a. en la uniksaj
          dosierindikoj kaj sekve, en TTT-adresoj.
        </dif>
        <trdgrp lng="ru"><trd>&c_k;&c_o;&c_s;&c_a;&c_ja;</trd>,
          <trd>&c_d;&c_r;&c_o;&c_b;&c_mol;</trd></trdgrp>
        <trdgrp lng="en">
          <trd>slash</trd>,
          <trd>forward <ind>slash</ind></trd>,
          <trd>diagonal</trd>,
          <trd>fraction bar</trd>,
          <trd>fraction</trd>,
          <trd>solidus</trd>,
          <trd>slant</trd>,
          <trd>oblique stroke</trd>,
          <trd>separatrix</trd>,
          <trd>shilling mark</trd>,
          <trd>shilling stroke</trd>,
          <trd>virgule</trd></trdgrp>
      </snc>
      <trdgrp lng="hu">
        <trd>t&ouml;rtjel</trd>,
        <trd>perjel</trd>
      </trdgrp>
      <trdgrp lng="nl"><trd>schuine <ind>streep</ind></trd>,
        <trd>breukstreep</trd></trdgrp>
      <trd lng="pt">barra</trd>
      </drv>
  </art>
