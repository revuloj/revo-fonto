<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pot.xml,v 1.38 2024/01/10 20:25:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pot</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="pot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pot.0o.KOMUNE">
    <dif>
      Kava <ref tip="super" cel="vaz.0o">vazo</ref> por enteni ion:
      <ekz>
        sur la kameno inter du <tld/>oj staras fera kaldrono
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        la patrino metis en la fornon malgrandan <tld/>on kun biero, por
        varmigi &gcirc;in
        <fnt><bib>Fab3</bib>, <lok>historio de unu patrino</lok></fnt>;
      </ekz>
      <ekz>
        ne batalu <tld/>' el tero kontra&ubreve; kaldrono el fero
        <klr>(ne ataku potenculon)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>on taksu la&ubreve; sono, sinjoron la&ubreve; tono
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bol<tld/>o, <ind>kuir<tld/>o</ind>.
        <trdgrp lng="nl">
          <trd>kookpot</trd>,
          <trd>pan</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_sh;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_c;&c_ja;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_je;&c_n;&c_d;&c_zh;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_g;&c_malmol;&c_r;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_naro;&t_ga;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hrnec</trd>,
    <trd>kotl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Topf</trd>
  <trd lng="en">pot</trd>
  <trdgrp lng="es">
    <trd>vasija</trd>,
    <trd>olla</trd>
  </trdgrp>
  <trd lng="fr">pot</trd>
  <trd lng="hu">faz&eacute;k</trd>
  <trdgrp lng="ja">
    <trd>&#12388;&#12412;</trd>,
    <trd>&#12399;&#12385;</trd>,
    <trd>&#12509;&#12483;&#12488;</trd>,
    <trd>&#12394;&#12409;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pot</trd>,
    <trd>pan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gar</trd>,
    <trd>garnek</trd>,
    <trd>garniec</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrniec</trd>,
    <trd>n&aacute;doba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_r;&c_shch;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_a;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_l;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pot.flor0o">
  <kap>flor<tld/>o <fnt><bib>PIV3</bib></fnt></kap>
  <snc mrk="pot.flor0o.ujo">
    <dif>
      <tld lit="P"/>o por kultivado de ornamaj plantoj:
      <ekz>
        el la flor<tld/>o elkreskis du malgrandaj verdaj folietoj
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        kaj ili plantis la kernon en flor<tld/>on
        <fnt>
          <bib>Fab3</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Blumentopf</trd>
  <trdgrp lng="en">
    <trd>flower pot</trd>,
    <trd>flowerpot</trd>, <trd>plant pot</trd>
  </trdgrp>
  <trd lng="es">florero</trd>
  <trd lng="fr">pot de fleurs</trd>
  <trd lng="hu">vir&aacute;gcser&eacute;p</trd>
  <trd lng="ja">&#26893;&#26408;&#37474;  <pr>&#12358;&#12360;&#12365;&#12400;&#12385;</pr></trd>
  <trd lng="nl">bloempot</trd>
  <trd lng="pl">wazon</trd>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;
  &c_g;&c_o;&c_r;&c_sh;&c_o;&c_k;</trd>
  <trd lng="uk">&c_v;&c_a;&c_z;&c_o;&c_n;</trd>
</drv>

<drv mrk="pot.kuir0o">
  <kap>kuir<tld/>o</kap>
  <snc mrk="pot.kuir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o metebla super fajro por kuiri nutra&jcirc;ojn en
      &gcirc;i:
      <ekz>
        mi rigardis la rostilon kaj, apud la fajro,
        etan napon longetan kaj a&ccirc;an, kiun oni &jcirc;etis
        tien sendube &ccirc;ar ne bona por la kuir<tld/>o
        <fnt>
        <aut>E. de Bonaero</aut>,
        <vrk>La Friponeto de Tormes</vrk>,
        <lok>1973</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kasero.0o">kaserolo</ref>,
      <ref cel="pat.0o">pato</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_sh;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrnec</trd>,
    <trd>n&aacute;doba na va&rcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kochtopf</trd>
  </trdgrp>
  <trd lng="en">cooking pot</trd>
  <trd lng="es">cacerola <klr>(cocina)</klr></trd>
  <trdgrp lng="fr">
    <trd>marmite</trd>,
    <trd>pot &agrave; feu <klr>(cuisine)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>faz&eacute;k</trd>,
    <trd>l&aacute;bas</trd>
  </trdgrp>
  <trd lng="ja">&#12394;&#12409;</trd>
  <trd lng="nl">kookpan</trd>
  <trdgrp lng="pl">
    <trd>gar</trd>,
    <trd>garnek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_sh;&c_o;&c_k; <klr>(&c_d;&c_l;&c_ja;
    &c_v;&c_a;&c_r;&c_k;&c_i;)</klr></trd>
  </trdgrp>
  <trd lng="sk">hrniec na varenie</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_z;&c_a;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_ch;&c_a;&c_v;&c_u;&c_n;</trd>,
    <trd>&c_ch;&c_a;&c_v;&c_u;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pot.miks0ajxo">
  <kap>miks<tld/>a&jcirc;o, <var><kap>miks<tld/>o</kap></var></kap>
  <snc mrk="pot.miks0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Mikso de diversaj man&gcirc;a&jcirc;oj, kune kuiritaj:
      <ekz>
        Lorena miks<tld/>o <klr>[&#8230;]</klr> estas origine kamparana
        nutra&jcirc;o
        <fnt>
          <aut>Esperanto-Nancy</aut>,
          <vrk><url ref="http://esperanto-nancy.fr/?page_id=355">
          La Speciala&jcirc;oj el Nancio</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia hodia&ubreve;a varma man&gcirc;o konsistos el
        miks<tld/>a&jcirc;o
        de krispa brasiko
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.esperanto-nederland.nl/esp/turista-informo/ANNE%20FRANK%20DOMO.doc">
          Anne Frank Domo</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>hash</trd>,
      <trd>medley</trd>
    </trdgrp>
    <trd lng="es">guiso</trd>
    <trd lng="fr">pot&eacute;e</trd>
  </snc>
  <snc mrk="pot.miks0ajxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mikso de diversaj, eventuale neharmoniaj aferoj;
      <ref tip="vid" cel="pelmel.0o.KOMUNE">pelmelo</ref>,
      <ref tip="vid" cel="salat.0o.FIG">salato<sncref/></ref>:
      <ekz>
        Silezio estis miks<tld/>o, en kiu miksi&gcirc;adis influoj
        a&ubreve;striaj,
        bohemaj, luzacaj, germanaj, polaj kaj prusaj
        <fnt>
          <aut>G. Mendyka k.a.</aut>,
          <vrk><url ref="http://eurogento.org/gento_eo.htm">
          genealogia retejo E&ubreve;rogento</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Melange</trd>
    <trdgrp lng="en">
      <trd>mixed bag</trd>,
      <trd>mishmash</trd>, <trd>hodgepodge</trd>
    </trdgrp>
    <trd lng="es">revuelto<klr> (de alimento)</klr></trd>
    <trd lng="fr">m&eacute;li-m&eacute;lo</trd>
  </snc>
  <trd lng="de">Allerlei</trd>
  <trd lng="es">guiso</trd>
  <trd lng="fr">salmigondis</trd>
  <trdgrp lng="ja">
    <trd>&#12372;&#12387;&#12383;&#29038;  <pr>&#12372;&#12387;&#12383;&#12395;</pr></trd>,
    <trd>&#38609;&#37682;  <pr>&#12374;&#12388;&#12429;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_gu;&c_u; &c_z; &c_r;&c_ib;&c_z;&c_n;&c_i;&c_h; &c_s;&c_o;&c_r;&c_t;&c_ib;&c_v; &c_m;&#8217;&c_ja;&c_s;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_ib;&c_sh;</trd>,
    <trd>&c_m;&c_ib;&c_sh;&c_a;&c_n;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_u;&c_r;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="pot.ter0o">
  <kap>ter<tld/>o <fnt><bib>PIV3</bib></fnt></kap>
  <snc mrk="pot.ter0o.ujo">
    <dif>
      <tld lit="P"/>o el bakita argilo:
      <ekz>
        paste&ccirc;o estas bakita en tega&jcirc;o el pasto a&ubreve; en
        ter<tld/>o
        <fnt><bib>PIV3</bib></fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="de">Keramiktopf</trd>
  <trdgrp lng="en">
    <trd>pottery</trd>,
    <trd>earthenware</trd>
  </trdgrp>
  <trd lng="es">vasija de loza</trd>
  <trd lng="fr">pot de fa&iuml;ence</trd>
  <trdgrp lng="ja">
    <trd>&#12388;&#12412;</trd>,
    <trd>&#12363;&#12417;</trd>
  </trdgrp>
  <trd lng="nl"><ind>pot</ind> uit aardewerk</trd>
  <trdgrp lng="pl">
    <trd>donica</trd>,
    <trd>doniczka</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_shch;&c_o;&c_k;</trd>
</drv>

<drv mrk="pot.unu0ajxo">
  <kap>
    unu<tld/>a&jcirc;o
  </kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Simpla <ref tip="super" cel="plad.0o.kombino">plado</ref> el plurspecaj legomoj,
      eventuale grena&jcirc;o kaj viando- a&ubreve; fi&scirc;opecoj,
      &ccirc;io kune kuirita en akvoplena poto:
      <ekz>
        beta unu<tld/>a&jcirc;o
        <fnt>
          <vrk><url ref="https://apetito.ikso.net/recepto/beta-unupotajxo"
              >Beta unupota&jcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi denove gustumis la faman nigran nudelon <klr>[&#8230;]</klr>, &ccirc;indevena unu<tld/>a&jcirc;o
        <fnt>
          <aut>M&aacute;rkus G&aacute;bor</aut>
          <vrk><url ref="http://verkoj.com/dn/Markus%20Gabor/Koreio%20la%20Kongreslando.pdf"
              >Koreio, la kongreslando</url></vrk>
          <lok>p. 57</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="olapodrid.0o">olapodrido</ref>
  </snc>
  <trd lng="cs">eintopf</trd>
  <trd lng="de">Eintopf</trd>
  <trd lng="en">stew</trd>
  <trd lng="fr">pot-au-feu</trd>
  <trd lng="nl">eenpansgerecht</trd>
  <trd lng="pl">eintopf</trd>
  <trd lng="ru">&c_a;&c_j;&c_n;&c_t;&c_o;&c_p;&c_f;</trd>
</drv>

<drv mrk="pot.0faristo">
  <kap><tld/>faristo</kap>
  <snc mrk="pot.0faristo.VAZ">
    <dif>
      Homo, kiu profesie okupi&gcirc;as pri farado de <tld/>oj,
      precipe tiu, kiu faras ilin per bakado de argilo:
      <ekz>
        &ccirc;u diras argilo al sia <tld/>faristo: kion vi faras? via
        farita&jcirc;o estas malforta?
        <fnt>
          <bib>MT</bib>,
          <lok>&Jes; 45:9</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="cerami.0isto">ceramikisto</ref>
  </snc>
  <trd lng="cs">hrn&ccaron;&iacute;&rcaron;</trd>
  <trdgrp lng="de">
    <trd>T&ouml;pfe</trd>,
    <trd>T&ouml;pfermeister</trd>
  </trdgrp>
  <trd lng="en">potter</trd>
  <trd lng="es">alfarero</trd>
  <trd lng="fr">potier</trd>
  <trdgrp lng="ja">
    <trd>&#38518;&#24037;  <pr>&#12392;&#12358;&#12371;&#12358;</pr></trd>,
    <trd>&#38518;&#22120;&#32887;&#20154;  <pr>&#12392;&#12358;&#12365;&#12375;&#12423;&#12367;&#12395;&#12435;</pr></trd>,
    <trd>&#38518;&#33464;&#23478;  <pr>&#12392;&#12358;&#12370;&#12356;&#12363;</pr></trd>
  </trdgrp>
  <trd lng="nl">pottenbakker</trd>
  <trd lng="pl">garncarz</trd>
  <trd lng="sk">hrn&ccaron;iar</trd>
</drv>

<drv mrk="pot.0farado">
  <kap><tld/>farado</kap>
  <snc mrk="pot.0farado.VAZ">
    <dif>
      Farado de <tld/>oj, precipe per formado kaj bakado de argilo:
      <ekz>
        somere estas organizataj tie
        <ctl>someraj lernejoj de <tld/>farado</ctl>
        <fnt>
          <vrk>
            <url
            ref="http://94uk.bialystok.pl/Podlahia-Vojevodio-kaj-Bjalistoko/Podla--ia-Vojevodio-kaj-Bjalistoko,22,7.html">
            Podla&hcirc;ia Vojevodio kaj Bjalistoko</url>
          </vrk>
       </fnt>;
      </ekz>
      <ekz>
        <tld/>farado estas tradicia metio en <nom>Bhaktapur</nom>
        <fnt>
          <aut>Sakari Kauppinen</aut>,
          <vrk>Voja&gcirc;oj al la mondo - Nepalo
            <url ref="http://www.sakarikauppinen.com/nepal/nepalo10.html">
            Placo de potistoj en Bhaktapur</url>
          </vrk>
       </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">hrn&ccaron;&iacute;&rcaron;stv&iacute;</trd>
  <trd lng="de">T&ouml;pferei</trd>
  <trdgrp lng="en">
    <trd>pot making</trd>,
    <trd>pottery making</trd>
  </trdgrp>
  <trd lng="es">vasija de barro</trd>
  <trd lng="fr">poterie</trd>
  <trdgrp lng="ja">
    <trd>&#38518;&#22120;&#20316;&#12426;  <pr>&#12392;&#12358;&#12365;&#12389;&#12367;&#12426;</pr></trd>,
    <trd>&#38518;&#33464;  <pr>&#12392;&#12358;&#12370;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">pottenbakkerskunst</trd>
  <trd lng="pl">garncarstwo</trd>
  <trd lng="sk">hrn&ccaron;iarstvo</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_g;&c_o;&c_t;&c_o;&c_v;&c_l;&c_je;&c_n;&c_n;&c_ja; &c_p;&c_o;&c_s;&c_u;&c_d;&c_u;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

</art>
