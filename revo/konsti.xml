<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konsti.xml,v 1.19 2023/12/03 09:58:56 revo Exp $">
<kap>
  <rad>konstip</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konsti.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Ka&ubreve;zi mallakson al iu:
      <ekz>
        incitaj nutroj <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_z;&c_a;&c_p;&c_o;&c_r;</ind></trd>
  <trd lng="de">verstopfen</trd>
  <trd lng="fr">constiper</trd>
  <trd lng="hu">sz&eacute;kreked&eacute;st okoz</trd> 
  <trd lng="ja">&#x4FBF;&#x79D8;&#x3055;&#x305B;&#x308B;  <pr>&#x3079;&#x3093;&#x3074;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>verstoppen<klr tip="ind"> (constipatie
    veroorzaken)</klr></trd>,
    <trd><ind>constipatie</ind> veroorzaken</trd>
  </trdgrp>
  <trd lng="pl">zatwardzenie powodowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>constipar</trd>,
    <trd>causar <ind>pris&atilde;o</ind> de ventre</trd>
  </trdgrp> 
  <trd lng="ro">cauza constipa&#x21B;ie</trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_z;&c_a;&c_p;&c_o;&c_r;</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x4FBF;&#x79D8; <pr>bi&#xE0;nb&#xEC;</pr></trd>,
    <trd>&#x963B;&#x585E; <pr>z&#x1D4;s&#xE8;</pr></trd>,
    <trd>&#x5C50; <pr>j&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konsti.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zanta mallakson:
      <ekz>
        cidonio estas <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_ib; &c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_je; <ind>&c_z;&c_a;&c_p;&c_o;&c_r;</ind></trd>
  <trd lng="de">verstopfend</trd>
  <trd lng="fr">constip&eacute;</trd>
  <trd lng="hu">sz&eacute;kreked&eacute;st okoz&oacute;</trd>  
  <trd lng="ja">&#x4FBF;&#x79D8;&#x3092;&#x5F15;&#x304D;&#x8D77;&#x3053;&#x3059;  <pr>&#x3079;&#x3093;&#x3074;&#x3092;&#x3072;&#x304D;&#x304A;&#x3053;&#x3059;</pr></trd>
  <trdgrp lng="nl">
    <trd>stoppend</trd>,
    <trd>constiperend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cierpi&aogonek;cy na zaparcie</trd>,
    <trd>zaambarasowany</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>care sufer&abreve; de constipa&#x21B;ie</trd>,
    <trd>jenat</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_z;&c_a;&c_p;&c_o;&c_r;</ind></trd>
</drv>

<drv mrk="konsti.0o">
  <kap>
    <tld/>o, 
    <var><kap><tld/>eco</kap></var>
  </kap>
  <snc>
    <dif>
      Malfacilo feki; <ref tip="sin" cel="laks.mal0o">mallakso</ref>:    
      <ekz>
        <klr>[la]</klr> sep-jara infano eble suferas pro <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2010/010045.php">Tre sukcesa komika dramo</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        en apudaj &ccirc;eloj de necesejo <klr>[ili]</klr>, iom ebriaj, provas liberigi sin de
        obstina <tld/>eco
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>    
    </dif>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Verstopfung</trd>
  <trd lng="fr">constipation</trd>
  <trdgrp lng="hu">
    <trd>szorul&aacute;s</trd>,
    <trd>sz&eacute;kreked&eacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x4FBF;&#x79D8;  <pr>&#x3079;&#x3093;&#x3074;</pr></trd>
  <trd lng="nl">constipatie</trd>
  <trdgrp lng="pl">
    <trd>zaparcie</trd>,
    <trd>zatwardzenie</trd>,
    <trd>obstrukcja</trd>
  </trdgrp>
  <trd lng="ro">constipa&#x21B;ie</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FBF;&#x79D8; <pr>bi&#xE0;nm&#xEC;</pr></trd>,
    <trd>&#x4FBF;&#x79D8; <pr>bi&#xE0;nb&#xEC;</pr></trd>
  </trdgrp>
</drv>



<drv mrk="konsti.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Ekhavi mallakson:
      <ekz>
        fine ili plu ne povis elteni la suferon de <tld/>i&gcirc;o, kaj levante la posta&jcirc;on
        petis sian patrinon bastonete elfosi la ferbulan feka&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="&Monato;/2012/010573.php">La biciklo de mia patro</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_r;&c_e;&c_c;&c_mol; &c_n;&c_a; <ind>&c_z;&c_a;&c_p;&c_o;&c_r;</ind></trd>,
    <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_l;&c_a; <klr tip="amb">(&c_p;&c_r;&c_a; &c_z;&c_a;&c_p;&c_o;&c_r;)</klr></trd>
  </trdgrp>
  <trd lng="de">unter Verstopfung leiden</trd>
  <trd lng="fr">se <ind>constiper</ind></trd>
  <trdgrp lng="hu">
    <trd>szorul&aacute;sa van</trd>,
    <trd>sz&eacute;kreked&eacute;se van</trd>
  </trdgrp>
  <trd lng="ja">&#x4FBF;&#x79D8;&#x3059;&#x308B;  <pr>&#x3079;&#x3093;&#x3074;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">aan <ind>verstopping</ind> lijden</trd>
  <trd lng="pl">zaprze&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; <ind>&c_z;&c_a;&c_p;&c_o;&c_r;&c_o;&c_m;</ind></trd>
</drv>
</art>
