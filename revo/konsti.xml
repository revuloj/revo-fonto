<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konstip</rad>/i
</kap>
<drv mrk="konsti.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ka&ubreve;zi la mallakson; mallaksigi:
      <ekz>
        incitaj nutroj <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verstopfen</trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_p;&c_o;&c_r;</trd>
</drv>

<drv mrk="konsti.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zanta la mallakson:
      <ekz>
        cidonio estas <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verstopfend</trd>
<trdgrp lng="nl">
    <trd>stoppend</trd>,
    <trd>constiperend</trd>
  </trdgrp>
</drv>

<drv mrk="konsti.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i kaj la rezulto de tiu ago.
    </dif>
    <ref tip="sin" cel="laks.mal0o">mallakso</ref>    
  </snc>
  <trd lng="de">Verstopfen</trd>
  <trd lng="nl">constipatie</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_r;</trd>
</drv>

<drv mrk="konsti.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc>
    <dif>
      Malfacilo feki.
    </dif>
  </snc>
  <trd lng="de">Verstopfung</trd>
  <trd lng="nl">verstopping</trd>
<trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_r;</trd>
</drv>

<drv mrk="konsti.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Esti mallaksa.
    </dif>
  </snc>
  <trd lng="de">unter Verstopfung leiden</trd>
<trd lng="nl">aan <ind>verstopping</ind> lijden</trd>
</drv>
</art>
