<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blond.xml,v 1.36 2023/10/29 19:11:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>blond</rad>/a
</kap>
<drv mrk="blond.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="blond.0a.hararo">
    <dif>
    <klr>(pri hararo)</klr>
    Havanta <ref tip="super" cel="flav.0a">flavan</ref> <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> mezan inter
    <ref tip="super" cel="or.0a">ora</ref> kaj helbruna, kiel la <ref cel="har.0aro">haroj</ref>
    de iuj nordaj popoloj:
    <ekz>
      <tld/>a infano
      <fnt><bib>FK</bib>,
        <lok>4, rakontoj, la forgesita pipo, rakonto
        de A. Edelmann, tradukis I. Kaminski</lok>
      </fnt>;
    </ekz>
    <ekz>
      akvo fluis el liaj longaj <tld/>aj haroj
      <fnt>
        <bib>Fab1</bib>
        <lok>Malbonkonduta knabo</lok>
      </fnt>;
    </ekz>
    <ekz>
      bela knabino kun <tld/>aj bukloj
      <fnt>
        <bib>Fab2</bib>
        <lok>Avineto</lok>
      </fnt>;
    </ekz>
    <ekz>
      ore <tld/>aj lipharoj
      <fnt>
        <bib>IK</bib>
      </fnt>;
    </ekz>
    <ekz>
      li havis <tld/>an ondumitan hararon
      <fnt>
        <aut>Spomenka &Scirc;timec</aut>,
        <vrk>Morto en Melburna tramo</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1997a, numero 8a, p. 24a</lok>
      </fnt>.
      </ekz>
    </dif>     
    <trd lng="ar">&a_sh;&a_q;&a_r;&a_A;&a_hamza;</trd>
    <trd lng="br">melen<klr tip="ind"> (blev)</klr></trd>
    <trd lng="ca">ros</trd>
    <trd lng="eu">horail</trd>
    <trd lng="fi">vaaleaverinen</trd>
    <trd lng="ga">foltbhu&iacute;</trd>
    <trd lng="gl">louro</trd>
    <trd lng="gv">fynn</trd>
    <trd lng="hu">sz&odblac;ke</trd>
    <trd lng="is">lj&oacute;sh&aelig;r&#xF0;ur</trd>
    <trd lng="it">biondo</trd>
    <trd lng="no">blond</trd>
    <trdgrp lng="pl">
      <trd>blond</trd>,
      <trd>p&lstroke;owy</trd>,
      <trd>jasnow&lstroke;osy</trd>
    </trdgrp>
    <trd lng="pt">louro</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_l;&c_o;&c_k;&c_u;&c_r;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_o;&c_r;&c_u;&c_s;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_o;&c_v;&c_o;&c_l;&c_o;&c_s;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="uk">&c_b;&c_ib;&c_l;&c_ja;&c_v;&c_i;&c_j;</trd>      
    <trdgrp lng="zh">
    <trd>&#x91D1;&#x53D1; <pr>j&#x12B;nf&#x1CE;</pr></trd>,
    <trd>&#x91D1;&#x9EC3;&#x8272; <pr>j&#x12B;nhu&#xE1;ngs&#xE8;</pr></trd>,
    <trd>&#x91D1;&#x9EC3; <pr>j&#x12B;nhu&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="blond.0a.biero">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(pri kelkaj a&jcirc;oj)</klr>
      Helflava:
      <ekz>
        &gcirc;i estas la lasta modo... &dash; Valencienoj, Alensonoj, Brugoj, Imita&jcirc;oj, 
        <tld lit="B"/>a&jcirc;oj, Iluzioj...
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj bieroj de alta fermentado
        <fnt>
        <aut>Lode Van De Velde</aut>,
          <vrk>Bierlando Belgio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 1a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>jasny<klr> (o piwie)</klr></trd>,
      <trd>p&lstroke;owy<klr> (wyblak&lstroke;y)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_l;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_y;</trd>
  <trd lng="bg">&c_r;&c_u;&c_s;&c_o;&c_k;&c_o;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>plavovlas&yacute;</trd>,
    <trd>sv&ecaron;tlovlas&yacute;</trd>
  </trdgrp>
  <trd lng="de">blond</trd>
  <trd lng="en">blond</trd>
  <trd lng="es">rubio</trd>
  <trd lng="fr">blond</trd>
  <trd lng="he">&bet;&#x5BC;&#x5B0;&lamed;&vav;&#x5B9;&nun;&dalet;&#x5B4;&yod;&nun;&#x5B4;&yod;&tav;</trd>
  <trd lng="id">blonde</trd>
  <trd lng="io">blonda</trd>
  <trd lng="ja">&#x30D6;&#x30ED;&#x30F3;&#x30C9;&#x306E;  <pr>&#x3076;&#x308D;&#x3093;&#x3069;&#x306E;</pr></trd>
  <trd lng="nl">blond</trd>
  <trd lng="ro">blond</trd>
  <trdgrp lng="sk">
    <trd>blond</trd>,
    <trd>svetlovlas&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="blond.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      <tld lit="B"/>hara homo:
      <ekz>
        juna <tld/>ulo kun melankolia senmova viza&gcirc;o
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u li estas <tld/>ulo a&ubreve; brunulo
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 2a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_b;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_n;</trd>
  </trdgrp>
  <trd lng="br">melegan</trd>
  <trd lng="cs">blond&yacute;n</trd>
  <trd lng="de">Blondine</trd>
  <trd lng="en">blond</trd>
  <trd lng="fr">blond<klr tip="amb"> (personne blonde)</klr></trd>
  <trd lng="io">blondo</trd>
  <trd lng="nl">blonde<klr tip="ind"> (blond persoon)</klr></trd>
  <trd lng="pl">blondyn<klr> {m}</klr></trd>
  <trd lng="ru">&c_b;&c_l;&c_o;&c_n;&c_d;&c_i;&c_n;</trd>
  <trd lng="sk">blond&iacute;n</trd>
  <trd lng="uk">&c_b;&#x43B;&#x43E;&#x43D;&c_d;&c_i;&#x43D;</trd>
</drv>

<drv mrk="blond.0ulino">
  <kap><tld/>ulino</kap>
  <snc>
    <dif>
      Inseksa <tld/>ulo:
      <ekz>
        <tld/>ulino, fre&scirc;a kaj rozkolora kiel printempo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        dekkvinjara knabineto, maldika, anemia <tld/>ulino
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la edzino ferias, dum vi pasigas la tempon kun <tld/>ulino
        <fnt><bib>SatirRak</bib>,
        <lok>la fluidumo de la &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_a;&c_ja; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_b;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_l;&c_o;&c_n;&c_d;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">meleganez</trd>
  <trdgrp lng="cs">
    <trd>blond&yacute;na</trd>,
    <trd>plavovl&aacute;ska</trd>
  </trdgrp>
  <trd lng="de">Blondine</trd>
  <trd lng="en">blonde</trd>
  <trd lng="fr">blonde<klr tip="amb"> (femme blonde)</klr></trd>
  <trd lng="he">&bet;&#x5BC;&#x5B0;&lamed;&vav;&#x5B9;&nun;&dalet;&#x5B4;&yod;&nun;&#x5B4;&yod;&tav;</trd>
  <trd lng="io">blondino</trd>
  <trd lng="ja">&#x30D6;&#x30ED;&#x30F3;&#x30C9;&#x5973;  <pr>&#x30D6;&#x30ED;&#x30F3;&#x30C9;&#x304A;&#x3093;&#x306A;</pr></trd>
  <trd lng="nl">blondine</trd>
  <trdgrp lng="pl">
    <trd>blondynka<klr> {f}</klr></trd>,
    <trd>blondynka<klr> {f} (pot., wulg.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_o;&c_n;&c_d;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">blond&iacute;nka</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43B;&#x43E;&#x43D;&c_d;&c_i;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&#x44F;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
