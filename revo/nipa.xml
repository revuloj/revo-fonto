<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nipa.xml,v 1.10 2022/05/01 16:20:54 revo Exp $">
<kap>
  <rad>nipa</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="nipa.0o">
  <kap><tld/>o</kap>
  <snc mrk="nipa.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de <ref tip="lst" lst="voko:arekoparencoj" cel="palm.0oj">palmoj</ref>
      kun unu specio (<trd lng="la">Nypa
      fruticans</trd>), kreskanta en mar&ccirc;oj de Banglade&scirc;o &gcirc;is
      Sudorientazio, kun diversaj uzoj kiel materialo kaj man&gcirc;a&jcirc;o:
      <ekz>
        mi eniris en la domon el <tld/>palmo por saluti la klanestron
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;la perdita gusto de
        Pinatubo&rightquot;, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/0e/Nypa_041117-024_krb.jpg/677px-Nypa_041117-024_krb.jpg"
      prm="CC BY-SA 4.0"
      >fruktaro kaj folibazoj de <tld/>o, Botanika &Gcirc;ardeno Bogor, Okcidenta
        Javo, Indonezio
        <fnt>
          <aut>Wie146, propra verko</aut>
          <lok><url ref="&WCW;/File:Nypa_041117-024_krb.jpg"
              >&WCW;/File:Nypa_041117-024_krb.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Nipapalme</trd>
  <trd lng="fr">nipa</trd>
  <trdgrp lng="hu">
    <trd>mangrovep&aacute;lma</trd>,
    <trd>nipap&aacute;lma</trd>
  </trdgrp>
  <trd lng="nl">mangrove</trd>
  <trd lng="pl">nipa krzewinkowa</trd>
  <trd lng="pt">nipa</trd>
  <trd lng="ro">nipa</trd>
</drv>
</art>

