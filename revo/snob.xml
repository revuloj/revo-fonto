<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: snob.xml,v 1.15 2022/03/27 17:06:36 revo Exp $">
<kap>
  <rad>snob</rad>/o
</kap>
<drv mrk="snob.0o">
  <kap><tld/>o</kap>
  <snc mrk="snob.0o.KOMUNE">
    <dif>
      Homo, kiu sendistinge imitas &ccirc;ion modan a&ubreve; altklasan, kaj sentigas
      malestimon al aliaj aferoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt.0ulo">afektulo</ref>,
      <ref cel="dand.0o">dando</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_n;&c_o;&c_b;</trd>
  <trd lng="ca">esnob</trd>
  <trdgrp lng="cs">
    <trd>pan&aacute;k</trd>,
    <trd>pov&yacute;&scaron;enec</trd>,
    <trd>snob</trd>,
    <trd>v&yacute;st&rcaron;edn&iacute; mu&zcaron;</trd>
  </trdgrp>
  <trd lng="de">Snob</trd>
  <trd lng="es">esnob</trd>
  <trd lng="fr">snob</trd>
  <trd lng="hu">sznob</trd>
  <trd lng="nl">snob</trd>
  <trd lng="ru">&c_s;&c_n;&c_o;&c_b;</trd>
  <trdgrp lng="sk">
    <trd>pov&yacute;&scaron;ene</trd>,
    <trd>snob</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43D;&#x43E;&#x431;</trd>,
    <trd>&#x444;&#x440;&#x430;&#x43D;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="snob.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="snob.0i.KOMUNE">
    <dif>
      Afekti superecon kontra&ubreve; aliuloj:
      <ekz>
        oni <tld/>as aliajn homojn per la kulto de la <ctl>&gcirc;usta
        vorto</ctl>
        <fnt>
        <aut>M. M. Campagna</aut>,
        <vrk>Re: Pri la cirkulero de R Eichholz</vrk>,
        <lok>soc.culture.esperanto, 1993-12-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">snober</trd>
</drv>

<drv mrk="snob.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="snob.0ismo.KOMUNE">
    <dif>
      <tld lit="S"/>a kutima sinteno:
      <ekz>
        <tld/>ismo eble estas influa fenomeno en la brita kulturo
        <fnt>
        <aut>M. Rauhamaa</aut>,
        <vrk>Re: Nomoj de lingvoj</vrk>,
        <lok>soc.culture.esperanto, 1996-05-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_n;&c_a;&c_b;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>pov&yacute;&scaron;enost</trd>,
    <trd>snobizmus</trd>,
    <trd>snobstv&iacute;</trd>
  </trdgrp>
  <trd lng="fr">snobisme</trd>
  <trdgrp lng="sk">
    <trd>pov&yacute;&scaron;enos&tcaron;</trd>,
    <trd>snobizmus</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x43D;&#x43E;&#x431;&#x456;&#x437;&#x43C;</trd>
</drv>
</art>
