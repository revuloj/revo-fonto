<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fabi.xml,v 1.6 2022/03/18 13:01:59 revo Exp $">
<kap>
  <rad>Fabi</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fabi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fabi.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o promenas anta&ubreve; la palaco kovrita &gcirc;is la
        okuloj per ru&gcirc;a 
        mantelo
        <fnt>
          <aut>J. Benavente, trad. V. Inglada</aut>,
          <vrk><url ref="http://www.delbarrio.eu/servanto.htm">
          La servanto de Don Juan</url></vrk>,
          <lok>[1913]</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tobusisto <tld/>o Nascimento diris al AFP, ke 15
        maskitaj 
        viroj, armitaj per pafiloj kaj pistoloj, ordonis al li forlasi 
        sian veturilon  
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.eventeo.net/web/">
          Bataloj inter polico kaj narkotistoj en Rio</url></vrk>,
          <lok>eventeo, 2009-10-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Fabi</trd>
    <trdgrp lng="fr">
      <trd>Fabio</trd>,
      <trd>Fabius</trd>
    </trdgrp>
    <trd lng="it">Fabio</trd>
    <trd lng="pl">Fabiusz</trd>
  </snc>
  <trd lng="uk">&#x444;&#x430;&#x431;&#x456;&#x439; (&#x456;&#x43C;&#x2019;&#x44F;)</trd>
</drv>
</art>

