<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kamomil</rad>/o
</kap>
<drv mrk="kamomi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Matricaria recutita</trd>), herbo kies floroj
      estas uzataj en medicino.
    </dif>
  </snc>
  <bld lok="../bld/kamomil.jpg">
    vera <tld/>o
  </bld>
  <trd lng="en">chamomile</trd>
  <trd lng="de">Kamille</trd>
</drv>
</art>
