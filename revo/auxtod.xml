<kap>
  <rad>a&ubreve;todafe</rad>/o
</kap>

<drv mrk="auxtod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Punbruligo de <ref cel="herez.0ulo">herezulo</ref>j a&ubreve; tiaj libroj:
      <ekz>
        en la <tld/>o okazinta en tiu urbo la tagojn 7-a kaj 8-a de novembro 1610
        <klr>[&#x2026;]</klr> ses personoj kiuj rezistis estis vive bruligataj kaj kvin
        postmorte
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=535444">Sor&ccirc;istinoj de Zugarramurdi</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> nazia Germanujo <klr>[&#x2026;]</klr> okazis multaj <tld/>oj, &ccirc;efe de
        libroj de a&ubreve;toroj judaj, komunismaj, ktp
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20239">Nazia Germanio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_d;&c_a;&c_f;&c_e;</trd>
  <trd lng="ca">acte de fe</trd>
  <trdgrp lng="de">
    <trd>Autodaf&eacute;</trd>,
    <trd>B&uuml;cherverbrennung</trd>,
    <trd>Verbrennung auf dem <ind>Scheiterhaufen</ind></trd>
  </trdgrp>
  <trd lng="en">burning at the stake</trd>
  <trd lng="fr">autodaf&eacute;</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;glyahal&aacute;l</trd>,
    <trd>autodaf&eacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>autodaf&eacute;</trd>,
    <trd>sermo generalis</trd>
  </trdgrp>
  <trd lng="nl">autodaf&eacute;</trd>
  <trdgrp lng="pl">
    <trd>palenie na stosie</trd>,
    <trd>auto-da-f&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_u;&c_t;&c_o;&c_d;&c_a;&c_f;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_zh;&c_zh;&c_je;&c_n;&c_i;&c_je;
    &c_n;&c_a; &c_k;&c_o;&c_s;&c_t;&c_r;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x432;&#x442;&#x43E;&#x434;&#x430;&#x444;&#x435;</trd>,
    <trd>&#x436;&#x438;&#x432;&#x43E;&#x441;&#x43F;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x441;&#x43F;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; &#x43D;&#x430; &#x432;&#x43E;&#x433;&#x43D;&#x438;&#x449;&#x456;</trd>
  </trdgrp>
</drv>
</art>
