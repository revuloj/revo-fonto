<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      -<rad>oz</rad> <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="oz.0">
      <kap>-<tld/></kap>
      <gra><vspec>sufikso</vspec></gra>
      <snc>
        <dif>
          Neoficiala sufikso kun la signifo
          <ctl>ri&ccirc;a je, plena je, havanta multe da ...</ctl>:
          <ekz>
            tuj aliris juna ridetanta muskol<tld/>a kampulo en nova
            surtuto kun lar&gcirc;a verda zono
            <fnt>
              <aut>L. Tolstoj, tr. V. Sapo&jcirc;nikov</aut>:
              <vrk>Resurekto</vrk>.
              <lok><url
              ref="http://esperanto.org/Ondo/Rn-tlst2.htm">Parto 1&#170;,
              &ccirc;ap. 15&#170;</url></lok>,
              <bib>LOdE</bib>
            </fnt>;
          </ekz><ekz>
            kaj vi, pa&ccirc;jo, anka&ubreve; estas bela, nur har<tld/>a!
            <fnt><bib>Metrop</bib></fnt>;
          </ekz><ekz>
            lia maljuna frunto estas sulketita kaj nod<tld/>a
            <fnt>
              <aut>Tolkien, tr. W. Auld</aut>:
              <vrk>La du turegoj</vrk>.
              <lok><url
              ref="http://esperanto.org/Ondo/Tolkien/Tolkien3.htm"
                  >Libro 3&#170;, &ccirc;ap. 4&#170;</url></lok>
            </fnt>;
          </ekz><ekz>
            gisa&jcirc;o, por<tld/>a, plej ofte fendita kaj etvalora
            <fnt>
              <aut>Szil&aacute;gyi</aut>: <vrk>Vespera ru&gcirc;o anoncas
              ventegon</vrk>
            </fnt>;
          </ekz><ekz>
            kelkaj &ccirc;uk&ccirc;oj <klr>...</klr> prenis la malsanulon kaj
            veturigis lin for
            en la patrinan sinon de la mister<tld/>a Tundro
            <fnt>
              <aut>V. Ero&scirc;enko</aut>:
              <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>,
              <lok><url
              ref="http://www.esperanto.mv.ru/Cxukcxoj/cxukcx3.html"
                  >La neatendita bombeksplodo</url></lok>
            </fnt>;
          </ekz><ekz>
            terure na&ubreve;zas min lia grasa ventrego, liaj &scirc;vit<tld/>aj
            manoj,
            lia ra&ubreve;ka vo&ccirc;o<klr>...</klr>
            <fnt>
              <aut>Vina&rcaron;</aut>: <vrk>La skandalo pro Jozefo</vrk>
            </fnt>;
          </ekz><ekz>
            malsupre &scirc;anceli&gcirc;is ru&gcirc;aj &scirc;ton<tld/>aj mont&ccirc;enoj,
            sukplenaj verdaj pinoj<klr>...</klr>
            <fnt><bib>Metrop</bib></fnt>;
          </ekz><ekz>
            ni sidi&gcirc;is rekte sur sabl<tld/>an teron
            <fnt>
              <aut>V. Glazunov</aut>,
              <vrk>Belulino Ajsul&uacute;</vrk>,
              <lok>La sfinkso de Steplando, 1988</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">reich an ...</trd>
      <trdgrp lng="fr">
        <trd>-eux<klr> (abondant en...)</klr></trd>,
        <trd>-u<klr> (abondant en...)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>-<ind>tartalm&uacute;</ind></trd>,
        <trd>-<ind>d&uacute;s</ind></trd>,
        <trd>-ban <ind>gazdag</ind></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>-oso (&ocirc;)</trd>,
        <trd>-osa (&ocirc;)</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>-&c_a;&c_t;&c_y;&c_j;<klr> (&c_o;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
        <trd>-&c_i;&c_s;&c_t;&c_y;&c_j;<klr> (&c_o;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>
      </trdgrp>
    </drv>
  </art>

