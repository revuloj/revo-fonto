<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: serb.xml,v 1.16 2022/08/05 10:33:41 revo Exp $">
<kap>
  <rad>serb</rad>/o
  <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="serb.0o">
  <kap><tld/>o</kap>
  <snc mrk="serb.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="serb.S0ujo"><tld lit="S"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="serb.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        <tld lit="S"/>o el <nom>Zemlin</nom>, publikigis pazilalion kaj
        pazigrafion
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en tiu regiono lo&gcirc;as plimulte bosnaj <tld/>oj, sed dum jarcentoj
        ili vivis kiel
        najbaroj de islamanoj kaj kroatoj kaj ili alkutimi&gcirc;is al
        diverseco kaj kuna
        vivo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Steloj;monato2/006811.html">Reago:
          Nekredeblaj malveroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>okroata lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Steloj;monato2/006815.html">Opinio:
          &Ccirc;u nova &scirc;anco por malfortaj lingvoj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_r;&c_b;</trd>
  <trd lng="cs">Srb</trd>
  <trd lng="de">Serbe</trd>
  <trd lng="en">Serbian</trd>
  <trd lng="fr">Serbe<klr> (subst.)</klr></trd>
  <trd lng="nl">Servi&euml;r</trd>
  <trd lng="pt">s&eacute;rvio <klr tip="ind"> (subst.)</klr></trd>
  <trd lng="sk">Srb</trd>
  <trd lng="uk">&c_s;&c_je;&c_r;&c_b;</trd>
</drv>

<drv mrk="serb.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>aj terora&jcirc;oj en Kosovo
        <fnt>
          <bib>Monato</bib>
          <aut>Douglas Draper</aut>
          <vrk><url ref="&Steloj;monato2/006834.html">Ne&ubreve;trala</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tute malaperis de la librejaj bretoj la titolo &leftquot;<tld/>okroata&rightquot;,
        sed abundas
        verkoj pri la <tld/>a, la kroata kaj la bosnia, nun rigardataj
        kaj traktataj
        kiel apartaj lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Steloj;monato2/006815.html">Opinio:
          &Ccirc;u nova &scirc;anco por malfortaj lingvoj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_r;&c_b;&c_s;&c_k;&c_ib;</trd>
  <trd lng="cs">srbsk&yacute;</trd>
  <trd lng="de">serbisch</trd>
  <trd lng="en">Serbian</trd>
  <trd lng="fr">serbe<klr> (adj.)</klr></trd>
  <trd lng="nl">Servisch</trd>
  <trd lng="pt">s&eacute;rvio <klr tip="ind"> (adj.)</klr></trd>
  <trd lng="sk">srbsk&yacute;</trd>
  <trd lng="uk">&c_s;&c_je;&c_r;&c_b;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="serb.S0ujo">
  <kap>
    <tld lit="S"/>ujo
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">CS</mlg>
  <snc mrk="serb.S0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la suda <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo <ref tip="prt" cel="beograd.B0o">Beogrado</ref>;
      teritorio de Respubliko <tld lit="S"/>ujo, limanta kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="bosn.B0ujoHercegovino">Bosnujo kaj
      Hercegovino</ref>,
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="kosov.K0o.GEOG">Kosovo</ref>,
      <ref tip="vid" cel="kroat.K0ujo.GEOG">Kroatujo</ref>,
      <ref tip="vid" cel="makedon.NordM0ujo.GEOG">Nord-Makedonujo</ref>
      kaj <ref tip="vid" cel="ruman.R0ujo.GEOG">Rumanujo</ref>:
      <ekz>
        en Kroatujo aperis anta&ubreve; la Unua mondmilito Kroata Esperantisto,
        La Zemuna
        Esperantisto kaj La Zenuma Stelo kaj, en <tld lit="S"/>ujo, <tld lit="S"/>a
        Esperantisto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16266">Frua
          Esperanto-gazetaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_S;&c_e;&c_r;&c_b;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_S;&c_malmol;&c_r;&c_b;&c_i;&c_ja;</trd>
    <trd lng="de">Serbien</trd>
    <trd lng="en">Serbia</trd>
    <trd lng="fa">&a_S;&a_r;&a_b;&a_s;&a_t;&a_A;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>Serbie</trd>,
      <trd>R&eacute;publique de Serbie</trd>
    </trdgrp>
    <trd lng="id">Serbia</trd>
    <trd lng="it">Serbia</trd>
    <trd lng="nl">Servi&euml;</trd>
    <trdgrp lng="pl">
      <trd>Serbia</trd>,
      <trd>Republika <ind>Serbii</ind></trd>
    </trdgrp>
    <trd lng="pt">S&eacute;rvia</trd>
</drv>

</art>

