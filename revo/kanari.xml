<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kanari</rad>/o
</kap>
<drv mrk="kanari.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fring.0edoj">fringedoj</ref>
      (<trd lng="la">Serinus</trd>), kies unu specio vivas sur
      Kanariaj insuloj, kun diversflava plumaro kaj kun tipa
      trila kantado, ofte tenata en ka&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kanarienvogel</trd>,
    <trd>Girlitz</trd>
  </trdgrp>
  <trd lng="en">canary</trd>
</drv>
</art>
