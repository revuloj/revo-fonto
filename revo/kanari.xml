<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kanari</rad>/o
</kap>
<drv mrk="kanari.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Serinus</trd>), kies unu specio vivas sur
      Kanariaj insuloj, kun diversflava plumaro kaj kun tipa
      trila kantado, ofte tenata en ka&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_r;&c_e;&c_j;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Kanarienvogel</trd>,
    <trd>Girlitz</trd>
  </trdgrp>
  <trd lng="en">canary</trd>
  <trd lng="es">canario</trd>
  <trdgrp lng="fr">
    <trd>serin</trd>,
    <trd>canari</trd>
  </trdgrp>
  <trd lng="hu">kan&aacute;ri</trd>
  <trd lng="nl">kanarie</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_r;&c_je;&c_j;&c_k;&c_a;</trd>
</drv>
</art>
