<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>c</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="c.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a afrikata
      <ref tip="dif" cel="dental.0o">dentalo</ref> [ts].
    </dif>
  <trd lng="fr">c<klr> (consonne)</klr></trd>
  </snc>
  <snc>
     <dif>
      La tria <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto.
     </dif>
     <trd lng="fr">c<klr> (lettre)</klr></trd>
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="c.0.nomo">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>    
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
    <trd lng="fr"><klr>(n'existe pas)</klr></trd>
   </snc>
  </drv>
</subart>
</art>

<!--
$Log$
-->
</vortaro>