<drv mrk="tru.0a">
  <kap><tld/>a, <var><kap><tld/>hava</kap></var></kap>
  <snc>
    <dif>
      <tld lit="T"/>ita, <tld/>plena:
      <ekz>
        envolvita en <tld/>an tukon;
      </ekz>
      <ekz>
        el la <tld/>aj plandoj estis vidata la pajla substerno
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kisus, mia feino, viajn piedetojn... en <tld/>aj &scirc;uoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>havaj putoj, kiuj ne povas teni akvon
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 2:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu laborenspezas monon, tiu laborenspezas por <tld/>hava saketo
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;agaj 1:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ajxur.0a">a&jcirc;ura</ref>,
      <ref cel="cxifon.0o">&ccirc;ifono</ref>,
      <ref cel="krad">krada</ref>,
      <ref cel="punt">punta</ref>,
      <ref cel="bor">traborita</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_k;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">foradat</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;rav&yacute;</trd>,
    <trd>d&ecaron;rovan&yacute;</trd>,
    <trd>prod&ecaron;rav&ecaron;l&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>l&ouml;cherig</trd>,
    <trd>verl&ouml;chert</trd>
  </trdgrp>
  <trd lng="fr">trou&eacute;</trd>
  <trd lng="he">&mem;<ind>&het;&vav;&resh;&resh;</ind></trd>
  <trd lng="hu">lyukas</trd>
  <trdgrp lng="it">
    <trd>forato</trd>,
    <trd>bucato</trd>
  </trdgrp>
  <trd lng="nl">vol <ind>gaten</ind></trd>
  <trdgrp lng="pl">
    <trd>dziurawy</trd>,
    <trd>perforowany</trd>
  </trdgrp>
  <trd lng="pt">esburacado</trd>
  <trd lng="ru">&c_d;&c_y;&c_r;&c_ja;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">prederaven&yacute;</trd>
  <trd lng="vo">hogik</trd>
</drv>

<drv mrk="tru.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tru.0i.KOMUNE">
    <dif>
      Provizi per <tld/>o a&ubreve; <tld/>oj:
      <ekz>
        la domoj <klr>[&#x2026;]</klr> similis blankajn kubojn, <tld/>itajn en
        la lokoj, kie
        estis la fenestroj kaj pordoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la muroj estas <tld/>itaj per kanaloj, dank' al kiuj
        <klr>[&#x2026;]</klr> oni povas
        a&ubreve;di, kion oni diras en la subtera eta&gcirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>itaj kvaza&ubreve; spongo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i <klr>[&#x2026;]</klr> oficejopaperon A4, ekzistas
        <tld/>iloj kun 2 a&ubreve; 4
        <tld/>ilaj stangetoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=261884">Truilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>foradar</trd>,
    <trd>perforar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&iacute;rkovat</trd>,
    <trd>d&ecaron;rovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>lochen</trd>,
    <trd>durchl&ouml;chern</trd>,
    <trd><klr>ein </klr>Loch machen</trd>,
    <trd><klr>ein </klr>Loch <ind>bohren</ind></trd>
  </trdgrp>
  <trd lng="fr">trouer</trd>
  <trd lng="he">&lamed;<ind>&het;&vav;&resh;&resh;</ind></trd>
  <trdgrp lng="hu">
    <trd>lyukaszt</trd>,
    <trd>kilyukaszt</trd>
  </trdgrp>
  <trd lng="id">me<ind>lubang</ind>i</trd>
  <trdgrp lng="it">
    <trd>forare</trd>,
    <trd>bucare</trd>
  </trdgrp>
  <trd lng="nl">een <ind>gat</ind> maken</trd>
  <trdgrp lng="pl">
    <trd>dziurawi&cacute;</trd>,
    <trd>dziurkowa&cacute;</trd>,
    <trd>perforowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furar</trd>,
    <trd>esburacar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_r;&c_ja;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_y;&c_r;&c_ja;&c_v;&c_i;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; derav&yacute;</trd>
  <trd lng="vo">hog&ouml;n</trd>
</drv>

<drv mrk="tru.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tru.tra0i.KOMUNE">
    <dif>
      <tld lit="T"/>i de unu flanko al alia:
      <ekz>
        generalo Mulan <klr>[&#x2026;]</klr> je unu pafo tra<tld/>is la rokegon
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk>
          <url ref="http://esperanto.cri.cn/1/2006/11/01/1@47734.htm">
          Belaj vidinda&jcirc;oj...</url></vrk>,
          <lok>2006-11-01</lok>
        </fnt>;
      </ekz>
      <ekz>
        nova turopordo kun levponto estas tra<tld/>ita en la
        &ccirc;irka&ubreve;remparo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=265999">Kastelo
