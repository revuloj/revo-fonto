<drv mrk="tru.0a">
  <kap><tld/>a, <var><kap><tld/>hava</kap></var></kap>
  <snc>
    <dif>
      <tld lit="T"/>ita, <tld/>plena:
      <ekz>
        envolvita en <tld/>an tukon;
      </ekz>
      <ekz>
        el la <tld/>aj plandoj estis vidata la pajla substerno
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kisus, mia feino, viajn piedetojn... en <tld/>aj &scirc;uoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>havaj putoj, kiuj ne povas teni akvon
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 2:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu laborenspezas monon, tiu laborenspezas por <tld/>hava saketo
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;agaj 1:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ajxur.0a">a&jcirc;ura</ref>,
      <ref cel="cxifon.0o">&ccirc;ifono</ref>,
      <ref cel="krad">krada</ref>,
      <ref cel="punt">punta</ref>,
      <ref cel="bor">traborita</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_k;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
