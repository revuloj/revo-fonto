<drv mrk="tru.buton0o">
  <kap>buton<tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <klr>(&ccirc;e vesto)</klr>
      Fendo, en kiun oni enigas <ref cel="buton.0o.TEKS">butonon</ref>:
      <ekz>
        lia ornamo konsistis en bukedo da violoj, kiun li portis en la
        buton<tld/>o
        <fnt><bib>Fab4</bib>, <lok>Dekdu per la po&scirc;to</lok></fnt>;
      </ekz>
      <ekz>
        oni tuj donas al vi la Vladimiran ordenon en la buton<tld/>on
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">knofl&iacute;kov&aacute; d&iacute;rka</trd>
  <trd lng="de">Knopfloch</trd>
  <trd lng="fr">boutonni&egrave;re</trd>
  <trd lng="id"><ind>lubang</ind> kancing</trd>
  <trd lng="it">asola</trd>
  <trdgrp lng="pl">
    <trd>dziurka</trd>,
    <trd>butonierka <klr>(najwy&zdot;sza dziurka w klapie
    marynarki)</klr></trd>
  </trdgrp>
  <trd lng="sk">gomb&iacute;kov&aacute; dierka</trd>
</drv>

<drv mrk="tru.fum0o">
  <kap>
    fum<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="T"/>o, tra kiu eli&gcirc;as fumo:
      <ekz>
        guto da akvo, tiel granda kiel el tegmenta defluilo, nun falis
        &gcirc;uste sur la
        fum<tld/>on de la lanterno
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        tra la fumo<tld/>o de la tendo en&scirc;vebis ne&gcirc;eroj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.net/2009/009812.php">&Gcirc;ui simplajn &scirc;atokupojn sub&ccirc;iele</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rauchabzug</trd>
</drv>


