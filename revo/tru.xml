  </trdgrp>
  <trdgrp lng="en">
    <trd>butthole</trd>,
    <trd>anus</trd>,
    <trd>asshole</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ind>trou</ind> du cul<klr tip="ind"> (anus)</klr></trd>,
    <trd>anus</trd>
  </trdgrp>
  <trd lng="id">anus</trd>
  <trdgrp lng="it">
    <trd>ano</trd>,
    <trd>buco del culo<klr> (volg.)</klr></trd>
