<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tru</rad>/o
</kap>
<drv mrk="tru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tru.0o.malplenajxo">
    <dif>
      Malplena&jcirc;o, kavo en io:
      <ekz>
        <tld/>o en la tero, en muro, en vesto;
      </ekz>
      <ekz>
        bori <tld/>on en tabulo;
      </ekz>
      <ekz>
        <tld/>o de kudrilo;
      </ekz>
      <ekz>
        plenigi <tld/>on<fnt>B</fnt>;
      </ekz>
      <ekz>
        &scirc;topi, fliki <tld/>on;
      </ekz>
      <ekz>
        la flika&jcirc;o estas pli malbela ol la <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>li havas <tld/>on en la manplato</ind>
        <klr>(estas tute mal&scirc;parema)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="fr">
          <trd>c'est un <ind>panier</ind> per&ccedil;&eacute;</trd>,
          <trd>il a l'argent qui lui br&ucirc;le les <ind>doigts</ind></trd>,
          <trd>il a l'argent qui lui file entre les <ind>doigts</ind></trd>,
          <trd>il a l'argent qui lui fond dans les <ind>mains</ind></trd>,
          <trd>il a un <ind>trou</ind> au fond de la poche<klr tip="ind"> (&ecirc;tre d&eacute;pensier)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
    <adm>
      La vorto "malplenajxo", kiu servas por difini "truo" kaj "kavo", ne
      havas difinon en REVO.
      <aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="apertu.0o">aperturo</ref>,
      <ref cel="brecx.0o">bre&ccirc;o</ref>,
      <ref cel="fend.0o">fendo</ref>,
      <ref cel="kavern.0o">kaverno</ref>,
      <ref cel="kav.0o">kavo</ref>,
      <ref cel="luk.0o">luko</ref>,
      <ref cel="ferm">malferma&jcirc;o</ref>,
      <ref cel="por1.0o">poro</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;</trd>,
      <trd>&c_d;&c_z;&c_ju;&c_r;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">forat</trd>
    <trdgrp lng="fr">
      <trd>trou</trd>,
      <trd>cavit&eacute;</trd>,
      <trd>chas</trd>,
      <trd>creux</trd>,
      <trd>trou&eacute;e</trd>,
      <trd>orifice</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dziura</trd>,
      <trd>otw&oacute;r</trd>,
      <trd>luka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_y;&c_r;&c_a;</trd>,
      <trd>&c_d;&c_y;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_je;&c_r;&c_s;&c_t;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tru.0o.manko">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Manko en io:
      <ekz>
        nur en unu punkto ilia informiteco havas <tld/>on;
      </ekz>
      <ekz>
        <tld/>on de l' honoro flikos neniu tajloro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_b;&c_je;&c_l;
      <klr tip="amb">(&c_u; &c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)
      </klr></trd>,
      <trd>&c_n;&c_ja;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>trou<klr tip="ind"> (fig.)</klr></trd>,
      <trd>blanc<klr tip="ind"> (dans une conversation)</klr></trd>,
      <trd>manque</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dziura</trd>,
      <trd>luka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_y;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_je;&c_l;
      <klr tip="amb">(&c_v; &c_ch;&c_jo;&c_m;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">d&iacute;ra</trd>
  <trd lng="de">Loch</trd>
  <trd lng="en">hole</trd>
  <trd lng="he">&het;&vav;&resh;</trd>
  <trd lng="hu">lyuk</trd>
  <trd lng="ja">&#31348; [<ind>&#12354;&#12394;</ind>]</trd>
  <trd lng="nl">gat</trd>
  <trdgrp lng="pt">
    <trd>furo</trd>,
    <trd>buraco</trd>
  </trdgrp>
  <trd lng="tp">lupa</trd>
  <trd lng="vo">hog</trd>
</drv>

<drv mrk="tru.0a">
  <kap><tld/>a, <var><kap><tld/>hava<fnt>Z</fnt></kap></var></kap>
  <snc>
    <dif>
      <tld lit="T"/>ita, <tld/>plena:
      <ekz>
        envolvita en <tld/>a tuko.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ajxur.0a">a&jcirc;ura</ref>,
      <ref cel="cxifon.0o">&ccirc;ifono</ref>,
      <ref cel="krad">krada</ref>,
      <ref cel="punt">punta</ref>,
      <ref cel="bor">traborita</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_k;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">foradat</trd>
  <trdgrp lng="de">
    <trd>l&ouml;cherig</trd>,
    <trd>verl&ouml;chert</trd>
  </trdgrp>
  <trd lng="fr">trou&eacute;</trd>
  <trd lng="he">&mem;<ind>&het;&vav;&resh;&resh;</ind></trd>
  <trd lng="hu">lyukas</trd>
  <trd lng="nl">vol <ind>gaten</ind></trd>
  <trdgrp lng="pl">
    <trd>dziurawy</trd>,
    <trd>perforowany</trd>
  </trdgrp>
  <trd lng="pt">esburacado</trd>
  <trd lng="ru">&c_d;&c_y;&c_r;&c_ja;&c_v;&c_y;&c_j;</trd>
  <trd lng="vo">hogik</trd>
</drv>

<drv mrk="tru.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tru.0i.KOMUNE">
    <dif>
      Fari <tld/>on a&ubreve; <tld/>ojn:
      <ekz>
        brako <tld/>ita de kuglo;
      </ekz>
      <ekz>
        vesto <tld/>ita de tineoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>foradar</trd>,
    <trd>perforar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>l&ouml;chern</trd>,
    <trd><klr>ein </klr>Loch machen</trd>,
    <trd><klr>ein </klr>Loch <ind>bohren</ind></trd>
  </trdgrp>
  <trd lng="fr">trouer</trd>
  <trd lng="he">&lamed;<ind>&het;&vav;&resh;&resh;</ind></trd>
  <trdgrp lng="hu">
    <trd>lyukaszt</trd>,
    <trd>kilyukaszt</trd>
  </trdgrp>
  <trd lng="nl">een <ind>gat</ind> maken</trd>
  <trdgrp lng="pl">
    <trd>dziurawi&cacute;</trd>,
    <trd>dziurkowa&cacute;</trd>,
    <trd>perforowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furar</trd>,
    <trd>esburacar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_r;&c_ja;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_y;&c_r;&c_ja;&c_v;&c_i;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trd lng="vo">hog&ouml;n</trd>
</drv>

<drv mrk="tru.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tru.tra0i.KOMUNE">
    <dif>
      <tld lit="T"/>i de unu flanko al alia:
      <ekz>
        generalo Mulan <klr>...</klr> je unu pafo tra<tld/>is la rokegon
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk>
        <url ref="http://esperanto.cri.cn/1/2006/11/01/1@47734.htm">
        Belaj vidinda&jcirc;oj...</url></vrk>,
        <lok>2006-11-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bor.0i">bori</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>percer</trd>,
    <trd>perforer</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dziurawi&cacute;</trd>,
    <trd>przedziurawia&cacute;</trd>,
    <trd>perforowa&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="tru.babil0o">
  <kap>babil<tld/>o</kap>
  <snc>
    <uzo tip="stl">VULG</uzo>
    <ref tip="dif" cel="busx.0o">bu&scirc;o</ref>
    <ekz>
      poste fermu vian babil<tld/>on kaj lasu paroli la alian
      <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
      28a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>bouche<klr tip="ind"> (vulg.)</klr></trd>,
    <trd>clapet<klr tip="ind"> (vulg. bouche)</klr></trd>,
    <trd>gueule<klr tip="ind"> (bouche)</klr></trd>,
    <trd>bo&icirc;te &agrave; camembert<klr tip="ind"> (vulg. bouche)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="tru.buton0o">
  <kap>buton<tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <klr>(&ccirc;e vesto)</klr>
      Fendo, en kiun oni enigas <ref cel="buton.0o.TEKS">butonon</ref>:
      <ekz>
        lia ornamo konsistis en bukedo da violoj, kiun li portis en la buton<tld/>o
        <fnt><bib>Fab4</bib>, <lok>Dekdu per la po&scirc;to</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">boutonni&egrave;re</trd>
</drv>

<drv mrk="tru.ink0o">
  <kap>ink<tld/>o</kap>
  <snc>
    <uzo tip="stl">VULG</uzo>
    <ref tip="dif" cel="ink.0ujo">inkujo</ref>
    <ekz>
      <ctl>Malsa&gcirc;a ink<tld/>o!</ctl> diris la plumo,
      <ctl>Skriba &scirc;tipo!</ctl> respondis la inkujo
      <fnt><bib>Fab4</bib>, <lok>Plumo kaj inkujo</lok></fnt>.
    </ekz>
  </snc>
</drv>

<drv mrk="tru.memor0o">
  <kap>memor<tld/>o</kap>
  <snc>
    <dif>
      Momenta forgeso:
      <ekz>
        Ste&ccirc;jo rapide faris planon: li &ccirc;iel ka&scirc;u sian
        nekredeblan memor<tld/>on, kaj nun anta&ubreve; &ccirc;io li ruze
        paroligu la alilandanon
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>trou</ind> de m&eacute;moire</trd>
    <trd>oubli</trd>
  </trdgrp>
</drv>

<drv mrk="tru.mus0o">
  <kap>mus<tld/>o</kap>
  <snc>
    <dif>
      Malgranda <tld/>o, en kiu lo&gcirc;as muso:
      <ekz>
        <ctl>Sed kiamaniere mi povos trapu&scirc;i min tra la malgranda
        mus<tld/>o en la planko?</ctl> demandis Hjalmar
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>;
      </ekz>
      <ekz>
        Sinjoro paro&hcirc;estro, sub la katedro estas mus<tld/>o, kaj dum vi parolas,
        ofte eliras muso, kaj kaprioletas anta&ubreve; viaj piedoj
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el
        Bervalo</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>trou</ind> de souris</trd>
  </trdgrp>
</drv>

<drv mrk="tru.naz0o">
  <kap>naz<tld/>o, <var><kap>nazo<tld/>o</kap></var></kap>
  <snc mrk="maz.0truo.ANA_ZOO">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Ccirc;iu el la du malferma&jcirc;oj de la <ref cel="naz.0o">nazo</ref>,
      tra kiu oni spiras:
      <ekz>
        li &scirc;topis unu naz<tld/>on kaj per la alia ekblovis sur la regimentojn
        <fnt><bib>ElektFab</bib>, <lok>Sesope oni trairas la tutan mondon</lok></fnt>;
      </ekz>
      <ekz>
        Mi metos Mian ringon en viajn nazo<tld/>ojn kaj Mian bu&scirc;brida&jcirc;on
        en vian bu&scirc;on <klr>(por vin submeti kiel bruton)</klr>
        <fnt><bib>MT</bib>, <lok>&Jes; 37:29</lok></fnt>;
      </ekz>
      <ekz>
        tio estis belegaj blankaj &ccirc;evaloj, iliaj ru&gcirc;etaj
        nazo<tld/>oj estis en sen&ccirc;esa movi&gcirc;ado
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>narine</trd>
    <trd>naseau</trd>
    <trd><ind>trou</ind> de nez</trd>
  </trdgrp>
</drv>

<drv mrk="tru.nigra0o">
  <kap>nigra <tld/>o</kap>
  <snc mrk="tru.nigra0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Kunpremi&gcirc;inta maso tiom koncentrita, ke neniu objekto
      povas eskapi &gcirc;ian intensan gravitan altiron,
      e&ccirc; ne lumo.
    </dif>
  </snc>
  <trd lng="be">
  &c_ch;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_d;&c_z;&c_ib;&c_r;&c_a;</trd>
  <trd lng="ca">forat negre</trd>
  <trd lng="da">sort hul</trd>
  <trd lng="de">schwarzes <ind>Loch</ind></trd>
  <trdgrp lng="en">
    <trd>black <ind>hole</ind></trd>,
    <trd>collapsar</trd>
  </trdgrp>
  <trd lng="es">agujero negro</trd>
  <trd lng="fi">musta aukko</trd>
  <trd lng="fr"><ind>trou</ind> noir</trd>
  <trd lng="he">&het;&vav;&resh; &shin;&het;&vav;&resh;</trd>
  <trd lng="hu">fekete lyuk</trd>
  <trd lng="it">tra noi</trd>
  <trd lng="ja">
  &#12502;&#12521;&#12483;&#12463;&#12507;&#12540;&#12523;&#12288;
  [<ind>&#12406;&#12425;&#12387;&#12367;&#12411;&#12362;&#12427;
  </ind>]</trd>
  <trd lng="nl">zwart <ind>gat</ind></trd>
  <trd lng="no">sort hull</trd>
  <trd lng="pl">czarna <ind>dziura</ind></trd>
  <trdgrp lng="pt">
    <trd>buraco negro</trd>,<trd>colapsar</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_d;&c_y;&c_r;&c_a;
  </trd>
  <trd lng="sv">svart h&aring;l</trd>
  <trd lng="tr">kara delik</trd>
  <trd lng="vo">hog bl&auml;gik</trd>
</drv>

<drv mrk="tru.postajxo0o">
  <kap><ofc>9</ofc>posta&jcirc;o<tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="anus.0o">anuso</ref>
    <ekz>
      vi povas &scirc;ovi &gcirc;in en vian posta&jcirc;<tld/>on
      <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el
      Bervalo</vrk></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>trou</ind> du cul<klr tip="ind"> (anus)</klr></trd>,
    <trd>anus</trd>
  </trdgrp>
</drv>

<drv mrk="tru.pug0o">
  <kap>pug<tld/>o</kap>
  <snc>
    <uzo tip="stl">VULG</uzo>
    <ref tip="dif" cel="anus.0o">anuso</ref>
    <ekz>
      nur tie genuo kun plena &gcirc;uo povas delikate pu&scirc;atingi kojonojn 
      a&ubreve; pug<tld/>on preter la karna valo
      <fnt><aut>P. Thorsen</aut>, 
      <vrk><url ref="http://www.poulthorsen.dk/mama.htm">Mamazono</url></vrk>,
      <lok>La Kancerkliniko, n-ro 68</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>trou</ind> du cul<klr tip="ind"> (anus)</klr></trd>,
  </trdgrp>
</drv>

<drv mrk="tru.serur0o">
  <kap>serur<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o, en kiun oni enigas &scirc;losilon:
      <ekz>
        de apuda &ccirc;ambro mi povis plu observi ilin tra serur<tld/>o
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el
        Bervalo</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>trou</ind> de serrure</trd>
  </trdgrp>
</drv>

<drv mrk="tru.son0o">
  <kap>son<tld/>o</kap>
  <snc mrk="tru.son0o.gxenerale">
    <dif>
      <tld lit="T"/>o, aran&gcirc;ita en io, por ke la sono povu el tio eliri:
      <ekz>
        la vento bruas al <klr>[la pre&gcirc;eja sonorilo]</klr> tra aperturoj kaj son<tld/>oj,
        tra &ccirc;iu fendo
        <fnt><bib>Fab4</bib>, <lok>Sonorila profundo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tru.son0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <fnt><bib>RokPop</bib></fnt>
    <dif>
      Sonellasa <tld/>o en la resonanca skatolo de akustika instrumento.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>baie<klr tip="ind"> (d'un clocher)</klr></trd>
    <trd>bouche<klr tip="ind"> (d'un violon)</klr></trd>
    <trd>ou&iuml;e<klr tip="ind"> (d'un violon)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="tru.ventol0o">
  <kap>ventol<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o, aran&gcirc;ita ekzemple en muro por enlasi fre&scirc;an aeron:
      <ekz>
        en la 119-an &ccirc;ambron estas venigita novulo, sen&ccirc;ese balbutanta ion
        pri valuto en ventol<tld/>o
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>bouche</ind> d'a&eacute;ration</trd>
  </trdgrp>
</drv>
<adm>
