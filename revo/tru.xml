      <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_v;&c_y;&c_b;&c_o;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tru.0o.manko">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="mank.0o">Manko</ref> en io:
      <ekz>
        nur en unu punkto ilia informiteco havas <tld/>on;
      </ekz>
      <ekz>
        fari <tld/>ojn en bud&gcirc;et'
        <fnt>
          <bib>LSB</bib>
          <lok>Homoj &Ccirc;e L' Marbordo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia mono servis por &scirc;topi tre rimarkeblajn <tld/>ojn
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        via registro <klr>[de mortoka&ubreve;zoj]</klr> havas <tld/>on: vi forgesis pri veneno
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia libro tre bone helpas plenigi la <tld/>ojn en la plejparte
        okcidentlandaj historioj de la esperanta movado
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2004/008478.html">Esperanto en la
          soclandoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        Mikrosofto devis pasintjare plurfoje konfesi
        <ind>sekurec<tld/>ojn</ind> <klr>[&#8230;]</klr> kaj
        fami&gcirc;intaj virusoj kaj vermoj misuzis la <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008483.php">Foliumiloj denove
          interbatalas</url></vrk>
        </fnt>;
        <trd lng="de">Sicherheitsl&uuml;cke <klr>(Comp.)</klr></trd>
      </ekz>
      <ekz>
        <tld/>on de l' honoro flikos neniu tajloro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_b;&c_je;&c_l;
      <klr tip="amb">(&c_u; &c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)
      </klr></trd>,
      <trd>&c_n;&c_ja;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
    <trd lng="de">L&uuml;cke</trd>
    <trdgrp lng="fr">
      <trd>trou<klr tip="ind"> (fig.)</klr></trd>,
      <trd>blanc<klr tip="ind"> (dans une conversation)</klr></trd>,
