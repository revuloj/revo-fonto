<drv mrk="tru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tru.0o.brecxo">
    <dif>
      Trarompa&jcirc;o, trairebla malferma&jcirc;o
      (<ref cel="apertu.0o">aperturo</ref>):
      <ekz>
        la flika&jcirc;o estas pli malbela ol la <tld/>o;
      </ekz><ekz>
        &scirc;topi, fliki <tld/>on;
      </ekz><ekz>
        <tld/>o de kudrilo;
      </ekz>
      <ekz>
        sakon kun <tld/>o vi neniam plenigos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri la &scirc;trumpa <tld/>o scias nur la &scirc;uo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e botisto la &scirc;uo estas &ccirc;iam kun <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i faris en la saketo malgrandan truon, tiel, ke la grio povis
        &scirc;utmarki la tutan
        vojon
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koboldo rigardis tra la serura <tld/>o
        <fnt><bib>Fab3</bib>,
          <lok>Koboldo &ccirc;e la butikisto</lok></fnt>;
      </ekz><ekz>
        tie ne ekzistis ia fenestra vitro, estis nur <tld/>o en la
        muro <klr>kaj</klr> la suno venis simile al ora globo kaj
        enrigardis tra la <tld/>o
        <fnt>
          <bib>Fab4</bib>,
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
          filinoj</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        pri la &scirc;trumpa <tld/>o scias nur la &scirc;uo
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>li havas <tld/>on en la manplato</ind>
        <klr>(estas tute mal&scirc;parema)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="fr">
          <trd>c'est un <ind>panier</ind> perc&eacute;</trd>,
          <trd>il a l'argent qui lui br&ucirc;le les
          <ind>doigts</ind></trd>,
          <trd>il a l'argent qui lui file entre les
            <ind>doigts</ind></trd>,
          <trd>il a l'argent qui lui fond dans les
            <ind>mains</ind></trd>,
          <trd>il a un <ind>trou</ind> au fond de la poche<klr tip="ind"> (&ecirc;tre d&eacute;pensier)</klr></trd>
        </trdgrp>
        <trd lng="it">aver le mani bucate</trd>
        <trd lng="ru">&c_I;&c_v;&c_a;&c_n; &mdash;
        <ind>&gt;&c_d;&c_y;&c_r;&c_ja;&c_v;&c_y;&c_j;
        &c_k;&c_a;&c_r;&c_m;&c_a;&c_n;</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apertu.0o">aperturo</ref>,
      <ref cel="brecx.0o">bre&ccirc;o</ref>,
      <ref cel="fend.0o">fendo</ref>,
      <ref cel="ferm">malferma&jcirc;o</ref>,
      <ref cel="luk.0o">luko</ref>,
      <ref cel="por1.0o">poro</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;</trd>,
      <trd>&c_d;&c_z;&c_ju;&c_r;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">forat</trd>
    <trdgrp lng="fr">
      <trd>trou</trd>,
      <trd>orifice</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dziura</trd>,
      <trd>otw&oacute;r</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_y;&c_r;&c_a;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_je;&c_r;&c_s;&c_t;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tru.0o.kavo">
    <dif>
      <ref tip="dif" cel="kav.0o">Kavo</ref>:
      <ekz>
        la knabo fosis per sia fingro <tld/>on en la tero, la
        malgranda knabino enmetis tien la kernon, kaj amba&ubreve; kovris
        &gcirc;in per tero
        <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de
        fra&ubreve;lo</lok></fnt>;
      </ekz><ekz>
        plenigi <tld/>on<fnt>B</fnt>.
      </ekz>
    </dif>
    <subsnc mrk="tru.0o.bestahejmo">
      <dif>
        Besta hejmo, kava&jcirc;o a&ubreve; tunelo fosita de besto en
        kiu &ccirc;i
        tiu lo&gcirc;as:
        <ekz>
          la <tld/>o de aspido
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 11:8</lok>
          </fnt>;
        </ekz>
        <ekz>
          li <klr>(la Fingreto-knabo)</klr> kuris momenton inter
          la terbuloj <klr>(sur la plugokampo)</klr> kaj subite
          englitis en musan <tld/>on, kiun li ser&ccirc;is
          <fnt>
            <aut>Fratoj Grimm, tr. Kabe</aut>: <vrk>Fingreto</vrk>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="en">
        <trd>burrow</trd>, <trd>hole</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_o;&c_r;&c_a;</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="apertu.0o">aperturo</ref>,
      <ref cel="kavern.0o">kaverno</ref>,
      <ref cel="kav.0o">kavo</ref>,
      <ref cel="ferm">malferma&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;</trd>,
      <trd>&c_d;&c_z;&c_ju;&c_r;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">forat</trd>
    <trdgrp lng="fr">
      <trd>trou</trd>,
      <trd>cavit&eacute;</trd>,
      <trd>creux</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dziura</trd>,
      <trd>luka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_m;&c_a;</trd>,
      <trd>&c_l;&c_u;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_v;&c_y;&c_b;&c_o;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
    <adm>
      La vorto "malplena&jcirc;o", kiu servas por difini "truo" kaj "kavo",
      ne havas difinon en REVO.
      <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="tru.0o.manko">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Manko en io:
      <ekz>
        nur en unu punkto ilia informiteco havas <tld/>on;
      </ekz>
      <ekz>
        <tld/>on de l' honoro flikos neniu tajloro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        lia libro tre bone helpas plenigi la <tld/>ojn en la plejparte
        okcidentlandaj
        historioj de la esperanta movado
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.net/2004/008478.html">Esperanto en la
          soclandoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        Mikrosofto devis pasintjare plurfoje konfesi
        <ind>sekurec<tld/>ojn</ind> <klr>[&#x2026;]</klr> kaj
        fami&gcirc;intaj virusoj kaj vermoj misuzis la <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.net/2005/008483.php">Foliumiloj denove
          interbatalas</url></vrk>
        </fnt>.
        <trd lng="de">Sicherheitsl&uuml;cke <klr>(Comp.)</klr></trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_b;&c_je;&c_l;
      <klr tip="amb">(&c_u; &c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)
      </klr></trd>,
      <trd>&c_n;&c_ja;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
    <trd lng="de">L&uuml;cke</trd>
    <trdgrp lng="fr">
      <trd>trou<klr tip="ind"> (fig.)</klr></trd>,
      <trd>blanc<klr tip="ind"> (dans une conversation)</klr></trd>,
      <trd>manque</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dziura</trd>,
      <trd>luka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_y;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_je;&c_l;
      <klr tip="amb">(&c_v; &c_ch;&c_jo;&c_m;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="af">gat</trd>
