<drv mrk="odor.0a">
  <kap><tld/>a</kap>
  <snc mrk="odor.0a.SEN">
    <dif>
      <tld lit="O"/>anta.
    </dif>
    <trdgrp lng="io">
      <trd>odoranta</trd>,
      <trd>odoroza</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_h;&c_n;&c_u;&c_ch;&c_y;</trd>
  <trd lng="cs">von&iacute;c&iacute; i p&aacute;chnouc&iacute;</trd>
