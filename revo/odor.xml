      <ref cel="miasm.0o">miasmo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;</trd>
  <trd lng="en">stench</trd>
  <trd lng="fr">mauvaise odeur</trd>
  <trd lng="io">malodoro</trd>
  <trd lng="ja">&#x60AA;&#x81ED;  <pr>&#x3042;&#x304F;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="nl">stank</trd>
    <trdgrp lng="pl">
    <trd>smr&oacute;d</trd>,
    <trd>fetor</trd>,
    <trd>od&oacute;r</trd>
  </trdgrp>
    <trdgrp lng="ro">
    <trd>putoare</trd>,
    <trd>duhoare</trd>,
    <trd>mirosurilor</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&#x43F;&c_r;&c_i;&c_jeu;&#x43C;&#x43D;&c_i;&c_j; &c_z;&c_a;&#x43F;&c_a;&c_h;</trd>,
    <trd>&c_s;&#x43C;&#x43E;&c_r;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6076;&#x81ED; <pr>&#xE8;ch&#xF2;u</pr></trd>,
    <trd>&#x60E1;&#x81ED; <pr>&#xE8;ch&#xF2;u</pr></trd>,
