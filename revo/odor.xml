  </trdgrp>
  <trdgrp lng="pl">
    <trd>pachnie&cacute;</trd>,
    <trd>czu&cacute;  <klr>(zapach)</klr></trd>,
    <trd>zalatywa&cacute;</trd>
  </trdgrp>
