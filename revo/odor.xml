    <trd>&c_s;&c_m;&c_o;&c_r;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24694;&#33261; <pr>&egrave;ch&ograve;u</pr></trd>,
    <trd>&#24801;&#33261; <pr>&egrave;ch&ograve;u</pr></trd>,
