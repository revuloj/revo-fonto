    <trd>&#x304B;&#x304A;&#x308B;</trd>,
    <trd>&#xFF5E;<ind>&#x306E;&#x306B;&#x304A;&#x3044;&#x3092;&#x653E;&#x3064; 
    </ind><pr>&#x306E;&#x306B;&#x304A;&#x3044;&#x3092;&#x306F;&#x306A;&#x3064;</pr></trd>,
    <trd>&#x6C17;&#x914D;&#x304C;&#x3042;&#x308B;  <pr>&#x3051;&#x306F;&#x3044;&#x304C;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x92C;&#x93E;&#x938;&#x94D;&#x928;&#x93E;</trd>
  <trdgrp lng="nl">
    <trd>ruiken</trd>,
