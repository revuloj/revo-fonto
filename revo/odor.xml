<drv mrk="odor.malbon0a">
  <kap>malbon<tld/>a</kap>
  <snc mrk="odor.malbon0a.SEN">
    <dif>
      Malbone <tld/>anta:
      <ekz>
        densa kaj malbon<tld/>a fumo plenigis la &ccirc;ambron
        <fnt>
          <aut>H. Vallienne</aut>
          <vrk>&Ccirc;u li?</vrk>
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo de Nilo mallevi&gcirc;is, la kanaloj estis malprofundaj
        kaj malbon<tld/>aj
        <fnt><bib>Far2</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fetor.0a">fetora</ref>
    <ref tip="vid" cel="haladz.0a">haladza</ref>
  </snc>
  <rim>
    Fojfoje oni renkontas la vortojn <em><tld/>a&ccirc;a</em> kaj
    <em>fi<tld/>a</em>, sed multe malpli ofte ol <em>malbon<tld/>a</em>.
    La verbaj formoj <em><tld/>a&ccirc;i</em> kaj <em>fi<tld/>i</em> estas
    multe pli uzataj ol iliaj adjektivaj ekvivalentoj.
  </rim>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_d;&c_z;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">pudent</trd>
  <trd lng="cs">zap&aacute;chaj&iacute;c&iacute;</trd>
  <trdgrp lng="de">
    <trd>stinkend</trd>,
    <trd>m&uuml;ffelnd</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stinking</trd>,
    <trd>smelly</trd>,
    <trd>reeking</trd>,
    <trd>fetid</trd>,
    <trd>malodorous</trd>
  </trdgrp>
  <trd lng="fr">puant</trd>
  <trdgrp lng="ja">
    <trd>&#12367;&#12373;&#12356;</trd>,
    <trd>&#24746;&#33261;&#12398;  <pr>&#12354;&#12367;&#12375;&#12421;&#12358;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">stinkend</trd>
    <trdgrp lng="pl">
    <trd>cuchn&aogonek;cy</trd>,
    <trd>&sacute;mierdz&aogonek;cy</trd>,
    <trd>smrodliwy</trd>
  </trdgrp>
    <trdgrp lng="ro">
    <trd>puturos</trd>,
    <trd>&icirc;mpu&#539;it</trd>,
    <trd>fetid</trd>
  </trdgrp>
  <trd lng="sk">smrad&lcaron;av&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_ju;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_o;&c_r;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z; &c_n;&c_je;&c_p;&c_r;&c_i;&c_jeu;&c_m;&c_n;&c_i;&c_m; &c_z;&c_a;&c_p;&c_a;&c_h;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
