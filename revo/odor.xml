  </trdgrp>
</drv>

<drv mrk="odor.0o">
  <kap><tld/>o</kap>
  <snc mrk="odor.0o.SEN">
    <dif>
      &Ccirc;io, kio impresas flarsenton:
      <ekz>
        en la fre&scirc;a aero senti&gcirc;is la <tld/>oj de frua printempo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i klinis <klr>[al la floroj sin por]</klr> enspiri ilian bon<tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la forta <tld/>o de la incensoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> flaris la <tld/>on de liaj vestoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas <tld/>on de maro
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i spiris la bon<tld/>on el la oran&gcirc;o&gcirc;ardenoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fojn<tld/>o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de gladitaj teksa&jcirc;oj
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la krono havas en si ion de la <tld/>o de sankteco
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>mono <tld/>on ne havas, sian devenon ne diras</ind>
        <trd lng="fr">l'argent n'a pas d'odeur</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arom.0o">aromo</ref>,
      <ref tip="vid" cel="parfum.0o.bonodoro">parfumo<sncref/></ref>
    </refgrp>
