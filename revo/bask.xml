<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bask</rad>/o
</kap>
<drv mrk="bask.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">TEKS</uzo>
   <snc num="1">
     <dif>
       Pendanta parto, sub la talio, de frako a&ubreve; de alia trunka <ref tip="malprt" cel="vest.0o">vesto</ref>.
   <trd lng="en">coattails</trd>
     </dif>
   </snc>
   <snc num="2">
     <dif>
       Trena&jcirc;o de vesto:
       <ekz>
         la <tld/>oj de la Sinjoro plenigis la templon
         <fnt><bib>MT</bib>, <lok>&Jes; 6:1</lok></fnt>.
       </ekz>
     </dif>
   <trd lng="en">train<klr tip="ind"> (of a dress or robe)</klr></trd>
   </snc>
   <trd lng="be">&c_k;&c_r;&c_y;&c_s;&c_o;</trd>
   <trd lng="de">Rockscho&szlig;</trd>
   <trd lng="fr">basque<klr tip="ind"> (d'une veste)</klr></trd>
   <trd lng="ru">&c_p;&c_o;&c_l;&c_a; 
<klr>(&c_o;&c_d;&c_je;&c_zh;&c_d;&c_y;)</klr></trd>
</drv>
</art>
