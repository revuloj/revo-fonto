<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bask.xml,v 1.26 2023/08/31 08:48:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bask</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bask.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bask.0o.vestoparto">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Pendanta parto de <ref tip="malprt" cel="vest.0o">vesto</ref>,
      plejofte trunka,
    </dif>
    <subsnc mrk="bask.0o.refaldebla">
      <dif>
        refaldebla:
        <ekz>
          jen homo portis sanktigitan viandon en la <tld/>o de sia vesto
          <fnt><bib>MT</bib>, <lok>&Hcirc;agaj 2:12</lok></fnt>;
        </ekz>
        <ekz>
          <klr>[tegmenta herbo,]</klr> per kiu ne plenigas rikoltanto
          sian manon nek garbiganto sian <tld/>on
          <fnt><bib>MT</bib>, <lok>&Psa; 129:7</lok></fnt>;
        </ekz>
        <ekz>
          Lemuel ree ku&scirc;i&gcirc;is kaj tiris la <tld/>ojn de
          sia kapovesto sur la viza&gcirc;on
          <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 6</lok></fnt>;
        </ekz>
      </dif>
      <trdgrp lng="es">
        <trd>fald&oacute;n</trd>,
        <trd>vuelo</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="bask.0o.ornamcela">
      <dif>
        a&ubreve; nur ornamcela:
        <ekz>
          <tld/>ojn de l' frako vivige agitis vento petola
          tra vintra pejza&gcirc;'
          <fnt>
          <aut>R. Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Tro homa sorto de iu ne&gcirc;homo</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi vidis la Sinjoron, sidantan sur alta kaj levita trono,
          kaj Liaj <tld/>oj plenigis la templon
          <fnt><bib>MT</bib>, <lok>&Jes; 6:1</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="es">cola</trd>
    </subsnc>
    <trdgrp lng="pl">
      <trd>po&lstroke;a<klr> (np. marynarki)</klr></trd>,
      <trd>tren<klr> (sukni)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="bask.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tia parto de vesto, konsiderata kiel &scirc;irmilo por
      valora poseda&jcirc;o, a&ubreve; simbolanta la povon de
      &gcirc;ia portanto:
      <ekz>
        ili venigos viajn filojn en la <tld/>oj, kaj viajn filinojn
        ili portos sur la &scirc;ultroj
        <fnt><bib>MT</bib>, <lok>&Jes; 49:22</lok></fnt>;
      </ekz>
      <ekz>
        malbenita estu, kiu ku&scirc;as kun la edzino de sia patro,
        &ccirc;ar li malkovris la <tld/>on de sia patro
        <fnt><bib>MT</bib>, <lok>&Rea; 27:20</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;io, pri kio anta&ubreve;e vi ne kura&gcirc;is e&ccirc;
        revi,
        dum vi vivis sub <tld/>o de la drinkulo edzo
        <fnt>
        <aut>A. Kruzs</aut>
        <bib>Voja&gcirc;o</bib>
        <vrk>La lingvo de okuloj</vrk>
        <lok>leciono 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><mll>vivi sub ies <ind><tld/>o</ind></mll></ind>;
        <trdgrp lng="fr">
          <trd>vivre dans l'<ind>ombre</ind> de qn</trd>,
          <trd>&ecirc;tre accroch&eacute; aux <ind>basques</ind> de
          qn</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>vki <ind>&aacute;rny&eacute;k&aacute;ban</ind>
          &eacute;l</trd>,
          <trd>vki <ind>szokny&aacute;j&aacute;ba</ind>
          kapaszkodik</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la friponoj tie sub la <tld/>o <klr>(ka&scirc;e)</klr>
        pretigas jam petoskribojn
        <fnt><bib>Revizoro</bib>, <lok>1:4</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><mll>fari ion sub la <ind><tld/>o</ind></mll></ind>;
        <trd lng="fr">faire qc sous le <ind>manteau</ind></trd>
        <trd lng="hu">vmi <ind>leple</ind> alatt tesz</trd>
      </ekz>
      <ekz>
        en tiu tempo forte kaptos dek homoj el &ccirc;ialingvaj nacioj
        la <tld/>on de Judo <klr>(humile petos permeson lin
        sekvi)</klr>,
        kaj diros: &leftquot;Ni iros kun vi, &ccirc;ar ni
        a&ubreve;dis, ke Dio estas kun vi&rightquot;
        <fnt><bib>MT</bib>, <lok>&Zeh; 8:23</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><mll>tiri ies <ind><tld/>on</ind></mll></ind>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sin.0o.vesto">sino <sncref/></ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_s;&c_o;</trd>
  <trd lng="br">pastell<klr tip="ind"> (dilhad)</klr></trd>
  <trdgrp lng="cs">
    <trd>vle&ccaron;ka</trd>,
    <trd>&scaron;os</trd>
  </trdgrp>
  <trd lng="de">Rockscho&szlig;</trd>
  <trdgrp lng="en">
    <trd>coattails</trd>,
    <trd>train<klr tip="ind"> (of a dress or robe)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>basque<klr tip="ind"> (d'une veste)</klr></trd>,
    <trd>pan<klr tip="ind"> (d'un v&ecirc;tement)</klr></trd>,
    <trd>rabat<klr tip="ind"> (d'un v&ecirc;tement)</klr></trd>,
    <trd>giron<klr tip="ind"> (pan d'un v&ecirc;tement,
    sein)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5E9;&#x5D5;&#x5D1;&#x5DC;</trd>,
    <trd>&#x5D3;&#x5E9; &#x5DE;&#x5E2;&#x5D9;&#x5DC;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ruhasz&aacute;rny</trd>,
    <trd>hajt&oacute;ka</trd>,
    <trd>kab&aacute;tsz&aacute;rny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x305D;</trd>,
    <trd>&#x305F;&#x308C;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aba<klr> (de casaca, de fraque)</klr></trd>,
    <trd>fralda<klr>  (de camisa)</klr></trd>,
    <trd>calda<klr>  (de vestimenta)</klr></trd>    
  </trdgrp>
  
  
  
  <trd lng="ru">&c_p;&c_o;&c_l;&c_a;
  <klr>(&c_o;&c_d;&c_je;&c_zh;&c_d;&c_y;)</klr></trd>
  <trd lng="sk">kr&iacute;dlo kab&aacute;ta</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&c_d;&c_ib;&#x43B;</trd>
  </trdgrp>
</drv>
</art>
