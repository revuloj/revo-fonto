<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: espres.xml,v 1.4 2025/03/19 20:31:24 afido Exp $">
<kap>
  <rad>espres</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="espres.0o">
  <kap><tld/>o</kap>
  <snc mrk="espres.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="kaf.0o.KUI">Kafo</ref> pretigita per forta trapremo
      de varmega akvo tra subtila kafpulvoro:
      <ekz>
        <tld/>o estas konsiderata perfekta se la fajnporeca &scirc;a&ubreve;mo restas staranta dum du
        minutoj
        <fnt>
          <vrk><url
          ref="http://esperantaretradio.blogspot.com/2020/01/formulo-por-perfekta-espreso-kafo.html"
              >Formulo por perfekta Espreso-kafo</url></vrk>
          <lok>Esperanta Retradio, 25a jan. 2020</lok>
        </fnt>;
      </ekz>
      <ekz>
        kafo makulata <klr>[&#8230;]</klr> estas <tld/>o kun iom da lakto&scirc;a&ubreve;mo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Kafo_makulata"
              >Kafo makulata</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Espresso</trd>
    <trd lng="en">espresso</trd>
    <trd lng="fr">espresso</trd>
    <trd lng="it">caff&egrave; espresso</trd>
    <trd lng="pl">kawa z ekspresu</trd>
    <trd lng="ru">&c_e;&c_s;&c_p;&c_r;&c_je;&c_s;&c_s;&c_o;</trd>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_p;&c_r;&c_e;&c_s;&c_a; <klr>(&c_k;&c_a;&c_v;&c_a;)</klr></trd>
</drv>
</art>

