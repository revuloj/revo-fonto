<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lukan</rad>/o
</kap>
<drv mrk="lukan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="lukan.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lucanus</trd>),
      granda koleoptero, kies vira genro posedas fortajn
      prenilojn sur la kapo.
    </dif>
  </snc>
  <trd lng="de">Hirschk&auml;fer</trd>
</drv>
<drv mrk="lukan.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lucanidae</trd>).
    </dif>
  </snc>
</drv>
</art>
