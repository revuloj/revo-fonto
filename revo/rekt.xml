<drv mrk="rekt.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-5179">mal'rekt'a'</url></lok></fnt></kap>
  <snc mrk="rekt.mal0a.kurba">
    <dif>
      Havanta kurbojn, angulojn:
      <ekz>
        mal<tld/>aj stratoj kaj stratetoj
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal<tld/>a estas la vojo de homo kulpa
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 21:8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">kurvig</trd>
    <trdgrp lng="en">
      <trd>curved</trd>,
      <trd>crooked <klr>(not straight)</klr></trd>
    </trdgrp>
    <trd lng="hu">g&ouml;rbe</trd>
    <trdgrp lng="id">
      <trd>belok</trd>,
      <trd>bengkok</trd>,
      <trd><baz>belok</baz>berbelok</trd>,
      <trd><baz>kelok</baz>berkelok</trd>,
      <trd><baz>keluk</baz>berkeluk</trd>,
      <trd><baz>lengkung</baz>melengkung</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>krzywy</trd>,
      <trd>powyginany</trd>,
      <trd>wygi&eogonek;ty</trd>,
      <trd>zgi&eogonek;ty</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_p;&c_r;&c_ja;&c_m;&c_o;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_i;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rekt.mal0a.pera">
    <dif>
      Ne <tld/>e alcela, uzanta peradon, kromvojon:
      <ekz>
        mal<tld/>a parolado;
      </ekz>
      <ekz>
        <ind>mal<tld/>a pensado</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2005/008619.php">Paradoksa libreto da paradoksaj
          rakontetoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
        <trd lng="de">Um-die-<ind>Ecke</ind>-Denken</trd>
      </ekz>
      <ekz>
        mal<tld/>aj aludoj
        <fnt>
          <bib>Monato</bib>
          <aut>Luiza Carol</aut>
          <vrk><url ref="&Monato;/2011/010287.php">Inter videbleco kaj
          nevidebleco</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        Albanio i&gcirc;os transita punkto inter la okcidenta kaj orienta merkatoj, anka&ubreve; la
        disvolvi&gcirc;o de suda Italio mal<tld/>e estas ligita al &gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007967.html">La Oka Koridoro</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kaj mal<tld/>aj &scirc;tataj subvencioj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2007/009344.php">&Ccirc;u biokarbura&jcirc;oj helpas a&ubreve;
          malhelpas?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is nun la ekzisto de planedoj ekster nia sunsistemo estis pruvata per
        mal<tld/>aj metodoj
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2006/008796.php">Unua fotografa&jcirc;o de ekstersunsistema
          planedo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">auf Umwegen</trd>
    <trd lng="en">indirect</trd>
    <trd lng="hu">k&ouml;zvetett</trd>
    <trd lng="id"><ind>tidak</ind> langsung</trd>
    <trdgrp lng="pl">
      <trd>niebezpo&sacute;redni</trd>,
      <trd>okr&eogonek;&zdot;ny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_p;&c_r;&c_ja;&c_m;&c_o;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_h;&c_o;&c_d;&c_n;&c_o;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_s;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rekt.mal0a.malvertikala">
    <dif>
      Klinita, nevertikala:
      <ekz>
        la tablo staras mal<tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        la pordo delevi&gcirc;is de unu hoko kaj pendis tiel mal<tld/>e, ke tra la fendo oni
        povis tra&scirc;ovi&gcirc;i
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu fenestro sidis mal<tld/>e, la pordo estis tro malalta
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">schief</trd>
    <trd lng="en">non<ind>vertical</ind></trd>
    <trd lng="hu">ferde</trd>
    <trdgrp lng="id">
      <trd>condong</trd>,
      <trd>miring</trd>,
      <trd>serong</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>krzywy</trd>,
      <trd>zgi&eogonek;ty</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_p;&c_r;&c_ja;&c_m;&c_o;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_i;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_m;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_ib;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">k&rcaron;iv&yacute;</trd>
  <trdgrp lng="fr">
    <trd>de <ind>guingois</ind></trd>,
    <trd>indirect</trd>,
    <trd>tordu</trd>,
    <trd>pench&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12414;&#12364;&#12387;&#12383;</trd>,
    <trd>&#12422;&#12364;&#12435;&#12384;</trd>,
    <trd>&#20670;&#12356;&#12383;  <pr>&#12363;&#12383;&#12416;&#12356;&#12383;</pr></trd>,
    <trd>&#36960;&#22238;&#12375;&#12398;  <pr>&#12392;&#12362;&#12414;&#12431;&#12375;&#12398;</pr></trd>,
    <trd>&#22238;&#12426;&#36947;&#12398;  <pr>&#12414;&#12431;&#12426;&#12415;&#12385;&#12398;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
  <trd>kriv&yacute;</trd>,
  <trd>nepriamy</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_ja;&c_m;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26354; <pr>q&umacron;</pr></trd>,
    <trd>&#36384; <pr>w&#462;n</pr></trd>,
    <trd>&#38291;&#25509; <pr>ji&agrave;nji&emacron;</pr></trd>,
    <trd>&#38388;&#25509; <pr>ji&agrave;nji&emacron;</pr></trd>,
    <trd>&#39595; <pr>w&ecaron;i</pr></trd>,
    <trd>&#26857; <pr>yu&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rekt.ne0a">
  <kap>ne<tld/>a</kap>
  <snc mrk="rekt.ne0a.LIN">
  <uzo tip="fak">LIN</uzo>
    <dif>
      Citanta ies vortojn sub formo de dependa propozicio:
    </dif>
    <ekz>
    en la ne<tld/>a parolo oni konservas la tempon kaj modon de la
    <tld/>a
    parolo.
    </ekz>
    <ref tip="ant" cel="rekt.0a.LIN">rekta</ref>
    <rim num="1">
      Ne<tld/>a parolo ordinare estas ke-frazo a&ubreve; demanda
      subfrazo. <klr>[&#8230;]</klr> Ofte oni devas iom &scirc;an&gcirc;i la formon de la origina
      diro a&ubreve; penso<fnt><bib>PMEG</bib></fnt>.
      Ekzemple, <ekz>Petro diris: &leftquot;Mi volas doni ion al vi
      morga&ubreve;.&rightquot;</ekz> &#11106; <ekz>Petro
      diris, ke li volas doni ion al mi la postan tagon</ekz>.
      Ekzemple, <ekz>Karlo demandis: &leftquot;&Ccirc;u vi volas iri kun mi hodia&ubreve;?&rightquot;</ekz>
      &#11106; <ekz>Karlo demandis, &ccirc;u mi volas iri kun li tiun tagon</ekz>. Ekzemple,
      <ekz>hiera&ubreve; li diris: &leftquot;Kial vi volas fari tion nun kaj ne poste!&rightquot;</ekz>
      &#11106; <ekz>Hiera&ubreve; li diris, kial mi volas fari tion tuj (a&ubreve; tiumomente a&ubreve; tiam) kaj
      ne poste</ekz>. Ekzemple, <ekz>mi volas diri: &leftquot;Iru tien!&rightquot;</ekz> &#11106; <ekz>Mi volas,
      ke vi iru tien</ekz>.
    </rim>
    <rim num="2">
    En Esperanto la konkordo de tempoj ne ekzistas, ilia uzo dependas nur de la postuloj de la
    penso. &Ccirc;iu propozicio konservas en subordigo la saman tempon kiel &gcirc;i havus, se &gcirc;i estus &ccirc;efa:
    <ekz> mi ne sciis, ke li venos</ekz>
    </rim>
  <trd lng="en">indirect</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_k;&c_o;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_s;&c_r;&c_o;&c_d;&c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nep&rcaron;&iacute;m&yacute;</trd>,
    <trd>zprost&rcaron;edkovan&yacute;</trd>
  </trdgrp>
  <trd lng="fr">indirect<klr> (discours)</klr></trd>
  <trd lng="id"><ind>tidak</ind> langsung</trd>
  <trdgrp lng="ja">
    <trd>&#38291;&#25509;&#12398;  <pr>&#12363;&#12435;&#12379;&#12388;&#12398;</pr></trd>,
    <trd>&#36960;&#22238;&#12375;&#12398;  <pr>&#12392;&#12362;&#12414;&#12431;&#12375;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">zale&zdot;na  <klr>(mowa)</klr></trd>
  <trdgrp lng="sk">
    <trd>nepriamy</trd>,
    <trd>sprostredkovan&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_je;&c_p;&c_r;&c_ja;&c_m;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#38291;&#25509; <pr>ji&agrave;nji&emacron;</pr></trd>,
    <trd>&#38388;&#25509; <pr>ji&agrave;nji&emacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rekt.sam0a">
  <kap>sam<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="rekt.sam0a.MAT">
    <dif>
      <klr>(p.p. <ref cel="punkt.0o.spaco">punktoj</ref> a&ubreve; aroj
        da punktoj)</klr>
      Apartenantaj al <klr>(a&ubreve; inkluzivataj de)</klr> unu sama
      <ref cel="rekt.0o.MAT"><tld/>o</ref>:
      <ekz>
        du punktoj estas &ccirc;iam sam<tld/>aj;
      </ekz>
      <ekz>
        sam<tld/>aj strekoj <klr>(segmentoj el unu sama
        <tld/>o)</klr>;
      </ekz>
      <ekz>
        &ccirc;iu punkto de <ref cel="strek.0o.segmento">streko</ref>
        estas sam<tld/>a kun ties randoj;
      </ekz>
      <ekz>
        tra tri nesam<tld/>aj punktoj trairas nur unu ebeno.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_n;&c_je;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="de">kollinear</trd>
