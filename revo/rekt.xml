    <trd>&#x56DE;&#x308A;&#x9053;&#x306E;  <pr>&#x307E;&#x308F;&#x308A;&#x307F;&#x3061;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
