    <trd><baz>garis</baz>segaris</trd>,
    <trd>kolinear</trd>
  </trdgrp>
  <trd lng="pl">wsp&oacute;&lstroke;liniowy</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_i;&c_n;&c_je;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#20849;&#32218; <pr>g&ograve;ngxi&agrave;n</pr></trd>,
    <trd>&#20849;&#32447; <pr>g&ograve;ngxi&agrave;n</pr></trd>
  </trdgrp>
</drv>

