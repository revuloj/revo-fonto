      </ekz>
      <ekz>
        Ale&ccirc;jo el<tld/>i&gcirc;is kaj almetis la manon al la koro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>aufrichten</ind></trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_r;&c_o;&c_s;&c_t;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">se <ind>redresser</ind></trd>
