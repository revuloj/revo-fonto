    <trd><baz>rata</baz>perata arus listrik</trd>,
    <trd><baz>arah</baz>penyearah</trd>
  </trdgrp>
  <trd lng="pl">prostownik</trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_r;&c_ja;&c_m;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#25972;&#27969;&#22120; <pr>zh&ecaron;ngli&uacute;q&igrave;</pr></trd>,
    <trd>&#25289;&#30452;&#32773; <pr>l&amacron;zh&iacute;zh&ecaron;</pr></trd>,
    <trd>&#25913;&#38761;&#32773; <pr>g&#462;ig&eacute;zh&ecaron;</pr></trd>,
    <trd>&#30699;&#27491;&#32773; <pr>ji&#462;ozh&egrave;ngzh&ecaron;</pr></trd>,
    <trd>&#30703;&#27491;&#32773; <pr>ji&#462;ozh&egrave;ngzh&ecaron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rekt.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib>, <lok>p. 25</lok></fnt>
  <snc mrk="rekt.0ilo.ilo">
    <dif>
      Ilo, uzata por desegni <tld/>ajn liniojn:
      <ekz>
        T-kapa <tld/>ilo <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        la e&ubreve;klida distanco <klr>[&#8230;]</klr> estas la &leftquot;ordinara&rightquot; distanco inter du
        punktoj, mezurebla per <tld/>ilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105486">E&ubreve;klida distanco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_r;&c_y;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="cs">prav&iacute;tko</trd>
  <trd lng="de">Lineal</trd>
  <trd lng="en">ruler</trd>
  <trd lng="fr">r&egrave;gle<klr tip="ind"> (&agrave; dessin)</klr></trd>
  <trd lng="hu">vonal&oacute;z</trd>
  <trdgrp lng="id">
    <trd><baz>garis</baz>penggaris</trd>,
    <trd>mistar</trd>
  </trdgrp>
  <trd lng="ja">&#23450;&#35215;  <pr>&#12376;&#12423;&#12358;&#12366;</pr></trd>
  <trdgrp lng="pl">
    <trd>linia</trd>,
    <trd>linijka</trd>,
    <trd>linia&lstroke;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_k;&c_a; <klr tip="ind">(&c_d;&c_l;&c_ja; &c_ch;&c_je;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <trd lng="sk">prav&iacute;tko</trd>
  <trd lng="uk">&c_l;&c_ib;&c_n;&c_ib;&c_j;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#30452;&#23610; <pr>zh&iacute;ch&#464;</pr></trd>,
    <trd>&#23610;&#23376; <pr>ch&#464;zi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rekt.duon0o">
  <kap>duon<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib>, <lok>p. 26</lok></fnt>
  <snc mrk="rekt.duon0o.MAT">
    <dif>
      <klr>(en <ref cel="spac.afina0o">afina spaco</ref>)</klr>
      <ref cel="ar.0o.MAT">Aro</ref> de &ccirc;iuj punktoj de la tipo
      <frm am="lambda*u+O">&lambda;&#8729;<k>u</k>+O</frm>, kie <frm am="O">O</frm>
      estas iu punkto <klr>(&gcirc;ia
        <ref cel="origin.0o.de_duonrekto">origino</ref>)</klr>,
      <frm am="u"><k>u</k></frm>
      estas iu nenula vektoro <klr>(direktanta vektoro)</klr> kaj
      <frm am="lambda">&lambda;</frm> estas arbitra pozitiva reelo:
      <ekz>
        la radiusa duon<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=228392">Rotacia simetrio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        duon<tld/>o ne estas afina subspaco;
      </ekz><ekz>
        intuicie, duon<tld/>o kun origino <frm am="O">O</frm> estas &ccirc;iu
        el la du linioj, kiujn oni estigas, rompante <tld/>on &ccirc;e
        unu &gcirc;ia punkto <frm am="O">O</frm>.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>ray <klr>(geometry)</klr></trd>,
    <trd>half-<ind>line</ind></trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_p;&c_r;&c_o;&c_s;&c_t;&c_a;&c_ja; <klr>(&c_l;&c_ib;&c_n;&c_ib;&c_ja;)</klr></trd>
  <trd lng="cs">polop&rcaron;&iacute;mka</trd>
  <trdgrp lng="de">
      <trd>Halbgerade</trd>,
      <trd>Strahl<klr tip="ind"> (Halbgerade)</klr></trd>
    </trdgrp>
  <trd lng="fr">demi-droite</trd>
  <trd lng="hu">f&eacute;legyenes</trd>
  <trd lng="id"><baz>tengah</baz>setengah garis</trd>
  <trd lng="pl">p&oacute;&lstroke;prosta</trd>
  <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_l;&c_u;&c_p;&c_r;&c_ja;&c_m;&c_a;&c_ja;</trd>,
      <trd>&c_l;&c_u;&c_ch;<klr tip="ind">
          (&c_g;&c_je;&c_o;&c_m;.)</klr></trd>
    </trdgrp>
  <trd lng="sk">polpriamka</trd>
  <trdgrp lng="zh">
    <trd>&#23556;&#32218; <pr>sh&egrave;xi&agrave;n</pr></trd>,
    <trd>&#23556;&#32447; <pr>sh&egrave;xi&agrave;n</pr></trd>,
    <trd>&#21322;&#30452;&#32218; <pr>b&agrave;nzh&iacute;xi&agrave;n</pr></trd>,
    <trd>&#21322;&#30452;&#32447; <pr>b&agrave;nzh&iacute;xi&agrave;n</pr></trd>,
    <trd>&#21322;&#32218; <pr>b&agrave;nxi&agrave;n</pr></trd>,
    <trd>&#21322;&#32447; <pr>b&agrave;nxi&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rekt.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rekt.el0igxi.sinteno">
    <dif>
      <tld lit="R"/>igi siajn dorson kaj nukon post kurba sinteno:
      <ekz>
        &scirc;i el<tld/>i&gcirc;is, volis haltigi la &ccirc;evalon kaj desalti
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ale&ccirc;jo el<tld/>i&gcirc;is kaj almetis la manon al la koro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
