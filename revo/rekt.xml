      <trd>direkt-</trd>,
      <trd>w&ouml;rtlich</trd>
    </trdgrp>
    <trd lng="fr">direct<klr> (discours)</klr></trd>
    <trd lng="pl">mowa niezale&zdot;na</trd>
  </snc>
