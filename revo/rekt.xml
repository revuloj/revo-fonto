<drv mrk="rekt.euxlera0o">
  <kap>e&ubreve;lera <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="rekt.euxlera0o.MAT">
    <dif>
      <klr>(de neegallatera <ref cel="angul.tri0o">triangulo</ref>)</klr>
      <ref tip="super" cel="rekt.0o.MAT"><tld lit="R"/>o<sncref/></ref>,
      trairanta &gcirc;ian <ref cel="ortoce.0o">ortocentron</ref>,
      &gcirc;ian <ref cel="centr.pezo0o.MAT">pezocentron</ref>,
      la centron de &gcirc;ia <ref cel="skrib.cxirkaux0ita">&ccirc;irka&ubreve;skribita</ref>
      cirklo kaj la centron de la <ref cel="cirkl.euxlera0o">e&ubreve;lera cirklo</ref>:
      <ekz>
        la altocentro, la pezocentro, centro de &ccirc;irka&ubreve;skribita cirklo kaj centro de la
        e&ubreve;lera cirklo &ccirc;iuj ku&scirc;as sur sola <tld/>o nomata la e&ubreve;lera <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=237789">Alto (triangulo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="euxler.0o">E&ubreve;lero.</ref>
  </snc>
  <trd lng="be">&c_e;&c_w;&c_l;&c_je;&c_r;&c_a;&c_v;&c_a; <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_a;&c_ja;</ind></trd>
