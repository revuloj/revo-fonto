<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inkub</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="inkub.0o">
  <kap><tld/>o</kap>
  <snc mrk="inkub.0o.songxo">
    <dif>
      Peniga a&ubreve; teruranta <ref tip="super" cel="songx.0o">son&gcirc;o</ref> kun sento de premeganta pezo.
    </dif>
    <trdgrp lng="de">
      <trd>Alptraum</trd>,
      <trd>Albtraum</trd>,
      <trd>Alpdr&uuml;cken</trd>,
      <trd>Alpdruck</trd>
    </trdgrp>
    <trd lng="en">nightmare</trd>
    <trd lng="es">pesadilla</trd>
    <trdgrp lng="fr">
      <trd>cauchemar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lid&eacute;rcnyom&aacute;s</trd>,
      <trd>r&eacute;m&aacute;lom</trd>
    </trdgrp>
    <trd lng="nl">nachtmerrie</trd>
    <trd lng="ru">&c_k;&c_o;&c_sh;&c_m;&c_a;&c_r;</trd>
  </snc>
  <snc mrk="inkub.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Diablo, kiu prenis formon de viro por kuni&gcirc;i kun virino.
    </dif>
    <trdgrp lng="de">
      <trd>Alp</trd>,
      <trd>Alb</trd>,
      <trd>Inkubus</trd>,
      <trd>Nachtmahr</trd>,
      <trd>Nachtgespenst</trd>
    </trdgrp>
    <trd lng="en">incubus</trd>
    <trdgrp lng="fr">
      <trd>incube</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lid&eacute;rc</trd>,
      <trd>gonosz <ind>szellem</ind></trd>
    </trdgrp>
    <trd lng="nl">incubus</trd>
    <trd lng="ru">&c_i;&c_n;&c_k;&c_u;&c_b;</trd>
  </snc>
</drv>
</art>
