<drv mrk="spec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spec.0o.klaso">
    <dif>
      <ref tip="super" cel="klas.0o.speco">Klaso</ref> de vivuloj
      a&ubreve; a&jcirc;oj kun komunaj karakterizaj ecoj:
      <ekz>
        niaj grenejoj <klr>[&#8230;]</klr> enhavas sufi&ccirc;e da greno de &ccirc;iu <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 144:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi punos ilin per kvar <tld/>oj <klr>[&#8230;]</klr>: per glavo, por mortigi, per
        hundoj, por treni, per birdoj de la &ccirc;ielo kaj per bestoj de la tero, por
        forman&gcirc;i kaj ekstermi
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 15:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas tiom da <tld/>oj de vo&ccirc;oj en la mondo, kaj nenia estas sensignifa
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 14:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i palaco estis konstruita el helflava brilanta <tld/>o de &scirc;tono
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la germana biero tiam estis bonega kaj ekzistis diversaj <tld/>oj de &gcirc;i: Brema,
        Prisinga, Emsa biero, e&ccirc; la tiel nomata Brunsvika mumio
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas homo de tute speciala <tld/>o! li havas bonan kapon, sed li nenion
        faras
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>!
      </ekz>
      <ekz>
        tie estis tre multe da ekzista&jcirc;oj, &ccirc;iuj estis el tiu <tld/>o, kiun ni nomas
        homoj, sed ili aspektis tute alie ol ni
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        el tiu <tld/>o de vivantaj malbonaj homoj staris nun du apud la mortinta viro
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="katego.0o">kategorio</ref>,
      <ref cel="manier.0o.KOMUNE">maniero</ref>,
      <ref cel="ras.0o.subspecio">raso<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_v;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="ms">semua jenis</trd>
    <trdgrp lng="ca">
      <trd>tipus</trd>,
      <trd>classe</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Art</trd>,
      <trd>Sorte</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>sort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_r;&c_t;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="th">&#3594;&#3609;&#3636;&#3604;</trd>
  </snc>
  <snc mrk="spec.0o.similajxo">
    <dif>
      <klr>(evitante precizan difinon per indiko de 
      karakteriza&jcirc;o a&ubreve; simila&jcirc;o)</klr> 
      &leftquot;io simila, komparebla al&rightquot;, &leftquot;reprezentanto de&rightquot;:
      <ekz>
        &gcirc;i havis en si ian <tld/>on de flartabako
        <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda
        Kristino</lok></fnt>;
      </ekz>
      <ekz>
        se oni povus rekte enrigardi al ili en la bruston! tio estus ja ekspozicio,
        <tld/>o de magazeno
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>!
      </ekz>
      <ekz>
        tio estis unu el la tiuepokaj gastejoj, <tld/>o de bierejo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        trovi&gcirc;is nur unu, kiu revenis kun ia <tld/>o de klarigo
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio ver&scirc;ajne estas <tld/>o de &gcirc;ardena kreska&jcirc;o! ili diris kaj moke ridis
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &scirc;ipo <klr>[&#8230;]</klr> oni anka&ubreve; trovi&gcirc;as en <tld/>o de ka&scirc;i&gcirc;ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la laboristinoj sekvis &scirc;in per siaj rigardoj, <klr>[&#8230;]</klr> kun simpatio kaj kun
        <tld/>o de triumfo sur la viza&gcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> sin banis en &scirc;tona banujo kaj &jcirc;etis sur sin <tld/>on de granda
        litotuko, kiun li bukis sub la kolo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alegor.0o">alegorio</ref>,
      <ref cel="fason.0o">fasono</ref>,
      <ref cel="form.0ajxo">forma&jcirc;o</ref>,
      <ref cel="simil.0ajxo">simila&jcirc;o</ref>,
      <ref cel="vari.0ajxo">varia&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Art <klr>(von)</klr></trd>,
      <trd>Auspr&auml;gung</trd>
    </trdgrp>
    <trd lng="en">a <ind>kind</ind> <klr>(of)</klr></trd>
  </snc>

