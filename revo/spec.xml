<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spec</rad>/o
</kap>
<drv mrk="spec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Subdivido de genro; grupo da vivantaj esta&jcirc;oj prezentantaj plurajn
      difinitajn karakterizajn ecojn, kiuj konsistigas heredan tipon,
      ordinare ne&scirc;an&gcirc;eblan en la nunepokaj kondi&ccirc;oj.
    </dif>
    <ref tip="sin" cel="speci.0o">specio</ref>
    <trd lng="ru">&c_v;&c_i;&c_d;</trd>
  </snc>
  <snc>
    <dif>
      Aro da esta&jcirc;oj a&ubreve; objektoj havantaj komunajn karakterizajn ecojn:
      <ekz>
        iu <tld/>o da bestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &gcirc;i havis en si ian <tld/>on de flartabako<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_r;&c_t;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Karakteriza eco a&ubreve; maniero:
      <ekz>
        divers<tld/>aj vazoj;
      </ekz>
      <ekz>
        diversa<tld/>a literaturo<fnt>Z</fnt>,
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_t;&c_i;&c_p;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Art</trd>
  <trd lng="en">species</trd>
  <trd lng="nl">soort</trd>
</drv>

<drv mrk="spec.super0o0aro">
  <kap>super<tld/>o, <tld/>aro</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="genr.0o">Genro</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_o;&c_d;</trd>
</drv>

<drv mrk="spec.sub0o">
  <kap>sub<tld/>o</kap>
  <snc>
    <dif>
      Subdivido de <tld/>o; grupo da vegeta&jcirc;oj, kiuj apartenas al sama
      <tld/>o, sed iom malsimilas inter si per kelkaj negravaj karakteroj.
    </dif>
  </snc>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_i;&c_d;</trd>
</drv>
</art>
