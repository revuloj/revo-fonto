<drv mrk="spec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spec.0o.klaso">
    <dif>
      <ref tip="super" cel="klas.0o.speco">Klaso</ref> de vivuloj
      a&ubreve;
      a&jcirc;oj kun komunaj karakterizaj ecoj:
      <ekz>
        <tld/>o de bestoj;
      </ekz>
      <ekz>
        &gcirc;i havis en si ian <tld/>on de flartabako
        <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda
        Kristino</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_v;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="ms">semua jenis</trd>
    <trdgrp lng="ca">
      <trd>tipus</trd>,
      <trd>classe</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>sort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_r;&c_t;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="th">&#xE0A;&#xE19;&#xE34;&#xE14;</trd>
  </snc>
  <snc mrk="spec.0o.subklaso">
    <dif>
      Karakteriza eco a&ubreve; maniero:
      <ekz>
        niaj grenejoj estas plenaj, enhavas sufi&ccirc;e da greno de
        &ccirc;iu <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 144:13</lok></fnt>;
      </ekz>
      <ekz>
        diversa<tld/>a literaturo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_v;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_t;&c_y;&c_p;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>g&egrave;nere</trd>,
      <trd>mena</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>sort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_t;&c_i;&c_p;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="th">&#xE1B;&#xE23;&#xE30;&#xE40;&#xE20;&#xE17;</trd>
  </snc>
  <snc mrk="spec.0o.BIO">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="speci.0o">specio</ref>
    <trd lng="be">&c_v;&c_ib;&c_d;</trd>
    <trd lng="en">species</trd>
    <trd lng="es">especie</trd>
    <trd lng="ru">&c_v;&c_i;&c_d;</trd>
    <trd lng="th">&#xE2A;&#xE1B;&#xE35;&#xE0A;&#xE35;&#xE2A;&#xE4C;</trd>
  </snc>
  <trd lng="af">soort</trd>
  <trd lng="am">&#x12F0;&#x130D;</trd>
  <trd lng="ar">&a_n;&a_w;&a_ayn;</trd>
  <trd lng="az">c&uuml;r</trd>
  <trd lng="bn">&#x9A7;&#x9B0;&#x9A8;&#x9C7;&#x9B0;</trd>
  <trd lng="bs">vrste</trd>
  <trd lng="ca">esp&egrave;cie</trd>
