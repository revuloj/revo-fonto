  <rad>spec</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="spec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spec.0o.klaso">
    <dif>
      <ref tip="super" cel="klas.0o.speco">Klaso</ref> de vivuloj
      a&ubreve;
      a&jcirc;oj kun komunaj karakterizaj ecoj:
      <ekz>
        <tld/>o de bestoj;
      </ekz>
      <ekz>
        &gcirc;i havis en si ian <tld/>on de flartabako
        <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda
        Kristino</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_v;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="ms">semua jenis</trd>
    <trdgrp lng="ca">
      <trd>tipus</trd>,
      <trd>classe</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>sort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_r;&c_t;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="spec.0o.subklaso">
    <dif>
      Karakteriza eco a&ubreve; maniero:
      <ekz>
        niaj grenejoj estas plenaj, enhavas sufi&ccirc;e da greno de
        &ccirc;iu <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 144:13</lok></fnt>;
      </ekz>
      <ekz>
        diversa<tld/>a literaturo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_v;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_t;&c_y;&c_p;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>g&egrave;nere</trd>,
      <trd>mena</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kind</trd>,
      <trd>sort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;</trd>,
      <trd>&c_t;&c_i;&c_p;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="spec.0o.BIO">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="speci.0o">specio</ref>
    <trd lng="be">&c_v;&c_ib;&c_d;</trd>
    <trd lng="en">species</trd>
    <trd lng="es">especie</trd>
    <trd lng="ru">&c_v;&c_i;&c_d;</trd>
  </snc>
  <trd lng="af">soort</trd>
  <trd lng="am">&#x12F0;&#x130D;</trd>
  <trd lng="ar">&#x646;&#x648;&#x639;</trd>
  <trd lng="az">c&#xFC;r</trd>
  <trd lng="bn">&#x9A7;&#x9B0;&#x9A8;&#x9C7;&#x9B0;</trd>
  <trd lng="bs">vrste</trd>
  <trd lng="ca">esp&egrave;cie</trd>
  <trd lng="co">curtisi</trd>
  <trdgrp lng="cs">
    <trd>druh <klr>(zbo&zcaron;&iacute;)</klr></trd>,
    <trd>sorta</trd>
  </trdgrp>
  <trd lng="da">slags</trd>
  <trd lng="de">Art</trd>
  <trd lng="et">liiki</trd>
  <trd lng="eu">nolako</trd>
  <trdgrp lng="fr">
    <trd>esp&egrave;ce</trd>,
    <trd>sorte</trd>
  </trdgrp>
  <trd lng="fy">soart</trd>
  <trd lng="gl">tipo</trd>
  <trd lng="gu">&#xAAA;&#xACD;&#xAB0;&#xA95;&#xABE;&#xAB0;&#xAA8;&#xAC0;</trd>
  <trd lng="ha">irin</trd>
  <trd lng="hi">&#x924;&#x930;&#x939;</trd>
  <trd lng="hr">vrsta</trd>
  <trdgrp lng="hu">
    <trd>fajta</trd>,
    <trd>nem <klr tip="amb">(f&eacute;les&eacute;g)</klr></trd>,
    <trd>f&eacute;les&eacute;g</trd>
  </trdgrp>
  <trd lng="hy">&#x562;&#x561;&#x580;&#x56B;</trd>
  <trd lng="is">g&#xF3;&#xF0;ur</trd>
  <trd lng="ja">&#x7A2E;&#x985E;</trd>
  <trd lng="jw">jenis</trd>
  <trd lng="ka">&#x10E1;&#x10D0;&#x10EE;&#x10D8;&#x10E1;</trd>
  <trd lng="km">&#x1794;&#x17D2;&#x179A;&#x1797;&#x17C1;&#x1791;</trd>
  <trd lng="ko">&#xC885;&#xB958;</trd>
  <trd lng="lo">&#xE9B;&#xEB0;&#xEC0;&#xE9E;&#xE94;</trd>
  <trd lng="ml">&#xD24;&#xD30;&#xD02;</trd>
  <trd lng="mr">&#x92A;&#x94D;&#x930;&#x915;&#x93E;&#x930;&#x91A;&#x940;</trd>
  <trd lng="my">&#x1021;&#x1019;&#x103B;&#x102D;&#x102F;&#x1038;&#x1021;&#x1005;&#x102C;&#x1038;</trd>
  <trd lng="ne">&#x926;&#x92F;&#x93E;&#x932;&#x941;</trd>
  <trd lng="nl">soort</trd>
  <trd lng="ny">mtundu</trd>
  <trd lng="pa">&#xA26;&#xA3F;&#xA06;&#xA32;&#xA42;</trd>
  <trd lng="ps">&#x689;&#x648;&#x644;</trd>
  <trd lng="sd">&#x642;&#x633;&#x645;</trd>
