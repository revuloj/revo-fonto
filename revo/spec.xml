<drv mrk="spec.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spec.0igi.KOMUNE">
    <dif>
      <ref tip="super" cel="klas.0i">Klasi</ref>, provante atribui
      la &gcirc;ustajn <tld/>ojn:
      <ekz>
        la paketojn oni en la lastaj tagoj elprenis el la kestoj kaj
        <tld/>igis
        <fnt><bib>FK</bib>, <lok>El la
        po&scirc;to</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">classificar</trd>
  <trdgrp lng="cs">
    <trd>rozt&rcaron;&iacute;dit podle druh&uring;</trd>,
    <trd>roz&ccaron;lenit podle druh&uring;</trd>,
    <trd>se&rcaron;adit podle druh&uring;</trd>
  </trdgrp>
  <trd lng="de">sortieren</trd>
  <trdgrp lng="fr">
    <trd>cat&eacute;goriser</trd>,
    <trd>trier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>oszt&aacute;lyoz</trd>,
    <trd>szort&iacute;roz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>uitzoeken</trd>,
    <trd>sorteren</trd>
  </trdgrp>
  <trd lng="sk">zoradi&tcaron; pod&lcaron;a druhov</trd>
  <trd lng="uk">&c_s;&c_o;&c_r;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="spec.ali0a">
  <kap>ali<tld/>a</kap>
  <snc mrk="spec.ali0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="ali.0a.diferenca">Alia</ref> la&ubreve; sia
      <tld/>o a&ubreve; konsisto:
      <ekz>
        li estis sentonta sub siaj piedoj pa&scirc;ejon ali<tld/>an ol la movi&gcirc;antan ferdekon
        de sia &scirc;ipo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj se iu ne obeus? &ndash; li ricevus ali<tld/>an alvokon: <klr>[&#8230;]</klr> tiun, de
        kiu oni ne revenas
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne estas via malamiko, nur ali<tld/>a ol vi
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        homgrupi&gcirc;oj, &ccirc;u teritoriaj, &ccirc;u profesiaj,
        &ccirc;u ali<tld/>aj
        <fnt>
          <bib>Lanti</bib>, 
          <lok>&leftquot;Liberiga Stelo&rightquot; al la Verdru&gcirc;uloj, celo kaj rimedoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apart.0a.speciala">aparta<sncref/></ref>,
      <ref cel="specia.0a">speciala</ref>
    </refgrp>
  </snc>
  <trd lng="ca">d'una altra mena</trd>
  <trdgrp lng="cs">
    <trd>druh&yacute;</trd>,
    <trd>jin&yacute;</trd>
  </trdgrp>
  <trd lng="de">andersartig</trd>
  <trd lng="en">of another kind</trd>
  <trd lng="es">de otro tipo</trd>
  <trd lng="fr">diff&eacute;rent<klr tip="amb"> (d'une autre
      sorte)</klr></trd>
  <trd lng="hu">m&aacute;sfajta</trd>
  <trd lng="nl">van een andere <ind>soort</ind></trd>
  <trdgrp lng="sk">
    <trd>druh&yacute;</trd>,
    <trd>inak&scaron;&iacute;</trd>,
    <trd>in&yacute;</trd>,
    <trd>onak&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="spec.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="spec.bon0a.KOMUNE">
    <dif>
      <ref tip="super" cel="bon.0a.tauga">Bona</ref> la&ubreve; sia
      <tld/>o, konsisto, propra kvalito:
      <ekz>
        plej bon<tld/>a ar&gcirc;ento
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej bon<tld/>a pano
        <fnt><bib>Fab3</bib>, <lok>kolo de botelo</lok></fnt>;
      </ekz>
      <ekz>
        li estis ri&ccirc;a, havis bon<tld/>ajn vestojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fajn.0a">fajna</ref>,
      <ref cel="pur.0a">pura</ref>,
      <ref cel="rafin.0ita">rafinita</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>de qualitat</trd>,
    <trd>de bona condici&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>edel</trd>,
    <trd>hochwertig</trd>
  </trdgrp>
  <trd lng="fr">de bonne <ind>qualit&eacute;</ind></trd>
  <trd lng="hu">j&oacute;fajta</trd>
  <trd lng="nl">van goede <ind>kwaliteit</ind></trd>
  <trd lng="uk">&c_d;&c_o;&c_b;&c_r;&c_o;&c_ja;&c_k;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="spec.cxia0a">
  <kap>&ccirc;ia<tld/>a</kap>
  <snc mrk="spec.cxia0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="cxia.0">&Ccirc;ia</ref>, tre <ref tip="vid" cel="divers.0a.kelkaj">diversa</ref> la&ubreve; <tld/>oj:
      <ekz>
        la popoloj de la lando venigos la komerca&jcirc;ojn a&ubreve;
        &ccirc;ia<tld/>an grenon
        <fnt><bib>MT</bib>, <lok>&Neh; 10:32</lok></fnt>;
      </ekz>
      <ekz>
        fari &ccirc;ia<tld/>ajn turni&gcirc;ojn kaj movi&gcirc;ojn en
        la akvo simile al fi&scirc;o
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="vari.0a.diversa">varia<sncref/></ref>
  </snc>
  <trdgrp lng="ca">
    <trd>tot tipus de</trd>,
    <trd>tota mena de</trd>
  </trdgrp>
  <trd lng="de">aller <ind>Art</ind></trd>
  <trd lng="en">all kinds of</trd>
  <trd lng="fr">de <ind>toutes sortes</ind></trd>
  <trdgrp lng="hu">
    <trd>mindenf&eacute;le</trd>,
    <trd>mindenfajta</trd>
  </trdgrp>
  <trd lng="nl">van iedere <ind>soort</ind></trd>
</drv>

<drv mrk="spec.hom0o">
  <kap>hom<tld/>o</kap>
  <snc mrk="spec.hom0o.KOMUNE">
    <dif>
      Tipa <tld/>o de homo, de tipa karaktero:
      <ekz>
        tiuepoke, por mi, la mondo estis dividita en du 
        hom<tld/>oj, la bonaj kaj la a&ccirc;aj
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li 
        venis trakosme?</vrk>, <lok>1980</lok></fnt>;
      </ekz>
      <ekz>
        kvankam tuta mondo min disigis de tiu malfeli&ccirc;a hom<tld/>o, tamen mi elkore
        beda&ubreve;ris &ccirc;i tiun mizeran, kompatinde idiotan kaj trompitan virsekson
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvina &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Menschentyp</trd>,
    <trd><ind>Art</ind> von Mensch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>genre de personnalit&eacute;</trd>,
    <trd>type de personne</trd>
  </trdgrp>
</drv>

<drv mrk="spec.mikso0a">
  <kap>mikso<tld/>a</kap>
  <snc mrk="spec.mikso0a.KOMUNE">
    <dif>
      Naskita a&ubreve; farita el pluraj devenaj <tld/>oj:
      <ekz>
        ne surmetu sur vin mikso<tld/>an veston el lano kaj lino
        kune
        <fnt><bib>MT</bib>, <lok>&Rea; 22:11</lok></fnt>;
      </ekz>
      <ekz>
        mikso<tld/>aj bildoj de nigraj arbaroj, bruantaj akvoj, nuboj kaj ne&gcirc;amasoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pur.mal0a">malpura</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>h&iacute;brid</trd>,
    <trd>mixt</trd>
  </trdgrp>
  <trd lng="cs">sm&iacute;&scaron;en&yacute;</trd>
  <trd lng="de">vermischt</trd>
  <trdgrp lng="fr">
    <trd>hybride</trd>,
    <trd>d'esp&egrave;ce <ind>m&eacute;lang&eacute;</ind>e</trd>,
    <trd>de race <ind>m&ecirc;l&eacute;</ind>e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vegyes</trd>,
    <trd>tarka</trd>,
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;
    <ind>fajt&aacute;j&uacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gemengd</trd>,
    <trd>van verschillende <ind>soort</ind></trd>
  </trdgrp>
  <trd lng="sk">zmie&scaron;an&yacute;</trd>
</drv>

<drv mrk="spec.sam0ulo">
  <kap>sam<tld/>ulo</kap>
  <snc mrk="spec.sam0ulo.KOMUNE">
    <dif>
      Ulo de sama <tld/>o:
      <ekz>
        estas en la naturo de homo, la&ubreve; ili, agrable rilati 
        kun la sam<tld/>uloj
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i montras la gravecon, kiun la prahomo atribuis al la ebleco komuniki kun la
        sam<tld/>uloj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen <klr>[&#8230;]</klr> la senco de hunda vivo <klr>[&#8230;]</klr>, &gcirc;i ofte vagas tute
        sola kun afereca mieno dum &gcirc;i priflaras domangulojn, ligas rilatojn kun siaj
        sam<tld/>uloj kaj kaptas oston
        <fnt>
          <bib>Kiso</bib>
          <lok>Hundo Sen Mastro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gleichartiger</trd>,
    <trd><ind>Vertreter</ind> der gleichen Art</trd>
  </trdgrp>
  <trd lng="fr">individu de m&ecirc;me esp&egrave;ce</trd>
</drv>

<drv mrk="spec.sia0a">
  <kap>sia<tld/>a</kap>
  <snc mrk="spec.sia0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="apart.0a.speciala">Aparta</ref>, surpriza,
      kvaza&ubreve; per si mem estiganta propran <tld/>on:
      <ekz>
        sia<tld/>a rimarkinda&jcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>kun sia<tld/>a spico</ctl>
        <klr>(poemaro de <nom>Raymond Schwartz</nom>)</klr>
        <fnt><aut>R. Schwartz</aut>, <vrk>Kun siaspeca
        spico</vrk>, <lok>1931</lok></fnt>;
      </ekz>
      <ekz>
        la pupteatra festivalo <klr>[&#8230;]</klr> estas unika sia<tld/>a evento en la mondo
        de kulturo
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="unik.0a">unika</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>espec&iacute;fic</trd>,
    <trd>sui generis</trd>
  </trdgrp>
  <trd lng="cs">sv&eacute;ho druhu</trd>
  <trdgrp lng="de">
    <trd>einzigartiger</trd>,
    <trd>besonderer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dans son <ind>genre</ind></trd>,
    <trd>particulier<klr tip="amb"> (sp&eacute;cial)</klr></trd>,
    <trd>sp&eacute;cial<klr tip="amb"> (particulier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>saj&aacute;tos</trd>,
    <trd>k&uuml;l&ouml;nleges</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dergelijk</trd>,
    <trd>in zijn/haar <ind>soort</ind></trd>
  </trdgrp>
  <trd lng="sk">svojho druhu</trd>
  <trd lng="uk">&c_s;&c_v;&c_o;&c_jeu;&c_r;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="spec.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="spec.sub0o.KOMUNE">
    <dif>
      Subdivido de <tld/>o; grupo de aferoj, kiuj apartenas al sama <tld/>o, sed iom
      malsimilas inter si per kelkaj negravaj ecoj:
      <ekz>
        okcidenta svingo estas &jcirc;az-sub<tld/>o, en kiu intermiksi&gcirc;is ritmoj de tradiciaj
        svingo, bluso, diksilando kaj e&ccirc; polko-danco
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="https://www.monato.be/2012/010439.php"
              >Svingante la &scirc;tatan flagon</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="spec.sub0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <ref tip="dif" cel="speci.sub0o">subspecio</ref>
  <trd lng="de">&Uuml;berart</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_ib;&c_d;</trd>
  <trd lng="ca">subesp&egrave;cie</trd>
  <trdgrp lng="cs">
    <trd>poddruh</trd>,
    <trd>subspecies <klr>(plemeno, rasa)</klr></trd>
  </trdgrp>
  <trd lng="de">Unterart</trd>
  <trd lng="en">subtype</trd>
  <trdgrp lng="fr">
    <trd>sous-esp&egrave;ce</trd>,
    <trd>vari&eacute;t&eacute;<klr> (biol.)</klr></trd>
  </trdgrp>
  <trd lng="hu">alfaj</trd>
  <trd lng="nl">ondersoort</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_i;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>odroda</trd>,
    <trd>poddruh</trd>
  </trdgrp>
</drv>

<drv mrk="spec.super0o">
  <kap>super<tld/>o, <var><kap><tld/>aro</kap></var></kap>
  <snc mrk="spec.super0o.BIO">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="genr.0o.BIO">genro<sncref/></ref>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_d;</trd>
  <trd lng="ca">g&egrave;nere <klr>(taxon.)</klr></trd>
  <trd lng="fr">genre<klr> (biol.)</klr></trd>
  <trd lng="hu">nem <klr tip="amb">(biol&oacute;giai)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_d;</trd>
</drv>

<drv mrk="spec.tia0a">
  <kap>tia<tld/>a</kap>
  <snc mrk="spec.tia0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="tia.0">Tia</ref>, de tia <tld/>o, aspekto,
      sinteno:
      <ekz>
        jen mi donas al vi grajnon de hordeo, &gcirc;i ne estas tia<tld/>a, kia kreskas sur
        kampo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni klarigas tia<tld/>ajn aerajn fenomenojn per norda lumo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi, mia kara, ne estas tia<tld/>a
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno
        6a</lok></fnt>!
      </ekz>
      <ekz>
        tio estis danco, kiu por amantoj de tia<tld/>aj plezuroj tre bele aspektas
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel malfeli&ccirc;aj ili kredeble sin sentas, se tia<tld/>aj esta&jcirc;oj &gcirc;enerale povas
        senti, kiel mi kaj mia<tld/>aj esta&jcirc;oj sentas
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>aix&iacute;</trd>,
    <trd>d'eixa mena</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>analogick&yacute;</trd>,
    <trd>podobn&yacute;</trd>,
    <trd>tohoto druhu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>solcher <ind>Art</ind></trd>,
    <trd>solcher</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de ce <ind>genre</ind></trd>,
    <trd>de cette <ind>esp&egrave;ce</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ilyenfajta</trd>,
    <trd>ilyesf&eacute;le</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>analogick&yacute;</trd>,
    <trd>podobn&yacute;</trd>,
    <trd>tohto druhu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_u;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="spec.vorto0o">
  <kap>vorto<tld/>o</kap>
  <snc mrk="spec.vorto0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Leksika atributo de <ref cel="vort.0o.LIN">vortoj</ref>,
      apartiganta subaron de la
      <ref tip="malprt" cel="vort.0oprovizo">vortoprovizo</ref>
      la&ubreve; iu el diversaj principoj, eventuale malpli rigoraj
      ol tiuj arigantaj la
      <ref tip="sub" cel="elemen.parol0o">parolelementojn</ref>:
      <ekz>
        la interpunkcio <klr>[&#8230;]</klr> funkcias sendepende de vort<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006843.html"
              >Kiun vojon?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <ref cel="determ.0ilo">determiniloj</ref> formas
        buntan vorto<tld/>on: iuj el ili havas &ccirc;iujn kazojn kiel
        <ctl>tiu</ctl>, alia havas kazojn nur plurnombre (unujn),
        dum tria estas tute nevaria (la artikolo);
      </ekz>
      <ekz>
        la <ref cel="nombr.0onomo">nombronomoj</ref> estas
        vortospeco formita la&ubreve; tre klara principo semantika, kiu
        tamen prezentas tre eklektan bildon gramatikan (kp
        <ctl>unu homon</ctl> &#8208; <ctl>milionon da homoj</ctl>).
      </ekz>
    </dif>
    <trd lng="th">&#3594;&#3609;&#3636;&#3604;&#3586;&#3629;&#3591;&#3588;&#3635;</trd>
  </snc>
  <trd lng="ca">categoria l&egrave;xica</trd>
  <trd lng="cs">slovn&iacute; druh</trd>
  <trd lng="de">Wortart</trd>
  <trd lng="en">part of speech</trd>
  <trd lng="fr">classe lexicale</trd>
  <trd lng="hu">sz&oacute;faj</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_ja;&c_d;
  &c_s;&c_l;&c_o;&c_v;</trd>
  <trd lng="sk">slovn&yacute; druh</trd>
  <trd lng="uk">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a; &c_m;&c_o;&c_v;&c_i;</trd>
</drv>
</art>
