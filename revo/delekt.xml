<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: delekt.xml,v 1.8 2025/04/04 08:37:09 afido Exp $">
<kap>
  <rad>delekt</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="delekt.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="delekt.0i.plezurigi">
    <dif>
      Tre <ref tip="dif" cel="plezur.0igi">plezurigi</ref>:
      <ekz>
        &scirc;i sidi&gcirc;is apud mian liton kaj <tld/>is min per liuta muziko
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        vin &ccirc;iam <tld/>is ege la propraj diroj
        <fnt>
          <aut>J. Camacho</aut>
          <vrk><url ref="&Tekstaro;?nomo=la-majstro-kaj-martinelli">La Majstro kaj
          Martinelli</url></vrk>
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amuz.0i">amuzi</ref>,
      <ref cel="distr.0i.amuzi">distri<sncref/></ref>,
      <ref cel="gxoj.0igi">&gcirc;ojigi</ref>,
      <ref cel="gxu.0igi">&gcirc;uigi</ref>,
      <ref cel="rav.0i">ravi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_s;&c_a;&c_l;&c_o;&c_d;&c_u;</ind></trd>,
    <trd>&c_c;&c_je;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erfreuen</trd>,
    <trd>erg&ouml;tzen</trd>,
    <trd>entz&uuml;cken</trd>,
    <trd>vergn&uuml;gen</trd>
  </trdgrp>
  <trd lng="en">delight</trd>
  <trd lng="fr">d&eacute;lecter</trd>
  <trd lng="he">&lamed;&ayin;&nun;&gimel;</trd>
  <trd lng="pl">delektowa&cacute;</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i; &#x43D;&c_a;&c_s;&#x43E;&#x43B;&#x43E;&c_d;&c_u;</trd>
</drv>

<drv mrk="delekt.0inda">
  <kap>
    <tld/>inda
  </kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="delekt.0inda.KOMUNE">
    <dif>
      Plezurega, <ref tip="vid" cel="delic.0a">delica</ref>:
      <ekz>
        la prohibicio estas grava hontinda&jcirc;o kaj peko <klr>[&ccirc;ar &gcirc;i]</klr> forprenis de
        ni tiun <tld/>indan donacon de Dio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_w;&c_n;&c_y; <ind>&c_a;&c_s;&c_a;&c_l;&c_o;&c_d;&c_y;</ind></trd>,
    <trd>&c_u;&c_c;&c_je;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;lectable</trd>
  <trd lng="pl">warty delektowania si&eogonek;</trd>
</drv>
</art>

