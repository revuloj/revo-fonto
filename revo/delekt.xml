<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: delekt.xml,v 1.5 2022/03/15 15:50:54 afido Exp $">
<kap>
  <rad>delekt</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="delekt.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="delekt.0i.plezurigi">
    <dif>
      Tre <ref tip="dif" cel="plezur.0igi">plezurigi</ref>:
      <ekz>
        &scirc;i sidi&gcirc;is apud mian liton kaj <tld/>is min per liuta muziko
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        vin &ccirc;iam <tld/>is ege la propraj diroj
        <fnt>
          <aut>J. Camacho</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=la-majstro-kaj-martinelli">La Majstro kaj
          Martinelli</url></vrk>
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amuz.0i">amuzi</ref>,
      <ref cel="distr.0i.amuzi">distri<sncref/></ref>,
      <ref cel="gxoj.0igi">&gcirc;ojigi</ref>,
      <ref cel="gxu.0igi">&gcirc;uigi</ref>,
      <ref cel="rav.0i">ravi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>erfreuen</trd>,
    <trd>erg&ouml;tzen</trd>,
    <trd>entz&uuml;cken</trd>,
    <trd>vergn&uuml;gen</trd>
  </trdgrp>
  <trd lng="en">delight</trd>
  <trd lng="fr">d&eacute;lecter</trd>
  <trd lng="he">&lamed;&ayin;&nun;&gimel;</trd>
  <trd lng="uk">&#x43F;&#x440;&#x438;&#x43D;&#x43E;&#x441;&#x438;&#x442;&#x438; &#x43D;&#x430;&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x443;</trd>
</drv>

<drv mrk="delekt.0inda">
  <kap>
    <tld/>inda
  </kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="delekt.0inda.KOMUNE">
    <dif>
      Plezurega, <ref tip="vid" cel="delic.0a">delica</ref>:
      <ekz>
        la prohibicio estas grava hontinda&jcirc;o kaj peko <klr>[&ccirc;ar &gcirc;i]</klr> forprenis de
        ni tiun <tld/>indan donacon de Dio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;lectable</trd>
</drv>
</art>

