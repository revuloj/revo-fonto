<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: drajx.xml,v 1.2 2025/06/12 10:35:54 afido Exp $">
    <kap>
      <rad>dra&jcirc;</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="drajx.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo><uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="sxos.0o">&Scirc;oso</ref>, kiu je certa distanco de la radiko
      kontakti&gcirc;anta kun grundo formas vegeta&jcirc;idon:
      <ekz>
        tiuj rozoj ne estis greftitaj, ties <tld/>oj kaj stika&jcirc;oj floros kiel la devena
        planto
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2008/009355.php"
              >Nordiaj rozoj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="markot.0o">markoto</ref>,
      <ref cel="stolon.0o">stolono</ref>
    </refgrp>
    <trd lng="de">Absenker</trd>
    <trd lng="en">layer <klr>(of a plant)</klr></trd>
    <trdgrp lng="fr">
      <trd>marcotte</trd>,
      <trd>provin</trd>
    </trdgrp>
  </snc>
<trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>
</drv>

  </art>

