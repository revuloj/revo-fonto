<drv mrk="hard.0iteco">
  <kap><tld/>iteco, <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="hard.0iteco.KOMUNE">
    <dif>
      Kvalito de io <tld/>ita a&ubreve; &gcirc;enerale rezista kontra&ubreve;
      la enigo de alia korpo:
      <ekz>
        <tld/>iteco de gipso, hardeco de &scirc;talo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kelkaj fotoj montris la arkitekton, bone substrekantaj ties batalemon kaj
        <tld/>itecon, ties guston al potenco
        <fnt>
          <bib>ChB</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tremp</trd>,
    <trd>fortalesa</trd>
  </trdgrp>
  <trd lng="cs">tvrdost</trd>
  <trd lng="de">H&auml;rte</trd>
  <trdgrp lng="fr">
    <trd>duret&eacute;</trd>,
    <trd>endurcissement</trd>,
    <trd>trempe<klr tip="ind"> (caract&egrave;re endurci)</klr></trd>,
  </trdgrp>
  <trd lng="hu">edzetts&eacute;g</trd>
  <trdgrp lng="it">
      <trd>tempra</trd>,
      <trd>durezza</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28988;&#20837;&#12428;&#21152;&#28187;  <pr>&#12420;&#12365;&#12356;&#12428;&#12363;&#12370;&#12435;</pr></trd>,
    <trd>&#24375;&#22266;&#12373;  <pr>&#12365;&#12423;&#12358;&#12371;&#12373;</pr></trd>
  </trdgrp>
  <trd lng="nl">gehardheid</trd>
  <trd lng="pt">dureza</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">tvrdos&tcaron;</trd>
  <trd lng="uk">&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&#1086;&c_v;&c_a;&#1085;&c_ib;&c_s;&c_t;&#1100;</trd>
  <trdgrp lng="zh">
    <trd>&#20005;&#37325;&#24615; <pr>y&#225;nzh&#242;ngx&#236;ng</pr></trd>,
    <trd>&#30828;&#24230; <pr>y&#236;ngd&#249;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hard.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hard.mal0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Renversi la rezulton de <tld/>ado, donante al
      metalo unuforman, konstantan strukturon, kiu
      estis dama&gcirc;e influita de anta&ubreve;aj procedoj.
    </dif>
    <trd lng="be">&c_a;&c_d;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>
    <trd lng="ca">destrempar</trd>
    <trd lng="de">weichgl&uuml;hen</trd>
    <trd lng="en">annealing (metallurgy)</trd>
        <trdgrp lng="hu">
      <trd>megereszt<klr tip="ind"> (f&eacute;met)</klr></trd>,
      <trd>kil&aacute;gy&iacute;t<klr tip="ind"> (f&eacute;met)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <ref tip="sin" cel="mol.0igi">Moligi</ref>, <ref tip="sin" cel="fort.mal0igi">malfortigi</ref>:
      <ekz>
        la delikatforma kaj mal<tld/>ita <nom>Petronius</nom> ekkaptis la manon de la juna atleto
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ablanir</trd>,
      <trd>estovar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elk&eacute;nyelmes&iacute;t</trd>,
      <trd>ellust&iacute;t</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">m&ecaron;k&ccaron;it</trd>
  <trdgrp lng="de">
  <trd>erweichen</trd>,
  <trd>schw&auml;chen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tremper<klr tip="ind"> (un m&eacute;tal)</klr></trd>,
    <trd>amollir</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>stemprare</trd>,
      <trd>stemperare<klr tip="amb"> (stemprare)</klr></trd>,
      <trd>far <ind>rinvenire</ind><klr> (metal.)</klr></trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28988;&#12365;&#12394;&#12414;&#12375;&#12377;&#12427;
    <pr>&#12420;&#12365;&#12394;&#12414;&#12375;&#12377;&#12427;</pr></trd>,
    <trd>&#36575;&#21270;&#12373;&#12379;&#12427;  <pr>&#12394;&#12435;&#12363;&#12373;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zacht maken</trd>,
    <trd>week maken</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(z)</klr>m&auml;k&ccaron;i&tcaron;</trd>,
    <trd>oslabi&tcaron;</trd>,
    <trd>popusti&tcaron;</trd>,
    <trd>zoslabi&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&#1086;&c_z;&c_g;&c_a;&c_r;&c_t;&#1086;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
