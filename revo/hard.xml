<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hard.xml,v 1.38 2025/03/30 10:24:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hard</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="hard.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hard.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Prilabori la internan strukturon de metalo,
      por akiri rezistemon kontra&ubreve;
      foruzi&gcirc;o, fleksado, premado, tordado, pere de
      varmigo, &hcirc;emia trakto a&ubreve; amba&ubreve;:
      <ekz>
        <tld/>ita fero
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i glavon.
      </ekz>
    </dif>
    <trd lng="ca">trempar</trd>
    <trd lng="de">h&auml;rten</trd>
    <trd lng="es">templar</trd>
    <trd lng="nl">stalen<klr tip="ind"> (staal harden)</klr></trd>
    <trd lng="pt">temperar<klr> a&ccedil;o</klr></trd>
  </snc>
  <snc mrk="hard.0i.malmoligi">
    <dif>
      Malmoligi:
      <ekz>
        <tld/>i gipson, cementon;
      </ekz>
      <ekz>
        &scirc;ablonoj preparitaj per metodo uzanta <tld/>itan argilon
        <fnt>
          <bib>Monato</bib>
          <aut>DJ Cane</aut>
          <vrk><url ref="&Monato;/2012/010564.php">Monfalsada ekonomio</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">endurir</trd>
    <trd lng="de">aush&auml;rten</trd>
    <trd lng="es">endurecer</trd>
    <trd lng="nl">harden</trd>
    <trdgrp lng="pt">
      <trd>endurecer</trd>,
      <trd>enrijecer</trd>
    </trdgrp>
  </snc>
  <snc mrk="hard.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Igi nesentema, krudigi, fortigi:
      <ekz>
        mizero <tld/>as korojn;
      </ekz>
      <ekz>
        favorkoreco kaj veremeco <tld/>as re&gcirc;on
        <fnt><bib>MT</bib>, <lok>&Sen; 20:28</lok></fnt>;
      </ekz>
      <ekz>
        Georgo ja multe spertis, anka&ubreve; la maldol&ccirc;an lagon de amo, kiu <tld/>as a&ubreve; moligas
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        longa kaj malfacila batalado nin <tld/>is
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> estas sufi&ccirc;e <tld/>itaj kontra&ubreve; la mara malsano
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nia bravega anaro,
        obstine &gcirc;i pa&scirc;as, provita, <ind><tld/>ita</ind>
        <fnt><aut>L. L. Zamenhof</aut>, <vrk>La Vojo</vrk></fnt>.
        <trdgrp lng="fr">
          <trd>tremp&eacute;<klr tip="ind"> (durci)</klr></trd>,
          <trd>endurci</trd>,
          <trd>inv&eacute;t&eacute;r&eacute;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="ca">insensibilitzar</trd>
    <trdgrp lng="de">
      <trd>abh&auml;rten</trd>,
      <trd>verh&auml;rten</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>hard maken</trd>,
      <trd>verharden<klr tip="ind"> (tr.)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>endurecer</trd>,
      <trd>enrijecer</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#28960; <pr>cu&#236;</pr></trd>,
    <trd>&#23454;&#20307;&#21270; <pr>sh&#237;t&#464;hu&#224;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>malmoligi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>otu&zcaron;it</trd>,
    <trd>otu&zcaron;ovat</trd>,
    <trd>utu&zcaron;it</trd>,
    <trd>utu&zcaron;ovat</trd>
  </trdgrp>
  <trd lng="en">harden</trd>
  <trdgrp lng="fr">
    <trd>tremper<klr tip="ind"> (le m&eacute;tal, le caract&egrave;re)</klr></trd>,
    <trd>aguerrir</trd>,
    <trd>durcir<klr tip="ind"> (tr.)</klr></trd>,
    <trd>endurcir</trd></trdgrp>
  <trdgrp lng="hu">
      <trd>megedz</trd>,
      <trd>megkem&eacute;ny&iacute;t</trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>temprare</trd>,
      <trd>indurire<klr> (tr.)</klr></trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28988;&#20837;&#12428;&#12377;&#12427;  <pr>&#12420;&#12365;&#12356;&#12428;&#12377;&#12427;</pr></trd>,
    <trd>&#30828;&#21270;&#12373;&#12379;&#12427;  <pr>&#12371;&#12358;&#12363;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#37723;&#12360;&#12427;  <pr>&#12365;&#12383;&#12360;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>otu&zcaron;i&tcaron;</trd>,
    <trd>otu&zcaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">h&auml;rda</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&#1086;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="hard.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="hard.0ado.KOMUNE">
    <dif>
      Procedo per kiu oni <tld/>as ion:
      <ekz>
        <tld/>ado en akvo, en oleo, per aerblovo
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">H&auml;rtung</trd>
    <trdgrp lng="zh">
    <trd>&#20859;&#25104;&#20064;&#24815; <pr>y&#462;ngch&#233;ngx&#237;gu&#224;n</pr></trd>,
    <trd>&#24375;&#21270; <pr>qi&#225;nghu&#224;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>kalen&iacute;</trd>,
    <trd>tvrzen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>trempe<klr tip="ind"> (traitement qui
    endurcit)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>edz&eacute;s<klr tip="ind"> (h&odblac;kezel&eacute;s)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#28988;&#20837;&#12428;  <pr>&#12420;&#12365;&#12356;&#12428;</pr></trd>
  <trdgrp lng="sk">
    <trd>kalenie</trd>,
    <trd>tvrdenie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_t;&c_u;&c_v;&c_a;&#1085;&#1085;&#1103;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&c_u;&c_v;&c_a;&#1085;&#1085;&#1103;</trd>
  </trdgrp>
</drv>

<drv mrk="hard.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hard.0igxi.KOMUNE">
    <dif>
      I&gcirc;i rezistema kontra&ubreve; eksteraj atakoj,
      malmola, fortika:
      <ekz>
        <tld/>i&gcirc;inta krimulo;
      </ekz>
      <ekz>
        ju pli la malsinceraj amikoj alskribis al lia heroeco la glorajn venkojn, des
        pli <klr>[&#8230;]</klr> lia koro <tld/>i&gcirc;is
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>verh&auml;rten</ind></trd>
    <trdgrp lng="zh">
    <trd>&#20351;&#30828;&#21270; <pr>sh&#464;y&#236;nghu&#224;</pr></trd>,
    <trd>&#30952;&#28860; <pr>m&#243;li&#224;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>endurir-se</trd>,
    <trd>empedreir-se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>aguerrir</ind></trd>,
    <trd>se <ind>durcir</ind></trd>,
    <trd>s'<ind>endurcir</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>edzett</trd>,
    <trd>megedz&odblac;d&ouml;tt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30828;&#21270;&#12377;&#12427;  <pr>&#12371;&#12358;&#12363;&#12377;&#12427;</pr></trd>,
    <trd>&#37723;&#12360;&#12425;&#12428;&#12427;  <pr>&#12365;&#12383;&#12360;&#12425;&#12428;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#1103;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&#1086;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#1103;</trd>
  </trdgrp>
</drv>

