  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="L"/>i &ccirc;iuflanke:
      <ekz>
        <klr>[maraj ondoj]</klr> kun tondrego faladis, disfluis
        sur la ferdekoj, &ccirc;irka&ubreve;<tld/>ante vandetojn
        de l' kabinoj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">om&yacute;t</trd>
  <trd lng="fr">laver le pourtour de</trd>
  <trd lng="pl">obmywa&cacute;</trd>
  <trd lng="sk">omy&tcaron;</trd>
</drv>



<drv mrk="lav.el0i">
  <kap>
    el<tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="lav.for0i">Forlavi</ref>:
      <ekz>
        oni devas el<tld/>i &gcirc;in per sapo kontra&ubreve; sangomakuloj,
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiun]</klr> permanentan koloron <klr>[&#8230;]</klr> oni ne povas el<tld/>i per
        a&ubreve; sen sapo
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lav.el0i.de_ene">
    <dif>
      <tld lit="L"/>i de interne:
      <ekz>
        li devas dismunti kaj el<tld/>i iun memlubrikantan ma&scirc;inparton
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 41</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">auswaschen</trd>
</drv>


<drv mrk="lav.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.for0i.KOMUNE">
    <dif>
      Forigi <tld/>ante:
      <ekz>
        &scirc;tonojn for<tld/>as la akvo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 14:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi for<tld/>is de vi vian sangon
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bapti&gcirc;u kaj for<tld/>u viajn pekojn
        <fnt><bib>NT</bib>, <lok>&Ago; 22:16</lok></fnt>;
      </ekz>
      <ekz>
        evitu malsekan sezonon, kiam pluvo kaj inundo kutime for<tld/>as vojojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;4</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lesiv.0i">lesivi</ref>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odplavit</trd>,
    <trd>sm&yacute;t</trd>,
    <trd>spl&aacute;chnout</trd>,
    <trd>vyplavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wegwaschen</trd>,
    <trd>auswaschen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire partir en lavant</trd>,
    <trd>purifier de</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spl&aacute;chnu&tcaron;</trd>,
    <trd>zmy&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_m;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_m;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

