<drv mrk="lav.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="pur.0igi">Purigi</ref> per
      <ref cel="akv.0o">akvo</ref> a&ubreve; alia
      flua&jcirc;o:
      <ekz>
        &scirc;i tuj <tld/>is sian kru&ccirc;on kaj &ccirc;erpis akvon <klr>[&#8230;]</klr> de la fonto
        <fnt>
          <bib>F</bib>
          <lok>&FE; 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is min en mia &ccirc;ambro, kaj &scirc;i <tld/>is sin en sia &ccirc;ambro
        <fnt>
          <bib>F</bib>
          <lok>&FE; 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kombis &scirc;iajn harojn, <tld/>is ilin en akvo kaj vino
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is sian viza&gcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is siajn vestojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni alportos iom da akvo, kaj vi <tld/>u viajn piedojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vir&scirc;afon dishaku en partojn, kaj <tld/>u &gcirc;ian interna&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filino de Faraono malsupreniris, por <tld/>i sin en la Rivero
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        ci <tld/>u tiun makulon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        brilaj ondoj <klr>[&#8230;]</klr> <tld/>as la bordojn
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>u de la malbono vian koron
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 4:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>fari al iu bonan <tld/>on</ind>
        <klr>
        (severan ripro&ccirc;on.
        <refgrp tip="vid">
          <ref cel="tan.0i">tani</ref>,
          <ref cel="predik.0i">prediki</ref>,
          <ref cel="lecion.0o">leciono</ref>)
        </refgrp>
        </klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">megmossa a fej&eacute;t</trd>
        <trd lng="nl">iemand de <ind>mantel</ind> uitvegen</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[pri tiu afero]</klr> <ind>mi <tld/>as al mi la manojn</ind>
        <klr>(mi ne prirespondas pri tio)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">ich <ind>wasche</ind> meine H&auml;nde in Unschuld</trd>
        <trd lng="hu">mosom kezeimet</trd>
        <trd lng="nl">ik was mijn <ind>hand</ind>en in onschuld</trd>
      </ekz>
      <ekz>
        mano manon <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ban.0i">bani</ref>,
      <ref cel="gargar.0i">gargari</ref>,
      <ref cel="lesiv.0i">lesivi</ref>
