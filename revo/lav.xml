  <trdgrp lng="ja">
    <trd>&#x6D17;&#x3044;&#x843D;&#x3068;&#x3059;  [<ind>&#x3042;&#x3089;&#x3044;&#x304A;&#x3068;&#x3059;</ind>]</trd>,
    <trd>&#x6D17;&#x3044;&#x6D41;&#x3059;  [<ind>&#x3042;&#x3089;&#x3044;&#x306A;&#x304C;&#x3059;</ind>]</trd>
  </trdgrp>
