<drv mrk="lav.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="L"/>i &ccirc;iuflanke:
      <ekz>
        <klr>[maraj ondoj]</klr> kun tondrego faladis, disfluis
        sur la ferdekoj, &ccirc;irka&ubreve;<tld/>ante vandetojn
        de l' kabinoj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 9</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Troms&oslash;</nom> <klr>[&#8230;]</klr> malgra&ubreve; sia norda loki&gcirc;o, <klr>[&#8230;]</klr>
        estas &ccirc;irka&ubreve;<tld/>ata de la golfa marfluo
        <fnt>
          <bib>Kon11</bib>
          <lok>Nordluma renkonti&gcirc;o en Norda Norvegio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la flamoj &ccirc;irka&ubreve;<tld/>is lin, kaj li grumble ruli&gcirc;is sur la alian flankon
        <fnt>
          <bib>Monato</bib>
          <aut>Frank Roger</aut>
          <vrk><url ref="https://www.monato.be/2004/008248.html"
              >La brulanta virino</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">om&yacute;t</trd>
  <trd lng="de">umsp&uuml;len</trd>
  <trd lng="fr">laver le pourtour de</trd>
  <trd lng="pl">obmywa&cacute;</trd>
  <trd lng="sk">omy&tcaron;</trd>
</drv>


<drv mrk="lav.el0i">
  <kap>
    el<tld/>i
  </kap>
  <snc mrk="lav.el0i.forlavi">
    <dif>
      <ref tip="dif" cel="lav.for0i">Forlavi</ref>:
      <ekz>
        oni devas el<tld/>i &gcirc;in per sapo kontra&ubreve; sangomakuloj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ploris tiel forte, ke la spegula grajneto el<tld/>i&gcirc;is el lia okulo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiun]</klr> permanentan koloron <klr>[&#8230;]</klr> oni ne povas el<tld/>i per
        a&ubreve; sen sapo
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lav.el0i.de_ene">
    <dif>
      <tld lit="L"/>i de interne:
      <ekz>
        li devas dismunti kaj el<tld/>i iun memlubrikantan ma&scirc;inparton
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 41</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>auswaschen</trd>,
    <trd>aussp&uuml;len</trd>
  </trdgrp>
</drv>


<drv mrk="lav.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.for0i.KOMUNE">
    <dif>
      Forigi <tld/>ante:
      <ekz>
        &scirc;tonojn for<tld/>as la akvo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 14:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi for<tld/>is de vi vian sangon
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bapti&gcirc;u kaj for<tld/>u viajn pekojn
        <fnt><bib>NT</bib>, <lok>&Ago; 22:16</lok></fnt>;
      </ekz>
      <ekz>
        evitu malsekan sezonon, kiam pluvo kaj inundo kutime for<tld/>as vojojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;4</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lesiv.0i">lesivi</ref>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odplavit</trd>,
    <trd>sm&yacute;t</trd>,
    <trd>spl&aacute;chnout</trd>,
    <trd>vyplavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wegwaschen</trd>,
    <trd>auswaschen</trd>,
    <trd>wegsp&uuml;len</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire partir en lavant</trd>,
    <trd>purifier de</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spl&aacute;chnu&tcaron;</trd>,
    <trd>zmy&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_m;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_m;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>


<drv mrk="lav.cerbo0o">
  <kap>cerbo<tld/>o, <var><kap>cerbo<tld/>ado</kap></var></kap>
  <snc mrk="lav.cerbo0o.TOR">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Premado al malliberulo, por perforte &scirc;an&gcirc;i liajn 
      opiniojn, lian personecon:
      <ekz>
        la ideo estas tute identa al tiu &ccirc;e cerbo<tld/>ado: vi
        zorgu ke iu perdu sian senton de realeco
        <fnt>
          <aut>P. Desmet'</aut>,
          <vrk>Blogo</vrk>,
          <lok><url ref="http://www.ipernity.com/blog/petro_desmet/51462/?lg=eo">
          2008-01-18</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        iom da cerbo<tld/>ilo
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Salomon</aut>
          <vrk><url ref="https://www.monato.be/2003/008016.html"
              >La mondolingvo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj tiel brutale subpremataj kaj torturataj per cerbo<tld/>ado kiel nord-koreoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2009/009837.php"
              >Monstra atompotenco</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_m;&c_a;&c_z;&c_g;&c_o;&c_w;</trd>
  <trd lng="cs">vym&yacute;v&aacute;n&iacute; mozku</trd>
  <trd lng="de">Gehirnw&auml;sche</trd>
  <trd lng="en">brainwashing</trd>
  <trd lng="fr">lavage de cerveau</trd>
  <trd lng="it"><ind>lavaggio</ind> del cervello</trd>
  <trd lng="nl">hersenspoeling</trd>
  <trd lng="pl">pranie m&oacute;zgu</trd>
  <trd lng="sk">vym&yacute;vanie mozgov</trd>
  <trd lng="sv">hj&auml;rntv&auml;tt</trd>
</drv>


