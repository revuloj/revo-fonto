<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lav</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="lav.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="pur.0igi">Purigi</ref> per
      <ref tip="vid" cel="akv.0o">akvo</ref> a&ubreve; alia
      flua&jcirc;o:
      <ekz>
        <tld/>i tola&jcirc;on, glasojn, tapi&scirc;on, makulojn,
        al si viza&gcirc;on;
      </ekz>
      <ekz>
        for<tld/>i makulon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ploris tiel forte, ke la grajneto el<tld/>i&gcirc;is el lia
        okulo
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>fari al iu bonan <tld/>on</ind>
        <klr>
        (severan ripro&ccirc;on.
        <refgrp tip="vid">
          <ref cel="tan.0i">tani</ref>,
          <ref cel="predik.0i">prediki</ref>,
          <ref cel="lecion.0o">leciono</ref>)
        </refgrp>
        </klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">megmossa a fej&eacute;t</trd>
        <trd lng="nl">iemand de <ind>mantel</ind> uitvegen</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ondoj <tld/>as la rokojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pri tiu afero <ind>mi <tld/>as al mi la manojn</ind><fnt>Z</fnt>
        <klr>(mi ne prirespondas pri tio)</klr>;
        <trd lng="hu">mosom kezeimet</trd>
        <trd lng="nl">ik was mijn <ind>hand</ind>en in onschuld</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>u de la malbono vian koron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ban.0i">bani</ref>,
      <ref cel="gargar.0i">gargari</ref>,
      <ref cel="lesiv.0i">lesivi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>gwalc'hi&ntilde;</trd>,
    <trd>kanna&ntilde;<klr> (vesta&jcirc;o)</klr></trd>,
    <trd>skaota&ntilde;<klr> (teleroj, ktp)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&yacute;t</trd>,
    <trd>pr&aacute;t</trd>
  </trdgrp>
  <trd lng="de">waschen</trd>
  <trd lng="el">&pi;&lambda;&epsilon_ton;&ny;&omega;</trd>
  <trd lng="en">wash</trd>
  <trd lng="es">lavar</trd>
  <trd lng="fr">laver</trd>
  <trdgrp lng="he">
    <trd>&lamed;&resh;&het;&vav;&ftsadi;
    \<ind>&resh;&het;&ftsadi;</ind>\</trd>,
    <trd>&lamed;&shin;&tet;&vav;&fpe;
    \<ind>&shin;&tet;&fpe;</ind>\</trd>;
    <trd>&lamed;<ind>&kaf;&bet;&samekh;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mos</trd>,
    <trd>elmos</trd>;
    <trd>mosogat</trd>
  </trdgrp>
  <trd lng="it">lavare</trd>
  <trd lng="nl">wassen</trd>
  <trdgrp lng="pl">
    <trd>my&cacute;</trd>,
    <trd>zmywa&cacute;</trd>,
    <trd>pra&cacute;</trd>
  </trdgrp>
  <trd lng="pt">lavar</trd>
  <trd lng="ru">&c_m;&c_y;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>oprať</trd>,
    <trd>poumývať</trd>,
    <trd>prať</trd>,
    <trd>umývať</trd>
  </trdgrp>
  <trd lng="sv">tv&auml;tta</trd>
  <trd lng="tp">telo</trd>
  <trd lng="tr">y&inodot;kamak</trd>
</drv>

<drv mrk="lav.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="lav.0ejo.KOMUNE">
    <dif>
      Loko aran&gcirc;ita por faciligi <tld/>adon.
      <ekz>
         ni kutimi&gcirc;is al tio, ke en <tld/>ejoj oni ne sole
         <tld/>as, sed anka&ubreve; flikas tola&jcirc;ojn.
        <fnt>
          <aut>I. Ilf, E. Petrov, trad. V. Okc</aut>,
          <vrk>Unueta&gcirc;a Usono</vrk>,
          <lok>&ccirc;apitro 42a, La Novjaro en San-Antonio</lok>
       </fnt>;
      </ekz>
      <ekz>
        la apartamento konsistas plej ofte el dormejoj, kuirejo, 
        saloneto kaj apartena&jcirc;o: necesejo, provizejo, banejo,
        karbejo kaj lignejo, deponejo, a&ubreve;tejo a&ubreve; gara&gcirc;o, <tld/>ejo
        kaj remizo
        <fnt>
          <aut>J. Chvosta</aut>,
          <vrk>Tekstoj por konversacio</vrk>,
          <lok>&ccirc;apitro 2a, lo&gcirc;ado</lok>
       </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="cxambr.lav0o.ARKI">lav&ccirc;ambro</ref>
  </snc>
  <trd lng="es">lavander&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>buanderie</trd>,
    <trd>laverie</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>lavanderia</trd>,
    <trd>lavatoio</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wasplaats</trd>,
    <trd>wassalon</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>umywalnia</trd>,
    <trd>myjnia</trd>,
    <trd>&lstroke;a&zacute;nia</trd>,
    <trd>pralnia</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>práčovňa</trd>,
    <trd>umyváreň</trd>
  </trdgrp>
</drv>

<drv mrk="lav.0istino">
  <kap><tld/>istino</kap>
  <snc mrk="lav.0istino.KOMUNE">
    <dif>
      Ino, kiu profesie <tld/>as precipe &scirc;tofojn, vestojn:
      <ekz>
        zumas en lesivtino nova kepra tuko en la manoj de <tld/>istino
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">lavandi&egrave;re</trd>
  <trd lng="sk">práčka <klr>(žena)</klr></trd>
</drv>

<drv mrk="lav.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="lav.0ujo.KOMUNE">
    <dif>
      Pelvo kun krano kutime por lavi la manojn a&ubreve;
      tola&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_i;&c_v;&c_k;&c_a;</trd>
  <trd lng="de">Waschbecken</trd>
  <trdgrp lng="es">
    <trd>lavabo</trd>,
    <trd>pila <klr tip="amb">(lavadero)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;vier</trd>,
    <trd>lavabo</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>lavabo</trd>, <trd>lavello</trd>, <trd>lavandino</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wasbak</trd>,
    <trd>wastafel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>umywalka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lavór</trd>,
    <trd>umývadlo</trd>
  </trdgrp>
</drv>

<drv mrk="lav.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.0umi.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Kolorigi surpaperan desegnon, etendante maldensan akvan kolorilon
      kaj pli kaj pli intensigante la nuancojn per plurfoja
      frotado de peniko post seki&gcirc;o de anta&ubreve;a kolortavolo.
    </dif>
    <refgrp tip="vid">
      <ref cel="tucx.0i">tu&ccirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">travailler au <ind>lavis</ind></trd>
  <trdgrp lng="hu">
    <trd>elmos</trd>,
    <trd>lav&iacute;roz</trd>;
    <trd>&aacute;rnyal</trd>
  </trdgrp>
  <trd lng="it">lavare<klr> (pitt.)</klr></trd>
  <trd lng="nl">wassen<klr tip="ind"> (v.tekening)</klr></trd>
  <trd lng="pl">lawowa&cacute;</trd>
  <trd
  lng="ru">&c_z;&c_a;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">lavírovať</trd>
</drv>

<drv mrk="lav.cerbo0o">
  <kap>cerbo<tld/>o, <var><kap>cerbo<tld/>ado</kap></var></kap>
  <snc mrk="lav.cerbo0o.TOR">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Premado al malliberulo, por perforte &scirc;an&gcirc;i liajn 
      opiniojn, lian personecon:
      <ekz>
        la ideo estas tute identa al tiu &ccirc;e cerbo<tld/>ado: vi
        zorgu ke iu perdu sian senton de realeco
        <fnt>
          <aut>P. Desmet'</aut>,
          <vrk>Blogo</vrk>,
          <lok><url
          ref="http://www.ipernity.com/blog/petro_desmet/51462/?lg=eo">
          2008-01-18</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">lavage de cerveau</trd>
  <trd lng="it"><ind>lavaggio</ind> del cervello</trd>
  <trd lng="nl">hersenspoeling</trd>
  <trd lng="pl">pranie m&oacute;zgu</trd>
  <trd lng="sk">vymývanie mozgov</trd>
  <trd lng="sv">hj&auml;rntv&auml;tt</trd>
</drv>

<drv mrk="lav.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="L"/>i &ccirc;iuflanke:
      <ekz>
        <klr>[maraj ondoj]</klr> kun tondrego faladis, disfluis
        sur la ferdekoj, &ccirc;irka&ubreve;<tld/>ante vandetojn
        de l' kabinoj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">laver le pourtour de</trd>
  <trd lng="pl">obmywa&cacute;</trd>
</drv>

<drv mrk="lav.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.for0i.KOMUNE">
    <dif>
      Forigi <tld/>ante:
      <ekz>
        bapti&gcirc;u kaj for<tld/>u viajn pekojn
        <fnt><bib>NT</bib>, <lok>&Ago; 22:16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>faire partir en lavant</trd>,
    <trd>purifier de</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spláchnuť</trd>,
    <trd>zmyť</trd>
  </trdgrp>
</drv>

<drv mrk="lav.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <snc>
    <dif>
      Ma&scirc;ino, kiu lavas vesta&jcirc;on.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  &c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_p;&c_je;&c_r;&c_a;&c_l;&c_n;&c_ja;</trd>
  <trd lng="br">kannerez<klr tip="ind"> (ardivink)</klr></trd>
  <trd lng="de">Waschmaschine</trd>
  <trd lng="en">washing machine</trd>
  <trd lng="es">lavadora</trd> 
  <trd lng="fr">machine &agrave; laver</trd>
  <trd lng="he">&mem;&kaf;&vav;&nun;&tav;
  &kaf;&bet;&yod;&samekh;&he;</trd>
  <trd lng="hu">mos&oacute;g&eacute;p</trd>
  <trd lng="it">lavatrice</trd>
  <trd lng="nl">wasmachine</trd>
  <trd lng="pl">pralka</trd>
  <trd lng="sv">tv&auml;ttmaskin</trd>
</drv>
</art>
