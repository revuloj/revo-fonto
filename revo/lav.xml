<drv mrk="lav.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lav.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="L"/>i &ccirc;iuflanke:
      <ekz>
        <klr>[maraj ondoj]</klr> kun tondrego faladis, disfluis
        sur la ferdekoj, &ccirc;irka&ubreve;<tld/>ante vandetojn
        de l' kabinoj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 9</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Troms&oslash;</nom> <klr>[&#8230;]</klr> malgra&ubreve; sia norda loki&gcirc;o, <klr>[&#8230;]</klr>
        estas &ccirc;irka&ubreve;<tld/>ata de la golfa marfluo
        <fnt>
          <bib>Kon11</bib>
          <lok>Nordluma renkonti&gcirc;o en Norda Norvegio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la flamoj &ccirc;irka&ubreve;<tld/>is lin, kaj li grumble ruli&gcirc;is sur la alian flankon
        <fnt>
                    <aut>Frank Roger</aut>
          <vrk><url ref="&Monato;/2004/008248.html">La brulanta virino</url></vrk>
              <bib>Monato</bib>
          <lok>2004/02, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
