<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adz</rad>/o
</kap>
<drv mrk="adz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="hak.0ilo">hakilo</ref> uzata de
      &ccirc;arpentistoj kaj barelistoj.
    </dif>
  </snc>
  <adm>
    La trd fr estas revizienda post eventuala pliprecizigo de
    la difino. <aut>MB</aut>
  </adm>
  <trd lng="en">adz(e)</trd>
  <trdgrp lng="fr">
    <trd>herminette</trd>,
    <trd>erminette</trd>,
    <trd>asse</trd>,
    <trd>hachereau<klr tip="ind"> (de charpentier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szekerce</trd>,
    <trd>&aacute;csb&aacute;rd</trd>
  </trdgrp>
  <trd lng="nl">dissel</trd>
  <trd lng="pt">enx&oacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_d;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">tes&aacute;rska <ind>sekera</ind></trd>
</drv>
</art>
