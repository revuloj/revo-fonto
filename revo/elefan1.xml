<kap>
  <rad>elefantiaz</rad>/o
</kap>
<drv mrk="elefan1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref>,
      kiu igas la ha&ubreve;ton raspece malglata simile al tiu de elefanto
      kaj kiu kelkfoje ka&ubreve;zas &scirc;velon de la &ccirc;ela histo:
      <ekz>
        <nom>Fabiano de Cristo</nom> <klr>[&#8230;]</klr> suferis erizipelon kaj <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33156">Fabiano de Cristo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_f;&c_a;&c_n;&c_t;&c_y;&c_ja;&c_z;</trd>
  <trd lng="ca">elefantiasi</trd>
  <trd lng="cs">elefanti&aacute;za</trd>
  <trd lng="de">Elefantiasis</trd>
  <trdgrp lng="fa">
  <trd>&f_p;&f_y;&a_l;&#8204;&f_p;&a_A;&f_y;&f_y;</trd>&a_komo;
  <trd>&a_d;&a_A;&a_hamza;&a_A;&a_l;&a_f;&f_y;&a_l;</trd>
</trdgrp>
  <trd lng="fr">&eacute;l&eacute;phantiasis</trd>
  <trdgrp lng="id">
  <trd>elefantiasis</trd>,
  <trd><ind>kaki</ind> gajah</trd>
</trdgrp>
  <trd lng="ja">&#x8C61;&#x76AE;&#x75C5;  <pr>&#x305E;&#12358;&#12402;&#12403;&#12423;&#12358;</pr></trd>
  <trd lng="pt">elefant&iacute;ase</trd>
  <trdgrp lng="ru">
  <trd>&c_e;&c_l;&c_je;&c_f;&c_a;&c_n;&c_t;&c_i;&c_a;&c_z;&c_i;&c_s;</trd>,
  <trd>&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a;&c_ja; &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
</trdgrp>
  <trd lng="sk">elefanti&aacute;za</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43B;&c_je;&c_f;&c_a;&#x43D;&c_t;&c_ib;&c_a;&c_z;</trd>,
    <trd>&c_s;&#x43B;&#x43E;&#x43D;&#x43E;&c_v;&c_a; &c_h;&c_v;&#x43E;&c_r;&#x43E;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x8C61;&#x76AE;&#x75C5; <pr>xi&#xE0;ngp&#xED;b&#xEC;ng</pr></trd>
</drv>
