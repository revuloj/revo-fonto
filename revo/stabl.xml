<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>stabl</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="stabl.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="stabl.0o.metitablo">
     <dif>
       Tablo por metia laboro; metitablo:
       <ekz>
         <tld/>o de seruristo<fnt>B</fnt>,
       </ekz>
       <ekz>
         de teksisto<fnt>B</fnt>,
       </ekz>
       <ekz>
         de tajloro<fnt>B</fnt>;
       </ekz>
       <ekz>
         la potisto laboras sur la <tld/>o<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <ind>torno<tld/>o</ind><fnt>Z</fnt>.
         <trd lng="hu">esztergapad</trd>
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_v;&c_a;&c_r;&c_sh;&c_t;&c_a;&c_t;</trd>,
       <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
     </trdgrp>
     <trd lng="ca">taula de treball</trd>
     <trd lng="de">Werkbank</trd>
     <trd lng="en">workbench</trd>
     <trd lng="es">mesa de trabajo</trd>
     <trdgrp lng="fr">
       <trd>&eacute;tabli</trd>,
       <trd>banc <klr>(de tourneur, menuisier)</klr></trd>
     </trdgrp>
     <trd lng="hu">munkapad</trd>
     <trd lng="nl">werkbank</trd>
     <trdgrp lng="ru">
       <trd>&c_v;&c_je;&c_r;&c_s;&c_t;&c_a;&c_k;</trd>,
       <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
     </trdgrp>
   </snc>
   <snc mrk="stabl.0o.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Subporta kunmeta&jcirc;o el traboj servanta kiel
       <ref tip="super" cel="baz.0o.postamento">bazo</ref>.
     </dif>
     <subsnc mrk="stabl.0o.tabulo">
       <dif>
         Movebla kvarpieda kunmeta&jcirc;o el trabetoj por subporti tabulon,
         tabularon, plankon.
       </dif>
       <trdgrp lng="be">
         <trd>&c_k;&c_o;&c_z;&c_l;&c_y;</trd>,
         <trd>&c_s;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
         <trd>&c_sh;&c_t;&c_a;&c_t;&c_y;&c_w;</trd>
       </trdgrp>
       <trd lng="ca">cavallet <klr>(general)</klr></trd>
       <trd lng="de">Gestell</trd>
       <trdgrp lng="en">
         <trd>trestle</trd>,
         <trd>support</trd>,
         <trd>stand</trd>
       </trdgrp>
       <trd lng="fr">tr&eacute;teau</trd>
       <trd lng="hu">&aacute;llv&aacute;ny</trd>
       <trdgrp lng="nl">
         <trd>schraag</trd>,
         <trd>stellage</trd>
       </trdgrp>
       <trdgrp lng="ru">
         <trd>&c_sh;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>,
         <trd>&c_s;&c_t;&c_a;&c_n;&c_i;&c_n;&c_a;</trd>,
         <trd>&c_k;&c_o;&c_z;&c_l;&c_y;</trd>
       </trdgrp>
     </subsnc>
     <subsnc mrk="stabl.0o.pentrajxo">
       <dif>
         Movebla tripieda a&ubreve; kvarpieda kunmeta&jcirc;o el trabetoj 
         por subporti prilaboratan pentra&jcirc;on; pentro<tld/>o.
       </dif>
       <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_b;&c_e;&c_r;&c_t;</trd>
       <trd lng="ca">cavallet <klr>(per a teles)</klr></trd>
       <trd lng="de">Staffelei</trd>
       <trd lng="en">easel</trd>
       <trd lng="es">caballete</trd>
       <trd lng="fr">chevalet</trd>
       <trd lng="hu">fest&odblac;&aacute;llv&aacute;ny</trd>
       <trdgrp lng="nl">
         <trd>ezel<klr tip="ind"> (v.schilder)</klr></trd>,
         <trd>schildersezel</trd>
       </trdgrp>
       <trd lng="ru">&c_m;&c_o;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;</trd>
     </subsnc>
   </snc>
   <trd lng="es">chevalet</trd>
</drv>
</art>
