<kap>
  <rad>i&hcirc;tiosa&ubreve;r</rad>/o
</kap>
<drv mrk="ihxtio1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ihxtio1.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el grupo de <ref tip="lst" cel="verteb.0ulo" lst="voko:bestoj">vertebruloj</ref>
      vivinta en maroj dum 150 milionoj da jaroj &gcirc;is anta&ubreve; 93 milionoj
      jaroj, forme iom simila al delfeno; fi&scirc;sa&ubreve;ro:
      <ekz>
        <tld/>o el <nom>Holzmaden</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=614203">Muzeo Wiesbaden</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/45/Ichthyosaurus_sp_2.jpg/640px-Ichthyosaurus_sp_2.jpg" prm="CC-BY-SA-3.0"><tld/>o el Dinosa&ubreve;rolando, Lyme Regis, Anglujo
        <fnt>
          <aut>Ballista</aut>
          <lok><url ref="&WCW;/File:Ichthyosaurus_sp_2.jpg">&WCW;/File:Ichthyosaurus_sp_2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ib;&c_h;&c_t;&c_y;&c_ja;&c_z;&c_a;&c_w;&c_r;</trd>
  <trd lng="bg">&c_i;&c_h;&c_t;&c_i;&c_o;&c_z;&c_a;&c_v;&c_malmol;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Ichthyosaurus</trd>,
    <trd>Fischsaurier</trd>
  </trdgrp>
  <trd lng="fr">ichtyosaure</trd>
  <trd lng="hu">Ichtyosaurus</trd>
  <trd lng="nl">ichthyosaurus</trd>
  <trdgrp lng="pl">
    <trd>ichtiozaur</trd>,
    <trd>ichtiozaurus</trd>,
    <trd>rybojaszczur</trd>
  </trdgrp>
  <trd lng="pt">ictioss&aacute;urio</trd>
  <trd lng="ru">&c_i;&c_h;&c_t;&c_i;&c_o;&c_z;&c_a;&c_v;&c_r;</trd>
  <trd lng="uk">&#x456;&#x445;&#x442;&#x456;&#x43E;&#x437;&#x430;&#x432;&#x440;</trd>
</drv>
