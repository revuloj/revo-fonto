<kap>
  <rad>diastol</rad>/o
</kap>
<drv mrk="diasto.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Periodo de kordilati&gcirc;o inter du 
      <ref tip="ant" cel="sistol.0o">sistolo</ref>j.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_s;&c_t;&c_a;&c_l;&c_a;</trd>
  <trd lng="de">Diastole</trd>
  <trd lng="en">diastole</trd>
  <trd lng="es">di&aacute;stole</trd>
  <trd lng="fr">diastole</trd>
  <trd lng="he">&#x5D3;&#x5B4;&#x5D9;&#x5D0;&#x5B8;&#x5E1;&#x5D8;&#x5D5;&#x5B9;&#x5DC;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>diasztol&eacute;</trd>,
    <trd>sz&iacute;velernyed&eacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x5FC3;&#x62E1;&#x5F35;&#x671F;  [<ind>&#x3053;&#x3053;&#x308D;&#x304B;&#x304F;&#x3061;&#x3087;&#x3046;&#x304D;</ind>]</trd>
  <trd lng="nl">diastole</trd>
  <trd lng="pl">rozkurcz<klr> serca</klr></trd>
  <trd lng="pt">di&aacute;stole</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_s;&c_t;&c_o;&c_l;&c_a;</trd>
  <trd lng="sk">diastola</trd>
  <trd lng="uk">&#x434;&#x456;&#x430;&#x441;&#x442;&#x43E;&#x43B;&#x430;</trd>
</drv>

<drv mrk="diasto.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Kordilati&gcirc;a: 
      <ekz>
        la diferenco inter sistola kaj <tld/>a premo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=483922">Aorta stenozo</url></lok>
        </fnt>.
      </ekz>    
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_s;&c_t;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">diastolick&yacute;</trd>
  <trd lng="de">diastolisch</trd>
  <trd lng="en">diastolic</trd>
  <trd lng="fr">diastolique</trd>
  <trd lng="he">&#x5D3;&#x5B4;&#x5D9;&#x5D0;&#x5B8;&#x5E1;&#x5D8;&#x5D5;&#x5B9;&#x5DC;&#x5B8;&#x5D9;</trd>
  <trd lng="hu">diasztol&eacute;s</trd>
  <trd lng="sk">diastolick&yacute;</trd>
</drv>

</art>
