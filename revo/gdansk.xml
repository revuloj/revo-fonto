<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Gdansk</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="gdansk.0o">
  <kap><tld/>o</kap>
  <snc mrk="gdansk.0o.urbo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Pola haven<ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      &ccirc;e la <tld/>a golfeto:
      <ekz>
        <tld/>o <klr>[...]</klr> estas urbo &ccirc;e la Balta maro kaj &ccirc;efurbo
        de 
        Pomeria Provinco en Pollando
        <fnt><bib>Viki</bib>, <lok>Gdansk</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Danzig</trd>
  <trdgrp lng="fr">
    <trd>Dantzig</trd>,
    <trd>Dantzick</trd>
  </trdgrp>
</drv>
</art>

