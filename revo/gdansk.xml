<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gdansk.xml,v 1.10 2024/10/08 20:25:50 revo Exp $">
<kap><ofc>10</ofc>
    <rad>Gdansk</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="gdansk.0o">
  <kap><tld/>o</kap>
  <snc mrk="gdansk.0o.urbo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Pola haven<ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref>
      &ccirc;e la <tld/>a golfeto:
      <ekz>
        <tld/>o <klr>[...]</klr> estas urbo &ccirc;e la Balta maro kaj &ccirc;efurbo
        de
        Pomeria Provinco en Pollando
        <fnt><bib>Viki</bib>, <lok>Gdansk</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="dancig.0o">Dancigo</ref>
  </snc>
  <trd lng="be">&c_G;&c_d;&c_a;&c_n;&c_mol;&c_s;&c_k;</trd>
  <trd lng="cs">Gda&ncaron;sk</trd>
  <trdgrp lng="de">
    <trd>Danzig</trd>,
    <trd>Gdansk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Dantzig</trd>,
    <trd>Dantzick</trd>
  </trdgrp>
  <trd lng="ja">&#x30B0;&#x30C0;&#x30CB;&#x30B9;&#x30AF;</trd>
  <trd lng="lat">Gedanum</trd>
  <trd lng="pl">Gda&nacute;sk</trd>
  <trd lng="sk">Gdansk</trd>
  <trd lng="uk">&c_Gu;&c_d;&c_a;&#x43D;&c_s;&#x44C;&#x43A;</trd>
  <trd lng="zh">&#x4F46;&#x6CFD; <pr>d&#xE0;nz&#xE9;</pr></trd>
</drv>
</art>

