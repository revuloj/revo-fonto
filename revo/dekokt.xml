<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dekokt.xml,v 1.25 2022/03/15 15:50:54 revo Exp $">
<kap>
  <rad>dekokt</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dekokt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dekokt.0i.MED">
    <uzo tip="fak">KUI</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Longe <ref tip="super" cel="bol.0igi">boligi</ref> en akvo vegeta&jcirc;on por 
      <ref tip="super" cel="solv.0i">solvi</ref> &gcirc;iajn solveblajn substancojn:
      <ekz>
        li ordonis <tld/>i unu dozon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 53a, p.
        182a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="infuz.0i">infuzi</ref>,
      <ref cel="macer.0i">maceri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_a;&c_d;&c_v;&c_a;&c_r;</ind></trd>
  <trd lng="ca">fer-ne la decocci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ipravit odvar</trd>,
    <trd>vyluhovat varem</trd>,
    <trd>vyva&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>auskochen</trd>,
    <trd>absieden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decoct</trd>,
    <trd>boil out</trd>
  </trdgrp>
  <trd lng="fr">faire une d&eacute;coction (de)</trd>
  <trd lng="he">&lamed;&he;&resh;&tav;&yod;&het;</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;zetet k&eacute;sz&iacute;t</trd>,
    <trd>forral</trd>,
    <trd>kif&odblac;z</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>warzy&cacute;</trd>,
    <trd>zaparza&cacute;</trd>,
    <trd>robi&cacute; <ind>dekokt</ind></trd>
  </trdgrp>
  <trd lng="pt">fazer <ind>decoc&ccedil;&atilde;o</ind></trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
  <ind>&c_o;&c_t;&c_v;&c_a;&c_r;</ind></trd>
  <trdgrp lng="sk">
    <trd>pripravi&tcaron; odvar</trd>,
    <trd>vyl&uacute;hova&tcaron; varom</trd>
  </trdgrp>
  <trd lng="uk">&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x432;&#x456;&#x434;&#x432;&#x430;&#x440;</trd>
</drv>

<drv mrk="dekokt.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="dekokt.0ajxo.MED">
    <uzo tip="fak">KUI</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="likv.0ajxo">Likva&jcirc;o</ref> farita per <tld/>ado:
      <ekz>
        timiana <tld/>a&jcirc;o
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        al malsana je la hepato oni devas doni florajn <tld/>ojn,
        anta&ubreve; &ccirc;io verbenajn kaj valerianajn
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="buljon.0o">buljono</ref>,
      <ref cel="tizan.0o">tizano</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="infuz.0ajxo">infuza&jcirc;o</ref>,
      <ref cel="tinktu.0o.MED">tinkturo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_r;</trd>
  <trd lng="ca">decocci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>dekokt</trd>,
    <trd>dekoktum</trd>,
    <trd>odvar</trd>,
    <trd>v&yacute;var</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sud</trd>,
    <trd>Auszug</trd>
  </trdgrp>
  <trd lng="en">decoction</trd>
  <trd lng="es">decocci&oacute;n</trd>
  <trd lng="fr">d&eacute;coction</trd>
  <trd lng="he">&he;&resh;&tav;&het;&he;</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;zet</trd>,
    <trd>kivonat<klr tip="ind"> (f&odblac;zet)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wywar</trd>,
    <trd>napar</trd>,
    <trd>dekokt</trd>
  </trdgrp>
  <trd lng="pt">decoc&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>odvar</trd>,
    <trd>v&yacute;var</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x456;&#x434;&#x432;&#x430;&#x440;</trd>
</drv>
</art>
