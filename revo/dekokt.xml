<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dekokt</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="dekokt.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KUI</uzo>
  <uzo tip="fak">MED</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Longe boligi en akvo vegeta&jcirc;on por solvi &gcirc;iajn solveblajn
      substancojn:
      <ekz>
        li ordonis <tld/>i unu dozon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 53a, p. 182a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="brog.0i">brogi</ref>,
    <ref cel="infuz.0i">infuzi</ref>,
    <ref cel="macer.0i">maceri</ref>.
  </refgrp>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_a;&c_d;&c_v;&c_a;&c_r;</ind></trd>
  <trdgrp lng="de">
    <trd>auskochen</trd>,
    <trd>absieden</trd>
  </trdgrp>
  <trd lng="en">decoct</trd>
  <trd lng="fr">faire une d&eacute;coction (de)</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;zetet k&eacute;sz&iacute;t</trd>,
    <trd>forral</trd>,
    <trd>kif&odblac;z</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
  &c_o;&c_t;&c_v;&c_a;&c_r;</trd>
</drv>

<drv mrk="dekokt.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <uzo tip="fak">KUI</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Likva&jcirc;o farita per <tld/>ado:
      <ekz>
        al malsana je la hepato oni devas doni florajn <tld/>ojn, anta&ubreve;
        &ccirc;io
        verbenajn kaj valerianajn
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sub">
    <ref cel="buljon.0o">buljono</ref>,
    <ref cel="tizan.0o">tizano</ref>.
  </refgrp>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_r;</trd>
  <trd lng="ca">decocci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Sud</trd>,
    <trd>Auszug</trd>
  </trdgrp>
  <trd lng="es">decocci&oacute;n</trd>
  <trd lng="fr">d&eacute;coction</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;zet</trd>,
    <trd>kivonat<klr tip="ind"> (f&odblac;zet)</klr></trd>
  </trdgrp>
</drv>
</art>
