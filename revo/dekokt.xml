<art mrk="$Id$">
<kap>
<rad>dekokt</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="dekokt.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KUI</uzo>
  <uzo tip="fak">MED</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Longe boligi en akvo vegeta&jcirc;on por solvi &gcirc;iajn solveblajn substancojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>auskochen</trd>,
    <trd>absieden</trd>
  </trdgrp>
  <trd lng="fr">faire une d&eacute;coction (de)</trd>
  <trd lng="en">decoct</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_o;&c_t;&c_v;&c_a;&c_r;</trd>
</drv>
<drv mrk="dekokt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      La rezultanta likva&jcirc;o.
    </dif>
  </snc>
  <trd lng="fr">d&eacute;coction</trd>
</drv>
</art>
