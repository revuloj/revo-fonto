<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kompres</rad>/o
</kap>
<drv mrk="kompre1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Plurfoje faldita tola peco, kiun oni metas sur vundon a&ubreve;
      doloran korpoparton por firmigi banda&gcirc;on, kolekti puson,
      mildigi brulumon ktp.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_r;&c_e;&c_s;</trd>
  <trd lng="ca">compresa</trd>
  <trdgrp lng="cs">
    <trd>náčinek</trd>,
    <trd>obklad</trd>,
    <trd>obkladek</trd>,
    <trd>zábal</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kompresse</trd>,
     <trd>feuchter <ind>Umschlag</ind></trd>
  </trdgrp>
  <trd lng="en">compress</trd>
  <trd lng="es">compresa</trd>
  <trd lng="fr">compresse</trd>
  <trd lng="hu">borogat&aacute;s</trd>
  <trd lng="nl">kompres</trd>
  <trd lng="pl">kompres</trd>
  <trd lng="pt">compressa</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_r;&c_je;&c_s;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kompresia</trd>,
    <trd>stlačenie</trd>
  </trdgrp>
</drv>
</art>
