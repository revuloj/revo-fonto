<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompre1.xml,v 1.24 2023/12/03 09:58:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kompres</rad>/o
</kap>
<drv mrk="kompre1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Plurfoje faldita tola peco, kiun oni metas sur vundon a&ubreve;
      doloran korpoparton por firmigi banda&gcirc;on, kolekti puson,
      mildigi brulumon ktp:
      <ekz>
        la petantoj malaperis de l' placo, kaj multaj dum kelke da tagoj metis <tld/>ojn
        sur la kapon a&ubreve; sur alian batitan parton de sia korpo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas banda&gcirc;o el unu peco da gazo, neteksa&jcirc;o, a&ubreve; anka&ubreve; faldita tuko, kiu
        servas kiel &ccirc;esigilo dum sangado (i. a. per premado) kaj kiel &scirc;irmilo de la
        vundo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=445962">Kompreso</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_r;&c_e;&c_s;</trd>
  <trd lng="ca">compresa</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;&ccaron;inek</trd>,
    <trd>obklad</trd>,
    <trd>obkladek</trd>,
    <trd>z&aacute;bal</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Wickel</trd>,
     <trd>Kompresse</trd>,
     <trd>Umschlag</trd>
     <trd>Verband</trd>,
     <trd>Wundverband</trd>,
  </trdgrp>
  <trd lng="en">compress</trd>
  <trd lng="es">compresa</trd>
  <trd lng="fr">compresse</trd>
  <trd lng="hu">borogat&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x5727;&#x8FEB;&#x30AC;&#x30FC;&#x30BC;  <pr>&#x3042;&#x3063;&#x3071;&#x304F;&#x30AC;&#x30FC;&#x30BC;</pr></trd>,
    <trd>&#x6E7F;&#x5E03;  <pr>&#x3057;&#x3063;&#x3077;</pr></trd>
  </trdgrp>
  <trd lng="nl">kompres</trd>
  <trd lng="pl">kompres</trd>
  <trd lng="pt">compressa</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_r;&c_je;&c_s;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kompresia</trd>,
    <trd>stla&ccaron;enie</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x43F;&c_r;&c_je;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x70ED;&#x6577; <pr>r&#xE8;f&#x16B;</pr></trd>,
    <trd>&#x51B7;&#x6577; <pr>l&#x11B;ngf&#x16B;</pr></trd>
  </trdgrp>
</drv>
</art>
