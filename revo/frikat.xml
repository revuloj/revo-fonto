<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frikat.xml,v 1.24 2022/03/20 13:07:55 revo Exp $">
  <kap>
    <rad>frikativ</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>

<drv mrk="frikat.0o">
  <kap><tld/>o</kap>
  <snc mrk="frikat.0o.FON">
  <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="dif" cel="frot.0sono.FON">Frotsono<sncref/></ref>:
      <ekz>
        unu el la plej konataj vortoj de Inuktituto, 
        &leftquot;<nac>iglu</nac>&rightquot;, estas en <nom>Kalaallisut</nom>
        &leftquot;<nac>illu</nac>&rightquot; (&ccirc;e tio &leftquot;ll&rightquot; staras por la laterala <tld/>o &leftquot;&#x26C;&rightquot;)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9293">Gronlanda lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y; &c_g;&c_u;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_n;&c_a; &c_s;&c_malmol;&c_g;&c_l;&c_a;&c_s;&c_n;&c_a;</trd>
  <trd lng="ca">consonant <ind>fricativa</ind></trd>
  <trd lng="cs">frikativa</trd>
  <trdgrp lng="de">
    <trd>Frikativ</trd>,
    <trd>Engelaut</trd>,
    <trd>Reibelaut</trd>
  </trdgrp>
  <trd lng="en">fricative</trd>
  <trd lng="es">fricativa</trd>
  <trd lng="fa">&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n;
      <ind>&a_s;&a_A;&f_y;&a_sh;&f_y;</ind></trd>
  <trdgrp lng="fr">
    <trd>fricative<klr> (subst.)</klr></trd>,
    <trd>spirante<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">fricatief</trd>
  <trdgrp lng="pt">
    <trd>consoante <ind>constritiva fricativa</ind></trd>,
    <trd>fricativa</trd>,
    <trd>consoante <ind>constritiva</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_r;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">tren&aacute; hl&aacute;ska</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x440;&#x438;&#x43A;&#x430;&#x442;&#x438;&#x432;&#x43D;&#x438;&#x439; &#x437;&#x432;&#x443;&#x43A;</trd>,
    <trd>&#x441;&#x43F;&#x456;&#x440;&#x430;&#x43D;&#x442;</trd>
  </trdgrp>
</drv>
  </art>
