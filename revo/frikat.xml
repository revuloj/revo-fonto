<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>frikativ</rad>/o</kap>
<drv mrk="frikat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Da&ubreve;ra konsonanto, produktata per malvastigo de la bu&scirc;kanalo,
      tra kiu la spirblovo eliras nur naskante frotan brueron
      (f, v, s, z, &scirc;, &jcirc;, &hcirc;).
    </dif>
    <refgrp tip="vid">
      <ref cel="plosiv.0o">plosivo</ref>,
      <ref cel="afrika.0o">afrikato</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y; &c_g;&c_u;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Frikativ</trd>,
    <trd>Engelaut</trd>,
    <trd>Reibelaut</trd>
  </trdgrp>
  <trd lng="es">fricativa</trd>
  <trdgrp lng="fr">
    <trd>fricative</trd>,
    <trd>spirante</trd>
  </trdgrp>
  <trd lng="nl">fricatief</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_r;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
