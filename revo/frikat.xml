<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>frikativ</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="frikat.0o">
  <kap><tld/>o</kap>
  <snc mrk="frikat.0o.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Da&ubreve;ra konsonanto, produktata per malvastigo de la bu&scirc;kanalo,
      tra kiu la spirblovo eliras nur naskante frotan brueron
      (f, v, s, z, &scirc;, &jcirc;, &hcirc;).
    </dif>
    <refgrp tip="vid">
      <ref cel="afrika.0o.FON">afrikato</ref>,
      <ref cel="plosiv.0o.FON">plosivo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y; &c_g;&c_u;&c_k;</trd>
  <trd lng="ca">consonant <ind>fricativa</ind></trd>
  <trdgrp lng="de">
    <trd>Frikativ</trd>,
    <trd>Engelaut</trd>,
    <trd>Reibelaut</trd>
  </trdgrp>
  <trd lng="en">fricative</trd>
  <trd lng="es">fricativa</trd>
  <trd lng="fa">&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n; <ind>&a_s;&a_A;&f_y;&a_sh;&f_y;</ind></trd>
  <trdgrp lng="fr">
    <trd>fricative<klr> (subst.)</klr></trd>,
    <trd>spirante<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">fricatief</trd>
  <trdgrp lng="pt">
    <trd>consoante <ind>constritiva fricativa</ind></trd>,
    <trd>fricativa</trd>,
    <trd>consoante <ind>constritiva</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_r;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
