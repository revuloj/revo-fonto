<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tinktu.xml,v 1.21 2023/10/21 13:52:03 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tinktur</rad>/o
</kap>
<drv mrk="tinktu.0o">
  <kap><tld/>o</kap>
  <snc mrk="tinktu.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Flua&jcirc;a <ref tip="super" cel="substa.0o.FIZ">substanco</ref> por kolorigi teksa&jcirc;ojn a&ubreve;
      fadenojn per ensorbado:
      <ekz>
        planto, kiu liveras verdan <tld/>on
        <fnt>
          <aut>Wells</aut>
          <vrk>Lingvistikaj aspektoj de Esperanto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sciis, kiel fari la pli malfacilajn <tld/>ojn: la bluon kaj la ru&gcirc;on, kiujn
        la plejmulto el la homoj e&ccirc; ne kura&gcirc;is provi
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>dye</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>textilfest&eacute;k</trd>,
    <trd>kelmefest&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tynktura</trd>,
    <trd>bejca</trd>,
    <trd>farba<klr> (do tkanin)</klr></trd>,
    <trd>barwnik</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tintura</trd>
  </trdgrp>
    <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol; 
<klr tip="amb">(&c_d;&c_l;&c_ja;
    &c_t;&c_k;&c_a;&c_n;&c_je;&c_j;)</klr></trd>
   <trdgrp lng="it">
      <trd>tintura</trd>, 
      <trd>tinta<klr> (sost.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="tinktu.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="solv.0ajxo">Solva&jcirc;o</ref> de iu substanco en 
      <ref tip="prt" cel="alkoho.0o.KUI">alkoholo</ref>: 
      <ekz>
        fenkola <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de opio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kuracisto <klr>[&#x2026;]</klr> energie trumpetis pri sia propra lerteco, la&ubreve;dis siajn
        <tld/>ojn kaj miraklajn &scirc;mira&jcirc;ojn
        <fnt>
          <bib>RBa</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekokt.0ajxo">dekokta&jcirc;o</ref>,
      <ref cel="infuz.0ajxo">infuza&jcirc;o</ref>
    </refgrp>
    <trd lng="hu">alkoholos oldat</trd>  
    <trd lng="pl">tynktura</trd>
    <trdgrp lng="pt">
      <trd>alcoolatura</trd>,
      <trd>tintura</trd>
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_t;&c_i;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_k;&c_a;</trd>
    </trdgrp>
   <trd lng="it">tintura</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>barvivo</trd>,
    <trd>mo&rcaron;idlo</trd>,
    <trd>tinktura</trd>
  </trdgrp>
  <trd lng="de">Tinktur</trd>
  <trd lng="en">tincture</trd>
  <trd lng="fr">teinture</trd>
  <trdgrp lng="ja">
    <trd>&#x67D3;&#x6599;  <pr>&#x305B;&#x3093;&#x308A;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x30C1;&#x30F3;&#x30AD;</trd>
  </trdgrp>
  <trd lng="nl">tinctuur</trd>
  <trdgrp lng="sk">
    <trd>alkoholov&yacute; roztok</trd>,
    <trd>tinkt&uacute;ra</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_d;&#x43A;&c_a; &c_f;&c_a;&c_r;&c_b;&c_a; (&c_d;&#x43B;&#x44F; &c_t;&#x43A;&c_a;&#x43D;&c_i;&#x43D;)</trd>,
    <trd>&c_t;&c_i;&#x43D;&#x43A;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_t;&#x43E;&c_j;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tinktu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tinktu.0i.kolorigi">
    <dif>
      <ref tip="super" cel="kolor.0igi">Kolorigi</ref> per <tld/>o:
      <ekz>
        arkeologiaj trovoj indikas ke jam &ccirc;. 3000 a.K. oni <tld/>is lanon ru&gcirc;a, blua,
        verda, kaj flava en Mezopotamio
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        batiko <klr>[estas]</klr> metodo, precipe uzata en Javo, por <tld/>i &scirc;tofojn per
        koloraj desegnoj
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili havis <tld/>itan hararon
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>barvit <klr>(l&aacute;tky, vlasy)</klr></trd>,
    <trd>mo&rcaron;it mo&rcaron;idlem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einf&auml;rben</trd>,
    <trd>tauchf&auml;rben</trd>
  </trdgrp>
  <trd lng="en">dye</trd>
  <trd lng="fr">teindre</trd>
  <trd lng="hu">fest <klr tip="ind">(kelm&eacute;t)</klr></trd>
  <trd lng="it">tingere<klr> (tessuti)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x67D3;&#x3081;&#x308B;  <pr>&#x305D;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x67D3;&#x8272;&#x3059;&#x308B;  <pr>&#x305B;&#x3093;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verven<klr tip="ind"> (v.stoffen)</klr></trd>
  <trdgrp lng="pl">
    <trd>barwi&cacute;</trd>,
    <trd>zabarwia&cacute;</trd>,
    <trd>farbowa&cacute;</trd>,
    <trd>bejcowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;
  <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">farbi&tcaron; tinkt&uacute;rou</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_r;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43A;&c_r;&c_a;&c_s;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="tinktu.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies <ref tip="lst" lst="voko:metioj" cel="meti.0o">metio</ref> 
      estas kolorigi a&ubreve; senmakuligi teksa&jcirc;ojn:
      <ekz>
        kiam <tld/>istoj volas <tld/>i lanon purpura, <klr>[&#x2026;]</klr> ili prenas el la
        multaj koloroj nur la blankan lanon
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiutempe urino estis utiligata de <tld/>istoj, ili uzis la likvon por prepari
        siajn &scirc;tofojn anta&ubreve; la tinkturado kaj por degrasigo de lano
        <fnt>
          <bib>Kon11</bib>
          <aut>Roxane Romann</aut>
          <vrk>Mono ne odoras</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">barv&iacute;&rcaron;</trd>
  <trd lng="de">F&auml;rber</trd>
  <trd lng="en">dyer</trd>
  <trd lng="fr">teinturier</trd>
  <trdgrp lng="hu">
    <trd>textilfest&odblac;</trd>,
    <trd>kelmefest&odblac;</trd>
  </trdgrp>
  <trd lng="it">tintore</trd>
  <trdgrp lng="ja">
    <trd>&#x67D3;&#x7269;&#x5C4B;  <pr>&#x305D;&#x3081;&#x3082;&#x306E;&#x3084;</pr></trd>,
    <trd>&#x67D3;&#x7269;&#x8077;&#x4EBA;  <pr>&#x305D;&#x3081;&#x3082;&#x306E;&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">verver</trd>
  <trd lng="pl">farbiarz</trd>
  <trd lng="ru">
&c_k;&c_r;&c_a;&c_s;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
  <trd lng="sk">chemick&yacute; &ccaron;isti&ccaron; a&nbsp;farbiar</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>,
    <trd>&#x43A;&c_r;&c_a;&c_s;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

</art>
