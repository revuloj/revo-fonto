<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tinktur</rad>/o
</kap>
<drv mrk="tinktu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Flua&jcirc;a substanco por kolorigi teksa&jcirc;ojn a&ubreve; fadenojn per ensorbado.
    </dif>
  <trdgrp lng="hu">
    <trd>textilfest&eacute;k</trd>,
    <trd>kelmefest&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tintura</trd>
  </trdgrp>
    <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_je;&c_j;)</klr></trd>
  </snc>
  <snc>
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Solva&jcirc;o de iu substanco en alkoholo.
    </dif>
    <trd lng="hu">alkoholos oldat</trd>  
  <trdgrp lng="pt">
    <trd>alcoolatura</trd>,
    <trd>tintura</trd>
  </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_t;&c_i;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Tinktur</trd>
  <trd lng="fr">teinture</trd>
  <trd lng="nl">tinctuur</trd>
</drv>

<drv mrk="tinktu.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies metio estas kolorigi a&ubreve; senmakuligi teksa&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="de">F&auml;rber</trd>
  <trd lng="fr">teinturier</trd>
  <trdgrp lng="hu">
    <trd>textilfest&odblac;</trd>,
    <trd>kelmefest&odblac;</trd>
  </trdgrp>
  <trd lng="nl">verver</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
</drv>

<drv mrk="tinktu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kolorigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">f&auml;rben</trd>
  <trd lng="fr">teindre</trd>
  <trd lng="hu">fest <klr tip="ind">(kelm&eacute;t)</klr></trd>  
  <trd lng="nl">verven<klr tip="ind"> (v.stoffen)</klr></trd>
  <trd lng="ru">&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
</drv>
</art>
