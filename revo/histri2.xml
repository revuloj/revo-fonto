<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: histri2.xml,v 1.17 2023/11/15 17:08:01 revo Exp $">
<kap>
   <rad>histriked</rad>/oj
</kap>
<drv mrk="histri2.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Hystricidae</trd>), kies haroj estas transformitaj
       al pingloj:
      <ekz>
        Longhara rato: <klr>[&#x2026;]</klr> &gcirc;ia formo <klr>[&#x2026;]</klr> similas al tiu de la
        <tld/>oj, &gcirc;ia kapo al tiu de la kobajo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=375548">Longhara rato</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_k;&c_a;&c_b;&c_r;&c_a;&c_z;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_b;&c_o;&c_d;&c_l;&c_i;&c_v;&c_i; &c_s;&c_v;&c_i;&c_n;&c_ch;&c_je;&c_t;&c_a;</trd>
  <trd lng="ca">histr&iacute;cids</trd>
  <trd lng="cs">dikobrazovit&iacute;</trd>
  <trd lng="de">Stachelschweine<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hystricid&eacute;s</trd>
  <trd lng="hu">gyalogs&uuml;lf&eacute;l&eacute;k</trd>
  <trd lng="nl">stekelvarkens<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">histric&iacute;deos</trd>
  <trd lng="ru">&c_d;&c_i;&c_k;&c_o;&c_b;&c_r;&c_a;&c_z;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">dikobrazovit&eacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x7BAD;&#x732A; <pr>ji&#xE0;nzh&#x16B;</pr></trd>,
    <trd>&#x8C6A;&#x732A; <pr>h&#xE1;ozh&#x16B;</pr></trd>
  </trdgrp>
</drv>
</art>
