<art mrk="$Id: porter.xml,v 1.18 2023/09/03 12:45:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>porter</rad>/o
</kap>
<drv mrk="porter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Forta angla <ref tip="super" cel="bier.0o">biero</ref>:
      <ekz>
        Nikolao Petrovi&ccirc; ordonis alporti kelke da boteloj de <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 22a, p.
        143a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_r;&c_t;&c_e;&c_r;</trd>
  <trd lng="cs">porter</trd>
  <trdgrp lng="de">
    <trd>Porter-Bier</trd>,
    <trd>Porter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>porter<klr> (beer)</klr></trd>
  </trdgrp>
  <trd lng="es">porter<klr> (cerveza)</klr></trd>
  <trdgrp lng="fr">
    <trd>porter<klr> (bi&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>porter</trd>,
    <trd>barna <ind>s&ouml;r</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x30DD;&#x30FC;&#x30BF;&#x30FC;</trd>
  <trd lng="nl">porter</trd>
  <trd lng="pl">porter</trd>
  <trd lng="pt">p&oacute;rter</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>porter</trd>,
    <trd>tmav&eacute; siln&eacute; pivo</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x440;&#x442;&#x435;&#x440; (&#x441;&#x43E;&#x440;&#x442; &#x43F;&#x438;&#x432;&#x430;)</trd>
</drv>
</art>
