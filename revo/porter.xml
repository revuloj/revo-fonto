</kap>
<drv mrk="porter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Forta angla <ref tip="super" cel="bier.0o">biero</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Porter-Bier</trd>,
    <trd>Porter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>porter</trd>,
    <trd>barna <ind>s&ouml;r</ind></trd>
  </trdgrp>
  <trd lng="nl">porter</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_je;&c_r;</trd>
</drv>
</art>
