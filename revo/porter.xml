<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>porter</rad>/o
</kap>
<drv mrk="porter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Forta angla <ref tip="super" cel="bier.0o">biero</ref>:
      <ekz>
        Nikolao Petrovi&ccirc; ordonis alporti kelke da boteloj de <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 22a, p. 143a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_r;&c_t;&c_e;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Porter-Bier</trd>,
    <trd>Porter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>porter<klr> (bi&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>porter</trd>,
    <trd>barna <ind>s&ouml;r</ind></trd>
  </trdgrp>
  <trd lng="nl">porter</trd>
  <trd lng="pt">p&oacute;rter</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_je;&c_r;</trd>
</drv>
</art>
