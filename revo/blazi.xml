<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: blazi.xml,v 1.10 2024/07/10 08:32:43 revo Exp $">
    <kap>
      <rad>Blazi</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="blazi.0o">
      <kap><tld/>o</kap>
      <snc mrk="blazi.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
          latindevena:
          <ekz>
            el la <ctl>pensoj</ctl> de <tld/>o Paskalo
            <fnt>
              <aut>trad. Valo</aut>,
              <vrk>El la &leftquot;pensoj&rightquot; de Blazio Paskalo</vrk>,
              <lok>SAT-Bro&scirc;urservo, 1986</lok>
            </fnt>;
          </ekz><ekz>
            <tld/>o estas lingvisto, kiu esploras historion de la
            hungara lingvo
            <fnt>
              <aut>K. Kov&aacute;ts</aut>,
              <vrk>Teamo</vrk>,
              <lok><url ref="http://www.edukado.net">Edukado.net</url>,
                2006-05-05</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_U;&c_l;&c_a;&c_s;</trd>,
        <trd>&c_B;&c_l;&c_je;&c_z;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Blasius</trd>,
        <trd>Blaise</trd>
      </trdgrp>
      <trd lng="fr">Blaise</trd>
      <trd lng="grc">&Beta;&lambda;&alfa_ton;&sigma;&jota;&omikron;&sigma_fina;</trd>
      <trd lng="it">Biagio</trd>
      <trd lng="pl">Blaise<klr> (brak polskiego
      odpowiednika)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_V;&c_l;&c_a;&c_s;</trd>,
        <trd>&c_V;&c_l;&c_a;&c_s;&c_i;&c_j;</trd>
      </trdgrp>
      <trdgrp lng="uk">
    <trd>&c_B;&#x43B;&c_a;&c_z;</trd>,
    <trd>&c_V;&#x43B;&c_a;&c_s;</trd>,
    <trd>&c_U;&#x43B;&c_a;&c_s;&#xA0;(&c_ib;&#x43C;&#8217;&#x44F;)</trd>
  </trdgrp>
</drv>
  </art>

