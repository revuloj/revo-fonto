<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fik.xml,v 1.21 2022/04/27 17:19:11 revo Exp $">
<kap>
  <rad>fik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="fik.0i.sekskunigxi">
    <dif>
      <ref tip="dif" cel="kun.seks0igxi">Sekskuni&gcirc;i</ref>:
      <ekz>
        <klr>[mia obeema Gnom']</klr> povos fine <tld/>i sen kondom'!
        <fnt><aut>P. Peneter</aut><vrk>Sekretaj
        Sonetoj</vrk><lok>25</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la gastiganto]</klr> fikas vin, kaj vi diras nenion!
        <fnt>
        <aut>L. Beaucaire</aut>
        <vrk>Kruko kaj Baniko en Bervalo</vrk>
        <lok>La dormanto veki&gcirc;as</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="amor.0i">amori</ref>,
      <ref cel="koit.0i">koiti</ref>,
      <ref cel="kopula.0i">kopulacii</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_je;&c_b;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>follar</trd>,
    <trd>cardar</trd>,
    <trd>fotre</trd>
  </trdgrp>
  <trd lng="cs">soulo&zcaron;it <klr>(vulg.)</klr></trd>
  <trdgrp lng="de">
    <trd>ficken</trd>,
    <trd>bumsen</trd>,
    <trd>pimpern</trd>,
    <trd>v&ouml;geln</trd>
  </trdgrp>
  <trd lng="en">fuck</trd>
  <trd lng="es">follar<klr> (vulg.)</klr></trd>
  <trdgrp lng="fr">
    <trd>baiser<klr tip="ind"> (faire l'amour)</klr></trd>,
    <trd>coucher (avec)</trd>
  </trdgrp>
  <trd lng="hu">baszik</trd>
  <trd lng="nl">neuken</trd>
  <trdgrp lng="pl">
    <trd>pieprzy&cacute;</trd>,
    <trd>dupczy&cacute;</trd>,
    <trd>wali&cacute;</trd>,
    <trd>jeba&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>foder</trd>,
    <trd>copular</trd>,
    <trd>transar</trd>,
    <trd>trepar</trd>,
    <trd>fazer <ind>amor</ind></trd>,
    <trd>meter</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jeba&tcaron;</trd>,
    <trd>s&uacute;lo&zcaron;i&tcaron; <klr>(vulg.)</klr></trd>
  </trdgrp>
  <trd lng="tok">unpa</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43B;&#x44F;&#x433;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x457;&#x431;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x456;&#x434;&#x448;&#x435; &#x457;&#x431;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x445;&#x456;&#x434;&#x43D;&#x456;</trd>
  </trdgrp>
</drv>

<drv mrk="fik.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="fik.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        kun &ccirc;armo pla&ccirc;a ektrilos al mi
        e&ccirc; la vorto: <tld/>'
        <fnt><aut>P. Peneter</aut><vrk>Sekretaj
        Sonetoj</vrk><lok>33</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="amor.0o.seksa">amoro</ref>,
      <ref cel="koit.0o">koito</ref>,
      <ref cel="kopula.0o">kopulacio</ref>,
      <ref cel="kun.seks0igxo">sekskuni&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_je;&c_b;&c_a;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>"follada"</trd>
  </trdgrp>
  <trd lng="cs">soulo&zcaron; <klr>(vulg.)</klr></trd>
  <trd lng="fr">baise<klr tip="ind"> (faire l'amour)</klr></trd>
  <trdgrp lng="pl">
    <trd>pieprzenie</trd>,
    <trd>dupczenie</trd>,
    <trd>walenie</trd>,
    <trd>jebanie</trd>
  </trdgrp>
  <trd lng="sk">s&uacute;lo&zcaron; <klr>(vulg.)</klr></trd>
</drv>
</art>
