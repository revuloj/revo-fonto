<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>menur</rad>/o
</kap>
<drv mrk="menur.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zooligaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="menure.0oj"><tld/>edoj</ref>
      (<trd lng="la">Menura</trd>), kun tre bela vostplumaro havanta
      formon de liro, kaj vivanta en A&ubreve;stralio.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_r;&c_a;&c_h;&c_v;&c_o;&c_s;&c_t;</trd>
  <trd lng="de">Leierschwanz</trd>
  <trd lng="en">lyrebird</trd>
  <trd lng="fr">oiseaux-lyres</trd>
  <trd lng="hu">lantfark&uacute; mad&aacute;r</trd>
  <trd lng="nl">liervogel</trd>
  <trdgrp lng="pt">
    <trd>ave-lira</trd>,
    <trd>lira <klr tip="amb">(ave)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_o;&c_h;&c_v;&c_o;&c_s;&c_t;</trd>
</drv>
</art>
