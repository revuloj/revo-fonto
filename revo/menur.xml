<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>menur</rad>/o
</kap>
<drv mrk="menur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="menur.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Menura</trd>), kun tre bela vostplumaro havanta formon de 
      liro, kaj vivanta en A&ubreve;stralio.
    </dif>
  </snc>
  <trd lng="de">Leierschwanz</trd>
  <trd lng="en">lyrebird</trd>
  <trd lng="fr">oiseaux-lyres</trd>
</drv>
<drv mrk="menur.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="de">Menuridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Leierschw&auml;nze <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
