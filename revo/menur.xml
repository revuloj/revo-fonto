<vortaro>
<art mrk="$Id$">
<kap>
<rad>menur</rad>/o
</kap>
<drv mrk="menur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="menure.0oj"><tld/>edoj</ref>
      (<trd lng="la">Menura</trd>), kun tre bela vostplumaro havanta formon de 
      liro, kaj vivanta en A&ubreve;stralio.
    </dif>
  </snc>
  <trd lng="de">Leierschwanz</trd>
  <trd lng="en">lyrebird</trd>
  <trd lng="fr">oiseaux-lyres</trd>
  <trd lng="nl">liervogel</trd>
</drv>

</art>

