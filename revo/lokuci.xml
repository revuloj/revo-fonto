        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="turn.parol0o">parolturno</ref>
    <trd lng="de">Redewendung</trd>
    <trd lng="en">locution</trd>
    <trd lng="fr">locution</trd>
  </snc>
</drv>

