<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krest.xml,v 1.18 2018/09/23 23:10:15 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>krest</rad>/o
</kap>
<drv mrk="krest.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Karna ru&gcirc;a elkreska&jcirc;o, kiu elstaras sur kapo de koko, meleagro
      kaj
      samspecaj birdoj.
    </dif>
    <trd lng="ca">cresta</trd>
    <trdgrp lng="de">
      <trd>Kamm</trd>,
      <trd>Federschopf</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crest</trd>,
      <trd>comb</trd>
    </trdgrp>
    <trd lng="es">cresta</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&a_A;&a_j;<klr tip="ind"> (&a_kh;&a_r;&a_w;&a_s;)</klr></trd>,
      <trd>&f_k;&a_A;&f_k;&a_l;<klr tip="ind"> (&f_p;&a_r;&a_n;&a_d;&a_h;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tar&eacute;j</trd>,
      <trd>taraj</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>hanenkam</trd>,
      <trd>kam</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>czub</trd>,
      <trd>grzebie&nacute; <klr>(ptaka)</klr></trd>
    </trdgrp>  
    <trd lng="pt">crista <klr tip="amb">(zool.)</klr></trd>
    <trd lng="ru">&c_g;&c_r;&c_je;&c_b;&c_je;&c_sh;&c_o;&c_k; <klr>(&c_u; &c_p;&c_t;&c_i;&c_c;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Longforma alta&jcirc;o:
      <ekz>
        monto<tld/>o, <tld/>akra&jcirc;o;
      </ekz>
      <ekz>
        ondo<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kamm</trd>,
      <trd>Grat</trd>,
      <trd>Krone</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crest</trd>,
      <trd>ridge</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&a_t;&f_y;&a_gh;</trd>,
      <trd>&a_q;&a_l;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tar&eacute;j</trd>,
      <trd>gerinc</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kam</trd>,
      <trd>bergkam</trd>
    </trdgrp>
    <trd lng="pl">grzbiet</trd>
    <trdgrp lng="pt">
      <trd>cume</trd>,
      <trd>cimo</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_mol; <klr>(&c_g;&c_o;&c_r;&c_y;)</klr></trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_b;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="fr">cr&ecirc;te</trd>
  <trd lng="ro">culme</trd>
</drv>
</art>
