<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>krest</rad>/o
</kap>
<drv mrk="krest.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Karna ru&gcirc;a elkreska&jcirc;o, kiu elstaras sur kapo de koko, meleagro kaj
      samspecaj birdoj.
    </dif>
    <trd lng="ca">cresta</trd>
    <trdgrp lng="de">
      <trd>Kamm</trd>,
      <trd>Federschopf</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crest</trd>,
      <trd>comb</trd>
    </trdgrp>
    <trd lng="es">cresta</trd>
    <trdgrp lng="hu">
      <trd>tar&eacute;j</trd>,
      <trd>taraj</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_je;&c_b;&c_je;&c_sh;&c_o;&c_k; <klr>(&c_u; &c_p;&c_t;&c_i;&c_c;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Longforma alta&jcirc;o:
      <ekz>
        monto<tld/>o, <tld/>akra&jcirc;o;
      </ekz>
      <ekz>
        ondo<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kamm</trd>,
      <trd>Grat</trd>,
      <trd>Krone</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crest</trd>,
      <trd>ridge</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tar&eacute;j</trd>,
      <trd>gerinc</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_mol; <klr>(&c_g;&c_o;&c_r;&c_y;)</klr></trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_b;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="fr">cr&ecirc;te</trd>
</drv>
</art>
