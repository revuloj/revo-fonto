<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>promoci</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="promoc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="promoc.0i.ofice">
    <dif>
      Altigi al iu pli alta grado, ofico:
      <ekz>
        <tld/>i episkopon al kardinaleco;
      </ekz>
      <ekz>
        <tld/>i iun le&ubreve;tenanto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bef&ouml;rdern</trd>
  <trdgrp lng="fr">
    <trd>&eacute;lever<klr> (en dignit&eacute;)</klr></trd>,
    <trd>promouvoir<klr> (&agrave; un poste)</klr></trd>
  </trdgrp>
  <trd lng="hu">el&odblac;l&eacute;ptet</trd>
  <trdgrp lng="nl">
    <trd>bevorderen</trd>,
    <trd>promoveren</trd>
  </trdgrp>
  <trd lng="pt">promover <klr>(a um cargo mais alto)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v; &c_ch;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_y;&c_sh;&c_a;&c_t;&c_mol; &c_v; &c_z;&c_v;&c_a;&c_n;&c_i;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="promoc.0o">
  <kap><tld/>o</kap>
  <snc mrk="promoc.0o.ofica">
    <dif>
      Alti&gcirc;o al ofico:
      <ekz>
        &leftquot;vidu, nun tiu ricevis <tld/>on&rightquot;
        <fnt><bib>Fab2</bib>, <lok>flikilo</lok></fnt>!
      </ekz>
      <ekz>
        ili estas el la sama <tld/>o
        <klr>(estis samtempe <tld/>itaj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bef&ouml;rderung</trd>
  <trd lng="fr">promotion</trd>
  <trdgrp lng="nl">
    <trd>bevordering</trd>,
    <trd>promotie</trd>
  </trdgrp>
  <trd lng="hu">el&odblac;l&eacute;ptet&eacute;s</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o; &c_v; &c_ch;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_v; &c_z;&c_v;&c_a;&c_n;&c_i;&c_i;</trd>
  </trdgrp>
</drv>
</art>
