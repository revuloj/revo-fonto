<vortaro>
<art mrk="$Id: promoc.xml,v 1.26 2026/01/29 16:00:21 revo Exp $">
<kap>
  <rad>promoci</rad>/i <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="promoc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="promoc.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="rang.pli0igi.POL">Plirangigi</ref>:
      <ekz>
        <tld/>i episkopon al kardinaleco;
      </ekz>
      <ekz>
        <tld/>i iun le&ubreve;tenanto;
      </ekz>
      <ekz>
        la patro estis <tld/>ita kaj fari&gcirc;is preposto
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1846 la kirko estis <tld/>ita al la rango de katedralo
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2004/008012.html">Katedralo renaski&gcirc;anta</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_y;&c_sh;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u;
    &c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib; &c_ib; &c_d;&c_a; &c_g;. &c_p;.)</klr></trd>
    <trdgrp lng="ca">
      <trd>ascendir<klr> (alg&uacute;)</klr></trd>,
      <trd>nomenar</trd>,
      <trd>promoure<klr> (alg&uacute;)</klr></trd>
    </trdgrp>
    <trd lng="en">promote</trd>
    <trdgrp lng="cs">
      <trd>obohatit</trd>,
      <trd>roz&scaron;&iacute;&rcaron;it</trd>,
      <trd>zv&yacute;&scaron;it &uacute;rove&ncaron;</trd>
    </trdgrp>
    <trd lng="de">bef&ouml;rdern</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&a_r;&a_f;&f_y;&a_ayn; &a_d;&a_A;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_r;&a_t;&a_q;&a_A; &a_d;&a_A;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">&eacute;lever<klr> (en dignit&eacute;)</klr></trd>
    <trd lng="hu">el&odblac;l&eacute;ptet</trd>
    <trdgrp lng="nl">
      <trd>bevorderen</trd>,
      <trd>promoveren</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>promowa&cacute;</trd>,
      <trd>awansowa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">promover <klr>(a um cargo mais alto)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v; &c_ch;&c_i;&c_n;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_y;&c_sh;&c_a;&c_t;&c_mol; &c_v; &c_z;&c_v;&c_a;&c_n;&c_i;&c_i;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>obohati&tcaron;</trd>,
      <trd>roz&scaron;&iacute;ri&tcaron;</trd>,
      <trd>zv&yacute;&scaron;i&tcaron; &uacute;rove&ncaron;</trd>
    </trdgrp>
  </snc>
  <snc mrk="promoc.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Famigi, konigi kaj valorigi &ccirc;e la publiko
      diversmaniere <klr>(per ludoj, rabatoj, konkursoj...)</klr>:
      <ekz>
        por <tld/>i la lingvon, la registaroj de la e&ubreve;ska kaj nafara a&ubreve;tonomaj komunumoj
        subvenciis la &jcirc;urnalon
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2003/008082.html">Kiam arestoj pruvas terorismajn ligojn
          ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Unesko akceptis la tiel nomatan Planon Arabian por <tld/>i la dialogon inter la
        kulturoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2005/008742.php">Inter Oriento kaj Okcidento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="reklam.0i">reklami</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>promoure</trd>,
      <trd>impulsar</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>promote</trd>,
      <trd>advertise</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bewerben <klr>(Produkt)</klr></trd>,
      <trd>f&ouml;rdern</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">promouvoir</trd>
  <trdgrp lng="ja">
    <trd>&#26119;&#36914;&#12373;&#12379;&#12427;  <pr>&#12375;&#12423;&#12358;&#12375;&#12435;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#36914;&#32026;&#12373;&#12379;&#12427;  <pr>&#12375;&#12435;&#12365;&#12421;&#12358;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#25104;&#12425;&#12379;&#12427;  <pr>&#12394;&#12425;&#12379;&#12427;</pr></trd>,
    <trd>&#12450;&#12483;&#12503;&#12464;&#12524;&#12540;&#12489;&#12377;&#12427;
    <pr>&#12354;&#12387;&#12407;&#12464;&#12524;&#12540;&#12489;&#12377;&#12427;</pr></trd>,
    <trd>&#12496;&#12540;&#12472;&#12519;&#12531;&#12450;&#12483;&#12503;&#12377;&#12427;
    <pr>&#12400;&#12540;&#12376;&#12423;&#12435;&#12354;&#12387;&#12407;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21319;&#36801; <pr>sh&emacron;ngqi&amacron;n</pr></trd>,
    <trd>&#21319;&#36983; <pr>sh&emacron;ngqi&amacron;n</pr></trd>,
    <trd>&#25512;&#37559; <pr>tu&#299;xi&amacron;o</pr></trd>,
    <trd>&#25512;&#38144; <pr>tu&#299;xi&amacron;o</pr></trd>,
    <trd>&#20419;&#36827; <pr>c&ugrave;j&igrave;n</pr></trd>,
    <trd>&#20419;&#36914; <pr>c&ugrave;j&igrave;n</pr></trd>,
    <trd>&#25512;&#21160; <pr>tu&#299;d&ograve;ng</pr></trd>,
    <trd>&#25512;&#21205; <pr>tu&#299;d&ograve;ng</pr></trd>,
    <trd>&#25552;&#25300; <pr>t&iacute;b&aacute;</pr></trd>,
    <trd>&#36004; <pr>y&iacute;</pr></trd>,
    <trd>&#36896;&#21183; <pr>z&agrave;osh&igrave;</pr></trd>,
    <trd>&#36896;&#21218; <pr>z&agrave;osh&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promoc.0o">
  <kap><tld/>o</kap>
  <snc mrk="promoc.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Alti&gcirc;o al ofico:
      <ekz>
        &leftquot;vidu, nun tiu ricevis <tld/>on&rightquot;
        <fnt><bib>Fab2</bib>, <lok>flikilo</lok></fnt>!
      </ekz>
      <ekz>
        ili estas el la sama <tld/>o
        <klr>(estis samtempe <tld/>itaj)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_y;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_u;
    &c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib; &c_ib; &c_d;&c_a; &c_g;. &c_p;.)</klr></trd>
    <trdgrp lng="ca">
      <trd>ascens</trd>,
      <trd>promoci&oacute;<klr tip="amb"> (conjunt de persones)</klr></trd>
    </trdgrp>
    <trd lng="en">promotion</trd>
    <trdgrp lng="cs">
      <trd>pov&yacute;&scaron;en&iacute;</trd>,
      <trd>promoce</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bef&ouml;rderung</trd>,
      <trd>Aufstieg</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_t;&a_r;&a_f;&f_y;&a_ayn;</trd>,
      <trd>&a_A;&a_r;&a_t;&a_q;&a_A;</trd>
    </trdgrp>
    <trd lng="hu">el&odblac;l&eacute;ptet&eacute;s</trd>
    <trdgrp lng="nl">
      <trd>bevordering</trd>,
      <trd>promotie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o; &c_v;
      &c_ch;&c_i;&c_n;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_v;
      &c_z;&c_v;&c_a;&c_n;&c_i;&c_i;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>pov&yacute;&scaron;enie</trd>,
      <trd>prom&oacute;cia</trd>
    </trdgrp>
  </snc>
  <snc mrk="promoc.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Organizita famigo kaj valorigo de venda&jcirc;o:
      <ekz>
        la informado konsistas ne nur el reklamado, sed el tuta gamo da
        metodoj por <tld/>ado, el publikaj rilatoj, kaj el profesiaj
        vendosistemoj
        <fnt>
          <aut>D. Keefe</aut>,
          <vrk><url ref="&Sezonoj;2012/05/212keefe/">
          La leciono de Vakero Marlboro kaj Esperanto</url></vrk>,
          <lok>La Balta Ondo, 2012&ndash;05-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        centron de <tld/>o de albana turismo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo
          anta&ubreve;eniras</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="ca">promoci&oacute;<klr tip="amb"> (difusi&oacute;, propaganda)</klr></trd>
    <trd lng="en">promotion</trd>
    <trdgrp lng="de">
      <trd>Werbung</trd>,
      <trd>F&ouml;rderung</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">promotion</trd>
  <trdgrp lng="ja">
    <trd>&#26119;&#36914;  <pr>&#12375;&#12423;&#12358;&#12375;&#12435;</pr></trd>,
    <trd>&#26119;&#26684;  <pr>&#12375;&#12423;&#12358;&#12363;&#12367;</pr></trd>,
    <trd>&#26119;&#32026;  <pr>&#12375;&#12423;&#12358;&#12365;&#12421;&#12358;</pr></trd>,
    <trd>&#36914;&#32026;  <pr>&#12375;&#12435;&#12365;&#12421;&#12358;</pr></trd>,
    <trd>&#25104;&#12426;  <pr>&#12394;&#12426;</pr></trd>,
    <trd>&#12450;&#12483;&#12503;&#12464;&#12524;&#12540;&#12489;</trd>,
    <trd>&#12496;&#12540;&#12472;&#12519;&#12531;&#12450;&#12483;&#12503;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>promocja</trd>,
    <trd>awans</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_i;&c_shch;&c_je;&c_n;&c_n;&c_ja; &c_v; &c_ch;&c_i;&c_n;&c_ib;</trd>,
    <trd>&c_z;&c_v;&c_a;&c_n;&c_n;&c_ib;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21319;&#36801; <pr>sh&emacron;ngqi&amacron;n</pr></trd>,
    <trd>&#21319;&#36983; <pr>sh&emacron;ngqi&amacron;n</pr></trd>,
    <trd>&#25512;&#37559; <pr>tu&#299;xi&amacron;o</pr></trd>,
    <trd>&#25512;&#38144; <pr>tu&#299;xi&amacron;o</pr></trd>,
    <trd>&#20419;&#37559;&#27963;&#21205; <pr>c&ugrave;xi&amacron;ohu&oacute;d&ograve;ng</pr></trd>,
    <trd>&#20419;&#38144;&#27963;&#21160; <pr>c&ugrave;xi&amacron;ohu&oacute;d&ograve;ng</pr></trd>,
    <trd>&#20419;&#37559; <pr>c&ugrave;xi&amacron;o</pr></trd>,
    <trd>&#20419;&#38144; <pr>c&ugrave;xi&amacron;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promoc.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="promoc.0igxi.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="rang.pli0igxi.POL">Plirangi&gcirc;i</ref>:
      <ekz>
        <nom>Barks</nom> <klr>[&#8230;]</klr> per konsciencaj laboroj <tld/>i&gcirc;is
        <fnt>
          <aut>M. Boulton</aut>,
          <vrk><url ref="http://esperanto.org.uk/lbe/arkivo/970/18.html">
          Pli fari ol fanfari</url></vrk>,
          <lok>La Brita Esperantisto, 2011 printempo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><ind>bef&ouml;rdert</ind> werden</trd>,
      <trd>aufsteigen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_n;&c_a;
    &c_s;&c_l;&c_u;&c_zh;&c_b;&c_je;)</klr></trd>,
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_p;&c_a;&c_d;&c_v;&c_y;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</ind> <klr tip="amb">(&c_u;
    &c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="ca">ascendir</trd>
  <trd lng="en">to get promoted</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre promu</trd>,
    <trd>monter en grade</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26119;&#36914;&#12377;&#12427;  <pr>&#12375;&#12423;&#12358;&#12375;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#36914;&#32026;&#12377;&#12427;  <pr>&#12375;&#12435;&#12365;&#12421;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pl">awansowa&cacute;</trd>
  <trdgrp lng="zh">
    <trd>&#21319;&#23448; <pr>sh&emacron;nggu&amacron;n</pr></trd>,
    <trd>&#21319;&#32844; <pr>sh&emacron;ngzh&iacute;</pr></trd>,
    <trd>&#21319;&#32887; <pr>sh&emacron;ngzh&iacute;</pr></trd>,
    <trd>&#26185;&#21319; <pr>j&igrave;nsh&emacron;ng</pr></trd>,
    <trd>&#26187;&#21319; <pr>j&igrave;nsh&emacron;ng</pr></trd>,
    <trd>&#36827;&#36523; <pr>j&igrave;nsh&emacron;n</pr></trd>,
    <trd>&#36914;&#36523; <pr>j&igrave;nsh&emacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promoc.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc mrk="promoc.0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iu, kiu profesie <tld/>as:
      <ekz>
        cent prelatoj laboras kiel <tld/>istoj, kaj kampanjas por eventualaj estontaj
        sanktuloj
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2005/008520.php">Sanktuloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_je;&c_c; &c_u;
  &c_r;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_n;&c_y;&c_h;
  &c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ja;&c_h;</trd>
  <trdgrp lng="ca">
    <trd>publicitari</trd>,
    <trd>publicista</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Werbefachmann</trd>,
    <trd>Promoter</trd>
  </trdgrp>
  <trd lng="pl">specjalista ds. <ind>promocji</ind></trd>
</drv>

<drv mrk="promoc.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="promoc.mal0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Malaltigo en hierar&hcirc;io, senigo je alta grado, ofico;
      malplirangigo:
      <ekz>
        mi akceptos mian mal<tld/>on sen plendo
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
      <ekz>
        &scirc;an&gcirc;i preferatan futbalteamon post unu mizera sezono, post malvenko a&ubreve;
        mal<tld/>i&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2010/010098.php">Paradoksa aserto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">demotion</trd>
    <trdgrp lng="de">
      <trd>Degradierung</trd>,
      <trd>Abstieg</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#38477;&#26684; <pr>ji&agrave;ngg&eacute;</pr></trd>,
    <trd>&#38477;&#32844; <pr>ji&agrave;ngzh&iacute;</pr></trd>,
    <trd>&#38477;&#32887; <pr>ji&agrave;ngzh&iacute;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_ib;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_u;
  &c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trd lng="ca">degradaci&oacute;<klr tip="amb"> (cessament, remoci&oacute;)</klr></trd>
  <trdgrp lng="fr">
    <trd>d&eacute;gradation<klr> (perte de rang)</klr></trd>,
    <trd>destitution</trd>
  </trdgrp>
  <trd lng="pl">degradacja</trd>
</drv>
</art>
