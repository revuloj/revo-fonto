<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mangan</rad>/o
</kap>
<drv mrk="mangan.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <mlg kod="IUPAC">Mn</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="25">elemento</ref> kun
      atomnumero 25, atompezo 54,9380; grize blanka
      transir<ref tip="super" cel="metal.0o">metalo</ref> malmola kaj 
      rompi&gcirc;ema.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_g;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="br">manganez</trd>
  <trd lng="ca">mangan&eacute;s</trd>
  <trd lng="de">Mangan</trd>
  <trd lng="en">manganese</trd>
  <trd lng="es">manganeso</trd>
  <trd lng="fr">mangan&egrave;se</trd>
  <trd lng="hu">mang&aacute;n</trd>
  <trd lng="it">manganese</trd>
  <trd lng="la">Manganum</trd>
  <trd lng="nl">mangaan</trd>
  <trd lng="pl">mangan</trd>
  <trd lng="pt">mangan&ecirc;s</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_g;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="sv">mangan</trd>
</drv>
</art>
