<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mangan.xml,v 1.24 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mangan</rad>/o
</kap>
<drv mrk="mangan.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <mlg kod="IUPAC">Mn</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="25">elemento</ref> kun
      atomnumero 25, atompezo 54,9380; grize blanka
      transir<ref tip="super" cel="metal.0o">metalo</ref> malmola kaj 
      rompi&gcirc;ema:
      <ekz>
        oni uzas <tld/>on en la produktado de <ref tip="malprt" cel="sxtal.0o">&scirc;talo</ref>
        kaj aliaj alojoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22344">Mangano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_g;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_m;&c_a;&c_n;&c_g;&c_a;&c_n;</trd>
  <trd lng="br">manganez</trd>
  <trd lng="ca">mangan&eacute;s</trd>
  <trd lng="de">Mangan</trd>
  <trd lng="en">manganese</trd>
  <trd lng="es">manganeso</trd>
  <trd lng="fr">mangan&egrave;se</trd>
  <trd lng="hu">mang&aacute;n</trd>
  <trd lng="id">mangan</trd>
  <trd lng="it">manganese</trd>
  <trd lng="ja">&#x30DE;&#x30F3;&#x30AC;&#x30F3;</trd>
  <trd lng="la">Manganum</trd>
  <trd lng="nl">mangaan</trd>
  <trd lng="pl">mangan</trd>
  <trd lng="pt">mangan&ecirc;s</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_g;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="sv">mangan</trd>
  <trd lng="uk">&#x43C;&#x430;&#x43D;&#x491;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x9333; <pr>m&#x11B;ng</pr></trd>,
    <trd>&#x9530; <pr>m&#x11B;ng</pr></trd>
  </trdgrp>
</drv>
</art>
