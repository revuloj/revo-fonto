    <trd>&#x90E1;&#x4FDD;&#x5B89;&#x5B98;  [<ind>&#x3050;&#x3093;&#x307B;&#x3042;&#x3093;&#x304B;&#x3093;</ind>]</trd>,
    <trd>&#x30B7;&#x30A7;&#x30EA;&#x30D5;</trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&c_je;&c_r;&c_i;&c_f;</trd>
</drv>
