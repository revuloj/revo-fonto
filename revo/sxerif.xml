          <aut>G. R. Ledon</aut>
          <vrk><url ref="http://www.fondation-besnard.org/IMG/pdf/sennaciece_tra_nia_mondo-2.pdf">Sennaciece
          tra nia Mondo</url></vrk>
          <lok>1989</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_r;&c_y;&c_f;</trd>
  <trd lng="en">sheriff</trd>
  <trd lng="fr">sh&eacute;rif</trd>
  <trdgrp lng="ja">
