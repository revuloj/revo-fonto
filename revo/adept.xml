<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc><rad>adept</rad>/o
</kap>
<drv mrk="adept.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo akceptinta doktrinon kaj interesi&gcirc;anta pri &gcirc;i.
    </dif>
    <trdgrp lng="sk">
       <trd>pr&iacute;vr&zcaron;enec</trd>,
       <trd>st&uacute;penec</trd>,
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Homo lerninta arton a&ubreve; sciencon kaj okupi&gcirc;anta pri &gcirc;i:
      <ekz>
        la <tld/>oj de Esperanto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="an">ano</ref>,
      <ref cel="al">ali&gcirc;into</ref>,
      <ref cel="discxi.0o">dis&ccirc;iplo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>follower</trd>,
    <trd>adept</trd>
  </trdgrp>
  <trd lng="es">adepto</trd>
  <trd lng="de">Anh&auml;nger</trd>
  <trd lng="fr">adepte</trd>
  <trd lng="it">adepto</trd>
  <trd lng="nl">aanhanger</trd>
  <trdgrp lng="pt">
    <trd>adepto</trd>,
    <trd>partid&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_je;&c_p;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">adept</trd>
</drv>

<drv mrk="adept.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>o al io:
      <ekz>
        mi <tld/>igis mian amikon al nia grupo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="varb.0i">varbi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>recruit</trd>,
    <trd>indoctrinate</trd>
  </trdgrp>
  <trd lng="es">reclutar</trd>
  <trd lng="de">werben</trd>
  <trd lng="fr">enr&ocirc;ler</trd>
  <trdgrp lng="nl">
    <trd>werven</trd>,
    <trd>recruteren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>filiar</trd>,
    <trd>recrutar</trd>
  </trdgrp>
</drv>
</art>
