<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>adept</rad>/o
</kap>
<drv mrk="adept.0o">
   <kap><tld/>o</kap>
   <snc num="1">
     <dif>
       Homo akceptinta doktrinon kaj interesi&gcirc;anta pri &gcirc;i.
     </dif>
   </snc>
   <snc num="2">
     <dif>
       Homo lerninta arton a&ubreve; sciencon kaj okupi&gcirc;anta pri
       &gcirc;i:
       <ekz>
         la <tld/>oj de Esperanto.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="an">ano</ref>,
       <ref cel="al">ali&gcirc;into</ref>,
       <ref cel="discxi.0o">dis&ccirc;iplo</ref>.
     </refgrp>
   </snc>
   <trd lng="en">adept</trd>
   <trd lng="de">Anh&auml;nger</trd>
   <trd lng="fr">adepte</trd>
   <trd lng="it">adepto</trd>
</drv>
<drv mrk="adept.0igi">
   <kap><tld/>igi</kap>
   <snc>
     <dif>
       Igi iun <tld/>o al io:
       <ekz>
         mi <tld/>igis mian amikon al nia grupo.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="varb.0i">varbi</ref>.
     </refgrp>
   </snc>
   <trd lng="en">recruit</trd>
   <trd lng="de">werben</trd>
   <trd lng="fr">enr&ocirc;ler</trd>
</drv>
</art>
