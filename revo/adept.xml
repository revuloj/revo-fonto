<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adept.xml,v 1.26 2021/02/28 09:26:17 revo Exp $">
<kap>
  <ofc>1</ofc><rad>adept</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="adept.0o">
  <kap><tld/>o</kap>
  <snc mrk="adept.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Homo akceptinta doktrinon kaj interesi&gcirc;anta pri &gcirc;i:
      <ekz>
        la sonoriloj de la pola pre&gcirc;ejo invitis
        la <tld/>ojn al la noktomeza meso
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>, 
        <lok>1933</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">seguidor</trd>
    <trdgrp lng="hu">
      <trd>h&iacute;v</trd>,
      <trd>h&iacute;ve (vmnek)</trd>
    </trdgrp>
    <trd lng="pl">zwolennik</trd>
    <trdgrp lng="sk">
       <trd>pr&iacute;vr&zcaron;enec</trd>,
       <trd>st&uacute;penec</trd>,
    </trdgrp>
  </snc>
  <snc mrk="adept.0o.TEK">
    <dif>
      Homo lerninta arton a&ubreve; sciencon kaj okupi&gcirc;anta pri 
      &gcirc;i:
      <ekz>
        la <tld/>oj de Esperanto.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>instru&iuml;t</trd>,
      <trd>coneixedor</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;vet&odblac; (pl.: vall&aacute;snak)</trd>,
      <trd>k&ouml;vet&odblac;je (pl.: vall&aacute;snak)</trd>
    </trdgrp>
    <trd lng="pl">adept</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="an.0o">ano</ref>,
    <ref cel="al.0igxi">ali&gcirc;into</ref>,
    <ref cel="discxi.0o">dis&ccirc;iplo</ref>,
    <ref cel="zelot.0o">zeloto</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_d;&c_o;&c_w;&c_n;&c_ib; &c_k;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">adepte</trd>
  <trdgrp lng="en">
    <trd>follower</trd>,
    <trd>adept</trd>
  </trdgrp>
  <trd lng="es">adepto</trd>
  <trd lng="de">Anh&auml;nger</trd>
  <trdgrp lng="fr">
    <trd>adepte</trd>,
    <trd>fid&egrave;le<klr tip="ind"> (subst.)</klr></trd>,
    <trd>supporter<klr tip="ind"> (qui soutient, encourage)</klr></trd>,
    <trd>supporteur</trd>,
    <trd>tenant<klr tip="ind"> (d'une doctrine)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5E2;&#x5E8;&#x5D9;&#x5E5;</trd>,
    <trd>&#x5D0;&#x5D5;&#x5D4;&#x5D3;</trd>
  </trdgrp>
  <trd lng="it">adepto</trd>
  <trd lng="nl">aanhanger</trd>
  <trdgrp lng="pt">
    <trd>adepto</trd>,
    <trd>partid&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_je;&c_c;
    </trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;
    &c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_je;&c_p;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">adept</trd>
  <trdgrp lng="cs">
    <trd>adept</trd>,
    <trd>p&rcaron;&iacute;vr&zcaron;enec</trd>,
    <trd>stoupenec</trd>,
    <trd>&ccaron;ekatel</trd>
  </trdgrp>
</drv>

<drv mrk="adept.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>o al io:
      <ekz>
        mi <tld/>igis mian amikon al nia grupo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="varb.0i">varbi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>afiliar</trd>,
    <trd>allistar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zasv&ecaron;tit</trd>,
    <trd>z&iacute;skat za &ccaron;lena</trd>
  </trdgrp>
  <trd lng="de">werben</trd>
  <trdgrp lng="en">
    <trd>recruit</trd>,
    <trd>indoctrinate</trd>
  </trdgrp>
  <trd lng="es">reclutar</trd>
  <trdgrp lng="fr">
    <trd>initier<klr tip="ind"> (instruire dans une
    doctrine)</klr></trd>,
    <trd>enr&ocirc;ler<klr tip="ind"> (dans une doctrine)</klr></trd>,
    <trd>recruter<klr tip="ind"> (dans une doctrine)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>werven</trd>,
    <trd>recruteren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pozyskiwa&cacute;<klr> kogo&sacute; na zwolennika</klr></trd>,
    <trd>wprowadza&cacute;<klr> do grupy</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>filiar</trd>,
    <trd>recrutar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pr&iacute;vr&zcaron;enca</trd>,
    <trd>z&iacute;ska&tcaron; za st&uacute;penca</trd>
  </trdgrp>
</drv>
</art>
