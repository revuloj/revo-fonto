<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>podagr</rad>/o
</kap>
<drv mrk="podagr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Dolora <ref tip="super" cel="san.mal0o">malsano</ref> de artikoj:
      <ekz>
        homo &ccirc;irka&ubreve; kvindekjara, jam tute griza kaj kvaza&ubreve; suferanta la
        <tld/>on
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 28a, p. 208a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_g;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Podagra</trd>,
    <trd>Fu&szlig;gicht</trd>,
    <trd>Zehengicht</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>goutte aux pieds<klr> (podagre)</klr></trd>,
    <trd>podagre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>podagra</trd>,
    <trd>l&aacute;bk&ouml;szv&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>podagra</trd>,
    <trd>jicht</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_a;&c_g;&c_r;&c_a;</trd>
</drv>
</art>
