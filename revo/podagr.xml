<art mrk="$Id: podagr.xml,v 1.25 2024/02/17 07:52:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>podagr</rad>/o
</kap>
<drv mrk="podagr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Dolora <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref> de artikoj:
      <ekz>
        homo &ccirc;irka&ubreve; kvindekjara, jam tute griza kaj kvaza&ubreve; suferanta
        la
        <tld/>on
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 28a, p.
        208a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_g;&c_r;&c_a;</trd>
  <trd lng="cs">podagra</trd>
  <trdgrp lng="de">
    <trd>Podagra</trd>,
    <trd>Fu&szlig;gicht</trd>,
    <trd>Zehengicht</trd>
  </trdgrp>
  <trd lng="en">gout</trd>
  <trd lng="fa">&a_n;&a_q;&a_r;&a_s;</trd>
  <trdgrp lng="fr">
    <trd>goutte<klr tip="amb"> (maladie)</klr></trd>,
    <trd>podagre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>podagra</trd>,
    <trd>l&aacute;bk&ouml;szv&eacute;ny</trd>
  </trdgrp>
  <trd lng="id">pirai</trd>
  <trd lng="ja">&#x75DB;&#x98A8;  <pr>&#x3064;&#x3046;&#x3075;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>podagra</trd>,
    <trd>jicht</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podagra</trd>,
    <trd>dna moczanowa</trd>
  </trdgrp>
  <trd lng="pt">gota <klr>(med.)</klr></trd>
  <trdgrp lng="ro">
    <trd>gut&abreve;</trd>,
    <trd>podagr&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_a;&c_g;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dna</trd>,
    <trd>l&#xE1;mka</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&c_d;&c_a;&c_g;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x75DB;&#x98A8; <pr>t&#xF2;ngf&#x113;ng</pr></trd>,
    <trd>&#x75DB;&#x98CE; <pr>t&#xF2;ngf&#x113;ng</pr></trd>
  </trdgrp>
</drv>
</art>
