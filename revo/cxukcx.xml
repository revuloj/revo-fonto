<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>&ccirc;uk&ccirc;</rad>/o
      <fnt>
        <aut>V. Ero&scirc;enko</aut>: <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>.
              <lok><url
                ref="http://www.esperanto.mv.ru/Cxukcxoj/">Esperanta
            Ligilo, 1933.</url></lok>
      </fnt>
      <fnt><aut>T. Sekelj</aut>:
        <vrk>Mondmapo, a&ubreve; Nepalo malfermas la pordon</vrk>, <lok>1958</lok>
      </fnt>
      <fnt><vrk>Po&scirc;atlaso de la Mondo</vrk>, <lok>1971</lok></fnt>
    </kap>
    <drv mrk="cxukcx.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Ano de indi&gcirc;ena popolo de la ekstrema nord-oriento de Azio
          (<ref tip="malprt" cel="cxukcx.Cx0aDuoninsulo"><tld lit="&Ccirc;"/>a
            Duoninsulo</ref>):
          <ekz>
            &ccirc;iu virino devas kiel ajn havi almena&ubreve; unu infanon, &ccirc;iu
            <tld/>ino scias tion
            <fnt>
              <aut>V. Ero&scirc;enko</aut>: <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>.
              <lok><url
                ref="http://www.esperanto.mv.ru/Cxukcxoj/cxukcx2.html">La
                  destinita vivi</url></lok>.</fnt>.
          </ekz>
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="azi.A0o.orienta">Orienta Azio</ref>
      </refgrp>
      <trd lng="de">Tschuktsche</trd>
      <trd lng="en">Chukchi man</trd>
      <trdgrp lng="fr">
        <trd>Tchouktche</trd>,
        <trd>Chukch</trd></trdgrp>
      <trd lng="ru">&c_ch;&c_u;&c_k;&c_ch;&c_a;</trd>
    </drv>
    <drv mrk="cxukcx.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al la <tld/>oj, ilia vivmaniero kaj lo&gcirc;loko:
          <ekz>
            la&ubreve; nia <tld/>a kutimo, niaj maljunuloj rajtas siavole
            forlasi tiun &ccirc;i vivon, se &gcirc;i i&gcirc;as por ili malfacila,
            dolora, se ili sentas, ke &gcirc;i i&gcirc;as por ili &scirc;ar&gcirc;o ne plu
            portebla, ke ili i&gcirc;as &scirc;ar&gcirc;o por siaj parencoj kaj amikoj
            kaj por si mem
            <fnt>
              <aut>V. Ero&scirc;enko</aut>: <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>.
              <lok><url
                ref="http://www.esperanto.mv.ru/Cxukcxoj/cxukcx3.html">La
                  trimova &scirc;akproblemo</url>,
              unue en </lok>.</fnt>;
          </ekz><ekz>
            <uzo tip="fak">LIN</uzo>
            la <ind><tld/>a lingvo</ind>;
            <!--(unu el <ref tip="malprt" cel="siberi.pra0alingvo">prasiberiaj
            lingvoj</ref>)-->
            <trdgrp lng="ru">
              <trd>&c_ch;&c_u;&c_k;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>,
              <trd>&c_l;&c_u;&c_o;&c_r;&c_a;&c_v;&c_je;&c_t;&c_l;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd></trdgrp>
          </ekz><ekz>
            <uzo tip="fak">GEOG</uzo>
            <tld lit="&Ccirc;"/>a maro, duoninsulo.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_ch;&c_u;&c_k;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
    <drv mrk="cxukcx.Cx0aDuoninsulo">
      <kap><tld lit="&Ccirc;"/>a Duoninsulo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Duoninsulo formanta la nord-orientan ekstremon de
          <ref tip="malprt" cel="azi.A0o">Azio</ref>, inter
          la <tld lit="&Ccirc;"/>a Maro norde kaj
          <ref cel="bering.0aMaro">Beringa Maro</ref> oriente kaj sude.
        </dif>
      </snc>
      <bld lok="../bld/Beringa.png">La <tld lit="&Ccirc;"/>a Duoninsulo en Azio</bld>
      <trd lng="de">Tschuktschen-Halbinsel</trd>
      <trd lng="en">Chukot(ski) Peninsula</trd>
      <trdgrp lng="fr">
        <trd>Tchoukotka</trd>,
        <trd>Chukotka</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&c_Ch;&c_u;&c_k;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j; &c_p;&c_o;&c_l;&c_u;&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>,
        <trd>&c_Ch;&c_u;&c_k;&c_o;&c_t;&c_k;&c_a;</trd></trdgrp>
    </drv>
    <drv mrk="cxukcx.Cx0aMaro">
      <kap><tld lit="&Ccirc;"/>a Maro</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Periferia maro en la <ref tip="malprt"
 cel="arkt.A0aOceano">Arkta Oceano</ref>, lavanta la bordojn
          de la <ref cel="cxukcx.Cx0aDuoninsulo"><tld lit="&Ccirc;"/>a
            Duoninsulo</ref> kaj <ref cel="alask.0o">Alasko</ref>,
          komuniki&gcirc;anta kun la Beringa Maro per la
          <ref cel="bering.0aMarkolo">Beringa Markolo</ref>.
        </dif>
      </snc>
      <trd lng="de">Tschuktschensee</trd>
      <trd lng="en">Chukchee Sea</trd>
      <trd lng="fr">Mer des Tchouktches</trd>
      <trd lng="ru">&c_Ch;&c_u;&c_k;&c_o;&c_t;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
    </drv>
  </art>

