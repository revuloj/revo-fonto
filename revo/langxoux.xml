<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: langxoux.xml,v 1.5 2023/10/18 06:31:26 afido Exp $">
    <kap>
      <rad>Lan&gcirc;o&ubreve;</rad>/o
    </kap>

<drv mrk="langxoux.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>, &ccirc;e la <ref cel="river.FlavaR0o">Flava
      Rivero</ref>,
      &ccirc;. 600&#x202F;km norde de <ref cel="cxengdu.0o">&Ccirc;engduo</ref>:
      <ekz>
        dum la tempo de Han-dinastio kaj Tang-dinastio, <tld/>o estis grava pasejo sur la
        antikva Silka Vojo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Lan%C4%9Do%C5%ADo">Lan&gcirc;o&ubreve;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Harbino, Nan&ccirc;ango, <tld/>o kaj kelkaj aliaj urboj lan&ccirc;is lokajn regularojn
        <klr>[por]</klr> malpermesi fumadon en publikaj lokoj
        <fnt>
          <vrk><url ref="&CRI;581/2011/05/29/1s124622.htm">Rapidi&gcirc;is le&gcirc;dona precezo pri tabaka
          regado en &Ccirc;inio</url></vrk>
          <lok>CRI online</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_L;&c_a;&c_n;&c_mol;&c_d;&c_zh;&c_o;&c_w;</trd>
    <trd lng="de">Lanzhou</trd>
    <trd lng="en">Lanzhou</trd>
    <trd lng="fr">Lanzhou</trd>
    <trd lng="zh">&#20848;&#x5DDE;&#x5E02;</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x41B;&c_a;&#x43D;&#x44C;&c_ch;&c_zh;&#x43E;&c_u;</trd>,
    <trd>&c_G;&c_a;&#x43E;&#x43B;&c_a;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>

  </art>

