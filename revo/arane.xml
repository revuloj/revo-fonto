<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>arane</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="arane.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="arane.0o.ordo">
    <dif>
      Besteto, kiu teksas reton el tre maldikaj,
      parte gluecaj fadenoj,
      per kiu &gcirc;i kaptas insektojn kaj ceterajn;
      el la ordo <ref tip="malprt"
      cel="arane.0uloj"><tld/>uloj</ref>.
    </dif>
    <trd lng="de">Spinne</trd>
    <trd lng="sv">spindel</trd>
    <trd lng="nl">spin</trd>
  </snc>
  <snc mrk="arane.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="aranee.0oj"><tld/>edoj</ref>
      (<trd lng="la">Aranea</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Radnetzspinne</trd>,
      <trd>Kreuzspinne</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_u;&c_k;</trd>
  <trd lng="bg">&c_p;&c_a;&c_ja;&c_k;</trd>
  <trd lng="br">kevnidenn</trd>
  <trd lng="de">Spinne</trd>
  <trd lng="en">spider</trd>
  <trd lng="es">ara&ntilde;a</trd>
  <trd lng="fr">araign&eacute;e</trd>
  <trd lng="he">&ayin;&kaf;&bet;&yod;&shin;</trd>
  <trd lng="hu">p&oacute;k</trd>
  <trd lng="pl">paj&aogonek;k</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;</trd>
  <trd lng="sk">pav&uacute;k</trd>
  <trd lng="tp">pipi</trd>
  <trd lng="tr">&ouml;r&uuml;mcek</trd>
</drv>

<drv mrk="arane.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc mrk="arane.0ajxo.ZOO">
    <dif>
      <tld lit="A"/>a reto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_u;&c_c;&c_ib;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_u;&c_c;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">gwiad-kevnid</trd>
  <trd lng="bg">&c_p;&c_a;&c_ja;&c_zh;&c_i;&c_n;&c_a;</trd>
  <trd lng="de">Spinnennetz</trd>
  <trd lng="en">spiderweb</trd>
  <trd lng="fr">toile d'araign&eacute;e</trd>
  <trd lng="he">&qof;&vav;&resh;&yod; &ayin;&kaf;&bet;&yod;&shin;</trd>
  <trd lng="hu">p&oacute;kh&aacute;l&oacute;</trd>
  <trd lng="nl">spinnenweb</trd>
  <trd lng="pl">paj&eogonek;czyna</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">pavu&ccaron;ina</trd>
  <trd lng="sv">spindelv&auml;v</trd>
</drv>

<drv mrk="arane.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="arane.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj kun kvar kruroparoj, kapablaj
      sekrecii fadenon;
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">ordo</ref> 
      el la filumo <ref tip="malprt"
      cel="artik.0uloj.ZOO">artikuloj</ref>
      (<trd lng="la">Araneae</trd>).
    </dif>
  </snc>
  <trd lng="br">araknided</trd>
  <trd lng="de">Webespinnen</trd>
  <trd lng="fr">arachnides</trd>
  <trd lng="hu">igazi <ind>p&oacute;kok</ind></trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;&c_i; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;
  &c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_h;)</klr></trd>
</drv>

<drv mrk="arane.lup0o">
  <kap>lup<tld/>o</kap>
  <snc mrk="arane.lup0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio
      <ref tip="super" cel="lupara.0oj">lup<tld/>edoj</ref>
      (<trd lng="la">Lycosa</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
  <trd lng="de">Wolfsspinne</trd>
  <trd lng="fr">araign&eacute;e-loup</trd>
  <trd lng="hu">farkasp&oacute;k</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
</drv>

<drv mrk="arane.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="arane.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda dorna krabo kun longaj kruroj, &scirc;ate man&gcirc;ata en iuj
      landoj, 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="majed.0oj">majedoj</ref> (<trd
      lng="la">Maja</trd>):
      <ekz>
        - Belegaj estas &ccirc;i tiuj soleoj!...
        - Bongustegaj estas &ccirc;i tiuj mar<tld/>oj!
        <fnt>
          <aut>C. Collodi, trad. G. Croatini</aut>,
          <vrk><url
          ref="http://digilander.libero.it/nicolaruggiero/pinokjo/">
          La aventuroj de Pinokjo</url></vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dreieckskrabbe</trd>,
    <trd>Seespinne</trd>
  </trdgrp>
  <trd lng="en">spider crab</trd>
  <trd lng="fr">araign&eacute;e de mer</trd>
</drv>
</art>
