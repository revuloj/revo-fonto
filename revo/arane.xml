<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arane.xml,v 1.45 2021/05/29 14:32:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>arane</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="arane.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="arane.0o.ordo">
    <dif>
      Iu el la ordo <ref tip="malprt" cel="arane.0uloj">araneuloj</ref>:
      <ekz>
        malgranda <tld/>o, kiun li ne povis vidi, <klr>[&#x2026;]</klr> &scirc;pinadis super li sian
        teksa&jcirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>  
      <ekz>
        abomena, kiel <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz> 
      <ekz>
        kiam iu ne havas tre sanan menson, oni diras, ke tiu havas <tld/>on en la cerbo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006916.html">Bugra bug-insekto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Spinne</trd>
    <trd lng="sv">spindel</trd>
    <trd lng="nl">spin</trd>
  </snc>
  <snc mrk="arane.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="aranee.0oj"><tld/>edoj</ref>
      (<trd lng="la">Araneus</trd>), kun makuloj surdorse, kiuj aspektas &ccirc;e kelkaj specioj krucforme.
    </dif>
    <bld lok="&WCU;/thumb/6/64/P1077171_Araneas_diadematus.jpg/320px-P1077171_Araneas_diadematus.jpg" prm="CC BY-SA 4.0"><tld/>o en sia reto (Araneus diadematus)
        <fnt>
          <aut>Tsugakuh, propra verko</aut>
          <lok><url ref="&WCW;/File:P1077171_Araneas_diadematus.jpg">https://commons.wikimedia.org/wiki/File:P1077171_Araneas_diadematus.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Radnetzspinne</trd>,
      <trd>Kreuzspinne</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_u;&c_k;</trd>
  <trd lng="bg">&c_p;&c_a;&c_ja;&c_k;</trd>
  <trd lng="bo">&#xF66;&#xFA1;&#xF7C;&#xF58;&#xF0B;</trd>
  <trd lng="br">kevnidenn</trd>
  <trd lng="cs">pavouk</trd>
  <trd lng="de">Spinne</trd>
  <trd lng="en">spider</trd>
  <trd lng="es">ara&ntilde;a</trd>
  <trd lng="fr">araign&eacute;e</trd>
  <trd lng="he">&ayin;&kaf;&bet;&yod;&shin;</trd>
  <trd lng="hu">p&oacute;k</trd>
  <trd lng="id">laba-laba</trd>
  <trd lng="pl">paj&aogonek;k</trd>
  <trd lng="pt">aranha</trd>
  <trd lng="ro">p&abreve;ianjen</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;</trd>
  <trd lng="sk">pav&uacute;k</trd>
  <trd lng="tp">pipi</trd>
  <trd lng="tr">&ouml;r&uuml;mcek</trd>
</drv>

<drv mrk="arane.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="arane.0ajxo.ZOO">
    <dif>
      <tld lit="A"/>a <ref tip="super" cel="ret.0o.TEKS">reto</ref>:
      <ekz>
        &scirc;tupo, sur kiu <tld/>oj &scirc;pinis densan <tld/>a&jcirc;on kaj &ccirc;irka&ubreve;volvis la mu&scirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>   
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_u;&c_c;&c_ib;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_u;&c_c;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">gwiad-kevnid</trd>
  <trd lng="bg">&c_p;&c_a;&c_ja;&c_zh;&c_i;&c_n;&c_a;</trd>
  <trd lng="cs">pavu&ccaron;ina</trd>
  <trd lng="de">Spinnennetz</trd>
  <trd lng="en">spiderweb</trd>
  <trd lng="es">telara&ntilde;a</trd>
  <trd lng="fr">toile d'araign&eacute;e</trd>
  <trd lng="he">&qof;&vav;&resh;&yod; &ayin;&kaf;&bet;&yod;&shin;</trd>
  <trd lng="hu">p&oacute;kh&aacute;l&oacute;</trd>
  <trd lng="id"><ind>jaring</ind> laba-laba</trd>
  <trd lng="nl">spinnenweb</trd>
  <trd lng="pl">paj&eogonek;czyna</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">pavu&ccaron;ina</trd>
  <trd lng="sv">spindelv&auml;v</trd>
</drv>

<drv mrk="arane.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="arane.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj kun kvar kruroparoj, kapablaj
      sekrecii fadenon;
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">ordo</ref> 
      el la filumo <ref tip="malprt" cel="artik.0uloj.ZOO">artikuloj</ref>
      (<trd lng="la">Araneae</trd>).
    </dif>
  </snc>
  <trd lng="br">araknided</trd>
  <trd lng="de">Webespinnen</trd>
  <trd lng="es">ar&aacute;cnidos</trd>
  <trd lng="fr">arachnides</trd>
  <trd lng="hu">igazi <ind>p&oacute;kok</ind></trd>
  <trd lng="pl">paj&aogonek;ki</trd>
  <trd lng="ro">p&abreve;ianjenii</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;&c_i; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;
  &c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_h;)</klr></trd>
</drv>

<drv mrk="arane.lup0o">
  <kap>lup<tld/>o</kap>
  <snc mrk="arane.lup0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el araneuloj
      (<trd lng="la">Lycosa</trd>), &gcirc;is 35&#x202F;mm grandaj, kiuj 
      embuskas kaj &ccirc;asas insektojn, anka&ubreve; nomata 
      <ref tip="sub" cel="tarant1.0o.luparaneo">tarantulo</ref>:
      <ekz>
        lup<tld/>oj povas vivi &gcirc;is 11 jaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=570619">Luparaneo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/63/Lycosa_sp.JPG/320px-Lycosa_sp.JPG" prm="CC-BY-SA-3.0">iu el genro lup<tld/>o
        <fnt>
          <aut>G.Egebrecht</aut>
          <lok><url ref="&WCW;/File:Lycosa_sp.JPG">https://commons.wikimedia.org/wiki/File:Lycosa_sp.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
  <trd lng="cs">tarantule</trd>
  <trd lng="de">Wolfsspinne</trd>
  <trd lng="es">tar&aacute;ntula</trd>
  <trd lng="fr">araign&eacute;e-loup</trd>
  <trd lng="hu">farkasp&oacute;k</trd>
  <trd lng="pl">tarantula</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
  <trd lng="sk">tarantula</trd>
</drv>

<drv mrk="arane.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="arane.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda dorna <ref tip="super" cel="krab.0o">krabo</ref> kun longaj kruroj, &scirc;ate man&gcirc;ata en iuj
      landoj, 
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio majedoj (<trd lng="la">Maja</trd>):
      <ekz>
        &dash; Belegaj estas &ccirc;i tiuj soleoj!...
        &dash; Bongustegaj estas &ccirc;i tiuj mar<tld/>oj!
        <fnt>
          <aut>C. Collodi, trad. G. Croatini</aut>,
          <vrk><url ref="http://digilander.libero.it/nicolaruggiero/pinokjo/">
          La aventuroj de Pinokjo</url></vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dreieckskrabbe</trd>,
    <trd>Seespinne</trd>
  </trdgrp>
  <trd lng="en">spider crab</trd>
  <trd lng="es">ara&ntilde;a de mar</trd>
  <trd lng="fr">araign&eacute;e de mer</trd>
  <trd lng="pl">paj&aogonek;k morski</trd>
  <trd lng="ro">p&abreve;ianjen de mare</trd>
</drv>
</art>
