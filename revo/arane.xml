<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>arane</rad>/o
</kap>
<drv mrk="arane.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="arane.0o.ordo">
    <dif>
      Iu el la ordo <ref tip="malprt" cel="arane.0uloj"><tld/>uloj</ref>,
      kiu teksas reton el tre maldikaj, parte gluecaj fadenoj,
      per kiu &gcirc;i kaptas insektojn kaj ceterajn.
    </dif>
    <trd lng="de">Spinne</trd>
    <trd lng="sv">spindel</trd>
    <trd lng="nl">spin</trd>
  </snc>
  <snc mrk="arane.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super"
      cel="aranee.0oj"><tld/>edoj</ref>
      (<trd lng="la">Aranea</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Radnetzspinne</trd>,
      <trd>Kreuzspinne</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_u;&c_k;</trd>
  <trd lng="es">ara&ntilde;a</trd>
  <trd lng="fr">araign&eacute;e</trd>
  <trd lng="he">&ayin;&kaf;&bet;&yod;&shin;</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;</trd>
  <trd lng="tr">&ouml;r&uuml;mcek</trd>
</drv>

<drv mrk="arane.0ajxo">
  <kap>
    <tld/>a&jcirc;o<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      <tld/>a reto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_u;&c_c;&c_ib;&c_n;&c_mol;&c_n;&c_je;</trd>, 
    <trd>&c_p;&c_a;&c_v;&c_u;&c_c;&c_ib;&c_n;&c_a;</trd> 
  </trdgrp>  
  <trd lng="nl">spinnenweb</trd>
  <trd lng="de">Spinnennetz</trd>
  <trd lng="fr">toile d'araign&eacute;e</trd>
  <trd lng="he">&qof;&vav;&resh;&yod; &ayin;&kaf;&bet;&yod;&shin;</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">spindelv&auml;v</trd>
</drv>

<drv mrk="arane.0uloj">
  <kap>
    <tld/>uloj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la filumo <ref tip="super"
      cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Araneae</trd>), kun kvar kruroparoj, kapablaj
      sekrecii fadenon.
    </dif>
  </snc>
  <trd lng="de">Webespinnen</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;&c_i; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d; &c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_h;)</klr></trd>
</drv>

<drv mrk="arane.lup0o">
  <kap>
    lup<tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="lupara.0oj">lup<tld/>edoj</ref>
      (<trd lng="la">Lycosa</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
  <trd lng="de">Wolfsspinne</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
</drv>
</art>
