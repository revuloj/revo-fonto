<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>arane</rad>/o
</kap>
<drv mrk="arane.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="arane.0o.ordo">
    <dif>
      Iu el la ordo <ref tip="malprt" cel="arane.0uloj"><tld/>uloj</ref>.
    </dif>
    <trd lng="de">Spinne</trd>
  </snc>
  <snc mrk="arane.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="arane.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Aranea</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Radnetzspinne</trd>,
      <trd>Kreuzspinne</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">araign&eacute;e</trd>
  <trd lng="ru">&#1087;&#1072;&#1091;&#1082;</trd>
  <trd lng="tr">&ouml;r&uuml;mcek</trd>
</drv>
<drv mrk="arane.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld/>a reto.
    </dif>
  </snc>
  <trd lng="de">Spinnennetz</trd>
  <trd lng="fr">toile d'araign&eacute;e</trd>
  <trd lng="ru">&#1087;&#1072;&#1091;&#1090;&#1080;&#1085;&#1072;</trd>
</drv>
<drv mrk="arane.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="arane.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Araneidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Radnetzspinnen</trd>
</drv>
<drv mrk="arane.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la filumo <ref tip="super" cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Araneae</trd>), kun kvar kruroparoj, kapablaj
      sekrecii fadenon.
    </dif>
  </snc>
  <trd lng="de">Webespinnen</trd>
</drv>
<drv mrk="arane.lup0o">
  <kap>lup<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="arane.lup0edoj">lup<tld/>edoj</ref>
      (<trd lng="la">Lycosa</trd>).
    </dif>
  </snc>
  <trd lng="de">Wolfsspinne</trd>
</drv>
<drv mrk="arane.lup0edoj">
  <kap>lup<tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="arane.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Lycosidae</trd>), flavbrunaj &gcirc;is nigraj, kiuj
      kaptas la bredon per rapida kuro.
    </dif>
  </snc>
  <trd lng="de">Wolfsspinnen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
