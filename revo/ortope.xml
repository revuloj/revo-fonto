<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ortope.xml,v 1.25 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>ortopedi</rad>/o <fnt><bib>Fab1</bib></fnt> 
</kap>
<drv mrk="ortope.0o">
  <kap><tld/>o</kap>
  <fnt><bib>ZR</bib></fnt>
  <snc mrk="ortope.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="medici.0o">Medicin</ref>a fako okupi&gcirc;anta pri malsanoj kaj misfunkcioj 
      de la tena kaj mova aparato de la korpo (<ref cel="ost.0o">osto</ref>j, 
      <ref cel="muskol.0o">muskolo</ref>j, <ref cel="tenden.0o">tendeno</ref>j kc):
      <ekz>
        artika, osta <tld/>o;
      </ekz>
      <ekz>
        li trovi&gcirc;as en <tld/>a instituto, kiel oni nomas tian domon, en kiu la
        kuracistoj liberigas homojn de iliaj elkreska&jcirc;oj kaj faras ilin denove rektaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        personoj kun la sekvoj de poliomjelito bezonas <tld/>an helpon
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010269.php">Poliomjelito: &ccirc;u la dua tutmonde
          likvidota infektmalsano?</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_a;&c_p;&c_e;&c_d;&c_y;&c_ja;</trd>
  <trd lng="cs">ortopedie</trd>
  <trd lng="de">Orthop&auml;die</trd>
  <trd lng="el">&omikron;&rho;&theta;&omikron;&pi;&alfa;&jota;&delta;&jota;&kappa;&eta_ton;</trd>
  <trdgrp lng="en">
    <trd>orthopedics</trd>,
    <trd>orthop&aelig;dics</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n;&zwnj;&f_p;&a_z;&a_sh;&f_k;&f_y;</trd>,
    <trd>&a_A;&a_damma;&a_r;&a_t;&a_w;&f_p;&a_d;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">orthop&eacute;die</trd>
  <trd lng="hu">ortop&eacute;dia</trd>
  <trd lng="ja">&#x6574;&#x5F62;&#x5916;&#x79D1;  <pr>&#x305B;&#x3044;&#x3051;&#x3044;&#x3052;&#x304B;</pr></trd>
  <trd lng="nl">orthopedie</trd>
  <trd lng="pl">ortopedia</trd>
  <trd lng="pt">ortopedia</trd>
  <trd lng="ro">ortopedie</trd>
  <trd lng="ru">&c_o;&c_r;&c_t;&c_o;&c_p;&c_je;&c_d;&c_i;&c_ja;</trd>
  <trd lng="sk">ortop&eacute;dia</trd>
  <trd lng="uk">&#x43E;&c_r;&c_t;&#x43E;&#x43F;&c_je;&c_d;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x9AA8;&#x79D1; <pr>g&#x1D4;k&#x113;</pr></trd>,
    <trd>&#x6574;&#x5F62;&#x5916;&#x79D1; <pr>zh&#x11B;ngx&#xED;ngw&#xE0;ik&#x113;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ortope.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ortope.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="fak.0ulo">Fakulo</ref> pri <tld/>o:
      <ekz>
        <tld/>isto nomata <nom>Sch&ouml;nborn</nom> pensis esti devigata en la &ccirc;eesto
        de <nom>R&ouml;ntgen</nom> averti kontra&ubreve; optimismo
        <fnt>
        <aut>R. K. Goldsmit Jentner, trad. F. Szil&aacute;gyi</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/rontgen.html">R&ouml;ntgen</url></vrk>,
        <lok>Norda Prismo, 1958:2, p. 95a-102a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_a;&c_p;&c_e;&c_d;</trd>
  <trd lng="cs">ortoped</trd>
  <trd lng="de">Orthop&auml;de</trd>
  <trd lng="en">orthopedist</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n;&zwnj;&f_p;&a_z;&a_sh;&f_k;</trd>,
    <trd>&a_A;&a_damma;&a_r;&a_t;&a_w;&f_p;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">orthop&eacute;diste</trd>
  <trd lng="ja">&#x6574;&#x5F62;&#x5916;&#x79D1;&#x533B;  <pr>&#x305B;&#x3044;&#x3051;&#x3044;&#x3052;&#x304B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>orthopedist</trd>,
    <trd>orthopeed</trd>
  </trdgrp>
  <trd lng="pl">ortopeda</trd>
  <trd lng="ro">ortoped</trd>
  <trd lng="sk">ortop&eacute;d</trd>
  <trd lng="uk">&#x43E;&c_r;&c_t;&#x43E;&#x43F;&c_je;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x6574;&#x5F62;&#x5916;&#x79D1;&#x533B;&#x5E08; <pr>zh&#x11B;ngx&#xED;ngw&#xE0;ik&#x113;y&#x12B;sh&#x12B;</pr></trd>,
    <trd>&#x6574;&#x5F62;&#x5916;&#x79D1;&#x91AB;&#x5E2B; <pr>zh&#x11B;ngx&#xED;ngw&#xE0;ik&#x113;y&#x12B;sh&#x12B;</pr></trd>,
    <trd>&#x77EB;&#x5F62;&#x533B;&#x751F; <pr>ji&#x1CE;ox&#xED;ngy&#x12B;sh&#x113;ng</pr></trd>,
    <trd>&#x77EF;&#x5F62;&#x91AB;&#x751F; <pr>ji&#x1CE;ox&#xED;ngy&#x12B;sh&#x113;ng</pr></trd>
  </trdgrp>
</drv>
</art>
