<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ortopedi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ortope.0o">
  <kap><tld/>o</kap>
  <fnt><bib>ZR</bib></fnt>
  <snc mrk="ortope.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuraca arto malhelpi a&ubreve; korekti deformi&gcirc;on de korpo:
      <ekz>
        artika, osta <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_a;&c_p;&c_e;&c_d;&c_y;&c_ja;</trd>
  <trd lng="de">Orthop&auml;die</trd>
  <trd lng="el">&omikron;&rho;&theta;&omikron;&pi;&alfa;&jota;&delta;&jota;&kappa;&eta_ton;</trd>
  <trd lng="fr">orthop&eacute;die</trd>
  <trd lng="hu">ortop&eacute;dia</trd>
  <trd lng="nl">orthopedie</trd>
  <trd lng="pt">ortopedia</trd>
  <trd lng="ru">&c_o;&c_r;&c_t;&c_o;&c_p;&c_je;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="ortope.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ortope.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        <tld/>isto nomata Sch&ouml;nborn pensis esti devigata en la &ccirc;eesto
        de R&ouml;ntgen averti kontra&ubreve; optimismo
        <fnt>
        <aut>R. K. Goldsmit Jentner, trad. F. Szil&aacute;gyi</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/rontgen.html">R&ouml;ntgen</url></vrk>,
        <lok>Norda Prismo, 1958:2, p. 95a-102a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">orthop&eacute;diste</trd>
</drv>
</art>
