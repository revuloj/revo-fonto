<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapitan</rad>/o
</kap>
<drv mrk="kapita1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kapita1.0o.MAR">
  <uzo tip="fak">MAR</uzo>
    <ref tip="dif" cel="sxip.0estro">&scirc;ipestro</ref>
    <trd lng="de">Kapit&auml;n</trd>
    <trd lng="en">captain</trd>
    <trd lng="fr">capitaine<klr tip="ind"> (de navire)</klr></trd>
    <trd lng="hu">haj&oacute;skapit&aacute;ny</trd>
  </snc>
  <snc mrk="kapita1.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      En terarmeo kaj aerarmeo, 
      <ref tip="super" cel="oficir.0o">oficiro</ref>, kiu rangas inter
      <ref cel="leuxte.0o">le&ubreve;tenanto</ref> kaj 
      <ref cel="major.0o">majoro</ref>.
    </dif>
    <trd lng="de">Hauptmann</trd>
    <trd lng="fr">capitaine<klr tip="ind"> (grade militaire)</klr></trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;zados</trd>,
      <trd>kapit&aacute;ny</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_n;</trd>
  <trd lng="es">capit&aacute;n</trd>
  <trd lng="nl">kapitein</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_t;&c_a;&c_n;</trd>
  <trd lng="sv">kapten</trd>
</drv>

<drv mrk="kapita1.korvet0o">
  <kap>korvet<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      En mararmeo, 
      <ref tip="super" cel="oficir.0o">oficiro</ref> kun
      rango ekvivalenta al tiu de <ref cel="major.0o">majoro</ref>.
    </dif>
    <ref tip="vid" cel="korvet.0o">korveto</ref>
  </snc>
  <trd lng="fr">capitaine de corvette</trd>
</drv>

<drv mrk="kapita1.fregat0o">
  <kap>fregat<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      En mararmeo, 
      <ref tip="super" cel="oficir.0o">oficiro</ref> kun
      rango ekvivalenta al tiu de <ref cel="kolone.sub0o">subkolonelo</ref>.
    </dif>
    <ref tip="vid" cel="fregat.0o.MIL">fregato</ref>
  </snc>
  <trd lng="fr">capitaine de fr&eacute;gate</trd>
</drv>

<drv mrk="kapita1.vesel0o">
  <kap>vesel<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      En mararmeo, 
      <ref tip="super" cel="oficir.0o">oficiro</ref> kun
      rango ekvivalenta al tiu de <ref cel="kolone.0o">kolonelo</ref>.
    </dif>
    <ref tip="vid" cel="vesel.0o">veselo</ref>
  </snc>
  <trd lng="fr">capitaine de vaisseau</trd>
</drv>

</art>
