<art mrk="$Id: hajk.xml,v 1.19 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>hajk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hajk.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">POE</uzo>
    <dif>
      Japan-devena fiksforma poemeto el 17 silaboj (5-7-5),
      klasike kun kontrastaj partoj kaj aludo al sezono:
      <ekz>
        temas entute pri 23 poemoj, de 17-silaba <tld/>o &gcirc;is dekpa&gcirc;a
        poemego
        <fnt><aut>Franz-Georg R&ouml;ssler</aut>,
        <vrk>Profite voja&gcirc;u kun Edwin</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1994a, numero 6a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_a;&c_j;&c_k;&c_u;</trd>,
    <trd>&c_h;&c_o;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_a;&c_j;&c_k;&c_u;</trd>
  <trd lng="ca">haiku</trd>
  <trd lng="cs">haiku</trd>
  <trd lng="de">Haiku</trd>
  <trd lng="en">haiku</trd>
  <trd lng="fr">ha&iuml;ku</trd>
  <trd lng="hu">hajku</trd>
  <trd lng="it">haiku</trd>
  <trd lng="ja">&#x4FF3;&#x53E5;  <pr>&#x306F;&#x3044;&#x304F;</pr></trd>
  <trd lng="nl">haiku</trd>
  <trd lng="pt">haicai</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_a;&c_j;&c_k;&c_u;</trd>,
    <trd>&c_h;&c_o;&c_k;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">haiku</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x43E;&#x43A;&#x443;</trd>,
    <trd>&#x445;&#x430;&#x439;&#x43A;&#x443;</trd>
  </trdgrp>
  <trd lng="zh">&#x4FF3;&#x53E5; <pr>p&#xE1;ij&#xF9;</pr></trd>
</drv>
