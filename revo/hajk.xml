<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hajk</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="hajk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Japan-devena fiksforma poemeto el 17 silaboj (5-7-5),
      versata per iu el la sezonaj vortoj, kiuj indikas
      apartan objekton a&ubreve; senton de la sezonoj.
    </dif>
  </snc>
  <trd lng="de">Haiku</trd>
  <trdgrp lng="fr">
    <trd>ha&iuml;ku</trd>
  </trdgrp>
  <trd lng="pt">haicai</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_a;&c_j;&c_k;&c_u;</trd>,
    <trd>&c_h;&c_o;&c_k;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
</art>

