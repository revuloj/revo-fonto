<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aikid</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="aikid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Japan-devena korpogarda <ref tip="super" cel="sport.0o">sporto</ref>,
      per kiu oni fulmrapide evitas atakanton, rebatas al sengarda punkto,
      senpovigas per artiko-tordo.
    </dif>
  </snc>
  <trd lng="de">Aikido</trd>
  <trdgrp lng="fr">
    <trd>a&iuml;kido</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_j;&c_k;&c_i;&c_d;&c_o;</trd>
</drv>
</art>

