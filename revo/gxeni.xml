<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gxeni.xml,v 1.3 2023/09/27 10:29:31 revo Exp $">
<kap>
  <rad>&gcirc;eni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gxeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="gxeni.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="dif" cel="dajmon.0o.REL">Dajmono</ref>:
      <ekz>
        <tld/>oj de la steloj
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>,
          <vrk>La Tragedio de l' Homo</vrk>,
          <lok>1965</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la Morto
        <fnt>
          <vrk><url ref="http://verkoj.com/lauteme/klasika/kalman-kalocsay/la-tragedio-de-lhomo/">La
          tragedio de l'homo - Verkoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o el lampo
        <fnt>
          <vrk><url ref="https://genekeyes.com/MC-MK/MC-MK-U-Foreword.html">Gene Keyes: Me Clone /
          Mi-Klono (U): Foreword</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o ridetis je mi kun kompatplena kaj afabla rigardo <klr>[kaj]</klr>
        levis min de la tero
        <fnt>
          <vrk><url ref="https://arkivo.esperanto-france.org/divers/alma1909/vizio.htm">La Vizio de
          Selio - arkivo.esperanto-france.org</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="geni.0o.dajmono">genio<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_h;</trd>,
    <trd>&c_d;&c_e;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
<trdgrp lng="de">
    <trd>Genius</trd>,
    <trd>Schutzgeist</trd>
  </trdgrp>
</drv>
</art>

