<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hekatomb</rad>/o
</kap>
<drv mrk="hekato.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Bu&ccirc;ofero el cent bovoj, &ccirc;e antikvuloj.
    </dif>
    <trd lng="nl">hecatombe</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Amasmortigo de multaj homoj:
      <ekz>
        la <tld/>o de la milito
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="masakr">masakro</ref>.
    </refgrp>
    <trd lng="nl">massaslachting</trd>
  </snc>
  <trd lng="de">Hekatombe</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_a;&c_t;&c_o;&c_m;&c_b;&c_a;</trd>
</drv>
</art>
