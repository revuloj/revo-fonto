  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_s;&c_t;&c_r;&c_y;&c_n;</trd>
  <trd lng="es">dextrina</trd>
  <trd lng="fr">dextrine</trd>
  <trd lng="hu">dextrin</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_s;&c_t;&c_r;&c_i;&c_n;</trd>
</drv>
</art>
