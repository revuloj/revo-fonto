<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barjon.xml,v 1.17 2021/03/04 19:18:49 revo Exp $">
<kap>
<rad>barjon</rad>/o
</kap>
<drv mrk="barjon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="vid" cel="partik2.0o">partikloj</ref>
      kun fortaj interagoj, al kiu apartenas
      la <ref tip="sub" cel="nukleon.0o">nukleonoj</ref> kaj la
      nestabilaj
      <ref tip="super" cel="hadron.0o">hadronoj</ref>
      nomataj <ref tip="sub" cel="hiperon.0o">hiperonoj</ref>:
      <ekz>
        majuskla &Sigma; estas uzata kiel simbolo por <klr>[&#x2026;]</klr> klaso de
        <tld/>oj en
        partikla fiziko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=245103">Sigmo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="kvark.0o">kvarko</ref>
    <refgrp tip="vid">
    <ref cel="mezon.0o">mezono</ref>,
    <ref cel="atom.0o">atomo</ref>,
    <ref cel="proton.0o">protono</ref>,
    <ref cel="neuxtr2.0o">ne&ubreve;trono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_jo;&c_n;</trd>
  <trd lng="ca">bari&oacute;</trd>
  <trd lng="cs">baryon</trd>
  <trd lng="de">Baryon</trd>
  <trd lng="en">baryon</trd>
  <trd lng="es">bari&oacute;n</trd>
  <trd lng="fa">&a_b;&a_A;&a_r;&f_y;&a_w;&a_n;</trd>
  <trd lng="fr">baryon</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D9;&#x5D5;&#x5DF;</trd>
  <trd lng="hu">barion</trd>
  <trd lng="it">barione</trd>
  <trd lng="pl">barion</trd>
  <trd lng="pt">b&aacute;rion</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">bary&oacute;n</trd>
</drv>
</art>
