<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>sofism</rad>/o
</kap>
<drv mrk="sofism.0o">
  <kap><ofc>*</ofc><tld/>o <fnt><bib>F</bib>, <lok>&FE;
  4</lok></fnt></kap>
  <snc>
    <dif>
      &Scirc;ajne &gcirc;usta sed fakte erariga rezono.
    </dif>
    <refgrp tip="sin">
      <ref cel="paralo.0o">paralogismo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_f;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">sofisma</trd>
  <trdgrp lng="cs">
    <trd>logick&yacute; klam</trd>,
    <trd>matouc&iacute; argumentace</trd>,
    <trd>sofizma</trd>,
    <trd>&uacute;skok</trd>
  </trdgrp>
  <trd lng="de">Sophismus</trd>
  <trd lng="es">sofisma</trd>
  <trd lng="fr">sophisme</trd>
  <trdgrp lng="hu">
    <trd>szofizmus</trd>,
    <trd>sz&odblac;rsz&aacute;lhasogat&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">sofisme</trd>
  <trd lng="pl">sofizmat</trd>
  <trd lng="ru">&c_s;&c_o;&c_f;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>logick&yacute; klam</trd>,
    <trd>m&auml;t&uacute;ca argument&aacute;cia</trd>,
    <trd>&uacute;skok</trd>
  </trdgrp>
</drv>
</art>
