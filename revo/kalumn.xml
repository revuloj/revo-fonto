<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalumn.xml,v 1.30 2024/08/19 09:27:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kalumni</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kalumn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kalumn.0i.KOMUNE">
    <dif>
      Malutili al ies honoro a&ubreve; &scirc;atateco per mensogaj
      <ref tip="super" cel="akuz.0i">akuzoj</ref>:
      <ekz>
        ili <tld/>is vin, insidis kontra&ubreve; vi
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili miras, ke vi ne kuras kune kun ili en la saman superfluon de dibo&ccirc;ado, kaj
        ili <tld/>as vin
        <fnt>
          <bib>NT</bib>
          <lok>I. Petro 4:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fu&scirc;agitisto <tld/>as pri la honestaj laborantoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mondo estas <tld/>ema
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        gvidantoj de <nom>ELNA</nom> kaj de <nom>UEA</nom> estis grave <tld/>ataj, kaj la tiama
        &gcirc;enerala
        sekretario de <nom>UEA</nom> estis e&ccirc; false denuncita &ccirc;e alilandaj sekurecaj servoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ante konstante, oni e&ccirc; an&gcirc;elon nigrigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="denunc.0i">denunci</ref>,
       <ref cel="klacx.0i">kla&ccirc;i</ref>,
       <ref cel="kulp.0igi">kulpigi</ref>,
       <ref cel="fam.mis0igi">misfamigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>nactiutrhat</trd>,
    <trd>pomlouvat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verleumden</trd>,
    <trd>schlechtreden</trd>,
    <trd>diffamieren</trd>,
    <trd>anschw&auml;rzen</trd>,
    <trd>runtermachen</trd>,
    <trd>niedermachen</trd>,
    <trd>f&auml;lschlich <ind>bezichtigen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>calumniate</trd>,
    <trd>slander</trd>,
    <trd>defame</trd>,
    <trd>libel</trd>,
    <trd>malign</trd>,
    <trd>run down</trd>
  </trdgrp>
  <trd lng="es">calumniar</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_h;&a_t;&a_A;&a_n; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_f;&a_t;&a_r;&a_A; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_h;&a_m;&a_t; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>calomnier</trd>,
    <trd>d&eacute;biner<klr tip="ind"> (m&eacute;dire)</klr></trd>,
    <trd>d&eacute;crier<klr tip="ind"> (qn)</klr></trd>,
    <trd>diffamer</trd>,
    <trd>m&eacute;dire (de)</trd>
  </trdgrp>
  <trd lng="hu">r&aacute;galmaz</trd>
  <trdgrp lng="ja">
    <trd>&#20013;&#20663;&#12377;&#12427; <pr>&#12385;&#12421;&#12358;&#12375;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#35513;&#35607;&#12377;&#12427; <pr>&#12402;&#12412;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#21517;&#35465;&#12434;&#27584;&#25613;&#12377;&#12427; <pr>&#12417;&#12356;&#12424;&#12434;&#12365;&#12381;&#12435;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">belasteren</trd>
  <trdgrp lng="pl">
    <trd>obgada&cacute;</trd>,
    <trd>obmawia&cacute;</trd>,
    <trd>oszczerstwa rzuca&cacute;</trd>,
    <trd>spotwarza&cacute;</trd>,
    <trd>oczernia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>s&abreve; vorbeasc&abreve; despre calomnie</trd>,
    <trd>pentru a arunca calomniile</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_ch;&c_je;&c_r;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ohov&aacute;ra&tcaron;</trd>,
    <trd>oso&ccaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i; &c_n;&c_a;&c_k;&c_l;&c_je;&c_p; (&c_n;&c_a;
    &c_k;&c_o;&c_g;&c_o;-&c_n;.)</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_mol;&c_b;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#35733; <pr>l&aacute;n</pr></trd>,
    <trd>&#35607; <pr>b&agrave;ng</pr></trd>,
    <trd>&#35730; <pr>ch&aacute;n</pr></trd>,
    <trd>&#20483; <pr>ji&ugrave;</pr></trd>,
    <trd>&#25187;&#23630;&#30406;&#23376; <pr>k&ograve;ush&#464;p&eacute;nzi</pr></trd>,
    <trd>&#35398;&#35693; <pr>d&#464;hu&#464;</pr></trd>,
    <trd>&#35733;&#35519; <pr>l&aacute;ndi&agrave;o</pr></trd>,
    <trd>&#34987;&#20013;&#20260; <pr>b&egrave;izh&omacron;ngsh&amacron;ng</pr></trd>,
    <trd>&#34987;&#35398;&#35693; <pr>b&egrave;id&#464;hu&#464;</pr></trd>,
    <trd>&#35491; <pr>w&umacron;</pr></trd>,
    <trd>&#35670; <pr>z&egrave;n</pr></trd>,
    <trd>&#22016; <pr>d&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalumn.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <klr>(pri akuzo)</klr> Mensoga:
      <ekz>
        kontra&ubreve; tiu &ccirc;i mensoga, malnobla kaj <tld/>a kulpigo ni protestas plej energie
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kampanjo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        kial li verkis kaj tradukis tiujn <tld/>ajn artikolojn kontra&ubreve; judoj
        <fnt>
          <bib>MMa</bib>
        </fnt>?
      </ekz>
      <ekz>
        mi klopodis protesti kontra&ubreve; tia <tld/>a supozo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_n;&c_ib;&c_c;&c_k;&c_ib;</trd>
  <trd lng="de">verleumderisch</trd>
  <trd lng="uk">&c_n;&c_a;&c_k;&c_l;&c_je;&c_p;&c_n;&c_i;&c_c;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trd lng="zh">&#36896;&#35616;&#20013;&#20663; <pr>z&agrave;oy&aacute;ozh&omacron;ngsh&amacron;ng</pr></trd>
</drv>

<drv mrk="kalumn.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalumn.0o.KOMUNE">
    <dif>
      Ago a&ubreve; vortoj de <tld/>anto:
      <ekz>
        malklereco, mokoj, <tld/>oj &mdash; &ccirc;io tio &ccirc;i estis venkita de la superhoma forto de
        lia persisteco
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni diras, ke mi &scirc;in vergis, &dash; tio &ccirc;i estas <tld/>o, per Dio, <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>o e&ccirc; pasas, &gcirc;i &ccirc;iam ion lasas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        serpenton oni povas eviti, sed <tld/>on neniam
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; virto ne evitas <tld/>on
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; <tld/>o helpas nenio
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kot.0o">koto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_jo;&c_p;</trd>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>drb</trd>,
    <trd>kalumnie</trd>,
    <trd>kleveta</trd>,
    <trd>nactiutrh&aacute;n&iacute;</trd>,
    <trd>pomluva</trd>
  </trdgrp>
  <trd lng="de">Verleumdung</trd>
  <trdgrp lng="en">
    <trd>calumny</trd>,
    <trd>false <ind>charge</ind></trd>,
    <trd>malicious <ind>misrepresentation</ind></trd>,
    <trd>slanderous <ind>report</ind></trd>
  </trdgrp>
  <trd lng="es">calumnia</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_h;&a_t;&a_A;&a_n;</trd>,
    <trd>&a_A;&a_f;&a_t;&a_r;&a_A;</trd>,
    <trd>&a_t;&a_h;&a_m;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>calomnie</trd>,
    <trd>diffamation</trd>,
    <trd>m&eacute;disance</trd>
  </trdgrp>
  <trd lng="hu">r&aacute;galom</trd>
  <trdgrp lng="ja">
    <trd>&#20013;&#20663;  <pr>&#12385;&#12421;&#12358;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#35513;&#35607;  <pr>&#12402;&#12412;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">laster</trd>
  <trdgrp lng="pl">
    <trd>kalumnia</trd>,
    <trd>potwarz</trd>,
    <trd>fa&lstroke;szywe oskar&zdot;enie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>calomnia</trd>,
    <trd>fals&abreve; acuza&tcomma;ie</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>klebeta</trd>,
    <trd>ohov&aacute;rka</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_a;&c_k;&c_l;&c_je;&c_p;</trd>
  <trdgrp lng="zh">
    <trd>&#35730;&#35328; <pr>ch&aacute;ny&aacute;n</pr></trd>,
    <trd>&#35639; <pr>&aacute;o</pr></trd>,
    <trd>&#35513;&#35607; <pr>f&ecaron;ib&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalumn.0ulo">
  <kap><tld/>ulo,
  <var><kap><tld/>anto</kap></var></kap>
  <snc mrk="kalumn.0ulo.KOMUNE">
    <dif>
      Tiu, kiu <tld/>as, kiu mensoge akuzas a&ubreve; misfamigas:
      <ekz>
        miaj <tld/>antoj miros, vidante, ke mi ilin kontra&ubreve;batalas kaj ne konfesas mian
        malvenkon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ulo! fortran&ccirc;u al li la langon
        <fnt>
          <aut>A. Svjento&hcirc;ovski, trad. Leono Zamenhof</aut>
          <vrk><url ref="&Gutenberg;files/45713/45713-h/45713-h.htm">
          Aspazio</url></vrk>
          <lok>1908</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Verleumder</trd>
  <trd lng="fr">calomniateur</trd>
  <trd lng="ja">&#20013;&#20663;&#32773;  <pr>&#12385;&#12421;&#12358;&#12375;&#12423;&#12358;&#12375;&#12419;</pr></trd>
  <trd lng="uk">&c_n;&c_a;&c_k;&c_l;&c_je;&c_p;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
