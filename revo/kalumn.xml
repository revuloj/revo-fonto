<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kalumni</rad>/i
</kap>
<drv mrk="kalumn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malutili al ies honoro a&ubreve; &scirc;atateco per mensogaj akuzoj:
      <ekz><tld/>ante konstante, oni e&ccirc; an&gcirc;elon nigrigas<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">verleumden</trd>
  <trdgrp lng="en">
    <trd>calumniate</trd>,
    <trd>slander</trd>,
    <trd>defame</trd>,
    <trd>libel</trd>,
    <trd>malign</trd>,
    <trd>run down</trd>
  </trdgrp>
  <trd lng="es">calumniar</trd>
  <trd lng="hu">r&aacute;galmaz</trd>
  <trd lng="nl">belasteren</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_ch;&c_je;&c_r;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kalumn.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago a&ubreve; vortoj de <tld/>anto:
      <ekz>se <tld/>o e&ccirc; pasas, &gcirc;i &ccirc;iam ion lasas<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        serpenton oni povas eviti, <tld/>on neniam<fnt>Z</fnt>;
      </ekz>
      <ekz>kontra&ubreve; <tld/>o helpas nenio<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kot.0o">koto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_jo;&c_p;</trd>
  <trd lng="de">Verleumdung</trd>
  <trdgrp lng="en">
    <trd>calumny</trd>,
    <trd>false <ind>charge</ind></trd>,
    <trd>malicious <ind>misrepresentation</ind></trd>,
    <trd>slanderous <ind>report</ind></trd>
  </trdgrp>
  <trd lng="es">calumnia</trd>
  <trd lng="hu">r&aacute;galom</trd>
  <trd lng="nl">laster</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_a;</trd>
</drv>
</art>
