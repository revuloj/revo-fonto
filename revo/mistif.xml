<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mistif</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mistif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mistif.0i.MOR">
    <ref tip="dif" cel="mistifik.0i">mistifiki</ref>
    <ekz>
      malicaj koboldoj <klr>...</klr> devojigas la
      karavanojn, elsekigas la putojn kaj dum la
      senlunaj noktoj senripozigas la <tld/>itajn
      kamelistojn
      <fnt>
        <aut>J. Ribillard</aut>,
        <vrk>Vagado sub palmoj</vrk>,
        <lok>Rotterdam, 1956</lok>
      </fnt>.      
    </ekz>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>foppen</trd>,
    <trd>narren</trd>,
    <trd>jemanden hinters <ind>Licht</ind> f&uuml;hren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire une <ind>blague</ind></trd>,
    <trd>mystifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&aacute;szed</trd>,
    <trd>becsap</trd>,
    <trd>f&eacute;lrevezet</trd>,
    <trd>beugrat</trd>,
    <trd>k&ouml;d&ouml;s&iacute;t</trd>
    <trd>misztifik&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">foppen</trd>
  <trdgrp lng="pt">
   <trd>mistificar</trd>,
   <trd>ludibriar</trd>,
   <trd>burlar</trd>   
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>mystifikovať</trd>,
    <trd>oklamať</trd>,
    <trd>poblázniť</trd>,
    <trd>podvádzať</trd>,
    <trd>zmámiť</trd>
  </trdgrp>
</drv>
</art>
