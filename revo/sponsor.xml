  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>x</vspec></gra>
  <snc mrk="sponsor.0i.mecenati">
    <ref tip="dif" cel="mecena.0i.BELA">mecenati</ref>
  </snc>
  <trdgrp lng="de">
    <trd>sponsorn</trd>
  </trdgrp>
  <trd lng="fr">sponsoriser</trd>
  <trd lng="it">sponsorizzare</trd>
  <trd lng="sk">sponzorova≈•</trd>
</drv>
</art>

