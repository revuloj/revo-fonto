<drv mrk="desegn.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="desegn.0ajxo.BELA">
    <dif>
      <tld lit="D"/>ita bildo:
      <ekz>
        skribado per hieroglifaj <tld/>a&jcirc;oj
        <fnt><bib>EE</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">BELA</uzo>
        la <tld/>a&jcirc;oj de Rembrandt<fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        la planko de la halo estis pavimita per lerte kunmetitaj platoj
        kun
        <tld/>a&jcirc;oj, jen geometriaj, jen arabeskaj
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;apitro 10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu plej malgranda objekto kaj vazo estis ornamita per
        skulpta&jcirc;o a&ubreve; kolora <tld/>a&jcirc;o
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 9</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">TEK</uzo>
        <tld/>a&jcirc;o la&ubreve; la ekstera vido, la&ubreve; la sekca&jcirc;o
        <fnt><bib>PIV1</bib></fnt>.
