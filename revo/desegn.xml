      <trd>&c_ch;&c_a;&c_r;&c_c;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_r;&c_e;&c_s;&c_mol;&c_l;&c_ib;&c_c;&c_mol;</trd>
