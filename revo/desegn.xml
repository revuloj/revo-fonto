<drv mrk="desegn.0o">
  <kap><tld/>o</kap>
  <snc mrk="desegn.0o.desegnado">
    <dif>
      <ref tip="dif" cel="desegn.0ado.BELA">Desegnado</ref>
      a&ubreve; maniero kiel io estas <tld/>ita:
      <ekz>
        mirinda facileco de <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>o, kiaj koloroj! &gcirc;i estas vesto de granda indo
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fason.0o">fasono</ref>,
      <ref cel="stil.0o.BELA.loka">stilo<sncref/></ref>,
      <ref cel="kolor.0o.stilo">koloro<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_y;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_k;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_ch;&c_a;&c_r;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szkicowanie</trd>,
      <trd>rysowanie<klr> (bez kolor&oacute;w)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="desegn.0o.desegnajxo">
    <dif>
      <ref tip="dif" cel="desegn.0ajxo.BELA">Desegna&jcirc;o</ref>:
      <ekz>
        grandaj, flor<tld/>aj tapi&scirc;oj pendis de oraj festonoj sur la
        muroj
        <klr>(tapi&scirc;oj kun floraj <tld/>a&jcirc;oj)</klr>
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;apitro 5</lok></fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o A&hcirc;az sendis al la pastro Urija la bildon de la
        altaro kaj la <tld/>on de &gcirc;ia tuta konstruo
        <fnt><bib>MT</bib>, <lok>&Re2; 16:10</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[David donis al sia filo]</klr> la <tld/>on de &ccirc;io,
        kion
        li havis en sia animo koncerne la kortojn de la domo de la
        Eternulo
        <fnt><bib>MT</bib>, <lok>&Kr1; 28:12</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_y;&c_s;&c_u;&c_n;&c_a;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_r;&c_c;&c_jo;&c_zh;</trd>,
      <trd>&c_d;&c_y;&c_z;&c_a;&c_j;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szkic</trd>,
      <trd>rysunek<klr> (bez kolor&oacute;w)</klr></trd>
    </trdgrp>
  </snc>
