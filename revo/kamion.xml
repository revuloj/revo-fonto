<drv mrk="kamion.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="kamion.0eto.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Malgranda <ref tip="super" cel="kamion.0o.AUT"><tld/>o</ref>,
      kun du- a&ubreve; triloka &scirc;oforejo kaj plejofte fermita &scirc;ar&gcirc;ejo:
      <ekz>
        feli&ccirc;e alveturis <tld/>eto kaj per &gcirc;i ni atingis la
        vila&gcirc;on
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>2001/12, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_a;&c_v;&c_ib;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_ib;&c_k;&c_a;&c_p;</trd>
  </trdgrp>
  <trd lng="ca">camioneta</trd>
  <trdgrp lng="cs">
    <trd>dod&aacute;vka</trd>,
    <trd>dod&aacute;vkov&eacute; auto</trd>
  </trdgrp>
  <trd lng="de">Lieferwagen</trd>
  <trd lng="en">van</trd>
  <trdgrp lng="es">
    <trd>camioneta</trd>,
    <trd>furgoneta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>camionnette</trd>,
    <trd>petit camion</trd>
  </trdgrp>
  <trd lng="hu">kisteheraut&oacute;</trd>
  <trdgrp lng="it">
    <trd>camionetta</trd>,
    <trd>furgone</trd>
  </trdgrp>
  <trd lng="ja">&#23567;&#22411;&#12488;&#12521;&#12483;&#12463;
  <pr>&#12371;&#12364;&#12383;&#12488;&#12521;&#12483;&#12463;</pr></trd>
  <trd lng="nl">bestelwagen</trd>
  <trd lng="oc">camioneta</trd>
  <trdgrp lng="pl">
    <trd>furgonetka</trd>,
    <trd>p&oacute;&lstroke;ci&eogonek;&zdot;ar&oacute;wka</trd>,
    <trd>samoch&oacute;d dostawczy</trd>
