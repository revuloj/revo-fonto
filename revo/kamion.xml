  <trd lng="bg">&c_k;&c_a;&c_m;&c_i;&c_o;&c_n;</trd>
  <trd lng="bn">&#x99F;&#x9CD;&#x9B0;&#x9BE;&#x995;</trd>
  <trd lng="bs">kamion</trd>
  <trd lng="ca">cami&oacute;</trd>
  <trd lng="co">camion</trd>
  <trdgrp lng="cs">
