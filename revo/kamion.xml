<drv mrk="kamion.baskul0o">
  <kap>baskul<tld/>o</kap>
  <snc mrk="kamion.baskul0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="kamion.0o.AUT">Kamiono</ref>
      kun platformo, kiu baskule klini&gcirc;as por el&scirc;uto
      de la enhavo:
      <ekz>
        baskul<tld/>oj faligis en la riveron &scirc;tonojn kaj gruzon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Novosibirska_hidroelektra_centralo">
          Novosibirska hidroelektra centralo</url>, 2013-04-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_l;</trd>
  <trd lng="cs">skl&aacute;p&ecaron;c&iacute; auto</trd>
  <trdgrp lng="es">
    <trd>volquete</trd>,
    <trd>cami&oacute;n de obras</trd>
  </trdgrp>
  <trd lng="fr">camion &agrave; benne basculante</trd>
  <trd lng="ja">&#12480;&#12531;&#12503;&#12459;&#12540;</trd>
  <trd lng="pl">wywrotka <klr>(ci&eogonek;&zdot;ar&oacute;wka)</klr></trd>
  <trdgrp lng="ro">
    <trd>autobasculant&abreve;</trd>,
    <trd>dumper</trd>
  </trdgrp>
  <trd lng="sk">vykl&aacute;pacie auto</trd>
</drv>

<drv mrk="kamion.remork0o">
  <kap>remork<tld/>o <fnt><aut>R. Harry, B. Fox</aut> <vrk><url
  ref="https://www.esperanto.org.nz/anzed/anzed.html">ustralian and New Zealand Esperanto
  Dictionary</url></vrk> <lok>2020</lok></fnt>,
  <var><kap><tld/>o kun remorko</kap></var></kap>
  <snc mrk="kamion.remork0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="kamion.0o.AUT">Kamiono</ref> provizita je posttirata
      <ref tip="prt" cel="remork.0o.TRA">remorko</ref>:
      <ekz>
        la germana nacia trafikisto planas trafikadon de <tld/>oj kun remorkoj ekde jaro 2017 tute
        &ccirc;esigi
        <fnt>
          <aut>J. Tom&iacute;&scaron;ek, H. Hoffmann</aut>
          <vrk>Malplimultaj kamionoj kunremorkaj trafikos en jaro 2015</vrk>
          <lok><url ref="http://www.eventoj.hu/steb/ingxenierado/trafiko/fervojo/EFM-2015-07.pdf">
          </url>El fervoja mondo, 2015:7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>semi-remorque<klr> (camion)</klr></trd>,
      <trd>v&eacute;hicule articul&eacute;</trd>
    </trdgrp>
    <trd lng="pl">ci&eogonek;&zdot;ar&oacute;wka z przyczep&aogonek;</trd>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_g;&c_a;&c_ch;</trd>
</drv>
