<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kamion</rad>/o
</kap>

<drv mrk="kamion.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Fortika, platplanka
      <ref tip="super" cel="auxt.0o">a&ubreve;to</ref>
      por pezaj &scirc;ar&gcirc;oj kun 4, 6 a&ubreve; pli da radoj:
      <ekz>
        poste oni transportis lignon per fervojo kaj
        <tld/>oj
        <fnt>
          <aut>Walter Klag</aut>,
          <vrk>Vakeroj de Alpoj</vrk>,
          <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        videblis nek a&ubreve;to, nek <tld/>o
        <fnt>
          <aut>Julian Modest</aut>,
          <vrk>La ikono</vrk>,
          <bib>Monato</bib><lok>jaro 2001a, numero 12a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="auxt.sxargx0o">&scirc;ar&gcirc;a&ubreve;to</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_i;&c_o;&c_n;</trd>
  <trd lng="ca">cami&oacute;</trd>
  <trdgrp lng="de">
    <trd>Lastwagen</trd>,
    <trd>Lkw</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lorry</trd>,
    <trd>truck</trd>
  </trdgrp>
  <trd lng="es">cami&oacute;n</trd>
  <trd lng="fr">camion</trd>
  <trdgrp lng="hu">
    <trd>kamion</trd>,
    <trd>teheraut&oacute;</trd>
  </trdgrp>
  <trd lng="it">camion</trd>
  <trd lng="nl">vrachtwagen</trd>
  <trd lng="oc">camion</trd>
  <trdgrp lng="pl">
    <trd>ci&eogonek;&zdot;ar&oacute;wka</trd>,
    <trd>samoch&oacute;d ci&eogonek;&zdot;arowy</trd>
    <trd>tir<klr> (zwyczajowo o ci&aogonek;gniku siod&lstroke;owym z
naczep&aogonek; wieloosiow&aogonek; 
    i napisem TIR)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_i;&c_k;</trd>
</drv>

<drv mrk="kamion.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Eta &scirc;ar&gcirc;<ref tip="super" cel="auxt.0o">a&ubreve;to</ref>,
ordinare kun du- a&ubreve; triloka &scirc;oforejo kaj fermita
&scirc;ar&gcirc;ejo:
      <ekz>
        feli&ccirc;e alveturis <tld/>eto kaj per &gcirc;i ni atingis la
        vila&gcirc;on
        <fnt>
          <aut>Werner Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib><lok>jaro 2001a, numero 12a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">camioneta</trd>
  <trd lng="es">camioneta</trd>
  <trd lng="de">Lieferwagen</trd>
  <trd lng="en">van</trd>
  <trd lng="fr">camionnette</trd>
  <trd lng="hu">kisteheraut&oacute;</trd>
  <trdgrp lng="it">
    <trd>camionetta</trd>,
    <trd>furgone</trd>
  </trdgrp>
  <trd lng="nl">bestelwagen</trd>
  <trd lng="oc">camioneta</trd>
  <trdgrp lng="pl">
    <trd>p&oacute;&lstroke;ci&eogonek;&zdot;ar&oacute;wka</trd>,
    <trd>samoch&oacute;d dostawczy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_i;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_i;&c_k;&c_a;&c_p;</trd>
  </trdgrp>
</drv>
</art>
