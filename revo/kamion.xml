<drv mrk="kamion.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="kamion.0eto.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Malgranda <ref tip="super" cel="kamion.0o.AUT"><tld/>o</ref>,
      kun du- a&ubreve; triloka &scirc;oforejo kaj plejofte fermita &scirc;ar&gcirc;ejo:
      <ekz>
        feli&ccirc;e alveturis <tld/>eto kaj per &gcirc;i ni atingis la
        vila&gcirc;on
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">camioneta</trd>
  <trdgrp lng="cs">
    <trd>dod&aacute;vka</trd>,
    <trd>dod&aacute;vkov&eacute; auto</trd>
  </trdgrp>
  <trd lng="de">Lieferwagen</trd>
  <trd lng="en">van</trd>
  <trdgrp lng="es">
    <trd>camioneta</trd>,
    <trd>furgoneta</trd>
  </trdgrp>
  <trd lng="fr">camionnette</trd>
  <trd lng="hu">kisteheraut&oacute;</trd>
  <trdgrp lng="it">
    <trd>camionetta</trd>,
    <trd>furgone</trd>
  </trdgrp>
  <trd lng="nl">bestelwagen</trd>
  <trd lng="oc">camioneta</trd>
  <trdgrp lng="pl">
    <trd>furgonetka</trd>,
    <trd>p&oacute;&lstroke;ci&eogonek;&zdot;ar&oacute;wka</trd>,
    <trd>samoch&oacute;d dostawczy</trd>
  </trdgrp>
  <trd lng="ro">furgonet&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_i;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_i;&c_k;&c_a;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dod&aacute;vka</trd>,
    <trd>dod&aacute;vkov&eacute; auto</trd>
  </trdgrp>
</drv>

<drv mrk="kamion.baskul0o">
  <kap>baskul<tld/>o</kap>
  <snc mrk="kamion.baskul0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="kamion.0o.AUT">Kamiono</ref>
      kun platformo, kiu baskule klini&gcirc;as por el&scirc;uto
      de la enhavo:
      <ekz>
        baskul<tld/>oj faligis en la riveron &scirc;tonojn kaj gruzon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Novosibirska_hidroelektra_centralo">
          Novosibirska hidroelektra centralo</url>, 2013-04-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">skl&aacute;p&ecaron;c&iacute; auto</trd>
  <trdgrp lng="es">
    <trd>volquete</trd>,
    <trd>cami&oacute;n de obras</trd>
  </trdgrp>
  <trd lng="fr">camion &agrave; benne basculante</trd>
  <trd lng="pl">wywrotka <klr>(ci&eogonek;&zdot;ar&oacute;wka)</klr></trd>
  <trdgrp lng="ro">
    <trd>autobasculant&abreve;</trd>,
    <trd>dumper</trd>
  </trdgrp>
  <trd lng="sk">vykl&aacute;pacie auto</trd>
</drv>
