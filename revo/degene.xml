<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: degene.xml,v 1.25 2022/04/26 17:11:47 revo Exp $">
<kap>
<ofc>*</ofc>
  <rad>degener</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="degene.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="degene.0i.KOMUNE">
    <dif>
      Perdi la originajn kvalitojn:
      <ekz>
        &ccirc;iuj ili estas <tld/>intoj
        <fnt><bib>MT</bib>, <lok>&Jer; 6:28</lok></fnt>;
      </ekz>
      <ekz>
        lia gajo <tld/>as en frenezon
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia krueleco, liaj <tld/>intaj instinktoj kaj fantazio &gcirc;uis tiajn vida&jcirc;ojn
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun la falo de la respubliko la retoriko kaj la parolarto komencis <tld/>i
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        se oni komencas malobservi tiajn bazajn kutimojn, &ccirc;io povas <tld/>i en pelmela
        &hcirc;aoso
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &leftquot;interna ideo&rightquot; <klr>[&#x2026;]</klr> <tld/>is al ia sentimentala sinteno, preska&ubreve;
        religieco, al ia majstro-kulto
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.de0i">defali</ref>,
      <ref cel="dekade.0i">dekadenci</ref>,
      <ref cel="gangre.0i">gangreni</ref>,
      <ref cel="bon.mal0igxi">malboni&gcirc;i</ref>,
      <ref cel="putr.0i">putri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">degenerar</trd>
  <trdgrp lng="cs">
    <trd>degenerovat</trd>,
    <trd>zakr&ncaron;ovat</trd>,
    <trd>zvrhnout se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>degenerieren</trd>,
    <trd>entarten</trd>,
    <trd>verfallen</trd>
  </trdgrp>
  <trd lng="en">degenerate</trd>
  <trd lng="es">degenerar</trd>
  <trd lng="fr">d&eacute;g&eacute;n&eacute;rer</trd>
  <trd lng="he">&lamed;&he;&tav;&nun;&vav;&vav;&fnun;</trd>
  <trdgrp lng="hu">
     <trd>elfajul</trd>,
     <trd>degener&aacute;l&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>degenerowa&cacute;</trd>,
    <trd>degradowa&cacute;<klr> (niszcze&cacute;)</klr></trd>,
    <trd>wyrabia&cacute; si&eogonek;<klr> (zu&zdot;ywa&cacute;
    si&eogonek;)</klr></trd>,
    <trd>wyradza&cacute; si&eogonek;<klr> (np. wyrodny syn)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>degenerar</trd>,
    <trd>corromper-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_r;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_g;&c_r;&c_a;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>degenerova&tcaron;</trd>,
    <trd>zvrhn&uacute;&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x440;&#x43E;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x440;&#x43E;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x434;&#x435;&#x491;&#x435;&#x43D;&#x435;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="degene.0o">
  <kap><tld/>o</kap>
  <snc mrk="degene.0o.KOMUNE">
    <dif>
      Perdo de siaj naturaj kvalitoj:
      <ekz>
        manko de sango kaj grasa <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;eco kaj malri&ccirc;eco <klr>[&#x2026;]</klr> enkondukas avaremon kaj <tld/>on
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de tradiciaj partioj, akompanata de korupto
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9308-alarmsignalo.txt">El mia vidpunkto</url></vrk>
          <lok>93/08</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la malapero de Fonto kaj la ideologia kaj kondutprincipa <tld/>o de
        Literatura foiro, restis nur revuoj parte literaturaj, kiel La gazeto a&ubreve; La
        kancerkliniko
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekade.0o">dekadenco</ref>,
      <ref cel="korupt.0o">korupto</ref>
    </refgrp>
  </snc>
  <trd lng="ca">degeneraci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>rozklad organick&yacute;</trd>,
    <trd>spole&ccaron;ensk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Degeneration</trd>,
    <trd>Verfall</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;ch&eacute;ance</trd>,
    <trd>d&eacute;g&eacute;n&eacute;rescence</trd>
  </trdgrp>
  <trd lng="sk">degener&aacute;cia</trd>
</drv>

<drv mrk="degene.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="degene.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on, <ref tip="vid" cel="korupt.0i.morale">korupti</ref>:
      <ekz>
        malri&ccirc;eco kaj ri&ccirc;eco <tld/>igas la laboron de la metiistoj kaj la metiistojn mem
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        la moderna kapitalismo nun kapablas <tld/>igi &ccirc;ian ajn homan agadon, ekde kiam
        &gcirc;i akaparas &gcirc;in kaj trudas al &gcirc;i sian logikon
        <fnt>
          <bib>MD14</bib>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trdgrp lng="ca">
    <trd>fer degenerar</trd>,
    <trd>depravar</trd>,
    <trd>corrompre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vyvolat degeneraci</trd>,
    <trd>zp&uring;sobit degeneraci</trd>
  </trdgrp>
  <trd lng="de">pervertieren</trd>
  <trdgrp lng="fr">
    <trd>ab&acirc;tardir</trd>,
    <trd>gangr&eacute;ner<klr> (fig.)</klr></trd>,
    <trd>pervertir</trd>
  </trdgrp>
  <trd lng="he">&lamed;&nun;&vav;&vav;&fnun;</trd>
  <trd lng="sk">vyvola&tcaron; degener&aacute;ciu</trd>
</drv>
</art>
