<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: negati.xml,v 1.36 2018/01/16 21:10:14 revo Exp $">
<kap>
  <ofc>2</ofc><rad>negativ</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="negati.0a">
  <kap><tld/>a</kap>
  <snc mrk="negati.0a.nega">
    <dif>
      <ref tip="dif" cel="neg.0a">Nega</ref>;
      inversa de
      <ref tip="ant" cel="jes.0a.KOMUNE">jesa</ref>:
      <ekz>
        <tld/>a vorto;
      </ekz>
      <ekz>
        <tld/>a respondo.
      </ekz>
    </dif>
    <trd lng="hu">nemleges</trd>
    <trdgrp lng="pl">
      <trd>przecz&aogonek;cy</trd>,
      <trd>odmowny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_g;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="negati.0a.malfavora">
    <dif>
      Malfavora, malaproba, malbona:
      <ekz>
        <tld/>a recenzo;
      </ekz>
      <ekz>
        esprimi sian <tld/>an rilaton al io;
      </ekz>
      <ekz>
        mia sinteno al la Manifesto de Ra&ubreve;mo estas <tld/>a
        <fnt><aut>C. Piron</aut>, <vrk>Kien esperanto?</vrk>
        <lok>Prelego kasede prezentita okaze de kunveno en
        Cully, Svislando, 1994-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa metodo de SAF estas projekcio de iu <tld/>a
        nuntempa fenomeno en estontecon
        <fnt>
        <aut>J. Mura&scirc;kovskij</aut>
        <vrk><url
        ref="http://www.esperanto.mv.ru/Arkivoj/Fantastiko/Pritakso.html">
        Pritakso de fantastikaj verkoj</url></vrk>
        <lok>1985</lok>
        </fnt>;
      </ekz>
      <ekz>
        bazi&gcirc;ante sur tiuj <tld/>aj spertoj, ni rifuzas projekton
        de kreado de liber&scirc;an&gcirc;a zono en Amerikoj
        <fnt>
        <aut>-</aut>
        <vrk><url
        ref="http://attac.org/geneve2000/textes/doc/resolutioneo.htm"
        >Konstrui la vojojn al alia mondo: ni tutmondigu
        rezistadojn</url></vrk>,
        <lok>[2000?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="aprob.0a">aproba</ref>,
      <ref cel="favor.0a">favora</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>negat&iacute;v</trd>,
      <trd>visszautas&iacute;t&oacute;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>negatywny</trd>,
      <trd>odmowny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_g;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="tp">ike</trd>
  </snc>
  <snc mrk="negati.0a.malpozitiva">
    <dif>
      Inversa de
      <ref tip="ant" cel="poziti.0a">pozitiva</ref>.
    </dif>
    <subsnc mrk="negati.0a.testo">
      <dif>
        Indikanta maleston de ser&ccirc;ato a&ubreve; koncernato:
        <ekz>
          <tld/>a provo;
        </ekz>
        <ekz>
          <tld/>a rezulto de eksperimento;
        </ekz>
        <ekz>
          <tld/>a virto, plezuro
        </ekz>
      </dif>
      <trd lng="hu">negat&iacute;v</trd>
      <trd lng="pl">negatywny</trd>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="negati.0a.ELE">
      <uzo tip="fak">ELE</uzo>
      <dif>
        La malo de elektre <ref tip="ant"
        cel="poziti.0a.ELE">pozitiva<sncref/></ref>;
        <ref tip="sin"
        cel="poziti.mal0a.ELE">malpozitiva<sncref/></ref>:
        <ekz>
          la pozitiva klemo de pilo.
        </ekz>
      </dif>
      <trd lng="hu">negat&iacute;v<klr tip="amb"> (elektr.)</klr></trd>
      <trd lng="pl">ujemny</trd>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="amb"> (&c_e;&c_l;.)</klr></trd>
    </subsnc>
    <subsnc mrk="negati.0a.FOT">
      <uzo tip="fak">FOT</uzo>
      <dif>
        Rilata al fota
        <ref cel="negati.0o.FOT">negativo</ref>;
        <ref tip="sin" cel="poziti.mal0a.FOT">malpozitiva<sncref/></ref>
      </dif>
      <trd lng="hu">negat&iacute;v<klr tip="amb"> (f&eacute;nyk&eacute;p)</klr></trd>
      <trd lng="pl">negatywowy</trd>
      <trd lng="ru">&c_n;&c_je;&c_g;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;<klr tip="amb"> (&c_f;&c_o;&c_t;.)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="negati.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="negati.0a.nombro">
      <fnt><bib>MatTerm</bib>, <lok>p. 7</lok></fnt>
      <dif>
        <klr>(pri <ref cel="nombr.0o">nombro</ref> a&ubreve; pri elemento en
        <ref cel="ord.0a.aro">orda</ref>, adicie signata
        <ref cel="monoid.0o">monoido</ref>)</klr>
        Malpli granda ol <ref cel="nul0.0o.neuxtra">nulo</ref>:
        <ekz>
          <tld/>a kvanto povas esti signata per la signo <frm>-</frm>
          <klr>(ekz-e <frm>-3</frm>)</klr>;
        </ekz>
        <ekz>
          la kvadrato de <tld/>a reelo estas pozitiva.
        </ekz>
      </dif>
      <ref tip="ant" cel="poziti.0a.nombro">pozitiva</ref>
      <rim>
        Kiel por &ccirc;iu <ref cel="rilat.ordo0o">ordo-rilato</ref>
        ekzistas ambigueco, &ccirc;u oni konsideras &gcirc;in strikte
        a&ubreve; malstrikte.
        La&ubreve; la kunteksto eblas konsideri,
        ke <frm>0</frm> estas <tld/>a a&ubreve; ne.
      </rim>
      <trd lng="en">negative</trd>
      <trd lng="hu">negat&iacute;v<klr tip="amb"> (pl. sz&aacute;m)</klr></trd>
      <trd lng="pl">ujemny</trd>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="negati.0a.izometrio">
      <dif>
        <klr>(p.p. <ref cel="afin.0a.bildigo">afina</ref>
        <ref tip="super" cel="izomet.0o">izometrio</ref>)</klr>
        Tia, ke la <ref cel="determ.0anto.endomorfio">determinanto</ref>
        de &gcirc;ia asociita endomorfio estas <tld/>a
        <sncref ref="negati.0a.nombro"/>.
      </dif>
      <ref tip="ant" cel="poziti.0a.izometrio">pozitiva.</ref>
      <rim>
        Vidu rimarkon sub <ctl>pozitiva</ctl>.
      </rim>
      <trd lng="de">uneigentlich<klr tip="ind"> (-e
      Bewegung)</klr></trd>
      <trd lng="en">improper<klr tip="ind"> (movement,
      motion)</klr></trd>
      <trdgrp lng="fr">
        <trd><klr tip="amb">(isom&eacute;trie) </klr>r&eacute;trograde</trd>,
        <trd><klr tip="amb">(isom&eacute;trie) </klr>n&eacute;gative</trd>
      </trdgrp>
      <trd lng="hu">negat&iacute;v <klr tip="amb">(izometria)</klr></trd>
      <trd lng="pl"><klr tip="amb">(izometria) </klr>nieparzysta</trd>
      <trd lng="ru">&c_n;&c_je;&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
    </subsnc>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_g;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>negativn&iacute;</trd>,
    <trd>z&aacute;porn&yacute;</trd>
  </trdgrp>
  <trd lng="de">negativ</trd>
  <trd lng="el">&alfa;&rho;&ny;&eta;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">n&eacute;gatif<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">negat&iacute;v <klr tip="amb">(fot&oacute;)</klr></trd>
  <trd lng="nl">negatief<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="pt">negativo</trd>
  <trdgrp lng="sk">
    <trd>bezv&yacute;sledn&yacute;</trd>,
    <trd>negat&iacute;vny</trd>,
    <trd>z&aacute;porn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">negativ<klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="negati.0o">
  <kap><tld/>o</kap>
  <snc mrk="negati.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Bildo en kiu la koloroj aperas kiel komplementaj al la
      originalo; speciale, sur nigra-blanka bildo la helaj
      partoj aperas malhele, kaj inverse:
      <ekz>
        la Esperanto-muzeo nuntempe konservas
        kaj disponigas jenan hava&jcirc;on:
        40.000 faldfolioj <klr>...</klr> 22.000 fotoj kaj foto<tld/>oj
        <fnt>
          <aut>B. Tuider</aut>,
          <vrk><url
          ref="http://www.liberafolio.org/2018/01/16/vizitrekordo-ce-la-esperanto-muzeo-en-vieno/">
          Vizitrekordo &ccirc;e la Esperanto-muzeo en Vieno</url></vrk>,
          <lok>libera folio, 2018-01-16 </lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_e;&c_g;&c_a;&c_t;&c_y;&c_w; <klr>(&c_f;&c_a;&c_t;&c_a;&c_z;&c_d;&c_y;&c_m;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Negativ</trd>
    <trd lng="el">&alfa;&rho;&ny;&eta;&tau;&jota;&kappa;&omikron_ton;<klr tip="ind"> (&phi;&omega;&tau;&omikron;&gamma;&rho;&alfa;&phi;&jota_ton;&alfa;&sigma_fina;)</klr></trd>
    <trd lng="fr">n&eacute;gatif<klr tip="ind"> (photog.)</klr></trd>
    <trd lng="hu">negat&iacute;v <klr tip="amb">(f&eacute;nyk&eacute;p&eacute;szet)</klr></trd>
    <trd lng="nl">negatief<klr tip="ind"> (foto.)</klr></trd>
    <trd lng="pl">negatyw</trd>
    <trd lng="pt">negativo</trd>
    <trd lng="ru">&c_n;&c_je;&c_g;&c_a;&c_t;&c_i;&c_v;</trd>
    <trd lng="sv">negativ<klr tip="ind"> (foto.)</klr></trd>
  </snc>
  <trd lng="cs">negativum</trd>
  <trd lng="sk">negat&iacute;vum</trd>
</drv>

<drv mrk="negati.ne0a">
  <kap>ne<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(pri <ref cel="nombr.0o">nombro</ref>)</klr>
      Pli granda ol <frm>0</frm> a&ubreve; egala al &gcirc;i.
    </dif>
    <rim>
      &Ccirc;i tiun vorton oni uzas por eviti la ambiguecon de
      &leftquot;<ref cel="poziti.0a.nombro">pozitiva</ref>&rightquot;...
      sed, kompreneble, oni devas fronti tiun de <ctl><tld/>a</ctl>, kiu
      reaperas, kiam oni analizas la kunmeta&jcirc;on.
    </rim>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_d;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>
  <trd lng="cs">nenegativn&iacute;</trd>
  <trd lng="de">nichtnegativ</trd>
  <trd lng="en">non-negative</trd>
  <trd lng="fr">non n&eacute;gatif</trd>
  <trd lng="hu">nemnegat&iacute;v</trd>
  <trd lng="nl">niet-negatief</trd>
  <trd lng="pl">nieujemny</trd>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nie negat&iacute;vny</trd>
</drv>
</art>
