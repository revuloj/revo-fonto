<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>negativ</rad>/a
</kap>
<drv mrk="negati.0a">
<kap><tld/>a</kap>
<snc num="1">
<dif>
Nea: <ekz><tld/>a vorto;
</ekz> <ekz><tld/>a respondo;
</ekz> <ekz><tld/>a (&ccirc;ion malaprobanta sed proponanta nenian efikan 
solvon) kritiko.
</ekz>
</dif>
</snc>
<snc num="2">
<dif>
Nula.
<!-- Nekomprenebla difino (Marc Bavant) -->
</dif>
</snc>
<snc num="3">
<uzo tip="fak">ELE</uzo>
<uzo tip="fak">FOT</uzo>
<uzo tip="fak">MAT</uzo>
<dif>
Malpozitiva.
</dif>
</snc>
</drv>

   <drv mrk="negati.0o">
     <kap><tld/>o</kap>
     <snc mrk="negati.0o.FOT">
       <uzo tip="fak">FOT</uzo>
       <dif>
         <tld lit="N"/>a bildo.
       </dif>
     </snc>

     <snc mrk="negati.0o.neuxtriganto">
       <uzo tip="fak">MAT</uzo>
       <dif>
         <ref cel="neuxtr.0iganto.MAT">Ne&ubreve;triganto</ref> rilate
         al adicie signata <ref cel="operac.0o.MAT">operacio</ref>.
       </dif>
       <trd lng="fr">oppos&eacute;</trd>
     </snc>
   </drv>
</art>

