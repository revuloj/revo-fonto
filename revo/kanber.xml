  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="kanber.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="auxstr1.A0o.sxtato">A&ubreve;stralio</ref>:
      <ekz>
        esperantistoj surprizi&gcirc;is, kiam ili vidis en Okcidenta A&ubreve;stralio la ciferplaton
        <frm>UEAxxx</frm>, kaj en <tld/>o iu ricevis la platon <frm>YAH 000</frm>
        <fnt>
          <bib>Monato</bib>
          <aut>Margaret Chaldecott</aut>
          <vrk><url ref="https://www.monato.be/2011/010497.php"
              >Pri UEA kaj BOOZ</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        aktoroj el Novsudkimrujo kaj unu el <tld/>o
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=aazgOkg1bcM"
              >Adelajdo: teatra&jcirc;o de Novsudkimrujo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Canberra</trd>
    <trd lng="en">Canberra</trd>
    <trd lng="fr">Canberra</trd>
  </snc>
</drv>

