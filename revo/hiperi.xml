<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hiperik</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="hiperi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hiperi.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Hypericum</trd>),
      kies floroj, infuzitaj en oleo, estas uzataj kontra&ubreve; vundoj:
      <ekz>
        la brando havis almiksa&jcirc;on de <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/hiperik.jpg">
    floroj de <tld/>o
  </bld>
  <trd lng="de">Johanniskraut</trd>
</drv>
<drv mrk="hiperi.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Hypericaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Johanniskrautgew&auml;chse</trd>
</drv>
</art>
