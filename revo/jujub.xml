<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jujub</rad>/o
</kap>
<drv mrk="jujub.0o">
  <kap><tld/>o</kap>
  <snc mrk="jujub.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref>
      de <tld/>arbo, iom simila al <ref cel="daktil.0o">daktilo</ref>.
    </dif>
    <trd lng="be">&c_ju;&c_ju;&c_b;&c_a;</trd>
    <trd lng="ca">g&iacute;njol</trd>
    <trdgrp lng="de">
      <trd>Jujube</trd>,
      <trd>rote Brustbeere</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jujubadatolya</trd>,
      <trd>zsid&oacute;t&ouml;visbogy&oacute;</trd>
    </trdgrp>
    <trd lng="nl">jujube</trd>
    <trdgrp lng="pl">
    <trd>g&lstroke;o&zdot;yna</trd>,
    <trd>jujuba</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja; &c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="jujub.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Suko ekstraktata el zizifo.
    </dif>
    <trd lng="hu">jujubal&eacute;</trd>
    <trd lng="nl">jujubesap</trd>
  </snc>
  <trd lng="cs">jujuba</trd>
  <trd lng="en">jujube</trd>
  <trd lng="fr">jujube</trd>
  <trd lng="pt">jujuba</trd>
  <trd lng="sk">jujuba</trd>
</drv>

<drv mrk="jujub.0arbo0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de zizifo (<trd lng="la">Ziziphus jujuba</trd>),
      <ekz>
        tiam en la bieno oni plantis multajn <tld/>-arbojn kaj pro tio oni nomis &gcirc;in Jujuba
        <fnt>
          <bib>Monato</bib>
          <aut>Hoikso</aut>
          <vrk><url ref="http://www.monato.net/2015/011205.php">Vizito al Zaozhuang</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>arbo <klr>[&#8230;]</klr> originas el norda kaj nordorienta 
        &Ccirc;inio <klr>[&#8230;]</klr>, &gcirc;iaj fruktoj (<tld/>oj) estas malgrandaj, ovalaj kaj man&gcirc;eblaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=377593">Jujubarbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">ginjoler</trd>
  <trd lng="cs">cicimek datlov&yacute;</trd>
  <trdgrp lng="de">
    <trd>Jujubenbaum</trd>,
    <trd>Judendorn</trd>
  </trdgrp>
  <trd lng="en">jujube</trd>
  <trd lng="fr">jujubier</trd>
  <trdgrp lng="hu">
    <trd>zsid&oacute;t&ouml;vis</trd>,
    <trd>jujuba(fa)</trd>
  </trdgrp>
  <trd lng="nl">jujubeboom</trd>
  <trdgrp lng="pl">
  <trd>g&lstroke;o&zdot;yna <klr>pospolita</klr></trd>,
  <trd>jujuba <klr>pospolita</klr></trd>
  </trdgrp>
  <trd lng="pt">jujuba</trd>
  <trdgrp lng="ru">
    <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
    <trd>&c_u;&c_n;&c_a;&c_b;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">jujuba &ccaron;&iacute;nska</trd>
  <adm>
     La vorto "zizifo" ne estas en REVO. Male al la difino
     en PV, laux PIV2, "jujubo" kaj "zizifo" ne sinonimas.
     <aut>MB</aut>
   </adm>
</drv>
</art>
