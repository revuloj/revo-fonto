<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jujub.xml,v 1.26 2023/12/03 07:09:18 revo Exp $">
<kap>
  <rad>jujub</rad>/o
</kap>
<drv mrk="jujub.0o">
  <kap><tld/>o</kap>
  <snc mrk="jujub.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref>
      de <tld/>arbo, iom simila al <ref cel="daktil.0o">daktilo</ref>.
    </dif>
    <bld lok="&WCU;/thumb/3/3f/Ziziphus_jujuba_%28California%29.jpg/320px-Ziziphus_jujuba_%28California%29.jpg" prm="CC BY-SA 2.0"><tld/>oj de Ziziphus jujuba, kamparana bazaro de Berkeley, CA

        <fnt>
          <aut>Franco Folini,
          https://www.flickr.com/photos/livenature/11302767225/in/photolist-idMCUZ</aut>
          <lok><url ref="&WCW;/File:Ziziphus_jujuba_(California).jpg">&WCW;/File:Ziziphus_jujuba_(California).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_ju;&c_ju;&c_b;&c_a; <klr>(&c_ja;&c_g;&c_a;&c_d;&c_a;)</klr></trd>
    <trd lng="ca">g&iacute;njol</trd>
    <trdgrp lng="de">
      <trd>Jujube</trd>,
      <trd>rote Brustbeere</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jujubadatolya</trd>,
      <trd>zsid&oacute;t&ouml;visbogy&oacute;</trd>
    </trdgrp>
    <trd lng="nl">jujube</trd>
    <trdgrp lng="pl">
    <trd>g&lstroke;o&zdot;yna</trd>,
    <trd>jujuba</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja; &c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
<!--
  <snc mrk="jujub.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Suko ekstraktata el tiuj fruktoj.
    </dif>
    <trd lng="hu">jujubal&eacute;</trd>
    <trd lng="nl">jujubesap</trd>
  </snc>

  <adm>
     Male al la difino en PV, laux PIV2, "jujubo" kaj "zizifo" ne sinonimas.
     <aut>MB</aut>
   </adm>

-->
  <trd lng="cs">jujuba</trd>
  <trd lng="en">jujube</trd>
  <trd lng="fr">jujube</trd>
  <trdgrp lng="ja">
    <trd>&#x30CA;&#x30C4;&#x30E1;  <pr>&#x306A;&#x3064;&#x3081;</pr></trd>,
    <trd>&#x30AD;&#x30E3;&#x30F3;&#x30C7;&#x30FC;</trd>,
    <trd>&#x30BC;&#x30EA;&#x30FC;</trd>
  </trdgrp>
  <trd lng="pt">jujuba</trd>
  <trd lng="sk">jujuba</trd>
  <trd lng="uk">&c_ju;&c_ju;&c_b;&c_a; (&c_ja;&c_g;&c_o;&c_d;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x67A3; <pr>z&#x1CE;o</pr></trd>,
    <trd>&#x67A3;&#x5B50; <pr>z&#x1CE;ozi</pr></trd>,
    <trd>&#x7D05;&#x68D7; <pr>h&#xF3;ngz&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="jujub.0arbo0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="zizif.0o">zizifo</ref> (<trd lng="la">Ziziphus jujuba</trd>),
      <ekz>
        tiam en la bieno oni plantis multajn <tld/>-arbojn kaj pro tio oni nomis &gcirc;in
        <nom>Jujuba</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Hoikso</aut>
          <vrk><url ref="&Monato;/2015/011205.php">Vizito al Zaozhuang</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>arbo <klr>[&#x2026;]</klr> originas el norda kaj nordorienta 
        &Ccirc;inio <klr>[&#x2026;]</klr>, &gcirc;iaj fruktoj (<tld/>oj) estas malgrandaj, ovalaj kaj man&gcirc;eblaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=377593">Jujubarbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d5/Ziziphus_jujuba-Jujubier_commun_2-20160804.jpg/640px-Ziziphus_jujuba-Jujubier_commun_2-20160804.jpg" prm="CC BY-SA 4.0">bran&ccirc;o de <tld/>ujo (Ziziphus jujuba)
        <fnt>
          <aut>Daniel Villafruela, propra verko</aut>
          <lok><url ref="&WCW;/File:Ziziphus_jujuba-Jujubier_commun_2-20160804.jpg">&WCW;/File:Ziziphus_jujuba-Jujubier_commun_2-20160804.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ju;&c_ju;&c_b;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="ca">ginjoler</trd>
  <trd lng="cs">cicimek datlov&yacute;</trd>
  <trdgrp lng="de">
    <trd>Jujubenbaum</trd>,
    <trd>Judendorn</trd>
  </trdgrp>
  <trd lng="en">jujube</trd>
  <trd lng="fr">jujubier</trd>
  <trdgrp lng="hu">
    <trd>zsid&oacute;t&ouml;vis</trd>,
    <trd>jujuba(fa)</trd>
  </trdgrp>
  <trd lng="ja">&#x30CA;&#x30C4;&#x30E1;  <pr>&#x306A;&#x3064;&#x3081;</pr></trd>
  <trd lng="nl">jujubeboom</trd>
  <trdgrp lng="pl">
  <trd>g&lstroke;o&zdot;yna <klr>pospolita</klr></trd>,
  <trd>jujuba <klr>pospolita</klr></trd>
  </trdgrp>
  <trd lng="pt">jujuba</trd>
  <trdgrp lng="ru">
    <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
    <trd>&c_u;&c_n;&c_a;&c_b;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">jujuba &ccaron;&iacute;nska</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_i;&c_z;&c_i;&c_f;&c_u;&c_s;</trd>,
    <trd>&c_ju;&c_ju;&c_b;&c_a; (&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x68EF; <pr>r&#x11B;n</pr></trd>,
    <trd>&#x679C;&#x5473;&#x80F6;&#x7CD6; <pr>gu&#x1D2;w&#xE8;iji&#x101;ot&#xE1;ng</pr></trd>,
    <trd>&#x67A3;&#x6811; <pr>z&#x1CE;osh&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
