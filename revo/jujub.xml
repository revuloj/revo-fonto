<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>jujub</rad>/o
</kap>
<drv mrk="jujub.0o">
<kap><tld/>o</kap>
<uzo tip="fak">BOT</uzo>
<snc num="1">
<dif>
Frukto de zizifo. 
</dif>
<trdgrp lng="ru">
  <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
  <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja; &c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
</trdgrp>
</snc>
<snc num="2">
<dif>
Suko ekstraktata el zizifo. 
</dif>
</snc>
</drv>
<drv mrk="jujub.0arbo">
<kap><tld/>arbo</kap>
<snc>
<dif>
Vulgara nomo de zizifo.  
</dif>
</snc>
<trdgrp lng="ru">
  <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
  <trd>&c_u;&c_n;&c_a;&c_b;&c_i;</trd>
</trdgrp>
</drv>
</art>
