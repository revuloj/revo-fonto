<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>jujub</rad>/o
</kap>
<drv mrk="jujub.0o">
<kap><tld/>o</kap>
<uzo tip="fak">BOT</uzo>
<snc num="1">
<dif>
Frukto de zizifo.
</dif>
<trdgrp lng="de">
   <trd>Jujube</trd>,
   <trd>rote Brustbeere</trd></trdgrp>
<trdgrp lng="hu">
   <trd>jujubadatolya</trd>,
   <trd>zsid&oacute;t&ouml;visbogy&oacute;</trd></trdgrp>
<trd lng="nl">jujube</trd>
<trdgrp lng="ru">
  <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
  <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja; &c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
</trdgrp>
</snc>
<snc num="2">
<dif>
Suko ekstraktata el zizifo.
</dif>
<trd lng="hu">jujubal&eacute;</trd>
<trd lng="nl">jujubesap</trd>
</snc>
<trd lng="pt">jujuba</trd>
</drv>
<drv mrk="jujub.0arbo">
<kap><tld/>arbo</kap>
<snc>
<dif>
Vulgara nomo de zizifo.
</dif>
</snc>
<trdgrp lng="de">
   <trd>Jujubenbaum</trd>,
   <trd>Judendorn</trd></trdgrp>
<trdgrp lng="hu">
   <trd>zsid&oacute;t&ouml;vis</trd>,
   <trd>jujuba(fa)</trd></trdgrp>
<trd lng="nl">jujubeboom</trd>
<trd lng="pt">jujuba</trd>
<trdgrp lng="ru">
  <trd>&c_ju;&c_ju;&c_b;&c_a;</trd>,
  <trd>&c_u;&c_n;&c_a;&c_b;&c_i;</trd>
</trdgrp>
</drv>
</art>
