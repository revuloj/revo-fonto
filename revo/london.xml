<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: london.xml,v 1.17 2020/11/02 11:11:03 revo Exp $">
    <kap>
     <ofc>*</ofc><ofc>7</ofc>
     <rad>London</rad>/o
   </kap>
<drv mrk="london.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="london.0o.urbo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref> 
      kaj plej grava urbo de <ref tip="malprt" cel="brit.B0ujo">Britujo</ref> 
      (<frm>0&#730;04'W, 51&#730;30'N</frm>):
      <ekz>
        post paso de unu semajno la ju&gcirc;isto estis vokita <tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la plej valoraj perloj <klr>[&#8230;]</klr> estas nomataj: Parizo, <tld/>o, Vieno,
        Neapolo
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi veturas <tld/>on?
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        estimata reprezentanto de la urbo <tld/>o, karaj samideanoj! &#8208; estas al mi tre
        agrable, ke mi havas nun la okazon saluti la britan popolon en &gcirc;ia granda
        &ccirc;efurbo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>.
      </ekz>
   </dif>
   <trd lng="be">&c_L;&c_o;&c_n;&c_d;&c_a;&c_n;</trd>
   <trd lng="bg">&c_L;&c_o;&c_n;&c_d;&c_o;&c_n;</trd>
   <trd lng="br">Londrez</trd>
   <trd lng="de">London</trd>
   <trd lng="en">London</trd>
   <trd lng="es">Londres</trd>
   <trd lng="fr">Londres</trd>
   <trd lng="hu">London</trd>
   <trd lng="lat">Londinium</trd>
   <trd lng="nl">Londen</trd>
   <trd lng="pl">Londyn</trd>
   <trd lng="pt">Londres</trd>
   <trd lng="ru">&c_L;&c_o;&c_n;&c_d;&c_o;&c_n;</trd>
  </snc>
  <trd lng="cs">Lond&yacute;n</trd>
  <trd lng="sk">Lond&yacute;n</trd>
</drv>

<drv mrk="london.0a">
  <kap>
    <tld lit="l"/>a
  </kap>
  <snc mrk="london.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la &ccirc;efa <tld lit="l"/>a flughaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2003/008052.html"
              >Taglibro de l' (anta&ubreve;)milito ... el la lando de &Scirc;ekspiro</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Lernejo de Ekonomika kaj Politika Sciencoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2005/008521.php"
              >Pledo por mondcivitana partio</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a Naturhistoria Muzeo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="https://www.monato.be/2008/009438.php"
              >Fosilioj en suda Germanio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="l"/>a vaksfigura muzeo de <nom>Madame Tussaud</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2011/010363.php"
              >Vakso vokas ...</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">EKON</uzo>
        <ind><tld/>a Akciborso</ind>.
        <trd lng="br">yalc'h Londrez</trd>
        <trdgrp lng="de">
           <trd>Londoner <ind>Aktienb&ouml;rse</ind></trd>,
           <trd>Londoner <ind>Effektenb&ouml;rse</ind></trd>
        </trdgrp>   
        <trdgrp lng="en">
           <trd>London <ind>Stock Exchange</ind></trd>,
           <trd>LSE</trd>,
           <trd>Lombard street</trd>
        </trdgrp>
        <trd lng="es"><ind>Bolsa de Valores</ind> de Londres</trd>
        <trdgrp lng="hu">
           <trd>Londoni <ind>&Eacute;rt&eacute;kt&odblac;zsde</ind></trd>,
           <trd>LSE</trd>,
        </trdgrp>
        <trd lng="fr">bourse de Londres</trd>
        <trd lng="nl">Londense beurs</trd>
        <trd lng="pl">Gie&lstroke;da Londy&nacute;ska</trd>
        <trd lng="ru">&c_L;&c_o;&c_n;&c_d;&c_o;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_f;&c_o;&c_n;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_b;&c_i;&c_r;&c_zh;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>londinense</trd>,
    <trd>de Londres</trd>
  </trdgrp>   
</drv>

</art>

