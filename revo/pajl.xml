<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pajl.xml,v 1.41 2025/10/22 16:42:42 revo Exp $">
<kap>
  <ofc>*</ofc><rad>pajl</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="pajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pajl.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Materialo el sekaj sengrajnigitaj trunketoj de <ref tip="malprt"
      cel="gren.0o.planto">greno</ref>:
      <ekz>
        sekala, riza <tld/>o;
      </ekz>
      <ekz>
        garbo da <tld/>o;
      </ekz>
      <ekz>
        <klr>[li]</klr> donis <tld/>on kaj fura&gcirc;on al la kameloj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>okolora
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 3a</lok></fnt>;
      </ekz>
      <ekz>
        kolekti pecojn da <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Via kolero <klr>[&#8230;]</klr> forbruligas ilin kiel <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        en angulo <klr>[&#8230;]</klr> ku&scirc;is <tld/>o kaj litkovriloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ose&gcirc;oj, korekte liniigitaj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e remburita
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>kabanoj <klr>(<tld/>tegitaj)</klr>
        <fnt>
          <bib>LSB</bib>
          <lok>Angkor'a&ubreve;!</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel dronanto kaptas <tld/>eton <klr>(vd <ref cel="pajl.0ero">pajlero</ref>)</klr>,
        tiel li ser&ccirc;as savon en tritaga limtempo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen fajro ne brulas e&ccirc; <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fojn.0o">fojno</ref>,
      <ref cel="furagx.0o">fura&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_o;&c_m;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_m;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_naro;&t_ga;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>kolo</trd>,
    <trd>plouz</trd>
  </trdgrp>
  <trd lng="ca">palla</trd>
  <trd lng="cs">sl&aacute;ma</trd>
  <trd lng="de">Stroh</trd>
  <trd lng="en">straw <klr>(dried grass)</klr></trd>
  <trd lng="es">paja</trd>
  <trd lng="fr">paille</trd>
  <trd lng="hu">szalma</trd>
  <trd lng="id">jerami</trd>
  <trd lng="ja">&#12431;&#12425;</trd>
  <trd lng="nl">stro</trd>
  <trd lng="no">str&aring;</trd>
  <trd lng="pl">s&lstroke;oma</trd>
  <trd lng="pt">palha</trd>
  <trd lng="ro">pai</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_o;&c_m;&c_a;</trd>
  <trd lng="sk">slama</trd>
  <trd lng="sv">halm</trd>
  <trd lng="uk">&c_s;&c_o;&c_l;&c_o;&c_m;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#21005; <pr>ch&uacute;</pr></trd>,
    <trd>&#33467; <pr>ch&uacute;</pr></trd>,
    <trd>&#31296;&#33609; <pr>g&#468;c&#462;o</pr></trd>,
    <trd>&#35895;&#33609; <pr>g&#468;c&#462;o</pr></trd>,
    <trd>&#31224;&#31174; <pr>ji&emacron;g&#462;n</pr></trd>,
    <trd>&#31224;&#31240; <pr>ji&emacron;g&#462;n</pr></trd>,
    <trd>&#40613;&#31224; <pr>m&agrave;iji&emacron;</pr></trd>,
    <trd>&#40614;&#31224; <pr>m&agrave;iji&emacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pajl.0a">
  <kap><tld/>a</kap>
  <snc mrk="pajl.0a.el_pajlo">
    <dif>
      Farita el <tld/>o:
      <ekz>
        <tld/>a matraco, substerno
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;apelo
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda stalo kun plata <tld/>a tegmento
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda <tld/>a apogse&gcirc;eto
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        se&gcirc;oj kun <tld/>a reto <klr>[&#8230;]</klr> kavigita
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  <trd lng="en">straw</trd>
  <trd lng="id">... <ind>jerami</ind></trd>
  </snc>
  <snc mrk="pajl.0a.FIG">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      &Scirc;ajniga, figuranta ion, kio &gcirc;i ne estas,
      kiel <tld/>opupo figuras homon:
      <ekz>
        tiu partio lin <klr>[&#8230;]</klr>
        forigis de &ccirc;iuj publikaj postenoj,
        e&ccirc; de pure formalaj kaj <tld/>aj
        <fnt>
          <aut>V. O&scaron;lak</aut>,
          <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hom.pajlo0o.FRAZ"><tld/>ohomo</ref>
    </refgrp>
    <trd lng="en">straw <klr>(imaginary but presented as real)</klr></trd>
    <trdgrp lng="es">
      <trd>pajizo</trd>,
      <trd>de paja</trd>
    </trdgrp>
    <trd lng="fr">de paille</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_a;&c_m;&c_ja;&c_n;&c_y;</trd>
  <trd lng="cs">slam&ecaron;n&yacute;</trd>
  <trd lng="de">Stroh-</trd>
  <trd lng="pl">s&lstroke;omiany</trd>
  <trd lng="ro">paie</trd>
  <trd lng="sk">slamen&yacute;</trd>
  <trd lng="uk">&c_s;&c_o;&c_l;&c_o;&c_m;&#8217;&c_ja;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="pajl.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="pajl.0ero.KOMUNE">
    <dif>
      Unuopa peco de grena tigo:
      <ekz>
        la ventego forportas ilin kiel <tld/>erojn
        <fnt><bib>MT</bib>, <lok>&Jes; 40:24</lok></fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo ma&ccirc;is <tld/>eron kaj rigardis trans la ondojn de la plugkampoj
        <fnt>
          <bib>VRA</bib>
          <lok>Gustav Sandgren</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bankrotinta registaro <klr>[&#8230;]</klr> avide kaptas populisman <tld/>eron
        <klr>(kiel lastan rimedon)</klr>, kiam &gcirc;i sin prezentas
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 7 (105)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eroj estas tran&ccirc;ataj al diverslongaj pecetoj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2014/011226.php">Pendantaj artaj pajla&jcirc;oj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stopl.0o">stoplo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_o;&c_m;&c_ib;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">bri de palla</trd>
  <trd lng="cs">st&eacute;blo sl&aacute;my</trd>
  <trd lng="de">Strohhalm</trd>
  <trd lng="en">piece of <ind>straw</ind></trd>
  <trd lng="es">brizna de paja</trd>
  <trdgrp lng="fr">
    <trd>f&eacute;tu</trd>,
    <trd>brin de paille</trd>
  </trdgrp>
  <trd lng="hu">szalmasz&aacute;l</trd>
  <trd lng="id">helai jerami</trd>
  <trd lng="ja">&#19968;&#26412;&#12398;&#12431;&#12425;  <pr>&#12402;&#12392;&#12411;&#12435;&#12398;&#12431;&#12425;</pr></trd>
  <trd lng="nl">strohalm</trd>
  <trd lng="no">halmstr&aring;</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;omka</trd>,
    <trd>&zacute;d&zacute;b&lstroke;o</trd>
  </trdgrp>
  <trd lng="ro">pai</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_o;&c_m;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">steblo slamy</trd>
  <trd lng="sv">halmstr&aring;</trd>
  <trd lng="uk">&c_s;&c_o;&c_l;&c_o;&c_m;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#31224;&#31174; <pr>ji&emacron;g&#462;ng</pr></trd>,
    <trd>&#31224;&#31240; <pr>ji&emacron;g&#462;ng</pr></trd>,
    <trd>&#40613;&#31240; <pr>m&agrave;ig&#462;n</pr></trd>,
    <trd>&#40614;&#31174; <pr>m&agrave;ig&#462;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pajl.0osxtopi">
  <kap><tld/>o&scirc;topi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="rembur.0i.KOMUNE">Remburi</ref> per <tld/>o felon
      de mortinta besto por konservi al &gcirc;i aspekton de vivulo:
      <ekz>
        <tld/>o&scirc;topitaj birdoj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katojn la piuloj donis al la para&scirc;itoj por balzamado a&ubreve; <tld/>o&scirc;topado, kaj
        poste ili reportis ilin hejmen, kiel respektatajn sanktajn resta&jcirc;ojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_s;&c_a;&c_l;&c_o;&c_m;&c_a;&c_j;</ind></trd>
  <trd lng="br">plouza&ntilde;</trd>
  <trd lng="ca">dissecar</trd>
  <trd lng="de">ausstopfen</trd>
  <trd lng="en">stuff <klr>(taxidermy)</klr></trd>
  <trd lng="es">disecar</trd>
  <trd lng="fr">empailler</trd>
  <trd lng="hu">kit&ouml;m<klr tip="ind"> (szalm&aacute;val)</klr></trd>
  <trd lng="id"><baz>opset</baz>mengopset<klr tip="amb"> (dgn jerami)</klr></trd>
  <trd lng="ja">&#12431;&#12425;&#12434;&#35440;&#12417;&#12427;  <pr>&#12431;&#12425;&#12434;&#12388;&#12417;&#12427;</pr></trd>
  <trd lng="nl">opzetten<klr tip="ind"> (v.dieren)</klr></trd>
  <trd lng="pl">wypycha&cacute;  <ind>s&lstroke;om&aogonek;</ind></trd>
  <trd lng="ro">chestii cu  <ind>paie</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_o;&c_l;&c_o;&c_m;&c_o;&c_j;</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i; &c_s;&c_o;&c_l;&c_o;&c_m;&c_o;&c_ju;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i; &c_ch;&c_u;&c_ch;&c_je;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34917;&#30333;&#20809; <pr>b&#468;b&aacute;igu&amacron;ng</pr></trd>,
    <trd>&#35036;&#30333;&#20809; <pr>b&#468;b&aacute;igu&amacron;ng</pr></trd>,
    <trd>&#22443;&#34924; <pr>di&agrave;nch&egrave;n</pr></trd>,
    <trd>&#22666;&#35183; <pr>di&agrave;nch&egrave;n</pr></trd>
  </trdgrp>
</drv>
</art>
