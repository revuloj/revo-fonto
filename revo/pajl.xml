<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>pajl</rad>/o</kap>
<drv mrk="pajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Seka sengrajnigita trunketo de greno:
      <ekz>
        sekala, riza <tld/>o;
      </ekz>
      <ekz>
        garbo da <tld/>o;
      </ekz>
      <ekz>
        <tld/>okolora;
      </ekz>
      <ekz>
        <tld/>a matraco, tegmento;
      </ekz>
      <ekz>
        se&gcirc;oj kun kavigita <tld/>a reto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sl&aacute;ma</trd>
  <trd lng="de">Stroh</trd>
  <trd lng="en">straw</trd>
  <trd lng="nl">stro</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_o;&c_m;&c_a;</trd>
</drv>

<drv mrk="pajl.0osxtopi">
  <kap><tld/>o&scirc;topi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Remburi per <tld/>o felon de mortinta besto por konservi al &gcirc;i
      aspekton de vivulo.
    </dif>
  </snc>
  <trd lng="nl">opzetten<klr tip="ind"> (v.dieren)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_o;&c_l;&c_o;&c_m;&c_o;&c_j;</ind></trd>
</drv>
</art>
