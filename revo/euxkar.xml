<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>2</ofc>
      <rad>e&ubreve;karisti</rad>/o
    </kap>
    <drv mrk="euxkar.0o">
      <kap><tld/>o, e&ubreve;&hcirc;aristio</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Kristana <ref tip="super" cel="sakram.0o.kri">sakramento</ref>,
          rememoriganta la <ref cel="mangx.sankta-0o">Sanktan Man&gcirc;on</ref>,
          dum kiu okazas konsekrado de pano kaj vino.
        </dif>
        <refgrp tip="vid">
          <ref cel="komuni.0o">komunio</ref>.
        </refgrp>
      </snc>
      <trd lng="fr">eucharistie</trd>
      <trd lng="ru">&c_je;&c_v;&c_h;&c_a;&c_r;&c_i;&c_s;&c_t;&c_i;&c_ja;</trd>
      <trd lng="sv">nattvard</trd>
    </drv>
  </art>
