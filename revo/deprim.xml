<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: deprim.xml,v 1.32 2025/10/27 20:32:23 revo Exp $">
<kap>
  <rad>deprim</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="deprim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="deprim.0i.PSI">
    <dif>
      Forte, da&ubreve;re <ref tip="super" cel="vigl.mal0igi">malvigligi</ref>
      kaj <ref tip="super" cel="kuragx.mal0igi">senkura&gcirc;igi</ref>:
      <ekz>
        li estas <tld/>ita de sia malsukceso;
      </ekz>
      <ekz>
        malgra&ubreve; mia <tld/>ita humoro mi ekridetis
        <fnt>
          <bib>VaK</bib>
          <lok>Kvina &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aflikt.0i">aflikti</ref>,
      <ref cel="cxagre.0i">&ccirc;agreni</ref>,
      <ref cel="frustr.0i">frustri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;
    <ind>&c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ju;</ind></trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
    <ind>&c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>deprimir</trd>,
    <trd>desanimar</trd>,
    <trd>descoratjar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>deprimovat</trd>,
    <trd>skli&ccaron;ovat</trd>,
    <trd>t&iacute;snit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>deprimieren</trd>,
    <trd>herunterziehen</trd>,
    <trd>entmutigen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>depress</trd>
    <trd>dispirit</trd>
    <trd>dishearten</trd>
    <trd>demoralize</trd>
  </trdgrp>
  <trd lng="es">deprimir</trd>
  <trd lng="fr">d&eacute;primer<klr tip="ind"> (tr.)</klr></trd>
  <trd lng="he">&mem;&#x5B0;&dalet;&vav;&#x5BC;&kaf;&#x5BC;&#x5B8;&alef;</trd>
  <trdgrp lng="hu">
    <trd>nyomaszt</trd>,
    <trd>elkedvetlen&iacute;t</trd>,
    <trd>deprim&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x610F;&#x6C17;&#x6D88;&#x6C88;&#12373;&#x305B;&#x308B; 
    <pr>&#12356;&#x304D;&#12375;&#12423;&#12358;&#12385;&#12435;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x843D;&#12385;&#x8FBC;&#x307E;&#x305B;&#x308B; 
    <pr>&#x304A;&#12385;&#12371;&#x307E;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>deprimeren</trd>,
    <trd>ontmoedigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>deprymowa&cacute;</trd>,
    <trd>przygn&eogonek;bia&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
  &c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ju;</trd>
  <trdgrp lng="sk">
    <trd>deprimova&tcaron;</trd>,
    <trd>kru&scaron;i&tcaron;</trd>,
    <trd>zroni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_je;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_n;&c_ib;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F7F;&#x5FE7;&#x90C1; <pr>sh&#x1D0;y&omacron;uy&ugrave;</pr></trd>,
    <trd>&#x4F7F;&#x6CAE;&#x4E27; <pr>sh&#x1D0;j&#x1D4;s&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="deprim.0o">
  <kap><tld/>o</kap>
  <snc mrk="deprim.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="lst" cel="san.mensmal0o.MED" lst="voko:malsanoj">Mensmalsano</ref>
      ka&ubreve;zanta da&ubreve;rajn malviglon, lacon, seniniciaton, anta&ubreve;timon:
      <ekz>
        en la konferenco 12 fakuloj el ok landoj traktis plurajn
        aspektojn
        de la <tld/>o, interalie emon al la malsano, memmortigojn inter
        maljunuloj kaj la efikecon de novaj kuraciloj kaj
        laborkondi&ccirc;oj
        <fnt>
          <aut>P. S. Viana</aut>,
          <vrk><url ref="&Monato;/2001/007239.html">Deprimo</url></vrk>,
          <bib>Monato</bib>,
          <lok>2001/06, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frustr.0o">frustro</ref>,
      <ref cel="indife.0eco">indiferenteco</ref>,
      <ref cel="splen.0o">spleno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>depressi&oacute;</trd>,
    <trd>des&agrave;nim</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>depression</trd>
    <trd>despondancy</trd>
  </trdgrp>
  <trd lng="es">depresi&oacute;n<klr> (psicol&oacute;gica)</klr></trd>
  <trd lng="be">&c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="de">Depression</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;pression<klr> (psychol.)</klr></trd>,
    <trd>d&eacute;prime</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyomott <ind>hangulat</ind></trd>,
    <trd>kedvtelens&eacute;g</trd>,
    <trd>deprim&aacute;lts&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x610F;&#x6C17;&#x6D88;&#x6C88;  <pr>&#12356;&#x304D;&#12375;&#12423;&#12358;&#12385;&#12435;</pr></trd>,
    <trd>&#24962;&#x9B31;  <pr>&#12422;&#12358;&#12358;&#12388;</pr></trd>,
    <trd>&#x9B31;&#x72B6;&#x614B;  <pr>&#12358;&#12388;&#12376;&#12423;&#12358;&#x305F;&#12356;</pr></trd>,
    <trd>&#x9B31;&#x75C5;  <pr>&#12358;&#12388;&#12403;&#12423;&#12358;</pr></trd>,
    <trd>&#25233;&#x9B31;&#x75C7;  <pr>&#12424;&#x304F;&#12358;&#12388;&#12375;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>neerslachtigheid</trd>,
    <trd>depressie<klr tip="ind"> (neerslachtigheid)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przygn&eogonek;bienie</trd>,
    <trd>depresja</trd>
  </trdgrp>
  <trd lng="ro">depresie</trd>
  <trdgrp lng="sk">
    <trd>depresia</trd>,
    <trd>skleslos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>deprese <klr>(du&scaron;evn&iacute;)</klr></trd>,
    <trd>skleslost</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_je;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_n;&c_ib;&c_ch;&c_je;&c_n;&c_i;&c_j; (&c_p;&c_s;&c_i;&c_h;&c_ib;&c_ch;&c_n;&c_i;&c_j;) &c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5FE7;&#x90C1;&#x75C7; <pr>y&omacron;uy&ugrave;zh&egrave;ng</pr></trd>,
    <trd>&#x7CBE;&#x795E;&#x840E;&#38753; <pr>j&imacron;ngsh&eacute;nw&emacron;im&iacute;</pr></trd>,
    <trd>&#25233;&#x90C1; <pr>y&igrave;y&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="deprim.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="deprim.0igxi.PSI">
    <dif>
      Sperti da&ubreve;ran senkura&gcirc;on, malfervoron kaj sekvan
      neagemon:
      <ekz>
        vi sentas vin bone nun, sed memoru, ke nun via sango estas plena
        de adrenalino pro la anestezilo, sed poste vi eble
        <tld/>i&gcirc;os
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_w;
    <ind>&c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ju;</ind></trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_c;&c_a; &c_w;
    <ind>&c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="ca">deprimir-se</trd>
  <trd lng="de"><ind>entmutigt</ind> werden</trd>
  <trdgrp lng="en">
    <trd>get depressed</trd>
    <trd>become despondant</trd>
    <trd>become disheartened</trd>
    <trd>become demoralized</trd>
  </trdgrp>
  <trd lng="es">deprimirse</trd>
  <trd lng="fr">d&eacute;primer<klr> (intr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>elkedvetlenedik</trd>,
    <trd>deprim&aacute;l&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x610F;&#x6C17;&#x6D88;&#x6C88;&#12377;&#x308B;  <pr>&#12356;&#x304D;&#12375;&#12423;&#12358;&#12385;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x9B31;&#12293;&#12392;&#12377;&#x308B;  <pr>&#12358;&#12388;&#12293;&#12392;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>neerslachtig worden</trd>,
    <trd>ontmoedigd worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>popada&cacute; w <ind>przygn&eogonek;bienie</ind></trd>,
    <trd>wpada&cacute; w <ind>depresj&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_o;&c_b;&c_i;&c_t;&c_i;&c_s;&c_ja; &c_p;&c_r;&c_i;&c_g;&c_n;&c_ib;&c_ch;&c_je;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_ji;</trd>,
    <trd>&c_v;&c_p;&c_a;&c_s;&c_t;&c_i; &c_v; &c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="deprim.kontraux0ilo">
  <kap>kontra&ubreve;<tld/>ilo</kap>
  <snc mrk="deprim.kontraux0ilo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuracilo kontra&ubreve; <tld/>o:
      <ekz>
        mi komencis preni kontra&ubreve;<tld/>ilojn iompostiome, kaj
        tio mildigis miajn simptomojn
        <fnt>
          <aut>M. Beno&icirc;t</aut>,
          <vrk>Sindromo de Kronika Laceco</vrk>,
          <lok><url ref="http://www.esperanto.qc.ca/files/riverego/riverego-098.pdf">
          La Riverego, 2009-12</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_y;&c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib; &c_a;&c_d; &c_d;&c_e;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">antidepressiu</trd>
  <trd lng="de">Antidepressivum</trd>
  <trd lng="en">antidepressant</trd>
  <trd lng="fr">antid&eacute;presseur<klr> (subst.)</klr></trd>
  <trd lng="nl">antidepressiva</trd>
  <trd lng="pl">antydepresant</trd>
  <trdgrp lng="zh">
    <trd>&#25239;&#25233;&#x90C1;&#x836F;&#29289; <pr>k&agrave;ngy&igrave;y&ugrave;y&agrave;ow&ugrave;</pr></trd>,
    <trd>&#25239;&#25233;&#x90C1;&#x836F; <pr>k&agrave;ngy&igrave;y&ugrave;y&agrave;o</pr></trd>
  </trdgrp>
</drv>
</art>
