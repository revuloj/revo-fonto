<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>tekneci</rad>/o
</kap>
<drv mrk="tekneci.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
       atomnumero 43, atompezo 98,9063, simbolo Tc. Eksa nomo de la
       elemento estis masurio. &Ccirc;iuj izotopoj de tiu elemento estas
       radioaktivaj.
     </dif>
   </snc>
   <trd lng="be">&c_t;&c_e;&c_h;&c_n;&c_e;&c_c;&c_y;&c_j;</trd>       
   <trd lng="be">teknetiom</trd>
   <trd lng="de">Technetium</trd>   
   <trd lng="fr">techn&eacute;tium</trd>
   <trd lng="hu">techn&eacute;cium</trd>
   <trd lng="nl">technetium</trd>
   <trd lng="ru">&c_t;&c_je;&c_h;&c_n;&c_je;&c_c;&c_i;&c_j;</trd>
</drv>
</art>
