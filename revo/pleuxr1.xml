<art mrk="$Id: pleuxr1.xml,v 1.17 2024/02/17 07:52:16 revo Exp $">
<kap>
  <rad>ple&ubreve;rit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pleuxr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="pleuxr1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref tip="vid" cel="pleuxr.0o">ple&ubreve;ro</ref>:
      <ekz>
        akuta, senakva a&ubreve; seka, pusa <tld/>o;
      </ekz>
      <ekz>
        li ne povas malvarmumi sen ke <tld/>o minacas
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_w;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Pleuritis</trd>,
    <trd>Rippenfellentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="fa">&a_dh;&a_A;&a_t;&#x200C;&a_A;&a_l;&a_j;&a_n;&a_b;</trd>
  <trd lng="fr">pleur&eacute;sie</trd>
  <trdgrp lng="hu">
    <trd>mellh&aacute;rtyagyullad&aacute;s</trd>,
    <trd>pleuritis</trd>
  </trdgrp>
  <trd lng="id">pleuritis</trd>
  <trdgrp lng="nl">
    <trd>pleuritis</trd>,
    <trd>borstvliesontsteking</trd>
  </trdgrp>
  <trd lng="pl">op&lstroke;ucnej zapalenie</trd>
  <trdgrp lng="pt">
    <trd>pleurisia</trd>,
    <trd>pleurite</trd>,
    <trd>pleuris</trd>
  </trdgrp>
  <trd lng="ro">pleurezie</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_v;&c_r;&c_i;&c_t;</trd>
  <trd lng="uk">&#x43F;&#x43B;&c_je;&c_v;&c_r;&c_i;&c_t;</trd>
  <trd lng="zh">&#x80F8;&#x819C;&#x708E; <pr>xi&#x14D;ngm&#xF2;y&#xE1;n</pr></trd>
</drv>
</art>
