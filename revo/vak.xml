<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>vak</rad>/i
</kap>
<drv mrk="vak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti ne okupata, pp loko, ofico ktp:
      <ekz>
        li kondukis &scirc;in al <tld/>anta spaco anta&ubreve; la statuo;
      </ekz>
      <ekz>
        <tld/>anta se&gcirc;o;
      </ekz>
      <ekz>
        ne estas <tld/>(ant)a loko en nia oficejo;
      </ekz>
      <ekz>
        la prezidanteco <tld/>as;
      </ekz>
      <ekz>
        <tld/>anteco de lo&gcirc;ejo, de posteno.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="liber.0a">libera</ref>,
      <ref cel="dispon">disponebla</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_s;&c_v;&c_a;&c_b;&c_o;&c_d;&c_n;&c_y;&c_m;</ind>
      <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;</ind>
      <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_n;&c_je;&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>frei sein</trd>,
    <trd>unbesetzt sein</trd>,
    <trd>leer sein</trd>,
    <trd>vakant sein</trd>
  </trdgrp>
  <trd lng="en">vacate</trd>
  <trdgrp lng="es">
    <trd>estar vac&iacute;o</trd>,
    <trd>estar libre</trd>
  </trdgrp>
  <trd lng="fr">vacant (&ecirc;tre)</trd>
  <trdgrp lng="nl">
    <trd>vrij zijn</trd>,
    <trd>vacant zijn</trd>
  </trdgrp>
  <trd lng="pt">estar vago</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_n;&c_je;
&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_s;&c_v;&c_o;&c_b;&c_o;&c_d;&c_n;&c_y;&c_m;</ind> <klr tip="amb">(&c_o;
&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
