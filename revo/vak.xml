<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>vak</rad>/i
</kap>
<drv mrk="vak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti ne okupata, pp loko, ofico ktp:
      <ekz>
        li kondukis ŝin al <tld/>anta spaco antaŭ la statuo;
      </ekz>
      <ekz>
        <ind><tld/>anta seĝo</ind>;
        <trd lng="hu">üres szék</trd>
      </ekz>
      <ekz>
        ne estas <tld/>(ant)a loko en nia oficejo;
      </ekz>
      <ekz>
        la prezidanteco <tld/>as;
      </ekz>
      <ekz>
        <tld/>anteco de loĝejo, de posteno.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="liber.0a">libera</ref>,
      <ref cel="dispon">disponebla</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>быць
      <ind>свабодным</ind>
      <klr>(пра месца)</klr></trd>,
    <trd>быць
      <ind>вольным</ind>
      <klr>(пра месца)</klr></trd>,
    <trd>быць
      <ind>незанятым</ind></trd>,
    <trd>быць
      <ind>вакантным</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>frei sein</trd>,
    <trd>unbesetzt sein</trd>,
    <trd>leer sein</trd>,
    <trd>vakant sein</trd>
  </trdgrp>
  <trd lng="en">be vacant</trd>
  <trdgrp lng="es">
    <trd>estar vacío</trd>,
    <trd>estar libre</trd>
  </trdgrp>
  <trd lng="fr">vacant (être)</trd>
  <trdgrp lng="hu">
    <trd>üres</trd>,
    <trd>szabad <klr tip="ind">(nincs elfoglalva)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vrij zijn</trd>,
    <trd>vacant zijn</trd>
  </trdgrp>
  <trd lng="pt">estar vago</trd>
  <trdgrp lng="ru">
    <trd>быть <ind>не
занятым</ind></trd>,
    <trd>быть
<ind>вакантным</ind></trd>,
    <trd>быть
<ind>свободным</ind> <klr tip="amb">(о
месте)</klr></trd>
  </trdgrp>
</drv>
</art>
