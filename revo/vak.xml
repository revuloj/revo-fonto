<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vak.xml,v 1.27 2024/04/11 22:24:58 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>vak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vak.0i.KOMUNE">
    <uzo tip="klr">(pri loko, ofico ktp)</uzo>
    <dif>
      Esti ne okupata, esti <ref tip="sin" cel="liber.0a.vaka">libera</ref>,
      <ref tip="sin" cel="dispon.0ebla">disponebla</ref>:
      <ekz>
        la apartamento <klr>[&#8230;]</klr> estos <tld/>anta por la venonta lutempo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        apuda benko estas <tld/>anta
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi reiros al mia domo <klr>[&#8230;]</klr> kaj alveninte <klr>[&#8230;]</klr> trovas &gcirc;in
        <tld/>anta, balaita
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 12:44</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel mi, senforta maljunulo, batalu kontra&ubreve; tiu senlima potenco <klr>[&#8230;]</klr>,
        kaj li <tld/>is tiel el la fundo de senborda doloro
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la loko, kie &gcirc;i ankrumis, <tld/>is
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiom da postenoj por &gcirc;eneralaj direktoroj <klr>[&#8230;]</klr> ja en la tuta Usono ne
        <tld/>as
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        jam de du jaroj <tld/>is la prezidanteco
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2004/008400.html">Nova prezidanto malnovaj
          problemoj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_s;&c_v;&c_a;&c_b;&c_o;&c_d;&c_n;&c_y;&c_m;</ind>
      <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;</ind>
      <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_n;&c_je;&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="ca">vacar</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t pr&aacute;zdn&yacute;</trd>,
    <trd>uvoln&ecaron;n&yacute; <klr>(m&iacute;sto, &uacute;&rcaron;ad)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>frei sein</trd>,
    <trd>unbesetzt sein</trd>,
    <trd><ind>leer</ind> sein</trd>,
    <trd><ind>vakant</ind> sein</trd>
  </trdgrp>
  <trd lng="en">be vacant</trd>
  <trdgrp lng="es">
    <trd>estar vac&iacute;o</trd>,
    <trd>estar libre</trd>
  </trdgrp>
  <trd lng="fr">vacant (&ecirc;tre)</trd>
  <trdgrp lng="hu">
    <trd>&uuml;res</trd>,
    <trd>szabad <klr tip="ind">(nincs elfoglalva)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#12354;&#12356;&#12390;&#12356;&#x308B;</trd>
  <trdgrp lng="nl">
    <trd>vrij zijn</trd>,
    <trd>vacant zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wakowa&cacute;</trd>,
    <trd>by&cacute; nieobsadzonym</trd>
  </trdgrp>
  <trd lng="pt">estar vago</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_n;&c_je;
&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_s;&c_v;&c_o;&c_b;&c_o;&c_d;&c_n;&c_y;&c_m;</ind> <klr tip="amb">(&c_o;
&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; neobsaden&yacute;</trd>,
    <trd>pr&aacute;zdny</trd>,
    <trd>uvo&lcaron;nen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43D;&c_je; &c_z;&c_a;&c_j;&#x43D;&#x44F;&c_t;&c_i;&#x43C;</trd>,
    <trd>&c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&#x43F;&c_u;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="vak.0a">
  <kap><tld/>a</kap>
  <snc mrk="vak.0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="liber.0a.vaka">Libera<sncref/></ref>,
      <ref tip="vid" cel="dispon.0ebla">disponebla</ref>:
      <ekz>
        se&gcirc;o <klr>[sur kiu]</klr> alia homo ne rajtas sidi, e&ccirc;, se &scirc;i estas <tld/>a
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la restantaj estraranoj elektas novajn estraranojn por plenigi la <tld/>ajn
        lokojn
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a kaj dezerta animo malproksime postrestas kaj erarvagadas
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam dungo <tld/>i&gcirc;is, ili indikis al mi
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        plen&scirc;topu iliajn <tld/>ajn rap-nap-kapojn per babila&jcirc;oj
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kav.0a">kava</ref>,
      <ref cel="plen.mal0a">malplena</ref>,
      <ref cel="virg.0a.io">virga<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vacant</trd>,
    <trd>lliure</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neobsazen&yacute;</trd>,
    <trd>pr&aacute;zdn&yacute;</trd>,
    <trd>upr&aacute;zdn&ecaron;n&yacute;</trd>,
    <trd>uvoln&ecaron;n&yacute;</trd>,
    <trd>voln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>frei</trd>,
    <trd>unbesetzt</trd>,
    <trd>vakant</trd>,
    <trd>leer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inoccup&eacute;</trd>,
    <trd>vacant</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12354;&#12356;&#12390;&#12356;&#x308B;</trd>,
    <trd>&#x7A7A;&#x5E2D;&#x306E;  <pr>&#x304F;&#12358;&#x305B;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x7A7A;&#x4F4D;&#x306E;  <pr>&#x304B;&#12425;&#12356;&#x306E;</pr></trd>,
    <trd/>,
    <trd>&#12358;&#12388;&#x308D;&#x306A;</trd>,
    <trd>&#x307C;&#12435;&#12420;&#x308A;&#12375;&#x305F;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wakuj&aogonek;cy</trd>,
    <trd>niezaj&eogonek;ty</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neobsaden&yacute;</trd>,
    <trd>pr&aacute;zdny</trd>,
    <trd>upr&aacute;zdnen&yacute;</trd>,
    <trd>vo&lcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je; &c_z;&c_a;&c_j;&#x43D;&#x44F;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
