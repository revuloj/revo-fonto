<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidrocefal</rad>/o
</kap>
<drv mrk="hidroc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsano ka&ubreve;zata per amasi&gcirc;o de akvo en la nervaj centroj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_c;&c_e;&c_f;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Wasserkopf</trd>,
    <trd>Hydrocephalus<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="es">hidrocefalia</trd>
  <trd lng="fr">hydroc&eacute;phalie</trd>
  <trdgrp lng="hu">
    <trd>agykamrat&aacute;gulat</trd>,
    <trd>v&iacute;zfej&udblac;s&eacute;g</trd>,
    <trd>hidrocef&aacute;lia</trd>
  </trdgrp>
  <trd lng="it">idrocefalia</trd>
  <trdgrp lng="nl">
    <trd>waterhoofd</trd>,
    <trd>hydrocefalie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>hydrocefalia</trd>,
    <trd>wodog&lstroke;owie</trd>
  </trdgrp>
  <trd lng="pt">hidrocefalia</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_c;&c_je;&c_f;&c_a;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_k;&c_a; &c_m;&c_o;&c_z;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="hidroc.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
    <trd lng="it">idrocefalico</trd>
  </snc>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a infano.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>wasserk&ouml;pfig</trd>,
       <trd>hydrocephalitisch<klr> [fak]</klr></trd>
    </trdgrp>
    <trd lng="fr">hydroc&eacute;phale<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="it">idrocefalo<klr> (agg.)</klr></trd>
  </snc>
  <trd lng="es">hidrosef&aacute;lico</trd>
  <trdgrp lng="hu">
    <trd>agykamrat&aacute;gulatos</trd>,
    <trd>v&iacute;zfej&udblac;</trd>,
    <trd>hidrocefalikus</trd>
  </trdgrp>
  <trd lng="nl">hydrocefalisch</trd>
  <trd lng="pt">hidrocef&aacute;lico</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_c;&c_je;&c_f;&c_a;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hidroc.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Malsanulo karakterizata per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_c;&c_e;&c_f;&c_a;&c_l;</trd>
    <trdgrp lng="de">
       <trd>Wasserkopf</trd>,
       <trd>Hydrocephaluspatient<klr> [fak]</klr></trd>
    </trdgrp>
  <trd lng="fr">hydroc&eacute;phale<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>agykamrat&aacute;gulatos<klr tip="ind"> (beteg)</klr></trd>,
    <trd>v&iacute;zfej&udblac;<klr tip="ind"> (beteg)</klr></trd>,
    <trd>hidrocefalikus<klr tip="ind"> (beteg)</klr></trd>
  </trdgrp>
  <trd lng="it">idrocefalo<klr> (sost.)</klr></trd>
  <trd lng="nl">hydrocefaal</trd>
  <trd lng="pt">hidroc&eacute;falo</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;
  <ind>&c_g;&c_i;&c_d;&c_r;&c_o;&c_c;&c_je;&c_f;&c_a;&c_l;&c_i;&c_je;&c_j;</ind></trd>
</drv>
</art>
