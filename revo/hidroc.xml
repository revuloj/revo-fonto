<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidrocefal</rad>/o
</kap>
<drv mrk="hidroc.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsano ka&ubreve;zata per amasi&gcirc;o de akvo en la nervaj centroj.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>waterhoofd</trd>,
    <trd>hydrocefalie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>hydrocefalia</trd>,
    <trd>wodog&lstroke;owie</trd>
  </trdgrp>
</drv>

<drv mrk="hidroc.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a infano.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">hydrocefalisch</trd>
</drv>

<drv mrk="hidroc.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc>
    <dif>
      Malsanulo karakterizata per <tld/>o.
    </dif>
  </snc>
  <trd lng="nl">hydrocefaal</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
