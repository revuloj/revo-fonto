<vortaro>
<art mrk="$Id$">
<kap>
  <rad>en</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="en1.0o">
  <kap><tld/>o</kap>
  <mlg>JPY</mlg>
  <mlg>&#165;</mlg>
  <snc mrk="en1.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de Japanujo:
      <ekz>
        &ccirc;iu telero kostas 100 <tld/>ojn (malpli ol e&ubreve;ro)
        <fnt>
        <aut>Sibazyun</aut>,
        <vrk><url ref="http://sibazyun.cocolog-nifty.com/sibazyun/">
        [blogo]</url></vrk>,
        <lok>&ccirc;e su&scirc;iejo kun cirkulanta pasejo, 2005-09-20
        </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">yen</trd>
  <trd lng="fr">yen</trd>
  <trd lng="id">yen</trd>
  <trd lng="pl">jen</trd>
  <trd lng="pt">iene</trd>
</drv>
</art>

