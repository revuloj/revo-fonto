<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bakala.xml,v 1.26 2023/04/29 16:32:12 revo Exp $">
<kap>
  <rad>bakala&ubreve;r</rad>/o
</kap>
<drv mrk="bakala.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Posedanto de la plej malalta universitata 
      <ref tip="super" cel="titol.0o.RANG">titolo</ref>:
      <ekz>
        la <tld/>o komencis paroli latine
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de
        feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        li studis en la praga universitato, en 1393 li fari&gcirc;is <tld/>o, en 1396 magistro
        de liberaj artoj
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        la novaj universitataj gradoj estu <tld/>o (ses-semestra studado), magistro
        (pliaj du a&ubreve; kvar semestroj) kaj doktoro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2010/010037.php"
              >Reformo fu&scirc;ita</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        kariero kadre de la <tld/>a nivelo
        <fnt>
          <bib>Monato</bib>
          <aut>Hugo Mora</aut>
          <vrk><url ref="&Monato;/2005/008770.php"
              >Instru-taksilo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abitur.0o">abituriento</ref>,
      <ref cel="magist.0o">magistro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_r;</trd>
  <trd lng="br">bachelour<klr tip="ind"> (skol-veur)</klr></trd>
  <trd lng="ca">llicenciat</trd>
  <trdgrp lng="de">
    <trd>Bakkalaureus</trd>,
    <trd>niedrigster <ind>akademischer Grad</ind> in den
    angels&auml;chsischen L&auml;ndern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>baccalaureate</trd>,
    <trd>bachelor's degree holder</trd>
  </trdgrp>
  <trd lng="es">bachiller</trd>
  <trd lng="fr">bachelier<klr tip="ind"> (personne re&ccedil;ue au
  premier examen du sup&eacute;rieur)</klr></trd>
  <trd lng="he">&bet;&vav;&gimel;&resh; &tav;&vav;&alef;&resh; &resh;&alef;&shin;&vav;&fnun;</trd>
  <trdgrp lng="pl">
    <trd>baka&lstroke;arz</trd>,
    <trd>licencjat</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_a;&c_l;&c_a;&c_v;&c_r;</trd>
  <trdgrp lng="nl">
    <trd>licentiaat <klr>(Belgio)</klr></trd>,
    <trd>doctorandus <klr>(Nederl.)</klr></trd>
  </trdgrp>
  <trd lng="hu">baccalaureus</trd>
  <trd lng="sk">bakal&aacute;r</trd>
  <trd lng="uk">&c_b;&c_a;&c_k;&c_a;&c_l;&c_a;&c_v;&c_r;</trd>
</drv>
</art>
