<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bakala&ubreve;r</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="bakala.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Persono, posedanta la plej malaltan universitatan titolon:
      <ekz>
        la <tld/>o komencis paroli latine
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abitur.0o">abituriento</ref>,
      <ref cel="magist.0o">magistro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_r;</trd>
  <trd lng="br">bachelour<klr tip="ind"> (skol-veur)</klr></trd>
  <trdgrp lng="de">
    <trd>Bakkalaureus</trd>,
    <trd>niedrigster <ind>akademischer Grad</ind> in den angels&auml;chsischen
    L&auml;ndern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>baccalaureate</trd>,
    <trd>bachelor's degree holder</trd>
  </trdgrp>
  <trd lng="es">bachiller</trd>
  <trd lng="fr">bachelier<klr tip="ind"> (personne re&ccedil;ue au premier 
examen du sup&eacute;rieur)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_a;&c_l;&c_a;&c_v;&c_r;</trd>
  <trdgrp lng="nl">
    <trd>licentiaat <klr>(Belgio)</klr></trd>,
    <trd>doctorandus <klr>(Nederl.)</klr></trd>
  </trdgrp>
  <trd lng="hu">baccalaureus</trd>
  <trd lng="sk">bakal&aacute;r</trd>
</drv>
</art>
