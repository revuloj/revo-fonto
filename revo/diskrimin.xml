<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diskrimin.xml,v 1.10 2022/03/15 16:19:58 revo Exp $">
<kap>
  <rad>diskrimin</rad>/i <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="diskrimin.0i">
  <kap><tld/>i</kap>
  <snc mrk="diskrimin.0i.POL">
  <dif>
    <ref tip="dif" cel="diskrim.0i.POL">Diskriminacii</ref>:
    <ekz>
      sur bazaro revenis al ni libereco mar&ccirc;andi kun vendistoj,
      kiuj ne 
      <tld/>is nin nur pro pli blanka ha&ubreve;to
      <fnt>
        <aut>Marzena</aut>,
        <vrk><url ref="http://www.naszawyprawa.com/relacje_es.asp">
        Voja&gcirc;raporto</url></vrk>,
        <lok>Vjetnamio, 2004-11-19</lok>
      </fnt>.
    </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_s;&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="pl">dyskryminowa&cacute;</trd>
    <trd lng="pt">discriminar</trd>
  </snc>
  <snc mrk="diskrimin.0i.distingi">
    <uzo tip="stl">RAR</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <ref tip="dif" cel="distin.0i">Distingi</ref>:
      <ekz>
        la tipa Atapaska lingvo <tld/>as pri la fizikaj formoj de naturaj 
        objektoj
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">
          taglibro</url></vrk>,
          <lok>2004-08-28</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="pl">wyr&oacute;&zdot;nia&cacute;</trd>
    <trd lng="pt">distinguir</trd>
  </snc>
  <trd lng="de">diskriminieren</trd>
  <trd lng="en">discriminate</trd>
  <trd lng="fr">discriminer</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x440;&#x456;&#x437;&#x43D;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x456;&#x437;&#x43D;&#x430;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x434;&#x456;&#x43B;&#x44F;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="diskrimin.0o">
  <kap><tld/>o</kap>
  <snc mrk="diskrimin.0o.POL">
    <dif>
      <ref tip="dif" cel="diskrim.0o.POL">Diskriminacio</ref>:
      <ekz>
        parlamentaj demandoj pri lingva <tld/>ado
        <fnt>
          <aut>-</aut>,
          <lok><url ref="http://www.lingvo.org/bulteno/16.pdf">
          E&ubreve;ropa Bulteno, 2001-08-23, numero 16a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_s;&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="pl">dyskryminacja</trd>
    <trd lng="pt">discrimina&ccedil;&atilde;o</trd>
  </snc>
  <snc mrk="diskrimin.0o.distingo">
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="distin.0o">distingo</ref>
    <trd lng="be">&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </snc>
  <trd lng="cs">diskriminace</trd>
  <trd lng="de">Diskriminierung</trd>
  <trd lng="en">discrimination</trd>
  <trd lng="fr">discrimination</trd>
  <trd lng="pl">wyr&oacute;&zdot;nienie<klr>
    (odr&oacute;&zdot;nienie)</klr></trd>
  <trd lng="pt">distin&ccedil;&atilde;o</trd>
  <trd lng="sk">diskrimin&aacute;cia</trd>
</drv>
</art>

