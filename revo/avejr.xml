<vortaro>
  <art mrk="$Id: avejr.xml,v 1.1 2020/06/27 10:33:09 afido Exp $">
    <kap>
      <rad>Avejr</rad>/o
    </kap>

<drv mrk="avejr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="avejr.0o.havenurbo">
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la nordo de <ref tip="malprt" cel="portugal.P0ujo">Portugalujo</ref>:
      <ekz>
        filino de portugalo el <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>pluraj amikoj de Izabel</aut>
          <vrk><url ref="https://www.monato.be/2006/008978.php"
              >Mortis Izabel Santiago</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        post doktori&gcirc;o, li aktivis kiel kuracisto en <tld/>o, kie li vivis &gcirc;is 1797
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Bernardino_Ant%C3%B3nio_Gomes"
              >Bernardino Ant&oacute;nio Gomes - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Aveiro</trd>
    <trd lng="en">Aveiro</trd>
    <trd lng="fr">Aveiro</trd>
  </snc>
</drv>

  </art>

