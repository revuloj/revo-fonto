<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mulcx.xml,v 1.9 2024/01/26 09:22:18 revo Exp $">
<kap>
  <rad>mul&ccirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mulcx.0o">
  <kap><tld/>o</kap>
  <snc mrk="mulcx.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Organika, nekompo&scirc;tita materialo (sega&jcirc;oj, pajlo, vegeta&jcirc;eroj),
      per kiu oni &scirc;irmas kaj sterkas la grundon:
      <ekz>
        <tld/>o malhelpas la kreskon de trudplantoj,
        konservas la humidecon de la grundo
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o utiligeblas en la agrikulturo kiel
        grundoplibonigilo,
        sed fabrikeblas el &gcirc;i ekzemple varmizolaj lamenoj
        <fnt>
          <aut>F. Moln&aacute;r</aut>,
          <vrk><url ref="http://www.freeweb.hu/eventoj/steb/industrio/rubajxo/rubajxo.htm">
          Apliko de modernaj ruba&jcirc;sendan&gcirc;erigaj metodoj</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komposxt.0o.HOR">kompo&scirc;to</ref>,
      <ref cel="sterk.0o">sterko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;&c_mol;&c_ch;&c_a;</trd>
  <trd lng="de">Mulch</trd>
  <trdgrp lng="fr">
    <trd>mulch</trd>,
    <trd>paillis</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25975;&#12365;&#34241;  <pr>&#12375;&#12365;&#12431;&#12425;</pr></trd>,
    <trd>&#25975;&#12365;&#33609;  <pr>&#12375;&#12365;&#12367;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kompost</trd>,
    <trd>gn&oacute;j</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>compost</trd>,
    <trd>baleg&abreve;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_u;&c_l;&c_mol;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_n;&c_ja; &c_r;&c_o;&c_s;&c_l;&c_i;&c_n; &c_g;&c_n;&c_o;&c_jeu;&c_m;</trd>,
    <trd>&c_s;&c_o;&c_l;&c_o;&c_m;&c_o;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="mulcx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mulcx.0i.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Provizi <tld/>on &ndash; ekzemple en bedo &ndash;
      por malhelpi trudherbojn, teni varmon kaj
      malsekon, <ref cel="sterk.0i">sterki</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;&c_mol;&c_ch;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">mulchen</trd>
  <trd lng="fr">pailler</trd>
  <trdgrp lng="ja">
    <trd>&#25975;&#12365;&#34241;&#12434;&#25975;&#12367;  <pr>&#12375;&#12365;&#12431;&#12425;&#12434;&#12375;&#12367;</pr></trd>,
    <trd>&#12510;&#12523;&#12481;&#12531;&#12464;&#12377;&#12427;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawozi&cacute;</trd>,
    <trd>u&zdot;y&zacute;nia&cacute; gleb&eogonek; kompostem</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>fertiliza</trd>,
    <trd>fertiliza&tcomma;i solul cu compost </trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_u;&c_l;&c_mol;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>

