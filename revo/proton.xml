<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: proton.xml,v 1.24 2022/03/26 15:53:38 revo Exp $">
<kap>
  <rad>proton</rad>/o
</kap>
<drv mrk="proton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Pozitiva <ref tip="super" cel="nukleon.0o">nukleono</ref>
      kun maso 1836-oble pli granda ol tiu de elektrono kaj
      <ref tip="vid" cel="spin2.0o">spino</ref> 1/2, atomkerno de
      malpeza hidrogeno; kune kun <ref cel="neuxtr2.0o">ne&ubreve;tronoj</ref> 
      &gcirc;i konsistigas &ccirc;iujn atomkernojn:
      <ekz>
        oni pelas <tld/>ojn per rekordaj rapidegoj tra ronda tunelo por tiel esplori la
        esti&gcirc;on kaj strukturon de la universo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010081.php">Misteroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        atomo de oksigeno havas 8 <tld/>ojn en sia kerno
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="atom.0o">atomo</ref>
    <ref tip="prt" cel="kvark.0o">kvarko</ref>
    <refgrp tip="vid">
      <ref cel="barjon.0o">barjono</ref>,
      <ref cel="partik2.0o">partiklo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_o;&c_n;</trd>
  <trd lng="cs">proton</trd>
  <trd lng="de">Proton</trd>
  <trd lng="en">proton</trd>
  <trd lng="es">prot&oacute;n</trd>
  <trd lng="fa">&f_p;&a_r;&a_w;&a_t;&a_w;&a_n;</trd>
  <trd lng="hu">proton</trd>
  <trd lng="nl">proton</trd>
  <trd lng="pl">proton</trd>
  <trdgrp lng="pt">
    <trd>pr&oacute;ton <klr>(BR)</klr></trd>,
    <trd>prot&atilde;o <klr>(PT)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_o;&c_n;</trd>
  <trd lng="sk">prot&oacute;n</trd>
  <trd lng="sv">proton</trd>
  <trd lng="uk">&#x43F;&#x440;&#x43E;&#x442;&#x43E;&#x43D;</trd>
</drv>

<drv mrk="proton.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="vid" cel="partik2.kontraux0o">Kontra&ubreve;partiklo</ref> de <tld/>o.
    </dif>
    <ref tip="sin" cel="antiproton.0o">antiprotono</ref>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_p;&c_r;&c_a;&c_t;&c_o;&c_n;</trd>
  <trd lng="de">Antiproton</trd>
  <trd lng="en">antiproton</trd>
  <trd lng="es">antiprot&oacute;n</trd>
  <trd lng="fa">&f_p;&a_A;&a_d;&f_p;&a_r;&a_w;&a_t;&a_w;&a_n;</trd>
  <trd lng="hu">antiproton</trd>
  <trd lng="nl">antiproton</trd>
  <trd lng="pl">antyproton</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_p;&c_r;&c_o;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
