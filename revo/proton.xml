<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>proton</rad>/o
</kap>
<drv mrk="proton.0o">
<kap><tld/>o</kap>
<uzo tip="fak">FIZ</uzo>
<snc>
<dif>
Pozitiva <ref tip="super" cel="nukleon.0o">nukleono</ref>
kun maso 1836-oble pli granda ol tiu de elektrono k
<ref tip="vid" cel="spin2.0o">spino</ref> 1/2, atomkerno de malpeza hidrogeno;
kune kun <ref cel="neuxtr2.0o">ne&ubreve;tronoj</ref> konsistigas &ccirc;iujn atomkernojn.
</dif>
<ref tip="malprt" cel="atom.0o">atomo</ref>
<ref tip="prt" cel="kvark.0o">kvarko</ref>
<refgrp tip="vid">
<ref cel="barjon.0o">barjono</ref>,
<ref cel="partik2.0o">partiklo</ref>
</refgrp>
</snc>
<trd lng="de">Proton</trd>
<trd lng="en">proton</trd>
<trd lng="nl">proton</trd>
<trd lng="pt">pr&oacute;ton</trd>
<trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_o;&c_n;</trd>
</drv>
<drv mrk="proton.kontraux0o">
<kap>kontra&ubreve;<tld/>o, anti<tld/>o</kap>
<uzo tip="fak">FIZ</uzo>
<snc>
<dif>
<ref tip="vid" cel="partik2.kontraux0o">Kontra&ubreve;partiklo</ref> de protono.
</dif>
</snc>
<trd lng="de">Antiproton</trd>
<trd lng="en">antiproton</trd>
<trd lng="nl">antiproton</trd>
<trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_p;&c_r;&c_o;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
