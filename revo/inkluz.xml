<drv mrk="inkluz.0e">
  <kap><ofc>2</ofc> <tld/>e</kap>
  <snc mrk="inkluz.0e.KOMUNE">
    <dif>
      Enhavante la koncernatan aferon a&ubreve; limon:
      <ekz>
        mi forestos de lando &gcirc;is &jcirc;a&ubreve;do <tld/>e
        <klr>(enkalkulante &jcirc;a&ubreve;don)</klr>;
      </ekz>
      <ekz>
        la prezoj estas kalkulataj <tld/>e afrankon;
      </ekz>
      <ekz>
        nia tuta literaturo, <tld/>e je lernolibroj, vortaroj kaj legolibroj por
        komencantoj, enhavis proksimume tricent librojn
        <fnt>
          <bib>KPr</bib>
          <lok>Anta&ubreve;vorto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jonatan</nom> deprenis <klr>[&#x2026;]</klr> siajn militajn vestojn, <tld/>e sian glavon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 18:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekrete babilante <klr>[&#x2026;]</klr> pri la vizito de la fremdlandaj gastoj, <tld/>e
        de li mem
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la enketo, <tld/>e nervopsikologian ekzamenon, plie pruvis, ke &scirc;i tute ne lertas
        maldekstramane
        <fnt>
          <bib>ChR</bib>
          <lok>Tri estas tro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>einschlie&szlig;lich</trd>,
    <trd>inklusive</trd>
  </trdgrp>
  <trd lng="en">inclusively</trd>
  <trdgrp lng="fr">
    <trd>inclusivement</trd>,
    <trd>y compris</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bele&eacute;rtve</trd>,
    <trd>belesz&aacute;m&iacute;tva</trd>,
    <trd>inkluz&iacute;ve</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x542B;&#x3093;&#x3067;  <pr>&#x3075;&#x304F;&#x3093;&#x3067;</pr></trd>,
    <trd>&#x542B;&#x3081;&#x3066;  <pr>&#x3075;&#x304F;&#x3081;&#x3066;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>inclusief<klr tip="ind"> (bw.)</klr></trd>,
    <trd>inbegrepen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>inclusive</trd>,
    <trd>inclusivamente</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>inkluz&iacute;vne</trd>,
    <trd>vr&aacute;tane</trd>,
    <trd>v&ccaron;&iacute;tane</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>inklusive</trd>,
    <trd>medr&auml;knat</trd>,
    <trd>medtaget</trd>
  </trdgrp>
