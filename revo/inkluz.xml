<drv mrk="inkluz.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anigi ion en io alia,
      <refgrp tip="sin">
        <ref cel="al.0igi">aligi</ref>,
        <ref cel="don.al0i">aldoni</ref>
      </refgrp>,
      enmeti:
      <ekz>
        <tld/>igi novajn artikolojn en kontrakton;
      </ekz>
      <ekz>
        <tld/>igi ies elpa&scirc;on en la programon de aran&gcirc;o;
      </ekz>
      <ekz>
        nur kelkaj esperantistoj havas politikajn celojn kaj
        <tld/>igas Esperanton en ilin
        <fnt>
          <aut>le_chaz</aut>:
          <lok><url ref="http://nl.lernu.net/komunikado/forumo/temo.php?t=4719&amp;p=2">lernu!</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la bapto estas la sakramento, kiu liberigas la homon de
        heredita kaj persona kulpo, kio lin <tld/>igas en
        Kriston kaj en lian eklezion
        <fnt>
          <aut>W. Plock, trad. V. O&scaron;lak</aut>,
          <vrk><url ref="http://www.kristana-misio-en-esperanto.si/docs/plock.pdf">
          Kio min malhelpas esti baptita?</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <em>ni</em> &ccirc;i tie fakte estas nur <nom>Zamenhof</nom>: unu
        persono. Li uzis <ctl>ni</ctl> por kvaza&ubreve; <tld/>igi
        anka&ubreve; aliajn esperantistojn
        <fnt>
          <bib>PMEG</bib>,
          <lok><url ref="&PMEG;gramatiko/pronomoj/unua.html">
          11.3. Unua persono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="inklud.0i">inkludi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_u; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_u;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufnehmen</trd>,
    <trd>inkludieren <klr>(hinzunehmen)</klr></trd>,
    <trd>hinzunehmen</trd>,
    <trd>hinzuf&uuml;gen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inclure<klr tip="ind"> (adjoindre)</klr></trd>,
    <trd>adjoindre<klr tip="ind"> (inclure)</klr></trd>,
    <trd>ajouter<klr tip="ind"> (inclure)</klr></trd>,
    <trd>ins&eacute;rer<klr tip="ind"> (inclure)</klr></trd>,
    <trd>joindre<klr tip="ind"> (inclure)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bevesz<klr tip="ind"> (vmi k&ouml;z&eacute;)</klr></trd>,
    <trd>felvesz<klr tip="ind"> (vmi k&ouml;z&eacute;)</klr></trd>,
    <trd>besorol</trd>
  </trdgrp>
  <trd lng="nl">insluiten</trd>
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;<klr> (&c_v;&c_o; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x9644;&#x4E0A; <pr>f&#xF9;sh&#xE0;ng</pr></trd>,
    <trd>&#x4E26;&#x628A; <pr>b&#xEC;ngb&#x1CE;</pr></trd>,
    <trd>&#x62EC; <pr>ku&#xF2;</pr></trd>,
    <trd>&#x6536;&#x5217; <pr>sh&#x14D;uli&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="inkluz.mal0i">
