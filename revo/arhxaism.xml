<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ar&hcirc;aism</rad>/o
</kap>
<drv mrk="arhxaism.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="arkais.0o">arkaismo</ref>
    <ekz>
      kvankam &ccirc;e li renkonti&gcirc;as ar&hcirc;aismoj, lia lingva&jcirc;o estas klara kaj
      bela anka&ubreve; hodia&ubreve;<fnt><aut>Boris Kolker</aut>: <vrk>Senmorta Pu&scirc;kin
      (Aleksandr Pu&scirc;kin, 200 jaroj, elektitaj verkoj)</vrk>,
      <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 32a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_a;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">arcaisme</trd>
  <trd lng="en">archaisme</trd>
  <trdgrp lng="fr">
    <trd>archa&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">archaizmus</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_a;&c_i;&c_z;&c_m;</trd>
</drv>
</art>

