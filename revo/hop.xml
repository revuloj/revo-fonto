<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hop.xml,v 1.27 2024/01/18 09:24:56 revo Exp $">
<kap>
  <rad>hop</rad>
  <fnt> <bib>PrV</bib></fnt>
</kap>
<drv mrk="hop.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="hop.0.KOMUNE">
    <dif>
      Saltu!:
      <ekz>
        ne diru <tld/> anta&ubreve; la salto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se vi volos rajdi rapide, klaku per la lango kaj kriu <tld/> <tld/>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>!
      </ekz>
      <ekz>
        kriu <tld/> kaj mi saltas
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_p;!</trd>,
    <trd>&c_g;&c_o;&c_c;!</trd>
  </trdgrp>
  <trd lng="de">hopp</trd>
  <trdgrp lng="en">
    <trd>hop to it</trd>,
    <trd>jump</trd>
  </trdgrp>
  <trd lng="es">salto<klr tip="ind"> (con un pie, a la pata coja)</klr></trd>
  <trd lng="fr">hop</trd>
  <trd lng="hu">hopp</trd>
  <trd lng="it">hop</trd>
  <trd lng="ja">&#x305D;&#x308C;&#12387;&#xFF01;</trd>
  <trd lng="nl">hop<klr tip="ind"> (tussenw.)</klr></trd>
  <trdgrp lng="pl">
    <trd>hop</trd>,
    <trd><klr>hej, </klr>hop</trd>
  </trdgrp>
  <trd lng="pt">upa</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_p;</trd>,
    <trd>&c_o;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="hop.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hop.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="salt.0i.sin_levi">Salt</ref>eti per paralelaj kuroj a&ubreve; per nur unu
      piedo:
      <ekz>
        li <ctl>eskapis</ctl>, tio estas, li <tld/>is per unu piedo kaj
        trenis la alian post si kaj eliris
        <fnt>
          <aut>T. Steele</aut>,
          <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>,
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kato <klr>[&#8230;]</klr> <tld/>is reen sur la kamenon
        <fnt><bib>MkM</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_s;&c_k;&c_o;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_c;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_c;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">poskakovat na jedn&eacute; noze</trd>
  <trdgrp lng="de">
    <trd>hopsen</trd>,
    <trd>h&uuml;pfen</trd>
  </trdgrp>
  <trd lng="en">to <ind>hop</ind></trd>
  <trd lng="es">&#xA1;salta!<klr> (con un pie, a la pata coja)</klr></trd>
  <trd lng="fr">sauter &agrave; cloche-pied</trd>
  <trd lng="it">saltellare<klr> (con un piede)</klr></trd>
  <trd lng="ja">&#12402;&#12423;&#12356;&#12392;&#x8DF3;&#12406;  <pr>&#12402;&#12423;&#12356;&#12392;&#12392;&#12406;</pr></trd>
  <trdgrp lng="nl">
   <trd>hinkelen</trd>,
   <trd>hinken</trd>,
   <trd>huppelen</trd>
  </trdgrp>
  <trd lng="sk">poskakova&tcaron; na jednej nohe</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_i;&c_b;&c_a;&c_t;&c_i; &#x43D;&c_a; &#x43E;&c_d;&#x43D;&c_ib;&c_j; &#x43D;&#x43E;&c_z;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x8DF3; <pr>ti&#xE0;o</pr></trd>,
    <trd>&#x8E66; <pr>b&#xE8;ng</pr></trd>,
    <trd>&#x8E66;&#x8DF3; <pr>b&#xE8;ngti&#xE0;o</pr></trd>,
    <trd>&#x72EC;&#x811A;&#x8DF3; <pr>d&#xFA;ji&#x1CE;oti&#xE0;o</pr></trd>,
    <trd>&#x8DC3; <pr>yu&#xE8;</pr></trd>,
    <trd>&#x8DF3;&#x8DC3; <pr>ti&#xE0;oyu&#xE8;</pr></trd>,
    <trd>&#21333;&#x811A;&#x8DF3; <pr>d&amacron;nji&#x1CE;oti&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hop.0ludo">
  <kap><tld/>ludo</kap>
  <snc mrk="hop.0ludo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Salt<ref tip="super" cel="lud.0o.ago">ludo</ref>,
      &ccirc;e kiu oni <tld/>as tra vico de desegnitaj kvadratoj
      sen tu&scirc;i la liniojn, anka&ubreve; konata kiel &leftquot;paradizludo&rightquot; kaj &leftquot;marelo&rightquot;:
      <ekz>
        en la dana nomi&gcirc;as &leftquot;<nac>hinke-leg</nac>&rightquot;, kies rekta traduko estas
        &leftquot;salteta ludo&rightquot;, en PIV2002 estas &leftquot;<tld/>ludo&rightquot;, &leftquot;paradizludo&rightquot; kaj
        &leftquot;marelo&rightquot;, &dash; mi &scirc;atus scii <klr>[kiun]</klr> vorton vi kutime uzas en via
        familio
        <fnt>
          <aut>J. Nordfalk, N. Budha</aut>,
            <vrk>
            <url
            ref="http://javabog.dk/filmoj_en_esperanto/mejzi_ludtempo/mejzi_ludtempo_notoj.html">
            Traduko de la filmo "Mejzi Ludtempo" al Esperanto</url>
          </vrk>,
          <lok>&ccirc;. 1.2.1 Saltludo</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/1/14/Michelle_Obama_plays_hopscotch.jpg/640px-Michelle_Obama_plays_hopscotch.jpg"
    prm="PD">Michelle Obama ludanta <tld/>ludon &ccirc;e la universitato de Mumbai, Barato, Nov. 6, 2010
        <fnt>
          <aut>Chuck Kennedy, The White House from Washington, DC, P110610CK-0355</aut>
          <lok><url
          ref="&WCW;/File:Michelle_Obama_plays_hopscotch.jpg">&WCW;/File:Michelle_Obama_plays_hopscotch.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Himmel und H&ouml;lle</trd>,
      <trd>Hickelkasten</trd>,
      <trd>Himmelhopse</trd>,
      <trd>Reise zum Mond</trd>
    </trdgrp>
    <trd lng="en">hopscotch</trd>
    <trdgrp lng="es">
      <trd>rayuela</trd>,
      <trd>avioncito<klr> (juego infantil)</klr></trd>,
      <trd>sambori</trd>,
      <trd>cascayo<klr> (en Asturias)</klr></trd>,
      <trd>tejo<klr> (el tejo tambi&eacute;n es el objeto que se usa en el juego)</klr></trd>
    </trdgrp>
    <trd lng="fr">marelle</trd>
    <trd lng="zh">&#x8DF3;&#x623F;&#x5B50; <pr>ti&#xE0;of&#xE1;ngzi</pr></trd>
</snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_s;&c_y;&c_k;&c_ib; <klr tip="amb">(&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trdgrp lng="it">
    <trd>pampano<klr> (gioco)</klr></trd>,
    <trd>settimana<klr> (gioco)</klr></trd>,
    <trd>campana<klr> (gioco)</klr></trd>
  </trdgrp>
  <trd lng="cs">sk&aacute;k&aacute;n&iacute; "pan&aacute;ka"</trd>
  <trd lng="da">hinke-leg</trd>
  <trd lng="ja">&#x77F3;&#12369;&#x308A;  <pr>&#12356;&#12375;&#12369;&#x308A;</pr></trd>
  <trd lng="nl">hinkelspel</trd>
  <trd lng="pl"><klr>gra w </klr>klasy</trd>
  <trd lng="sk">sk&aacute;kanie pan&aacute;ka <klr>(hra)</klr></trd>
  <trdgrp lng="uk">
    <trd><klr>(&c_g;&c_r;&c_a; &c_v;)</klr> &#x43A;&#x43B;&c_a;&c_s;&c_i;</trd>,
    <trd>&#x43D;&c_je;&c_b;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
