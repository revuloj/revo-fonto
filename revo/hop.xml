<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hop.xml,v 1.21 2022/04/30 08:42:41 revo Exp $">
<kap>
  <rad>hop</rad> 
  <fnt> <bib>PrV</bib></fnt>
</kap>
<drv mrk="hop.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="hop.0.KOMUNE">
    <dif>
      Saltu!:
      <ekz>
        ne diru <tld/> anta&ubreve; la salto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se vi volos rajdi rapide, klaku per la lango kaj kriu <tld/> <tld/>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>!
      </ekz>
      <ekz>
        kriu <tld/> kaj mi saltas
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">hopp</trd>
  <trdgrp lng="en">
    <trd>hop to it</trd>,
    <trd>jump</trd>
  </trdgrp>
  <trd lng="fr">hop</trd>
  <trd lng="hu">hopp</trd>
  <trd lng="it">hop</trd>
  <trd lng="nl">hop<klr tip="ind"> (tussenw.)</klr></trd>
  <trdgrp lng="pl">
    <trd>hop</trd>,
    <trd><klr>hej, </klr>hop</trd>
  </trdgrp>
  <trd lng="pt">upa</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_p;</trd>,
    <trd>&c_o;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="hop.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hop.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="salt.0i.sin_levi">Salt</ref>eti per paralelaj kuroj a&ubreve; per nur unu piedo:
      <ekz>
        li <ctl>eskapis</ctl>, tio estas, li <tld/>is per unu piedo kaj
        trenis la alian post si kaj eliris
        <fnt>
          <aut>T. Steele</aut>,
          <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>,
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kato <klr>[&#x2026;]</klr> <tld/>is reen sur la kamenon
        <fnt><bib>MkM</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>hopsen</trd>,
    <trd>h&uuml;pfen</trd>
  </trdgrp>
  <trd lng="en">to <ind>hop</ind></trd>
  <trd lng="es">saltar<klr> (con un pie, a la pata coja)</klr></trd>
  <trd lng="fr">sauter &agrave; cloche-pied</trd>
  <trd lng="it">saltellare<klr> (con un piede)</klr></trd>
  <trdgrp lng="nl">
   <trd>hinkelen</trd>,
   <trd>hinken</trd>,
   <trd>huppelen</trd>
  </trdgrp>
  <trd lng="sk">poskakova&tcaron; na jednej nohe</trd>
  <trd lng="cs">poskakovat na jedn&eacute; noze</trd>
  <trd lng="uk">&c_s;&#x442;&#x440;&#x438;&#x431;&#x430;&#x442;&#x438; &#x43D;&#x430; &#x43E;&#x434;&#x43D;&#x456;&#x439; &#x43D;&#x43E;&#x437;&#x456;</trd>
</drv>

<drv mrk="hop.0ludo">
  <kap><tld/>ludo</kap>
  <snc mrk="hop.0ludo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Salt<ref tip="super" cel="lud.0o.ago">ludo</ref>,
      &ccirc;e kiu oni <tld/>as tra vico de desegnitaj kvadratoj
      sen tu&scirc;i la liniojn, anka&ubreve; konata kiel &leftquot;paradizludo&rightquot; kaj &leftquot;marelo&rightquot;:
      <ekz>
        en la dana nomi&gcirc;as &leftquot;<nac>hinke-leg</nac>&rightquot;, kies rekta traduko estas 
        &leftquot;salteta ludo&rightquot;, en PIV2002 estas &leftquot;<tld/>ludo&rightquot;, &leftquot;paradizludo&rightquot; kaj 
        &leftquot;marelo&rightquot;, &dash; mi &scirc;atus scii <klr>[kiun]</klr> vorton vi kutime uzas en via
        familio
        <fnt>
          <aut>J. Nordfalk, N. Budha</aut>,
            <vrk>
            <url ref="http://javabog.dk/filmoj_en_esperanto/mejzi_ludtempo/mejzi_ludtempo_notoj.html">
            Traduko de la filmo "Mejzi Ludtempo" al Esperanto</url>
          </vrk>,
          <lok>&ccirc;. 1.2.1 Saltludo</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/14/Michelle_Obama_plays_hopscotch.jpg/640px-Michelle_Obama_plays_hopscotch.jpg" prm="PD">Michelle Obama ludanta <tld/>ludon &ccirc;e la universitato de Mumbai, Barato, Nov. 6, 2010
        <fnt>
          <aut>Chuck Kennedy, The White House from Washington, DC, P110610CK-0355</aut>
          <lok><url ref="&WCW;/File:Michelle_Obama_plays_hopscotch.jpg">&WCW;/File:Michelle_Obama_plays_hopscotch.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Himmel und H&ouml;lle</trd>,
      <trd>Hickelkasten</trd>,
      <trd>Himmelhopse</trd>,
      <trd>Reise zum Mond</trd>
    </trdgrp>
    <trd lng="en">hopscotch</trd>
    <trd lng="fr">marelle</trd>
  </snc>
  <trd lng="cs">sk&aacute;k&aacute;n&iacute; "pan&aacute;ka"</trd>
  <trd lng="da">hinke-leg</trd>
  <trdgrp lng="it">
    <trd>pampano<klr> (gioco)</klr></trd>, 
    <trd>settimana<klr> (gioco)</klr></trd>, 
    <trd>campana<klr> (gioco)</klr></trd>
  </trdgrp>
  <trd lng="nl">hinkelspel</trd>
  <trd lng="pl"><klr>gra w </klr>klasy</trd>
  <trd lng="sk">sk&aacute;kanie pan&aacute;ka <klr>(hra)</klr></trd>
  <trdgrp lng="uk">
    <trd><klr>(&#x433;&#x440;&#x430; &#x432;)</klr> &#x43A;&#x43B;&#x430;&#x441;&#x438;</trd>,
    <trd>&#x43D;&#x435;&#x431;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
