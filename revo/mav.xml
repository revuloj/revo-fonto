<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mav.xml,v 1.4 2024/01/25 20:10:35 afido Exp $">
    <kap>
      <rad>mav</rad>/a
    </kap>

<drv mrk="mav.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">POE</uzo>
  <snc mrk="mav.0a.malbona">
    <dif>
      <ref tip="dif" cel="bon.mal0a.maltauxga">Malbona</ref>:
      <ekz>
        se via verko estos <esc>hiperplena</esc> da redundaj neologismoj, &gcirc;i aspektos turpe kaj
        hide, kaj oni etikedos &gcirc;in <tld/>a
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ajn neologismojn ili anatemas imite la metodojn de la afganaj taliboj
        <fnt>
          <vrk><url ref="https://jorgecice.blogspot.com/2001/01/la-mava-lingvo-neologismoj-kaj.html">La mava lingvo: neologismoj kaj malneologismoj en esperanto</url></vrk>
          <lok>artikolo en la libro Lingva arto, 1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="acx.0a">a&ccirc;a</ref>,
      <ref cel="mis.0a">misa</ref>
    </refgrp>
    <trd lng="be">&c_d;&c_r;&c_e;&c_n;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>schlecht</trd>,
      <trd>mies</trd>
    </trdgrp>
    <trd lng="en">bad</trd>
    <trd lng="fr">mal</trd>
    <trdgrp lng="zh">
    <trd>&#x4E0D;&#x826F; <pr>b&#xF9;li&#xE1;ng</pr></trd>,
    <trd>&#x7CDF;&#x7CD5; <pr>z&#x101;og&#x101;o</pr></trd>,
    <trd>&#x4E0D;&#x5584; <pr>b&#xF9;sh&#xE0;n</pr></trd>
  </trdgrp>
</snc>
</drv>

  </art>

