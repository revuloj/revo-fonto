<art mrk="$Id: astak.xml,v 1.25 2023/10/13 17:08:38 revo Exp $">
<kap>
  <rad>astak</rad>/o
</kap>
<drv mrk="astak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="astake.0oj"><tld/>edoj</ref>
      (<trd lng="la">Astacus</trd>);
      <ref tip="lst" cel="kankr.0o" lst="voko:krustuloj">kankro</ref>
      kies pin&ccirc;iloj havas ru&gcirc;ajn artikojn kaj malsupran flankon:
      <ekz>
        <tld/>bredado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=422250">Listo de bredbestoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/22/Crayfish(Astacus_astacus).jpg/319px-Crayfish(Astacus_astacus).jpg" prm="CC-BY-SA-3.0"><tld/>o (Astacus_astacus), Portugalujo
        <fnt>
          <aut>MarioM, propra verko</aut>
          <lok><url ref="&WCW;/File:Crayfish(Astacus_astacus).jpg">&WCW;/File:Crayfish(Astacus_astacus).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_ch;&c_n;&c_y; &c_r;&c_a;&c_k; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_r;&c_je;&c_ch;&c_je;&c_n; &c_r;&c_a;&c_k;</trd>
  <trd lng="cs">rak</trd>
  <trd lng="de">Edelkrebs</trd>
  <trdgrp lng="en">
    <trd>crawfish</trd>,
    <trd>crayfish</trd>,
    <trd>crawdad</trd>,
    <trd>craw daddy</trd></trdgrp>
  <trd lng="fr">&eacute;crevisse</trd>
  <trdgrp lng="hu">
    <trd>folyami <ind>r&aacute;k</ind></trd>,
    <trd>nemes <ind>r&aacute;k</ind></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>lobster</trd>,
    <trd><ind>udang</ind> karang</trd>
  </trdgrp>
  <trd lng="it"><ind>gambero</ind> di fiume</trd>
  <trd lng="ja">&#x30B6;&#x30EA;&#x30AC;&#x30CB;  <pr>&#x3056;&#x308A;&#x304C;&#x306B;</pr></trd>
  <trd lng="nl">rivierkreeft</trd>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_n;&c_o;&c_j; <ind>&c_r;&c_a;&c_k;</ind> <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">rak <klr>(rie&ccaron;ny)</klr></trd>
  <trd lng="uk">&#x440;&#x430;&#x43A;</trd>
</drv>
</art>
