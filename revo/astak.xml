<vortaro>
<art mrk="$Id$">
<kap>
<rad>astak</rad>/o
</kap>
<drv mrk="astak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astak.0edoj"><tld/>edoj</ref>
      (<trd lng="la">astacus</trd>).  
    </dif>
  </snc>
  <trd lng="de">Edelkrebs</trd>
</drv>
<drv mrk="astak.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo de
      <ref tip="super" cel="krust.dekpiedaj0uloj">dekpiedaj krustuloj</ref>
      (<trd lng="la">Astacidae</trd>).
    </dif>
    <ref tip="prt" cel="kankr.0o">kankro</ref>
  </snc>
  <trd lng="de">Flu&szlig;krebse</trd>
</drv>
</art>
