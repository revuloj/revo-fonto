<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: psikol.xml,v 1.23 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>psikolog</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="&hcirc;">psi&hcirc;olog</rad>/o</kap></var>
</kap>
<drv mrk="psikol.0o">
  <kap><tld/>o,
    <var><kap><tld var="&hcirc;"/>o</kap></var>
  </kap>
  <uzo tip="fak">PSI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="psikol1.0isto">Psikologiisto</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la a&ubreve;toro bone sukcesas fari tion, &ccirc;ar li posedas la talenton de
        subtila <tld/>o, kiu atente esploras siajn pensojn kaj
        sentojn
        <fnt>
          <aut>Julian Modest</aut>: 
          <vrk>Sincere kaj honeste pri si mem (Gerrit Berveling: La unuaj 25 jaroj de mia
          memoro)</vrk>,
          <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 22a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj instruis, kiel solvi kverelojn kun klientoj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2004/008290.html">Kondukistoj bonkondutaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        strange, li estis <tld var="&hcirc;"/>o kaj povis brile konsili al aliaj, sed lia sarkasmemo
        estis finfine tro por mi
        <fnt>
          <bib>Monato</bib>
          <aut>Trevor Steele</aut>
          <vrk><url ref="&Monato;/2015/011474.php">Tridek jarojn poste</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">psycholog</trd>
  <trd lng="de">Psychologe</trd>
  <trd lng="en">psychologist</trd>
  <trdgrp lng="fr">
    <trd>psychologue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pszichol&oacute;gus</trd>,
    <trd>l&eacute;lekb&uacute;v&aacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#x5FC3;&#x7406;&#x5B66;&#x8005;  <pr>&#x3057;&#x3093;&#x308A;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">psycholoog</trd>
  <trd lng="no">psykolog</trd>
  <trd lng="pl">psycholog</trd>
  <trd lng="ro">psiholog</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">psychol&oacute;g</trd>
  <trd lng="uk">&#x43F;&c_s;&c_i;&c_h;&#x43E;&#x43B;&#x43E;&c_g;</trd>
</drv>
</art>
