<drv mrk="gust.0uminda">
  <kap><tld/>uminda</kap>
  <snc mrk="gust.0uminda.KUI">
    <dif>
      Kiun indas <tld/>umi &ndash; normale pro &gcirc;ia bongusto:
      <ekz>
        el&scirc;eli&gcirc;as la dol&ccirc;a kerno <tld/>uminda de frukto
        <fnt>
          <aut>N. Rossi</aut>,
          <vrk>Pri la lingvo serena de Baldur Ragnarsson</vrk>,
          <lok>Beletra Almanako, 2011-02 (10)</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_r;&c_t;&c_y;, &c_k;&c_a;&c_b; <ind>&c_p;&c_a;&c_k;&c_a;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_s;&c_m;&c_a;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
