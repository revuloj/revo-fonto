<drv mrk="gust.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="gust.sen0a.senkaraktera">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Indiferenta por la palato a&ubreve; la spirito; senkaraktera,
      seninteresa:
      <ekz>
        azoto estas senkolora, senodora kaj sen<tld/>a gaso;
      </ekz>
      <ekz>
        oni man&gcirc;as sen<tld/>a&jcirc;on sen salo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 6:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvo likas sen<tld/>e tra mia gor&gcirc;o
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> englutis grandegajn kvantojn de la sen<tld/>a, fadena kaj
        pastema turka konfita&jcirc;o
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e A&hcirc;med&#8217; Pa&scirc;a&#8217;, la granda pentristo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_ja;&c_s;&c_m;&c_a;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_z; <ind>&c_s;&c_m;&c_a;&c_k;&c_u;</ind></trd>
    </trdgrp>
    <trd lng="de">fade <klr>(ohne Geschmack)</klr></trd>
    <trdgrp lng="es">
      <trd>ins&iacute;pido</trd>,
      <trd>insulso</trd>
    </trdgrp>
    <trd lng="hu">&iacute;zetlen</trd>
    <trdgrp lng="pt">
      <trd>ins&iacute;pido</trd>,
      <trd>insulso</trd>
    </trdgrp>
  </snc>
  <snc mrk="gust.sen0a.neestetika">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Ne posedanta estetikan <ref cel="gust.0o.priVerko">guston<sncref/></ref>:
      <ekz>
        sen<tld/>e pompa kaj bombasta stilo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjorinoj vestitaj bon<tld/>e kaj sinjorinoj vestitaj sen<tld/>e
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis laca <klr>[&#8230;]</klr> de amindumaj rigardoj kaj sen<tld/>a flata&jcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> fini&gcirc;as kiel sen<tld/>a ludo de marionetoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_je;&c_z;&c_g;&c_u;&c_s;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_e;&c_s;&c_t;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="hu">&iacute;zl&eacute;stelen</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bez chuti</trd>,
    <trd>bez vkusu</trd>,
    <trd>nechutn&yacute;</trd>,
    <trd>nevkusn&yacute;</trd>
  </trdgrp>
  <trd lng="de">geschmacklos</trd>
  <trd lng="en">tasteless</trd>
  <trdgrp lng="es">
    <trd>mal gusto<klr> (vestir, decorar)</klr></trd>,
    <trd>sin gusto</trd>,
    <trd>vulgar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sans <ind>go&ucirc;t</ind></trd>,
    <trd>insipide</trd>,
    <trd>vulgaire<klr tip="ind"> (d&eacute;pourvu de go&ucirc;t)</klr></trd>
  </trdgrp>
  <trd lng="id">hambar</trd>
  <trdgrp lng="ja">
    <trd>&#21619;&#12398;&#12394;&#12356;  <pr>&#12354;&#12376;&#12398;&#12394;&#12356;</pr></trd>,
    <trd>&#12362;&#12418;&#12375;&#12429;&#12415;&#12398;&#12394;&#12356;</trd>,
    <trd>&#28961;&#21619;&#20094;&#29157;&#12394;  <pr>&#12416;&#12415;&#12363;&#12435;&#12381;&#12358;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">smaakloos</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bez chuti</trd>,
    <trd>bez vkusu</trd>,
    <trd>nechutn&yacute;</trd>,
    <trd>nevkusn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_s;&c_m;&c_a;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_b;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_j; &c_s;&c_m;&c_a;&c_k;&c_u;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_s;&c_m;&c_a;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20047;&#21619; <pr>f&aacute;w&egrave;i</pr></trd>,
    <trd>&#19981;&#21487;&#21475; <pr>b&ugrave;k&ecaron;k&#466;u</pr></trd>,
    <trd>&#27794;&#26377;&#21619;&#36947; <pr>m&eacute;iy&#466;uw&egrave;idao</pr></trd>,
    <trd>&#24248;&#20439; <pr>y&omacron;ngs&uacute;</pr></trd>,
    <trd>&#27794;&#21619;&#20818; <pr>m&eacute;iw&egrave;ir</pr></trd>,
    <trd>&#26080;&#21619; <pr>w&uacute;w&egrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gust.vin0umado">
  <kap>vin<tld/>umado</kap>
  <snc>
    <dif>
      Ekprovi la <tld/>on kaj kvaliton de la vinoj:
      <ekz>
        elekti inter vin<tld/>umado a&ubreve; ripozado &ccirc;e varmega parko
        <fnt>
          <bib>Kon11</bib>
          <lok>En la lando de la kuloj &ndash; Kronikoj de IJS 2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        bona <ind>vin<tld/>umisto</ind> rekonas la originan landon je la unua
        sorbo, la grundon je la dua kaj la jaron je la tria.
        <trd lng="be">&c_d;&c_e;&c_g;&c_u;&c_s;&c_t;&c_a;&c_t;&c_a;&c_r; &c_v;&c_ib;&c_n;</trd>
        <trdgrp lng="cs">
          <trd>ko&scaron;t&eacute;r</trd>,
          <trd>ko&scaron;t&eacute;&rcaron;</trd>,
          <trd>ko&scaron;t&yacute;&rcaron;</trd>,
          <trd>ochutn&aacute;va&ccaron; v&iacute;na</trd>
        </trdgrp>
        <trd lng="de">Weinverkoster</trd>
        <trd lng="en">wine taster</trd>
        <trdgrp lng="es">
         <trd>degustador</trd>
         <trd>catador</trd>
        </trdgrp>
        <trd lng="fr">d&eacute;gustateur<klr tip="ind"> (de vin)</klr></trd>
        <trd lng="hu">bork&oacute;stol&oacute;</trd>
        <trd lng="id">pen<ind>cicip</ind> anggur</trd>
        <trd lng="ja">&#12527;&#12452;&#12531;&#37969;&#23450;&#20154;  <pr>&#12431;&#12356;&#12435;&#12363;&#12435;&#12390;&#12356;&#12376;&#12435;</pr></trd>
        <trd lng="nl">wijnproever</trd>
        <trdgrp lng="pt">
         <trd>degustador</trd>
         <trd>provador</trd>
        </trdgrp>
        <trd lng="ru">&c_d;&c_je;&c_g;&c_u;&c_s;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>
        <trd lng="sk">ochutn&aacute;va&ccaron; v&iacute;na</trd>
        <trd lng="uk">&c_d;&c_je;&c_gu;&c_u;&c_s;&c_t;&c_a;&c_t;&c_o;&c_r; &c_v;&c_i;&c_n;</trd>
      </ekz>
    </dif>
  </snc>
<trd lng="de">Weinverkostung</trd>
<trd lng="id"><baz>cicip</baz>pencicipan anggur</trd>
</drv>
