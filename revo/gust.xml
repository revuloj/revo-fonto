  </trdgrp>
</drv>

<drv mrk="gust.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      Anticipe imagata <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tuta animo de Anjo jubilis pro la anta&ubreve;<tld/>o de tio,
        kion morga&ubreve; &scirc;i rakontados al la najbaroj
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Vorgeschmack</trd>
  <trd lng="en">foretaste</trd>
  <trd lng="es">imaginar (anticipar) el sabor</trd>
  <trd lng="fr">avant-gout</trd>
  <trd lng="nl">voorsmaak</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_v;&c_k;&c_u;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="gust.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="gust.bon0a.agrabla">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Agrabla por la palato a&ubreve; la spirito:
      <ekz>
        bon<tld/>a viando, vesto, meblaro, dramo.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_m;&c_a;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>k&ouml;stlich</trd>,
      <trd>vorz&uuml;glich</trd>,
      <trd>lecker</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>rico</trd>,
      <trd>sabroso</trd>,
      <trd>exquisito</trd>
    </trdgrp>
    <trd lng="fr">bon<klr tip="ind"> (au go&ucirc;t)</klr></trd>
    <trdgrp lng="hu">
      <trd>j&oacute;&iacute;z&udblac;</trd>,
      <trd>&iacute;zletes</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>lekker</trd>,
      <trd>smakelijk</trd>
    </trdgrp>
    <trd lng="pt">saboroso</trd>
    <trd lng="ru">&c_v;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
    <trd lng="zh">&#39854;&#32654; <pr>xi&amacron;nm&ecaron;i</pr></trd>
</snc>
  <snc mrk="gust.bon0a.estetika">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Posedanta estetikan <tld/>on:
      <ekz>
        bon<tld/>a kritikisto.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_u;&c_s;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
    <trd lng="de">geschmackvoll</trd>
    <trd lng="es">con <ind>gusto</ind></trd>
    <trd lng="fr">de bon <ind>gout</ind></trd>
    <trd lng="hu">j&oacute; <ind>&iacute;zl&eacute;s&udblac;</ind></trd>
    <trd lng="nl">smaakvol</trd>
    <trd lng="pt">de bom <ind>gosto</ind><klr tip="amb">
    (pessoa)</klr></trd>
    <trd lng="ru">&c_s; &c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_m; <ind>&c_v;&c_k;&c_u;&c_s;&c_o;&c_m;</ind></trd>
  </snc>
  <trd lng="bo">&t_ba;&t_rata;&t_naro;&t_ba;&t_tsheg;&t_cha;&t_drengbu;&t_na;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>chutn&yacute;</trd>,
    <trd>lahodn&yacute;</trd>
  </trdgrp>
  <trd lng="en">tasteful</trd>
  <trdgrp lng="id">
    <trd>enak</trd>,
    <trd>lezat</trd>,
    <trd>sedap</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12362;&#12356;&#12375;&#12356;</trd>,
    <trd>&#12358;&#12414;&#12356;</trd>,
    <trd>&#36259;&#21619;&#12398;&#33391;&#12356;  <pr>&#12375;&#12421;&#12415;&#12398;&#12356;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="sk">chutn&yacute;</trd>
  <trdgrp lng="uk">
