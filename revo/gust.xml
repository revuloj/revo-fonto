    </trdgrp>
    <trdgrp lng="nl">
      <trd>lekker</trd>,
      <trd>smakelijk</trd>
    </trdgrp>
    <trd lng="pt">saboroso</trd>
    <trd lng="ru">&c_v;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
    <trd lng="zh">&#39854;&#32654; <pr>xi&amacron;nm&ecaron;i</pr></trd>
  </snc>
  <snc mrk="gust.bon0a.estetika">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Montranta <ref cel="esteti.0a">estetika</ref>n <ref
      cel="gust.0o.priVerko">guston<sncref/></ref>:
      <ekz>
        bon<tld/>a kritikisto;
