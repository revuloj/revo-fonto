<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cikloid</rad>/o
</kap>
<drv mrk="cikloi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto apartenanta al cirklo, kiu ruli&gcirc;as
      sur fiksa rekto:
      <ekz>
        la parametraj ekvacioj de <tld/>o estas de la tipo 
        <frm><k>x</k> = R(<k>t</k>-sin<k>t</k>)</frm>
        kaj <frm><k>y</k> = R(1-cos<k>t</k>)</frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="epicik.0o">epicikloido</ref>,
      <ref cel="hipoci.0o">hipocikloido</ref>.
    </refgrp>
  </snc>
  <bld lok="../bld/cikloi.gif">
    cikloido
  </bld>
  <trd lng="de">Zykloide</trd>
  <trd lng="el">&kappa;&ypsilon;&kappa;&lambda;&omikron;&epsilon;&jota;&delta;&epsilon_ton;&sigma_fina;</trd>
  <trd lng="en">cycloid</trd>
  <trd lng="fr">cyclo&iuml;de</trd>
  <trd lng="hu">ciklois</trd>
  <trd lng="pt">cicl&oacute;ide</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</trd>
  <trd lng="sk">cykloida</trd>
</drv>
</art>
