<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diagno.xml,v 1.26 2023/11/04 14:29:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diagnoz</rad>/o
</kap>
<drv mrk="diagno.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="diagno.0o.MED">
    <dif>
      Ekkono de malsano per &gcirc;iaj simptomoj:
      <ekz>
        laboratoriaj testoj por tuja <tld/>o
        <fnt>
        <aut>P. S. Viana</aut>, 
        <vrk><url ref="&Monato;/2002/007848.html">Sukceso kontra&ubreve; aidoso</url></vrk>,
        <bib>Monato</bib>, 
        <lok>2002/08, p. 15</lok></fnt>.
      </ekz>
      
      <ekz>
      la &gcirc;ustan <tld/>on oni do devas determini per laboratoriaj esploroj
      <fnt>
      <aut>Vladimir Lemelev</aut>
      <vrk><url ref="&Monato;/2003/007746.html">Virusaj hepatitoj</url></vrk>
      <bib>Monato</bib>
      <lok>2003/01, p. 18</lok>
      </fnt>
      </ekz>
    </dif>
    <trd lng="zh">&#x8A3A;&#x65B7; <pr>zh&#x11B;ndu&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_g;&c_n;&c_a;&c_z;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>diagn&oacute;za</trd>,
    <trd>l&eacute;ka&rcaron;sk&yacute; n&aacute;lez</trd>
  </trdgrp>
  <trd lng="de">Diagnose</trd>
  <trd lng="en">diagnosis</trd>
  <trd lng="es">diagn&oacute;stico</trd>
  <trd lng="fr">diagnostic</trd>
  <trd lng="he">&#x5D0;&#x5B4;&#x5D1;&#x5D7;&#x5D5;&#x5BC;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>diagn&oacute;zis</trd>,
    <trd>k&oacute;risme</trd>,
    <trd>k&oacute;rmeghat&aacute;roz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A3A;&#x65AD;  <pr>&#x3057;&#x3093;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x8A18;&#x76F8;  <pr>&#x304D;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">diagnose</trd>
  <trd lng="pl">diagnoza</trd>
  <trdgrp lng="pt">
    <trd>diagn&oacute;stico</trd>,
    <trd>diagnose</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>diagn&oacute;za</trd>,
    <trd>diagn&oacute;za</trd>
  </trdgrp>
  <trd lng="sv">diagnos</trd>
  <trd lng="uk">&c_d;&c_ib;&c_a;&c_g;&#x43D;&#x43E;&c_z;</trd>
</drv>

<drv mrk="diagno.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="diagno.0i.MED">
    <dif>
      Difini malsanon la&ubreve; &gcirc;iaj simptomoj:
      <ekz>
        estas malfacile <tld/>i mildan koratakon
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>;
      </ekz>
      <ekz>
      oni menciis universitaton en Maputo, kie oni esploras novan procedon por <tld/>i tuberkulozon
      <fnt>
      <aut>Garbhan MacAoidh</aut>
      <vrk><url ref="&Monato;/2016/011620.php">Renkonti&gcirc;o kun rimarkinda homo</url></vrk>
      <bib>Monato</bib>
      <lok>2016/05, p. 25</lok>
      </fnt>
      </ekz>
    </dif>
    <trd lng="zh">&#x786E;&#x8BCA; <pr>qu&#xE8;zh&#x11B;n</pr></trd>
</snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_g;&c_n;&c_a;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>diagnostikovat</trd>,
    <trd>diagnozovat</trd>,
    <trd>rozpoznat</trd>,
    <trd>stanovit diagn&oacute;zu</trd>,
    <trd>vy&scaron;et&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>diagnostizieren</trd>,
    <trd>feststellen</trd>
  </trdgrp>
  <trd lng="en">diagnose</trd>
  <trd lng="es">diagnosticar</trd>
  <trd lng="fr">diagnostiquer</trd>
  <trd lng="he">&#x5DC;&#x5D0;&#x5D1;&#x5D7;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>diagnosztiz&aacute;l</trd>,
    <trd>k&oacute;rt meg&aacute;llap&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#x8A3A;&#x65AD;&#x3059;&#x308B;  <pr>&#x3057;&#x3093;&#x3060;&#x3093;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>stellen van een diagnose</trd>,
    <trd>diagnosticeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>diagnozowa&cacute;</trd>,
    <trd>stawia&cacute; <ind>diagnoz&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
    &c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>,
    <trd>&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>diagnostikova&tcaron;</trd>,
    <trd>ur&ccaron;i&tcaron; <ind>diagn&oacute;zu</ind></trd>,
    <trd>ur&ccaron;i&tcaron; diagn&oacute;zu</trd>
  </trdgrp>
  <trd lng="sv">diagnosticera</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_ib;&c_a;&c_g;&#x43D;&#x43E;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_d;&c_ib;&c_a;&c_g;&#x43D;&#x43E;&c_z;</trd>,
    <trd>&c_v;&c_i;&c_z;&#x43D;&c_a;&c_ch;&c_a;&c_t;&c_i; (&c_h;&c_v;&#x43E;&c_r;&#x43E;&c_b;&c_u;)</trd>
  </trdgrp>
</drv>
</art>
