<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>diagnoz</rad>/o
</kap>
<drv mrk="diagno.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ekkono de malsano per &gcirc;iaj simptomoj:
      <ekz>
        laboratoriaj testoj por tuja <tld/>o<fnt><aut>Paulo S.
        Viana</aut>: <vrk>Sukceso kontra&ubreve; aidoso</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 8a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Diagnose</trd>
  <trd lng="es">diagn&oacute;stico</trd>
  <trd lng="fr">diagnostic</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>
  <trd lng="sk">diagn&oacute;za</trd>
</drv>

<drv mrk="diagno.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Difini malsanon la&ubreve; &gcirc;iaj simptomoj:
      <ekz>
        estas malfacile <tld/>i mildan koratakon<fnt><aut>David
        Curtis</aut>: <vrk>Koratako</vrk>, <bib>Monato</bib><lok>jaro
        2002a, numero 1a, p. 28-34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>diagnostizieren</trd>,
    <trd>feststellen</trd>
  </trdgrp>
  <trd lng="es">diagnosticar</trd>
  <trd lng="fr">diagnostiquer</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>,
    <trd>&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ur&ccaron;i&tcaron; <ind>diagn&oacute;zu</ind></trd>,
    <trd>diagnostikova&tcaron;</trd>
  </trdgrp>
</drv>
</art>
