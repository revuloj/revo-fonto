<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>diagnoz</rad>/o
</kap>
<drv mrk="diagno.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Ekkono de malsano per &gcirc;iaj simptomoj. 
</dif>
</snc>
  <trd lng="de">Diagnose</trd>
<trd lng="fr">diagnostic</trd>
<trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>
</drv>
<drv mrk="diagno.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Difini malsanon la&ubreve; &gcirc;iaj simptomoj.  
</dif>
</snc>
  <trdgrp lng="de">
     <trd>diagnostizieren</trd>,
     <trd>feststellen</trd>
  </trdgrp>
<trd lng="fr">diagnostiquer</trd>
<trdgrp lng="ru">
  <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>,
  <trd>&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</trdgrp>
</drv>
</art>
