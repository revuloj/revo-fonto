<art mrk="$Id: abrota.xml,v 1.24 2023/10/12 16:57:42 revo Exp $">
<kap>
  <rad>abrotan</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="abrota.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="artemi.0o">artemizio</ref>
      (<trd lng="la">Artemisia abrotanum</trd>)
      havanta odoron de citrona &scirc;elo:
      <ekz>
        <tld/>o estas arbusto alta 1&nbsp;m <klr>[havanta]</klr> duoble pinatajn,
        citron&scirc;el-odorajn foliojn, kiujn oni iam uzis kiel spica&jcirc;o, kaj el
        kiuj el&ccirc;erpata esenco <klr>[protektas]</klr> kontra&ubreve; kuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29631">Abrotano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/8f/Gardenology.org-IMG_2771_rbgs11jan.jpg/640px-Gardenology.org-IMG_2771_rbgs11jan.jpg" prm="CC BY-SA 3.0">abrotano en la Re&gcirc;a Botanika &Gcirc;ardeno de Sidneo, A&ubreve;stralio
         <fnt>
           <aut>Raffi Kojian</aut>
           <lok><url ref="&WCW;/File:Gardenology.org-IMG_2771_rbgs11jan.jpg">&WCW;/File:Gardenology.org-IMG_2771_rbgs11jan.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_y;&c_n; &c_m;&c_ja;&c_c;&c_jo;&c_l;&c_ch;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_n; &c_l;&c_je;&c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bo&zcaron;&iacute; d&rcaron;evec</trd>,
    <trd>brotan</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Eberraute</trd>,
    <trd>Zitronenkraut</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>southern wormwood</trd>,
    <trd>lad's love</trd>,
    <trd>old man</trd>
  </trdgrp>
  <trd lng="fr">aurone</trd>
  <trdgrp lng="hu">
    <trd>istenfa</trd>,
    <trd>ebrutafa</trd>
  </trdgrp>
  <trd lng="it">abrotano</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30C0;&#x30C1;&#x30E8;&#x30E2;&#x30AE;  <pr>&#x304D;&#x3060;&#x3061;&#x3088;&#x3082;&#x304E;</pr></trd>,
    <trd>&#x30B5;&#x30B6;&#x30F3;&#x30A6;&#x30C3;&#x30C9;  <pr>&#x3055;&#x3056;&#x3093;&#x3046;&#x3063;&#x3069;</pr></trd>
  </trdgrp>
  <trd lng="nl">citroenkruid</trd>
  <trd lng="pl">drzewko bo&zdot;e</trd>
  <trd lng="pt">abr&oacute;tano</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_zh;&c_mol;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;<klr tip="ind"> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">bo&zcaron;ie drievko</trd>
  <trd lng="sv">&aring;brodd</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&c_i;&#x43D; &#x43B;&c_ib;&#x43A;&c_a;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>
