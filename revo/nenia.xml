<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nenia.xml,v 1.20 2020/11/24 18:34:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nenia</rad>
</kap>
<drv mrk="nenia.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="nenia.0.KOMUNE">
    <dif>
      De neniu ajn <ref cel="spec.0o">speco</ref> a&ubreve; <ref cel="kvalit.0o">kvalito</ref>:
      <ekz>
        ni bezonas <tld/>n helpon;
      </ekz>
      <ekz>
        <tld/> el liaj malbenoj povas min iom malhelpi
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        tian dancanton &scirc;i ankora&ubreve; <tld/>n havis
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis, kion <tld/> homo devus scii
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi nun faras persone <tld/>jn &scirc;an&gcirc;ojn en la lingvo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        fian&ccirc;on de l' sorto difinitan forpelos <tld/> malhelpo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; bato senatenda ekzistas <tld/> defendo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> konstruo povas esti sen bruo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        deklinacioj tute estas bezonaj <tld/>j
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_ja;&c_k;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>nijak&yacute;</trd>,
    <trd>&zcaron;&aacute;dn&yacute;</trd>
  </trdgrp>
  <trd lng="de">keinerlei</trd>
  <trdgrp lng="en">
    <trd>not any</trd>,
    <trd>no <klr>(adj.)</klr></trd>
  </trdgrp>
  <trd lng="fr">d'<ind>aucune sorte</ind></trd>
  <trd lng="hu">semmilyen</trd>
  <trd lng="nl">generlei</trd>
  <trdgrp lng="pl">
    <trd>&zdot;aden</trd>,
    <trd>nijaki</trd>
  </trdgrp>
  <trd lng="pt">de nenhum <klr tip="amb">tipo</klr></trd>
  <trd lng="ro">nici unul</trd>
  <trd lng="ru">&c_n;&c_i;&c_k;&c_a;&c_k;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nijak&yacute;</trd>,
    <trd>&zcaron;iaden</trd>
  </trdgrp>
  <trd lng="sv">inget slags</trd>
</drv>
</art>
