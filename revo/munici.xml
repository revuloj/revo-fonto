<kap>
   <ofc>1</ofc>
   <rad>munici</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="munici.0o">
  <kap><tld/>o</kap>
  <snc mrk="munici.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Provizo de militistaj pafa&jcirc;oj: pulvo, kugloj,
      obusoj kaj similaj:
      <ekz>
       la matrosoj amasigis <tld/>on malanta&ubreve; la barikadoj kaj ili 
       ka&ubreve;ri&gcirc;is 
       <fnt>
         <aut>Hevesi L., trad. Vizi L.</aut>,
         <vrk><url ref="https://sites.google.com/site/laaventurojdeandreojelky">
         La aventuroj de Andreo Jelky</url></vrk>,
         <lok>[2010]</lok>
       </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_o;&c_je;&c_p;&c_r;&c_y;&c_p;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_z;&c_a;&c_p;&c_a;&c_s;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_k;&c_a;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">munici&oacute;</trd>
  <trdgrp lng="cs">
    <trd>munice</trd>,
    <trd>st&rcaron;elivo</trd>
  </trdgrp>
  <trd lng="en">munitions</trd>
  <trd lng="es">munici&oacute;n</trd>
  <trd lng="fr">munitions</trd>
  <trdgrp lng="hu">
    <trd>mun&iacute;ci&oacute;</trd>,
    <trd>l&odblac;szer</trd>
  </trdgrp>
  <trd lng="it">munizione</trd>
  <trd lng="nl">munitie</trd>
  <trd lng="pl">amunicja</trd>
  <trd lng="pt">muni&ccedil;&atilde;o</trd>
  <trd lng="ro">muni&#539;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_je;&c_p;&c_r;&c_i;&c_p;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_z;&c_a;&c_p;&c_a;&c_s;</trd>
