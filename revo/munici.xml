<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: munici.xml,v 1.24 2024/01/26 19:05:54 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>munici</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="munici.0o">
  <kap><tld/>o</kap>
  <snc mrk="munici.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="proviz.0o.ajxo">Provizo</ref> de militistaj <ref cel="paf.0ajxo">pafa&jcirc;o</ref>j: 
      <ref cel="pulv.0o">pulvo</ref>, 
      <ref tip="sub" cel="kugl.0o">kuglo</ref>j, <ref tip="sub" cel="obus.0o">obuso</ref>j kaj similaj:
      <ekz>
       la matrosoj amasigis <tld/>on malanta&ubreve; la barikadoj kaj ili 
       ka&ubreve;ri&gcirc;is 
       <fnt>
         <aut>Hevesi L., trad. Vizi L.</aut>,
         <vrk><url ref="https://sites.google.com/site/laaventurojdeandreojelky">
         La aventuroj de Andreo Jelky</url></vrk>,
         <lok>[2010]</lok>
       </fnt>;
      </ekz>
      <ekz>
        milicano en la tuta ekipo kaj <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i domo ne estis <tld/>paligita
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        artileria <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009466.php">Tragedio en Gerdec</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ofte tia interparolado &ndash; post kiam la kutimaj sakrokanonadoj forpafis &ccirc;ian
        <tld/>on &ndash; fini&gcirc;as
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_o;&c_je;&c_p;&c_r;&c_y;&c_p;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_z;&c_a;&c_p;&c_a;&c_s;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_k;&c_a;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">munici&oacute;</trd>
  <trdgrp lng="cs">
    <trd>munice</trd>,
    <trd>st&rcaron;elivo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Munition</trd>,
    <trd>Ladung <klr>(Gewehr)</klr></trd>
  </trdgrp>
  <trd lng="en">munitions</trd>
  <trd lng="es">munici&oacute;n</trd>
  <trd lng="fr">munitions</trd>
  <trdgrp lng="hu">
    <trd>mun&iacute;ci&oacute;</trd>,
    <trd>l&odblac;szer</trd>
  </trdgrp>
  <trd lng="it">munizione</trd>
  <trd lng="ja">&#x5F3E;&#x85AC;  <pr>&#x3060;&#x3093;&#x3084;&#x304F;</pr></trd>
  <trd lng="nl">munitie</trd>
  <trd lng="pl">amunicja</trd>
  <trd lng="pt">muni&ccedil;&atilde;o</trd>
  <trd lng="ro">muni&#x21B;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_je;&c_p;&c_r;&c_i;&c_p;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_z;&c_a;&c_p;&c_a;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mun&iacute;cia</trd>,
    <trd>strelivo</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_o;&c_j;&c_o;&c_v;&c_ib; &c_p;&c_r;&c_i;&c_p;&c_a;&c_s;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x5F39;&#x836F; <pr>d&#xE0;ny&#xE0;o</pr></trd>,
    <trd>&#x5F48;&#x85E5; <pr>d&#xE0;ny&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="munici.0ejo">
  <kap>
    <tld/>ejo, <var><kap><tld/>deponejo</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="arsena.0o.armilejo">Arsenalo</ref> por stoki precipe <tld/>on:
      <ekz>
        <klr>[li]</klr> finance subtenis <klr>[&#x2026;]</klr> familiojn afliktitajn de
        eksplodo &ccirc;e la <nom>Gerdec</nom>-<tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2009/009669.php">Pli amu ...</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Serbio estis konstruitaj novaj <tld/>o-deponejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2013/010867.php">Detruo-dilemo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Munitionsdepot</trd>
  <trdgrp lng="ja">
    <trd>&#x5F3E;&#x85AC;&#x5EAB;  <pr>&#x3060;&#x3093;&#x3084;&#x304F;&#x3053;</pr></trd>,
    <trd>&#x706B;&#x85AC;&#x5EAB;  <pr>&#x304B;&#x3084;&#x304F;&#x3053;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x519B;&#x706B;&#x5E93; <pr>j&#x16B;nhu&#x1D2;k&#xF9;</pr></trd>,
    <trd>&#x8ECD;&#x706B;&#x5EAB; <pr>j&#x16B;nhu&#x1D2;k&#xF9;</pr></trd>
  </trdgrp>
</drv>



</art>
