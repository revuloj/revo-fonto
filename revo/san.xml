<drv mrk="san.re0igxi">
  <kap>re<tld/>iĝi</kap>
  <snc mrk="san.re0igxi.MED">
    <dif>
      Retrovi la <tld/>on post misfarto:
      <ekz>
        re<tld/>iĝi el rabio.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">guérir<klr tip="amb"> (intr.)</klr></trd>
  <trd lng="hu">meggyógyul</trd>
  <trdgrp lng="it">
