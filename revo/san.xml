<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>san</rad>/a
</kap>
<drv mrk="san.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Kvalifikas organismon, en kiu &ccirc;iuj organoj normale funkcias:
      <ekz>
        se mi estus <tld/>a, mi estus feli&ccirc;a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en <tld/>a korpo <tld/>a spirito
        (<ref tip="vid" cel="pruden.0o">prudento</ref>);
      </ekz>
      <ekz>
        (f) esp-o <tld/>e eltenis &ccirc;iujn ventojn kaj
        pu&scirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio malpermesas  al la lingvo libere kaj <tld/>e 
        disvolvi&gcirc;i<fnt>Z</fnt>.  
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zdrav&yacute;</trd>
  <trd lng="de">gesund</trd>
  <trd lng="en">healthy</trd>
  <trd lng="nl">gezond</trd>
  <trd lng="ru">&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
<drv mrk="san.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io <tld/>a; bonfarto:
      <ekz><tld/>o estas pli valora ol oro;</ekz>
      <ekz><tld/>osubtenanta ridado<fnt>Z</fnt>;</ekz>
      <ekz>trinki pro ies <tld/>o<fnt>Z</fnt>;</ekz>
      <ekz>&gcirc;i estu por vi al <tld/>o<fnt>Z</fnt>;</ekz>
      <ekz>
        anstata&ubreve; interfrapado de la glasoj li faris por<tld/>an
        signon per siaj okuloj<fnt>Z</fnt>;
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">Gesundheit</trd>
  <trd lng="en">health</trd>
  <trd lng="nl">gezondheid</trd>
  <trd lng="ru">&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>
</drv>
<drv mrk="san.0o.MOS">
<kap>Monda Organiza&jcirc;o pri <tld lit="S"/>o</kap>
<mlg>MOS</mlg>
<uzo tip="fak">MED</uzo>
<fnt><bib>ManPol</bib></fnt>
<snc>
<dif>
Speciala <ref tip="super" cel="organi2.0ajxo">organiza&jcirc;o</ref> de <ref tip="malprt" cel="naci.0o.regno.UN">Unui&gcirc;intaj Nacioj</ref> por kontra&ubreve;batali <ref cel="epidem.0o">epidemio</ref>jn kaj por plibonigi higienon kaj <tld/>on: <ekz>Monda Organiza&jcirc;o pri <tld lit="S"/>o (MOS) en sia lastatempa raporto deklaris, ke la proverbo &leftquot;&ccirc;iutage unu glason da biero kaj la kuracistoj estos superfluaj&rightquot; havas nenian sciencan bazon, kaj refoje avertis, ke alkoholo povas ka&ubreve;zi la plej gravajn san-problemojn en la mondo. 
<fnt><aut>PENG Juan</aut>:
<vrk><url ref="&Monato;/007379.html">Alkohola&jcirc;o - &ccirc;iam dan&gcirc;era</url></vrk>, 
<bib>Monato</bib>, 
<lok>n-ro 04/2001</lok>
</fnt>.
</ekz>
</dif>
</snc>
<trdgrp lng="de">
   <trd>Weltgesundsheids-Organisation</trd>,
<trd>WGO</trd>
</trdgrp>
<trdgrp lng="en">
   <trd>World <ind>Health</ind> Organisation</trd>,
<trd>WHO</trd>
</trdgrp>
<trdgrp lng="es">
   <trd>Organizaci&oacute;n Mundial de la <ind>Salud</ind></trd>,
<trd>OMS</trd>
</trdgrp>
<trdgrp lng="fr">
   <trd>Organisation Mondiale de la <ind>Sant&eacute;</ind></trd>,
<trd>OMS</trd>
</trdgrp>
<trdgrp lng="nl">
   <trd>Wereldgezondheidsorganisatie</trd>,
<trd>WGO</trd>
</trdgrp>
<trdgrp lng="pt">
   <trd>Organiza&ccedil;&atilde;o Mundial de <ind>Sa&uacute;de</ind></trd>,
<trd>OMS</trd>
</trdgrp>
</drv>
<drv mrk="san.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io mal<tld/>a; malbonfarto.
    </dif>
  </snc>
  <trd lng="de">Krankheit</trd>
  <trd lng="en">desease</trd>
  <trd lng="nl">ziekte</trd>
</drv>

<drv mrk="san.0iga">
  <kap><tld/>(ig)a</kap>
  <snc>
    <dif>
      Favora por la <tld/>o, ta&ubreve;ga por subteni la bonfarton:
      <ekz><tld/>a pano<fnt>Z</fnt>;</ekz>
      <ekz>maldol&ccirc;a por la lango, sed <tld/>iga por la sango<fnt>Z</fnt>;</ekz>
      <ekz>&gcirc;oja koro estas <tld/>iga<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="higien.0a">higiena</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>gezond</trd>,
    <trd>heilzaam</trd>
  </trdgrp>
<trdgrp lng="ru">
    <trd>&c_o;&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j; &c_d;&c_l;&c_ja; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="san.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Malbonfarta:
      <ekz>cerbomal<tld/>a;</ekz>
      <ekz>mal<tld/>o de gor&gcirc;o;</ekz>
      <ekz>mal<tld/>eca paleco <klr>(f.)</klr>;</ekz>
      <ekz>tiu okazo mal<tld/>igis nian financan staton;</ekz>
      <ekz>de rigardo tro alta mal<tld/>i&gcirc;as okulo<fnt>Z</fnt>.</ekz> 
    </dif>
  </snc>
  <trd lng="de">ungesund</trd>
  <trdgrp lng="nl">
    <trd>ziek</trd>,
    <trd>ongezond</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;</trd>
</drv>
<drv mrk="san.mal0ulejo">
  <kap>mal<tld/>ulejo</kap>
  <snc>
    <dif>
      Loko, kie oni kuracas mal<tld/>ulojn.
    </dif>
    <refgrp tip="sub">
      <ref cel="hospit.0o">hospitalo</ref>,
      <ref cel="sanato.0o">sanatorio</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">ziekenhuis</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
<drv mrk="san.re0igi">
  <kap>re<tld/>igi</kap>
  <snc>
    <dif>
      Redoni la <tld/>on al iu sukcese kuraci:
      <ekz>
        la Eternulo banda&gcirc;os la difektojn de sia popolo kaj re<tld/>igos
        &gcirc;iajn vundojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>re<tld/>i&gcirc;i</ind> <trd lng="ru">&c_v;&c_y;&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_je;&c_t;&c_mol;</trd>
         el (a&ubreve; post) rabio;
      </ekz>
      <ekz>nere<tld/>igebla vundo<fnt>Z</fnt>.</ekz> 
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>genezen</trd>,
    <trd>herstellen</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_l;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="san.re0igilo">
  <kap><tld/>ilo<fnt>B</fnt>, re<tld/>igilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Kuracilo. 
    </dif>
    <refgrp tip="sin">
      <ref cel="medika1.0o">medikamento</ref>,
      <ref cel="drog.0o">drogo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Medizin</trd>
  <trdgrp lng="nl">
    <trd>geneesmiddel</trd>,
    <trd>medicijn</trd>
  </trdgrp>
<trd lng="ru">&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
