<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>san</rad>/a
</kap>
<drv mrk="san.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Kvalifikas organismon, en kiu &ccirc;iuj organoj normale funkcias:
      <ekz>Se mi estus sana, mi estus feli&ccirc;a
      <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;</ekz>
      <ekz>
        <ind>en <tld/>a korpo <tld/>a spirito</ind>
        (<ref tip="vid" cel="pruden.0o">prudento</ref>);
        <trd lng="ru">&c_v; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_o;&c_m; &c_t;&c_je;&c_l;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;
        &c_d;&c_u;&c_h;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto <tld/>e eltenis &ccirc;iujn ventojn kaj
        pu&scirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio malpermesas al la lingvo libere kaj <tld/>e
        disvolvi&gcirc;i<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zdrav&yacute;</trd>
  <trd lng="de">gesund</trd>
  <trd lng="en">healthy</trd>
  <trdgrp lng="fr">
    <trd>sain<klr> (en bonne sant&eacute;)</klr></trd>,
    <trd>en bonne <ind>sant&eacute;</ind></trd>
  </trdgrp>
  <trd lng="he">&bet;&resh;&yod;&alef;</trd>
  <trd lng="hu">eg&eacute;szs&eacute;ges</trd>
  <trd lng="nl">gezond</trd>
  <trd lng="ru">&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="vo">saunik</trd>
</drv>

<drv mrk="san.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io <tld/>a; bonfarto:
      <ekz>
        <tld/>o estas pli valora ol oro;
      </ekz>
      <ekz>
        <tld/>osubtenanta ridado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trinki pro ies <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;i estu por vi al <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; interfrapado de la glasoj li faris por<tld/>an
        signon per siaj okuloj<fnt>Z</fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gesundheit</trd>
  <trd lng="en">health</trd>
  <trdgrp lng="fr">
    <trd>sant&eacute;</trd>
  </trdgrp>
  <trd lng="he">&bet;&resh;&yod;&alef;&vav;&tav;</trd>
  <trd lng="hu">eg&eacute;szs&eacute;g</trd>
  <trd lng="nl">gezondheid</trd>
  <trd lng="ru">&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>
  <trd lng="vo">saun</trd>
</drv>

<drv mrk="san.0o.MOS">
  <kap>Monda Organiza&jcirc;o pri <tld lit="S"/>o</kap>
  <mlg>MOS</mlg>
  <uzo tip="fak">MED</uzo>
  <fnt><bib>ManPol</bib></fnt>
  <snc>
    <dif>
      Speciala <ref tip="super" cel="organi2.0ajxo">organiza&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj Nacioj</ref>
      por kontra&ubreve;batali <ref cel="epidem.0o">epidemio</ref>jn kaj por
      plibonigi higienon kaj <tld/>on:
      <ekz>
        Monda Organiza&jcirc;o pri <tld lit="S"/>o (MOS) en sia lastatempa
        raporto deklaris, ke la proverbo &leftquot;&ccirc;iutage unu glason da biero kaj
        la kuracistoj estos superfluaj&rightquot; havas nenian sciencan bazon.
        <fnt><aut>Peng Juan</aut>:
        <vrk><url ref="&Monato;/007379.html">Alkohola&jcirc;o &ndash; &ccirc;iam
        dan&gcirc;era</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 04/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Weltgesundsheits-Organisation</trd>,
    <trd>WGO</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>World <ind>Health</ind> Organisation</trd>,
    <trd>WHO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organizaci&oacute;n Mundial de la <ind>Salud</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation mondiale de la <ind>sant&eacute;</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trd lng="he">&alef;&resh;&gimel;&vav;&fnun; &he;&bet;&resh;&yod;&alef;&vav;&tav; &he;&ayin;&vav;&lamed;&mem;&yod;</trd>
  <trdgrp lng="hu">
    <trd>Eg&eacute;szs&eacute;g&uuml;gyi Vil&aacute;gszervezet</trd>,
    <trd>WHO</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Wereldgezondheidsorganisatie</trd>,
    <trd>WGO</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organiza&ccedil;&atilde;o Mundial de <ind>Sa&uacute;de</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_V;&c_s;&c_je;&c_m;&c_i;&c_r;&c_n;&c_a;&c_ja; &c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;
    <ind>&c_Z;&c_d;&c_r;&c_a;&c_v;&c_o;&c_o;&c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja;</ind></trd>,
    <trd>&c_V;&c_O;&c_Z;</trd>
  </trdgrp>
</drv>

<drv mrk="san.0iga">
  <kap><tld/>(ig)a</kap>
  <snc>
    <dif>
      Favora por la <tld/>o, ta&ubreve;ga por subteni la bonfarton:
      <ekz>
        <tld/>a pano<fnt>Z</fnt>;
      </ekz>
      <ekz>
        maldol&ccirc;a por la lango, sed <tld/>iga por la sango
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;oja koro estas <tld/>iga<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="higien.0a">higiena</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>sain<klr> (favorable &agrave; la sant&eacute;)</klr></trd>,
    <trd>sanitaire</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&bet;&resh;&yod;&alef;</trd>,
    <trd>&mem;&resh;&pe;&alef;</trd>
  </trdgrp>
  <trd lng="hu">eg&eacute;szs&eacute;ges<klr tip="ind"> (pl. &eacute;letm&oacute;d)</klr></trd>
  <trdgrp lng="nl">
    <trd>gezond</trd>,
    <trd>heilzaam</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j; &c_d;&c_l;&c_ja; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="san.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sukcese kuraci, meti iun en sanan staton:
      <ekz>Li <tld/>igas la korprematojn kaj banda&gcirc;as iliajn vundojn
      <fnt><bib>MT</bib> <lok>&Psa; 147:3</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gu&eacute;rir</trd>,
    <trd>r&eacute;tablir<klr> (gu&eacute;rir)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="san.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Malbonfarta:
      <ekz>
        cerbomal<tld/>a;
      </ekz>
      <ekz>
        mal<tld/>o de gor&gcirc;o;
      </ekz>
      <ekz>
        mal<tld/>eca paleco <klr>(f.)</klr>;
      </ekz>
      <ekz>
        tiu okazo mal<tld/>igis nian financan staton;
      </ekz>
      <ekz>
        de rigardo tro alta mal<tld/>i&gcirc;as okulo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ungesund</trd>
  <trdgrp lng="fr">
    <trd>malade<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&het;&vav;&lamed;&he;</trd>
  <trd lng="hu">beteg</trd>
  <trdgrp lng="nl">
    <trd>ziek</trd>,
    <trd>ongezond</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;</trd>
</drv>

<drv mrk="san.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io mal<tld/>a; malbonfarto:
      <ekz>Li pardonas &ccirc;iujn viajn pekojn, Li <tld/>igas &ccirc;iujn viajn mal<tld/>ojn
      <fnt><bib>MT</bib> <lok>&Psa; 103:3</lok></fnt>.</ekz>
      <ekz>
        &ccirc;iutage homoj kolapsas pro kormal<tld/>o
        <fnt><aut>Jomo Ipfelkofer</aut>: <vrk>Esploroj kortu&scirc;aj</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 9a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Krankheit</trd>
  <trd lng="en">desease</trd>
  <trdgrp lng="fr">
    <trd>maladie</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&het;&lamed;&he;</trd>,
    <trd>&het;&vav;&lamed;&yod;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>betegs&eacute;g</trd>,
    <trd>k&oacute;r</trd>
  </trdgrp>
  <trd lng="nl">ziekte</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
</drv>

<drv mrk="san.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eksperti korpan a&ubreve; spiritan misfunkciadon:
      <ekz>
        mal<tld/>i&gcirc;as pro kancero ankora&ubreve; triono de la viroj kaj kvarono
        de la virinoj anta&ubreve; la 75a jaro
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>Pli maljunaj, pli sa&gcirc;aj, pli
        flekseblaj</vrk>, <bib>Monato</bib>, <lok>jaro 1999a, numero 3a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>tomber <ind>malade</ind></trd>
  </trdgrp>
</drv>

<drv mrk="san.mal0ulejo">
  <kap>mal<tld/>ulejo</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Granda, ordinare publika kuracejo kun ekipa&jcirc;oj kaj oficantaj
      medicinistoj:
      <ekz>
        La konstruado de lernejoj, mal<tld/>ulejoj kaj pontoj kondukis al
        la estigo de modesta bonstato
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>Sir Edmund Hillary</vrk>,
        <bib>Monato</bib>, <lok>jaro 2000a, numero 10a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>hopital</trd>
  </trdgrp>
  <trd lng="he">&bet;&yod;&tav; &het;&vav;&lamed;&yod;&fmem;</trd>
  <trd lng="hu">k&oacute;rh&aacute;z</trd>
  <trd lng="nl">ziekenhuis</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="san.re0igi">
  <kap>re<tld/>igi</kap>
  <snc>
    <dif>
      Redoni la <tld/>on al iu, sukcese kuraci:
      <ekz>
        la Eternulo banda&gcirc;os la difektojn de sia popolo kaj re<tld/>igos
        &gcirc;iajn vundojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>re<tld/>i&gcirc;i</ind> <trd lng="ru">&c_v;&c_y;&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_je;&c_t;&c_mol;</trd>
        el (a&ubreve; post) rabio;
      </ekz>
      <ekz>
        nere<tld/>igebla vundo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gu&eacute;rir<klr> (tr.)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&resh;&pe;&alef;</ind></trd>
  <trd lng="hu">meggy&oacute;gy&iacute;t</trd>
  <trdgrp lng="nl">
    <trd>genezen</trd>,
    <trd>herstellen</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_l;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="san.re0igilo">
  <kap><tld/>ilo<fnt>B</fnt>, re<tld/>igilo<fnt>Z</fnt></kap>
  <snc>
    <ref tip="dif" cel="kurac.0ilo">kuracilo</ref>
  </snc>
  <trd lng="de">Medizin</trd>
  <trdgrp lng="fr">
    <trd>rem&egrave;de</trd>
  </trdgrp>
  <trd lng="he">&tav;&resh;&vav;&pe;&he;</trd>
  <trd lng="hu">gy&oacute;gy&aacute;szati eszk&ouml;z</trd>
  <trdgrp lng="nl">
    <trd>geneesmiddel</trd>,
    <trd>medicijn</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="san.flavmal0o">
  <kap>flavmal<tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="ikter.0o">iktero</ref>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_u;&c_h;&c_a;</trd>
  <trd lng="de">Gelbsucht</trd>
  <trd lng="en">jaundice</trd>
  <trdgrp lng="fr">
    <trd>ict&egrave;re</trd>,
    <trd>jaunisse</trd>
  </trdgrp>
  <trd lng="he">&tsadi;&he;&bet;&tav;</trd>
  <trd lng="hu">s&aacute;rgas&aacute;g</trd>
  <trd lng="nl">geelzucht</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_h;&c_a;</trd>
  <trd lng="sv">gulsot</trd>
</drv>
</art>
