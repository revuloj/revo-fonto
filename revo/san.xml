</kap>
<drv mrk="san.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="san.0a.bonfarta">
    <dif>
      Havanta korpon, organaron, menson, kiuj bone kaj normale
      funkcias:
      <ekz>
        Se mi estus <tld/>a, mi estus feliĉa
        <fnt><bib>F</bib>, <lok>§ 20</lok></fnt>;
      </ekz>
      <ekz>
        <ind>en korpo <tld/>a la menso <tld/>a</ind>;
        <trd lng="fr">un esprit sain dans un corps sain</trd>
        <trd lng="hu">ép testben ép lélek</trd>
        <trd lng="ru">в здоровом теле здоровый дух</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto <tld/>e eltenis ĉiujn ventojn kaj
        puŝojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio malpermesas al la lingvo libere kaj <tld/>e
        disvolviĝi<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="san.0a.saniga">
    <ref tip="dif" cel="favor.san0a.MED"><tld/>favora</ref>
    <ekz>
      <tld/>a pano<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="be">здаровы</trd>
  <trd lng="bg">здрав</trd>
  <trd lng="cs">zdravý</trd>
  <trd lng="de">gesund</trd>
  <trd lng="en">healthy</trd>
  <trdgrp lng="fr">
    <trd>sain</trd>,
    <trd>en bonne <ind>santé</ind></trd>
  </trdgrp>
  <trd lng="he">בריא</trd>
  <trd lng="hu">egészséges</trd>
  <trd lng="it">sano</trd>
  <trd lng="nl">gezond</trd>
  <trdgrp lng="pt">
    <trd>são</trd>
  </trdgrp>
  <trd lng="ru">здоровый</trd>
  <trd lng="vo">saunik</trd>
  <trd lng="tr">sağlıklı</trd>
</drv>

<drv mrk="san.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="san.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Stato de iu aŭ io <tld/>a, en <ref tip="vid"
      cel="fart.bon0o">bonfarto</ref>:
      <ekz>
        <tld/>o estas pli valora ol oro;
      </ekz>
      <ekz>
        <tld/>osubtenanta ridado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trinki pro ies <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ĝi estu por vi al <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">здароўе</trd>
  <trd lng="de">Gesundheit</trd>
  <trd lng="en">health</trd>
  <trdgrp lng="fr">
    <trd>santé</trd>
  </trdgrp>
  <trd lng="he">בריאות</trd>
  <trd lng="hu">egészség</trd>
  <trd lng="it">salute</trd>
  <trd lng="nl">gezondheid</trd>
  <trd lng="ru">здоровье</trd>
  <trd lng="vo">saun</trd>
  <trd lng="tr">sağlık</trd>
</drv>

<drv mrk="san.0o.MOS">
  <kap>Monda Organizaĵo pri <tld lit="S"/>o</kap>
  <mlg>MOS</mlg>
  <fnt><bib>ManPol</bib></fnt>
  <snc mrk="san.0o.MOS.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Speciala <ref tip="super"
      cel="organi2.0ajxo">organizaĵo</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unuiĝintaj
      Nacioj</ref>
      por kontraŭbatali <ref cel="epidem.0o">epidemiojn</ref> kaj
      por
      plibonigi higienon kaj <tld/>on:
      <ekz>
        Monda Organizaĵo pri <tld lit="S"/>o (MOS) en sia
        lastatempa
        raporto deklaris, ke la proverbo „ĉiutage unu
        glason da biero kaj
        la kuracistoj estos superfluaj“ havas nenian sciencan
        bazon
        <fnt><aut>Peng Juan</aut>,
        <vrk><url ref="&Monato;/007379.html">Alkoholaĵo –
        ĉiam
        danĝera</url></vrk>,
        <bib>Monato</bib>,
        <lok>numero 04/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>Сусьветная
    арганізацыя аховы
    здароўя</trd>,
    <trd>САЗ</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Weltgesundheits-Organisation</trd>,
    <trd>WGO</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>World <ind>Health</ind> Organisation</trd>,
    <trd>WHO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organización Mundial de la <ind>Salud</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation mondiale de la <ind>santé</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trd lng="he">ארגון
  הבריאות העולמי</trd>
  <trdgrp lng="hu">
    <trd>Egészségügyi Világszervezet</trd>,
    <trd>WHO</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>Organizzazione Mondiale della <ind>Sanità</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Wereldgezondheidsorganisatie</trd>,
    <trd>WGO</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organização Mundial de <ind>Saúde</ind></trd>,
    <trd>OMS</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>Всемирная
    Организация
    <ind>Здравоохранения</ind></trd>,
    <trd>ВОЗ</trd>
  </trdgrp>
  <trd lng="tr">DSÖ<klr>(Dünya Sağlık Örgütü)</klr></trd>
</drv>

<drv mrk="san.0iga">
  <kap><ofc>*</ofc><tld/>iga</kap>
  <snc mrk="san.0iga.MED">
    <dif>
      Favora por la <tld/>o, taŭga por subteni la bonfarton:
      <ekz>
        maldolĉa por la lango, sed <tld/>iga por la sango
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ĝoja koro estas <tld/>iga
        <fnt><bib>MT</bib>, <lok>&Sen; 17:22</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="higien.0a">higiena</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>аздараўленчы</trd>,
    <trd>гаючы</trd>,
    <trd>карысны для
    здароўя</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sain<klr> (favorable à la santé)</klr></trd>,
    <trd>sanitaire</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>בריא</trd>,
    <trd>מרפא</trd>
  </trdgrp>
  <trd lng="hu">egészséges<klr tip="ind"> (pl.
  életmód)</klr></trd>
  <trdgrp lng="it">
    <trd>salutare</trd>,
    <trd>salubre</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gezond</trd>,
    <trd>heilzaam</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>оздоровительный</trd>,
    <trd>целебный</trd>,
    <trd>полезный для
    здоровья</trd>
    <trd lng="tr">sağlığa yararlı</trd>
  </trdgrp>
</drv>

