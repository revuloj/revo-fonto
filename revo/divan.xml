<drv mrk="divan.0o">
<kap><tld/>o</kap>
<snc num="1">
<dif>
Turka sofo; benko sen brakoj nek apogilo, garnita per kusenoj. 
</dif>
<refgrp tip="vid">
<ref cel="sof.0o">sofo</ref>,
<ref cel="kanap.0o">kanapo</ref>.
</refgrp>
  <trdgrp lng="de">
     <trd>Diwan</trd>,
     <trd>Ruhebett</trd>,
     <trd>niedriges <ind>Liegesofa</ind></trd>
  </trdgrp>
  <trd lng="es">div&aacute;n</trd>
  <trd lng="fr">divan<klr tip="ind"> (si&egrave;ge)</klr></trd>
  <trdgrp lng="hu">
     <trd>d&iacute;v&aacute;ny</trd>,
     <trd>kerevet</trd>,
     <trd>pamlag</trd>
  </trdgrp>
  <trd lng="pt">div&#227;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>,
    <trd>&c_o;&c_t;&c_t;&c_o;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</snc>
<snc num="2">
<uzo tip="fak">HIS</uzo>
<dif>
Turka &scirc;tata konsilantaro.  
</dif>
  <trd lng="de">Diwan</trd>
<trd lng="fr">divan<klr tip="ind"> (salle du Conseil turc)</klr></trd>
<trd lng="hu">d&iacute;v&aacute;n</trd>
  <trd lng="pt">div&#227;</trd>
<trd lng="ru">&c_d;&c_i;&c_v;&c_a;&c_n;</trd>
</snc>
<trd lng="cs">pohovka</trd>
</drv>
