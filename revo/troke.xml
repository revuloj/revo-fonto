<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: troke.xml,v 1.23 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>troke</rad>/o <fnt><bib>PV</bib></fnt>,
  <var>
    <kap>tro&hcirc;e/o <fnt><bib>BoER</bib></fnt></kap>
  </var>
</kap>

<drv mrk="troke.0o">
  <kap><tld/>o, <var>
      <kap>tro&hcirc;eo</kap>
    </var>
  </kap>
  <uzo tip="fak">POE</uzo>
  <snc mrk="troke.0o.verspiedo">
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du silaboj, el kiuj la unua estas longa a&ubreve; akcenta, kaj la
      dua mallonga a&ubreve; senakcenta; simbole /&#8208;&ypsilon;/:
      <ekz>
        la vorto <ctl>maro</ctl> formas <tld/>on;
      </ekz><ekz>
        s-ro <nom>Brendon Clark</nom> <klr>[&#8230;]</klr> rekomendis la tro&hcirc;eon,
        kaj d-ro <nom>Cecil Bean</nom> <klr>[&#8230;]</klr> la amfibra&hcirc;on, kiel
        la plej <ctl>naturan</ctl> kaj sekve plej vaste uzindan
        metrikon en Esperanto
        <fnt>
          <aut>William Auld</aut>:
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr24/ritmou.html">Unu ritmo
          en &#xAB;La infana raso&#xBB;</url></vrk>.
          <lok>La nica literatura revuo, 2:4</lok>
        </fnt>;
      </ekz><ekz>
        senprobleme la poeto saltas de okcidentaj tro&hcirc;eoj al
        japane hajkecaj versoj
        <fnt>
          <aut>Marc van Oostendorp, tr. Istv&aacute;n Ertl</aut>:
          <vrk>Nia trezoro: Mao Zifu</vrk>. <bib>LOdE</bib>,
          <lok><url ref="&LOdE;Ondo/218-lode.htm">n-ro&nbsp;218 (2012:12)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_h;&c_je;&c_j;</trd>
  <trdgrp lng="ca">
    <trd>troqueu</trd>,
    <trd>coreu</trd>
  </trdgrp>
  <trd lng="de">Troch&auml;us</trd>
  <trd lng="en">trochee</trd>
  <trd lng="fr">troch&eacute;e</trd>
  <trd lng="hu">trocheus</trd>
  <trdgrp lng="ja">
    <trd>&#38263;&#x77ED;&#x683C;  <pr>&#12385;&#12423;&#12358;&#x305F;&#12435;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x5F37;&#x5F31;&#x683C;  <pr>&#x304D;&#12423;&#12358;&#12376;&#12419;&#x304F;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x63DA;&#25233;&#x683C; <pr>&#12424;&#12358;&#12424;&#12367;&#12363;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">trochee</trd>
  <trd lng="pt">troqueu</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_h;&c_je;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&#x43E;&c_h;&c_je;&c_j;</trd>,
    <trd>&c_h;&#x43E;&c_r;&c_je;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="troke.n0o">
  <kap>n-<tld/>o, <var>
      <kap>n-tro&hcirc;eo</kap>
    </var>
  </kap>
  <uzo tip="fak">POE</uzo>
  <snc mrk="troke.n0o.metro">
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe
      konsistanta
      el n <tld/>oj; simbole <frm>/&#8208;&ypsilon;/<sup>n</sup></frm> a&ubreve;
      <frm>/&#8208;&ypsilon;/<sup>n-1</sup>/&#8208;(&ypsilon;)/</frm>:
      <ekz>
        <ind>kvin<tld/>o</ind>.
        <trd lng="ca">pent&agrave;metre trocaic</trd>
        <trd lng="fr">pentam&egrave;tre trocha&iuml;que</trd>
        <trd lng="de">vierhebiger Troch&auml;us</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">n-metre trocaic</trd>
  <trd lng="de">n-hebiger Troch&auml;us</trd>
  <trd lng="ru">n-&c_s;&c_t;&c_o;&c_p;&c_n;&c_y;&c_j; &c_h;&c_o;&c_r;&c_je;&c_j;</trd>
</drv>
</art>
