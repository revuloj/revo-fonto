<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>troke</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="troke.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du silaboj, el kiuj la unua estas longa a&ubreve; akcenta, kaj la
      dua mallonga a&ubreve; senakcenta; simbolo /&#8208;&ypsilon;/:
      <ekz>
        &leftquot;maro&rightquot; konsistigas <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_h;&c_je;&c_j;</trd>
  <trdgrp lng="ca">
    <trd>troqueu</trd>,
    <trd>coreu</trd>
  </trdgrp>
  <trd lng="de">Troch&auml;us</trd>
  <trd lng="en">trochee</trd>
  <trd lng="fr">troch&eacute;e</trd>
  <trd lng="hu">trocheus</trd>
  <trd lng="nl">trochee</trd>
  <trd lng="pt">troqueu</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_h;&c_je;&c_j;</trd>
</drv>

<drv mrk="troke.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe konsistanta
      el
      n <tld/>oj; simbolo /&#8208;&ypsilon;/<sup>n</sup> a&ubreve;
      /&#8208;&ypsilon;/<sup>n-1</sup>/&#8208;(&ypsilon;)/:
      <ekz>
        <ind>kvin<tld/>o</ind>.
        <trd lng="ca">pent&agrave;metre trocaic</trd>
        <trd lng="fr">pentam&egrave;tre trocha&iuml;que</trd>
        <trd lng="de">vierhebiger Troch&auml;us</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">n-metre trocaic</trd>
  <trd lng="de">n-hebiger Troch&auml;us</trd>
</drv>
<adm>
  Sxajnas al mi, ke la formo "trohxeo" devus
  ekzisti. <aut>MB</aut>
</adm>
</art>
