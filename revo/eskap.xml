</kap>
<drv mrk="eskap.0i">
<kap><tld/>i</kap>
<gra><vspec>x</vspec></gra>
<snc>
<dif>
Sukcese eviti per forkuro: <ekz><tld/>i ion, de io, el io.</ekz>
</dif>
</snc>
<trdgrp lng="de">
   <trd>entkommen</trd>,
   <trd>entrinnen</trd>,
   <trd>entwischen</trd>,
   <trd>entfliehen</trd></trdgrp>
<trdgrp lng="fr">
  <trd>&eacute;chapper (&agrave;)</trd>,
  <trd>&eacute;viter<klr tip="ind"> (&eacute;chapper &agrave;)</klr></trd></trdgrp> 
<trdgrp lng="ru">
  <trd>&c_i;&c_z;&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol;</trd>,
  <trd>&c_s;&c_p;&c_a;&c_s;&c_t;&c_i;&c_s;&c_mol;</trd>
</trdgrp>
</drv>
</art>
