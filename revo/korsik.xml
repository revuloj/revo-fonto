<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: korsik.xml,v 1.2 2024/10/08 18:35:14 afido Exp $">
    <kap>
      <ofc>10</ofc><rad>Korsik</rad>/o
    </kap>

<drv mrk="korsik.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="korsik.0o.insulo">
    <dif>
      Granda <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">insulo</ref> en
      <ref tip="malprt" cel="mediterane.M0o">Mediteraneo</ref>, &ccirc;. 200&#8239;km sude de &Gcirc;enovo:
      <ekz>
        li sin proklamigis re&gcirc;o de <tld/>o en 1736 kun la nomo <nom>Teodor</nom> la I.
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; esti ekzekutita, li estis sendita al <tld/>o
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Korsika</trd>
    <trd lng="en">Corsica</trd>
    <trd lng="fr">Corse</trd>
  </snc>
</drv>

<drv mrk="korsik.0a">
  <kap>
    <ofc>10</ofc><tld lit="k"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o, la <tld lit="k"/>anoj, ilia italolatinida lingvo:
      <ekz>
        en Francio, la bretona, <tld lit="k"/>a, kataluna, flandra kaj aliaj lingvoj
        frontas gravan minacon
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="k"/>a diablo forlasis la &scirc;ipon en la golfo &Jcirc;uano
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Chrysothemis</nom>, mem direktante kvar malgrandajn <tld lit="k"/>ajn &ccirc;evalojn,
        disdonadis &ccirc;irka&ubreve;en ridetojn kaj facilajn batojn per ora vipo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">korsisch</trd>
</drv>

<drv mrk="korsik.0ano">
  <kap>
    <ofc>10</ofc><tld lit="k"/>ano
  </kap>
  <snc>
    <dif>
      Iu el <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Korse</trd>
</drv>

<drv mrk="korsik.Sud_0o">
  <kap>
    <ofc>10</ofc>Sud-<tld/>o
  </kap>
  <mlg>FR&#8209;2A</mlg>
  <snc>
    <dif>
      Franca <ref tip="super" cel="depart.0o.franca">departemento</ref>
      formanta la sudan parton de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Corse-du-Sud</trd>
  <trd lng="fr">Corse-du-Sud</trd>
</drv>

<drv mrk="korsik.Supra_0o">
  <kap>
    <ofc>10</ofc>Supra <tld/>o
  </kap>
  <mlg>FR&#8209;2B</mlg>
  <snc>
    <dif>
      Franca <ref tip="super" cel="depart.0o.franca">departemento</ref> formanta la nordan parton de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Haute-Corse</trd>
  <trd lng="fr">Haute-Corse</trd>
</drv>

  </art>

