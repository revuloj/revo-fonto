<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuler</rad>/o
</kap>
<drv mrk="kuler.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Unu el la man&gcirc;iloj, konsistanta
      el tenilo kaj el kava parto, ovforma a&ubreve; ronda,
      per kiu oni &ccirc;erpas flua&jcirc;ojn a&ubreve;
      duonsolidajn nutra&jcirc;ojn:
      <ekz>
        <tld/>o el ligno;
      </ekz>
      <ekz>
        kaf<tld/>o, sup<tld/>o
        <klr>(amba&ubreve; estas uzataj en receptoj kiel mezurunuoj)</klr>;
      </ekz>
      <ekz>pli allogas <tld/>o da mielo ol da vinagro barelo<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>el malplena telero vane &ccirc;erpas <tld/>o
        <klr>(kontra&ubreve; neesto ne helpas protesto)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="en">spoon</trd>
  <trdgrp lng="be">
    <trd>&c_l;&c_y;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_l;&c_o;&c_n;&c_ib;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_s;&c_u;&c_p;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ca">cullera</trd>
  <trd lng="cs">l&zcaron;&iacute;ce</trd>
  <trd lng="de">L&ouml;ffel</trd>
  <trd lng="es">cuchara</trd>
  <trd lng="fr">cuill&egrave;re</trd>
  <trdgrp lng="he">
    <trd>&kaf;&fpe;</trd>,
    <trd>&kaf;&pe;&yod;&tav;</trd>
  </trdgrp>
  <trd lng="hu">kan&aacute;l</trd>
  <trd lng="nl">lepel</trd>
  <trd lng="pl">&lstroke;y&zdot;ka</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_zh;&c_k;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_je;&c_d;&c_y;)</klr></trd>,
    <trd>&c_ch;&c_je;&c_r;&c_p;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_r;&c_a;&c_z;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_ja; &c_s;&c_u;&c_p;&c_a;)</klr></trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">sked</trd>
  <trd lng="tr">ka&scedil;&inodot;k</trd>
</drv>
</art>
