  <trd lng="tr">bo&scedil;almak</trd>
</drv>

<drv mrk="ejakul.0o">
  <kap><tld/>o</kap>
  <snc mrk="ejakul.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        la unua fojo, ke ina <tld/>o estis menciita en scienca literaturo, estis en la
        16-a jarcento, de la nederlanda medicinisto <nom>Laevinius Lemnius</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=596179">Ina ejakulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_je;&c_ja;&c_k;&c_u;&c_l;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">ejaculaci&oacute;</trd>
