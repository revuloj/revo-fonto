<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ejakul.xml,v 1.14 2023/12/12 08:31:52 revo Exp $">
<kap>
  <rad>ejakul</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ejakul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="ejakul.0i.vira">
    <dif>
      El&jcirc;eti <ref tip="vid" cel="sperm.0o.FIZL">spermon</ref> tra la
      <ref tip="vid" cel="uretr.0o">uretro</ref> reage al <ref tip="vid"
      cel="seks.0a.FIZL">seksa<sncref/></ref> stimulo; <ref tip="sin" cel="cxur.0i.FIZL">&ccirc;uri</ref>:
      <ekz>
        la knaboj ofte tro rapide <tld/>as, la knabinoj pro
        timoj tute ne pretas akcepti la penison
        <fnt>
          <aut>Zlatko Ti&scirc;ljar</aut>,
          <vrk><url ref="http://miresperanto.com//biblioteko/duobla_spiralo/21.htm">
          Duobla Spiralo</url></vrk>,
          <lok>1991</lok>
        </fnt>;
      </ekz>
      <ekz>
        la masklo <klr>[&#8230;]</klr> <tld/>as ejakula&jcirc;on, kiu enhavas spermon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3713">Reprodukta sistemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&a_q;&a_dh;&a_f; &a_A;&a_l;&a_m;&a_n;&a_y;</trd>
    <trd lng="tok">pana e telo mije</trd>
    <trd lng="vi">xu&#x1EA5;t tinh</trd>
  </snc>
  <snc mrk="ejakul.0i.ina">
    <dif>
      El&jcirc;eti <ref tip="vid" cel="likv.0o">likvon</ref> de la <ref tip="vid"
      cel="vulv.0o">vulvo</ref> reage al <ref tip="vid" cel="seks.0a.FIZL">seksa<sncref/></ref>
      stimulo.
    </dif>
    <trdgrp lng="tok">
      <trd>pana e telo meli</trd>,
      <trd>pana e telo tan lupa meli</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="orgasm.0o">orgasmo</ref>
  </refgrp>
  <trd lng="be">&c_e;&c_ja;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_je;&c_ja;&c_k;&c_u;&c_l;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">ejacular</trd>
  <trd lng="cs">ejakulovat</trd>
  <trd lng="de">ejakulieren</trd>
  <trd lng="en">ejaculate</trd>
  <trd lng="es">eyacular</trd>
  <trd lng="fi">ejakuloida</trd>
  <trd lng="fr">&eacute;jaculer</trd>
  <trd lng="fy">ejakulaasje</trd>
  <trd lng="it">eiaculare</trd>
  <trd lng="ja">&#x5C04;&#x7CBE;&#12377;&#x308B;  <pr>&#12375;&#12419;&#x305B;&#12356;&#12377;&#x308B;</pr></trd>
  <trd lng="lv">ejakul&emacron;t</trd>
  <trd lng="mk">&c_je;&#1112;&c_a;&c_k;&c_u;&c_l;&c_i;&c_r;&c_a;</trd>
  <trd lng="nl">ejaculeren</trd>
  <trd lng="pt">ejacular</trd>
  <trd lng="ro">ejacula</trd>
  <trd lng="ru">&c_e;&c_ja;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ejakulova&tcaron;</trd>
  <trd lng="sv">ejakulera</trd>
  <trd lng="tr">bo&scedil;almak</trd>
  <trdgrp lng="zh">
    <trd>&#22823;&#x58F0;&#x53EB;&#x558A; <pr>d&#xE0;sh&emacron;ngji&#xE0;oh&#x1CE;n</pr></trd>,
    <trd>&#x5C04;&#x7CBE; <pr>sh&#xE8;j&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ejakul.0o">
  <kap><tld/>o</kap>
  <snc mrk="ejakul.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        la unua fojo, ke ina <tld/>o estis menciita en scienca literaturo, estis en la
        16-a jarcento, de la nederlanda medicinisto <nom>Laevinius Lemnius</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=596179">Ina ejakulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_ja;&c_k;&c_u;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_je;&c_m;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_c;&c_mol;&c_c;&c_jo;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_ja;&c_k;&c_u;&c_l;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">ejaculaci&oacute;</trd>
  <trd lng="cs">ejakulace</trd>
  <trd lng="de">Ejakulation</trd>
  <trd lng="en">ejaculation</trd>
  <trd lng="es">eyaculaci&oacute;n</trd>
  <trd lng="fi">ejakulaatio</trd>
  <trd lng="fr">&eacute;jaculation</trd>
  <trd lng="he">&#x5E4;&#x5DC;&#x5D9;&#x5D8;&#x5D4;</trd>
  <trd lng="it">eiaculazione</trd>
  <trd lng="lv">ejakul&amacron;cija</trd>
  <trd lng="mk">&c_je;&#1112;&c_a;&c_k;&c_u;&c_l;&c_i;&c_r;&c_a;</trd>
  <trd lng="nl">ejaculatie</trd>
  <trd lng="pt">ejacula&ccedil;&atilde;o</trd>
  <trd lng="ro">ejaculare</trd>
  <trd lng="ru">&c_e;&c_ja;&c_k;&c_u;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">ejakul&aacute;cia</trd>
  <trd lng="sv">ejakulation</trd>
  <trd lng="tr">bo&scedil;alma</trd>  <trdgrp lng="uk">
    <trd>&c_je;&#x44F;&#x43A;&c_u;&#x43B;&#x44F;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_s;&c_ib;&#x43C;&#8217;&#x44F;&c_v;&c_i;&#x43F;&#x43E;&c_r;&c_s;&#x43A;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_s;&c_ib;&#x43C;&#8217;&#x44F;&c_v;&c_i;&c_v;&c_je;&c_r;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x5C04;&#x7CBE; <pr>sh&#xE8;j&#x12B;ng</pr></trd>
</drv>
