<drv mrk="supoz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="supoz.0i.KOMUNE">
    <dif>
      Provizore akcepti ion kiel veran a&ubreve; eblan, ne posedante pruvojn
      tute certajn:
      <ekz>
        neniam mi <tld/>is, ke mi ree vidos vin vivanta
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i multe laboris, pli multe ol oni povus <tld/>i la&ubreve; &scirc;ia maljuna a&gcirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la interkomuniki&gcirc;o de la popoloj per la telegrafoj, vapor&scirc;ipoj kaj fervojoj
        grandi&gcirc;is en maniero neniam <tld/>ita
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili, vidante lin iranta sur la maro, <tld/>is, ke &gcirc;i estas fantomo
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:49</lok>
        </fnt>;
      </ekz>
      <ekz>
        li amis siajn paro&hcirc;anojn, kaj li anka&ubreve; <tld/>is, ke ili
        estimas lin
        <fnt><bib>Kiso</bib>, <lok>la bovinoj de la
        paro&hcirc;estro</lok></fnt>.
      </ekz>
      <ekz>
        ili estas pli simpatiaj, ol li anta&ubreve;e estus <tld/>inta
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia viza&gcirc;o restis &ccirc;e tio &ccirc;i tiel senmova, ke
        <klr>[&#8230;]</klr> neniu <tld/>is en li la farinton
        <fnt>
          <aut>C. Dickens, trad. L. L. Zamenhof</aut>,
          <vrk>La batalo de l&#8217; vivo</vrk>,
          <lok>1891</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>us min impertinenta mensogulo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diven.0i">diveni</ref>,
      <ref cel="hipote2.0i">hipotezi</ref>,
      <ref cel="konjek.0i">konjekti</ref>,
      <ref cel="kred.0i">kredi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ib;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>domn&iacute;vat se</trd>,
    <trd>p&rcaron;edpokl&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>annehmen</trd>,
    <trd>vermuten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>suppose</trd>,
    <trd>assume</trd>,
    <trd>presume</trd>,
    <trd>surmise</trd>
  </trdgrp>
  <trd lng="es">suponer</trd>
  <trd lng="fr">supposer</trd>
  <trdgrp lng="hu">
    <trd>felt&eacute;telez</trd>,
    <trd>feltesz <klr tip="ind">(feltesz)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20206;&#23450;&#12377;&#12427;  <pr>&#12363;&#12390;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#24819;&#23450;&#12377;&#12427;  <pr>&#12381;&#12358;&#12390;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#24819;&#20687;&#12377;&#12427;  <pr>&#12381;&#12358;&#12382;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#20104;&#24819;&#12377;&#12427;  <pr>&#12424;&#12381;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">veronderstellen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol;<klr tip="ind">
(&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_d;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="ind">
(&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="sk">predpoklada&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20551;&#35373; <pr>ji&#462;sh&egrave;</pr></trd>,
    <trd>&#20551;&#35774; <pr>ji&#462;sh&egrave;</pr></trd>,
    <trd>&#26009;&#24819; <pr>li&agrave;oxi&#462;ng</pr></trd>,
    <trd>&#20551;&#20351; <pr>ji&#462;sh&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="supoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="supoz.0o.KOMUNE">
    <dif>
      Penso a&ubreve; eldiro, ke io povas esti vera, ankora&ubreve; ne
      havante pruvon:
      <ekz>
        tio konfirmas miajn <tld/>ojn;
      </ekz>
      <ekz>
        ili ne havis e&ccirc; la plej malgrandan <tld/>on, ke la kantado povas koncerni ilin
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi proponas <klr>[&#8230;]</klr> nur <tld/>ojn kaj teoriajn opiniojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi malpravas fari tian <tld/>on koncerne min
        <fnt><bib>Lanti</bib>, <lok>tri semajnoj en Rusio, &ccirc;e la
        sidejo de Komintern</lok></fnt>;
      </ekz>
      <ekz>
        mi havas pri tio nek scion nek <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_u;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_ib;&c_p;&c_o;&c_t;&c_e;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>domn&ecaron;nka</trd>,
    <trd>hypot&eacute;za</trd>,
    <trd>p&rcaron;edpoklad</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vermutung</trd>,
    <trd>Annahme</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>assumption</trd>,
    <trd>presumption</trd>,
    <trd>supposition</trd>
  </trdgrp>
  <trd lng="es">suposici&oacute;n</trd>
  <trd lng="fr">supposition</trd>
  <trdgrp lng="hu">
    <trd>felt&eacute;telez&eacute;s</trd>,
    <trd>feltev&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20206;&#23450;  <pr>&#12363;&#12390;&#12356;</pr></trd>,
    <trd>&#24819;&#23450;  <pr>&#12381;&#12358;&#12390;&#12356;</pr></trd>,
    <trd>&#24819;&#20687;  <pr>&#12381;&#12358;&#12382;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>hypot&eacute;za</trd>,
    <trd>odhad</trd>,
    <trd>predpoklad</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_i;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#20551;&#23450; <pr>ji&#462;d&igrave;ng</pr></trd>,
    <trd>&#35373;&#24819; <pr>sh&egrave;xi&#462;ng</pr></trd>,
    <trd>&#35774;&#24819; <pr>sh&egrave;xi&#462;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="supoz.0eble">
  <kap><tld/>eble</kap>
  <snc mrk="supoz.0eble.ARG">
    <dif>
      Kiel oni povas <tld/>i:
      <ekz>
        juna knabo en livreo kun metalaj butonoj, <tld/>eble &gcirc;ardenisto a&ubreve; lakeeto
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eble inter ili ne estis e&ccirc; unu, kiu kredus la vortojn de
        sinjorino <nom>Szwejc</nom>
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
      <ekz>
        tial ke <esc>kvardekok</esc> horoj jam fluis depost la katastrofo, estas <tld/>eble, ke oni
        devas forlasi &ccirc;iun esperon lin retrovi
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne estas tro ri&ccirc;aj, ili ne havas propran a&ubreve;ton <tld/>eble
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kred.0eble">kredeble</ref>,
      <ref cel="probab.0e">probable</ref>,
      <ref cel="sxajn.ver0e">ver&scirc;ajne</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_a;&c_g;&c_o;&c_d;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>doufejme</trd>,
    <trd>pravd&ecaron;podobn&ecaron;</trd>,
    <trd>snad</trd>
  </trdgrp>
  <trd lng="de">vermutlich</trd>
  <trdgrp lng="en">
    <trd>presumably</trd>,
    <trd>supposedly</trd>,
    <trd>one can assume that</trd>,
    <trd>probably</trd>
  </trdgrp>
  <trd lng="fr">vraisemblablement</trd>
  <trd lng="hu">feltehet&odblac;en</trd>
  <trdgrp lng="ja">
    <trd>&#12362;&#12381;&#12425;&#12367;&#12399;</trd>,
    <trd>&#12383;&#12406;&#12435;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>d&uacute;fajme</trd>,
    <trd>pravdepodobne</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_u;&c_s;&c_t;&c_i;&c_m;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_m;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22823;&#21322; <pr>d&agrave;b&agrave;n</pr></trd>,
    <trd>&#20551;&#23450; <pr>ji&#462;d&igrave;ng</pr></trd>,
    <trd>&#38752;&#35676; <pr>k&agrave;op&#468;</pr></trd>,
    <trd>&#38752;&#35889; <pr>k&agrave;op&#468;</pr></trd>,
    <trd>&#20381;&#31232; <pr>y&imacron;x&imacron;</pr></trd>,
    <trd>&#22823;&#32004; <pr>d&agrave;yu&emacron;</pr></trd>,
    <trd>&#22823;&#32422; <pr>d&agrave;yu&emacron;</pr></trd>,
    <trd>&#26377;&#21487;&#33021; <pr>y&#466;uk&ecaron;n&eacute;ng</pr></trd>,
    <trd>&#27178;&#26159; <pr>h&eacute;ngshi</pr></trd>,
    <trd>&#27243;&#26159; <pr>h&eacute;ngshi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="supoz.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="supoz.0igi.indiki">
    <dif>
      Agi a&ubreve; aspekti tiel, ke oni emas <tld/>i:
      <ekz>
        tio <tld/>igas, ke la sukceso estos granda;
      </ekz>
      <ekz>
        la aspekto de lia viza&gcirc;o <tld/>igis, ke li ne razis sin
        almena&ubreve; tri semajnojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 15a</lok></fnt>;
      </ekz>
      <ekz>
        la garna&jcirc;oj de la kameloj <tld/>igas, ke ili venas el
        la lando Egipta
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_n;&c_a; <ind>&c_d;&c_u;&c_m;&c_k;&c_u;</ind></trd>
    <trdgrp lng="de">
      <trd><ind>vermuten</ind> lassen</trd>,
      <trd>die Annahme <ind>st&uuml;tzen</ind></trd>
    </trdgrp>
    <trd lng="hu">arra enged <ind>k&ouml;vetkeztetni</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_t;&c_mol; &c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;
       <ind>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</ind></trd>,
      <trd>&c_u;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="supoz.0igi.neprigi">
    <dif>
      Logike <ref tip="super" cel="nepr.0igi">neprigi</ref>:
      <ekz>
        la juro diferencas de la filozofiaj konceptoj precipe pro
        tio, ke &gcirc;i <tld/>igas realan sankcion
        <fnt>
        <aut>J. Thierry</aut>,
        <vrk>Unu internacia problemo: la mortigisto</vrk>,
        <lok>Nica Literatura Revuo, 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        sed se, anstata&ubreve; <tld/>igi la mankantan ligantan vorton
        <ctl>ke</ctl>, la frazo havas ligantan vorton alian
        <klr>[&#8230;]</klr>,
        tiam devas esti uzata tiu kazo, kiun postulas la
        senco de la ekzistanta liganta vorto
        <fnt><bib>LR</bib>,
        <lok>respondo 7a, La Revuo, 1907, februaro</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_u; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_p;&c_je;&c_r;&c_a;&c_d;&c_u;&c_m;&c_o;&c_v;&c_y;</ind></trd>,
      <trd>&c_p;&c_a;&c_s;&c_t;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">nahelegen</trd>
    <trdgrp lng="hu">
      <trd>posztul&aacute;l</trd>,
      <trd>el&odblac;felt&eacute;telk&eacute;nt megk&iacute;v&aacute;n</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_t;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;<klr tip="amb">
      (&c_k;&c_a;&c_k;
&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_s;&c_y;&c_l;&c_k;&c_u;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">vyvolat dojem</trd>
  <trdgrp lng="en">
    <trd>suppose</trd>,
    <trd>assume</trd>,
    <trd>presume</trd>,
    <trd>surmise</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>laisser supposer</trd>,
    <trd>supposer<klr tip="amb"> (exiger)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24819;&#36215;&#12373;&#12379;&#12427;  <pr>&#12381;&#12358;&#12365;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#24819;&#20687;&#12373;&#12379;&#12427;  <pr>&#12381;&#12358;&#12382;&#12358;&#12373;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="sk">vyvola&tcaron; dojem</trd>
  <trd lng="uk">&c_n;&c_a;&c_sh;&c_t;&c_o;&c_v;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i; &c_n;&c_a; &c_d;&c_u;&c_m;&c_k;&c_u;</trd>
</drv>

<drv mrk="supoz.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="supoz.antaux0o.LOG">
    <dif>
      <tld lit="S"/>o, kiu estas konscia a&ubreve; nekonscia bazo de rezono:
      <ekz>
        tiuj studoj uzas siajn anta&ubreve;<tld/>ojn kiel bazon
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Ses</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni neniam konscias pri &ccirc;iuj niaj anta&ubreve;<tld/>oj
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.airbits.com/~miner/TAGLIBRO_package/Marto_2007.html">
          taglibro</url></vrk>,
          <lok>2007-03-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aksiom.0o.FIL">aksiomo<sncref/></ref>,
      <ref cel="jugx.antaux0o">anta&ubreve;ju&gcirc;o</ref>,
      <ref cel="punkt.elir0o">elirpunkto</ref>,
      <ref cel="hipote2.0o">hipotezo</ref>,
      <ref cel="postul1.0o.FIL">postulato<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_p;&c_u;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_z;&c_y;&c_h;&c_o;&c_d;&c_n;&c_a;&c_je; &c_w; &c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_h;)</klr></trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_u;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_u;&c_m;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
