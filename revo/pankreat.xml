<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pankreat.xml,v 1.8 2019/06/30 18:21:48 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>pankreat</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap>pankreas/o <fnt><bib>PV</bib></fnt></kap></var> 
</kap>
<drv mrk="pankreat.0o">
  <kap><tld/>o,
    <var><kap>pankreaso</kap></var>
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Granda ventra <ref tip="super" cel="gland.0o">glando</ref>
      (malanta&ubreve; la stomako),
      kiu sekrecias senkoloran sukon helpantan al digesto,
      kaj kiu anka&ubreve; produktas hormonojn, interalie 
      <ref tip="prt" cel="insulin.0o">insulinon</ref>:
      <ekz>
        la <tld/>o estas malgranda longa organo konektita al la duodeno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3390">Pankreato</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ivan Pavlov</nom> <klr>[&#8230;]</klr> en katedro pri fiziologio en veterinara 
        instituto <klr>[&#8230;]</klr> da&ubreve;rigis siajn esplorojn pri nerva regulado de koro kaj pankreaso 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19944">Ivan Pavlov</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la virga haringo, parenteze, havas grasecon de pli ol 16&nbsp;%, &gcirc;i dankas sian specialan guston precipe al la fi&scirc;a <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/009004.php">Jubile, jubilade, gargantuece palatkareseme</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_o;&c_z;&c_a;</trd>
  <trd lng="ca">p&agrave;ncrees</trd>
  <trdgrp lng="cs">
    <trd>pankreas</trd>,
    <trd>slinivka b&rcaron;i&scaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bauchspeicheldr&uuml;se</trd>,
    <trd>Pankreas</trd>
  </trdgrp>
  <trd lng="en">pancreas</trd>
  <trd lng="es">p&aacute;ncreas</trd>
  <trdgrp lng="fa">
    <trd>&a_l;&a_w;&a_z;&a_A;&a_l;&a_m;&a_ayn;&a_d;&a_h;</trd>,
    <trd>&f_p;&a_A;&a_n;&f_k;&a_r;&a_A;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">pancr&eacute;as</trd>
  <trd lng="hu">hasny&aacute;lmirigy</trd>
  <trd lng="it">pancreas</trd>
  <trdgrp lng="nl">
    <trd>alvleesklier</trd>,
    <trd>pancreas</trd>
  </trdgrp>
  <trd lng="pl">trzustka</trd>
  <trd lng="pt">p&acirc;ncreas</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="sk">slinivka bru&scaron;n&aacute;</trd>
</drv>
</art>

