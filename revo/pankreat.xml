<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>pankreat</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="pankreat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Granda ventra glando (malanta&ubreve; la stomako),
      kiu sekrecias senkoloran sukon helpantan al digesto,
      kaj kiu anka&ubreve; produktas hormonojn, interalie insulinon.
    </dif>
    <ref tip="vid" cel="pankre.0o">pankreaso</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Bauchspeicheldr&uuml;se</trd>,
    <trd>Pankreas</trd>
  </trdgrp>
  <trd lng="en">pancreas</trd>
  <trd lng="es">p&aacute;ncreas</trd>
  <trdgrp lng="fa">
    <trd>&a_l;&a_w;&a_z;&a_A;&a_l;&a_m;&a_ayn;&a_d;&a_h;</trd>,
    <trd>&f_p;&a_A;&a_n;&f_k;&a_r;&a_A;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">pancr&eacute;as</trd>
  <trd lng="hu">hasny&aacute;lmirigy</trd>
  <trdgrp lng="nl">
    <trd>alvleesklier</trd>,
    <trd>pancreas</trd>
  </trdgrp>
  <trd lng="pl">trzustka</trd>
  <trd lng="pt">p&acirc;ncreas</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
</drv>
