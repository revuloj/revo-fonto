<art mrk="$Id: stand.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>stand</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stand.0o">
  <kap><tld/>o</kap>
  <snc mrk="stand.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="bud.0o.ARKI">Budo</ref> a&ubreve; montrotablo 
      funkcianta kiel montrejo de iu firmao en ekspozicio:
      <ekz>
        junularaj asocioj aran&gcirc;os inform<tld/>ojn
        <fnt>
          <aut>U. Matthias</aut>,
          <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">
          Fajron sentas mi interne</url></vrk>,
          <lok>Pro Esperanto, 1990</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_e;&c_n;&c_d;</trd>, 
    <trd>&c_sh;&c_ch;&c_y;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">parada</trd>
  <trd lng="cs">stánek <klr>(obchodní| propagační)</klr></trd>
  <trd lng="de">Stand</trd>
  <trdgrp lng="en">
    <trd>stand</trd>,
    <trd>stall</trd>,
    <trd>booth</trd>
  </trdgrp>
  <trd lng="fa">&a_gh;&a_r;&a_f;&a_h;</trd>
  <trd lng="fr">stand</trd>
  <trdgrp lng="hu">
    <trd>ki&aacute;ll&iacute;t&oacute;hely</trd>,
    <trd>stand</trd>,
    <trd>pavilon</trd>
  </trdgrp>
  <trd lng="nl">stand</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>stánok <klr>(výstavný, propagačný)</klr></trd>,
    <trd>stánok <klr>(výstavný| propagačný)</klr></trd>
  </trdgrp>
</drv>
</art>
