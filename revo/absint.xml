<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>absint</rad>/o
</kap>
<drv mrk="absint.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="artemi.0o">artemizio</ref>
      (<trd lng="la">Artemisia absinthium</trd>), kun tre
      amara aromo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_n;</trd>
  <trd lng="br">huelenn-c'hwerv</trd>
  <trd lng="en">wormwood</trd>
  <trd lng="es">ajenjo</trd>
  <trd lng="de">Wermut</trd>
  <trd lng="fr">absinthe<klr tip="ind"> (plante)</klr></trd>
  <trd lng="hu">&uuml;r&ouml;m</trd>
  <trd lng="it">assenzio</trd>
  <trd lng="nl">alsem</trd>
  <trd lng="pt">absinto</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_y;&c_n;&c_mol;</trd>
  <trd lng="sk">palina prav&aacute;</trd>
  <trd lng="sv">mal&ouml;rt</trd>
</drv>

<drv mrk="absint.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Verda <ref tip="super" cel="likvor.0o">likvoro</ref> preparita el <tld/>
o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_e;&c_n;&c_t;</trd>
  <trd lng="br">absint</trd>
  <trd lng="de">Absinth</trd>
  <trd lng="en">absinthe</trd>
  <trd lng="es">absenta</trd>
  <trd lng="fr">absinthe<klr tip="ind"> (liqueur)</klr></trd>
  <trd lng="hu">abszint</trd>
  <trd lng="nl">absint</trd>
  <trd lng="pt">absinto</trd>
  <trd lng="ru">&c_a;&c_b;&c_s;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">absint</trd>
</drv>
</art>
