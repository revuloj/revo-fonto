<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: absint.xml,v 1.38 2025/03/03 08:34:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>absint</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="absint.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="absint.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="artemi.0o">artemizio</ref>
      kun tre amara aromo (<trd lng="la">Artemisia absinthium</trd>):
      <ekz>
        maldol&ccirc;a kiel <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la mielo, kiu fluas el via bu&scirc;o estas multe da <tld/>o <klr>(<ref cel="amar.0eco">amareco</ref>)</klr>
        interrompis <nom>Sara</nom>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        triono de la akvoj fari&gcirc;is <tld/>a&jcirc;o <klr>(venena)</klr>, kaj multe da homoj mortis de la akvoj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 8:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i eniris en la altan densan sekalon, en kiu abunde kreskis cejanoj kaj <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i bestetoj ja ne gustas tiel amare,
        kiel &scirc;oso de <tld/>o, &ccirc;u ne vere
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>?
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/e/e1/Illustration_Artemisia_absinthium0.jpg/466px-Illustration_Artemisia_absinthium0.jpg"
      prm="PD"
      >botanika tabulo de <tld/>o, Flora von Deutschland, &Ouml;sterreich
        und der Schweiz, Gera 1885
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute; Flora von Deutschland, &Ouml;sterreich und der Schweiz 1885, Gera, Germany, http://www.biolib.de/</aut>
          <lok><url ref="&WCW;/File:Illustration_Artemisia_absinthium0.jpg"
              >https://commons.wikimedia.org/wiki/File:Illustration_Artemisia_absinthium0.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
      <trd>&#20013;&#20122;&#33510;&#33983; <pr>zh&omacron;ngy&agrave;k&#468;h&amacron;o</pr></trd>,
      <trd>&#20013;&#20126;&#33510;&#33983; <pr>zh&omacron;ngy&agrave;k&#468;h&amacron;o</pr></trd>
    </trdgrp>
    <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_n;</trd>
    <trdgrp lng="bg">
      <trd>&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_ja;&c_t; &c_p;&c_je;&c_l;&c_i;&c_n;</trd>,
      <trd>&c_b;&c_ja;&c_l; &c_p;&c_je;&c_l;&c_i;&c_n;</trd>
    </trdgrp>
    <trd lng="br">huelenn-c'hwerv</trd>
    <trd lng="ca">donzell</trd>
    <trdgrp lng="cs">
      <trd>absint</trd>,
      <trd>pelyn&ecaron;k</trd>
    </trdgrp>
    <trd lng="de">Wermut</trd>
    <trd lng="en">wormwood</trd>
    <trd lng="es">ajenjo</trd>
    <trd lng="fa">&a_A;&a_f;&a_s;&a_n;&a_t;&f_y;&a_n;</trd>
    <trd lng="fr">absinthe<klr> (plante)</klr></trd>
    <trd lng="he">&alef;&#1463;&bet;&#1468;&#1456;&samekh;&#1460;&yod;&nun;&#1456;&tav;&#1456;</trd>
    <trd lng="hu">&uuml;r&ouml;m</trd>
    <trd lng="it">assenzio</trd>
    <trd lng="ja">&#12491;&#12460;&#12520;&#12514;&#12462;  <pr>&#12395;&#12364;&#12424;&#12418;&#12366;</pr></trd>
    <trd lng="nl">alsem</trd>
    <trd lng="pl">pio&lstroke;un</trd>
    <trd lng="pt">absinto</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_y;&c_n;&c_mol;</trd>
    <trd lng="sk">palina prav&aacute;</trd>
    <trd lng="sv">mal&ouml;rt</trd>
    <trd lng="uk">&c_p;&c_o;&c_l;&c_i;&c_n; (&c_g;&c_ib;&c_r;&c_k;&c_i;&c_j;)</trd>
  </snc>
  <snc mrk="absint.0o.likvoro">
    <dif>
      <ref tip="dif" cel="absint.0ajxo"><tld lit="A"/>a&jcirc;o</ref>:
      <ekz>
        sur sofo vid-al-vide sidis episkopo <nom>Billing</nom> sola &ccirc;e sia <tld/>o reve a&ubreve;skultante
        Largon de <nom>H&auml;ndel</nom>
        <fnt>
          <bib>Kiso</bib>
          <lok>Poetoj Kaj Homoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="absint.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Verda <ref tip="super" cel="likvor.0o">likvoro</ref> preparita el
      <tld/>o:
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_a;&c_b;&c_s;&c_je;&c_n;&c_t;</trd>
  <trd lng="br">absint</trd>
  <trd lng="ca">absenta</trd>
  <trd lng="de">Absinth</trd>
  <trd lng="en">absinthe</trd>
  <trd lng="es">absenta</trd>
  <trd lng="fr">absinthe<klr> (liqueur)</klr></trd>
  <trd lng="he">&alef;&#1463;&bet;&#1468;&#1456;&samekh;&#1460;&yod;&nun;&#1456;&tav;&#1456;</trd>
  <trd lng="hu">abszint</trd>
  <trd lng="ja">&#12491;&#12460;&#12520;&#12514;&#12462;&#25277;&#20986;&#29289;  <pr>&#12395;&#12364;&#12424;&#12418;&#12366;&#12385;&#12421;&#12358;&#12375;&#12421;&#12388;&#12406;&#12388;</pr></trd>
  <trd lng="nl">absint</trd>
  <trdgrp lng="pl">
    <trd>absynt</trd>,
    <trd>pio&lstroke;un&oacute;wka</trd>
  </trdgrp>
  <trd lng="pt">absinto</trd>
  <trd lng="ru">&c_a;&c_b;&c_s;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">absint</trd>
</drv>
</art>
