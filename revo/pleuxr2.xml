<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>ple&ubreve;ronekt</rad>/o
</kap>
<drv mrk="pleuxr2.0o">
  <kap><tld/>o</kap>
  <snc mrk="pleuxr2.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Delikatgusta marfi&scirc;o kun plate ronda korpo
      Genro el familio <ref tip="super" cel="pleuxr3.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pleuronectes</trd>):
      <ekz>
        el la Balta Maro oni &ccirc;erpis haringojn, <tld/>ojn, angilojn kaj foje iom da moruoj
        <fnt>
          <aut>Sten Johansson</aut>,
          <vrk><url ref="http://www.angelfire.com/ok/stenjo/hitu.html">Hitler, Ma&ubreve;, Strindberg kaj mi</url></vrk>,
          <lok>1996</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_a;</trd>
  <trd lng="de">Scholle</trd>
  <trdgrp lng="fr">
    <trd>carrelet</trd>,
    <trd>pleuronecte</trd>,
    <trd>plie<klr> (poisson)</klr></trd>
  </trdgrp>
  <trd lng="hu">lep&eacute;nyhal</trd>
  <trdgrp lng="nl">
    <trd>platvis</trd>,
    <trd>schol</trd>
  </trdgrp>
  <trd lng="pt">pleuronecto</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_a;</trd>
</drv>

<drv mrk="pleuxr2.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="pleuxr2.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Fi&scirc;oj kun plata korpo kaj amba&ubreve; okuloj sur la supra flanko,
      ordo el la klaso <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Pleuronectiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Plattfische</trd>
  <trd lng="fr">pleuronectiformes</trd>
  <trd lng="hu">f&eacute;lszeg&uacute;sz&oacute;-alak&uacute;ak</trd>
  <trd lng="nl">platvisachtigen</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_y;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
