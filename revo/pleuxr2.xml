<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>ple&ubreve;ronekt</rad>/o
</kap>
<drv mrk="pleuxr2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="pleuxr3.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pleuronectes</trd>), delikatgusta marfi&scirc;o kun
      plate ronda korpo.
    </dif>
  </snc>
  <trd lng="de">Scholle</trd>
<trdgrp lng="nl">
    <trd>platvis</trd>,
    <trd>schol</trd>
  </trdgrp>
</drv> 

<drv mrk="pleuxr2.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Pleuronectiformes</trd>), kun plata korpo kaj amba&ubreve;
      okuloj sur la supra flanko.
    </dif>
  </snc>
  <trd lng="de">Plattfische</trd>
  <trd lng="nl">platvisachtigen</trd>
</drv>
</art>

