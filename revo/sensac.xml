<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sensac.xml,v 1.23 2024/03/24 20:00:01 revo Exp $">
<kap>
  <rad>sensac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sensac.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sensac.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="dif" cel="sens.0ajxo.SEN">Sensa&jcirc;o</ref>:
      <ekz>
        frazoj kutime tre mallongaj, kelkfoje tute esencaj, senornamaj, 
        rekte al la celoj, alifoje priskribaj pri lokoj, mebloj sed ofte
        <tld/>oj, 
        nepalpeblaj impresoj, retropensoj
        <fnt>
          <aut>C. Minnaja</aut>
          <vrk><url ref="&Monato;/2003/007927.html">Promene tra
          la Centra Oficejo</url></vrk>
          <bib>Monato</bib>
          <lok>2003:12, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        estus interese scii, &ccirc;u aliaj homoj kiuj simile retrovis
        neglektitan tavolon
        de sia etna kulturo kaj lingvo spertas la samajn <tld/>ojn de
        rekono kaj ekscito kiel mi
        <fnt>
          <aut>G. MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008548.php">Lingva
          atavismo</url></vrk>
          <bib>Monato</bib>
          <lok>2005:03, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">senzace</trd>
  <trdgrp lng="de">
    <trd>Empfindung</trd>,
    <trd>Sinneswahrnehmung</trd>,
    <trd>Erregung</trd>
  </trdgrp>
  <trd lng="en">sensation</trd>
  <trdgrp lng="fr">
    <trd>perception</trd>,
    <trd>sensation<klr> (perception)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;rzet</trd>
  <trdgrp lng="it">
    <trd>percezione</trd>,
    <trd>sensazione<klr> (percezione)</klr></trd>
  </trdgrp>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  <trdgrp lng="pl">
    <trd>sensacja</trd>,
    <trd>wra&zdot;enie</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">senz&aacute;cia</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_ch;&c_u;&c_t;&c_t;&#x44F;</trd>,
    <trd><klr>(&c_u;&c_s;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&#x43B;&c_je;&#x43D;&c_je;)</klr> &c_s;&#x43F;&c_r;&c_i;&c_j;&#x43D;&#x44F;&c_t;&c_t;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x611F;&#x53D7; <pr>g&#x1CE;nsh&#xF2;u</pr></trd>
</drv>
</art>
