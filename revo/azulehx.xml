<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: azulehx.xml,v 1.11 2024/02/28 09:23:58 revo Exp $">
<kap>
  <rad>azule&hcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="azulehx.0o">
  <kap><tld/>o</kap>
  <snc mrk="azulehx.0o.CER">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="vid" cel="fajenc.0o">Fajenco</ref> surpentrita kaj
      kovrita de diafana glazuro, uzata sur muro a&ubreve; planko kiel kahela
      tega&jcirc;o:
      <ekz>
        li <klr>...</klr> igis instali kahelojn sur la ban&ccirc;ambran
        plankon kaj <tld/>ojn sur la muroj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        kastildevena vorto en esperanto <klr>...</klr> estas
        &leftquot;<tld/>o&rightquot;
        <fnt>
        <aut>A. Martins</aut>,
        <vrk>&Hcirc;oj denove</vrk>,
        <lok><url ref="&SCE;">soc.culture.esperanto</url>,
        1997-09-14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_f;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_h;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">rajola</trd>
  <trdgrp lng="de">
    <trd>Azulejo</trd>,
    <trd>bunt bemalte und glasifizierte Keramikfliesen</trd>
  </trdgrp>
  <trd lng="en">glazed <ind>tile</ind></trd>
  <trd lng="es">azulejo</trd>
  <trd lng="fr">azulejo</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D9;&#x5D7; &#x5D7;&#x5E8;&#x5E1;&#x5D9;&#x5E0;&#x5D4;
  &#x5DE;&#x5E7;&#x5D5;&#x5E9;&#x5D8;</trd>
  <trd lng="it">azulejo</trd>
  <trdgrp lng="nl">
    <trd>azulejo</trd>,
    <trd>siertegel</trd>
  </trdgrp>
  <trd lng="hu">azulejos</trd>
  <trd lng="uk">&#x43A;&c_a;&c_h;&c_je;&#x43B;&#x44C;</trd>
</drv>
</art>

