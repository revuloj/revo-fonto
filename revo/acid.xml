<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>acid</rad>/a
</kap>
<drv mrk="acid.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc num="1">
     <dif>
       Havanta akran guston, kiel tiu de vinagro a&ubreve; citrono:
       <ekz>
         via vino estas nur ia abomena <tld/>a&jcirc;o<fnt>Z</fnt>.
       </ekz>
     </dif>
   <trd lng="en">acidic</trd>
   <trdgrp lng="hu">
     <trd>savas</trd>,
     <trd>savany&uacute;</trd></trdgrp>  
   <trd lng="ru">&#1082;&#1080;&#1089;&#1083;&#1099;&#1081;</trd>
   <trd lng="sv">sur</trd>
   </snc>
   <snc num="2">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Malagrable pika:
       <ekz>
         li estas en <tld/>a humoro
         <fnt>
           (<aut>Zamenhof</aut>:
           <vrk>Proverbaro</vrk>)
         </fnt>;
       </ekz>
       <ekz>
         <tld/>a tono.
       </ekz>
     </dif>
     <trdgrp lng="hu">
       <trd>savany&uacute;</trd>,
       <trd>mar&oacute;</trd></trdgrp>
     <trd lng="ru">&#1077;&#1076;&#1082;&#1080;&#1081;</trd>
     <trd lng="sv">syrlig</trd>
   </snc>
   <trd lng="de">sauer</trd>
   <trd lng="en">acerbic</trd>
   <trdgrp lng="fr">
     <trd>acide</trd>,
     <trd>aigre</trd></trdgrp>
   <trd lng="nl">zuur</trd>
   <trd lng="pt">&aacute;cido</trd>
   <trd lng="pl">kwa&sacute;ny</trd>
   <trd lng="it">acido</trd>
</drv>
<drv mrk="acid.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       Kombina&jcirc;o de hidrogeno kun metalo, ru&gcirc;iganta lakmuson.
     </dif>
   </snc>
   <trd lng="en">acid</trd>
   <trd lng="de">S&auml;ure</trd>
   <trd lng="fr">acide</trd>
   <trd lng="hu">sav</trd>
   <trd lng="nl">zuur</trd>
   <trd lng="pl">kwas</trd>
   <trd lng="pt">&aacute;cido</trd>
   <trd lng="ru">&#1082;&#1080;&#1089;&#1083;&#1086;&#1090;&#1072;</trd>
   <trd lng="sv">syra</trd>
</drv>
</art>
