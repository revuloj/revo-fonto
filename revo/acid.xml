<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: acid.xml,v 1.51 2021/05/29 14:32:02 revo Exp $">
<kap>
  <ofc>*</ofc><rad>acid</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="acid.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="acid.0a.akra">
    <dif>
      Havanta akran <ref tip="lst" cel="gust.0o.eco" lst="voko:gustoj">guston</ref>, 
      kiel tiu de vinagro a&ubreve; citrono:
      <ekz>
	via vino estas nur ia abomena <ind><tld/>a&jcirc;o</ind>
	<fnt><bib>F</bib>, <lok>ekzercaro, &para; 35</lok></fnt>;
	<trd lng="fr">piquette</trd>
      </ekz>
      <ekz>
        post dol&ccirc;a vino restas <tld/>a vinagro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> eltrinkis <tld/>an bieron
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; <tld/>a&jcirc;o en la lakto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de tiu &ccirc;i vinagro estas tre malforta
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_ib;&c_s;&c_l;&c_y;</trd>
    <trdgrp lng="br">
      <trd>trenk</trd>,
      <trd>put</trd>
    </trdgrp>
    <trd lng="en">acidic</trd>
    <trd lng="es">&aacute;cido</trd>
    <trdgrp lng="hu">
      <trd>savas</trd>,
      <trd>savany&uacute;</trd>
    </trdgrp>
    <trd lng="id">asam</trd>
    <trd lng="ja">&#x9178;&#x3063;&#x3071;&#x3044;
    [<ind>&#x3059;&#x3063;&#x3071;&#x3044;</ind>]</trd>
    <trd lng="pl">kwa&sacute;ny</trd>
    <trd lng="ro">acid</trd>
    <trd lng="ru">&c_k;&c_i;&c_s;&c_l;&c_y;&c_j;</trd>
    <trd lng="sk">kysl&yacute;</trd>
    <trd lng="sv">sur</trd>
    <trd lng="ms">asid</trd>
    <trdgrp lng="fr">
      <trd>acide<klr tip="ind"> (adj.)</klr></trd>,
      <trd>aigre</trd>
    </trdgrp>
  </snc>
  <snc mrk="acid.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malagrable pika:
      <ekz>
	li estas en <tld/>a humoro
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la duone <tld/>a, duone apatia mieno montris homon &hcirc;ronike malsanan
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
	<tld/>a tono.
      </ekz>
    </dif>
    <trd lng="be">&c_z;'&c_je;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>
    <trdgrp lng="br">
      <trd>flemmus</trd>,
      <trd>c'hwerv</trd>
    </trdgrp>
    <trd lng="de">s&auml;uerlich</trd>
    <trdgrp lng="hu">
      <trd>savany&uacute;</trd>,
      <trd>mar&oacute;</trd>
    </trdgrp>
    <trd lng="ja">&#x8F9B;&#x8FA3;&#x306A;
    [<ind>&#x3057;&#x3093;&#x3089;&#x3064;&#x306A;</ind>]</trd>
    <trdgrp lng="pl">
      <trd>kwa&sacute;ny</trd>,
      <trd>uszczypliwy</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>acid</trd>,
      <trd>acru</trd>
    </trdgrp>
    <trd lng="ru">&c_je;&c_d;&c_k;&c_i;&c_j;</trd>
    <trd lng="sk">ostr&yacute;</trd>
    <trd lng="sv">syrlig</trd>
    <trdgrp lng="fr">
      <trd>acerbe</trd>,
      <trd>caustique</trd>
      <trd>acrimonieux</trd>
    </trdgrp>
    <trd lng="th">&#xE41;&#xE2A;&#xE1A;</trd>
  </snc>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF74;&#xF62;&#xF0B;&#xF58;&#xF7C;&#xF0B;</trd>
  <trd lng="ca">&agrave;cid<klr> (adj.)</klr></trd>
  <trd lng="cs">kysel&yacute;</trd>
  <trd lng="de">sauer</trd>
  <trd lng="en">acerbic</trd>
  <trd lng="fa">&a_t;&a_r;&a_sh;</trd>
  <trd lng="he">&#x5D7;&#x5D5;&#x5BC;&#x5DE;&#x5E6;&#x5B8;&#x5D4;</trd>
  <trd lng="it">acido<klr> (agg.)</klr></trd>
  <trd lng="nl">zuur</trd>
  <trd lng="pt">&aacute;cido</trd>
  <trd lng="sk">kysl&yacute;</trd>
<trd lng="th">&#xE40;&#xE1B;&#xE23;&#xE35;&#xE49;&#xE22;&#xE27;</trd>
</drv>

<drv mrk="acid.0o">
  <kap><tld/>o</kap>
  <snc mrk="acid.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kombina&jcirc;o de hidrogeno kun acida resta&jcirc;o
      (<frm>SO<sub>4</sub><sup>2-</sup></frm>,
      <frm>NO<sub>3</sub><sup>-</sup></frm>, <frm>Cl<sup>-</sup></frm> kaj aliaj),
      kiu reagas kun metaloj kaj bazoj, formante
      <ref cel="sal.0o">salojn</ref>, kaj ru&gcirc;igas lakmuson:
      <ekz>
        bora <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
	en tiuj &ccirc;i boteletoj sin trovas diversaj <tld/>oj:
	vinagro, sulfur<tld/>o, azot<tld/>o kaj aliaj
	<fnt><bib>F</bib>, <lok>ekzercaro, &para; 35</lok></fnt>;
      </ekz>
      <ekz>
        citrono estas frukto <klr>[&#x2026;]</klr> <tld/>hava
        <fnt>
          <bib>Fab4</bib>
          <lok>Abocolibro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE01;&#xE23;&#xE14;</trd>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="br">trenkenn</trd>
  <trd lng="ca">&agrave;cid<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>acidum</trd>,
    <trd>kyselina</trd>
  </trdgrp>
  <trd lng="de">S&auml;ure</trd>
  <trd lng="en">acid</trd>
  <trd lng="es">&aacute;cido</trd>
  <trd lng="fa">&a_A;&a_s;&f_y;&a_d;</trd>
  <trd lng="fr">acide<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="he">&#x5D7;&#x5D5;&#x5BC;&#x5DE;&#x5E6;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">sav</trd>
  <trd lng="id"><ind>asam</ind> (senyawa)</trd>
  <trd lng="it">acido<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x9178; [<ind>&#x3055;&#x3093;</ind>]</trd>
  <trd lng="nl">zuur</trd>
  <trd lng="pl">kwas</trd>
  <trd lng="pt">&aacute;cido</trd>
  <trd lng="ro">acid</trd>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">kyselina</trd>
  <trd lng="sv">syra</trd>
</drv>

<drv mrk="acid.amin0o">
  <kap>amin<tld/>o, <var><kap>amina <tld/>o</kap></var></kap>
  <snc mrk="acid.amin0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Eta <ref tip="super" cel="moleku.0o.KEM">molekulo</ref> 
      konsistanta el acido (<frm>-COOH</frm>) kaj amino (<frm>-NH<sub>2</sub></frm> a&ubreve;
      <frm>-NH-</frm>), 
      baza elemento de <ref tip="malprt" cel="protein.0o.FIZL">proteino</ref>:
      <ekz>
        la proteinaj fibroj entenataj de la silka&jcirc;oj havas aminajn
        <tld/>ojn utilajn al la homa sano, kiuj estas efikaj
        por preventi radiadon de sunlumo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-living/kalejdoskopo/64.htm">
          Silka&jcirc;oj estas utilaj...</url></vrk>,
          <lok>[2003]</lok>
        </fnt>;
      </ekz>
      <ekz>
        sciencistoj pensas, ke formi&gcirc;is el la mineralaj eroj de la
        unuaj &scirc;tonoj kun la helpo de la fulmenergioj la unuaj 
        amin<tld/>oj
        <fnt>
          <aut>G. V. L&uuml;pke, trad. Donjo kaj Cezar</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/28475/107775">
          &Ccirc;u &scirc;tonoj tamen vivas?</url></vrk>,
          <lok>2008-11-14</lok>
        </fnt>.
      </ekz>
      <ekz>
        nomoj de la plej oftaj aminoacidoj:
         <refgrp tip="ekz">
           <ref cel="glicin.0o">Glicino</ref> (Gly, G), 
         </refgrp>
           Alanino (Ala, A), 
           Valino (Val, V), 
           Le&ubreve;cino (Leu, L), 
           Izole&ubreve;cino (Ile, I), 
           Fenilalanino (Phe, F), 
           Tirozino (Tyr, Y), 
           Triptofano (Trp, W), 
           Serino (Ser, S), 
           Treonino (Thr, T), 
           Prolino (Pro), 
           Asparagino (Asn, N), 
           Cisteino (Cys, C), 
           Glutamino (Gln, Q), 
           Aspartato (Asp, D), 
           Glutama acido/<ref tip="ekz" cel="glutamat.0o.acido">Glutamato</ref> (Glu, E), 
           Lizino (Lys, K), 
           Arginino (Arg, R), 
           Histidino (His, H)
           kaj Metionino (Met, M).
           <fnt>
             <bib>Viki</bib>,
             <vrk><url ref="https://eo.wikipedia.org/wiki/Aminoacido">Aminacido</url></vrk>,
           </fnt>
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_ib;&c_n;&c_a;&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">amino&agrave;cid</trd>
  <trd lng="cs">aminokyselina</trd>
  <trd lng="de">Aminos&auml;ure</trd>
  <trd lng="es">amino&aacute;cido</trd>
  <trd lng="fr">acide amin&eacute;</trd>
  <trd lng="id"><ind>asam</ind> amino</trd>
  <trdgrp lng="it">
    <trd>amminoacido</trd>,
    <trd>aminoacido</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aminokwasy<klr> {pl.}</klr></trd>,
    <trd>aminokwas<klr> {sing.}</klr></trd>
  </trdgrp>
  <trd lng="ro">aminoacid</trd>                
  <trd lng="sk">aminokyselina</trd>
  <trd lng="sv">aminosyra</trd>
</drv>

<drv mrk="acid.desoksiribonuklea0o">
  <kap>desoksiribonuklea <tld/>o</kap>
  <mlg>DNA</mlg>
  <snc mrk="acid.desoksiribonuklea0o.GNT">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="acid.nukle0o.GNT">Nukleacido</ref>
      enhavanta la plenajn genajn informojn de &ccirc;elo:
      <ekz>
        la desoksiribonuklea <tld/>o <klr>[&#x2026;]</klr> estas la portanto
        de genetika informo
        <fnt>
          <aut>D. Benoit-Brow&aelig;ys, trad. J.-M. Cash</aut>,
          <vrk><url ref="http://eo.mondediplo.com/article1637.html">
          Kaj nun : konstrui vivan materialon</url></vrk>, 
          <lok>le Monde diplomatique, 2010-08</lok>
        </fnt>;
      </ekz>
      <ekz>
        la strukturo de la DNA estis <klr>[&#x2026;]</klr> malkovrita en 1953
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/wiki/DNA">
          DNA</url>, 2010-08-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_z;&c_o;&c_k;&c_s;&c_y;&c_r;&c_y;&c_b;&c_a;&c_n;&c_u;&c_k;&c_l;&c_je;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_ja;
  &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="ca">
    <trd><ind>&agrave;cid</ind> desoxiribonucleic</trd>,
    <trd>ADN</trd>,
    <trd>DNA</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>DNA</trd>,
    <trd>deoxyribonukleov&aacute; kyselina</trd>
  </trdgrp>
  <trd lng="de">Desoxyribonukleins&auml;ure</trd>
  <trdgrp lng="es">
    <trd>&aacute;cido <ind>desoxirribonucleico</ind></trd>,
    <trd>ADN</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acide d&eacute;soxyribonucl&eacute;ique</trd>,
    <trd>ADN</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>asam</ind> deoksiribonukleat</trd>,
    <trd>ADN</trd>,
    <trd>DNA</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd><ind>acido</ind> desossiribonucleico</trd>,
    <trd><ind>acido</ind> deossiribonucleico</trd>,
    <trd>DNA</trd>
  </trdgrp> 
  <trdgrp lng="pl">
    <trd>kwas deoksyrybonukleinowy</trd>,
    <trd>DNA</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>acid dezoxiribonucleic</trd>,
    <trd>ADN</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>DNA</trd>,
    <trd>deoxyribonukleov&aacute; kyselina</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>deoxiribonukleinsyra</trd>,
    <trd>DNA</trd>
  </trdgrp>
</drv>

<drv mrk="acid.karbon0o">
  <kap>
    karbon<tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      (<frm>H<sub>2</sub>CO<sub>3</sub></frm>) Acido, 
      reaga produkto de karbondioksido kun akvo:
      <ekz>
        &scirc;a&ubreve;mvino kun aldonita karbon<tld/>o
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/%C5%9Ca%C5%ADmvino">&Scirc;a&ubreve;mvino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        karbon<tld/>-enhavaj trinka&jcirc;oj kiel limonado kaj biero
        <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2020/05/trinkboteloj-el-sukero.html#!">Trinkboteloj
          el sukero | Esperanta Retradio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kohlens&auml;ure</trd>
  <trd lng="en">carbonic acid</trd>
  <trd lng="fr">acide carbonique</trd>
</drv>

<drv mrk="acid.nukle0o">
  <kap>nukle<tld/>o,
  <var><kap>nuklea <tld/>o</kap></var>,
  <var><kap>kern<tld/>o</kap></var></kap>
  <snc mrk="acid.nukle0o.GNT">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Granda molekulo el serio de
      <ref tip="prt" cel="nukleotid.0o.GNT">nukleotidoj</ref>,
      trovebla en la &ccirc;eloj de &ccirc;iuj vivuloj:
      <ekz>
        <nom>Asimov</nom> laboris kiel profesoro de bio&hcirc;emio <klr>[&#x2026;]</klr>, 
        esplorante pri nukle<tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Isaac_Asimov">
          Isaac Asimov</url>, 2007-03-10</lok>
        </fnt>;
     </ekz>
     <ekz>
       la&ubreve; nuna scio, el &ccirc;iuj molekuloj sin reprodukti
       povas nur 
       molekuloj de nuklea <tld/>o
       <fnt>
         <aut>D. Budden</aut>,
         <vrk><url ref="http://www.biblio-misio.org/index.asp?id=46">
         Kiel Aperis Vivo</url></vrk>,
         <lok>2010-09-17</lok>
       </fnt>.
     </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_k;&c_l;&c_je;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_ja;
  &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca"><ind>&agrave;cid</ind> nucleic</trd>
  <trd lng="de">Nukleins&auml;ure</trd>
  <trd lng="es">&aacute;cido nucleico</trd>
  <trd lng="fr">acide nucl&eacute;ique</trd>
  <trd lng="id"><ind>asam</ind> nukleat</trd>
  <trd lng="it"><ind>acido</ind> nucleico</trd>
  <trd lng="pl">kwas nukleinowy</trd>
  <trd lng="ro">acid nucleic</trd>
  <trd lng="sv">nukleinsyra</trd>
</drv>

<drv mrk="acid.ribonuklea0o">
  <kap>ribonuklea <tld/>o</kap>
  <mlg>RNA</mlg>
  <snc mrk="acid.ribonuklea0o.GNT">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="acid.nukle0o.GNT">Nukleacido</ref>
      povanta interalie kopii kaj transporti partan informon de
      <ref tip="vid" cel="acid.desoksiribonuklea0o.GNT">DNA</ref>-serio:
      <ekz>
        oni klasas la nukleajn acidojn en du kategoriojn: <tld/>ajn 
        acidojn (RNA), trovi&gcirc;antajn &ccirc;efe en la citoplasmo de
        
        vivantaj &ccirc;eloj kaj desoksiribonukleajn acidojn (DNA), 
        trovi&gcirc;antajn en la nukleoj de &ccirc;eloj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Nuklea_acido">
          nuklea acido</url>, 2010-08-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_b;&c_a;&c_n;&c_u;&c_k;&c_l;&c_je;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_ja;
  &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="ca">
    <trd><ind>&agrave;cid</ind> ribonucleic</trd>,
    <trd>ARN</trd>,
    <trd>RNA</trd>
  </trdgrp>
  <trd lng="cs">ribonukleov&aacute; kyselina</trd>
  <trd lng="de">Ribonukleins&auml;ure</trd>
  <trd lng="es">&aacute;cido ribonucleico</trd>
  <trdgrp lng="fr">
    <trd>acide ribonucl&eacute;ique</trd>,
    <trd>ARN</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>asam</ind> ribonukleat</trd>,
    <trd>ARN</trd>,
    <trd>RNA</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd><ind>acido</ind> ribonucleico</trd>,
    <trd>RNA</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kwas rybonukleinowy</trd>,
    <trd>RNA</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>acid ribonucleic</trd>,
    <trd>ARN</trd>
  </trdgrp>
  <trd lng="sk">ribonukleov&aacute; kyselina</trd>
  <trdgrp lng="sv">
    <trd>ribonukleinsyra</trd>,
    <trd>RNA</trd>
  </trdgrp>
</drv>
</art>
