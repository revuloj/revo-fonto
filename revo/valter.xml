        <tld/>o la 6-a de Brienne <klr>...</klr> estis francdevena  
        militisto kaj generalo
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Valtero_la_6-a_%28Brienne%29">
          Valtero la 6-a</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o, la frato de Frederiko, mortis hiera&ubreve;
        <fnt>
          <aut>M. Lennartz</aut>
          <vrk><url ref="http://www.ipernity.com/group/38154/discuss/26267/comment/7752531">
          Komo</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_a;&c_l;&c_mol;&c_t;&c_e;&c_r;</trd>
  <trd lng="ca">Walter</trd>
  <trd lng="de">Walter</trd>
  <trd lng="en">Walter</trd>
  <trdgrp lng="fr">
    <trd>Gauthier</trd>,
    <trd>Walter</trd>
  </trdgrp>
  <trd lng="uk">&c_V;&c_a;&#x43B;&#x44C;&c_t;&c_je;&c_r;</trd>
</drv>
