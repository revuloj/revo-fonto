<kap>
  <rad>Valter</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="valter.0o">
  <kap><tld/>o</kap>
  <snc mrk="valter.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
