<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: harian.xml,v 1.4 2020/09/09 15:10:34 revo Exp $">
<kap>
  <rad>Harian</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="j">Harjan</rad>/o <fnt><bib>Viki</bib></fnt></kap></var>
</kap>
<drv mrk="harian.0o">
  <kap><tld/>o,
    <var><kap><tld var="j"/>o</kap></var>
  </kap>
  <snc mrk="harian.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la norda parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        <tld var="j"/>o estas la hindia-parolanta parto de barata Pan&gcirc;abo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22496"
              >Harjano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Nigra ibiso <klr>[&#8230;]</klr> estas komuna <klr>[&#8230;]</klr> en <tld var="j"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48988"
              >Nigra ibiso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Haryana</trd>
  <trd lng="en">Haryana</trd>
  <trd lng="fr">Haryana</trd>
</drv>
</art>

