<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kortizon</rad>/o
</kap>
<drv mrk="kortizon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kortikosurrena hormono.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_y;&c_z;&c_o;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Kortison</trd>,
      <trd>Cortison</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_t;&c_i;&c_z;&c_o;&c_n;</trd>
  </snc>
</drv>
</art>
