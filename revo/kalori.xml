<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalori.xml,v 1.29 2023/12/02 21:20:05 revo Exp $">
<kap>
  <rad>kalori</rad>/o
</kap>
<drv mrk="kalori.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PV</bib></fnt>
  <mlg>cal</mlg>
  <snc>
    <dif>
      Malnova komunuza
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">mezurunuo</ref> de 
      <ref cel="varm.0o.FIZ">varmo</ref>, egala al 4,18
      <ref cel="jxul.jx0o">&jcirc;uloj</ref>:
      <ekz>
        unu <tld/>o egalas al la varmo necesa por altigi la
        temperaturon de unu gramo da akvo de 14,5&deg; &gcirc;is 
        15,5&deg; sub normala aerpremo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_jo;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="cs">kalorie</trd>
  <trdgrp lng="de">
    <trd>Kalorie</trd>,
    <trd>Calorie</trd>
  </trdgrp>
  <trd lng="en">calorie</trd>
  <trd lng="es">calor&iacute;a</trd>
  <trd lng="fr">calorie</trd>
  <trd lng="hu">kal&oacute;ria</trd>
  <trd lng="id">kalori</trd>
  <trd lng="ja">&#x30AB;&#x30ED;&#x30EA;&#x30FC;  <pr>&#x304B;&#x308D;&#x308A;&#x30FC;</pr></trd>
  <trd lng="nl">calorie</trd>
  <trd lng="pl">kaloria</trd>
  <trd lng="pt">caloria</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">kal&oacute;ria</trd>
  <trd lng="tr">kalori</trd>
  <trd lng="uk">&c_k;&c_a;&c_l;&c_o;&c_r;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x5361; <pr>k&#x1CE;</pr></trd>,
    <trd>&#x5361;&#x8DEF;&#x91CC; <pr>k&#x1CE;l&#xF9;l&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalori.granda0o">
  <kap>granda <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="kiloka.0o">kilokalorio</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_k;&c_a;&c_l;&c_jo;&c_r;&c_y;&c_ja;</trd>
  <trd lng="cs">velk&aacute; kalorie</trd>
  <trdgrp lng="de">
    <trd>Kilocalorie</trd>,
    <trd>Kilogrammkalorie</trd>,
    <trd>Gro&szlig;e Kalorie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Calorie</trd>,
    <trd>kilocalorie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nagykal&oacute;ria</trd>,
    <trd>kilogramkal&oacute;ria</trd>
  </trdgrp>
  <trd lng="id">kilokalori</trd>
  <trd lng="ja">&#x5927;&#x30AB;&#x30ED;&#x30EA;&#x30FC;  <pr>&#x3060;&#x3044;&#x304B;&#x308D;&#x308A;&#x30FC;</pr></trd>
  <trd lng="nl">kilocalorie<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pl">kilokaloria</trd>
  <trd lng="sk">ve&lcaron;k&aacute; kal&oacute;ria</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x5361; <pr>d&#xE0;k&#x1CE;</pr></trd>,
    <trd>&#x5343;&#x5361;&#x8DEF;&#x88E1; <pr>qi&#x101;nk&#x1CE;l&#xF9;l&#x1D0;</pr></trd>,
    <trd>&#x5343;&#x5361; <pr>qi&#x101;nk&#x1CE;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalori.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="kalori.0o"><tld/>o</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_m;-&c_k;&c_a;&c_l;&c_jo;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Grammcalorie</trd>,
    <trd>Kleine Calorie</trd>,
    <trd>Internationale Dampftafel-Calorie</trd>
  </trdgrp>
  <trd lng="en">calorie</trd>
  <trdgrp lng="fr">
    <trd>calorie</trd>,
    <trd>petite <ind>calorie</ind></trd>
  </trdgrp>
  <trd lng="hu">kiskal&oacute;ria</trd>
  <trd lng="id">kalori</trd>
  <trd lng="ja">&#x5C0F;&#x30AB;&#x30ED;&#x30EA;&#x30FC;  <pr>&#x3057;&#x3087;&#x3046;&#x304B;&#x308D;&#x308A;&#x30FC;</pr></trd>
  <trd lng="nl">calorie<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="zh">&#x5361;&#x8DEF;&#x91CC; <pr>k&#x1CE;l&#xF9;l&#x1D0;</pr></trd>
</drv>
</art>
