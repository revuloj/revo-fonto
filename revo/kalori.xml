<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalori</rad>/o
</kap>
<drv mrk="kalori.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Unuo de varmokvanto. 
    </dif>
  </snc>
  <trd lng="hu">kal&oacute;ria</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>
<drv mrk="kalori.granda0o">
  <kap>granda <tld/>o</kap>
  <snc>
    <dif>
      La varmokvanto necesa por varmigi 1 kg da akvo je 1&ring; C. 
    </dif>
  </snc>
</drv>
<drv mrk="kalori.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc>
    <dif>
      La varmokvanto necesa por varmigi 1 g da akvo je 1&ring; C.  
    </dif>
  </snc>
</drv>
</art>
