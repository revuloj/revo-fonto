<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>pastr</rad>/o
    </kap>

    <drv mrk="pastr.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Rajtigita funkciulo de religia kulto:
          <ekz>
            <tld/>o vivas de pre&gcirc;oj, advokato de le&gcirc;oj<fnt>Z</fnt>.
          </ekz> 
        </dif>
        <refgrp tip="super">
          <ref tip="super" cel="eklezi.0ulo">ekleziulo</ref>,
          <ref cel="klerik.0o">kleriko</ref></refgrp>
        <refgrp tip="sub">
          <ref cel="sacerd.0o">sacerdoto</ref>,
          <ref cel="pastor.0o">pastoro</ref></refgrp>
        <trd lng="grc">&jota_densa;&epsilon;&rho;&epsilon;&ypsilon_acute;&sigma_fina;</trd>
        <trdgrp lng="ru">
          <trd><ind>&c_zh;&c_r;&c_je;&c_c;</ind> <klr>(pagana)</klr></trd>,
          <trd><ind>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</ind> <klr>(kristana)</klr></trd>,
          <trd><ind>&c_i;&c_je;&c_r;&c_je;&c_j;</ind> <klr>(kristana)</klr></trd>,
          <trd><ind>&c_p;&c_o;&c_p;</ind> <klr>(ortodoksa)</klr></trd></trdgrp>
      </snc>
    </drv>

    <drv mrk="pastr.&ccirc;ef0o">
      <kap>&ccirc;ef<tld/>o</kap>
      <snc mrk="pastr.&ccirc;ef0o.episkopo">
        <dif>
          Altranga <ref tip="super" cel="pastr.0o"><tld/>o</ref>
          rajtigita plenumi &ccirc;iujn sakramentojn de koncerna kulto:
          <ekz>
            en la ortodoksa kristanismo &ccirc;iu episkopo estas
            &ccirc;ef<tld/>o;
          </ekz><ekz>
            kaj kunvenis &ccirc;e li &ccirc;iuj &ccirc;efpastroj kaj plia&gcirc;uloj
            kaj skribistoj
            <fnt><vrk>&N;</vrk>. <lok>&Mar; 14:53.</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_je;&c_r;&c_je;&c_j;</trd>
      </snc>
      <snc mrk="pastr.&ccirc;ef0o.estro">
        <dif>
          Regiona a&ubreve; tutmonda estro de koncerna kulto religia:
          <ekz>
            la milita ministro Sem-amen-Herhor, kiu estis &ccirc;ef<tld/>o
            de la plej granda templo de Amon en Teboj
            <fnt>
              <aut>&PtK;</aut>: <vrk>&Far1;</vrk> <lok>&Ccirc;ap. 1.</lok></fnt>;
          </ekz><ekz>
            &ccirc;iufoje, kiam ili vidis, ke en la kesto estas
            multe da mono, venadis skribisto de la re&gcirc;o kaj
            la &ccirc;efpastro, kaj ili kunligadis kaj kalkuladis
            la monon, trovitan en la domo de la Eternulo
            <fnt>
              <aut>&tZ;</aut>: <vrk>&X;</vrk>. <lok>&Re2; 12:10.</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="en">high priest</trd>
        <trdgrp lng="ru">
          <trd><ind>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_n;&c_y;&c_j; &c_zh;&c_r;&c_je;&c_c;</ind> <klr>(pagana)</klr></trd>,
          <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</trd></trdgrp>
      </snc>
      <trd lng="grc">&alfa_psili;&rho;&chi;&jota;&epsilon;&rho;&epsilon;&ypsilon_acute;&sigma_fina;</trd>
      <trdgrp lng="en">
        <trd>archpriest</trd>,
        <trd>hierarch</trd>,
        <trd>prelate</trd></trdgrp>
    </drv>

    <drv mrk="pastr.0i">
      <kap><tld/>i<fnt>Z</fnt></kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Esti <tld/>o. 
        </dif>
        <refgrp tip="vid">
          <ref cel="ordinac">ordino</ref>.
        </refgrp>
        <trd lng="ru">&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </snc>
    </drv>
  </art>
<!--
$Log$
-->
</vortaro>
