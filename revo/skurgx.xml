<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skurgx.xml,v 1.19 2023/10/21 07:46:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skur&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skurgx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skurgx.0o.ARM">
    <dif>
      <ref tip="super" cel="vip.0o.ARM">Vipo</ref> kun mallonga tenilo
      kaj mallonga rimeno por punbati iun:
      <ekz>
        V&auml;in&auml;m&ouml;inen vipis la &ccirc;evalon
        verge, svingis per perlita <tld/>o
        <fnt>
        <aut>trad. J. E. Lepp&auml;koski</aut>
        <vrk>Kalevala</vrk>,
        <lok>1964</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_z;&c_u;&c_n;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flagell</trd>,
    <trd>assot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gei&szlig;el</trd>,
    <trd>Peitsche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scourge</trd>,
    <trd>whip</trd>,
    <trd>lash</trd>
  </trdgrp>
  <trd lng="fr">fouet</trd>
  <trd lng="hu">korb&aacute;cs</trd>
  <trd lng="ja">&#x3080;&#x3061;</trd>
  <trd lng="nl">gesel</trd>
  <trdgrp lng="pl">
    <trd>bicz</trd>,
    <trd>nachajka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_t;&c_ib;&c_g;</trd>,
    <trd>&#x43A;&c_a;&#x43D;&c_ch;&c_u;&#x43A;</trd>,
    <trd>&#x43D;&c_a;&c_g;&c_a;&c_j;&#x43A;&c_a;</trd>,
    <trd>&c_b;&c_i;&c_ch;</trd>,
    <trd>&#x43A;&c_ib;&c_sh;&#x43A;&c_a; (&c_b;&c_a;&c_t;&c_ib;&c_g; &c_z; &c_d;&c_je;&#x43A;&c_ib;&#x43B;&#x44C;&#x43A;&#x43E;&#x43C;&c_a;
    &c_h;&c_v;&#x43E;&c_s;&c_t;&c_a;&#x43C;&c_i;)</trd>
  </trdgrp>
</drv>

<drv mrk="skurgx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skurgx.0i.ARM">
    <dif>
      Bati per <tld/>o a&ubreve; simile forte kaj dolorige.
    </dif>
  </snc>
  <snc mrk="skurgx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akre satire kritiki.
    </dif>
    <ref tip="sin" cel="vip.0i">vipi</ref>
    <trd lng="hu">ostoroz</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_b;&c_ib;&c_z;&c_u;&c_n;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_je;&c_b;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flagel&middot;lar</trd>,
    <trd>assotar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scourge</trd>,
    <trd>whip</trd>,
    <trd>lash</trd>
  </trdgrp>
  <trd lng="fr">fouetter</trd>
  <trd lng="hu">megkorb&aacute;csol</trd>
  <trdgrp lng="ja">
    <trd>&#x3080;&#x3061;&#x6253;&#x3064;  <pr>&#x3080;&#x3061;&#x3046;&#x3064;</pr></trd>,
    <trd>&#x6FC0;&#x3057;&#x304F;&#x975E;&#x96E3;&#x3059;&#x308B;  <pr>&#x306F;&#x3052;&#x3057;&#x304F;&#x3072;&#x306A;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">geselen</trd>
  <trdgrp lng="pl">
    <trd>biczowa&cacute;</trd>,
    <trd>ok&lstroke;ada&cacute; <ind>biczem</ind></trd>,
    <trd>la&cacute; <ind>nachajk&aogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_l;&c_je;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_g;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_l;&c_je;&c_t;&c_mol;&c_ju;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_t;&c_i;
    &c_b;&c_a;&c_t;&#x43E;&c_g;&#x43E;&#x43C;/&#x43A;&c_a;&#x43D;&c_ch;&c_u;&#x43A;&#x43E;&#x43C;/&#x43D;&c_a;&c_g;&c_a;&c_j;&#x43A;&#x43E;&#x44E;/&c_b;&c_i;&c_ch;&c_je;&#x43C;</trd>,
    <trd>&c_b;&c_i;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
