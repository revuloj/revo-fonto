<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>skur&gcirc;</rad>/o
    </kap>
    <drv mrk="skurgx.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Vipo kun mallonga tenilo k mallonga rimeno por punbati iun.
	  <ekz>
            A&gcirc;a, sa&gcirc;a V&auml;in&auml;m&ouml;inen prenis viglan vir&ccirc;evalon, jungis
            junan &ccirc;evalidon anta&ubreve; brunan lar&gcirc;an sledon, levis lar&gcirc;e
            sin ensleden kaj aran&gcirc;is sin por sidi, vipis la &ccirc;evalon
            verge, svingis per perlita <tld/>o.
            <fnt><vrk>Kalevala</vrk>, deka kanto</fnt>
          </ekz>
        </dif>
      </snc>
      <trd lng="nl">gesel</trd>
    </drv>
    <drv mrk="skurgx.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc num="1">
        <dif>
          Bati per <tld/>o.
        </dif>
      </snc>
      <snc num="2">
        <dif>
          Forte k dolorige batadi.
        </dif>
      </snc>
      <snc num="3">
        <uzo tip="klr">(f)</uzo><dif>
          Akre satire kritiki.
        </dif>
        <refgrp tip="sin">
          <ref cel="vip.0i">vipi</ref>.
        </refgrp>
      </snc>
      <trd lng="nl">geselen</trd>
    </drv>
  </art>

