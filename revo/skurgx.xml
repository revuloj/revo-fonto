<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skurgx.xml,v 1.21 2024/03/25 06:40:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skur&gcirc;</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="skurgx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skurgx.0o.ARM">
    <dif>
      <ref tip="super" cel="vip.0o.ARM">Vipo</ref> kun mallonga tenilo
      kaj mallonga rimeno por punbati iun:
      <ekz>
        li sentigos sian <tld/>on, li kra&ccirc;os al vi en la viza&gcirc;on, kiam li renkontos vin
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ricevis la <tld/>opunon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>?
      </ekz>
      <ekz>
        <nom>V&auml;in&auml;m&ouml;inen</nom> vipis la &ccirc;evalon
        verge, svingis per perlita <tld/>o
        <fnt>
        <aut>trad. J. E. Lepp&auml;koski</aut>
        <vrk>Kalevala</vrk>,
        <lok>1964</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="knut.0o">knuto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_z;&c_u;&c_n;</trd>,
    <trd>&c_n;<ofc>*</ofc>&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flagell</trd>,
    <trd>assot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ofc>*</ofc>Gei&szlig;el</trd>,
    <trd>Peitsche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><ofc>*</ofc>scourge</trd>,
    <trd>whip</trd>,
    <trd>lash</trd>
  </trdgrp>
  <trd lng="fr"><ofc>*</ofc>fouet</trd>
  <trd lng="hu">korb&aacute;cs</trd>
  <trd lng="ja">&#x3080;&#x3061;</trd>
  <trd lng="nl">gesel</trd>
  <trdgrp lng="pl">
    <trd><ofc>*</ofc>bicz</trd>,
    <trd><ofc>*</ofc>nachajka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_t;&c_ib;&c_g;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_ch;&c_u;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_i;&c_ch;</trd>,
    <trd>&c_k;&c_ib;&c_sh;&c_k;&c_a; (&c_b;&c_a;&c_t;&c_ib;&c_g; &c_z; &c_d;&c_je;&c_k;&c_ib;&c_l;&c_mol;&c_k;&c_o;&c_m;&c_a;
    &c_h;&c_v;&c_o;&c_s;&c_t;&c_a;&c_m;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76AE;&#x97AD; <pr>p&#xED;bi&#x101;n</pr></trd>,
    <trd>&#x68F0; <pr>chu&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skurgx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skurgx.0i.ARM">
    <dif>
      <ref tip="super" cel="bat.0i.puni">Bati</ref> per <tld/>o a.s. forte kaj dolorige:
      <ekz>
        ili transdonos vin al sinedrioj, kaj <klr>[&#x2026;]</klr> vin <tld/>os
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 10:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kultivistoj, kaptinte liajn sklavojn, <tld/>is unu, kaj mortigis alian
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 21:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        hajleroj <klr>[&#x2026;]</klr> <tld/>is lian viza&gcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nacioj <tld/>itaj de malsanoj, epidemioj kaj katastrofoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">auspeitschen</trd>
  </snc>
  <snc mrk="skurgx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akre <ref cel="insult.0i">insulti</ref>, satire <ref tip="super" cel="kritik.0i">kritiki</ref>:
      <ekz>
        pro tiuj vortoj, kiuj lin <tld/>is en plena viza&gcirc;o, la nekonatulo eksaltegis,
        kaj faris geston por survangi la insultanton
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        iam recenzistoj tro forte &leftquot;<tld/>is&rightquot; lin kaj finfine <nom>Simenon</nom> e&ccirc; rezignis pri
        verkado
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 6 (104)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vip.0i.mallauxdi">vipi<sncref/></ref>
    <trd lng="hu">ostoroz</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_b;&c_ib;&c_z;&c_u;&c_n;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_je;&c_b;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flagel&middot;lar</trd>,
    <trd>assotar</trd>
  </trdgrp>
  <trd lng="de">gei&szlig;eln</trd>
  <trdgrp lng="en">
    <trd>scourge</trd>,
    <trd>whip</trd>,
    <trd>lash</trd>
  </trdgrp>
  <trd lng="fr">fouetter</trd>
  <trd lng="hu">megkorb&aacute;csol</trd>
  <trdgrp lng="ja">
    <trd>&#x3080;&#x3061;&#x6253;&#x3064;  <pr>&#x3080;&#x3061;&#x3046;&#x3064;</pr></trd>,
    <trd>&#x6FC0;&#x3057;&#x304F;&#x975E;&#x96E3;&#x3059;&#x308B;  <pr>&#x306F;&#x3052;&#x3057;&#x304F;&#x3072;&#x306A;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">geselen</trd>
  <trdgrp lng="pl">
    <trd>biczowa&cacute;</trd>,
    <trd>ok&lstroke;ada&cacute; <ind>biczem</ind></trd>,
    <trd>la&cacute; <ind>nachajk&aogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_l;&c_je;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_g;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_l;&c_je;&c_t;&c_mol;&c_ju;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_t;&c_i;
    &c_b;&c_a;&c_t;&c_o;&c_g;&c_o;&c_m;/&c_k;&c_a;&c_n;&c_ch;&c_u;&c_k;&c_o;&c_m;/&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_o;&c_ju;/&c_b;&c_i;&c_ch;&c_je;&c_m;</trd>,
    <trd>&c_b;&c_i;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x97AD; <pr>bi&#x101;n</pr></trd>,
    <trd>&#x68F0; <pr>chu&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
