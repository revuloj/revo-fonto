<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skur&gcirc;</rad>/o
</kap>
<drv mrk="skurgx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vipo kun mallonga tenilo kaj mallonga rimeno por punbati iun.
      <ekz>
        V&auml;in&auml;m&ouml;inen vipis la &ccirc;evalon
        verge, svingis per perlita <tld/>o.
        <fnt>(<vrk>Kalevala</vrk>, <lok>deka kanto</lok>)</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_z;&c_u;&c_n;</trd>, 
    <trd>&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="hu">korb&aacute;cs</trd>  
  <trd lng="nl">gesel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="skurgx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Bati per <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      Forte kaj dolorige batadi.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akre satire kritiki.
    </dif>
    <ref tip="sin" cel="vip.0i">vipi</ref>
    <trd lng="hu">ostoroz</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_b;&c_ib;&c_z;&c_u;&c_n;&c_o;&c_m;</trd>, 
    <trd>&c_s;&c_mol;&c_c;&c_je;&c_b;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="hu">megkorb&aacute;csol</trd>  
  <trd lng="nl">geselen</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_l;&c_je;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_g;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_l;&c_je;&c_t;&c_mol;&c_ju;)</klr></trd>
  </trdgrp>
</drv>
</art>
