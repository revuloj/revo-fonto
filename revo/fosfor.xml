<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fosfor.xml,v 1.30 2023/11/10 06:08:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fosfor</rad>/o
</kap>
<drv mrk="fosfor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">P</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="15">elemento</ref> kun
      atomnumero 15, atompezo 30,9738;
      <ref tip="super" cel="metalo.0o">metaloido</ref>, facile
      ekbruligebla, uzata interalie por fabriki alumetojn.
    </dif>
    <rim>
      Por formi la nomon de <tld/>aj kompona&jcirc;oj, oni ofte uzas en
      &hcirc;emio la mallongigon <em>fosf-</em>, ekz.
      <ekz>fosfato, fosfito, fosfido.</ekz>
    </rim>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;</trd>
  <trd lng="bg">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;</trd>
  <trd lng="br">fosfor</trd>
  <trd lng="ca">f&ograve;sfor</trd>
  <trd lng="cs">fosfor</trd>
  <trd lng="de">Phosphor</trd>
  <trd lng="en">phosphorus</trd>
  <trd lng="es">f&oacute;sforo</trd>
  <trd lng="fa">&a_f;&a_s;&a_f;&a_r;</trd>
  <trd lng="fr">phosphore</trd>
  <trd lng="hu">foszfor</trd>
  <trd lng="id">fosforus</trd>
  <trd lng="it">fosforo</trd>
  <trd lng="ja">&#x30EA;&#x30F3;  <pr>&#x308A;&#x3093;</pr></trd>
  <trd lng="la">Phosphorus</trd>
  <trd lng="nl">fosfor</trd>
  <trd lng="pl">fosfor</trd>
  <trd lng="pt">f&oacute;sforo</trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;</trd>
  <trd lng="sk">fosfor</trd>
  <trd lng="sv">fosfor</trd>
  <trd lng="uk">&#x444;&#x43E;&#x441;&#x444;&#x43E;&#x440;</trd>
  <trd lng="zh">&#x71D0; <pr>l&#xED;n</pr></trd>
</drv>
</art>
