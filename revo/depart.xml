<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: depart.xml,v 1.24 2024/10/07 06:37:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>departement</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="depart.0o">
  <kap><tld/>o</kap>
  <snc mrk="depart.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ampleksa <ref tip="super" cel="sekci.0o.POL">sekcio</ref> de
      ministrejo a&ubreve; de granda administrejo:
      <ekz>
        la <tld lit="D"/>o de Komerco kaj Industria Disvolvi&gcirc;o de la Urbo Novjorko
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="D"/>o pri Edukado de Unesko
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iufoje, kiam mi trairadis tra la <tld/>o <klr>[&#8230;]</klr> &ccirc;io tremas, kiel a&ubreve;tuna
        folio
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili mem devas pagi al la &scirc;tataj komunikaj <tld/>oj la&ubreve;
        la oficialaj prezoj
        <fnt><aut>Liu Xiaozhe</aut>, <vrk>Interreto en
        malfacilo</vrk>, <bib>Monato</bib>, <lok>jaro 1997a, numero 8a,
        p. 15a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fak.0o.sekcio">fako<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Departement <klr>(schweiz.)</klr></trd>,
      <trd>Verwaltungsabteilung</trd>,
      <trd>Amt <klr>(f&uuml;r...)</klr></trd>,
      <trd>Referat <klr>(Ministeriumsabteilung)</klr></trd>,
      <trd>Dezernat</trd>
    </trdgrp>
    <trd lng="en">administrative department</trd>
    <trdgrp lng="hu">
      <trd>&uuml;gyoszt&aacute;ly</trd>,
      <trd>f&odblac;oszt&aacute;ly</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_v;&c_je;&c_d;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#20844;&#20849;&#26435;&#21147;&#26426;&#26500; <pr>g&omacron;ngg&ograve;ngqu&aacute;nl&igrave;j&imacron;g&ograve;u</pr></trd>,
      <trd>&#23448;&#26041; <pr>gu&amacron;nf&amacron;ng</pr></trd>,
      <trd>&#25919;&#24220;&#37096;&#38272; <pr>zh&egrave;ngf&#468;b&ugrave;m&eacute;n</pr></trd>,
      <trd>&#38498; <pr>yu&agrave;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="depart.0o.franca">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="gubern1.0o.POL">Gubernio</ref> en Francio k.a.:
      <ekz>
        ili estis vizitintaj <klr>[&#8230;]</klr> la marbordon de la <tld/>o
        <nom>Finist&egrave;re</nom>
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        iuj gloraj francaj tenoroj elvenis el la suda Francujo, dum &ccirc;iuj basuloj &mdash; el la
        <tld/>oj nordaj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        dokumentoj de laborista esperantista Movado en <tld/>o <nom>Bekes</nom> 1925-1934
        (hungarlingva)
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distri1.0o">distrikto</ref>
    </refgrp>
    <trd lng="de">D&eacute;partement</trd>
    <trd lng="be">&c_d;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>
    <trd lng="en">French administrative district</trd>
  </snc>
  <trd lng="ca">departament</trd>
  <trdgrp lng="cs">
    <trd>departement ve Francii</trd>,
    <trd>odbor</trd>,
    <trd>odd&ecaron;len&iacute; <klr>(&uacute;&rcaron;adu / instituce)</klr></trd>
  </trdgrp>
  <trd lng="es">departamento</trd>
  <trd lng="fr">d&eacute;partement</trd>
  <trd lng="he">&mem;&#1463;&het;&lamed;&#1464;&qof;&#1464;&he;</trd>
  <trd lng="hu">megye</trd>
  <trdgrp lng="ja">
    <trd>&#37096;&#38272;  <pr>&#12406;&#12418;&#12435;</pr></trd>,
    <trd>&#37096;  <pr>&#12409;</pr></trd>,
    <trd>&#23616;  <pr>&#12388;&#12412;&#12397;</pr></trd>,
    <trd>&#30465;  <pr>&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#23398;&#37096;  <pr>&#12364;&#12367;&#12406;</pr></trd>,
    <trd>&#23398;&#31185;  <pr>&#12364;&#12387;&#12363;</pr></trd>,
    <trd>&#30476;  <pr>&#12354;&#12364;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="pl">departament</trd>
  <trd lng="pt">departamento</trd>
  <trd lng="ru">&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>odbor</trd>,
    <trd>oddelenie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>
