<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>departement</rad>/o
</kap>
<drv mrk="depart.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc mrk="depart.0o.fako">
    <dif>
      Sekcio de ministrejo a&ubreve; de granda administrejo:
      <ekz>
        <tld/>o pri fervojoj.
      </ekz>
      <ekz>
        ili mem devas pagi al la &scirc;tataj komunikaj <tld/>oj la&ubreve;
la
        oficialaj prezoj<fnt><aut>Liu Xiaozhe</aut>: <vrk>Interreto en
        malfacilo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 8a, p.
        15a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&uuml;gyoszt&aacute;ly</trd>,
      <trd>f&odblac;oszt&aacute;ly</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_v;&c_je;&c_d;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="depart.0o.teritorio">
    <dif>
      En Francio administra <ref tip="super"
cel="terito.0o">teritorio</ref>, distriktaro estrata de prefekto.
    </dif>
    <trd
lng="be">&c_d;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>
  </snc>
  <trd lng="es">departamento</trd>
  <trd lng="fr">d&eacute;partement</trd>
  <trd lng="hu">megye</trd>
  <trd lng="pl">departament</trd>
  <trd
lng="ru">&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
