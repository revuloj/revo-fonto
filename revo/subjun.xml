<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>2</ofc>
      <rad>subjunktiv</rad>/o
    </kap>

    <drv mrk="subjun.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">LIN</uzo>
      <snc>
	<dif>
	  <ref tip="super" cel="modal.verba0o">Verba modalo</ref> ne
	  ekzistanta en Esperanto kaj montranta, ke iu
	  propozicio dependas de alia kaj ke iu ago estas konceptata
	  kiel dependa de alia ago, do kiel duba kaj necerta.
	</dif>
	<refgrp tip="vid">
	  <ref cel="voliti">volitivo</ref>.
	</refgrp>
      </snc>
      <trdgrp lng="de">
	<trd>Subjunktiv</trd>,
	<trd>Konjunktiv</trd>
      </trdgrp>
      <trd lng="es">subjuntivo</trd>
      <trd lng="fr">subjonctif</trd>
      <trdgrp lng="nl">
	<trd>subjunctief</trd>,
	<trd>conjunctief</trd>,
	<trd>aanvoegende <ind>wijs</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_o;&c_s;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
    </drv>
  </art>
