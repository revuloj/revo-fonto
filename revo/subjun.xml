</trdgrp>
<trd lng="es">subjuntivo</trd>
<trdgrp lng="nl">
    <trd>subjunctief</trd>,
    <trd>conjunctief</trd>,
    <trd>aanvoegende <ind>wijs</ind></trd>
  </trdgrp>
</drv>
</art>
