<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>i</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="i2.0.vokalo">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      Unu el la kvin <ref tip="dif" cel="vokal.0o">vokaloj</ref> de
      Esperanto, la malvasta anta&ubreve;a, kaj
      nomo de tiu <ref tip="dif" cel="fonem.0o">fonemo</ref> [<tld/>].
    </dif>
    <trd lng="fr">i<klr> (voyelle)</klr></trd>
  </snc>
  <snc>
     <dif>
      La dekdua <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
     <trd lng="fr">i<klr> (lettre)</klr></trd>
     <subsnc>
     <uzo tip="fak">MAT</uzo>
     <dif>
     (maj.) romana cifero kun valoro de unu: <ekz><tld lit="I"/> (unu),
     <tld lit="I"/><tld lit="I"/> (du)</ekz>;
     </dif>
     <trd lng="fr">1<klr> (chiffre romain)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">KEM</uzo>
     <dif>
     (maj.) simbolo de iodo (a&ubreve; jodo): <ekz><tld lit="I"/></ekz>;
     </dif>
     <trd lng="fr">iode</trd>
     </subsnc>
     <subsnc>
     <dif>
     (maj.) signo de formo: <ekz>li estas rekta kiel <tld lit="I"/>;
     <tld
     lit="I"/>-fero</ekz>;
     </dif>
     <trd lng="fr">I<klr> (forme de)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">MAT</uzo>
     <dif>
     (min.) simbolo de la imaginara unuo (unu el la du radikoj de la
     ekvacio z&quadrat; = -1):<ekz> <tld/></ekz>;
     </dif>
     <trd lng="fr">i<klr> (nombre imaginaire)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">MAT</uzo>
     <dif>
     (min.) romana samvalora cifero en iuj presa&jcirc;oj: <ekz><tld/></ekz>;
     </dif>
     <trd lng="fr">1<klr> (chiffre romain)</klr></trd>
     </subsnc> 
  </snc>
  <snc>
  <ref tip="vid" cel="i1.0">-<tld/> (fina&jcirc;o)</ref>
  <trd lng="fr">-i<klr> (terminaison)</klr></trd>
  </snc>
  <snc>
  <ref tip="vid" cel="i.0.sufikso">-<tld/>/ (&scirc;ajna sufikso)</ref>
  <trd lng="fr">-i/<klr> (pseudosuffixe)</klr></trd>
  </snc>
</drv>

<drv mrk="i2.0.greka">
  <kap><tld/> greka</kap>
    <snc>    
    <ref tip="dif" cel="ipsilon.0o">ipsilono</ref>
    </snc>
  <trdgrp lng="cs">
    <trd>ypsilon</trd>,
    <trd>&Ypsilon;</trd>,
    <trd>&ny; &rcaron;eck&eacute; i</trd>
  </trdgrp>
  <trd lng="fr">i grec</trd>
  <trd lng="pl">igrek <klr>(litera Y)</klr></trd>
  <trdgrp lng="sk">
    <trd>Y</trd>,
    <trd>ypsilon</trd>
  </trdgrp>
</drv>
</art>

