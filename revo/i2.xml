<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: i2.xml,v 1.18 2025/06/14 18:00:21 revo Exp $">
<kap>
    <ofc>*</ofc><ofc>7</ofc><rad>i</rad> <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="i2.0">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc mrk="i2.0.vokalo">
     <dif>
      Nomo de la dekdua litero de la Esperanta alfabeto:
        <ekz>
          nomoj de la literoj: <klr>[...]</klr> fo, go, &gcirc;o, ho, &hcirc;o, i,
          <klr>[...]</klr>
          <fnt>
            <bib>F</bib>
            <lok>&FE; 1</lok>
          </fnt>,</ekz>
        uzata i.a. kiel:
     </dif>
     <subsnc>
       <uzo tip="fak">MAT</uzo>
       <dif>
       (maj.) romana cifero kun valoro de unu: <ekz><tld lit="I"/>
       (unu),
       <tld lit="I"/><tld lit="I"/> (du);</ekz>
       </dif>
       <trd lng="fr">1<klr> (chiffre romain)</klr></trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">KEM</uzo>
       <dif>
       (maj.) simbolo de iodo (a&ubreve; jodo): <ekz><tld lit="I"/>;</ekz>
       </dif>
       <trd lng="fr">iode</trd>
     </subsnc>
     <subsnc>
       <dif>
       (maj.) signo de formo: <ekz>li estas rekta kiel <tld lit="I"/>;
       <tld lit="I"/>-fero;</ekz>
       </dif>
       <trd lng="fr">I<klr> (forme de)</klr></trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">MAT</uzo>
       <dif>
       (min.) simbolo de la imaginara unuo (unu el la du radikoj de la
       ekvacio <frm am="z^2=-1">z&quadrat; = -1</frm>):<ekz>
       <tld/>;</ekz>
       </dif>
       <trd lng="fr">i<klr> (nombre imaginaire)</klr></trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">MAT</uzo>
       <dif>
       (min.) romana samvalora cifero en iuj presa&jcirc;oj:
       <ekz><tld/>.</ekz>
       </dif>
       <trd lng="fr">1<klr> (chiffre romain)</klr></trd>
     </subsnc>
  </snc>
  <refgrp tip="hom">
     <ref cel="i1.0">-<tld/> (fina&jcirc;o)</ref>,
     <ref cel="i.0.sufikso">-<tld/>/ (&scirc;ajna sufikso)</ref>
  </refgrp>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a; i</trd>
  <trd lng="ja">&#12452;&#12540;</trd>
</drv>

<drv mrk="i2.0_greka">
  <kap><tld/> greka</kap>
    <snc>
    <ref tip="dif" cel="ipsilon.0o">ipsilono</ref>
    </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_g;&c_r;&c_e;&c_k;</trd>,
    <trd>&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a; y</trd>
  </trdgrp>
  <trd lng="ca">i grega</trd>
  <trdgrp lng="cs">
    <trd>ypsilon</trd>,
    <trd>&Ypsilon;</trd>,
    <trd>&ny; &rcaron;eck&eacute; i</trd>
  </trdgrp>
  <trd lng="es">ye</trd>
  <trd lng="fr">i grec</trd>
  <trd lng="pl">igrek <klr>(litera Y)</klr></trd>
  <trdgrp lng="sk">
    <trd>Y</trd>,
    <trd>ypsilon</trd>
  </trdgrp>
</drv>
</art>

