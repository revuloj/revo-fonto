<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sabek.xml,v 1.22 2023/10/21 06:31:28 revo Exp $">
<kap>
  <rad>sabek</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sabek.0o">
  <kap><tld/>o</kap>
  <snc mrk="sabek.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antikva hebrea
      <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumento</ref>
      triangula:
      <ekz>
        &ccirc;iuj popoloj eka&ubreve;dis la sonon de korno, fluto, harpo, <tld/>o,
        psaltero, kaj diversaj muzikaj instrumentoj
        <fnt><bib>MT</bib>, <lok>&Dan; 3:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_b;&c_u;&c_k;&c_a; <klr>(&c_m;&c_u;&c_z;. &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  <trd lng="cs">sabek</trd>
  <trd lng="de">Sambuka</trd>
  <trd lng="en">sabeca</trd>
  <trd lng="fr">sambuque</trd>
  <trd lng="hu">sambuca <klr>(h&aacute;romsz&ouml;g alak&uacute; h&aacute;rfa)</klr></trd>
  <trd lng="it">sambuca</trd>
  <trdgrp lng="ja">
    <trd>&#x4E09;&#x89D2;&#x7434;  <pr>&#x3055;&#x3093;&#x304B;&#x304F;&#x3054;&#x3068;</pr></trd>,
    <trd>&#x30B5;&#x30F3;&#x30D6;&#x30AB;</trd>
  </trdgrp>
  <trd lng="nl">sabbeca</trd>
  <trdgrp lng="pl">
    <trd>sambuka</trd>,
    <trd>sambyke</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>sabeca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_b;&c_u;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_b;&c_u;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">sabek</trd>
  <trd lng="uk">&c_s;&c_a;&#x43C;&c_b;&c_ib;&#x43A;&c_a; <klr>(&c_s;&c_t;&c_r;&c_u;&#x43D;&#x43D;&c_i;&c_j;
  &c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;)</klr></trd>
</drv>
</art>
