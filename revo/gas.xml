<drv mrk="gas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gas.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Aersimila fluida&jcirc;o, kies molekuloj tre malmulte
      alteni&gcirc;as
      inter si kaj &ccirc;iam emas disi&gcirc;i (aero, hidrogeno,
      brul<tld/>o,
      ktp):
      <ekz>
        jam post tri tagoj la ventro de mortinta besto tre &scirc;velas
        pro
        la <tld/>oj de la putrado
        <fnt>
        <aut>W. M. A. De Smet</aut>,
        <vrk>Gigantoj sur strandoj</vrk>,
        <bib>Monato</bib>, <lok>jaro 1998a, numero 11a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pt">g&aacute;s</trd>
  </snc>
  <snc mrk="gas.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="gas.brul0o.ENE">brul<tld/>o</ref>
    <trd lng="pt">g&aacute;s<klr tip="amb"> (combust&iacute;vel, de
    cozinha)</klr></trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="vapor.0o">vaporo</ref>
  </refgrp>
  <trd lng="af">gas</trd>
  <trd lng="am">&#4875;&#4829;</trd>
  <trd lng="ar">&a_A;&a_l;&a_gh;&a_A;&a_z;</trd>
  <trd lng="az">qaz</trd>
