<drv mrk="gas.0eca">
  <kap><tld/>eca</kap>
  <snc mrk="gas.0eca.FIZ">
    <dif>
      Havanta la ecojn de <tld/>o:
      <ekz>
        <tld/>eca stato de materio;
      </ekz>
      <ekz>
        la <tld/>eca planedo Jupitero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=526690">Limo de Roche</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trd lng="ca">gas&oacute;s</trd>
