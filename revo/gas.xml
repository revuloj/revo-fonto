<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gas</rad>/o
</kap>
<drv mrk="gas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Aersimila fluida&jcirc;o, kies molekuloj tre malmulte alteni&gcirc;as
      inter si k &ccirc;iam emas disi&gcirc;i (aero, hidrogeno, lum<tld/>o, ktp).  
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="gas.brul0o">brul<tld/>o.</ref>
  </snc>
  <refgrp tip="vid">
    <ref cel="vapor.0o">vaporo</ref>.
  </refgrp>
   <trd lng="de">Gas</trd>
  <trd lng="en">gas</trd>
  <trd lng="fr">gaz</trd>
  <trd lng="nl">gas</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;</trd>
</drv>
<drv mrk="gas.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en stato de <tld/>o. 
    </dif>
  </snc>
   <trd lng="de">gasf&ouml;rmig</trd>
  <trd lng="fr">gazeux</trd>
  <trdgrp lng="nl">
    <trd>gas-</trd>,
    <trd>gasachtig</trd>,
    <trd>gasvormig</trd>
  </trdgrp>
<trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
<drv mrk="gas.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta la naturon de <tld/>o. 
    </dif>
  </snc>
  <trd lng="fr">gazeux</trd>
  <trd lng="nl">gasachtig</trd>
<trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="gas.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a, meti en <tld/>an staton. 
    </dif>
  </snc>
  <trd lng="fr">gaz&eacute;ifier <klr tip="ind">(transformer en gaz)</klr></trd>
</drv>

<drv mrk="gas.brul0o">
  <kap>brul<tld/>o, lum<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>, 
      produktata miksante diversajn naturajn a&ubreve; artefaritajn bruligeblajn 
      <tld/>ojn, kaj uzata por lumigi kaj hejti. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Brenngas</trd>,
     <trd>Stadtgas</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>gaz d'&eacute;clairage</trd>,
    <trd>gaz de ville</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lichtgas</trd>,
    <trd>stadsgas</trd>
  </trdgrp>
</drv>
<drv mrk="gas.0beko">
  <kap><tld/>beko</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ekstrema&jcirc;o de tubo, &ccirc;e kiu eli&gcirc;as k brulas lum<tld/>o.
    </dif>
    <refgrp tip="super">
      <ref cel="brul.0ilo">brulilo</ref>,
      <ref cel="flam.0ingo">flamingo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Brenner</trd>,
     <trd>Brennd&uuml;se</trd></trdgrp>
  <trd lng="fr">bec de gaz</trd>
  <trd lng="nl">gasbekken</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_a;&c_ja; &c_g;&c_o;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
</drv>
<drv mrk="gas.0mufo">
  <kap><tld/>mufo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de &ccirc;apeto, teksita el asbesto, kiun oni metas sur
      <tld/>bekon por pliintensigi la lumon de la flamo, kiu igas
      &gcirc;in ardanta. 
    </dif>
  </snc>
  <trd lng="fr">manchon <klr>(&agrave; incandescence)</klr></trd>
  <trd lng="nl">gasmof</trd>
</drv>
<drv mrk="gas.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Granda rezervujo por lum<tld/>o &ccirc;e la fabriko. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Gasbeh&auml;lter</trd>,
     <trd>Gastank</trd></trdgrp>
  <trd lng="fr">gazom&egrave;tre</trd>
  <trd lng="nl">gastank</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j; &c_b;&c_a;&c_l;&c_l;&c_o;&c_n;</trd>
</drv>
<drv mrk="gas.0mezurilo">
  <kap><tld/>mezurilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Aparato por reguligi la fluon kaj registri la kvanton de la
      distribuita lum<tld/>o. 
    </dif>
  </snc>
  <trd lng="fr">compteur &agrave; gaz</trd>
  <trd lng="nl">gasmeter</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j; &c_s;&c_ch;&c_jo;&c_t;&c_ch;&c_i;&c_k;</trd>
</drv>
<drv mrk="gas.min0o">
  <kap>min<tld/>o</kap>
  <snc>
    <dif>
      Flami&gcirc;ema kaj eksplodema <tld/>o en la karbominejoj, miksa&jcirc;o
      el aero kaj <ref cel="metan.0o">metano</ref>. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Grubengas</trd>,
     <trd>Biogas</trd></trdgrp>
  <trd lng="fr">grisou</trd>
  <trd lng="nl">mijngas</trd>
  <trd lng="ru">&c_r;&c_u;&c_d;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j; &c_g;&c_a;&c_z;</trd>
</drv>
<drv mrk="gas.ter0o">
  <kap>ter<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>o, konsistanta &ccirc;efe el metano, eli&gcirc;anta el teraj fontoj. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Erdgas</trd>,
     <trd>Naturgas</trd></trdgrp>
  <trd lng="fr">gaz naturel</trd>
  <trd lng="nl">aardgas</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j; &c_g;&c_a;&c_z;</trd>
</drv>
<drv mrk="gas.nobla0o">
  <kap>nobla <tld/>o</kap>
  <snc>
    <dif>
      Inerta <tld/>o, kies molekuloj ne povas kombini&gcirc;i kun alaij 
      molekuloj. 
    </dif>
  </snc>
  <trd lng="de">Edelgas</trd>
  <trd lng="nl">edelgas</trd>
  </drv>

</art>
