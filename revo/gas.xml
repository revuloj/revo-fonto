    <trd>gas hivernacle</trd>
  </trdgrp>
  <trd lng="de">Treibhausgas</trd>
  <trd lng="en">greenhouse gas</trd>
  <trd lng="es">gas de efecto invernadero</trd>
  <trd lng="fr">gaz &agrave; effet de serre</trd>
  <trd lng="nl">broeikasgas</trd>
  <trd lng="zh">&#28201;&#23460;&#27668;&#20307; <pr>w&#275;nsh&igrave;q&igrave;t&#464;</pr></trd>
</drv>

<drv mrk="gas.kuir0o">
  <kap>
    kuir<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="G"/>o uzata por kuirado, kutime metano a&ubreve; propano:
      <ekz>
        avanta&gcirc;o de tiu kuirmetodo estis <klr>[&#8230;]</klr> &scirc;paro de kuir<tld/>o
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi nenie ricevas rapid-efikan venenon, ja <klr>[&#8230;]</klr> kuir<tld/>o jam delonge
        ne estas
