<drv mrk="gas.0a">
  <kap><tld/>a</kap>
  <snc mrk="gas.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      El <tld/>o, funkcianta per <tld/>o:
      <ekz>
        <tld/>aj lanternoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        solidaj, likvaj a&ubreve; <tld/>aj substancoj atingas la
        tersupra&jcirc;on
        <fnt>
        <aut>E. Georgiev</aut>,
        <vrk><url ref="&Monato;/2001/007234.html">Vulkanoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2001/02, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>gas&oacute;s</trd>,
    <trd>de gas</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>na plyn <klr>(spot&rcaron;ebi&ccaron;)</klr></trd>,
    <trd>plynov&yacute;</trd>
  </trdgrp>
  <trd lng="de">gasf&ouml;rmig</trd>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>gaz</ind></trd>,
    <trd>gazeux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&aacute;z-</trd>,
    <trd>g&aacute;z halmaz&aacute;llapot&uacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12460;&#12473;&#29366;&#12398;  <pr>&#12364;&#12377;&#12376;&#12423;&#12358;&#12398;</pr></trd>,
    <trd>&#27671;&#20307;&#12398;  <pr>&#12365;&#12383;&#12356;&#12398;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gas-</trd>,
    <trd>gasachtig</trd>,
    <trd>gasvormig</trd>
  </trdgrp>
  <trd lng="pl">gazowy</trd>
  <trd lng="pt">gasoso</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
