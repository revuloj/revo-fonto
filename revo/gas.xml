<drv mrk="gas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gas.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Aersimila fluida&jcirc;o, kies molekuloj tre malmulte
      alteni&gcirc;as
      inter si kaj &ccirc;iam emas disi&gcirc;i (aero, hidrogeno,
      brul<tld/>o,
      ktp):
      <ekz>
        jam post tri tagoj la ventro de mortinta besto tre &scirc;velas
        pro
        la <tld/>oj de la putrado
        <fnt>
        <aut>W. M. A. De Smet</aut>,
        <vrk>Gigantoj sur strandoj</vrk>,
        <bib>Monato</bib>, <lok>jaro 1998a, numero 11a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pt">g&aacute;s</trd>
  </snc>
  <snc mrk="gas.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="gas.brul0o.ENE">brul<tld/>o</ref>
    <trd lng="pt">g&aacute;s<klr tip="amb"> (combust&iacute;vel, de
    cozinha)</klr></trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="vapor.0o">vaporo</ref>
  </refgrp>
  <trd lng="af">gas</trd>
  <trd lng="am">&#x130B;&#x12DD;</trd>
  <trd lng="ar">&#x627;&#x644;&#x63A;&#x627;&#x632;</trd>
  <trd lng="az">qaz</trd>
  <trd lng="be">&c_g;&c_a;&c_z;</trd>
  <trd lng="bg">&c_g;&c_a;&c_z;</trd>
  <trd lng="bn">&#x997;&#x9CD;&#x9AF;&#x9BE;&#x9B8;</trd>
  <trd lng="bo">&#xF62;&#xFB3;&#xF44;&#xF66;&#xF0B;&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;</trd>
  <trd lng="bs">gas</trd>
  <trd lng="da">gas</trd>
  <trd lng="de">Gas</trd>
  <trd lng="el">&alfa;&epsilon_ton;&rho;&jota;&omikron;</trd>
  <trd lng="en">gas</trd>
  <trd lng="es">gas</trd>
  <trd lng="et">gaasi</trd>
  <trd lng="fr">gaz</trd>
  <trd lng="fy">gas</trd>
  <trd lng="gl">gas</trd>
  <trd lng="gu">&#xA97;&#xAC7;&#xAB8;</trd>
  <trd lng="ha">gas</trd>
  <trd lng="hi">&#x917;&#x948;&#x938;</trd>
  <trd lng="hr">plin</trd>
  <trd lng="hu">g&aacute;z</trd>
  <trd lng="hy">&#x563;&#x561;&#x566;</trd>
  <trd lng="id">gas</trd>
  <trd lng="ja">&#x30AC;&#x30B9;</trd>
  <trd lng="ka">&#x10D2;&#x10D0;&#x10D6;&#x10D8;&#x10E1;</trd>
  <trd lng="km">&#x17A7;&#x179F;&#x17D2;&#x1798;&#x17D0;&#x1793;</trd>
  <trd lng="ko">&#xAC00;&#xC2A4;</trd>
  <trd lng="lo">&#xEAD;&#xEB2;&#xE8D;&#xEC1;&#xE81;&#xEB1;&#xEAA;</trd>
  <trd lng="ml">&#xD35;&#xD3E;&#xD24;&#xD15;&#xD02;</trd>
  <trd lng="mr">&#x917;&#x945;&#x938;</trd>
  <trd lng="my">&#x1013;&#x102C;&#x1010;&#x103A;&#x1004;&#x103D;&#x1031;&#x1037;</trd>
  <trd lng="ne">&#x917;&#x94D;&#x92F;&#x93E;&#x901;&#x938;</trd>
  <trd lng="nl">gas</trd>
  <trd lng="ny">gasi</trd>
  <trd lng="pa">&#xA17;&#xA48;&#xA38;</trd>
  <trd lng="pl">gaz</trd>
  <trd lng="ps">&#x62F; &#x6AB;&#x627;&#x632;&#x648; &#x62F;</trd>
