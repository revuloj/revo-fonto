<drv mrk="gas.forcej0o">
  <kap>
    forcej<tld/>o
  </kap>
  <snc mrk="gas.forcej0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Gasoj, kiu en la atmosfero respondas pri la <ref cel="forc.0ejaefiko">forceja efiko</ref>,
      t.e. <frm>CO<sub>2</sub></frm>, <frm>CH<sub>4</sub></frm>, <frm>N<sub>2</sub>O</frm>,
      <frm>CF<sub>4</sub></frm>, <frm>SF<sub>6</sub></frm> k.a.:
      <ekz>
        <klr>[ili]</klr> pasintjare reduktis siajn forcej<tld/>ojn
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009365.php">Du mil sep</url></vrk>
              <bib>Monato</bib>
          <lok>2007/12, p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Protokolo de Kioto <klr>[&#x2026;]</klr> fiksas akcepteblan ellasatan nivelon de
        karbona dioksido kaj de kvin aliaj forcejaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19546">Protokolo de Kioto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_n;&c_ib;&c_k;&c_o;&c_v;&c_y; <ind>&c_g;&c_a;&c_z;</ind></trd>
  <trd lng="de">Treibhausgas</trd>
  <trd lng="en">greenhouse gas</trd>
  <trd lng="es">gas de efecto invernadero</trd>
  <trd lng="fr">gaz &agrave; effet de serre</trd>
  <trd lng="nl">broeikasgas</trd>
  <trd lng="zh">&#x6E29;&#x5BA4;&#x6C14;&#x4F53; <pr>w&#x113;nsh&#xEC;q&#xEC;t&#x1D0;</pr></trd>
</drv>

<drv mrk="gas.kuir0o">
  <kap>
    kuir<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="G"/>o uzata por kuirado, kutime metano a&ubreve; propano:
      <ekz>
        avanta&gcirc;o de tiu kuirmetodo estis <klr>[&#x2026;]</klr> &scirc;paro de kuir<tld/>o
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi nenie ricevas rapid-efikan venenon, ja <klr>[&#x2026;]</klr> kuir<tld/>o jam delonge
        ne estas
        <fnt>
          <bib>SatirRak</bib>
          <lok>Honorigo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z; &c_d;&c_l;&c_ja; &c_b;&c_y;&c_t;&c_a;&c_v;&c_y;&c_h; &c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_a;&c_w;</trd>
  <trd lng="de">Kochgas</trd>
</drv>

<drv mrk="gas.larmo0o">
  <kap>larmo<tld/>o, <var><kap>larmiga <tld/>o</kap></var></kap>
  <snc mrk="gas.larmo0o.KOMUNE">
    <dif>
      <tld lit="G"/>o incita al la okuloj, uzata kontra&ubreve; strataj
      manifestaciantoj:
      <ekz>
        oni veturigis min al malsanulejo. Miaj okuloj estis difektitaj
        per larmiga <tld/>o
        <fnt>
        <aut>D. Paisner, R. Picciotto</aut>,
        <vrk><url ref="http://www.esperanto.pl/page.php?tid=311342">La
        lastaj el travivintoj</url></vrk>,
        <lok>[vidita en 2006]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la demonstraciintoj estis vunditaj pro pafado, batado kaj
        larmo-<tld/>o hodia&ubreve;
        <fnt>
        <aut>R. Manandhar</aut>,
        <vrk><url ref="http://razeno.blog.com/">Razeno blogas
        Esperante...</url></vrk>,
        <lok>2006-04-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_je;&c_z;&c_a;&c_c;&c_je;&c_ch;&c_n;&c_y; <ind>&c_g;&c_a;&c_z;</ind></trd>
  <trd lng="de">Tr&auml;nengas</trd>
  <trd lng="fr">gaz lacrymog&egrave;ne</trd>
  <trd lng="ja">&#x50AC;&#x6D99;&#x30AC;&#x30B9;  <pr>&#x3055;&#x3044;&#x308B;&#x3044;&#x30AC;&#x30B9;</pr></trd>
  <trd lng="pl">gaz &lstroke;zawi&aogonek;cy</trd>
  <trd lng="pt">g&aacute;s lacrimog&ecirc;neo</trd>
  <trdgrp lng="zh">
    <trd>&#x50AC;&#x6CEA;&#x74E6;&#x65AF; <pr>cu&#x12B;l&#xE8;iw&#x1CE;s&#x12B;</pr></trd>,
    <trd>&#x50AC;&#x6CEA;&#x5F39; <pr>cu&#x12B;l&#xE8;id&#xE0;n</pr></trd>,
    <trd>&#x50AC;&#x6CEA;&#x6C14;&#x4F53; <pr>cu&#x12B;l&#xE8;iq&#xEC;t&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gas.min0o">
