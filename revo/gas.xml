<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>gas</rad>/o
</kap>
<drv mrk="gas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Aersimila fluida&jcirc;o, kies molekuloj tre malmulte alteni&gcirc;as
      inter si kaj &ccirc;iam emas disi&gcirc;i (aero, hidrogeno, brul<tld/>o,
      ktp).
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="gas.brul0o">brul<tld/>o.</ref>
  </snc>
  <refgrp tip="vid">
    <ref cel="vapor.0o">vaporo</ref>.
  </refgrp>
  <trd lng="de">Gas</trd>
  <trd lng="en">gas</trd>
  <trd lng="fr">gaz</trd>
  <trd lng="nl">gas</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;</trd>
</drv>

<drv mrk="gas.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en stato de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">gasf&ouml;rmig</trd>
  <trd lng="fr">gazeux</trd>
  <trdgrp lng="nl">
    <trd>gas-</trd>,
    <trd>gasachtig</trd>,
    <trd>gasvormig</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="gas.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta la naturon de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">gazeux</trd>
  <trd lng="nl">gasachtig</trd>
  <trd
  lng="ru">&c_g;&c_a;&c_z;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="gas.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a, meti en <tld/>an staton.
    </dif>
  </snc>
  <trd lng="fr">gaz&eacute;ifier <klr tip="ind">(transformer en
  gaz)</klr></trd>
</drv>

<drv mrk="gas.brul0o">
  <kap>brul<tld/>o, lum<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a <ref tip="super"
      cel="brul.0igajxo">bruliga&jcirc;o</ref>,
      produktata miksante diversajn naturajn a&ubreve; artefaritajn
      bruligeblajn <tld/>ojn, kaj uzata por lumigi kaj hejti.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Brenngas</trd>,
    <trd>Stadtgas</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gaz d'&eacute;clairage</trd>,
    <trd>gaz de ville</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lichtgas</trd>,
    <trd>stadsgas</trd>
  </trdgrp>
</drv>

<drv mrk="gas.0beko">
  <kap><tld/>beko</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ekstrema&jcirc;o de tubo, &ccirc;e kiu eli&gcirc;as kaj brulas
      lum<tld/>o.
    </dif>
    <refgrp tip="super">
      <ref cel="brul.0ilo">brulilo</ref>,
      <ref cel="flam.0ingo">flamingo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Brenner</trd>,
    <trd>Brennd&uuml;se</trd>
  </trdgrp>
  <trd lng="fr">bec de gaz</trd>
  <trd lng="nl">gasbekken</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_a;&c_ja;
  &c_g;&c_o;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="gas.0mufo">
  <kap><tld/>mufo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de &ccirc;apeto, teksita el asbesto, kiun oni metas sur
      <tld/>bekon por pliintensigi la lumon de la flamo, kiu igas
      &gcirc;in ardanta.
    </dif>
    <ref tip="sin" cel="ard.0ilo">ardilo.</ref>
  </snc>
  <trd lng="en">mantle</trd>
  <trd lng="fr">manchon<klr tip="ind">(de flamme)</klr></trd>
  <trd lng="nl">gasmof</trd>
</drv>

<drv mrk="gas.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="gas.0ujo.gxenerale">
    <dif>
      Ajna ujo, destinita por teni <tld/>on, kaj pli speciale:
    </dif>
    <subsnc mrk="gas.0ujo.gasbotelo">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        metala ujo, destinita por teni de kelkcent &gcirc;is kelkmil
        gramojn da <ref cel="prem.kun0i.FIZ">kunpremita</ref> <tld/>o, ofte
        brul<tld/>o;
      </dif>
      <ref tip="sin" cel="gas.0botelo"><tld/>botelo.</ref>
      <trd lng="fr">bouteille de gaz</trd>
      <trdgrp lng="nl">
        <trd>gasfles</trd>,
        <trd>gastank</trd>
      </trdgrp>
      <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;
      &c_b;&c_a;&c_l;&c_l;&c_o;&c_n;</trd>
    </subsnc>
    <subsnc mrk="gas.0ujo.granda_rezervujo">
      <fnt><bib>PV</bib></fnt>
      <dif>
        granda rezervujo por brul<tld/>o &ccirc;e la fabriko.
      </dif>
      <refgrp tip="vid">
        Komparu kun:
        <ref cel="gasome.0o">gasometro</ref>.
      </refgrp>
      <adm>
      Cxu la precizigo "cxe la fabriko" estas esenca kaj suficxe klara?
      Kia alia diferenco ekzistas inter ~ujo kaj
      &leftquot;gasometro&rightquot;?
      <aut>MB</aut>
      </adm>
      <trd lng="nl">gashouder</trd>
    </subsnc>
    <trd lng="fr">r&eacute;servoir de gaz</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Gasbeh&auml;lter</trd>,
    <trd>Gastank</trd>
  </trdgrp>
</drv>

<drv mrk="gas.0botelo">
  <kap><tld/>botelo</kap>
  <snc>
    <ref tip="dif" cel="gas.0ujo.gasbotelo"><tld/>ujo <sncref
    ref="gas.0ujo.gasbotelo"/>.</ref>
  </snc>
  <trd lng="fr">bouteille de gaz</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;
  &c_b;&c_a;&c_l;&c_l;&c_o;&c_n;</trd>
</drv>

<drv mrk="gas.0mezurilo">
  <kap><tld/>mezurilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Aparato por reguligi la fluon kaj registri la kvanton de la
      distribuita brul<tld/>o.
    </dif>
  </snc>
  <trd lng="fr">compteur &agrave; gaz</trd>
  <trd lng="nl">gasmeter</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;
  &c_s;&c_ch;&c_jo;&c_t;&c_ch;&c_i;&c_k;</trd>
</drv>

<drv mrk="gas.min0o">
  <kap>min<tld/>o</kap>
  <snc>
    <dif>
      Flami&gcirc;ema kaj eksplodema <tld/>o en la karbominejoj, miksa&jcirc;o
      el aero kaj <ref cel="metan.0o">metano</ref>.
    </dif>
  </snc>
  <trd lng="de">Grubengas</trd>
  <trd lng="fr">grisou</trd>
  <trd lng="nl">mijngas</trd>
  <trd lng="ru">&c_r;&c_u;&c_d;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j;
  &c_g;&c_a;&c_z;</trd>
</drv>

<drv mrk="gas.ter0o">
  <kap>ter<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>o, konsistanta &ccirc;efe el metano, eli&gcirc;anta el
      teraj
      fontoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Erdgas</trd>,
    <trd>Naturgas</trd>
  </trdgrp>
  <trd lng="en">natural <ind>gas</ind></trd>
  <trd lng="fr">gaz naturel</trd>
  <trd lng="nl">aardgas</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;
  &c_g;&c_a;&c_z;</trd>
</drv>

<drv mrk="gas.nobla0o">
  <kap>nobla <tld/>o</kap>
  <snc>
    <dif>
      Inerta <tld/>o, kies molekuloj ne povas kombini&gcirc;i kun aliaj
      molekuloj.
    </dif>
  </snc>
  <trd lng="de">Edelgas</trd>
  <trdgrp lng="en">
    <trd>inert <ind>gas</ind></trd>,
    <trd>noble <ind>gas</ind></trd>
  </trdgrp>
  <trd lng="fr">gaz rare<klr tip="ind"> (gaz inerte, gaz noble)</klr></trd>
  <trd lng="nl">edelgas</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;
    &c_g;&c_a;&c_z;</trd>,
    <trd>&c_i;&c_n;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j; &c_g;&c_a;&c_z;</trd>
  </trdgrp>
</drv>
</art>
