  <trdgrp lng="ja">
    <trd>&#x89AA;&#x985E;  <pr>&#12375;&#12435;&#x308B;&#12356;</pr></trd>,
    <trd>&#x89AA;&#x65CF;  <pr>&#12358;&#x304B;&#12425;</pr></trd>,
    <trd>&#x89AA;&#x621A;  <pr>&#12375;&#12435;&#x305B;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="ka">&#x10DC;&#x10D0;&#x10D7;&#x10D4;&#x10E1;&#x10D0;&#x10D5;&#x10D8;</trd>
  <trd lng="lt">giminaitis</trd>
  <trdgrp lng="nl">
    <trd>familielid</trd>,
    <trd>lid van de <ind>familie</ind></trd>
  </trdgrp>
  <trd lng="pl">krewny</trd>
  <trd lng="pt">parente</trd>
  <trd lng="ro">rud&abreve;</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;buzn&yacute;</trd>,
    <trd>rodina</trd>
  </trdgrp>
  <trd lng="sv">sl&auml;kting</trd>
  <trd lng="te">&#xC2C;&#xC02;&#xC27;&#xC41;&#xC35;&#xC41;</trd>
  <trd lng="tr">akraba</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_d;&c_i;&c_ch;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; (&c_ib;&c_m;.)</trd>
  </trdgrp>
