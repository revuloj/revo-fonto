<drv mrk="parenc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="GEN"/>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="parenc.0o.individuo">
    <dif>
      <klr>(pp individuo)</klr>
      Iu apartenanta al la sama familio kiel la koncernata individuo,
      a&ubreve; havanta komunan anta&ubreve;ulon kun &gcirc;i:
      <ekz>
        <nom>Laban</nom> diris al <nom>Jakob</nom>: &ccirc;u pro tio, ke vi estas mia <tld/>o, vi devas
        servi min
        senpage
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:15</lok>
        </fnt>?
      </ekz>
      <ekz>
        &scirc;i mem solene anoncis al &ccirc;iuj siaj <tld/>oj kaj konatoj, ke &scirc;i jam estas fian&ccirc;ino
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve;e li preska&ubreve; nenion konis pri tiu popolo, sed nun li iamaniere eksciis, ke
        ili estas <tld/>oj de la francoj, sekve <tld/>oj de <nom>d&#8217;Artagnan</nom>
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun &ccirc;i bienon a&ccirc;etis unu mia malproksima <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli bona amiko intima ol <tld/>o malproksima
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        malri&ccirc;ulo <tld/>ojn ne havas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o per Adamo
        <klr>(treege malproksima, nur fantazia)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:parencoj"><tld/>oj</lstref>
    <refgrp tip="sub">
      <ref cel="kuz.0o">kuzo</ref>,
      <ref cel="patr.ge0oj">gepatroj</ref>
    </refgrp>
  </snc>
  <snc mrk="parenc.0o.boparenco">
    <dif>
      <ref tip="dif" cel="parenc.bo0o">Bo<tld/>o</ref>
      <ekz>
        <nom>Naomi</nom> havis <tld/>on de &scirc;ia edzo, viron eminentan
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:1</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <snc mrk="parenc.0o.samdevena">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(pp pri grupo a&ubreve; strukturo de interdependaj
      aferoj)</klr> Iu a&ubreve; io kun sama deveno, de sama kategorio:
      <ekz>
        parencoj de tukanoj <klr>(devenante de sama pratribo kiel
        tukanoj)</klr>
        <fnt>
          <vrk><url ref="&Viki;/Buntaj_barbuloj"/></vrk>,
          <bib>Wiki</bib>,
          <lok>Buntaj barbuloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_ja;&c_k;</trd>
  <trd lng="bo">&t_sa;&t_pata;&t_shabchu;&t_na;&t_tsheg;&t_sa;&t_kata;&t_yata;&t_tsheg;</trd>
  <trd lng="br">kar<klr tip="ind"> (ak.)</klr></trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;buzn&yacute;</trd>,
    <trd>p&rcaron;&iacute;buzn&yacute; <klr>(osoba)</klr></trd>
  </trdgrp>
  <trd lng="cy">perthynas</trd>
  <trd lng="de">Verwandter</trd>
  <trd lng="el">&sigma;&ypsilon;&gamma;&gamma;&epsilon;&ny;&eta_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>relative</trd>,
    <trd>relation</trd>,
    <trd>kinsman</trd>
  </trdgrp>
  <trd lng="es">pariente</trd>
  <trd lng="fr">parent<klr> (subst., personne
  apparent&eacute;e)</klr></trd>
  <trd lng="fy">famyljelid</trd>
