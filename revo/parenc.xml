<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parenc.xml,v 1.35 2022/06/18 22:28:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>parenc</rad>/o
</kap>
<drv mrk="parenc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="GEN"/>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="parenc.0o.individuo">
    <dif>
      <klr>(pp individuo)</klr>
      Iu apartenanta al la sama familio kiel la koncernata individuo,
      a&ubreve;
      havanta komunan anta&ubreve;ulon kun &gcirc;i:
      <ekz>
        pli bona amiko intima ol <tld/>o malproksima
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o per Adamo
        <klr>(treege malproksima, nur fantazia)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        personoj starantaj en la sama linio de <tld/>eco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:parencoj"><tld/>oj</lstref>   
    <refgrp tip="sub">
      <ref cel="kuz.0o">kuzo</ref>,
      <ref cel="patr.ge0oj">gepatroj</ref>.
    </refgrp>
  </snc>
  <snc mrk="parenc.0o.boparenco">
    <ref tip="dif" cel="parenc.bo0o">bo<tld/>o</ref>
  </snc>
  <snc mrk="parenc.0o.samdevena">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(pp pri grupo a&ubreve; strukturo de interdependaj
      aferoj)</klr>
      Iu a&ubreve; io kun sama deveno, de sama kategorio:
      <ekz>
        parencoj de tukanoj <klr>(devenante de sama pratribo kiel
        tukanoj)</klr>
        <fnt>
          <vrk><url ref="&Viki;/Buntaj_barbuloj"/></vrk>,
          <bib>Wiki</bib>,
          <lok>Buntaj barbuloj</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_ja;&c_k;</trd>
  <trd lng="bo">&#xF66;&#xFA4;&#xF74;&#xF53;&#xF0B;&#xF66;&#xF90;&#xFB1;&#xF0B;</trd>
  <trd lng="br">kar<klr tip="ind"> (ak.)</klr></trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;buzn&yacute;</trd>,
    <trd>p&rcaron;&iacute;buzn&yacute; <klr>(osoba)</klr></trd>
  </trdgrp>
  <trd lng="cy">perthynas</trd>
  <trd lng="de">Verwandter</trd>
  <trd lng="el">&sigma;&ypsilon;&gamma;&gamma;&epsilon;&ny;&eta_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>relative</trd>,
    <trd>relation</trd>,
    <trd>kinsman</trd>
  </trdgrp>
  <trd lng="es">pariente</trd>
  <trd lng="fr">parent<klr> (subst., personne
  apparent&eacute;e)</klr></trd>
  <trd lng="fy">famyljelid</trd>
  <trd lng="hu">rokon</trd>
  <trd lng="io">parento</trd>
  <trd lng="it">parente</trd>
  <trd lng="ka">&#x10DC;&#x10D0;&#x10D7;&#x10D4;&#x10E1;&#x10D0;&#x10D5;&#x10D8;</trd>
  <trd lng="lt">giminaitis</trd>
  <trdgrp lng="nl">
    <trd>familielid</trd>,
    <trd>lid van de <ind>familie</ind></trd>
  </trdgrp>
  <trd lng="pl">krewny</trd>
  <trd lng="pt">parente</trd>
  <trd lng="ro">rud&abreve;</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;buzn&yacute;</trd>,
    <trd>rodina</trd>
  </trdgrp>
  <trd lng="sv">sl&auml;kting</trd>
  <trd lng="te">&#xC2C;&#xC02;&#xC27;&#xC41;&#xC35;&#xC41;</trd>
  <trd lng="tr">akraba</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_d;&c_i;&c_ch;</trd>,
    <trd>&#x43A;&c_r;&c_je;&c_v;&#x43D;&c_i;&c_j; (&c_ib;&#x43C;.)</trd>
  </trdgrp>
  <trd lng="vo">r&ouml;letan</trd>
</drv>

<drv mrk="parenc.0a">
  <kap><tld/>a</kap>
  <snc mrk="parenc.0a.propre">
    <dif>
      <klr>(pp pri individuo)</klr> Apartenanta al la sama familio
      a&ubreve;
      <tld/>aro:
      <ekz>
        &ccirc;i tie la arboj &scirc;ajnas al ni <tld/>aj
         <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>aj rilatoj</ind>.
        <trd lng="hu">rokoni kapcsolatok</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_v;&c_a;&c_ja;&c_c;&c_k;&c_ib;</trd>,
      <trd>&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="hu">rokoni</trd>
  </snc>
  <snc mrk="parenc.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Samdevena a&ubreve; samkategoria:
      <ekz>
        <tld/>aj lingvoj, organizoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analog.0a">analoga</ref>,
      <ref cel="najbar.0a">najbara</ref>,
      <ref cel="proksi.0a">proksima</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_d;&c_n;&c_a;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_ib;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">kar<klr tip="ind"> (ag.)</klr></trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;buzn&yacute;</trd>,
    <trd>sp&rcaron;&iacute;zn&ecaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">related</trd>
  <trdgrp lng="fr">
    <trd>apparent&eacute;</trd>,
    <trd>parent<klr> (adj., apparent&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">rokon-</trd>
  <trd lng="nl">verwant</trd>
  <trdgrp lng="pl">
    <trd>pokrewny</trd>,
    <trd>spokrewniony</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>parente<klr tip="ind"> (adj.)</klr></trd>,
   <trd>aparentado<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>conexe</trd>,
    <trd>aproapele</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">pr&iacute;buzn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_d;&c_i;&#x43D;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="parenc.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Ina parenco:
      <ekz>
        &scirc;i <klr>...</klr> estas e&ccirc; via proksima <tld/>ino...
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        tio estas mia <tld/>ino kaj servistino, Ester &mdash; respondis
        Sara
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        kaj jen via <tld/>ino Elizabeto anka&ubreve; gravedi&gcirc;is je
        filo
        en sia maljuneco
        <fnt><bib>NT</bib>, <lok>&Luk; 1:36</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_ja;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Verwandte</trd>
  <trd lng="hu">n&odblac;rokon</trd>
  <trd lng="io">parentino</trd>
  <trd lng="nl">familielid<klr tip="ind"> (vr.)</klr></trd>
  <trd lng="pl">krewna</trd>
  <trd lng="ro">rud&abreve;</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&c_r;&#x43E;&c_d;&c_i;&c_ch;&#x43A;&c_a;</trd>
  <trd lng="vo">jir&ouml;letan</trd>
</drv>

<drv mrk="parenc.bo0o">
  <kap><ofc>*</ofc>bo<tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <tld lit="P"/>o de unu el geedzoj rilate al la <tld/>aro
      de la alia:
      <ekz>
        &ccirc;iuj <tld/>oj de mia edzino estas miaj bo<tld/>oj
        <fnt><bib>F</bib>, <lok>&FE;36.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_ja;&c_k; <klr>(&c_n;&c_ja;&c_k;&c_r;&c_e;&c_w;&c_n;&c_y;)</klr></trd>
  <trd lng="cs">p&rcaron;&iacute;buzn&yacute; z&iacute;skan&yacute;
  s&ncaron;atkem</trd>
  <trd lng="en">in-law</trd>
  <trd lng="hu">rokon <klr tip="amb">(h&aacute;zass&aacute;g
  r&eacute;v&eacute;n)</klr></trd>
  <trdgrp lng="nl">
    <trd>schoonfamilielid</trd>,
    <trd>lid van de <ind>schoonfamilie</ind></trd>
  </trdgrp>
  <trd lng="pl">powinowaty</trd>
  <trdgrp lng="pt">
    <trd>parente afim</trd>,
    <trd>afim</trd>,
    <trd>parente por <ind>afinidade</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>rela&#539;ionat</trd>,
    <trd>inrudit</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">pr&iacute;buzn&yacute; z&iacute;skan&yacute;
  man&zcaron;elstvom</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_d;&c_i;&c_ch; &c_ch;&c_je;&c_r;&c_je;&c_z; &c_sh;&#x43B;&#x44E;&c_b;</trd>,
    <trd>&c_s;&c_v;&#x43E;&#x44F;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
