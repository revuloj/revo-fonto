    <trd>&#x8FD1;&#x89AA; <pr>j&#xEC;nq&#x12B;n</pr></trd>,
    <trd>&#x540C;&#x5B97; <pr>t&#xF3;ngz&#x14D;ng</pr></trd>,
    <trd>&#x4EB2;&#x621A; <pr>q&#x12B;nq&#x12B;</pr></trd>,
    <trd>&#x89AA;&#x621A; <pr>q&#x12B;nq&#x12B;</pr></trd>,
    <trd>&#x621A;&#x5C5E; <pr>q&#x12B;sh&#x1D4;</pr></trd>,
    <trd>&#x621A;&#x5C6C; <pr>q&#x12B;sh&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="parenc.0a">
  <kap><tld/>a</kap>
  <snc mrk="parenc.0a.propre">
    <dif>
      <klr>(pp pri individuo)</klr> Apartenanta al la sama familio
      a&ubreve;
      <tld/>aro:
      <ekz>
        ili estas proksime <tld/>aj al ni kaj ofte faras al ni vizitojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        dank&#8217; al la <tld/>aj ligiloj, kiuj nin kunigas, mi povos &scirc;in oftege vizitadi en
        intimeco de familio
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie la arboj &scirc;ajnas al ni <tld/>aj
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>aj rilatoj</ind>.
        <trd lng="hu">rokoni kapcsolatok</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_v;&c_a;&c_ja;&c_c;&c_k;&c_ib;</trd>,
      <trd>&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="hu">rokoni</trd>
  </snc>
  <snc mrk="parenc.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="ven.samde0a">Samdevena</ref> a&ubreve; samkategoria:
      <ekz>
        amba&ubreve; ideoj estas tre <tld/>aj inter si, sed tute ne identaj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        forlasante la hejmon, lo&gcirc;ejon, &ccirc;ambron, kie oni lo&gcirc;is multajn jarojn kaj kie &ccirc;io
        estas tiom <tld/>a kaj kara
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiumomente fari&gcirc;is <nom>Olga</nom> proksima, <tld/>a kaj komprenebla, kiel iam
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        amikigi du <tld/>ajn popolojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu speco <tld/>aj estas ekzemple la lingvoj franca, itala, hispana,
        portugala kaj rumana, &ccirc;ar &ccirc;iuj evoluis el la latina
        <fnt>
          <bib>LAE</bib>
          <lok>1. Enkonduko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kukurboj, <klr>[tiuj]</klr> globoformaj fruktoj <klr>[&#8230;]</klr> botanike estas <tld/>aj al
        la beroj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2008/009386.php">La kukurbo frukto
          multviza&gcirc;a</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        pretervide la pezon en movoj kaj manieroj, lia aspekto estis iom <tld/>a al tiu
        de la muskedistoj en la romanoj
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgracio kaj misritmo kaj malharmonio estas <tld/>aj al fiparolado kaj fikonduto
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Tri</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analog.0a">analoga</ref>,
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="kutim.0a.io">kutima<sncref/></ref>,
      <ref cel="najbar.0a">najbara</ref>,
      <ref cel="proksi.0a">proksima</ref>,
      <ref cel="simil.0a">simila</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_d;&c_n;&c_a;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_ib;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">kar<klr tip="ind"> (ag.)</klr></trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;buzn&yacute;</trd>,
    <trd>sp&rcaron;&iacute;zn&ecaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">related</trd>
  <trdgrp lng="fr">
    <trd>apparent&eacute;</trd>,
    <trd>parent<klr> (adj., apparent&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">rokon-</trd>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x65CF;&#x306E;  <pr>&#12375;&#12435;&#x305E;&#x304F;&#x306E;</pr></trd>,
    <trd>&#34880;&#x7E01;&#x306E;  <pr>&#12369;&#12388;&#12360;&#12435;&#x306E;</pr></trd>,
    <trd>&#x540C;&#x65CF;&#x306E;  <pr>&#12393;&#12358;&#x305E;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x540C;&#x985E;&#x306E;  <pr>&#12393;&#12358;&#x308B;&#12356;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">verwant</trd>
  <trdgrp lng="pl">
    <trd>pokrewny</trd>,
    <trd>spokrewniony</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>parente<klr tip="ind"> (adj.)</klr></trd>,
   <trd>aparentado<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>conexe</trd>,
    <trd>aproapele</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">pr&iacute;buzn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_i;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D27;&#x5BC6;&#x76F8;&#32852; <pr>j&#x1D0;nm&#xEC;xi&amacron;ngli&#xE1;n</pr></trd>,
    <trd>&#x7DCA;&#x5BC6;&#x76F8;&#x806F; <pr>j&#x1D0;nm&#xEC;xi&amacron;ngli&#xE1;n</pr></trd>,
    <trd>&#x621A; <pr>q&#x12B;</pr></trd>,
    <trd>&#x652F;&#x4E0E;&#x6D41;&#x88D4; <pr>zh&#x12B;y&#x1D4;li&#xFA;y&#xEC;</pr></trd>,
    <trd>&#x652F;&#33287;&#x6D41;&#x88D4; <pr>zh&#x12B;y&#x1D4;li&#xFA;y&#xEC;</pr></trd>,
    <trd>&#x4EB2; <pr>q&#x12B;n</pr></trd>,
    <trd>&#x89AA; <pr>q&#x12B;n</pr></trd>,
    <trd>&#x4EB2;&#x5C5E; <pr>q&#x12B;nsh&#x1D4;</pr></trd>,
    <trd>&#x89AA;&#x5C6C; <pr>q&#x12B;nsh&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="parenc.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc>
    <dif>
      La <ref tip="super" cel="rilat.0o.interpersona">rilato</ref> inter <tld/>oj:
      <ekz>
        ili tute ne estis en ia <tld/>eco inter si
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la senkompara sa&gcirc;ulo <klr>[&#8230;]</klr> estas mia avo, mi fieras pri &ccirc;i tiu <tld/>eco
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        personoj starantaj en la sama linio de <tld/>eco
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_v;&c_a;&c_ja;&c_c;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="parenc.0eco.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Proksima rilato inter du aferoj:
      <ekz>
        mi ekkonis mian internan naturon, la <tld/>econ kiun mi havas kun la poezio
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la esperantismo kaj la homaranismo ekzistas nenia <tld/>eco
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiu spirita <tld/>eco inter mi kaj vi estas la&ubreve;vola
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        inter la amba&ubreve; ideoj ekzistas <tld/>eco tre granda
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0o">akordo</ref>,
      <ref cel="ident.0eco.identigho">identeco<sncref/></ref>,
      <ref cel="simil.0eco">simileco</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_d;&c_n;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Verwandtschaft</trd>
  <trdgrp lng="ja">
    <trd>&#34880;&#x7E01;  <pr>&#12369;&#12388;&#12360;&#12435;</pr></trd>,
    <trd>&#x89AA;&#x985E;&#x95A2;&#x4FC2;  <pr>&#12375;&#12435;&#x308B;&#12356;&#x304B;&#12435;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">pokrewie&nacute;stwo</trd>
  <trdgrp lng="zh">
    <trd>&#34880;&#x7DE3;&#x95DC;&#x4FC2; <pr>xi&#x11B;yu&#xE1;ngu&amacron;nxi</pr></trd>,
    <trd>&#34880;&#x7F18;&#20851;&#x7CFB; <pr>xi&#x11B;yu&#xE1;ngu&amacron;nxi</pr></trd>,
    <trd>&#x4EB2;&#x5C5E; <pr>q&#x12B;nsh&#x1D4;</pr></trd>,
    <trd>&#x89AA;&#x5C6C; <pr>q&#x12B;nsh&#x1D4;</pr></trd>,
    <trd>&#x540C;&#x5B97; <pr>t&#xF3;ngz&#x14D;ng</pr></trd>,
    <trd>&#x540C;&#x65CF; <pr>t&#xF3;ngz&#xFA;</pr></trd>,
    <trd>&#26377;&#34880;&#x7DE3;&#x95DC;&#x4FC2; <pr>y&#x1D2;uxi&#x11B;yu&#xE1;ngu&amacron;nxi</pr></trd>,
    <trd>&#26377;&#34880;&#x7F18;&#20851;&#x7CFB; <pr>y&#x1D2;uxi&#x11B;yu&#xE1;ngu&amacron;nxi</pr></trd>,
    <trd>&#26377;&#20851;&#x7CFB; <pr>y&#x1D2;ugu&amacron;nxi</pr></trd>,
    <trd>&#26377;&#x95DC;&#x4FC2; <pr>y&#x1D2;ugu&amacron;nxi</pr></trd>,
    <trd>&#x4EB2;&#x7F18; <pr>q&#x12B;nyu&#xE1;n</pr></trd>,
    <trd>&#x89AA;&#x7DE3; <pr>q&#x12B;nyu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="parenc.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Ina parenco:
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> estas e&ccirc; via proksima <tld/>ino...
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        tio estas mia <tld/>ino kaj servistino, <nom>Ester</nom> &mdash; respondis
        Sara
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        kaj jen via <tld/>ino Elizabeto anka&ubreve; gravedi&gcirc;is je
        filo
        en sia maljuneco
        <fnt><bib>NT</bib>, <lok>&Luk; 1:36</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_ja;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Verwandte</trd>
  <trd lng="hu">n&odblac;rokon</trd>
  <trd lng="io">parentino</trd>
  <trd lng="nl">familielid<klr tip="ind"> (vr.)</klr></trd>
  <trd lng="pl">krewna</trd>
