      Ligna ebura a&ubreve; metala kruco, sur kiu estas figurata Jesuo
      krucumita:
      <ekz>
        montrante lian portreton sub sia <tld/>o
        <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 12a, p. 129a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">crucifix</trd>
  <trd lng="cs">obraz uk&rcaron;i&zcaron;ovan&eacute;ho</trd>
  <trd lng="de">Kruzifix</trd>
  <trd lng="en">crucifix</trd>
  <trd lng="es">crucifijo</trd>
  <trdgrp lng="fr">
    <trd>croix<klr tip="ind"> (crucifix)</klr></trd>,
    <trd>crucifix</trd>
  </trdgrp>
  <trd lng="hu">fesz&uuml;let</trd>
  <trd lng="ja">&#x5341;&#x5B57;&#x67B6;&#x50CF;  [<ind>&#x3058;&#x3085;&#x3046;&#x3058;&#x304B;&#x305E;&#x3046;</ind>]</trd>
  <trd lng="nl">kruisbeeld</trd>
  <trd lng="pl">krucyfiks</trd>
  <trd lng="pt">crucifixo</trd>
  <trd lng="ro">crucifix</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_ja;&c_t;&c_i;&c_je; <klr>(&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">kr&iacute;&zcaron; s&nbsp;podobou ukri&zcaron;ovan&eacute;ho Krista</trd>
  <trd lng="sv">krucifix</trd>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x43F;&#x2019;&#x44F;&#x442;&#x442;&#x44F; (&#x444;&#x456;&#x491;&#x443;&#x440;&#x43D;&#x435; &#x437;&#x43E;&#x431;&#x440;&#x430;&#x436;&#x435;&#x43D;&#x43D;&#x44F;)</trd>
