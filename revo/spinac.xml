<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spinac</rad>/o
</kap>
<drv mrk="spinac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="kenopo1.0oj">kenopodiacoj</ref>
      (<trd lng="la">Spinacia</trd>), la foliojn de unu specio
      (<trd lng="la">Spinacia oleracea</trd>) oni man&gcirc;as
      kiel legomo.
    </dif>
  </snc>
  <trd lng="de">Spinat</trd>
  <trd lng="fr">&eacute;pinard</trd>
  <trdgrp lng="hu">
    <trd>spen&oacute;t</trd>,
    <trd>paraj</trd>
  </trdgrp>
  <trd lng="nl">spinazie</trd>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_n;&c_a;&c_t;</trd>
  <trd lng="tr">&inodot;spanak</trd>
</drv>
</art>
