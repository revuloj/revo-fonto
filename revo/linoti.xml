<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>linotip</rad>/o
</kap>
<drv mrk="linoti.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Ma&scirc;ino por perlinia kompostado kaj fandado de presliteroj,
      iam uzita en la presarto.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_a;&c_t;&c_y;&c_p;</trd>
  <trd lng="de">Linotype-Setzmaschine</trd>
  <trdgrp lng="es">
    <trd>linotipia</trd>,
    <trd>linotipo</trd>
  </trdgrp>
  <trd lng="nl">regelzetmachine</trd>
  <trd lng="pt">linotipo</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_o;&c_t;&c_i;&c_p;</trd>
</drv>

<drv mrk="linoti.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Tiu, kiu kompostas sur <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_s;&c_t;</trd>
  <trd lng="de">Linotype-Setzer</trd>
  <trd lng="es">linotipista</trd>
  <trd lng="nl">regelzetter</trd>
  <trd lng="pt">linotipista</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_o;&c_t;&c_i;&c_p;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
