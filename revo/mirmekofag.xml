<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mirmekofag.xml,v 1.1 2025/02/25 08:33:13 afido Exp $">
    <kap>
      <rad>mirmekofag</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="mirmekofag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de formik- kaj termitman&gcirc;aj mamuloj (<trd lng="la">Myrmecophaga</trd>),
      vivanta en Sud- kaj Centrameriko:
      <ekz>
        per la akraj ungoj la <tld/>o skrapas la grundon kaj per la lango &gcirc;i kaptas
        formikojn en la galerioj de formikejo
        <fnt>
          <aut>Pa&ubreve;lo Sergio Viana</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0405/0007.php"
              >Niaj Kunuloj, la Bestoj</url></vrk>
          <lok>La Soleca Mirmekofago</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mangx.formik0ulo">formikman&gcirc;ulo</ref>
    <bld
      lok="&WCU;/thumb/c/c4/Myrmecophaga_tridactyla%2C_Pantanal_region%2C_Brazil.jpg/640px-Myrmecophaga_tridactyla%2C_Pantanal_region%2C_Brazil.jpg"
      prm="CC BY 3.0"
      ><tld/>o (Myrmecophaga tridactyla), Brazilo
        <fnt>
          <aut>Anagoria, propra verko</aut>
          <lok><url ref="&WCW;/File:2010-11-14_IMG_3932.JPG"
              >https://commons.wikimedia.org/wiki/File:2010-11-14_IMG_3932.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_m;&c_u;&c_r;&c_a;&c_sh;&c_k;&c_a;&c_je;&c_d;</trd>
    <trdgrp lng="de">
      <trd>Ameisenfresser</trd>,
      <trd>Ameisenb&auml;r</trd>
    </trdgrp>
    <trd lng="en">anteater</trd>
    <trd lng="fr">fourmilier</trd>
    <trdgrp lng="id">
      <trd><baz>makan</baz>pemakan semut</trd>,
      <trd>sarawaboga</trd>
    </trdgrp>
    <trd lng="nl">miereneter</trd>
    <trd lng="pl">mr&oacute;wkojad</trd>
    <trd lng="pt">tamandu&aacute;</trd>
    <trd lng="ro">furnicar</trd>
    <trd lng="ru">&c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_je;&c_d;</trd>
    <trdgrp lng="zh">
      <trd>&#x98DF;&#34433;&#x517D; <pr>sh&#xED;y&#x1D0;sh&#xF2;u</pr></trd>,
      <trd>&#x98DF;&#x87FB;&#29560; <pr>sh&#xED;y&#x1D0;sh&#xF2;u</pr></trd>
    </trdgrp>
  </snc>
</drv>

  </art>

