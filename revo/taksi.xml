<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>taksi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="taksi.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="taksi.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Privata <ref tip="super" cel="auxt.0o">a&ubreve;to</ref> kun
      &scirc;oforo, je dispono de la publiko, por veturigi klientojn
      kontra&ubreve; pago:
      <ekz>
        preni <klr>(uzi)</klr> <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiakr.0o">fiakro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Taxi</trd>,
    <trd>Droschke<klr> [ark.]</klr></trd>
  </trdgrp>
  <trd lng="en">taxi</trd>
  <trd lng="fa">&a_t;&a_A;&f_k;&a_s;&f_y;</trd>
  <trd lng="fr">taxi<klr tip="ind"> (v&eacute;hicule)</klr></trd>
  <trdgrp lng="he">
    <trd>&mem;&vav;&nun;&yod;&tav;</trd>,
    <trd>&tet;&qof;&samekh;&yod;<klr tip="ind">
    (&mem;&vav;&nun;&yod;&tav;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>taxi</trd>,
    <trd>b&eacute;rkocsi</trd>
  </trdgrp>
  <trdgrp lng="it"><trd>taxi</trd>, <trd>tass&igrave;</trd></trdgrp>
  <trd lng="nl">taxi</trd>
  <trdgrp lng="pl">
    <trd>taks&oacute;wka</trd>,
    <trd>taxi</trd>,
    <trd>taryfa<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>t&aacute;xi</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_i;</trd>
  <trd lng="sv">taxi</trd>
</drv>

<drv mrk="taksi.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="taksi.0ejo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Loko, kie <tld/>oj haltas, atendante veturontojn:
      <ekz>
        mi pa&scirc;is <klr>...</klr> eksteren kune kun la alvenintaj
        pasa&gcirc;eroj, al la <tld/>ejo
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">taxi stand</trd>
  <trd lng="fa">&a_A;&f_y;&a_s;&a_t;&f_g;&a_A;&a_h; &a_t;&a_A;&f_k;&a_s;&f_y;</trd>
  <trd lng="fr">station de taxis</trd>
  <trd lng="it"><ind>stazione</ind> dei taxi</trd>
  <trd lng="pl">post&oacute;j <ind>taks&oacute;wek</ind></trd>
  <trdgrp lng="pt">
   <trd>ponto de t&aacute;xi</trd>
  </trdgrp>
</drv>

<drv mrk="taksi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="taksi.0isto.AUT">
    <dif>
      Homo, kiu profesie &scirc;oforas <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_ib;&c_s;&c_t;</trd>
  <trd lng="en">taxi driver</trd>
  <trd lng="fa">&a_r;&a_A;&a_n;&a_n;&a_d;&a_h;&a_hamza_sure; &a_t;&a_A;&f_k;&a_s;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>chauffeur de taxi</trd>,
    <trd>taxi<klr tip="ind"> (chauffeur)</klr></trd>
  </trdgrp>
  <trd lng="he">&nun;&he;&gimel; &mem;&vav;&nun;&yod;&tav;</trd>
  <trdgrp lng="hu">
    <trd>taxis</trd>,
    <trd>taxisof&odblac;r</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tassista</trd>,
    <trd>taxista</trd>
  </trdgrp>
  <trd lng="nl">taxichauffeur</trd>
  <trdgrp lng="pl">
    <trd>taks&oacute;wkarz</trd>,
    <trd>taryfiarz<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>taxista</trd>,
   <trd>motorista de t&aacute;xi</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="taksi.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="taksi.akvo0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Boato a&ubreve; &scirc;ipeto uzata kiel <tld/>o:
      <ekz>
        &ccirc;e la &scirc;tonmoleo mi ekvidis &ccirc;irka&ubreve; cent motorajn
        akvo<tld/>ojn
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">bateau taxi</trd>
</drv>
</art>
