<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>taksi</rad>/o
</kap>
<drv mrk="taksi.0o">
  <kap><tld/>o</kap>
  <snc mrk="taksi.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Privata <ref tip="super" cel="auxt.0o">a&ubreve;to</ref> kun &scirc;oforo,
      je dispono de la publiko, por veturigi klientojn kontra&ubreve; pago:
      <ekz>
        preni <klr>(uzi)</klr> <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiakr.0o">fiakro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Taxi</trd>,
    <trd>Droschke<klr> [ark.]</klr></trd>
  </trdgrp>
  <trd lng="fr">taxi<klr tip="ind"> (v&eacute;hicule)</klr></trd>
  <trdgrp lng="he">
    <trd>&mem;&vav;&nun;&yod;&tav;</trd>,
    <trd>&tet;&qof;&samekh;&yod;<klr tip="ind"> (&mem;&vav;&nun;&yod;&tav;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>taxi</trd>,
    <trd>b&eacute;rkocsi</trd>
  </trdgrp>
  <trd lng="nl">taxi</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_i;</trd>
  <trd lng="sv">taxi</trd>
</drv>

<drv mrk="taksi.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="taksi.0ejo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Loko, kie <tld/>oj haltas, atendante veturontojn:
      <ekz>
        mi pa&scirc;is <klr>...</klr> eksteren kune kun la alvenintaj pasa&gcirc;eroj,
        al la <tld/>ejo
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">station de taxis</trd>
</drv>

<drv mrk="taksi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="taksi.0isto.AUT">
    <dif>
      Homo, kiu profesie &scirc;oforas <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="fr">
    <trd>chauffeur de taxi</trd>,
    <trd>taxi<klr tip="ind"> (chauffeur)</klr></trd>
  </trdgrp>
  <trd lng="he">&nun;&he;&gimel; &mem;&vav;&nun;&yod;&tav;</trd>
  <trdgrp lng="hu">
    <trd>taxis</trd>,
    <trd>taxisof&odblac;r</trd>
  </trdgrp>
  <trd lng="nl">taxichauffeur</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
