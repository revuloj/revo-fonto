<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: flis.xml,v 1.3 2024/11/24 19:24:09 afido Exp $">
    <kap>
      <rad>flis</rad>/o <fnt><bib>Viki</bib></fnt>
    </kap>

<drv mrk="flis.0o">
  <kap><tld/>o</kap>
  <snc mrk="flis.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Lansimila <ref tip="super" cel="teks.0ajxo">teksa&jcirc;o</ref> el poliesteraj fibroj:
      <ekz>
        <nom>Hanne Adler-Kruse</nom> komencis krom klasikaj pupoj dizajni aferojn el <ref
        cel="felp.0o">felpo</ref> a&ubreve; <tld/>o por beboj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=710376"
              >K&auml;the Kruse</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Fleece</trd>
  <trd lng="en">polar fleece</trd>
  <trd lng="fr">polaire<klr> (textile)</klr></trd>
  <trd lng="it">pile</trd>
  <trd lng="ru">&c_f;&c_l;&c_i;&c_s;</trd>
  <trd lng="uk">&c_f;&c_l;&c_ib;&c_s;</trd>
  <trd lng="be">&c_f;&c_l;&c_ib;&c_s;</trd>
</drv>
</art>

