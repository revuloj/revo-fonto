<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lazaret.xml,v 1.10 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>lazaret</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="lazaret.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lazaret.0o.MED">
    <uzo tip="fak">MED</uzo>
      <dif>
	Porokaza, malluksa <ref tip="super" 
	cel="san.mal0ulejo.MED">malsanulejo</ref>:
      </dif>
      <subsnc mrk="lazaret.0o.kontagxulejo">
	<dif>
	  Izolmalsanulejo por konta&gcirc;uloj:
	  <ekz>
	    oni starigis anka&ubreve; novan <tld/>on tute el kabanoj 
	    &ccirc;irka&ubreve;itan per simpla ligno-barilo,
	    ta&ubreve;gan enteni kvarmil personojn
	    <fnt><aut>A. Manzoni, trad. B. Cadei</aut>,
	    <vrk>La Gefian&ccirc;oj</vrk>, <lok>FEI, 2006</lok></fnt>.
	  </ekz>
	</dif>
        <trd lng="fa">&a_q;&a_r;&a_n;&a_T;&f_y;&a_n;&a_h;</trd>
      </subsnc>
      <subsnc mrk="lazaret.0o.MIL">
	<dif>
	  Militmalsanulejo por vunditoj:
	  <ekz>
	    li estas forsendita hejmen el <tld/>o por resani&gcirc;i
	    <fnt>
	      <aut>L. Lazarevic, trad. M. Nikolic</aut>,
	      <vrk>
	      <url ref="http://www.espernis.org.yu/popoloorumos.htm">
	      Popolo &Ccirc;ion Tion Orumos</url></vrk>,
	      <lok>2004</lok>
	    </fnt>.
	  </ekz>
	</dif>
        <trd lng="fa">&a_d;&a_r;&a_m;&a_A;&a_n;&f_g;&a_A;&a_h; &a_S;&a_H;&a_r;&a_A;&f_y;&f_y;</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_z;&c_a;&c_r;&c_e;&c_t;</trd>
  <trd lng="bg">&c_l;&c_a;&c_z;&c_a;&c_r;&c_je;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>lazaret</trd>,
    <trd>polní nemocnice</trd>
  </trdgrp>
  <trd lng="de">Lazarett</trd>
  <trdgrp lng="fr">
    <trd>h&ocirc;pital de campagne</trd>,
    <trd>lazaret</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lazarett</trd>,
    <trd>j&aacute;rv&aacute;nyk&oacute;rh&aacute;z</trd>,
    <trd>t&aacute;bori <ind>k&oacute;rh&aacute;z</ind></trd>
  </trdgrp>
  <trd lng="it">lazzaretto</trd>
  <trd lng="nl">lazaret</trd>
  <trd lng="pl">lazaret</trd>
  <trd lng="pt">lazareto</trd>
  <trdgrp lng="sk">
    <trd>dočasná nemocnica</trd>,
    <trd>lazaret</trd>
  </trdgrp>
</drv>
</art>

