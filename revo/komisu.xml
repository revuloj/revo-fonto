<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komisu.xml,v 1.20 2023/12/03 09:58:53 revo Exp $">
<kap>
  <rad>komisur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="komisu.0o">
  <kap><tld/>o</kap>
  <snc mrk="komisu.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Punkto, kie du similaj partoj kuni&gcirc;as:
      <ekz>
        <ind>lipa <tld/>o</ind> <klr>(<ref tip="sub" cel="angul.lip0o">lipangulo</ref>)</klr>;
        <trd lng="de">Mundwinkel</trd>
        <trd lng="fa">&f_g;&a_w;&a_sh;&a_h;&a_hamza_sure; &a_l;&a_b;</trd>
        <trd lng="nl">mondhoek</trd>
      </ekz>
      <ekz>
        <ind>palpebra <tld/>o</ind> <klr>(<ref tip="sub" cel="angul.okul0o">okulangulo</ref>)</klr>;
        <trd lng="de">Augenwinkel</trd>
        <trd lng="fa">&f_g;&a_w;&a_sh;&a_h;&a_hamza_sure; &f_ch;&a_sh;&a_m;</trd>
        <trd lng="fr">canthus</trd>
        <trd lng="it"><ind>canto</ind> oculare</trd>
        <trd lng="nl">ooghoek</trd>
      </ekz>
      <ekz>
        viro staranta sur sahara duno kun mallar&gcirc;aj <ind>okul<tld/>o</ind>j,
        kiuj klopodas vidi fore
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
          <lok>2015</lok>
        </fnt>.
        <trd lng="de">Augenwinkel</trd>
      </ekz>
    </dif>
    <trd lng="fr">commissure<klr tip="ind"> (anat.)</klr></trd>
    <trdgrp lng="hu">
      <trd>testzug</trd>,
      <trd>testsz&ouml;glet</trd>
    </trdgrp>
    <trd lng="pl">spoid&lstroke;o</trd>
    <trd lng="zh">&#x5634;&#x89D2; <pr>zu&#x1D0;ji&#x1CE;o</pr></trd>
</snc>
  <snc mrk="komisu.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kuni&gcirc;a punkto de du karpeloj.
    </dif>
    <trd lng="hu">term&odblac;lev&eacute;lzug</trd>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_v;&c_ja;&c_z;&c_a;&c_k; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;., &c_b;&c_a;&c_t;.)</klr></trd>
  <trd lng="cs">komisura</trd>
  <trd lng="fa">&f_p;&f_y;&a_w;&a_n;&a_d;&f_g;&a_A;&a_h;</trd>
  <trd lng="it">commessura</trd>
  <trdgrp lng="ja">
    <trd>&#x4EA4;&#x9023;  <pr>&#x3053;&#x3046;&#x308C;&#x3093;</pr></trd>,
    <trd>&#x63A5;&#x5408;&#x9762;  <pr>&#x305B;&#x3064;&#x3054;&#x3046;&#x304A;&#x3082;&#x3066;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>naad<klr tip="ind"> (commissuur)</klr></trd>,
    <trd>commissuur</trd>
  </trdgrp>
  <trd lng="pt">comissura</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">komis&uacute;ra</trd>
</drv>
</art>
