  <trd lng="pl">Brugia</trd>
  <trd lng="ru">&c_B;&c_r;&c_ju;&c_g;&c_g;&c_je;</trd>
  <trd lng="uk">&#x411;&#x440;&#x44E;&#x433;&#x433;&#x435;</trd>
