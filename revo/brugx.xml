<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Bru&gcirc;</rad>/o
</kap>
<drv mrk="brugx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la belga provinco
      <ref tip="malprt" cel="flandr.F0ujo.prov">Okcident-Flandrujo</ref>
      (3&ring;13'E, 51&ring;13'N):
      <ekz>
        la belga urbo <tld/>o, samtempe kun la hispana Salamanca, estas
        kultura &ccirc;efurbo de E&ubreve;ropo dum 2002<fnt><aut>Roland 
Rotsaert</aut>:
        <vrk>Mezepoka Hanso-asocio revivi&gcirc;as</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 4a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_u;&c_g;&c_je;</trd>
  <trd lng="de">Br&uuml;gge</trd>
  <trd lng="en">Bruges</trd>
  <trd lng="es">Brujas</trd>
  <trd lng="fr">Bruges</trd>
  <trdgrp lng="hu">
    <trd>Brugge</trd>,
    <trd>Bruges</trd>
  </trdgrp>
  <trd lng="lat">Brugis Flandrorum</trd>
  <trd lng="nl">Brugge</trd>
  <trd lng="it">Bruges</trd>
  <trd lng="pl">Brugia</trd>
  <trd lng="ru">&c_B;&c_r;&c_ju;&c_g;&c_g;&c_je;</trd>
</drv>
</art>
