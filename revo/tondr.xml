<kap>
  <ofc>*</ofc>
  <rad>tondr</rad>/i
</kap>
<drv mrk="tondr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
  <snc mrk="tondr.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Postfulme bruegi:
      <ekz>
        fulmis kaj <tld/>is, forte pluvegis, estis terure
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li endormi&gcirc;is kaj ekson&gcirc;is, dum ekstere <tld/>is kaj fulmis
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is kaj fulmis, sur la maro levi&gcirc;is nigraj ondoj
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">d&ouml;r&ouml;g az &eacute;g</trd>
  </snc>
  <snc mrk="tondr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Bruegi:
      <ekz>
        la pafilegoj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>ne &ccirc;io batas, kio <tld/>as</ind>
        <trd lng="fr">chien qui aboie ne mord pas</trd>
        <trd lng="es">no todo lo que brilla es oro</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ek<tld/>is la vo&ccirc;o de la trumpetoj kaj cimbaloj
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 5:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;tege <tld/>is la komando Mar&scirc;'!
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;&jcirc;etu malsupren tiun &ccirc;i felon!&rightquot; ek<tld/>is el lia bu&scirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne&gcirc;tavoloj lavange <tld/>is trans fendri&ccirc;ajn montmurojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Maljunulo de la Migranta Monto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> estas <tld/>e apla&ubreve;dataj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la popolo <klr>[&#8230;]</klr> ek<tld/>is per <klr>[&#8230;]</klr> grandega
        aklamo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>d&ouml;r&ouml;g</trd>,
      <trd>d&ouml;ng</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_m;&c_je;&c_c;&c_mol;</trd>
  <trd
  lng="bo">&#xF60;&#xF56;&#xFB2;&#xF74;&#xF42;&#xF0B;&#xF66;&#xF90;&#xF51;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>h&rcaron;m&iacute;t</trd>,
    <trd>h&rcaron;m&ecaron;t</trd>,
    <trd>h&rcaron;&iacute;mat</trd>
  </trdgrp>
  <trd lng="de">donnern</trd>
  <trd lng="en">thunder <klr>(verb)</klr></trd>
  <trd lng="es">tronar</trd>
  <trd lng="fa">&a_r;&a_ayn;&a_d; &a_z;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>retentir</trd>,
    <trd>tonner</trd>
  </trdgrp>
  <trd lng="id">ber<ind>gemuruh</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x96F7;&#x304C;&#x9CF4;&#x308B; 
    <pr>&#x304B;&#x307F;&#x306A;&#x308A;&#x304C;&#x306A;&#x308B;</pr></trd>,
    <trd>&#12392;&#12393;&#x308D;&#x304F;</trd>
  </trdgrp>
  <trd lng="nl">donderen</trd>
  <trd lng="pl">grzmie&cacute;</trd>
  <trd lng="pt">trovejar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_je;&c_m;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_o;&c_m;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bur&aacute;ca&tcaron;</trd>,
    <trd>hrmie&tcaron; <klr>(po blesku)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_r;&c_i;&#x43C;&c_ib;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#25171;&#x96F7; <pr>d&#x1CE;l&#xE9;i</pr></trd>,
    <trd>&#x96F7;&#x58F0; <pr>l&#xE9;ish&emacron;ng</pr></trd>,
    <trd>&#x96F7;&#32882; <pr>l&#xE9;ish&emacron;ng</pr></trd>,
    <trd>&#x96F7;&#x9CF4; <pr>l&#xE9;im&#xED;ng</pr></trd>,
    <trd>&#x96F7;&#x9E23; <pr>l&#xE9;im&#xED;ng</pr></trd>,
    <trd>&#30768; <pr>p&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tondr.0o">
  <kap><tld/>o</kap>
    <snc mrk="tondr.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Krakbruego a&ubreve; rulbruego akompananta fulmon:
      <ekz>
        <tld/>o eksonis pli proksime
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
      <ekz>
        el klara &ccirc;ielo <tld/>o ekbatis
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o a&ubreve;digadis brue frapon post frapo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;esi&gcirc;is la pluvo kaj la hajlo kaj la <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 9:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; diablo <tld/>on <klr>(fulmon)</klr> suferos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">mennyd&ouml;rg&eacute;s</trd>
  </snc>
  <snc mrk="tondr.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="malprt" cel="bru.0o.sonaro">Bru</ref>ego:
      <ekz>
        <tld/>o de bataloj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>o de pafilego
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de Lia veturilo la popoloj falas de teruro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uragan.0o.FIG">uragano<sncref/></ref>
    </refgrp>
    <trd lng="hu">d&ouml;rg&eacute;s</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_m;</trd>
  <trd lng="cs">hrom</trd>
  <trd lng="de">Donner</trd>
  <trd lng="en">thunder <klr>(noun)</klr></trd>
  <trd lng="es">trueno</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_ayn;&a_d;</trd>,
    <trd>&a_t;&a_n;&a_d;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">tonnerre</trd>
  <trdgrp lng="id">
    <trd>geledek</trd>,
    <trd>gemuruh</trd>,
    <trd>guntur</trd>,
    <trd>guruh</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x96F7;  <pr>&#x306A;&#x308B;&#x304B;&#x307F;</pr></trd>,
    <trd>&#x96F7;&#x9CF4;  <pr>&#12425;&#12356;&#12417;&#12356;</pr></trd>,
    <trd>&#22823;&#x97F3;&#x97FF;  <pr>&#12384;&#12356;&#x304A;&#12435;&#x304D;&#12423;&#12358;</pr></trd>,
    <trd>&#12372;&#12358;&#x97F3;  <pr>&#12372;&#12358;&#x304A;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">donder</trd>
  <trd lng="pl">grzmot</trd>
  <trd lng="pt">trov&atilde;o</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>hrmenie</trd>,
    <trd>hrom</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_r;&c_ib;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#38713;&#x96F3; <pr>p&#x12B;l&#xEC;</pr></trd>,
    <trd>&#38713;&#38722; <pr>p&#x12B;l&#xEC;</pr></trd>,
    <trd>&#38713; <pr>p&#x12B;</pr></trd>,
    <trd>&#x96F7;&#x58F0; <pr>l&#xE9;ish&emacron;ng</pr></trd>,
    <trd>&#x96F7;&#32882; <pr>l&#xE9;ish&emacron;ng</pr></trd>,
    <trd>&#38736; <pr>b&#xEC;ng</pr></trd>,
    <trd>&#x96F7; <pr>l&#xE9;i</pr></trd>,
    <trd>&#38713;&#x96F7; <pr>p&#x12B;l&#xE9;i</pr></trd>,
    <trd>&#x96F7;&#x9CF4; <pr>l&#xE9;im&#xED;ng</pr></trd>,
    <trd>&#x96F7;&#x9E23; <pr>l&#xE9;im&#xED;ng</pr></trd>,
    <trd>&#30768; <pr>p&emacron;ng</pr></trd>,
    <trd>&#x78E4; <pr>y&#x1D0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tondr.fulmo0o">
  <kap>fulmo<tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Neda&ubreve;ra pluvego kun fulmoj kaj <tld/>oj:
      <ekz>
        la varmego kaj la letargio de l' insektoj anta&ubreve;diras fulmo<tld/>on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen prepari&gcirc;is fulmo<tld/>o kaj la vespero alproksimi&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fulmo<tld/>o kuris super ili
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu semas venton, rikoltos fulmo<tld/>on
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uragan.0o">uragano</ref>,
      <ref cel="sxtorm.0o">&scirc;tormo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_b;&c_u;&c_r;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>bou&rcaron;e</trd>,
    <trd>bou&rcaron;ka</trd>,
    <trd>hromobit&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gewitter</trd>
  <trd lng="en">thunderstorm</trd>
  <trd lng="es">tormenta el&eacute;ctrica</trd>
  <trd lng="fa">&a_t;&a_w;&a_f;&a_A;&a_n;
  &a_t;&a_n;&a_d;&a_r;&f_y;</trd>
  <trd lng="fr">orage</trd>
  <trdgrp lng="hu">
    <trd>zivatar</trd>,
    <trd>&eacute;gih&aacute;bor&uacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x96F7;&#x96E8;  <pr>&#12425;&#12356;&#12358;</pr></trd>,
    <trd>&#x96F7;&#12434;&#12392;&#12418;&#x306A;&#12358;&#x6FC0;&#12375;&#12356;&#x306B;&#x308F;&#x304B;&#x96E8; 
    <pr>&#x304B;&#x307F;&#x306A;&#x308A;&#12434;&#12392;&#12418;&#x306A;&#12358;&#x306F;&#12370;&#12375;&#12356;&#x306B;&#x308F;&#x304B;&#12354;&#12417;</pr></trd>
  </trdgrp>
  <trd lng="nl">onweer</trd>
  <trd lng="no">uv&aelig;r</trd>
  <trd lng="pl">burza<klr> z piorunami</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hromobitie</trd>,
    <trd>kr&aacute;tka b&uacute;rka</trd>,
    <trd>letn&aacute; b&uacute;rka</trd>
  </trdgrp>
  <trd lng="sv">&aring;ska</trd>
  <trd lng="uk">&c_g;&c_r;&#x43E;&c_z;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x96F7;&#x96E8; <pr>l&#xE9;iy&#x1D4;</pr></trd>,
    <trd>&#x897F;&#21271;&#x96E8; <pr>x&#x12B;b&#x11B;iy&#x1D4;</pr></trd>,
    <trd>&#x72C2;&#x98A8;&#x66B4;&#x96E8; <pr>ku&#xE1;ngf&emacron;ngb&#xE0;oy&#x1D4;</pr></trd>,
    <trd>&#x72C2;&#x98CE;&#x66B4;&#x96E8; <pr>ku&#xE1;ngf&emacron;ngb&#xE0;oy&#x1D4;</pr></trd>,
    <trd>&#x96F7;&#x66B4;&#x96E8; <pr>l&#xE9;ib&#xE0;oy&#x1D4;</pr></trd>
  </trdgrp>
</drv>

