<drv mrk="respon.0eci0umi">
  <kap><tld/>eci,
  <var><kap><tld/>umi</kap></var></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="respons.0i">Responsi</ref>:
    <ekz>
        en multaj regionoj la romkatolika eklezio <tld/>ecas pri sano
        <fnt>
         <aut>W. H. Simcock</aut>
            <vrk><url ref="&Monato;/2000/006948.php">Kie la toleremo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/07, p. 10</lok>
          </fnt>;
      </ekz><ekz>
        kaj &ccirc;ar tiuj le&gcirc;oj <klr>[&#8230;]</klr> efikas plej intense &ccirc;e la popolo, malplej &ccirc;e la
        kleruloj, ni rajtas diri, ke la popolo <tld/>umas pri la &hcirc;aosa stato de
        la lingvo, kiun &gcirc;i parolas!
        <fnt><aut>G. Waringhien</aut>: <vrk>Letero al K-do Lanti</vrk>.
        <lok>Sennacieca Revuo, jun. 1939</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
        <ref cel="kacx.0o.konfuzo">ka&ccirc;o<sncref/></ref>,
        <ref cel="kas.0o">kaso</ref>,
        <ref cel="kulp.0o">kulpo</ref>
    </refgrp>
  </snc>
  <rim mrk="respon.0eci0umi.RIM">
    &leftquot;<tld lit="R"/>eci&rightquot; estas konverto de la malregula
    <ref cel="respon.0eco.responso"><tld/>eco<sncref/></ref>.
    &leftquot;<tld lit="R"/>umi&rightquot; estas logike neripro&ccirc;ebla, sed malpli ofta ol la
    paronima &leftquot;responsi&rightquot;. <aut>Sergio Pokrovskij</aut>
  </rim>
  <trd lng="id"><baz>tanggung jawab</baz>bertanggung jawab</trd>
  <trdgrp lng="ja">
    <trd>&#x8CAC;&#x4EFB;&#12434;&#25345;&#12388;  <pr>&#x305B;&#x304D;&#x306B;&#12435;&#12434;&#12418;&#12388;</pr></trd>,
    <trd>&#x4FDD;&#x8A3C;&#12377;&#x308B;  <pr>&#x307B;&#12375;&#12423;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odpowiada&cacute;</trd>,
    <trd>wzi&aogonek;&cacute; na siebie</trd>,
    <trd>by&cacute;  <ind>odpowiedzialnym</ind></trd>
  </trdgrp>
