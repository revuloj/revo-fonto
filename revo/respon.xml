          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007989.html">Sen solvo neniu paco</url></vrk>
        </fnt>;
      </ekz><ekz>
        <ind>sen<tld/>a amo</ind>.
        <trd lng="de">unerwiderte <ind>Liebe</ind></trd>
        <trd lng="hu">viszonzatlan szerelem</trd>
