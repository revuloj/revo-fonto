  <gra><vspec>x</vspec></gra>
  <snc mrk="zorg.0i.pri_ies_bono">
    <dif>
      Serioze direkti sian atenton kaj aktivecon al ies bono:
      <ekz>
        mi <tld/>as pri &scirc;i tiel kiel mi <tld/>as pri mi mem
        <fnt><bib>F</bib> <lok>&FE; 18</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>u pri lia kuracado
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri la azeninoj, kiuj perdi&gcirc;is <klr>[&#8230;]</klr> ne <tld/>u, &ccirc;ar ili estas trovitaj
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 9:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        Li &ccirc;irka&ubreve;is lin, <tld/>is pri li, &parsl; Gardis lin kiel pupilon de Sia okulo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 32:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        li la&ubreve;te ploris, homoj eniradis kaj eliradis, <klr>[&#8230;sed]</klr> neniu <tld/>is
        pri la knabo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ku&scirc;igis lin sur la sablo kaj <tld/>is, ke lia kapo ku&scirc;u alte
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli <tld/>as unu patrino pri dek infanoj 
        ol dek infanoj pri unu patrino
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>u pri vi kaj nenion pli</ind>
        <klr>(vd <ref tip="vid" cel="naz.0o">nazo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">t&ouml;r&odblac;dj a magad dolg&aacute;val</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fleg.0i">flegi</ref>,
      <ref cel="klopod.0i">klopodi</ref>,
      <ref cel="gard">prigardi</ref>,
      <ref cel="vart.0i">varti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_p;&c_je;&c_k;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
