<drv mrk="zorg.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="zorg.sen0a.ne">
    <dif>
      Ne maltrankvila pro <tld/>oj:
      <ekz>
        Johano povis dormi tute sen<tld/>e, kaj tion li efektive faris
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        jaroj de <klr>[&#8230;]</klr> sen<tld/>a kaj feli&ccirc;a vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas ja tiel bele, se oni en la maljuneco povas vivi sen<tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li metis la manojn en la po&scirc;ojn, apogis sin sen<tld/>ece al angulo de muro kaj
        kun rideto sur la lipoj <klr>[&#8230;]</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li deziregis denove foriri en la drinkejon, por dibo&ccirc;i kun siaj
        gajaj sen<tld/>aj kolegoj
        <fnt>
        <aut>A. Edelmann, tradukis I. Kaminski</aut>
        <vrk>La forgesita pipo</vrk>
        <lok>en: Fundamenta Krestomatio de la lingvo Esperanto, 1904</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lum.0a">luma</ref>,
      <ref cel="nub">sennuba</ref>,
      <ref cel="seren.0a">serena</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_s;&c_k;&c_l;&c_a;&c_p;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_t;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_drengbu;&t_ma;&t_sa;&t_tsheg;&t_ga;&t_shabchu;&t_tsheg;&t_ya;&t_nga;&t_sa;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="cs">bezstarostn&yacute;</trd>
  <trdgrp lng="de">
