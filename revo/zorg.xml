    <trd>zaopatri&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#3604;&#3641;&#3649;&#3621;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_t;&c_i;</trd>,
