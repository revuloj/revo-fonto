<drv mrk="zorg.dom0isto">
  <kap>
    dom<tld/>isto
  </kap>
  <snc>
    <dif>
      Homo komisiita per <tld/>ado pri la bonstato de domo,
      t.e. hejtado, purigado, riparoj k.s.:
      <ekz>
        la domon posedis la firmo, kie li laboris, kaj li estis dom<tld/>isto
        <fnt>
          <bib>Kiso</bib>
          <lok>&Aring;GE</lok>
        </fnt>;
      </ekz>
      <ekz>
        li laboris kiel gazetportisto, &gcirc;ardena kaj mueleja laboristo, purigisto kaj
        dom<tld/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=83831"
              >Aimo Rantanen</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekonom.0o">ekonomo</ref>,
      <ref cel="kort.0isto">kortisto</ref>
    </refgrp>
  </snc>
  <trd lng="de">Hausmeister</trd>
  <trd lng="en">caretaker</trd>
  <trd lng="fr">concierge</trd>
</drv>

<drv mrk="zorg.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="zorg.0ede.KOMUNE">
    <dif>
      <ref tip="super" cel="de.0.aganto">De</ref>, kun la helpo de,
      plenumate de:
      <ekz>
        oni surscenigas <klr>[&#8230;]</klr> altnivelajn spektaklojn, <tld/>e
        de la Torina Manifestacia Instanco
        <fnt><aut>Turisma skabenejo de la urbo Torino</aut>,
        <vrk>Torino,
        1984</vrk></fnt>;
      </ekz>
      <ekz>
        oni intencas balda&ubreve; <klr>[la tekston]</klr> publikigi, kune kun centoj da aliaj
        tiaj tekstoj, en papera kaj cifereca versioj, <tld/>e de la eldonejo <nom>Edistudio</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011330.php">Nuntempo sciencfikcia</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dank.0e_al">danke al</ref>,
      <ref cel="far.0ede">fare de</ref>,
      <ref cel="flank.0ede">flanke de</ref>,
      <ref cel="egid.0o.protekto">egido</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_k;&c_u;&c_ju;&c_ch;&c_y; <klr>(&c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib; &c_v;&c_y;&c_s;&c_ib;&c_l;&c_k;&c_a;&c_m;, &c_k;&c_l;&c_o;&c_p;&c_a;&c_t;&c_u;)</klr></trd>
  <trdgrp lng="de">
    <trd>unter <ind>Leitung</ind> von</trd>,
    <trd>unter <ind>Aufsicht</ind> von</trd>,
    <trd><ind>betreut</ind> von</trd>,
    <trd>durch</trd>
  </trdgrp>
  <trd lng="en">in the <ind>care</ind> of</trd>
  <trd lng="fr">par les soins de</trd>
  <trd lng="hu">gondoz&aacute;s&aacute;ban</trd>
  <trd lng="it">a cura di</trd>
  <trd lng="pl">pod opiek&aogonek;</trd>
  <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_d;&c_a;&c_r;&c_ja; &c_u;&c_s;&c_i;&c_l;&c_i;&c_ja;&c_m;</trd>
</drv>







