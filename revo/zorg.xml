      <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>se <ind>soucier</ind> de</trd>,
      <trd>se faire du <ind>souci</ind></trd>
    </trdgrp>
    <trd lng="hu">agg&oacute;dik</trd>
    <trdgrp lng="pl">
