<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: falanster.xml,v 1.6 2019/10/04 07:10:20 revo Exp $">
<kap>
  <rad>falanster</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="falanster.0o">
  <kap><tld/>o</kap>
  <snc mrk="falanster.0o.POL">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="falang.0ejo.furiera">Falangejo</ref>:
      <ekz>
        &ccirc;iuj apartenantaj al la <tld/>o estas egale vestitaj
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kolekt1.0o">kolektivo</ref>,
      <ref cel="koloni.0o.setlejo">kolonio<sncref/></ref>,
      <ref cel="komun.0umo.societo">komunumo<sncref/></ref>,
      <ref cel="kibuc.0o">kibuco</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/6/6d/Phalanst&egrave;re.jpg/320px-Phalanst&egrave;re.jpg"
      prm="PD"
      ><tld/>o la&ubreve; la ideo de Karlo Furiero (sen kampoj kaj &gcirc;ardenoj)
        <fnt>
          <aut>Victor Consid&eacute;rant</aut>
          <lok><url ref="&WCW;/File:Phalanst%C3%A8re.jpg"
              >https://commons.wikimedia.org/wiki/File:Phalanst&egrave;re.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="cs">
    <trd>falanga</trd>,
    <trd>falanst&eacute;ra</trd>
  </trdgrp>
  <trd lng="de">Phalanst&egrave;re</trd>
  <trd lng="fr">phalanst&egrave;re</trd>
  <trd lng="sk">falanga</trd>
</drv>
</art>

