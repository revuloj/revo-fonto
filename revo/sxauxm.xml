<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;a&ubreve;m</rad>/o
</kap>
<drv mrk="sxauxm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Svarmo da veziketoj formi&gcirc;antaj el boligita a&ubreve; forte
kirlita
      flua&jcirc;o:
      <ekz>
        blanka <tld/>o na&gcirc;as sur la akvo;
      </ekz>
      <ekz>
        post tia kurego mia &ccirc;evalo estis kovrita de <tld/>o;
      </ekz>
      <ekz>
        <tld/>a vino, omleto;
      </ekz>
      <ekz>
        <tld/>kirli kremon, ovblankojn.
      </ekz>
    </dif>
    <trd lng="de">Schaum</trd>
    <trd lng="fr">mousse</trd>
    <trdgrp lng="hu">
      <trd>hab</trd>,
      <trd>tajt&eacute;k</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plej malbona, plej malestiminda parto:
      <ekz>
        <tld/>o de la socio
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fecx.0o">fe&ccirc;o</ref>,
      <ref cel="bala.0ajxo">balaa&jcirc;o</ref>,
      <ref cel="skori.0o">skorio</ref>,
      <ref cel="rub.0o">rubo</ref>.
    </refgrp>
    <trd lng="de">Abschaum</trd>
    <trdgrp lng="hu">
      <trd>s&ouml;pred&eacute;k</trd>,
      <trd>alja<klr tip="ind"> (pejorat&iacute;v)</klr></trd>
  </trdgrp>
  </snc>
  <trd lng="en">foam</trd>
  <trd lng="nl">schuim</trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_a;</trd>
</drv>

<drv mrk="sxauxm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Formi <tld/>on el si:
      <ekz>
        la cidro, la &ccirc;ampano, la biero <tld/>as en
        la glasoj;
      </ekz>
      <ekz>
        la ondoj <tld/>as kontra&ubreve; la rokoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;ie la vivo <tld/>as<fnt>B</fnt>
        (<ref tip="sin" cel="svarm.0i">svarmi</ref>);
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i pro furiozo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        niaj klopodoj vane dis<tld/>i&gcirc;is
        <klr>(nenii&gcirc;is)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sch&auml;umen</trd>
  <trd lng="fr">mousser</trd>
  <trd lng="hu">habzik</trd>
  <trd lng="nl">schuimen</trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
