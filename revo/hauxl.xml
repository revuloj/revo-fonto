<art mrk="$Id$">
<kap>
  <rad>ha&ubreve;l</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hauxl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="tren.0i">Treni</ref> peza&jcirc;on per kabloj:
      <ekz>
        ni elprenis el la boato niajn provizojn, <tld/>is &gcirc;in el la sablo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 41a, p.
        318a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_k;&c_a;&c_n;&c_a;&c_c;&c_je;, &c_l;&c_a;&c_n;&c_c;&c_u;&c_g;&c_u;)</klr></trd>
  <trd lng="fr">haler</trd>
  <trd lng="hu">vontat<klr tip="ind"> (dr&oacute;tk&ouml;t&eacute;len)</klr></trd>
  <trd lng="ru">&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;<klr tip="amb"> (&c_n;&c_a; &c_k;&c_a;&c_n;&c_a;&c_t;&c_je;, &c_c;&c_je;&c_p;&c_i;, &c_k;&c_a;&c_k; &c_b;&c_u;&c_r;&c_l;&c_a;&c_k;&c_i;)</klr></trd>
</drv>
</art>
