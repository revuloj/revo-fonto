<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: te.xml,v 1.38 2020/12/29 08:10:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>te</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="te.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="te.0o.folioj">
    <dif>
      Sekigitaj folioj de <ref tip="malprt"
      cel="te.0arbo"><tld/>arbo</ref>:
      <ekz>
        la ofica salajro ne sufi&ccirc;as e&ccirc; por <tld/>o kaj sukero
        <fnt>
          <bib>Revizoro</bib>,
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#33590;&#21494;<klr> [ch&aacute;y&egrave;]</klr></trd>
  </snc>
  <snc mrk="te.0o.trinkajxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:trinka&jcirc;oj"
      cel="trink.0ajxo">Trinka&jcirc;o</ref> infuze preparita
      el folioj de <tld/>arbo, eventuale el alispecaj folioj:
      <ekz>
        &scirc;ian teon el milfolio kun pistita sova&gcirc;a rozo kaj fragaj folioj
        tie ne eblis servi
        <fnt>
          <aut>S. &Scirc;timec</aut>,
          <vrk>Morto en Melburna tramo</vrk>, 
          <bib>Monato</bib>,
          <lok>1997:8, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne malhelpas, se oni trinkas superfluan glason da <tld/>o
        <fnt>
          <bib>Revizoro</bib>,
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hi">&#2330;&#2366;&#2351; [<ind>c&#257;y</ind>]</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_b;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_a;&c_j;</trd>
  <trd lng="bo">&#3911;&#3851;</trd>
  <trd lng="ca">te</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;aj</trd>,
    <trd>&ccaron;ajovn&iacute;k &ccaron;&iacute;nsk&yacute;</trd>
  </trdgrp>
  <trd lng="de">Tee</trd>
  <trd lng="en">tea</trd>
  <trd lng="es">t&eacute;</trd>
  <trd lng="fr">th&eacute;</trd>
  <trd lng="hu">tea</trd>
  <trd lng="id">teh</trd>
  <trd lng="nl">thee</trd>
  <trd lng="pl">herbata</trd>
  <trdgrp lng="pt">
   <trd>ch&aacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_j;</trd>
  <trd lng="sk">&ccaron;ajovn&iacute;k &ccaron;&iacute;nsky</trd>
  <trd lng="sv">te</trd>
  <trd lng="tr">&ccedil;ay</trd>
  <trd lng="zh">&#33590;<klr> [ch&aacute;]</klr></trd>
</drv>

<drv mrk="te.0arbo">
  <kap><tld/>arbo</kap>
  <snc mrk="te.0arbo.azia">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> de <ref tip="malprt"
      cel="kameli.0o.BOT">kamelio</ref>
      (<trd lng="la">Camellia sinensis</trd>), azia arbedo uzata por
      produktado de la <tld/>o:
      <ekz>
        iun tagon, kiam li ripozis sub <tld/>arbo, folio falis en lian
        tason
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=4109'>Teo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">te</trd>
    <trd lng="de">Teestrauch</trd>
    <trd lng="en">tea tree</trd>
    <trd lng="fr">th&eacute;ier</trd>
    <trd lng="hu">k&iacute;nai teacserje</trd>
    <trd lng="nl">theestruik</trd>
    <trdgrp lng="pt">
     <trd>ch&aacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_ch;&c_a;&c_j; <klr
    tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
    <trd lng="tr">&ccedil;ay</trd>
  </snc>
  <snc mrk="te.0arbo.australia">
    <uzo tip="fak">BOT</uzo>
    <dif>
       Arboj kaj arbustoj el la genroj 
       <trd lng="la">Baeckea</trd>, <trd lng="la">Kunzea</trd>, <trd
       lng="la">Leptospermum</trd> 
       kaj <trd lng="la">Melaleuca</trd>, el familio <ref tip="malprt"
       cel="mirtac.0oj">mirtacoj</ref>,
       el kies folioj oni preparis en a&ubreve;straliaj regionoj teon en la 18a
       jc., sed nuntempe uzataj por produkti
       odorantajn oleojn:
       <ekz>
        la plej multaj el la pli grandaj specioj <klr>[de la genro
        melale&ubreve;ko]</klr> anka&ubreve; estas konataj kiel <ctl><tld/>arboj</ctl>
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=392191'>Melale&ubreve;ko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Teebaum</trd>
  </snc>
</drv>

<drv mrk="te.0masxino">
  <kap><ofc>*</ofc><tld/>ma&scirc;ino <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5220">te'ma&scirc;in'</url></lok></fnt></kap>
  <snc>
    <dif>
      Metala aparato, en kiu oni boligas akvon por teo,
      <sup>2</sup><ref tip="sin" cel="samova.0o">samovaro</ref>.
    </dif>
  </snc>
    <trd lng="de">Theemaschine</trd>
    <trd lng="en">tea-kettle</trd>
    <trd lng="fr">bouilloire &agrave; th&eacute;</trd>
</drv>

</art>
