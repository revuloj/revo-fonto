<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>te</rad>/o
</kap>
<drv mrk="te.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="te.0o.folioj">
    <dif>
      Sekigitaj folioj de <tld/>arbo.
    </dif>
  </snc>
  <snc mrk="te.0o.trinkajxo">
    <dif>
      Trinka&jcirc;o preparata el ili, <tld/>infuza&jcirc;o.
    </dif>
  </snc>
  <trd lng="cs">&ccaron;aj</trd>
  <trd lng="de">Tee</trd>
  <trd lng="en">tea</trd>
  <trd lng="fr">th&eacute;</trd>
  <trd lng="nl">thee</trd>
  <trd lng="pl">herbata</trd>
  <trd lng="tr">&ccedil;ay</trd>
</drv>
<drv mrk="te.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="kameli.0o.BOT">kamelio</ref>
      (<trd lng="la">Camellia sinensis</trd>), azia arbedo uzata por
      produktado de la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Teestrauch</trd>
  <trd lng="tr">&ccedil;ay</trd>
</drv>
<drv mrk="te.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Theaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Teegew&auml;chse</trd>
</drv>
</art>
