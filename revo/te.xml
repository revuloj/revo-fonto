<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: te.xml,v 1.47 2023/10/21 13:02:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>te</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="te.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="te.0o.folioj">
    <dif>
      Sekigitaj folioj de <ref tip="malprt" cel="te.0arbo"><tld/>arbo</ref>:
      <ekz>
        mi anka&ubreve; donacos al vi du plenajn po&scirc;ojn da <tld/>o, tute verda kaj fre&scirc;a, kiun
        mi mem &scirc;irkolektis
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ofica salajro ne sufi&ccirc;as e&ccirc; por <tld/>o kaj sukero
        <fnt>
          <bib>Revizoro</bib>,
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Teebl&auml;tter</trd>
    </trdgrp>
    <trd lng="zh">&#33590;&#x53F6;<klr> [ch&aacute;y&egrave;]</klr></trd>
  </snc>
  <snc mrk="te.0o.trinkajxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="trink.0ajxo">Trinka&jcirc;o</ref> infuze preparita
      el folioj de <tld/>arbo:
      <ekz>
        anstata&ubreve; kafo li donis al mi <tld/>on kun sukero, sed sen kremo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i balda&ubreve; pretigos por ili <tld/>on, por ke ili denove fari&gcirc;u bonfartaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi devas veni diman&ccirc;e <klr>[&#8230;]</klr> al <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne malhelpas, se oni trinkas superfluan glason da <tld/>o
        <fnt>
          <bib>Revizoro</bib>,
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Tee <klr>[anka&ubreve; aliaj plantoj ol Camellia sinensis]</klr></trd>
    </trdgrp>
    <trd lng="hi">&#x91A;&#x93E;&#x92F; <pr>c&amacron;y</pr></trd>
  </snc>
  <snc mrk="te.0o.tizano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="dif" cel="tizan.0o">Tizano</ref> el iu alia vegeta&jcirc;o ol
      <ref tip="vid" cel="te.0o.folioj"><tld/>o<sncref/></ref>:
      <ekz>
        &scirc;i enportigis la <ref cel="masxin.te0o"><tld/>ma&scirc;ino</ref>n,
        por kuiri por li bonan tason da sambuka <tld/>o,
        &ccirc;ar &ccirc;i tiu varmigas
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ian <tld/>on el milfolio kun pistita sova&gcirc;a rozo kaj fragaj folioj
        tie ne eblis servi
        <fnt>
          <aut>S. &Scirc;timec</aut>
          <vrk>Morto en Melburna tramo</vrk>
          <bib>Monato</bib>
          <lok>1997:8, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Aufguss<klr> [aliaj plantoj ol Camellia sinensis]</klr></trd>,
      <trd>Kr&auml;utertee<klr> [aliaj plantoj ol Camellia sinensis]</klr></trd>,
      <trd>tee&auml;hnliches Erzeugnis<klr> [ISO3720, aliaj plantoj ol Camellia sinensis]</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_b;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_a;&c_j;</trd>
  <trd lng="bo">&t_ja;&t_tsheg;</trd>
  <trd lng="ca">te</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;aj</trd>,
    <trd>&ccaron;ajovn&iacute;k &ccaron;&iacute;nsk&yacute;</trd>
  </trdgrp>
  <trd lng="de">Tee</trd>
  <trd lng="en">tea</trd>
  <trd lng="es">t&eacute;</trd>
  <trd lng="fr">th&eacute;</trd>
  <trd lng="hu">tea</trd>
  <trd lng="id">teh</trd>
  <trdgrp lng="ja">
    <trd>&#33590;  <pr>&#12385;&#12419;</pr></trd>,
    <trd>&#x7D05;&#33590;  <pr>&#12371;&#12358;&#12385;&#12419;</pr></trd>,
    <trd>&#x30C1;&#x30E3;</trd>
  </trdgrp>
  <trd lng="nl">thee</trd>
  <trd lng="pl">herbata</trd>
  <trdgrp lng="pt">
   <trd>ch&aacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_j;</trd>
  <trd lng="sk">&ccaron;ajovn&iacute;k &ccaron;&iacute;nsky</trd>
  <trd lng="sv">te</trd>
  <trd lng="tr">&ccedil;ay</trd>
  <trd lng="uk">&c_ch;&c_a;&c_j;</trd>
  <trd lng="zh">&#33590;<klr> [ch&aacute;]</klr></trd>
</drv>

<drv mrk="te.blanka0o">
  <kap>blanka <tld/>o</kap>
  <snc mrk="te.blanka0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Delikata, ordinare multekosta
      <ref tip="super" cel="te.0o.folioj"><tld/>o<sncref/></ref>
      precipe el bur&gcirc;onoj, malmulte oksidigita,
      kaj trinka&jcirc;o per &gcirc;i preparita:
      <ekz>
         trinku multe da akvo, kelke da tasoj da verda a&ubreve; blanka <tld/>o
         kaj almena&ubreve; unu glason da fre&scirc;e elpremita frukt- a&ubreve; legom-suko kun fibroj tage
         <fnt>
           <aut>L. Van De Velde</aut>
           <vrk><url ref="http://www.monato.be/2013/010934.php">
           Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
           <lok>Monato, 2013:07, p. 19a</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">th&eacute; blanc</trd>
</drv>

<drv mrk="te.nigra0o">
  <kap>nigra <tld/>o,
  <var><kap>ru&gcirc;a <tld/>o</kap></var></kap>
  <snc mrk="te.nigra0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longe konservebla <ref tip="super" cel="te.0o.folioj"><tld/>o<sncref/></ref>
      kies folioj estis krom sekigitaj anka&ubreve; oksidigitaj,
      kaj trinka&jcirc;o per &gcirc;i preparita:
      <ekz>
        tiu juna ni&gcirc;erano alvenis anta&ubreve; 8 jaroj ... konanta nur nigran <tld/>on... ne verdan
        <fnt>
          <aut>T. Coloma. trad. J.-M. Cash</aut>
          <vrk><url ref="https://eo.mondediplo.com/2010/05/article1601.html">
          La never&scirc;ajna epopeo de la Afrikanoj en &Ccirc;inujo</url></vrk>
          <lok>Le Monde diplomatique, 2010-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bonodoro de ru&gcirc;a <tld/>o kaj la buljono el grandaj ostoj miksi&gcirc;as
        <fnt>
          <aut>Li Lu</aut>
          <vrk><url ref="https://esperanto.cri.cn/1341/2013/05/03/173s149315_2.htm">
          Dek pladoj el te-festeno</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    &leftquot;Ru&gcirc;a <tld/>o&rightquot; estas la ordinara &ccirc;ina nomo de tiuspecaj <tld/>oj,
    kaj en tiu nomsistemo la nigraj <tld/>oj estas nur postfermentintaj specoj.
    Tial en internacia uzo &leftquot;nigra <tld/>o&rightquot; povas esti malklara.
    <aut>&jcirc;v</aut>
  </rim>
  <trd lng="fr">th&eacute; noir</trd>
</drv>

<drv mrk="te.verda0o">
  <kap>verda <tld/>o</kap>
  <snc mrk="te.verda0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="te.0o.folioj"><tld lit="T"/>o<sncref/></ref>
      kies folioj estis sekigitaj kun apena&ubreve;a oksidigo,
      kaj trinka&jcirc;o per &gcirc;i preparita:
      <ekz>
        verda <tld/>o la&ubreve;dire kontribuas al resani&gcirc;o
        <fnt>
          <aut>A. Oberndorfer</aut>,
          <vrk><url ref="http://peranto.posterous.com/verda-teo-la-miraklajo">
          Verda teo - la mirakla&jcirc;o</url></vrk>,
          <lok>Esperanta Retradio, 2012-12-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">th&eacute; vert</trd>
</drv>

<drv mrk="te.0arbo">
  <kap><tld/>arbo</kap>
  <snc mrk="te.0arbo.azia">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> de <ref
      tip="malprt" cel="kameli.0o.BOT">kamelio</ref>
      (<trd lng="la">Camellia sinensis</trd>), azia arbedo uzata por
      produktado de la <tld/>o:
      <ekz>
        iun tagon, kiam li ripozis sub <tld/>arbo, folio falis en lian
        tason
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4109">Teo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/62/Camellia_sinensis_01.jpg/340px-Camellia_sinensis_01.jpg" prm="CC    
    BY-SA 3.0">bran&ccirc;o kun <tld/>floro Tea, Hangzhou, &Ccirc;inujo
        <fnt>
          <aut>Armin K&uuml;belbeck, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Camellia_sinensis_01.jpg">&WCW;/File:Camellia_sinensis_01.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">te</trd>
    <trd lng="de">Teestrauch</trd>
    <trd lng="en">tea tree</trd>
    <trd lng="fr">th&eacute;ier</trd>
    <trd lng="hu">k&iacute;nai teacserje</trd>
    <trd lng="nl">theestruik</trd>
    <trdgrp lng="pt">
     <trd>ch&aacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_ch;&c_a;&c_j; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
    <trd lng="tr">&ccedil;ay</trd>
  </snc>
  <snc mrk="te.0arbo.australia">
    <uzo tip="fak">BOT</uzo>
    <dif>
       Arboj kaj arbustoj el la genroj
       <trd lng="la">Baeckea</trd>, <trd lng="la">Kunzea</trd>, <trd lng="la">Leptospermum</trd>
       kaj <trd lng="la">Melaleuca</trd>, el familio <ref tip="malprt"
       cel="mirtac.0oj">mirtacoj</ref>,
       el kies folioj oni preparis en a&ubreve;straliaj regionoj teon en la 18a
       jc., sed nuntempe uzataj por produkti
       odorantajn oleojn:
       <ekz>
        la plej multaj el la pli grandaj specioj <klr>[de la genro
        melale&ubreve;ko]</klr> anka&ubreve; estas konataj kiel <ctl><tld/>arboj</ctl>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=392191">Melale&ubreve;ko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/c/cd/Melaleuca_alternifolia_fruits.jpg/320px-Melaleuca_alternifolia_fruits.jpg"
    prm="CC BY-SA 4.0">bran&ccirc;o kun fruktokapsuloj de a&ubreve;stralia <tld/>arbo (Melaleuca alternifolia)
        <fnt>
          <aut>Geoff Derrin, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Melaleuca_alternifolia_fruits.jpg">&WCW;/File:Melaleuca_alternifolia_fruits.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Teebaum</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30E3;</trd>,
    <trd>&#33590;&#x306E;&#26408;  <pr>&#12385;&#12419;&#x306E;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_a;&c_j;&#x43D;&c_i;&c_j; &#x43A;&c_u;&c_shch;</trd>
</drv>
</art>
