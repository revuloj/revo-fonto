<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>te</rad>/o
</kap>
<drv mrk="te.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc num="1">
<dif>
Sekigitaj folioj de <tld/>arbo. 
</dif>
</snc>
<snc num="2">
<dif>
Trinka&jcirc;o preparata el ili, <tld/>infuza&jcirc;o. 
</dif>
</snc>
<trd lng="cs">&ccaron;aj</trd>
<trd lng="de">Tee</trd>
<trd lng="en">tea</trd>
<trd lng="fr">th&eacute;</trd>
<trd lng="nl">thee</trd>
<trd lng="pl">herbata</trd>
<trd lng="tr">&ccedil;ay</trd>
</drv>
<drv mrk="te.0arbo">
<kap><tld/>arbo</kap>
<uzo tip="fak">BOT</uzo>
<snc>
<dif>
Azia arbeto produktanta la <tld/>on (<trd lng="la">Camellia sinensis</trd>).  
</dif>
</snc>
<trd lng="de">Teestrauch</trd>
<trd lng="tr">&ccedil;ay</trd>
</drv>
</art>
