<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>te</rad>/o
</kap>
<drv mrk="te.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="te.0o.folioj">
    <dif>
      Sekigitaj folioj de <tld/>arbo.
    </dif>
  </snc>
  <snc mrk="te.0o.trinkajxo">
    <dif>
      <ref tip="super" cel="trink.0ajxo">Trinka&jcirc;o</ref> infuze preparita
      el folioj de <tld/>arbo, eventuale el alispecaj folioj:
      <ekz>
        &scirc;ian teon el milfolio kun pistita sova&gcirc;a rozo kaj fragaj folioj
        tie ne eblis servi<fnt><aut>Spomenka &Scirc;timec</aut>: <vrk>Morto en
        Melburna tramo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 8a,
        p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&ccaron;aj</trd>
  <trd lng="de">Tee</trd>
  <trd lng="en">tea</trd>
  <trd lng="fr">th&eacute;</trd>
  <trd lng="hu">tea</trd>
  <trd lng="nl">thee</trd>
  <trd lng="pl">herbata</trd>
  <trd lng="ru">&c_ch;&c_a;&c_j;</trd>
  <trd lng="sv">te</trd>
  <trd lng="tr">&ccedil;ay</trd>
</drv>

<drv mrk="te.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="kameli.0o.BOT">kamelio</ref>
      (<trd lng="la">Camellia sinensis</trd>), azia arbedo uzata por
      produktado de la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Teestrauch</trd>
  <trd lng="fr">th&eacute;ier</trd>
  <trd lng="hu">k&iacute;nai teafa</trd>
  <trd lng="nl">theestruik</trd>
  <trd lng="ru">&c_ch;&c_a;&c_j; <klr
  tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="tr">&ccedil;ay</trd>
</drv>
</art>
