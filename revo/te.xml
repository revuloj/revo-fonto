<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>te</rad>/o
</kap>

<drv mrk="te.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="te.0o.folioj">
    <dif>
      Sekigitaj folioj de <ref tip="malprt" cel="te.0arbo"><tld/>arbo</ref>:
      <ekz>
        la ofica salajro ne sufi&ccirc;as e&ccirc; por <tld/>o kaj sukero
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>

    </dif>
    <trd lng="zh">&#33590;&#21494;<klr> [ch&aacute;y&egrave;]</klr></trd>
  </snc>
  <snc mrk="te.0o.trinkajxo">
    <dif>
      <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="trink.0ajxo">Trinka&jcirc;o</ref> infuze preparita
      el folioj de <tld/>arbo, eventuale el alispecaj folioj:
      <ekz>
        &scirc;ian teon el milfolio kun pistita sova&gcirc;a rozo kaj fragaj folioj
        tie ne eblis servi
        <fnt>
          <aut>Spomenka &Scirc;timec</aut>: 
          <vrk>Morto en Melburna tramo</vrk>, 
          <bib>Monato</bib>
          <lok>jaro 1997a, numero 8a, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne malhelpas, se oni trinkas superfluan glason da<tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_b;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_a;&c_j;</trd>
  <trd lng="ca">te</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;aj</trd>,
    <trd>&ccaron;ajovn&iacute;k &ccaron;&iacute;nsk&yacute;</trd>
  </trdgrp>
  <trd lng="de">Tee</trd>
  <trd lng="en">tea</trd>
  <trd lng="es">t&eacute;</trd>
  <trd lng="fr">th&eacute;</trd>
  <trd lng="hu">tea</trd>
  <trd lng="id">teh</trd>
  <trd lng="nl">thee</trd>
  <trd lng="pl">herbata</trd>
  <trdgrp lng="pt">
   <trd>ch&aacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_j;</trd>
  <trd lng="sk">&ccaron;ajovn&iacute;k &ccaron;&iacute;nsky</trd>
  <trd lng="sv">te</trd>
  <trd lng="tr">&ccedil;ay</trd>
  <trd lng="zh">&#33590;<klr> [ch&aacute;]</klr></trd>
</drv>

<drv mrk="te.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> de <ref tip="malprt"
      cel="kameli.0o.BOT">kamelio</ref>
      (<trd lng="la">Camellia sinensis</trd>), azia arbedo uzata por
      produktado de la <tld/>o:
      <ekz>
        iun tagon, kiam li ripozis sub <tld/>arbo, folio falis en lian tason
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=4109'>Teo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej multaj el la pli grandaj specioj anka&ubreve; estas konataj kiel <ctl><tld/>arboj</ctl>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=392191'>Melale&ubreve;ko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">te</trd>
  <trd lng="de">Teestrauch</trd>
  <trd lng="en">tea tree</trd>
  <trd lng="fr">th&eacute;ier</trd>
  <trd lng="hu">k&iacute;nai teacserje</trd>
  <trd lng="nl">theestruik</trd>
  <trdgrp lng="pt">
   <trd>ch&aacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_j; <klr
  tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="tr">&ccedil;ay</trd>
</drv>
</art>
