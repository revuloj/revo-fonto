<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>flot</rad>/o
</kap>
<drv mrk="flot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc num="1">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Kelke da eskadroj sub la komando de admiralo.
    </dif>
  </snc>
  <snc num="2">
    <dif>
      La tuta &scirc;iparo de unu lando:
      <ekz>la komerca norvega <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="de">Flotte</trd>
  <trd lng="en">fleet</trd>
  <trd lng="fr">flotte<klr tip="ind"> (ensemble de navires)</klr></trd>
  <trd lng="nl">vloot</trd>
<trd lng="ru">&c_f;&c_l;&c_o;&c_t;</trd>
</drv>
<drv mrk="flot.aero0o">
  <kap>aero<tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      La tuta aviadilaro de unu lando.
    </dif>
  </snc>
  <trd lng="de">Luftflotte</trd>
  <trd lng="fr">flotte a&eacute;rienne</trd>
  <trd lng="nl">luchtvloot</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; &c_f;&c_l;&c_o;&c_t;</trd>,
    <trd>&c_a;&c_e;&c_r;&c_o;&c_f;&c_l;&c_o;&c_t;</trd>
  </trdgrp>
</drv>
</art>

