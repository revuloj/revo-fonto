<kap>
  <rad>papiliac</rad>/oj
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="papili1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Tradicia nomo de la familio
      <ref tip="dif" cel="fabac.0oj">fabacoj</ref>:
      <ekz>
        dudekkvarpunktan kokcinelon <klr>[&#8230;]</klr> oni trovas sur la <tld/>oj, sur terpomoj, betoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9459">Kokcinelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">mot&yacute;lokv&ecaron;t&eacute;</trd>
  <trd lng="de">Schmetterlingsbl&uuml;tengew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;velyesek</trd>,
    <trd>pillang&oacute;svir&aacute;g&uacute;ak</trd>
  </trdgrp>
  <trd lng="nl">vlinderbloemigen<klr tip="ind"> (arch.)</klr></trd>
  <trdgrp lng="pl">
    <trd>motylkowate</trd>,
    <trd>bobowate</trd>
  </trdgrp>
  <trd lng="pt">papilion&aacute;ceas</trd>
  <trd lng="ro">Fabaceae</trd>
  <trd lng="ru">&c_m;&c_o;&c_t;&c_y;&c_l;&c_mol;&c_k;&c_o;&c_v;&c_y;&c_je;<klr tip="amb">
  (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>
  <trd lng="sk">mot&yacute;&lcaron;okvet&eacute; <klr>(nov&scaron;ie: fabacoj)</klr></trd>
</drv>
