<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tremat.xml,v 1.11 2024/01/25 15:26:32 revo Exp $">
<kap>
  <rad>trematod</rad>/oj
</kap>
<drv mrk="tremat.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_klasoj" cel="klas.0o.BIO">Klaso</ref>
      el la filumo <ref tip="malprt" cel="verm.plat0oj">platvermoj</ref>
      (<trd lng="la">Trematoda</trd>), parazitantaj en a&ubreve; sur diversaj
      vertebruloj:
      <ekz>
        ovo sen &scirc;elo estas unu simptomo de malsano ka&ubreve;zata per la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6797">Libelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        studo pri 120 kadavra&jcirc;oj de Blankaj cikonioj el Saksio-Anhalto
        kaj Brandenburgo en Germanio montris ok speciojn de trematodoj, kvar
        speciojn de cestodoj, kaj almena&ubreve; tri speciojn de nematodoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17326">Blanka cikonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_k;&c_t;&c_u;&c_n;&c_y; <klr>(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>,
    <trd>&c_t;&c_r;&c_e;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Saugw&uuml;rmer</trd>
  <trd lng="en">trematode</trd>
  <trd lng="fr">tr&eacute;matodes</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;telyek</trd>,
    <trd>sz&iacute;v&oacute;f&eacute;rgek</trd></trdgrp>
  <trd lng="nl">zuigwormen</trd>
  <trd lng="pl">przywry</trd>
  <trd lng="pt">tremat&oacute;deos</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_m;&c_a;&c_t;&c_o;&c_d;&c_y; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_g;&c_je;&#x43D;&c_je;&c_t;&c_i;&c_ch;&#x43D;&c_ib; &#x43F;&c_r;&c_i;&c_s;&c_i;&c_s;&#x43D;&c_ib;</trd>,
    <trd>&c_t;&c_r;&c_je;&#x43C;&c_a;&c_t;&#x43E;&c_d;&c_i;</trd>
  </trdgrp>
</drv>
</art>
