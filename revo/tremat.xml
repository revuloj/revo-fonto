<vortaro>
<art mrk="$Id$">
<kap>
<rad>trematod</rad>/oj
</kap>
<drv mrk="tremat.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso el la filumo <ref tip="super" cel="verm.plat0oj">platvermoj</ref>
      (<trd lng="la">Trematoda</trd>), parazitantaj en a&ubreve; sur diversaj
      vertebruloj.
    </dif>
  </snc>
  <trd lng="de">Saugw&uuml;rmer</trd>
  <trd lng="en">trematode</trd>
</drv>
</art>

