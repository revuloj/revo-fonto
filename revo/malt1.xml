<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>malt</rad>/o
</kap>

<drv mrk="malt1.0o">
  <kap><tld/>o</kap>
  <snc mrk="malt1.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="malt1.M0o"><tld lit="M"/>o</ref>.
    </dif>
  </snc>
  <snc mrk="malt1.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="pt">malt&ecirc;s <klr tip="ind"> (subst.)</klr></trd>
</drv>

<drv mrk="malt1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="pt">malt&ecirc;s <klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="malt1.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="malt1.M0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la samnoma 
      insulo kun &ccirc;efurbo Valeto.
      Teritorio de Respubliko Malto.
      ISO-3166 kodo: MT.
    </dif>
    <trdgrp lng="pl">
      <trd>Malta</trd>,
      <trd>Republika <ind>Malty</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="malt1.M0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insulo en <ref tip="vid" cel="mediterane.M0o.GEOG">
      Mediteranea Maro</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Malta</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_M;&c_a;&c_l;&c_t;&c_a;</trd>
  <trd lng="de">Malta</trd>
  <trd lng="en">Malta</trd>
  <trd lng="nl">Malta</trd>
  <trd lng="pt">Malta</trd>
</drv>

</art>

