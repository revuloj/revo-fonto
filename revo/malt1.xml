<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>malt</rad>/o
</kap>

<drv mrk="malt1.0o">
  <kap><tld/>o</kap>
  <snc mrk="malt1.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="malt1.M0o"><tld lit="M"/>o</ref>.
    </dif>
  </snc>
  <snc mrk="malt1.0o.naciano">
    <dif>
      Homo kun <tld/>a <ref tip="lst" cel="naci.0o"
      lst="voko:popolanoj">nacieco</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_t;&c_y;&c_je;&c_c;</trd>
  <trd lng="ca">malt&eacute;s <klr>(subst.)</klr></trd>
  <trd lng="pt">malt&ecirc;s <klr tip="ind"> (subst.)</klr></trd>
</drv>

<drv mrk="malt1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_t;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">malt&eacute;s <klr>(adj.)</klr></trd>
  <trd lng="pt">malt&ecirc;s <klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="malt1.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="malt1.M0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>MT</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&Scirc;tato</ref> 
      en la samnoma 
      insulo kun &ccirc;efurbo <ref tip="prt" cel="valet.0o">Valeto</ref>,
      teritorio de Respubliko Malto.
    </dif>
    <trdgrp lng="pl">
      <trd>Malta</trd>,
      <trd>Republika <ind>Malty</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="malt1.M0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      en <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteranea
      Maro</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Malta</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_l;&c_mol;&c_t;&c_a;</trd>
  <trd lng="bg">&c_M;&c_a;&c_l;&c_t;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>Malta</trd>,
    <trd>Rep&uacute;blica de Malta</trd>
  </trdgrp>
  <trd lng="de">Malta</trd>
  <trd lng="en">Malta</trd>
  <trd lng="nl">Malta</trd>
  <trd lng="pt">Malta</trd>
</drv>

</art>

