<art mrk="$Id: otoman1.xml,v 1.1 2025/03/09 22:24:51 afido Exp $">
<kap>
    <rad>otoman</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="otoman1.0o">
  <kap><tld/>o</kap>
  <snc mrk="otoman1.0o.MEBL">
    <dif>
      <ref tip="dif" cel="divan.0o.MEBL">Divano</ref>:
      <ekz>
        li denove sidi&gcirc;is sur la <tld/>o
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>
          <vrk>Sub Cirkotendo</vrk>
          <lok>1969</lok>
        </fnt>.
       </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_t;&c_a;&c_h;&c_t;&c_a;</trd>
    <trdgrp lng="de">
       <trd>Diwan</trd>,
       <trd>Ruhebett</trd>,
       <trd>niedriges <ind>Liegesofa</ind></trd>
    </trdgrp>
    <trd lng="en">ottoman</trd>
    <trd lng="fr">divan<klr> (si&egrave;ge)</klr></trd>
    <trdgrp lng="hu">
       <trd>d&iacute;v&aacute;ny</trd>,
       <trd>kerevet</trd>,
       <trd>pamlag</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>otomana</trd>
    </trdgrp>
    <trd lng="pt">div&atilde;</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_o;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
</drv>
