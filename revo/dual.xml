<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dual.xml,v 1.44 2025/08/01 20:36:03 revo Exp $">
<kap>
  <rad>dual</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="dual.0o">
  <kap><tld/>o</kap>
  <snc mrk="dual.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <ref tip="dif" cel="nombr.du0o">dunombro</ref>
    <trd lng="be">&c_p;&c_a;&c_r;&c_n;&c_y; <ind>&c_l;&c_ib;&c_k;</ind></trd>
    <trd lng="ca">dual (gram.)</trd>
    <trdgrp lng="de">
      <trd>Dual</trd>,
      <trd>Zweizahl</trd>
    </trdgrp>
    <trd lng="es">dual</trd>
    <trd lng="fr">duel<klr tip="ind"> (nombre)</klr></trd>
    <trdgrp lng="hu">
      <trd>kett&odblac;s sz&aacute;m</trd>,
      <trd>du&aacute;lis</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>liczba podw&oacute;jna</trd>,
      <trd>dualis</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je;
    &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
    <trd lng="zh">&#21452;&#25968; <pr>shu&amacron;ngsh&ugrave;</pr></trd>
  </snc>
  <snc mrk="dual.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <ref cel="dual.algebra0o">Algebra dualo</ref> a&ubreve;
      <ref cel="dual.topologia0o">topologia dualo</ref>,
      la&ubreve; la kunteksto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a; &c_s;&c_p;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a; &c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;</trd>
    </trdgrp>
    <trd lng="ca">espai dual</trd>
    <trdgrp lng="de">
      <trd>Dual</trd>,
      <trd>Dualraum</trd>
    </trdgrp>
    <trd lng="en">dual <ind>space</ind></trd>
    <trd lng="fr">dual<klr tip="ind"> (subst., math.)</klr></trd>
    <trdgrp lng="hu">
      <trd>du&aacute;lis</trd>,
      <trd>du&aacute;lis t&eacute;r</trd>
    </trdgrp>
    <trd lng="pl">przestrze&nacute; dualna</trd>
    <trd lng="pt">dual</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_je;
      <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>,
      <trd>&c_d;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je;
      <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>,
      <trd>&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
      <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>du&aacute;l</trd>,
    <trd>dvojn&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21452;&#25968;  <pr>&#12381;&#12358;&#12377;&#12358;</pr></trd>,
    <trd>&#20001;&#25968;  <pr>&#12426;&#12423;&#12358;&#12377;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="sk">du&aacute;l</trd>
  <trd lng="uk">&c_l;&c_ib;&c_n;&c_gu;&c_v;. &c_d;&c_v;&c_o;&c_ji;&c_n;&c_a;</trd>
</drv>

<drv mrk="dual.0a">
  <kap><tld/>a</kap>
  <snc mrk="dual.0a.gxenerale">
    <dif>
      Rilata al <tld/>o<sncref ref="dual.0o.GRA"/> a&ubreve;
      <sncref ref="dual.0o.MAT"/>:
      <ekz>
        <ref cel="baz.duala0o"><tld/>a bazo</ref>;
      </ekz>
      <ekz>
        <tld/>a <ref cel="nombr.0o.GRA">nombro</ref>.
      </ekz>
    </dif>
    <trd lng="pl">dualny</trd>
  </snc>
  <snc mrk="dual.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 3,
    &leftquot;dualeco&rightquot;</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      <klr>(p.p. matematikaj nocioj)</klr>
      Tiaj, ke ili iel spegulas unu la alian:
      <ekz>
        la operacioj <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref>
        kaj <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref> estas
        <tld/>aj inter si;
      </ekz>
      <ekz>
        oni diras, ke du vektoraj spacoj estas <tld/>aj inter si, se
        &ccirc;iu el ili estas egala a&ubreve; izomorfia al la
        <ref cel="dual.0o.MAT"><tld/>o</ref> de la alia.
      </ekz>
    </dif>
    <ref tip="vid" cel="latis.0o.MAT">latiso.</ref>
    <trd lng="de">dual</trd>
    <trd lng="en">dual</trd>
    <trd lng="fr">dual<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="pl">dualny</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#20108;&#36827; <pr>&egrave;rj&igrave;n</pr></trd>
  </snc>
  <snc mrk="dual.0a.dualisma">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref cel="dualis.0o">Dualism</ref>a:
      <ekz>
        la <tld/>eco i&gcirc;as unueco: ne estas milito sen rompo de paco, ne estas paco sen
        milita konkero
        <fnt>
       <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2000/006861.php">Milito kaj paco</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>dueca</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_v;&c_o;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">dual</trd>
  <trdgrp lng="cs">
    <trd>du&aacute;ln&iacute;</trd>,
    <trd>dvojn&yacute;</trd>
  </trdgrp>
  <trd lng="he">&kaf;&pe;&vav;&lamed;</trd>
  <trd lng="hu">du&aacute;lis</trd>
  <trdgrp lng="ja">
    <trd>&#20108;&#37325;&#12398;  <pr>&#12405;&#12383;&#12376;&#12421;&#12358;&#12398;</pr></trd>,
    <trd>&#20108;&#20803;&#30340;&#12394;  <pr>&#12405;&#12383;&#12370;&#12435;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#21452;&#23550;&#12398;  <pr>&#12381;&#12358;&#12383;&#12356;&#12398;</pr></trd>,
    <trd>&#21452;&#25968;&#12398;  <pr>&#12381;&#12358;&#12377;&#12358;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="sk">dvojkov&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_d;&c_v;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="dual.du0o">
  <kap>du<tld/>o, <var><kap>duobla <tld/>o</kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="dual.0o.MAT"><tld lit="D"/>o</ref> de la <tld/>o.
    </dif>
    <ref tip="sin" cel="bidual.0o">bidualo.</ref>
    <rim>
      Ni ne trovis a&ubreve;toritatan fonton por la formo
      <ctl>du<tld/>o</ctl>, sed &gcirc;i sekvas la skemisman modelon de
      <ctl>dubemolo</ctl>, <ctl>dulorneto</ctl>, <ctl>dusako</ctl> ks
      kaj tial evitigas enkonduki la radikon <ctl>bidual</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a; &c_b;&c_ib;&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;</trd>
  <trd lng="ca">espai bidual</trd>
  <trd lng="cs">bidu&aacute;l</trd>
  <trdgrp lng="de">
    <trd>Bidual</trd>,
    <trd>Bidualraum</trd>
  </trdgrp>
  <trd lng="en">double <ind>dual</ind></trd>
  <trd lng="fr">bidual</trd>
  <trdgrp lng="hu">
    <trd>bidu&aacute;lis</trd>,
    <trd>bidu&aacute;lis t&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przestrze&nacute; bidualna</trd>,
    <trd>przestrze&nacute; podw&oacute;jnie dualna</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
  <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
  <trd lng="sk">bidu&aacute;l</trd>
</drv>

<drv mrk="dual.algebra0o">
  <kap>algebra <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref tip="super" cel="spac.vektora0o">vektora spaco</ref>
      <frm><g>E</g></frm>)</klr>
      Vektora spaco, konsistanta el &ccirc;iuj
      <ref cel="linear.0a.homomorfio">linearaj</ref>
      <ref cel="form.0o.bildigo">formoj</ref> super <frm><g>E</g></frm>:
      <ekz>
        finidimensia spaco kaj &gcirc;ia <tld/>o havas la saman
        dimension kaj estas izomorfiaj;
      </ekz>
      <ekz>
        la algebran <tld/>on de <frm><g>E</g></frm> oni ofte signas per
        <frm><g>E</g><sup>*</sup></frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a; &c_s;&c_p;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_a;&c_ja; &c_a;&c_l;&c_mol;&c_g;&c_je;&c_b;&c_r;&c_a;&c_ib;&c_ch;&c_n;&c_a;&c_ja;</trd>
  <trd lng="ca">espai dual</trd>
  <trd lng="de">algebraischer <ind>Dual</ind></trd>
  <trd lng="en">full dual <ind>space</ind></trd>
  <trd lng="fr">dual alg&eacute;brique</trd>
  <trd lng="hu">algebrai <ind>du&aacute;lis</ind> t&eacute;r</trd>
  <trdgrp lng="pl">
    <trd>dual algebraiczny</trd>,
    <trd>podw&oacute;jna przestrze&nacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_g;&c_je;&c_b;&c_r;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  &c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_je;
  <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>

<drv mrk="dual.topologia0o">
  <kap>topologia <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de
      <ref cel="topolo.0a.modulo">topologia</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>
      <frm><g>E</g></frm>)</klr>
      Topologia vektora spaco, konsistanta el &ccirc;iuj
      <ref cel="kontin3.0a.cxie">kontinuaj</ref>
      <ref cel="linear.0a.homomorfio">linearaj</ref>
      <ref cel="form.0o.bildigo">formoj</ref> super <frm><g>E</g></frm>:
      <ekz>
        la topologian <tld/>on de <frm><g>E</g></frm> oni foje signas
        per
        <frm><g>E</g>'</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a; &c_s;&c_p;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_a;&c_ja; &c_t;&c_a;&c_p;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_ja;</trd>
  <trd lng="ca">espai dual topol&ograve;gic</trd>
  <trd lng="de">topologischer <ind>Dual</ind></trd>
  <trd lng="en">adjoint <ind>space</ind></trd>
  <trd lng="fr">dual topologique</trd>
  <trd lng="hu">topologikus <ind>du&aacute;lis</ind></trd>
  <trdgrp lng="pl">
    <trd>dual topologiczny</trd>,
    <trd>przestrze&nacute; sprz&eogonek;&zdot;ona</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  &c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_je;
  <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>

<drv mrk="dual.topologiadu0o">
  <kap>topologia du<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de topologia vektora spaco <frm><g>E</g></frm>)</klr>
      <ref tip="super" cel="dual.topologia0o">Topologia <tld/>o</ref> de
      &gcirc;ia topologia <tld/>o:
      <ekz>
        la topologian du<tld/>on de <frm><g>E</g></frm> oni foje signas
        per
        <frm><g>E</g>"</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="dual.du0o">du<tld/>o</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a; &c_s;&c_p;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_a;&c_ja; &c_t;&c_a;&c_p;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_ja; &c_d;&c_r;&c_u;&c_g;&c_a;&c_ja;</trd>
  <trd lng="ca">espai bidual topol&ograve;gic</trd>
  <trd lng="de">topologischer <ind>Bidual</ind></trd>
  <trd lng="en">second adjoint <ind>space</ind></trd>
  <trd lng="fr">bidual topologique</trd>
  <trd lng="hu">topologikus <ind>bidu&aacute;lis</ind></trd>
  <trd lng="pl">druga <ind>przestrze&nacute;</ind> sprz&eogonek;&zdot;ona</trd>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_o;&c_je;
  &c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  &c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_je;
  <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>
</art>
