<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dual</rad>/o
</kap>
<drv mrk="dual.0o">
  <kap><tld/>o</kap>
  <snc mrk="dual.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Gramatika nombro (formo deklinacia kaj konjugacia), kiu en
      kelkaj lingvoj (i.a. greka, hebrea, Slavono, slovena) estas
      uzata, kiam oni parolas pri du personoj a&ubreve; du aferoj.
    </dif>
    <ref tip="sin" cel="nombr.du0o">dunombro</ref>
    <refgrp tip="vid">
      <ref cel="singul.0o">singularo</ref>,
      <ref cel="plural.0o">pluralo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Dual</trd>,
      <trd>Zweizahl</trd>
    </trdgrp>
    <trd lng="es">dual</trd>
    <trd lng="fr">duel<klr tip="ind"> (nombre)</klr></trd>
    <trd
    lng="ru">&#x434;&#x432;&#x43e;&#x439;&#x441;&#x442;&#x432;&#x435;&#x43d;&#x43d;&#x43e;&#x435;
    &#x447;&#x438;&#x441;&#x43b;&#x43e;</trd>
  </snc>
  <snc mrk="dual.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <ref cel="dual.algebra0o">Algebra dualo</ref> a&ubreve;
      <ref cel="dual.topologia0o">topologia dualo</ref>,
      la&ubreve; la kunteksto.
    </dif>
    <trdgrp lng="de">
      <trd>Dual</trd>,
      <trd>Dualraum</trd></trdgrp> 
    <trd lng="en">dual <ind>space</ind></trd>
    <trd lng="fr">dual<klr tip="ind"> (subst., math.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_n;&c_o;&c_je; <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>,
      <trd>&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="dual.0a">
  <kap><tld/>a</kap>
  <snc mrk="dual.0a.gxenerale">
    <dif>
      Rilata al <tld/>o <sncref ref="dual.0o.GRA"/> a&ubreve;
      <sncref ref="dual.0o.MAT"/>:
      <ekz>
        <ref cel="baz.duala0o"><tld/>a bazo</ref>;
      </ekz>
      <ekz>
        <tld/>a <ref cel="nombr.0o.gra">nombro</ref>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="dual.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 3, sub formo "dualeco"</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      <klr>(p.p. matematikaj nocioj)</klr>
      Tiaj, ke ili iel spegulas unu la alian:
      <ekz>
        la operacioj <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref>
        kaj <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref> estas
        <tld/>aj inter si;
      </ekz>
      <ekz>
        oni diras, ke du vektoraj spacoj estas <tld/>aj inter si, se
        &ccirc;iu el ili estas la <tld/>o, a&ubreve; izomorfia al la
        <tld/>o, de la alia.
      </ekz>
    </dif>
    <trd lng="fr">dual<klr tip="ind"> (adj.)</klr></trd>
  </snc>
</drv>

<drv mrk="dual.du0o">
  <kap>du<tld/>o, duobla <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <tld lit="D"/>o de la <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bidual</trd>,
    <trd>Bidualraum</trd></trdgrp> 
  <trd lng="en">double <ind>dual</ind></trd>
  <trd lng="fr">bidual</trd>
  <trd lng="ru">&c_b;&c_i;&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>

<drv mrk="dual.algebra0o">
  <kap>algebra <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de vektora spaco <em>E</em>)</klr>
      <ref tip="super" cel="spac.vektora0o">Vektora spaco</ref>,
      konsistanta el &ccirc;iuj
      <ref cel="linear.0a.homomorfio">linearaj</ref>
      <ref cel="form.0o.bildigo">formoj</ref> super <em>E</em>:
      <ekz>
        finidimensia spaco kaj &gcirc;ia <tld/>o havas la saman
        dimension kaj estas izomorfiaj;
      </ekz>
      <ekz>
        la algebran <tld/>on de <em>E</em> oni ofte signas per
        <em>E</em><sup>*</sup>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">algebraischer <ind>Dual</ind></trd>
  <trd lng="en">full dual <ind>space</ind></trd>
  <trd lng="fr">dual alg&eacute;brique</trd>
  <trd lng="ru">&c_a;&c_l;&c_g;&c_je;&c_b;&c_r;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_n;&c_o;&c_je; <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>

<drv mrk="dual.topologia0o">
  <kap>topologia <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de topologia vektora spaco <em>E</em>)</klr>
      <ref cel="topolo.0a.modulo">Topologia</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>,
      konsistanta el &ccirc;iuj
      <ref cel="kontin3.0a.cxie">kontinuaj</ref>
      <ref cel="linear.0a.homomorfio">linearaj</ref>
      <ref cel="form.0o.bildigo">formoj</ref> super <em>E</em>:
      <ekz>
        la topologian <tld/>on de <em>E</em> oni foje signas per
        <em>E</em>'.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">topologischer <ind>Dual</ind></trd>
  <trd lng="en">adjoint <ind>space</ind></trd>
  <trd lng="fr">dual topologique</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_n;&c_o;&c_je; <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>
</art>
