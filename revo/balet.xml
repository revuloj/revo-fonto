<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>balet</rad>/o
</kap>
<drv mrk="balet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Teatra akto kun danco kaj muziko kaj mimiko, sen paroloj:
      <ekz>
        la baleto
        <ctl>La konsekro de la printempo</ctl> de Igor Stravinskij,
        surscenigita en Parizo en 1913<fnt><aut>Natalia
        Arolovi&ccirc;</aut>:
        <vrk>La homo de la kosma konscio</vrk>,
        <bib>Monato</bib><lok>jaro
        2002a, numero 7a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_je;&c_t;</trd>
  <trd lng="cs">balet <klr>(tanec i skladba)</klr></trd>
  <trd lng="de">Ballet</trd>
  <trd lng="en">ballet</trd>
  <trd lng="es">ballet</trd>
  <trd lng="fa">&a_b;&a_A;&a_l;&a_h;</trd>
  <trd lng="fr">ballet</trd>
  <trd lng="hu">balett</trd>
  <trd lng="nl">ballet</trd>
  <trd lng="pl">balet</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">balet</trd>
  <trd lng="sv">balett</trd>
</drv>
</art>
