<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ranc.xml,v 1.18 2022/02/08 23:30:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ranc</rad>/a
</kap>
<drv mrk="ranc.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <uzo tip="klr">(pp grasoj)</uzo>
    <dif>
      Ricevinta akran malagrablan odoron kaj guston pro komenco de
      malkomponi&gcirc;o:
      <ekz>
        <tld/>a butero, lardo, oleo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_o;&c_r;&c_k;&c_l;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>zka&zcaron;en&yacute;</trd>,
    <trd>&zcaron;lukl&yacute;</trd>
  </trdgrp>
  <trd lng="de">ranzig</trd>
  <trd lng="en">rancid</trd>
  <trd lng="es">rancio</trd>
  <trd lng="fr">rance</trd>
  <trd lng="hu">avas</trd>
  <trd lng="io">ranca</trd>
  <trdgrp lng="ja">
    <trd>&#37240;&#25943;&#12375;&#12383; [<ind>&#12373;&#12435;&#12401;&#12356;&#12375;&#12383;</ind>]</trd>,
    <trd>&#39248;&#12360;&#12383; [<ind>&#12377;&#12360;&#12383;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ranzig</trd>,
    <trd>rans</trd>
  </trdgrp>
  <trd lng="pl">zje&lstroke;cza&lstroke;y</trd>
  <trd lng="pt">ran&ccedil;oso</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_o;&c_r;&c_k;&c_l;&c_y;&c_j;</trd>
  <trd lng="sk">stuchnut&yacute; <klr>(tuk)</klr></trd>
  <trd lng="sv">h&auml;rsken</trd>
</drv>
</art>
