<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tonik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tonik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="medika1.0o">Medikamento</ref> kun vigliga,
      fortiga efiko, kontra&ubreve; debilo:
      <ekz>
        mi donu al &scirc;i <tld/>on por &scirc;in refortigi
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, p. 567a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tonique<klr> (subst.)</klr></trd>
</drv>

<drv mrk="tonik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Vigliga, refortiga.
    </dif>
  </snc>
  <trd lng="fr">tonique<klr> (adj.)</klr></trd>
</drv>
</art>

<!--
