<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tonik</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tonik.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="medika1.0o">Medikamento</ref> kun vigliga,
      fortiga efiko, kontra&ubreve; korpa malforto:
      <ekz>
        mi donu al &scirc;i <tld/>on por &scirc;in refortigi
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        567a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tonique<klr> (subst.)</klr></trd>
  <trd lng="pt">t&ograve;nico</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_je;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="tonik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Vigliga, refortiga.
    </dif>
  </snc>
  <trd lng="fr">tonique<klr> (adj.)</klr></trd>
  <trd lng="pt">tonificante</trd>
  <trd lng="ru">&c_t;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
</drv>
</art>
