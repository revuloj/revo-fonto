<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: saksifrag.xml,v 1.17 2023/06/30 21:26:11 revo Exp $">
<kap>
  <rad>saksifragac</rad>/oj
</kap>
<drv mrk="saksifrag.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>      
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref> de herboj
      unujaraj a&ubreve; sta&ubreve;doj
      el la ordo de saksifragoparencaj
      <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Saxifragaceae</trd>) kun:
      folioj kruce alternaj a&ubreve; malofte kontra&ubreve;aj, sen kromfolioj;
      floroj duseksaj, stelaj;
      petaloj ofte kvin;
      du ovolujoj supraj, kunkreskintaj &ccirc;e-baze;
      kapsuloj:
      <ekz>
        &scirc;a&ubreve;mfloroj (tiarelo) estas genro de naturfloroj kaj
        &gcirc;ardenplantoj 
        trovi&gcirc;antaj en Azio kaj Nordameriko <klr>[el]</klr> <tld lit="S"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=427951">Tiarella</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bergenio estas palearktisa sta&ubreve;da genro de <tld/>oj ofte uzata
        kiel ornamplanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=477203">Bergenia</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_l;&c_o;&c_m;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">saxifrag&agrave;cies</trd>
  <trd lng="de">Steinbrechgew&auml;chse</trd>
  <trd lng="en">saxifrage</trd>
  <trd lng="fr">saxifrage</trd>
  <trd lng="hu">k&odblac;t&ouml;r&odblac;f&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="it">sassifragacee</trd>
  <trd lng="nl">steenbreekfamilie</trd>
  <trd lng="pl">skalnicowate</trd>
  <trd lng="pt">saxifrag&aacute;ceas</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_n;&c_je;&c_l;&c_o;&c_m;&c_k;&c_o;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43B;&#x43E;&#x43C;&c_i;&#x43A;&c_a;&#x43C;&c_je;&#x43D;&c_je;&c_v;&c_ib;</trd>
</drv>
</art>
