<vortaro>
<art mrk="$Id$">
<kap>
  <rad>miristik</rad>/o
</kap>
<drv mrk="mirist.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mirist1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Myristica</trd>), kun la specio
      <ref tip="sub" cel="muskat.0arbo">muskatujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Muskatbaum</trd>
  <trd lng="nl">muskaatboom</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
