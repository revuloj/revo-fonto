<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>miristik</rad>/o
</kap>
<drv mrk="mirist.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mirist.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Myristica</trd>), kun la specio
      <ref tip="sub" cel="muskat.0arbo">muskatujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Muskatbaum</trd>
</drv>
<drv mrk="mirist.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj, &ccirc;iamverdaj,
      tropikaj lignoplantoj
      (<trd lng="la">Myristicaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Muskatgew&auml;chse</trd>
</drv>
</art>
