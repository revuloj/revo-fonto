  <trd lng="hu">&aacute;llam</trd>
  <trd lng="id">negara</trd>
  <trd lng="it">stato<klr> (pol.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#22269;&#23478;  <pr>&#12371;&#12387;&#12363;</pr></trd>,
    <trd>&#22269;  <pr>&#12367;&#12395;</pr></trd>,
    <trd>&#25919;&#24220;  <pr>&#12379;&#12356;&#12405;</pr></trd>
  </trdgrp>
