  <trd lng="pl">nacjonalizacja</trd>
  <trdgrp lng="pt">
    <trd>estatiza&ccedil;&atilde;o</trd>
    <trd>nacionaliza&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
