  <trd lng="hu">&aacute;llam</trd>
  <trd lng="it">stato<klr> (pol.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x56FD;&#x5BB6;  <pr>&#x3053;&#x3063;&#x304B;</pr></trd>,
    <trd>&#x56FD;  <pr>&#x304F;&#x306B;</pr></trd>,
    <trd>&#x653F;&#x5E9C;  <pr>&#x305B;&#x3044;&#x3075;</pr></trd>
