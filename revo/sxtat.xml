    <uzo tip="fak">POL</uzo>
    <dif>
      Transmeto de io el privata en <tld/>an posedon
      a&ubreve; administradon:
      <ekz>
        en 1929, dum al<tld/>igo de la kolektoj de akad. <nom>N.P. Li&hcirc;a&ccirc;ov</nom>,
        prof. <nom>A.S. Orlov</nom> erare klasis pergamenan psalmaron etiopan kiel manuskripton
        glagolican
        <fnt>
          <aut>Sergio Pokrovskij</aut>
          <vrk><url ref="https://kovro.heliohost.org/eo/artikoloj/Slavonaj_skriboj.pdf">Slavonaj
          skriboj</url></vrk>
          <lok>La Ondo de Esperanto, 1998:12 kaj 1999:1&ndash;4</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="sxtat.de0igo.POL">de<tld/>igo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
