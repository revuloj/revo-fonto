      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_f;&c_e;&c_r;&c_n;&c_a;&c_ja; <ind>&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_v;&c_a;</ind></trd>
