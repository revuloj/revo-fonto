<drv mrk="sxtat.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sxtat.0isto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="&Scirc;"/>reganto, precipa a&ubreve;
      iu el la precipaj estroj de &scirc;tato:
      prezidanto, &ccirc;efministro...:
      <ekz>
        vi estas samtempe <tld/>isto kaj budha sacerdoto.
        &Ccirc;u vi opinias, ke religio kaj &scirc;tato devas kaj
        povas esti institucie kunligitaj?
        <fnt>
          <aut>D. Pili&cacute;, S. Dalmacija</aut>,
          <vrk><url ref="http://www.radicalparty.org/eo/node/5071760">
          Intervjuo: Profesoro Samdhong Rinpoche</url></vrk>,
          <lok>Partio transpartio Radikala, 2004-11-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">estadista</trd>
  <trd lng="en">statesman</trd>
  <trd lng="fr">homme d'&Eacute;tat</trd>
  <trdgrp lng="id">
    <trd>negarawan</trd>,
    <trd><ind>ahli</ind> kenegaraan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25919;&#27835;&#23478;  <pr>&#12379;&#12356;&#12376;&#12363;</pr></trd>,
    <trd>&#22269;&#23478;&#20027;&#32681;&#32773;  <pr>&#12371;&#12387;&#12363;&#12375;&#12421;&#12366;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="pl">polityk</trd>
  <trd lng="pt">estadista</trd>
  <trdgrp lng="uk">
