<drv mrk="sxtat.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="sxtat.0ano.POL">
    <dif>
      Ano de <tld/>o:
      <ekz>
        ne &ccirc;iu <tld/>ano estas plenrajta
        <ref tip="sub" cel="civit.0ano">civitano</ref>;
      </ekz>
      <ekz>
        &ccirc;u ekzistas ia ajn pli deca destino por angla <tld/>ano, ol mar&scirc;adi sub la
        standardo de Lia Majesta Mo&scirc;to
        <fnt>
          <bib>VaK</bib>
          <lok>Unua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        poemoj de <nom>Eugeniusz Matkowski</nom> (anta&ubreve;e: <nom>Matwiejczuk</nom>), Ukrajnano, nun
        pollanda
        <tld/>ano
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="reg.0ato">regato</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_n;</trd>,
