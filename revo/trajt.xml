<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trajt</rad>/o
</kap>
<drv mrk="trajt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Linio de la viza&gcirc;o:
      <ekz>
        belaj, severaj <tld/>oj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gesichtszug</trd>
    <trd lng="hu">arcvon&aacute;s</trd>
    <trd lng="nl">gelaatstrek</trd>
    <trd lng="es">facci&oacute;n<klr> (del rostro)</klr></trd>
    <trdgrp lng="pt">
      <trd>tra&ccedil;o</trd>,
      <trd>fei&ccedil;&atilde;o</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Karakteriza eco:
      <ekz>
        forgeso pri si mem estas la &ccirc;efa <tld/>o de lia
        karaktero<fnt>B</fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa <tld/>o de la egipta klimato estas la varmego<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi rakontos al vi <ind>en &gcirc;eneralaj <tld/>oj</ind> la historion de la naski&gcirc;o de la lingvo<fnt>Z</fnt>;
        <trd lng="es">a grandes rasgos</trd>
      </ekz>
      <ekz>
        subite la <tld/>aro de l' afero alii&gcirc;is.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Charakterzug</trd>,
      <trd>Eigenschaft</trd>
    </trdgrp>
    <trd lng="hu">jellemvon&aacute;s</trd>    
    <trd lng="es">rasgo caracter&iacute;stico, -a</trd>    
    <trd lng="nl">karaktertrek</trd>
  </snc>
  <snc>
    <fnt>Z</fnt>
    <dif>
      Ago rimarkinda kiel ekzemplo:
      <ekz>
        admirinda <tld/>o de kura&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zug</trd>,
      <trd>Eigenheit</trd>
    </trdgrp>
    <trd lng="hu">ismertet&odblac;jegy</trd>    
    <trd lng="nl">trek</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_s;&c_a; <klr tip="amb">(&c_t;&c_v;&c_a;&c_r;&c_u;, &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;&c_u; &c_i; &c_d;&c_a; &c_g;.&c_p;.)</klr></trd>,
    <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_e;&c_r;&c_n;&c_a;&c_ja; &c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">rasgo</trd>    
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_t;&c_a; <klr tip="amb">(&c_l;&c_i;&c_c;&c_a;, &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
</drv>
</art>
