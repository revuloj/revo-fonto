<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trajt</rad>/o
</kap>
<drv mrk="trajt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Linio de la viza&gcirc;o:
      <ekz>
        belaj, severaj <tld/>oj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gesichtszug</trd>
    <trd lng="nl">gelaatstrek</trd>
  </snc>
  <snc>
    <dif>
      Karakteriza eco:
      <ekz>
        forgeso pri si mem estas la &ccirc;efa <tld/>o de lia
        karaktero<fnt>B</fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa <tld/>o de la egipta klimato estas la varmego<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi rakontos al vi en &gcirc;eneralaj <tld/>oj la historion de la naski&gcirc;o
        de la lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        subite la <tld/>aro de l' afero alii&gcirc;is.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Charakterzug</trd>,
      <trd>Eigenschaft</trd>
    </trdgrp>
    <trd lng="nl">karaktertrek</trd>
  </snc>
  <snc>
    <fnt>Z</fnt>
    <dif>
      Ago rimarkinda kiel ekzemplo:
      <ekz>
        admirinda <tld/>o de kura&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zug</trd>,
      <trd>Eigenheit</trd>
    </trdgrp>
    <trd lng="nl">trek</trd>
  </snc>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_t;&c_a; <klr tip="amb">(&c_l;&c_i;&c_c;&c_a;, &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
</drv>
</art>
