<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trajt.xml,v 1.20 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trajt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trajt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Linio de la <ref tip="vid" cel="vizagx.0o">viza&gcirc;o</ref>:
      <ekz>
        belaj, severaj <tld/>oj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gesichtszug</trd>
    <trd lng="en">line <klr>(of face)</klr></trd>
    <trd lng="hu">arcvon&aacute;s</trd>
    <trd lng="nl">gelaatstrek</trd>
    <trd lng="es">facci&oacute;n<klr> (del rostro)</klr></trd>
    <trd lng="pl">rysy<klr tip="amb"> twarzy</klr></trd>
    <trdgrp lng="pt">
      <trd>tra&ccedil;o</trd>,
      <trd>fei&ccedil;&atilde;o</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Karakteriza <ref tip="vid" cel="ec.0o.KLA">eco</ref>:
      <ekz>
        forgeso pri si mem estas la &ccirc;efa <tld/>o de lia
        karaktero<fnt>B</fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa <tld/>o de la egipta klimato estas la varmego
        <fnt>B</fnt>;
      </ekz>
      <ekz>
        mi rakontos al vi <ind>en &gcirc;eneralaj <tld/>oj</ind> 
        la historion de la naski&gcirc;o de la lingvo<fnt>Z</fnt>;
        <trd lng="es">a grandes rasgos</trd>
        <trdgrp lng="pl">
          <trd>w og&oacute;lnych <ind>zarysach</ind></trd>,
          <trd>w zarysie</trd>,
          <trd>szkicowo</trd>
        </trdgrp>
      </ekz>
      <ekz>
        subite la <tld/>aro de l' afero alii&gcirc;is;
        <trd lng="pl">okoliczno&sacute;ci<klr tip="amb"> sprawy</klr>
        </trd>    
      </ekz>
      <ekz>
        ili draste reduktas la <tld/>ojn de la reprezentata afero: 
        literoj fakte ne peras al ni la kvalitaron de la parolsonoj
        <fnt>
          <aut>V. Benczik</aut>,
          <vrk>Skriba kaj vo&ccirc;a Esperanto...</vrk>,          
          <lok>IKU 2005</lok>
        </fnt>.
        <trd lng="pl">okoliczno&sacute;ci<klr tip="amb"> sprawy</klr>
        </trd>    
      </ekz>
      <refgrp tip="vid">
        <ref cel="difere.0igilo.KLA">diferencigilo</ref>,
        <ref cel="distin.0ilo">distingilo</ref>,
        <ref cel="karakt1.0o.KLA">karakterizo</ref>
      </refgrp>
    </dif>
    <trdgrp lng="de">
      <trd>Charakterzug</trd>,
      <trd>Eigenschaft</trd>
    </trdgrp>
    <trd lng="hu">jellemvon&aacute;s</trd>    
    <trd lng="es">rasgo caracter&iacute;stico, -a</trd>    
    <trd lng="it">caratteristica</trd>
    <trd lng="nl">karaktertrek</trd>
    <trdgrp lng="pl">
      <trd>w&lstroke;a&sacute;ciwo&sacute;&cacute;</trd>,
      <trd>cecha<klr tip="amb"> charakterystyczna</klr></trd>,
      <trd>rys</trd>
    </trdgrp>
  </snc>
  <snc>
    <fnt>Z</fnt>
    <dif>
      Ago rimarkinda kiel ekzemplo:
      <ekz>
        admirinda <tld/>o de kura&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zug</trd>,
      <trd>Eigenheit</trd>
    </trdgrp>
    <trd lng="hu">ismertet&odblac;jegy</trd>    
    <trd lng="nl">trek</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_s;&c_a; <klr tip="amb">(&c_t;&c_v;&c_a;&c_r;&c_u;,
    &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;&c_u; &c_i; &c_d;&c_a; &c_g;
    .&c_p;.)
    </klr></trd>,
    <trd>
    &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_e;&c_r;&c_n;&c_a;&c_ja; 
    &c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;
    </trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rys obličeje</trd>,
    <trd>rys povahy</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>trait</trd>,
    <trd>feature</trd>
  </trdgrp>
  <trd lng="es">rasgo</trd>
  <trd lng="fr">trait<klr> (du visage, de caract&egrave;re...)</klr></trd>
  <trdgrp lng="pl">
    <trd>w&lstroke;a&sacute;ciwo&sacute;&cacute;</trd>,
    <trd>cecha</trd>,
    <trd>rys</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_t;&c_a; <klr tip="amb">
  (&c_l;&c_i;&c_c;&c_a;, 
  &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_a; &c_i; &c_t;.&c_p;.)
  </klr></trd>
  <trdgrp lng="sk">
    <trd>charakteristická vlastnosť</trd>,
    <trd>znak</trd>,
    <trd>črta <klr>(na tvári)</klr></trd>
  </trdgrp>
</drv>
</art>
