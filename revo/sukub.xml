<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sukub</rad>/o
</kap>
<drv mrk="sukub.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Demono, preninta formon de virino por sekskuni&gcirc;i
      kun dormanta viro.
    </dif>
    <ref tip="vid" cel="inkub.0o.KAT">inkubo.</ref>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_k;&c_u;&c_b;</trd>
  <trd lng="bg">&c_s;&c_u;&c_k;&c_u;&c_b;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>s&uacute;cub</trd>,
    <trd>s&uacute;cube</trd>
  </trdgrp>
  <trd lng="en">succubus</trd>
  <trd lng="es">s&uacute;cubo</trd>
  <trd lng="fr">succube</trd>
  <trdgrp lng="hu">
    <trd>par&aacute;zna lid&eacute;rc</trd>,
    <trd>par&aacute;zna d&eacute;mon</trd>,
    <trd>succuba</trd>
  </trdgrp>
  <trd lng="it">succube</trd>
  <trd lng="nl">succubus</trd>
  <trdgrp lng="pl">
    <trd>sukub</trd>,
    <trd>sukkub</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_k;&c_k;&c_u;&c_b;</trd>
</drv>
</art>

