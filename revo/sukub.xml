<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sukub.xml,v 1.16 2025/04/06 06:47:46 revo Exp $">
<kap>
  <rad>sukub</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="sukub.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="demon.0o.diablo">Demono</ref>
      preninta formon de virino por sekskuni&gcirc;i
      kun dormanta viro:
      <ekz>
        folkloraj monstroj similaj al vampiroj, empuzoj
        kaj <tld/>oj, kiuj allogis junajn virojn, por tuj poste su&ccirc;i ilian sangon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=410925">Lamio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="inkub.0o.KAT">inkubo.</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>amordiablino</ke>.
  </rim>

  <trd lng="be">&c_s;&c_u;&c_k;&c_u;&c_b;</trd>
  <trd lng="bg">&c_s;&c_u;&c_k;&c_u;&c_b;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>s&uacute;cub</trd>,
    <trd>s&uacute;cube</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hanl. &zcaron;ena</trd>,
    <trd>m&uring;ra</trd>,
    <trd>&ccaron;ertice</trd>,
    <trd>&dcaron;&aacute;blice</trd>
  </trdgrp>
  <trd lng="en">succubus</trd>
  <trd lng="es">s&uacute;cubo</trd>
  <trd lng="fr">succube</trd>
  <trdgrp lng="hu">
    <trd>par&aacute;zna lid&eacute;rc</trd>,
    <trd>par&aacute;zna d&eacute;mon</trd>,
    <trd>succuba</trd>
  </trdgrp>
  <trd lng="it">succube</trd>
  <trd lng="nl">succubus</trd>
  <trdgrp lng="pl">
    <trd>sukub</trd>,
    <trd>sukkub</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_k;&c_k;&c_u;&c_b;</trd>
  <trdgrp lng="sk">
    <trd>diablica</trd>,
    <trd>&ccaron;ertica</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1089;&#1091;&#1082;&#1091;&#1073;</trd>,
    <trd>&#1076;&#1080;&#1103;&#1074;&#1086;&#1083; &#1074; &#1086;&#1073;&#1088;&#1072;&#1079;&#1110; &#1078;&#1110;&#1085;&#1082;&#1080;</trd>
  </trdgrp>
</drv>
</art>

