<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>presti&gcirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="presti1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="impon.0eco">Imponeco</ref> pro &scirc;ajno de
      beleco, gloro, &ccirc;iesa admiro:
      <ekz>
        la <tld/>o de la re&gcirc;eco, de la elokventeco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtor1.0o">a&ubreve;toritato</ref>,
      <ref cel="fam.0o">famo</ref>,
      <ref cel="influ.0o">influo</ref>,
      <ref cel="kredit.0o">kredito</ref>,
      <ref cel="nimb.0o">nimbo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_t;&c_y;&c_zh;</trd>
  <trdgrp lng="de">
    <trd>Prestige</trd>,
    <trd>Ansehen</trd>,
    <trd>Geltung</trd>
  </trdgrp>
  <trd lng="fr">prestige</trd>
  <trdgrp lng="hu">
    <trd>presztizs</trd>,
    <trd>tekint&eacute;ly</trd>
  </trdgrp>
  <trd lng="nl">prestige</trd>
  <trdgrp lng="pt">
    <trd>prest&iacute;gio</trd>,
    <trd>import&acirc;ncia social</trd>,    
    <trd>cr&eacute;dito</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_i;&c_zh;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_o;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="presti1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Impona pro gloro kaj vasta famo:
      <ekz>
        &gcirc;i havas konsideron por <klr>...</klr> tiuj, kiuj ne apartenas al
        <tld/>aj kulturoj
        <fnt><bib>BonaLingvo</bib>, <lok>p. 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">prestigieux</trd>
  <trdgrp lng="hu">
    <trd>elismert</trd>,
    <trd>tekint&eacute;lyes</trd>
  </trdgrp>
</drv>
</art>
