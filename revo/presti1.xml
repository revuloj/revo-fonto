<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>presti&gcirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="presti1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Imponeco pro &scirc;ajno de beleco a&ubreve; gloro:
      <ekz>
        la <tld/>o de la re&gcirc;eco, de la elokventeco.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="influ.0o">influo</ref>,
      <ref cel="kredit.0o">kredito</ref>,
      <ref cel="auxtor1.0o">a&ubreve;toritato</ref>,
      <ref cel="nimb.0o">nimbo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Prestige</trd>,
    <trd>Ansehen</trd>,
    <trd>Geltung</trd>
  </trdgrp>
  <trd lng="nl">prestige</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_i;&c_zh;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_o;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
