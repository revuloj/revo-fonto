<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: presti1.xml,v 1.18 2023/09/03 12:45:11 revo Exp $">
<kap>
  <ofc>9</ofc><rad>presti&gcirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="presti1.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="impon.0eco">Imponeco</ref> pro &scirc;ajno
      de
      beleco, gloro, &ccirc;iesa admiro:
      <ekz>
        la <tld/>o de la re&gcirc;eco, de la elokventeco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtor1.0o">a&ubreve;toritato</ref>,
      <ref cel="fam.0o">famo</ref>,
      <ref cel="influ.0o">influo</ref>,
      <ref cel="kredit.0o">kredito</ref>,
      <ref cel="nimb.0o">nimbo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_t;&c_y;&c_zh;</trd>
  <trdgrp lng="de">
    <trd>Prestige</trd>,
    <trd>Ansehen</trd>,
    <trd>Geltung</trd>
  </trdgrp>
  <trd lng="en">prestige</trd>
  <trd lng="fr">prestige</trd>
  <trdgrp lng="hu">
    <trd>presztizs</trd>,
    <trd>tekint&eacute;ly</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5A01;&#x4FE1;  [<ind>&#x3044;&#x3057;&#x3093;</ind>]</trd>,
    <trd>&#x5A01;&#x5149;  [<ind>&#x3044;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x4FE1;&#x671B;  [<ind>&#x3057;&#x3093;&#x307C;&#x3046;</ind>]</trd>,
    <trd>&#x540D;&#x58F0;  [<ind>&#x3081;&#x3044;&#x305B;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">prestige</trd>
  <trdgrp lng="pl">
    <trd>autorytet</trd>,
    <trd>presti&zdot;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prest&iacute;gio</trd>,
    <trd>import&acirc;ncia social</trd>,    
    <trd>cr&eacute;dito</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>autoritate</trd>,
    <trd>prestigiu</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_i;&c_zh;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_o;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_je;&c_s;&c_t;&c_i;&c_zh;</trd>,
    <trd>&c_a;&c_v;&c_t;&#x43E;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_r;&c_je;&#x43F;&c_u;&c_t;&c_a;&c_c;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="presti1.0a">
  <kap><ofc>9</ofc><tld/>a</kap>
  <snc>
    <dif>
      Impona pro gloro kaj vasta famo:
      <ekz>
        &gcirc;i havas konsideron por <klr>...</klr> tiuj, kiuj ne
        apartenas
        al
        <tld/>aj kulturoj
        <fnt><bib>BonaLingvo</bib>, <lok>p. 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_t;&c_y;&c_zh;&c_n;&c_y;</trd>
  <trd lng="de">angesehen</trd>
  <trd lng="en">prestigious</trd>
  <trd lng="fr">prestigieux</trd>
  <trdgrp lng="hu">
    <trd>elismert</trd>,
    <trd>tekint&eacute;lyes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5A01;&#x4FE1;&#x306E;&#x3042;&#x308B;  [<ind>&#x3044;&#x3057;&#x3093;&#x306E;&#x3042;&#x308B;</ind>]</trd>,
    <trd>&#x4FE1;&#x671B;&#x306E;&#x3042;&#x308B;  [<ind>&#x3057;&#x3093;&#x307C;&#x3046;&#x306E;&#x3042;&#x308B;</ind>]</trd>,
    <trd>&#x540D;&#x9AD8;&#x3044;  [<ind>&#x306A;&#x3060;&#x304B;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">prestigieus</trd>
  <trd lng="pl">presti&zdot;owy</trd>
  <trd lng="ro">prestigiosul</trd>
  <trd lng="uk">&#x43F;&c_r;&c_je;&c_s;&c_t;&c_i;&c_zh;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>
