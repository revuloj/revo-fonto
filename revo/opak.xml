<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: opak.xml,v 1.11 2025/04/04 18:41:12 revo Exp $">
<kap>
  <rad>opak</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="opak.0a">
  <kap><tld/>a</kap>
  <snc mrk="opak.0a.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="dif" cel="diafan.mal0a.FOT">Maldiafana</ref>,
      ne tralasanta lumradiojn:
      <ekz>
        metalo <klr>[&#8230;]</klr> estas tipe malmola, <tld/>a, brila
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2809">Metalo</url></lok>
        </fnt>.
      </ekz>
    </dif>  
    <trdgrp lng="be">
      <trd>&c_m;&c_u;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_z;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>matn&yacute;</trd>,
      <trd>neprieh&lcaron;adn&yacute;</trd>,
      <trd>zakalen&yacute;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>matn&yacute;</trd>,
      <trd>nepr&uring;hledn&yacute;</trd>,
      <trd>opakn&iacute;</trd>,
      <trd>zakalen&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>lichtundurchl&auml;ssig</trd>,
      <trd>opak</trd>
    </trdgrp><trd lng="en">opaque</trd>
    <trdgrp lng="zh">
      <trd>&#29888; <pr>ru&#462;n</pr></trd>,
      <trd>&#19981;&#36879;&#26126; <pr>b&#249;t&#242;um&#237;ng</pr></trd>,
      <trd>&#19981;&#36879;&#20809; <pr>b&#249;t&#242;ugu&#257;ng</pr></trd>,
      <trd>&#19981;&#36879;&#20809;&#30340; <pr>b&#249;t&#242;ugu&#257;ngde</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="opak.0a.malluma">
    <uzo tip="fak">POE</uzo>
    <dif>
      Nepenetreble <ref tip="super" cel="lum.mal0a">malluma</ref>, 
      <ref tip="super" cel="klar.mal0a">malklara</ref>:
      <ekz>
        &scirc;i kutimis stari apudfenestre &gcirc;is <tld/>i&gcirc;is 
        ekstere
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_c;&c_jo;&c_m;&c_n;&c_y;</trd>
    </trdgrp>
  <trd lng="de">undurchdringlich<klr> (dunkel)</klr></trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>netravidebla</ke>, <ke>maldiafana</ke>, <ke>lumbara</ke>.
  </rim>

  <trd lng="uk">&#1085;&#1077;&#1087;&#1088;&#1086;&#1079;&#1086;&#1088;&#1080;&#1081;</trd>
</drv>

</art>

