<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>opak</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="opak.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="opak.0a.KOMUNE">
    <ref tip="dif" cel="vid.netra0ebla.FOT">netravidebla</ref>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
</drv>

<drv mrk="opak.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="opak.0igxi.KOMUNE">
    <dif>
      I&gcirc;i netravidebla:
      <ekz>
        &scirc;i kutimis stari apudfenestre &gcirc;is <tld/>i&gcirc;is 
        ekstere
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
</drv>
</art>

<!--
$Log$
-->
</vortaro>