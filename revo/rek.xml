<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rek</rad>/o
</kap>
<drv mrk="rek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Gimnastikilo, konsistanta el du vertikalaj fostoj, kiuj tenas
      horizontalan stangon el &scirc;talo a&ubreve; ligno.
    </dif>
  </snc>
  <trd lng="de">Reck</trd>
  <trd lng="en">horizontal bar</trd>
  <trdgrp lng="nl">
    <trd>rekstok</trd>,
    <trd>horizontale <ind>barre</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_l;&c_a;&c_d;&c_i;&c_n;&c_a;<klr tip="ind"> (&c_s;&c_p;&c_o;&c_r;&c_t;.)</klr></trd>
  </trdgrp>
</drv>
</art>
