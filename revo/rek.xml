<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rek.xml,v 1.16 2022/03/26 18:10:08 revo Exp $">
<kap>
  <rad>rek</rad>/o
</kap>
<drv mrk="rek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="gimnas1.0ilo">Gimnastikilo</ref>,
      konsistanta el du vertikalaj fostoj, kiuj tenas
      horizontalan <ref tip="prt" cel="stang.0o">stangon</ref>
      el &scirc;talo a&ubreve; ligno:
      <ekz>
        la plej simplaj ekzercoj sur la <tld/>o estas <esc>suprentiri&gcirc;o</esc>
        kaj turnlevi&gcirc;o, aliaj ekzercoj konsistas el turnado &ccirc;irka&ubreve; la reko kaj transflugoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=286852">Reko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_k;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">hrazda</trd>
  <trd lng="de">Reck</trd>
  <trd lng="en">horizontal bar</trd>
  <trd lng="fr">barre fixe horizontale</trd>
  <trd lng="hu">korl&aacute;t</trd>
  <trdgrp lng="nl">
    <trd>rekstok</trd>,
    <trd>horizontale <ind>barre</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dr&aogonek;&zdot;ek <klr>gimnastyczny</klr></trd>,
    <trd>rek</trd>
  </trdgrp>
  <trd lng="pt">barra-fixa</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_l;&c_a;&c_d;&c_i;&c_n;&c_a;<klr tip="ind"> (&c_s;&c_p;&c_o;&c_r;&c_t;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">hrazda</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x440;&#x43D;&#x456;&#x43A;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43A;&#x43B;&#x430;&#x434;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>
</art>
