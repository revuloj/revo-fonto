<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hipokondri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hipokondri.0o">
  <kap><tld/>o</kap>
  <snc mrk="hipokondri.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <ref tip="dif" cel="hipohx.0o.PSI">hipo&hcirc;ondrio</ref>
      <ekz>
        lia <tld/>o trovi&gcirc;is en stranga kontrasto al lia forto
        <fnt>
          <aut>trad. Donjo, trad. Cezar</aut>,
          <vrk><url
          ref="http://karapacopanoramo.blogspot.com/2008/04/spitzweg-bidermajra-honestulo-kaj.html">
          Spitzweg - Bidermajra honestulo kaj revoluciulo</url></vrk>,
          <lok>2008-04-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl><tld/>o estas la sola malsano, pro kiu mi ne suferas</ctl>
        <fnt>
          <aut>trad. P. Thorsen</aut>,
          <vrk><url ref="http://www.poulthorsen.dk/grafit.htm">
           Grafita&jcirc;oj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="fr">hypocondrie</trd>
  <trd lng="it">ipocondria</trd>
  <trd lng="pt">hipocondria</trd>
</drv>

<drv mrk="hipokondri.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="hipokondri.0ulo.PSI">
    <uzo tip="fak">PSI</uzo>
    <ref tip="dif" cel="hipohx.0ulo.PSI">hipo&hcirc;ondriulo</ref>
      <ekz>
        mi nutris svagan esperon, ke la ekscito nun agitanta la 
        <tld/>ulon eble trovos mildi&gcirc;on
        <fnt>
          <aut>E. A. Poe, trad. E. Grimley-Evans</aut>,
          <vrk><url
          ref="http://www.esperanto.org/Ondo/Tl-ang07.htm#Tradukinto">
          La falo de la domo Usher</url></vrk>,
          <lok>Sezonoj, 2002</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="fr">hypocondriaque<klr> (subst.)</klr></trd>
  <trdgrp lng="it">
   <trd>ipocondriaco</trd>, 
   <trd><ind>malato</ind> immaginario</trd>
  </trdgrp>
  <trd lng="pt">hipocondr&iacute;aco</trd>
</drv>
</art>

