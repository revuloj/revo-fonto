<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: margaret.xml,v 1.7 2023/09/02 06:04:09 afido Exp $">
    <kap>
      <rad>Margaret</rad>/o, <var><kap><rad var="i">Margerit</rad>/o</kap></var>
    </kap>

<drv mrk="margaret.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var></kap>
  <uzo tip="fak">NOM</uzo>
  <snc mrk="margaret.0o.virina_nomo">
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">persona nomo</ref>,
      devenanta de greka <trd lng="grc">&my;&alfa;&rho;&gamma;&alfa;&rho;&jota_ton;&tau;&eta;&sigma_fina;</trd> (<ref cel="perl.0o">perlo</ref>):
      <ekz>
        li rigardis la trian leonon kaj rememoris <tld/>on, kiu unuigis la Skandinavajn
        regnojn sub unu sceptro
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        patrino <tld/>o, la malri&ccirc;a virino, tre deziris konstrui al si malgrandan dometon
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        venigu ilin amba&ubreve;, <tld/>on kaj Vincentinon
        <fnt>
          <aut>M. Serao, trad. Kabe</aut>
          <vrk><url ref="&Tekstaro;?nomo=internacia-krestomatio">Internacia krestomatio</url></vrk>
          <lok>Neniam Plu!</lok>
        </fnt>;
      </ekz>
      <ekz>
        doktoro Fa&ubreve;sto kaj <tld var="i"/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="i"/>a pico
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1771">Italio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Margarethe</trd>
    <trdgrp lng="en">
      <trd>Margaret</trd>
      <trd>Margarita</trd>
    </trdgrp>
    <trd lng="it">Margherita</trd>
    <trd lng="pl">Ma&lstroke;gorzata</trd>     
    <trd lng="ro">Margarita</trd>
  </snc>
  <rim>
    En multaj lingvoj <ref cel="lekant.0o">lekanto</ref> havas similan nomon.
  </rim>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30EB;&#x30AC;&#x30EA;&#x30FC;&#x30BF;</trd>,
    <trd>&#x30DE;&#x30FC;&#x30AC;&#x30EC;&#x30C3;&#x30C8;</trd>
  </trdgrp>
  <trd lng="uk">&#x41C;&#x430;&#x440;&#x491;&#x430;&#x440;&#x438;&#x442;&#x430;</trd>
</drv>

  </art>

