<vortaro>
<art mrk="$Id$">
<kap>
  <rad>komputik</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="komputik.0o">
  <kap><tld/>o</kap>
  <snc mrk="komputik.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Te&hcirc;niko pri algoritma solvado de problemoj, prilaborado de datumoj
      helpe de <ref tip="vid" cel="komput.0ilo">komputiloj</ref>:
      <ekz>
        oni trovas en &gcirc;i &ccirc;iujn &ccirc;iutagajn nociojn de teoria
        <tld/>o
        <fnt>
        <aut>PP</aut>,
        <vrk>Valora konsultlibro (Sergio Pokrovskij, Komputika
        leksikono)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1995a, numero 12a, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komput.0ado">komputado</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_a;&c_r;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="de">Informatik</trd>
  <trdgrp lng="en">
    <trd>computer science</trd>,
    <trd>information technology<klr> (IT)</klr></trd>
  </trdgrp>
  <trd lng="es">inform&aacute;tica</trd>
  <trdgrp lng="fr">
    <trd>informatique<klr tip="ind"> (subst., discipline)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;m&iacute;t&aacute;stechnika</trd>
  <trdgrp lng="pt">
    <trd>computa&ccedil;&atilde;o</trd>,
    <trd>ci&ecirc;ncias da <ind>computa&ccedil;&atilde;o</ind></trd>,    
    <trd>inform&aacute;tica</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;<klr tip="amb"> (&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;
    &c_z;&c_n;&c_a;&c_n;&c_i;&c_j;)</klr></trd>,
    <trd>&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_t;&c_je;&c_h;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="komputik.0a">
  <kap><tld/>a</kap>
  <snc mrk="komputik.0a.KOMP">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a teknologio helpos al la senlaboruloj
        <fnt>
        <aut>Evgeni Georgiev</aut>,
        <vrk>Komputika teknologio helpos al la senlaboruloj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 5a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Informatik-</trd>
  <trd lng="es">inform&aacute;tico</trd>
  <trdgrp lng="fr">
    <trd>informatique<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;m&iacute;t&aacute;stechnikai</trd>  
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_mol;&c_ju;&c_t;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_n;&c_o;&c_s;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_k; <ind>&c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_j;
    &c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_je;</ind></trd>
  </trdgrp>
</drv>
</art>
