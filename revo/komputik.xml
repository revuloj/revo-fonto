<art mrk="$Id: komputik.xml,v 1.23 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>komputik</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="komputik.0o">
  <kap><tld/>o</kap>
  <snc mrk="komputik.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Te&hcirc;niko pri algoritma solvado de problemoj, prilaborado de
      datumoj
      helpe de <ref tip="vid" cel="komput.0ilo">komputiloj</ref>:
      <ekz>
        oni trovas en &gcirc;i &ccirc;iujn &ccirc;iutagajn nociojn de teoria
        <tld/>o
        <fnt>
        <aut>PP</aut>,
        <vrk>Valora konsultlibro (Sergio Pokrovskij, Komputika
        leksikono)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1995a, numero 12a, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komput.0ado">komputado</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_a;&c_r;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">inform&agrave;tica</trd>
  <trdgrp lng="cs">
    <trd>komputika</trd>,
    <trd>po&ccaron;&iacute;ta&ccaron;ov&aacute; technika</trd>
  </trdgrp>
  <trd lng="de">Informatik</trd>
  <trdgrp lng="en">
    <trd>computer science</trd>,
    <trd>information technology<klr> (IT)</klr></trd>
  </trdgrp>
  <trd lng="es">inform&aacute;tica</trd>
  <trdgrp lng="fr">
    <trd>informatique<klr tip="ind"> (subst., discipline)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;m&iacute;t&aacute;stechnika</trd>
  <trd lng="ja">&#x8A08;&#x7B97;&#x6A5F;&#x79D1;&#x5B66;  <pr>&#x3051;&#x3044;&#x3055;&#x3093;&#x304D;&#x304B;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">informatica</trd>
  <trd lng="pl">informatyka</trd>
  <trdgrp lng="pt">
    <trd>computa&ccedil;&atilde;o</trd>,
    <trd>ci&ecirc;ncias da <ind>computa&ccedil;&atilde;o</ind></trd>,
    <trd>inform&aacute;tica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;<klr tip="amb">
    (&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;
    &c_z;&c_n;&c_a;&c_n;&c_i;&c_j;)</klr></trd>,
    <trd>&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_t;&c_je;&c_h;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">po&ccaron;&iacute;ta&ccaron;ov&aacute; technika</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_ch;&c_i;&c_s;&c_l;&c_ju;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a; &c_t;&c_je;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8A08;&#x7B97;&#x6A5F;&#x79D1;&#x5B78; <pr>j&#xEC;su&#xE0;nj&#x12B;k&#x113;xu&#xE9;</pr></trd>,
    <trd>&#x4FE1;&#x606F;&#x5B66; <pr>x&#xEC;nx&#x12B;xu&#xE9;</pr></trd>,
