<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>maksim</rad>/o
</kap>
<drv mrk="maksim.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Mallonge esprimita principo de konduto
    </dif>
    <refgrp tip="sin">
      <ref cel="aforis.0o">aforismo</ref>,
      <ref cel="senten.0o">sentenco</ref>,
      <ref cel="deviz.0o">devizo</ref>,
      <ref cel="prover.0o">proverbo</ref>,
      <ref cel="mot.0o">moto</ref>.
    </refgrp>
  </snc>
  <trd lng="en">maxim</trd> 
  <trdgrp lng="nl">
    <trd>kernspreuk</trd>,
    <trd>grondregel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_k;&c_s;&c_i;&c_m;&c_a;</trd>,
    <trd>&c_a;&c_f;&c_o;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_i;&c_z;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
