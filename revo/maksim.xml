<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: maksim.xml,v 1.22 2021/08/06 15:41:48 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>maksim</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="maksim.0o">
  <kap><tld/>o</kap>
  <snc mrk="maksim.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="super" cel="senten.0o">Sentenco</ref> esprimanta
      &gcirc;eneralan
      konstaton, sa&gcirc;a&jcirc;on pri homa konduto, moroj, moralo:
      <ekz>
        <ctl>Arabaj <tld/>oj de la sepa jarcento</ctl>
        <fnt>
          <aut>H. Al-Amily</aut>,
          <vrk>Arabaj maksimoj de la sepa jarcento</vrk>,
          <lok>1967</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvu la sa&gcirc;an <tld/>on de la antikvuloj: <ctl>similan kuracu 
        per simila</ctl>
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        validas plu la <klr>[&#8230;]</klr> <tld/>o de objektiva &jcirc;urnalismo: skeptiko kaj
        distanco pri kaj al &ccirc;io kaj &ccirc;iu
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2008/009430.php"
              >Fido kaj skeptiko</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        baza <tld/>o &jcirc;urnalisma estas la jena: se iu, ie, deziras ion ka&scirc;i, jen
        &jcirc;urnalismo: &ccirc;io alia estas reklamado
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2014/011303.php"
              >&Ccirc;iam eblas revi ...</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="deviz.0o.HER">devizo</ref>,
       <ref cel="princi.0o">principo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_s;&c_ib;&c_m;&c_a;</trd>
  <trd lng="br">krennlavar</trd>
  <trd lng="ca">m&agrave;xima</trd>
  <trdgrp lng="cs">
    <trd>maxima</trd>,
    <trd>mravn&iacute; z&aacute;sada</trd>,
    <trd>pravidlo chov&aacute;n&iacute;</trd>,
    <trd>&zcaron;ivot.z&aacute;sada</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Maxime</trd>,
    <trd>Grundsatz</trd>,
    <trd>Leitsatz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>maxim</trd>,
    <trd>principle</trd>
  </trdgrp>
  <trd lng="es">m&aacute;xima</trd>
  <trd lng="fr">maxime</trd>
  <trdgrp lng="hu">
    <trd>maxima</trd>,
    <trd>&eacute;letelv</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kernspreuk</trd>,
    <trd>grondregel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>maksyma</trd>,
    <trd>aforyzm</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>m&aacute;xima</trd>,
    <trd>prov&eacute;rbio</trd>,    
    <trd>anexim</trd>
  </trdgrp>
  <trd lng="ro">maxim&abreve; <klr>(filozofie)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_k;&c_s;&c_i;&c_m;&c_a;</trd>,
    <trd>&c_a;&c_f;&c_o;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_i;&c_z;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>maxima</trd>,
    <trd>princ&iacute;p</trd>,
    <trd>z&aacute;sada</trd>
  </trdgrp>
</drv>
</art>
