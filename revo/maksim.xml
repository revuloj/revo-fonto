<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>maksim</rad>/o
</kap>
<drv mrk="maksim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="senten.0o">Sentenco</ref> esprimanta &gcirc;eneralan
      konstaton, sa&gcirc;a&jcirc;on pri homa konduto, moroj, moralo.
    </dif>
  </snc>                       
  <trd lng="be">&c_m;&c_a;&c_k;&c_s;&c_ib;&c_m;&c_a;</trd>
  <trd lng="br">krennlavar</trd>
  <trdgrp lng="de">
    <trd>Maxime</trd>,
    <trd>Grundsatz</trd>,
    <trd>Leitsatz</trd>
  </trdgrp>
  <trd lng="en">maxim</trd>
  <trd lng="es">m&aacute;xima</trd>
  <trd lng="fr">maxime</trd>
  <trdgrp lng="hu">
    <trd>maxima</trd>,
    <trd>&eacute;letelv</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kernspreuk</trd>,
    <trd>grondregel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>m&aacute;xima</trd>,
    <trd>prov&eacute;rbio</trd>,    
    <trd>anexim</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_k;&c_s;&c_i;&c_m;&c_a;</trd>,
    <trd>&c_a;&c_f;&c_o;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_i;&c_z;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
