<kap>
  <rad>gipur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gipur.0o">
  <kap><tld/>o</kap>
  <snc mrk="gipur.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="vid" cel="punt.0o.TEKS">Punto</ref> sen baza &scirc;tofo,
      sekve a&jcirc;ura, ofte reliefa, uzata sur iuj vestoj, por kurtenoj
      kaj aliaj:
      <ekz>
        <tld/>a tablotuko
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">guipure</trd>
</drv>
</art>

