<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gipur.xml,v 1.5 2026/02/08 10:43:49 revo Exp $">
<kap>
  <rad>gipur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gipur.0o">
  <kap><tld/>o</kap>
  <snc mrk="gipur.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="vid" cel="punt.0o.TEKS">Punto</ref> sen baza &scirc;tofo,
      sekve <ref cel="ajxur.0a">a&jcirc;ura</ref>, ofte reliefa, uzata sur iuj vestoj, por tablotukoj, kurtenoj
      kaj aliaj:
      <ekz>
        <tld/>a tablotuko
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_ju;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Nadelspitze</trd>,
    <trd>Guipure-Spitze</trd>,
    <trd>Gip&uuml;re</trd>
  </trdgrp>
  <trd lng="fr">guipure</trd>
  <trd lng="pl">gipiura</trd>
  <trd lng="uk">&c_g;&c_ib;&c_p;&c_ju;&c_r;</trd>
</drv>
</art>

