<vortaro>
<art mrk="$Id: kubut.xml,v 1.37 2023/12/03 09:59:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kubut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kubut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kubut.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="artik.0o.ANA">Artiko</ref> inter supra kaj
      anta&ubreve;a partoj de la <ref tip="malprt" cel="brak.0o.homa_membro">brako</ref>,
      kuniganta <ref cel="humer.0o">humero</ref>n kun <ref cel="radius.0o.ANA">radiuso</ref>
      kaj <ref cel="uln.0o.kubutosto">ulno<sncref/></ref>:
      <ekz>
        <klr>[ili]</klr> kudras kusenojn sub &ccirc;iujn <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 13:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> ekpu&scirc;is la knabon per la <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo apogis la <tld/>ojn sur la tablo kaj kalkulis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        Klaro apogis la <tld/>ojn sur la genuoj kaj ka&scirc;is la viza&gcirc;on en la manplatoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj manikoj estis falditaj &gcirc;is la <tld/>oj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i portis &ccirc;iam mallongajn manikojn kaj havis &ccirc;iam kontuzitajn <tld/>ojn
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li dispu&scirc;is per la <tld/>oj rondon de la kunvenintoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_o;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_l;&c_a;&c_k;&c_malmol;&c_t;</trd>
  <trd lng="bo">&t_ga;&t_rata;&t_shabchu;&t_tsheg;&t_ma;&t_naro;&t_tsheg;</trd>
  <trd lng="ca">colze</trd>
  <trdgrp lng="cs">
    <trd>loket</trd>,
    <trd>ohyb</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ellbogen</trd>,
    <trd>Ellenbogen</trd>
  </trdgrp>
  <trd lng="en">elbow</trd>
  <trd lng="es">codo</trd>
  <trd lng="fr">coude</trd>
  <trd lng="hu">k&ouml;ny&ouml;k</trd>
  <trd lng="id">siku</trd>
  <trd lng="it">gomito</trd>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x3058;</trd>,
    <trd>&#x66F2;&#x304C;&#x308A;&#x89D2;  <pr>&#x307E;&#x304C;&#x308A;&#x304B;&#x3069;</pr></trd>,
    <trd>&#x5C48;&#x66F2;&#x90E8;  <pr>&#x304F;&#x3063;&#x304D;&#x3087;&#x304F;&#x3076;</pr></trd>,
    <trd>&#x30A8;&#x30EB;&#x30DC;</trd>,
    <trd>&#xFF2C;&#x5B57;&#x7D99;&#x624B;  <pr>&#x3048;&#x308B;&#x3042;&#x3056;&#x3064;&#x304E;&#x3066;</pr></trd>
  </trdgrp>
  <trd lng="nl">elleboog</trd>
  <trd lng="pl">&lstroke;okie&cacute;</trd>
  <trdgrp lng="pt">
    <trd>cotovelo <klr tip="amb">(&ecirc;)</klr></trd>,
    <trd>c&uacute;bito</trd>
  </trdgrp>
  <trd lng="ro">cot</trd>
  <trd lng="ru">&c_l;&c_o;&c_k;&c_o;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>lake&tcaron;</trd>,
    <trd>ohyb</trd>
  </trdgrp>
  <trd lng="sv">armb&aring;ge</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_ib;&c_k;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_ib;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x624B;&#x8098; <pr>sh&#x1D2;uzh&#x1D2;u</pr></trd>,
    <trd>&#x8098; <pr>zh&#x1D2;u</pr></trd>,
    <trd>&#x81C2;&#x8098; <pr>b&#xEC;zh&#x1D2;u</pr></trd>,
    <trd>&#x8098;&#x5173;&#x8282; <pr>zh&#x1D2;ugu&#x101;nji&#xE9;</pr></trd>,
    <trd>&#x8098;&#x95DC;&#x7BC0; <pr>zh&#x1D2;ugu&#x101;nji&#xE9;</pr></trd>,
    <trd>&#x80F3;&#x818A;&#x8098; <pr>g&#x113;bozh&#x1D2;u</pr></trd>,
    <trd>&#x80F3;&#x81C2;&#x8098;&#x513F; <pr>g&#x113;beizh&#x1D2;ur</pr></trd>,
    <trd>&#x80F3;&#x81C2;&#x8098;&#x5152; <pr>g&#x113;beizh&#x1D2;ur</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kubut.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="kubut.0umi.MOV">
    <dif>
      <ref tip="super" cel="pusx.0i.delokigi">Pu&scirc;i</ref>, bati per <tld/>o:
      <ekz>
        kamaradoj, lasu min trapasi, ne <tld/>umu!
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        Onklo <nom>Pan</nom> <klr>[&#x2026;]</klr>
        komencis <tld/>umi min kaj flustris, &leftquot;vidu la
        novajn oficistojn!&rightquot;
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la diversaj naciaj lingvoj inter<tld/>umas por amasigi al si influon
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008616.php">Lingva egaleco en internaciaj rilatoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_h;&c_a;&c_c;&c_mol; &c_l;&c_o;&c_k;&c_c;&c_je;&c_m;</trd>
  <trd lng="de">mit dem Ellenbogen <ind>anstossen</ind></trd>
  <trd lng="en">elbow<klr> (push with the elbow or elbows)</klr></trd>
  <trdgrp lng="es">
    <trd><ind>codear</ind>se</trd>,
    <trd>dar con el <ind>codo</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>donner des coups de <ind>coude</ind></trd>,
    <trd>jouer des coudes</trd>
  </trdgrp>
  <trd lng="pl">szturcha&cacute; <ind>&lstroke;okciami</ind></trd>
  <trd lng="ro">ghiont <ind>coate</ind></trd>
</drv>

<drv mrk="kubut.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kubut.tra0i.KOMUNE">
    <dif>
      Anta&ubreve;eni&gcirc;i, per la <tld/>oj alflankigante embarasa&jcirc;ojn:
      <ekz>
        li tra<tld/>is al si lokon <klr>[&#x2026;]</klr> en la unua vico
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_h;&c_v;&c_a;&c_c;&c_mol; &c_l;&c_o;&c_k;&c_c;&c_ja;&c_m;&c_ib; <klr>(&c_k;&c_a;&c_b; &c_p;&c_r;&c_a;&c_s;&c_u;&c_n;&c_u;&c_c;&c_c;&c_a; &c_n;&c_a;&c_p;&c_je;&c_r;&c_a;&c_d;)</klr></trd>
  <trdgrp lng="de">
    <trd>sich mit den Ellenbogen <ind>durchdr&auml;ngeln</ind></trd>,
    <trd>vordr&auml;ngeln</trd>
  </trdgrp><trdgrp lng="en">
    <trd>elbow</trd>,
    <trd>elbow forward</trd>,
    <trd>elbow through<klr> (forge ahead using the elbows to assist)</klr></trd>
  </trdgrp>
  <trd lng="es">usar los <ind>codos</ind> para avanzar</trd>
  <trd lng="fr">jouer des <ind>coudes</ind></trd>
  <trd lng="pl">przepycha&cacute; si&eogonek; <ind>&lstroke;okciami</ind></trd>
  <trd lng="ro">coturi de &icirc;mpingere</trd>
  <trdgrp lng="zh">
    <trd>&#x5411;&#x524D;&#x63A8; <pr>xi&#xE0;ngqi&#xE1;ntu&#x12B;</pr></trd>,
    <trd>&#x5411;&#x524D;&#x6324; <pr>xi&#xE0;ngqi&#xE1;nj&#x1D0;</pr></trd>,
    <trd>&#x5411;&#x524D;&#x64E0; <pr>xi&#xE0;ngqi&#xE1;nj&#x1D0;</pr></trd>,
    <trd>&#x63A8;&#x5165; <pr>tu&#x12B;r&#xF9;</pr></trd>,
    <trd>&#x7A81;&#x51FA;&#x81EA;&#x5DF1; <pr>t&#x16B;ch&#x16B;z&#xEC;j&#x1D0;</pr></trd>,
    <trd>&#x52A0;&#x585E; <pr>ji&#x101;s&#x101;i</pr></trd>
  </trdgrp>
</drv>
</art>
