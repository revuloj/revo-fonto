<drv mrk="agx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="agx.0i.KOMUNE">
    <dif>
      Esti atinginta la <tld/>on de:
      <ekz>
        kiom li <tld/>is
        <fnt>
          <bib>ChB</bib>
          <lok>2</lok>
        </fnt>?
      </ekz>
      <ekz>
        <tld/>i ne pli ol dek jarojn
        <fnt>
          <bib>ChV</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la libro nun <tld/>as pli ol 10 jarojn
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
         tiu &ccirc;i tombo <tld/>as almena&ubreve; jarcenton
         <fnt><bib>Kiso</bib>, <lok>tombeja arabesko</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE21;&#xE35;&#xE2D;&#xE32;&#xE22;&#xE38;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_mol; <klr>(&c_g;&c_a;&c_d;&c_o;&c_w; &c_v;&c_je;&c_k;&c_u;)</klr></trd>,
    <trd>&c_d;&c_a;&c_s;&c_ja;&c_g;&c_a;&c_c;&c_mol; <ind>&c_u;&c_z;&c_r;&c_o;&c_s;&c_t;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="br">beza&ntilde; <ind>oadet</ind> a</trd>
  <trd lng="de">alt sein</trd>
  <trd lng="en">be the age of</trd>
  <trd lng="es">cumplir a&ntilde;os</trd>
  <trd lng="fr">&ecirc;tre <ind>&acirc;g&eacute;</ind> <klr>(de)</klr></trd>
  <trd lng="he">&lamed;&he;&zayin;&dalet;&qof;&fnun;</trd>
  <trdgrp lng="id">
     <trd>ber<ind>umur</ind></trd>,
     <trd>ber<ind>usia</ind></trd>
   </trdgrp>
   <trd lng="ja">&#xFF08;&#x301C;&#x6B73;&#x306E;&#xFF09;&#x5E74;&#x9F62;&#12391;&#12354;&#x308B;
   <pr>&#x306D;&#12435;&#x308C;&#12356;&#12391;&#12354;&#x308B;</pr></trd>
   <trd lng="pl"><klr tip="amb">(mie&cacute;)</klr> <ind>lat</ind></trd>
   <trd lng="pt">ter a <ind>idade</ind> <klr>(de)</klr></trd>
   <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
</drv>

<drv mrk="agx.0a">
  <kap><tld/>a</kap>
  <snc mrk="agx.0a.KOMUNE">
    <dif>
      Havanta ian <tld/>on:
      <ekz>
        mia pli <tld/>a frato
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 2:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <tld/>a filo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi jam estas sufi&ccirc;e <tld/>a por labori
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Montan</nom> estis iom <tld/>a, sed la respondeco <klr>[&#8230;]</klr> estis doninta al li
        aspekton de modesta digno
        <fnt>
          <bib>HsT</bib>
          <lok>Kuni&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ok jarojn <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj <tld/>aj je pli ol 60 jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="https://www.monato.be/2009/009736.php">Kubo maljuni&gcirc;as</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
	 <tld/>ulo de unu monato &gcirc;is kvin jaroj
	 <fnt><bib>MT</bib>, <lok>&Lev; 27:6</lok></fnt>;
      </ekz>
      <ekz>
        forveturis malsa&gcirc;a, revenis nur pli <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_u;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
     <trd>&c_m;&c_a;&c_ju;&c_ch;&c_y; <ind>&c_w;&c_z;&c_r;&c_o;&c_s;&c_t;</ind></trd>
   </trdgrp>
   <trd lng="br">oadet</trd>
   <trd lng="de">alt</trd>
   <trd lng="en">aged</trd>
   <trd lng="fr">&acirc;g&eacute;</trd>
   <trd lng="he">&mem;&bet;&vav;&gimel;&resh;</trd>
  <trdgrp lng="id">
     <trd>tua</trd>,
     <trd>ber<ind>umur</ind></trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x5E74;&#x9F62;&#x306E;  <pr>&#x306D;&#12435;&#x308C;&#12356;&#x306E;</pr></trd>,
    <trd>&#x5E74;&#12434;&#12392;&#12387;&#x305F;  <pr>&#x306D;&#12435;&#12434;&#12392;&#12387;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">oud</trd>
   <trd lng="pl"><klr tip="amb">(maj&aogonek;cy)</klr>
   <ind>lat</ind></trd>
   <trd lng="pt">idoso</trd>
   <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j;
  <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
  <trd lng="tr">ya&scedil;l&inodot;</trd>
</drv>

<drv mrk="agx.flor0o">
  <kap>flor<tld/>o</kap>
  <snc mrk="agx.flor0o.KOMUNE">
    <dif>
      A&gcirc;o de florado, de prospera juneco:
      <ekz>
        kie oni volas esti en sia matura <tld/>o,
        tien oni devas strebi jam en sia flor<tld/>o!
        <fnt>
        <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
        <vrk>Fabloj kaj Aforismoj</vrk>
        <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_r;&c_o;&c_s;&c_t; &c_r;&c_o;&c_s;&c_k;&c_v;&c_ib;&c_t;&c_u;</trd>
  <trdgrp lng="fr">
    <trd>fleur de l'&acirc;ge</trd>,
    <trd>printemps <klr>de la vie</klr></trd>
  </trdgrp>
</drv>

