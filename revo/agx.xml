  <trd lng="ja">&#x5E74;&#x3092;&#x53D6;&#x308B; [<ind>&#x3068;&#x3057;&#x3092;&#x3068;&#x308B;</ind>]</trd>
  <trd lng="nl">ouder worden</trd>
</drv>

