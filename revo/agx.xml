<drv mrk="agx.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="agx.0i.KOMUNE">
     <dif>
       Esti atinginta la <tld/>on de:
       <ekz>
         kiom li <tld/>as? li estas dekkvinjara;
       </ekz>
       <ekz>
         tiu &ccirc;i tombo <tld/>as almena&ubreve; jarcenton
         <fnt><bib>Kiso</bib>, <lok>tombeja arabesko</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="th">&#xE21;&#xE35;&#xE2D;&#xE32;&#xE22;&#xE38;</trd>
   </snc>
   <trd lng="br">beza&ntilde; <ind>oadet</ind> a</trd>
   <trd lng="de">alt sein</trd>
   <trd lng="en">be the age of</trd>
   <trd lng="es">cumplir a&ntilde;os</trd>
   <trd lng="fr">&ecirc;tre <ind>&acirc;g&eacute;</ind> <klr>(de)</klr></trd>
   <trd lng="he">&#x5DC;&#x5D4;&#x5D6;&#x5D3;&#x5E7;&#x5DF;</trd>
  <trdgrp lng="id">
     <trd>ber<ind>umur</ind></trd>,
     <trd>ber<ind>usia</ind></trd>
   </trdgrp>
   <trd lng="ja">&#xFF08;&#x301C;&#x6B73;&#x306E;&#xFF09;&#x5E74;&#x9F62;&#x3067;&#x3042;&#x308B;</trd>
   <trd lng="pl"><klr tip="amb">(mie&cacute;)</klr> <ind>lat</ind></trd>
   <trd lng="pt">ter a <ind>idade</ind> <klr>(de)</klr></trd>
   <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
</drv>

<drv mrk="agx.0a">
   <kap><tld/>a<fnt>Z</fnt></kap>
   <snc mrk="agx.0a.KOMUNE">
     <dif>
       Havanta ian <tld/>on:
       <ekz>
	 mi estas pli <tld/>a ol vi;
       </ekz>
       <ekz>
	 esti 5 jarojn <tld/>a;
       </ekz>
       <ekz>
	 <tld/>ulo de unu monato &gcirc;is kvin jaroj
	 <fnt><bib>MT</bib>, <lok>&Lev; 27:6</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">oadet</trd>
   <trd lng="de">alt</trd>
   <trd lng="en">aged</trd>
   <trd lng="fr">&acirc;g&eacute;</trd>
   <trd lng="he">&#x5DE;&#x5D1;&#x5D5;&#x5D2;&#x5E8;</trd>
  <trdgrp lng="id">
     <trd>tua</trd>,
     <trd>ber<ind>umur</ind></trd>
   </trdgrp>
   <trd lng="nl">oud</trd>
   <trd lng="pl"><klr tip="amb">(maj&aogonek;cy)</klr>
   <ind>lat</ind></trd>
   <trd lng="pt">idoso</trd>
   <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j;
  <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
  <trd lng="tr">ya&scedil;l&inodot;</trd>
</drv>

<drv mrk="agx.flor0o">
  <kap>flor<tld/>o</kap>
  <snc mrk="agx.flor0o.KOMUNE">
    <dif>
      A&gcirc;o de florado, de prospera juneco:
      <ekz>
        kie oni volas esti en sia matura <tld/>o,
        tien oni devas strebi jam en sia flor<tld/>o!
        <fnt>
        <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
        <vrk>Fabloj kaj Aforismoj</vrk>
        <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fleur de l'&acirc;ge</trd>,
    <trd>printemps <klr>de la vie</klr></trd>
  </trdgrp>
</drv>

<drv mrk="agx.grand0a">
   <kap>grand<tld/>a</kap>
   <snc mrk="agx.grand0a.KOMUNE">
     <dif>
       Jam tre <ref tip="super" cel="jun.mal0a">maljuna</ref>:
       <ekz>
	 kiel vi ne hontas, grand<tld/>a viro, tiel brui pro kelkaj
	 mankantaj butonoj
	 <fnt><bib>FK</bib> <lok>3, anekdotoj</lok></fnt>!
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
   <trdgrp lng="br">
     <trd>kozh-tre</trd>,
     <trd>er maez a <ind>oad</ind></trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>aged</trd>,
     <trd>of great <ind>age</ind></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>de grand <ind>&acirc;ge</ind></trd>,
     <trd>tr&egrave;s <ind>&acirc;g&eacute;</ind></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>tua</trd>,
     <trd>ber<ind>umur</ind></trd>
   </trdgrp>
   <trd lng="nl">oud</trd>
   <trd lng="pl">wiekowy</trd>
   <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
</drv>

