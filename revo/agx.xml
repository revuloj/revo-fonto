      <ref tip="vid" cel="jun.mal0igxi">maljuni&gcirc;i</ref>:
      <ekz>
        li &scirc;ajne neniel &scirc;an&gcirc;i&gcirc;is a&ubreve;
        pli<tld/>i&gcirc;is
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd
    lng="th">&#xE21;&#xE35;&#xE2D;&#xE32;&#xE22;&#xE38;&#xE21;&#xE32;&#xE01;&#xE02;&#xE36;&#xE49;&#xE19;</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; <ind>&c_s;&c_t;&c_a;&c_r;&c_e;&c_j;&c_sh;&c_y;&c_m;</ind></trd>
  <trdgrp lng="en">
    <trd>age</trd>
    <trd>grow older</trd>
  </trdgrp>
