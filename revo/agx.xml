<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&gcirc;</rad>/o
</kap>
<drv mrk="agx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kiomo de la tempo pasinta depost la naski&gcirc;o:
      <ekz>
        kia estas via <tld/>o?<fnt>Z</fnt>
      </ekz>
      <ekz>
        kioman, kian <tld/>on vi havas?
      </ekz>
      <ekz>
        en la <tld/>o de ok tagoj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Biblio</vrk>,
        <lok>Genezo 17, 12</lok>)</fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de dudek jaroj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Biblio</vrk>,
        <lok>Nombroj 1, 20</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_r;&c_o;&c_s;&c_t;</trd>
  <trd lng="cs">st&aacute;&rcaron;&iacute;</trd>
  <trd lng="de">Alter</trd>
  <trd lng="en">age</trd>
  <trd lng="fr">&acirc;ge</trd>
  <trd lng="it">et&agrave;</trd>
  <trd lng="nl">leeftijd</trd>
  <trd lng="pt">idade</trd>
  <trd lng="tr">ya&scedil;</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</trd>
  <trd lng="sv">&aring;lder</trd>
</drv>

<drv mrk="agx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti atinginta la <tld/>on de:
      <ekz>
        kiom li <tld/>as? li estas dekkvinjara.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&ecirc;tre <ind>&acirc;g&eacute;</ind> <klr>(de)</klr></trd>
  <trd lng="pt">ter a <ind>idade</ind> <klr>(de)</klr></trd>
</drv>

<drv mrk="agx.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Havanta ian <tld/>on:
      <ekz>
        mi estas pli <tld/>a ol vi;
      </ekz>
      <ekz>
        esti 5 jarojn <tld/>a;
      </ekz>
      <ekz>
        <tld/>ulo de unu monato &gcirc;is kvin jaroj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Biblio</vrk>,
        <lok>Levidoj 27, 6</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">alt</trd>
  <trd lng="fr">ag&eacute;</trd>
  <trd lng="nl">oud</trd>
  <trd lng="pt">idoso</trd>
</drv>

<drv mrk="agx.plen0a">
  <kap>plen<tld/>a</kap>
  <snc>
    <dif>
      Havanta la <tld/>on, kiu donas &ccirc;iujn civitanajn rajtojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="grand.0a">granda</ref>,
      <ref cel="profun.0a">profunda</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_n;&c_a;&c_l;&c_je;&c_t;&c_n;&c_ib;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="de">vollj&auml;hrig</trd>
  <trd lng="fr">majeur</trd>
  <trd lng="nl">meerderjarig</trd>
  <trdgrp lng="pt">
    <trd>adulto</trd>,
    <trd>maior de idade</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
