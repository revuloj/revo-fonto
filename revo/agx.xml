</drv>

<drv mrk="agx.infan0o">
   <kap>infan<tld/>o</kap>
   <snc mrk="agx.infan0o.KOMUNE">
     <dif>
       Unua vivotempo inter nasko kaj jun<tld/>o:
       <ekz>
         li komencis blindi&gcirc;i jam en sia infan<tld/>o
         <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj
         &ccirc;uk&ccirc;oj</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="th">&#3623;&#3633;&#3618;&#3648;&#3604;&#3655;&#3585;</trd>
   </snc>
   <trdgrp lng="br">
     <trd>bugaleaj</trd>,
     <trd>bugeliezh</trd>
   </trdgrp>
   <trd lng="cs">d&ecaron;tstv&iacute;</trd>
   <trd lng="de">Kindheit</trd>
   <trd lng="en">infancy <klr tip="amb">(age)</klr></trd>
   <trd lng="fr">enfance<klr tip="amb"> (&acirc;ge)</klr></trd>
   <trdgrp lng="id">
     <trd><ind>masa</ind> kanak-kanak</trd>,
     <trd><ind>masa</ind> kecil</trd>
   </trdgrp>
   <trd lng="nl">jeugd</trd>
   <trd lng="pl">dzieci&nacute;stwo</trd>
   <trd lng="ro">copil&abreve;rie</trd>
   <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
   <trd lng="sv">barndom</trd>
