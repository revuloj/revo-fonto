</drv>

<drv mrk="agx.plen0a">
   <kap><ofc>*</ofc>plen<tld />a <fnt><bib>F</bib>, <lok>Universala
   Vortaro, <url
   ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5161">plen'a&gcirc;'</url></lok></fnt></kap>
   <snc mrk="agx.plen0a.POL">
     <dif>
       Havanta la <tld />on, kiu donas &ccirc;iujn civitanajn rajtojn:
       <ekz>
         &ccirc;iuj plen<tld />aj lo&gcirc;antoj de amba&ubreve; seksoj
         havas la rajton de vo&ccirc;donado
         <fnt><bib>FK</bib>, <lok>El la vivo kaj sciencoj</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="th">&#3610;&#3619;&#3619;&#3621;&#3640;&#3609;&#3636;&#3605;&#3636;&#3616;&#3634;&#3623;&#3632;</trd>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
     <trd>&c_p;&c_a;&c_w;&c_n;&c_a;&c_l;&c_je;&c_t;&c_n;&c_ib;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
   </trdgrp>
   <trd lng="br">deuet (d'e oad gour)</trd>
   <trd lng="de">vollj&auml;hrig</trd>
   <trd lng="en">over the age of majority</trd>
   <trd lng="es">mayor de edad</trd>
   <trd lng="fr">majeur<klr tip="amb"> (civiquement)</klr></trd>
   <trd lng="ja">&#38738;&#24180;&#12398; [<ind>&#12379;&#12356;&#12397;&#12435;&#12398;</ind>]</trd>
   <trd lng="nl">meerderjarig</trd>
   <trd lng="pl">pe&lstroke;noletni</trd>
   <trdgrp lng="pt">
     <trd>adulto</trd>,
     <trd>maior de idade</trd>
   </trdgrp>
   <trd lng="ro">major</trd>
   <trdgrp lng="ru">
     <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>
   </trdgrp>
