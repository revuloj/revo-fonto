       </ekz>
     </dif>
     <trd lng="th">&#3623;&#3633;&#3618;&#3619;&#3640;&#3656;&#3609;, &#3623;&#3633;&#3618;&#3627;&#3609;&#3640;&#3656;&#3617;&#3626;&#3634;&#3623;</trd>
   </snc>
