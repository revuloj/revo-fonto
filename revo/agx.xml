<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&gcirc;</rad>/o
</kap>
<drv mrk="agx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kiomo de la tempo pasinta depost la naski&gcirc;o a&ubreve; esti&gcirc;o:
      <ekz>
        kia estas via <tld/>o?<fnt>Z</fnt>
      </ekz>
      <ekz>
        kioman <tld/>on vi havas?
      </ekz>
      <ekz>
        kian <tld/>on mi havas
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>?
      </ekz>
      <ekz>
        en la <tld/>o de ok tagoj
        <fnt><bib>MT</bib>, <lok>&Gen; 17:12</lok></fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de dudek jaroj
        <fnt><bib>MT</bib>, <lok>&Nom; 1:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_r;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">oad</trd>
  <trd lng="cs">st&aacute;&rcaron;&iacute;</trd>
  <trd lng="de">Alter</trd>
  <trd lng="en">age</trd>
  <trd lng="es">edad</trd>
  <trd lng="fr">&acirc;ge</trd>
  <trd lng="it">et&agrave;</trd>
  <trd lng="nl">leeftijd</trd>
  <trd lng="pt">idade</trd>
  <trd lng="tr">ya&scedil;</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</trd>
  <trd lng="sk">vek</trd>
  <trd lng="sv">&aring;lder</trd>
</drv>

<drv mrk="agx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti atinginta la <tld/>on de:
      <ekz>
        kiom li <tld/>as? li estas dekkvinjara;
      </ekz>
      <ekz>
        tiu &ccirc;i tombo <tld/>as almena&ubreve; jarcenton
        <fnt><bib>Kiso</bib>, <lok>tombeja arabesko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">beza&ntilde; <ind>oadet</ind> a</trd>
  <trd lng="de">alt sein</trd>
  <trd lng="en">be the age of</trd>
  <trd lng="fr">&ecirc;tre <ind>&acirc;g&eacute;</ind> <klr>(de)</klr></trd>
  <trd lng="pt">ter a <ind>idade</ind> <klr>(de)</klr></trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
</drv>

<drv mrk="agx.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Havanta ian <tld/>on:
      <ekz>
        mi estas pli <tld/>a ol vi;
      </ekz>
      <ekz>
        esti 5 jarojn <tld/>a;
      </ekz>
      <ekz>
        <tld/>ulo de unu monato &gcirc;is kvin jaroj
        <fnt><bib>MT</bib>, <lok>&Lev; 27:6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">oadet</trd>
  <trd lng="de">alt</trd>
  <trd lng="en">aged</trd>
  <trd lng="fr">&acirc;g&eacute;</trd>
  <trd lng="nl">oud</trd>
  <trd lng="pt">idoso</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
</drv>

<drv mrk="agx.grand0a">
  <kap>grand<tld/>a</kap>
  <snc>
    <dif>
      Jam tre <ref tip="super" cel="jun.mal0a">maljuna</ref>:
      <ekz>
        kiel vi ne hontas, grand<tld/>a viro, tiel brui pro kelkaj
        mankantaj butonoj
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trdgrp lng="br">
    <trd>kozh-tre</trd>,
    <trd>er maez a <ind>oad</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de grand <ind>&acirc;ge</ind></trd>,
    <trd>tr&egrave;s <ind>ag&eacute;</ind></trd>
  </trdgrp>
  <trd lng="nl">oud</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
</drv>

<drv mrk="agx.grand0ulo">
  <kap>grand<tld/>ulo</kap>
  <snc>
    <dif>
      Ulo tre maljuna:
      <ekz>
        ilia respektego anta&ubreve; la grand<tld/>ulo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a, Babel</lok></fnt>
        .
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">personne &acirc;g&eacute;e</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_i;&c_k;</trd>
</drv>

<drv mrk="agx.infan0o">
  <kap>infan<tld/>o</kap>
  <snc>
    <dif>
      Unua vivotempo inter nasko kaj jun<tld/>o:
      <ekz>
        li komencis blindi&gcirc;i jam en sia infan<tld/>o
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj &ccirc;uk&ccirc;oj, (du skizoj), unua
        skizo: Filip Onkudimov, 1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>bugaleaj</trd>,
    <trd>bugeliezh</trd>
  </trdgrp>
  <trd lng="fr">enfance<klr tip="amb"> (&acirc;ge)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="agx.jun0o">
  <kap>jun<tld/>o</kap>
  <snc>
    <dif>
      Vivotempo, dum kiu oni estas juna:
      <ekz>
        mi memoras tagojn de mia jun<tld/>o
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">yaouankiz</trd>
  <trd lng="fr">jeunesse<klr tip="amb"> (&acirc;ge)</klr></trd>
  <trd lng="ru">&c_ju;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="agx.mez0a">
  <kap>mez<tld/>a</kap>
  <snc>
    <dif>
      Proksimume en la mezo de normale longa vivo, nek tre juna, nek tre
      maljuna:
      <ekz>
        mez<tld/>a nerazita viro en kalvi&gcirc;inta leda jako
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">etre daou <ind>oad</ind></trd>
  <trd lng="fr">d'<ind>&acirc;ge</ind> moyen</trd>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_n;&c_je;&c_g;&c_o; <ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;&c_a;</ind></trd>
</drv>

<drv mrk="agx.mez0ulo">
  <kap>mez<tld/>ulo</kap>
  <snc>
    <dif>
      Ulo de meza <tld/>o, nek tre juna, nek tre maljuna:
      <ekz>
        la viro, mez<tld/>ulo, estis Hagaj, iama sklavo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 2a, antikva foiro
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">homme d'&acirc;ge moyen</trd>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k; &c_s;&c_r;&c_je;&c_d;&c_n;&c_je;&c_g;&c_o; &c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;&c_a;</trd>
</drv>

<drv mrk="agx.mond0o">
  <kap>mond<tld/>o</kap>
  <snc mrk="agx.mond0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Longa <ref tip="super" cel="epok.0o">epoko</ref>, parto de la
      homa a&ubreve; universa historio:
      <ekz>
        tiel estos &ccirc;e la fino de la mond<tld/>o
        <fnt><bib>NT</bib>, <lok>&Mat; 13:40</lok></fnt>;
      </ekz>
      <ekz>
        la sa&gcirc;eco ka&scirc;ita, kiun Dio anta&ubreve;destinis anta&ubreve; la mond<tld/>oj por
        nia glorado
        <fnt><bib>NT</bib>, <lok>&Ko1; 2:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">oadvezh</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;riode<klr tip="amb"> (historique)</klr></trd>,
    <trd>si&egrave;cles<klr tip="amb"> (longue &eacute;poque)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_p;&c_o;&c_h;&c_a;</trd>
</drv>

<drv mrk="agx.neplen0a">
  <kap>neplen<tld/>a</kap>
  <snc mrk="agx.neplen0a.POL">
    <dif>
      Ankora&ubreve; ne atinginta la <tld/>on de plenaj sociaj rajtoj:
      <ekz>
        Ijob volonte konsentis kontroladi la administradon de la bienoj de
        Omar dum la neplen<tld/>o de ties filino
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 23a, paca Betra&hcirc;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">dindan an <ind>oad</ind></trd>
  <trd lng="fr">mineur<klr tip="amb"> (civiquement)</klr></trd>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="agx.plej0ulo">
  <kap>plej<tld/>ulo</kap>
  <snc mrk="agx.plej0ulo.POL">
    <ref tip="dif" cel="agx.pli0ulo.POL">pli<tld/>ulo</ref>
    <ekz>
      &ccirc;e &ccirc;iu fenestro staris tri skribistoj kaj unu urba plej<tld/>ulo
      <fnt><bib>Fab3</bib>, <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>a&icirc;n&eacute;<klr tip="amb"> (subst.)</klr></trd>,
    <trd>ancien<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_je;&c_j;&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="agx.plen0a">
  <kap>plen<tld/>a</kap>
  <snc mrk="agx.plen0a.POL">
    <dif>
      Havanta la <tld/>on, kiu donas &ccirc;iujn civitanajn rajtojn:
      <ekz>
        &ccirc;iuj plen<tld/>aj lo&gcirc;antoj de amba&ubreve; seksoj havas la rajton de
        vo&ccirc;donado
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_n;&c_a;&c_l;&c_je;&c_t;&c_n;&c_ib;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="br">deuet (d'e oad gour)</trd>
  <trd lng="de">vollj&auml;hrig</trd>
  <trd lng="en">over the age of majority</trd>
  <trd lng="es">mayor de edad</trd>
  <trd lng="fr">majeur<klr tip="amb"> (civiquement)</klr></trd>
  <trd lng="nl">meerderjarig</trd>
  <trdgrp lng="pt">
    <trd>adulto</trd>,
    <trd>maior de idade</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">plnolet&yacute;</trd>
</drv>

<drv mrk="agx.pli0ulo">
  <kap>pli<tld/>ulo</kap>
  <snc mrk="agx.pli0ulo.POL">
    <dif>
      Iu el la pli <tld/>aj, respektataj homoj en grupo a&ubreve; socio:
      <ekz>
        kiam la &ccirc;efpastroj kaj pli<tld/>uloj akuzis lin, li respondis
        nenion
        <fnt><bib>NT</bib>, <lok>&Mat; 27:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>a&icirc;n&eacute;<klr tip="amb"> (subst.)</klr></trd>,
    <trd>ancien<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="de">&Auml;lterer</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_je;&c_j;&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="agx.profund0a">
  <kap>profund<tld/>a</kap>
  <snc>
    <ref tip="dif" cel="agx.grand0a">grand<tld/>a</ref>
    <ekz>
      Abraham <klr>...</klr> mortis en bona maljuneco, profund<tld/>a kaj
      sata de vivo
      <fnt><bib>NT</bib>, <lok>&Gen; 25:8</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="br">er maez a <ind>oad</ind></trd>
  <trdgrp lng="fr">
    <trd>combl&eacute; de jours</trd>,
    <trd>de grand <ind>&acirc;ge</ind></trd>,
    <trd>tr&egrave;s <ind>ag&eacute;</ind></trd>
  </trdgrp>
  <trd lng="nl">oud</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
</drv>

<drv mrk="agx.profund0ulo">
  <kap>profund<tld/>ulo</kap>
  <snc>
    <ref tip="dif" cel="agx.grand0ulo">grand<tld/>ulo</ref>
    <ekz>
      kiel viro, tiel anka&ubreve; virino estos kaptita, maljunulo kaj
      profund<tld/>ulo
      <fnt><bib>MT</bib>, <lok>&Jer; 6:11</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">personne &acirc;g&eacute;e</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_i;&c_k;</trd>
</drv>

<drv mrk="agx.sam0ulo">
  <kap>sam<tld/>ulo</kap>
  <snc mrk="agx.sam0ulo.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Ulo de proksimume sama a&gcirc;o kiel iu alia, same juna a&ubreve; maljuna:
      <ekz>
        per nenio &scirc;i similis al siaj sam<tld/>ulinoj en Kj&ouml;ge
        <fnt><bib>Fab3</bib>, <lok>sub la saliko</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="genera.0o">generacio</ref>
    </refgrp>
  </snc>
  <trd lng="br">den <ind>kenoad</ind></trd>
  <trd lng="fr">personne de m&ecirc;me &acirc;ge</trd>
  <trd lng="ru">&c_r;&c_o;&c_v;&c_je;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="agx.temp0o">
  <kap>temp<tld/>o</kap>
  <snc mrk="agx.temp0o.KAL">
    <ref tip="dif" cel="agx.mond0o">mond<tld/>o</ref>
    <ekz>
      kie estas la diskutisto de &ccirc;i tiu temp<tld/>o
      <fnt><bib>NT</bib>, <lok>&Ko1; 1:20</lok></fnt>?
    </ekz>
  </snc>
  <trd lng="br">oadvezh</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;riode<klr tip="amb"> (historique)</klr></trd>,
    <trd>si&egrave;cles<klr tip="amb"> (longue &eacute;poque)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_p;&c_o;&c_h;&c_a;</trd>
</drv>
</art>
