   <trd lng="ja">&#x672A;&#x6210;&#x5E74;&#x306E; [<ind>&#x307F;&#x305B;&#x3044;&#x306D;&#x3093;&#x306E;</ind>]</trd>
   <trd lng="nl">minderjarig</trd>
