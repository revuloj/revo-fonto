<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trombo.xml,v 1.23 2017/11/06 07:10:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>trombon</rad>/o
</kap>
<drv mrk="trombo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Latuna <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>
      pli granda ol trumpeto kun du tuboj &scirc;ovglitantaj unu en la
      alia por
      modifi la tonon:
        <ekz>
        kiu dekomence ludis funebran mar&scirc;on en muta &scirc;ajnigo sur imaga
        <tld/>o
             <fnt>
        <aut>Trad. Edwin Grobe</aut>,
        <vrk>Tri Noveloj de Usona Verkisto Bret Harte</vrk>,
        <lok><url ref=
        "http://www.gutenberg.org/ebooks/20931">
        gutenberg.org</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_b;&c_o;&c_n;</trd>
  <trd lng="ca">tromb&oacute;</trd>
  <trdgrp lng="cs">
    <trd>pozoun</trd>,
    <trd>tromb&oacute;n</trd>
  </trdgrp>
  <trd lng="de">Posaune</trd>
  <trd lng="en">trombone</trd>
  <trd lng="es">tromb&oacute;n</trd>
  <trd lng="fr">trombone<klr> (musique)</klr></trd>
  <trdgrp lng="hu">
    <trd>harsona</trd>,
    <trd>puz&oacute;n</trd>
  </trdgrp>
  <trd lng="id">trombon</trd>
  <trd lng="it">trombone</trd>
  <trd lng="nl">schuiftrompet</trd>
  <trdgrp lng="pl">
    <trd>trombon</trd>,
    <trd>puzon</trd>
  </trdgrp>
  <trd lng="pt">trombone</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_m;&c_b;&c_o;&c_n;</trd>
  <trd lng="sk">tromb&oacute;n</trd>
</drv>
</art>
