<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>trombon</rad>/o
</kap>
<drv mrk="trombo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Kupra <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>
      pli granda ol trumpeto kun du tuboj &scirc;ovglitantaj unu en la alia por
      modifi la tonon.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_b;&c_o;&c_n;</trd>
  <trd lng="en">trombone</trd>
  <trd lng="es">tromb&oacute;n</trd>
  <trd lng="de">Posaune</trd>
  <trd lng="fr">trombone<klr> (musique)</klr></trd>
  <trdgrp lng="hu">
    <trd>harsona</trd>,
    <trd>puz&oacute;n</trd>
  </trdgrp>
  <trd lng="nl">schuiftrompet</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_m;&c_b;&c_o;&c_n;</trd>
</drv>
</art>
