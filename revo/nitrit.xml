<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nitrit.xml,v 1.10 2023/09/03 06:36:21 revo Exp $">
<kap><rad>nitrit</rad>/o</kap>

<drv mrk="nitrit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref>
      de <ref cel="nitrit.0a_acido">nitrita acido</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_y;&c_t;</trd>
  <trd lng="cs">dusitan</trd>
  <trd lng="de">Nitrit</trd>
  <trd lng="en">nitrite</trd>
  <trd lng="fr">nitrite</trd>
  <trd lng="hu">nitrit</trd>
  <trd lng="ja">&#x4E9C;&#x785D;&#x9178;&#x5869;  [<ind>&#x3042;&#x3057;&#x3087;&#x3046;&#x3055;&#x3093;&#x3048;&#x3093;</ind>]</trd>
  <trd lng="nl">nitriet</trd>
  <trd lng="pl">azotyn</trd>
  <trd lng="pt">nitrito</trd>
  <trd lng="ro">azotit</trd>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_i;&c_t;</trd>
  <trd lng="sk">dusitan</trd>
  <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_i;&c_t;</trd>
</drv>

<drv mrk="nitrit.0a_acido">
  <kap><tld/>a acido</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Neorganika <ref tip="super" cel="acid.0o">acido</ref>
      (<frm>HNO<sub>2</sub></frm>), 
      uzata en industrio:
      <ekz>
        oni transformas la aminojn per <tld/>a acido al duazoniaj kombina&jcirc;oj, bazmaterialoj de
        azokoloriga&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=84879">Nitrata acido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_o;&c_c;&c_ib;&c_s;&c_t;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</ind></trd>
  <trd lng="cs">kyselina dusit&aacute;</trd>
  <trd lng="de">salpetrige S&auml;ure</trd>
  <trd lng="en">nitrous acid</trd>
  <trd lng="fr">acide nitreux</trd>
  <trd lng="nl">salpeterigzuur</trd>
  <trd lng="pl">kwas <ind>azotawy</ind></trd>
  <trd lng="pt">&aacute;cido nitroso</trd>
  <trd lng="ro">acid azotos</trd>
  <trd lng="ru">&c_a;&c_z;&c_o;&c_t;&c_i;&c_s;&c_t;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">kyselina dusit&aacute;</trd>
  <trd lng="uk">&c_a;&c_z;&c_o;&c_t;&c_i;&c_s;&c_t;&c_a; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
</drv>

</art>

