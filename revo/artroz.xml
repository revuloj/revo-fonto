<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: artroz.xml,v 1.14 2023/10/28 08:33:09 revo Exp $">
<kap>
    <rad>artroz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="artroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="artroz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Neinflama degenera kronika afekcio de la <ref cel="artik.0o">artikoj</ref>:
      <ekz>
        <tld lit="A"/>o estas neinflama degenera procezo de artiko
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</trd>
  <trd lng="cs">artr&oacute;za</trd>
  <trdgrp lng="en">
    <trd>osteoarthritis</trd>,
    <trd>arthrosis</trd>
  </trdgrp>
  <trd lng="fr">arthrose</trd>
  <trd lng="nl">artrose</trd>
  <trd lng="no">artrose</trd>
  <trd lng="sk">artr&oacute;za</trd>
  <trd lng="uk">&c_a;&c_r;&c_t;&c_r;&#x43E;&c_z;</trd>
  <trdgrp lng="zh">
    <trd>&#x5173;&#x8282;&#x708E; <pr>gu&#x101;nji&#xE9;y&#xE1;n</pr></trd>,
    <trd>&#x95DC;&#x7BC0;&#x708E; <pr>gu&#x101;nji&#xE9;y&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="artroz.genu0o">
  <kap>genu<tld/>o</kap>
  <snc mrk="artroz.genu0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> de la genua
      artiko:
      <ekz>
         La <tld/>o de la kokso (koks<tld/>o) estas malpli ofta ol tiu
         de la genuo (genu<tld/>o)
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_n;&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_z; &c_k;&c_a;&c_l;&c_je;&c_n;&c_n;&c_a;&c_g;&c_a; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="en">gonarthrosis</trd>
  <trd lng="fr">gonarthrose</trd>
  <trdgrp lng="nl">
    <trd>artrose van de knie</trd>,
    <trd>gonartrose</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>artrose i kne</trd>,
    <trd>gonartrose</trd>
  </trdgrp>
</drv>

<drv mrk="artroz.koks0o">
  <kap>koks<tld/>o</kap>
  <snc mrk="artroz.koks0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> de la koksa
      artiko:
      <ekz>
         La <tld/>o de la kokso (koks<tld/>o) estas malpli ofta ol tiu
         de la genuo (genu<tld/>o)
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_k;&c_s;&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_z; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a; &c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">koxartr&oacute;za</trd>
  <trd lng="en">coxarthrosis</trd>
  <trd lng="fr">coxarthrose</trd>
  <trdgrp lng="nl">
    <trd>artrose van de heup</trd>,
    <trd>coxartrose</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>hofteleddsartrose</trd>,
    <trd>artrose i hofte</trd>
    <trd>coxartrose</trd>
  </trdgrp>
  <trd lng="sk">koxartr&oacute;za</trd>
</drv>

<drv mrk="artroz.lumb0o">
  <kap>lumb<tld/>o</kap>
  <snc mrk="artroz.lumb0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> &ccirc;e la lumba
      vertebraro:
      <ekz>
         La vertebraro estas anka&ubreve; ofta loko trafita de la <tld/>o, &ccirc;efe
         &ccirc;e la nuka kaj lumba eta&gcirc;oj (nuk<tld/>o, lumb<tld/>o).
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ju;&c_m;&c_b;&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_z; &c_p;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_j; &c_h;&c_r;&c_y;&c_b;&c_je;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="en">lumbar arthrosis</trd>
  <trd lng="fr">lombarthrose</trd>
  <trdgrp lng="nl">
    <trd>artrose van de lendenwervels</trd>,
    <trd>lumbartrose</trd>
  </trdgrp>
  <trd lng="no">artrose i korsryggen</trd>
</drv>

<drv mrk="artroz.nuk0o">
  <kap>nuk<tld/>o</kap>
  <snc mrk="artroz.nuk0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> &ccirc;e la nuka
      vertebraro:
      <ekz>
         La vertebraro estas anka&ubreve; ofta loko trafita de la <tld/>o, &ccirc;efe
         &ccirc;e la nuka kaj lumba eta&gcirc;oj (nuk<tld/>o, lumb<tld/>o).
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_r;&c_v;&c_ib;&c_k;&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_z; &c_sh;&c_y;&c_j;&c_n;&c_y;&c_h; &c_h;&c_r;&c_y;&c_b;&c_je;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">cervicarthrose</trd>
  <trdgrp lng="nl">
    <trd>artrose van de nekwervels</trd>,
    <trd>cervicartrose</trd>
  </trdgrp>
  <trd lng="no">artrose i nakken</trd>
  <trd lng="uk">&c_a;&c_r;&c_t;&c_r;&#x43E;&c_z; &#x43F;&#x43E;&c_t;&c_i;&#x43B;&c_i;&c_ch;&#x43D;&c_i;&c_h; &c_h;&c_r;&c_je;&c_b;&c_c;&c_ib;&c_v;</trd>
</drv>

<drv mrk="artroz.poli0o">
  <kap>poli<tld/>o</kap>
  <snc mrk="artroz.poli0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Plurartika <ref tip="malprt" cel="artroz.0o">artrozo</ref>:
      <ekz>
         Asocio de fingra <tld/>o, lumba <tld/>o, kaj <tld/>o &ccirc;e almena&ubreve;
         du gravaj artikoj de la malsupraj membroj, nomi&gcirc;as poli<tld/>o.
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_n;&c_y; <ind>&c_a;&c_r;&c_t;&c_r;&c_o;&c_z;</ind></trd>
  </trdgrp>
  <trd lng="en">polyarthrosis</trd>
  <trd lng="fr">polyarthrose</trd>
  <trd lng="nl">poly-artrose</trd>
  <trd lng="no">polyartrose</trd>
</drv>
</art>

