<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>artroz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="artroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="artroz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Neinflama degenera kronika afekcio de la <ref
      cel="artik.0o">artikoj</ref>.
    </dif>
  </snc>
  <trd lng="en">arthrosis</trd>
  <trd lng="fr">arthrose</trd>
</drv>

<drv mrk="artroz.genu0o">
  <kap>genu<tld/>o</kap>
  <snc mrk="artroz.genu0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> de la genua
      artiko.
    </dif>
  </snc>
  <trd lng="en">gonarthrosis</trd>
  <trd lng="fr">gonarthrose</trd>
</drv>

<drv mrk="artroz.koks0o">
  <kap>koks<tld/>o</kap>
  <snc mrk="artroz.koks0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> de la koksa
      artiko.
    </dif>
  </snc>
  <trd lng="en">coxarthrosis</trd>
  <trd lng="fr">coxarthrose</trd>
</drv>

<drv mrk="artroz.lumb0o">
  <kap>lumb<tld/>o</kap>
  <snc mrk="artroz.lumb0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> &ccirc;e la lumba
      vertebraro.
    </dif>
  </snc>
  <trd lng="en">lumbar arthrosis</trd>
  <trd lng="fr">lombarthrose</trd>
</drv>

<drv mrk="artroz.nuk0o">
  <kap>nuk<tld/>o</kap>
  <snc mrk="artroz.nuk0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> &ccirc;e la nuka
      vertebraro.
    </dif>
  </snc>
  <trd lng="fr">cervicarthrose</trd>
</drv>

<drv mrk="artroz.poli0o">
  <kap>poli<tld/>o</kap>
  <snc mrk="artroz.poli0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Plurartika <ref tip="malprt" cel="artroz.0o">artrozo</ref>.
    </dif>
  </snc>
  <trd lng="en">polyarthrosis</trd>
  <trd lng="fr">polyarthrose</trd>
</drv>
</art>

