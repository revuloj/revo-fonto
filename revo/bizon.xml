<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bizon.xml,v 1.26 2023/08/31 08:48:51 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>bizon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bizon.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Bison</trd>), granda sova&gcirc;a mamulo kun du
      nordamerikaj kaj du e&ubreve;ropaj specioj:
      <ekz>
        paleo-indianoj &ccirc;asis la nun formortintan mamuton kaj antikvan
        <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33671">Roka
          Montaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj nun migras la&ubreve; la mallong-herbaj prerioj de Kanado
        unufoje depost ilia preska&ubreve;-formortigo komence  de la 20-a
        jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128202">Preria
          bizono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
         Nacia Parko de la Arbara <tld lit="B"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=116">Alberto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/bizon.jpg">
    juna <ind>e&ubreve;ropa <tld/>o</ind> <trd lng="la">Bison bonasus</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_z;&c_o;&c_n; <klr>(nordamerika)</klr></trd>,
    <trd>&c_z;&c_u;&c_b;&c_r; <klr>(e&ubreve;ropa)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_i;&c_z;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Bison <klr>(nordamerika)</klr></trd>,
    <trd>Wisent <klr>(e&ubreve;ropa)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bison</trd>,
    <trd>buffalo <klr>(apokrife)</klr></trd>
  </trdgrp>
  <trd lng="es">bisonte</trd>
  <trd lng="fr">bison</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B4;&#x5D9;&#x5D6;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>b&ouml;l&eacute;ny <klr>(nordamerika)</klr></trd>,
    <trd>eur&oacute;pai <ind>b&ouml;l&eacute;ny</ind> <klr>(e&ubreve;ropa)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30A4;&#x30BD;&#x30F3;</trd>,
    <trd>&#x30E4;&#x30AE;&#x30E5;&#x30A6;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bizon <klr>(nordamerika)</klr></trd>,
    <trd>wisent <klr>(e&ubreve;ropa)</klr></trd>
  </trdgrp>
  <trd lng="pl">&zdot;ubr</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_z;&c_o;&c_n; <klr>(nordamerika)</klr></trd>,
    <trd>&c_z;&c_u;&c_b;&c_r; <klr>(e&ubreve;ropa)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x456;&#x437;&#x43E;&#x43D;</trd>
</drv>
</art>
