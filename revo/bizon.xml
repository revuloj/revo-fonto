<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>bizon</rad>/o
    </kap>
    <drv mrk="bizon.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
	<dif>
	  Genro el familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>
	  (<trd lng="la">Bison</trd>), granda sova&gcirc;a mamulo kun du
	  nordamerikaj kaj du e&ubreve;ropaj specioj.
	</dif>
      </snc>
      <bld lok="http://131.211.121.124/REVO/NovajBildoj/bizono.jpg">
	E&ubreve;ropa <tld/>o (<trd lng="la">bison bonasus</trd>)
      </bld>
      <trdgrp lng="de">
	<trd>Bison <klr>(nordamerika)</klr></trd>,
	<trd>Wisent <klr>(e&ubreve;ropa)</klr></trd>
      </trdgrp>
      <trd lng="fr">bison</trd>
      <trdgrp lng="nl">
	<trd>bizon <klr>(nordamerika)</klr></trd>
	<trd>wisent <klr>(e&ubreve;ropa)</klr></trd>
      </trdgrp>
      <trd lng="pl">&zdot;ubr</trd>
    </drv>
  </art>
