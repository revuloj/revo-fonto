<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>substrat</rad>/o
</kap>
<drv mrk="substr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Ia ajn reala&jcirc;o, substanca a&ubreve; fenomena, kiu ebligas la ekziston de
      alia, kaj kvaza&ubreve; subportas &gcirc;in.
    </dif>
    <trd lng="hu">szubsztr&aacute;tum</trd>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Bazo, fundamenta ideo de doktrino.
    </dif>
    <trd lng="hu">eszmei <ind>alap</ind></trd>    
  </snc>
  <trd lng="be">&c_s;&c_u;&c_b;&c_s;&c_t;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Substrat</trd>
  <trd lng="nl">substraat</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_b;&c_s;&c_t;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>
