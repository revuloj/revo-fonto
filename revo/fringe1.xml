<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fringe1.xml,v 1.11 2022/03/20 13:07:55 revo Exp $">
<kap>
  <rad>fringed</rad>/oj
</kap>
<drv mrk="fringe1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> de <ref tip="malprt" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Fringillidae</trd>):
      <ekz>
        la dieto de la idoj de <tld/>oj inkludas variajn kvantojn de malgrandaj
        artropodoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17682">Fringedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
   <refgrp tip="prt">
     <ref cel="fringe.0o">fringelo (verda kardelo)</ref>,
     <ref cel="fring.0o">fringo</ref>,
     <ref cel="kanabe.0o">kanabeno</ref>,
     <ref cel="kanari.0o">kanario</ref>,
     <ref cel="kardel.0o">kardelo</ref>,
     <ref cel="pirol.0o">pirolo</ref>
   </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_ju;&c_r;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>,
    <trd>&c_ju;&c_r;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="de">Finken</trd>
  <trd lng="fr">fringillid&eacute;s</trd>
  <trd lng="hu">pintyf&eacute;l&eacute;k</trd>
  <trd lng="pt">fringil&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_mol;&c_ju;&c_r;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_v;&c_mol;&c_ju;&c_r;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x2019;&#x44E;&#x440;&#x43A;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
