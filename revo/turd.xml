<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>turd</rad>/o
</kap>
<drv mrk="turd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="turd.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda arbara birdo de mezvarmaj landoj
      kun bela kanto, genro el familio <ref tip="super"
      cel="turded.0oj"><tld/>edoj</ref>
      (<trd lng="la">Turdus</trd>):
      <ekz>
        nur merlo kaj <tld/>o restas en la arbaro, kaj kelkfoje en 
        a&ubreve;roro ekbruas plengor&gcirc;e ilia glorkanto
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">
          Slavonia Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_z;&c_d;</trd>
  <trd lng="cs">drozd</trd>
  <trd lng="de">Drossel</trd>
  <trd lng="en">thrush</trd>
  <trd lng="fr">grive</trd>
  <trd lng="hu">rig&oacute;</trd>
  <trd lng="nl">lijster</trd>
  <trd lng="pl">drozd</trd>
  <trd lng="pt">turdo</trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_z;&c_d;</trd>
</drv>
</art>
