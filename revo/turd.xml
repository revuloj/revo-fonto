  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Turdidae</trd>).
    </dif>
  </snc>
