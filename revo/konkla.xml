<kap>
  <rad>konklav</rad>/o
</kap>
<drv mrk="konkla.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkla.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      <ref tip="super" cel="ven.kun0o.KOMUNE">Kunveno</ref> de la
      kardinaloj por elekto de papo:
      <ekz>
        Domo de sankta Marta <klr>...</klr> estas la domo en kiu
        lo&gcirc;as la kardinaloj dum la <tld lit="K"/>o
        <fnt>
        <aut>IKUE</aut>,
        <vrk><url ref="http://www.ikue.org/papoj/konklavo/terminoj.html">Vortoj de
        la Konklavo</url></vrk>,
        <lok>Konklavo 2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_l;&c_a;&c_w;</trd>
  <trd lng="ca">conclave</trd>
  <trd lng="cs">konkl&#xE1;ve</trd>
  <trd lng="de">Konklave</trd>
  <trd lng="en">conclave</trd>
  <trd lng="es">c&oacute;nclave</trd>
  <trd lng="fr">conclave</trd>
  <trd lng="hu">konkl&aacute;v&eacute;</trd>
  <trd lng="it">conclave</trd>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x7687;&#x9078;&#x6319;&#x4F1A;&#x8B70;  <pr>&#x304D;&#x3087;&#x3046;&#x3053;&#x3046;&#x305B;&#x3093;&#x304D;&#x3087;&#x304B;&#x3044;&#x304E;</pr></trd>,
    <trd>&#x30B3;&#x30F3;&#x30AF;&#x30E9;&#x30FC;&#x30D9;</trd>
  </trdgrp>
  <trd lng="nl">conclaaf</trd>
  <trd lng="pl">konklawe</trd>
  <trd lng="pt">conclave</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_k;&c_l;&c_a;&c_v;</trd>
  <trd lng="sk">konkl&#xE1;ve</trd>
</drv>
