<kap>
  <rad>perkal</rad>/o
</kap>
<drv mrk="perkal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Delikata densa <ref tip="super" cel="katun.0o">katuno</ref>:
      <ekz>
        &scirc;i portis puran <tld/>an veston
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 5a, p.
        25a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i a&ccirc;etis anta&ubreve; nelonge <tld/>on por ses &ccirc;emizoj por la frateto
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_k;&c_a;&c_l;&c_mol;</trd>
  <trd lng="cs">perk&aacute;l</trd>
  <trd lng="de">Perkal</trd>
  <trdgrp lng="en">
    <trd>percale</trd>,
    <trd>cotton cambric</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>percale</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>perk&aacute;l</trd>,
    <trd>sz&iacute;nes <ind>karton</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x30D1;&#x30FC;&#x30B1;&#x30FC;&#x30EB;</trd>
  <trd lng="nl">perkal</trd>
  <trd lng="pl">perkal</trd>
  <trd lng="pt">percal</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_k;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sk">jemn&aacute; bavlnen&aacute; l&aacute;tka</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x43A;&#x430;&#x43B;&#x44C; (&#x442;&#x43A;&#x430;&#x43D;&#x438;&#x43D;&#x430;)</trd>
</drv>
