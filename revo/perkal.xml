<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>perkal</rad>/o
</kap>
<drv mrk="perkal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Delikata densa <ref tip="super" cel="katun.0o">katuno</ref>:
      <ekz>
        &scirc;i portis puran <tld/>an veston
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 5a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_k;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Perkal</trd>
  <trd lng="en">percale</trd>
  <trdgrp lng="fr">
    <trd>percale</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>perk&aacute;l</trd>,
    <trd>sz&iacute;nes <ind>karton</ind></trd>
  </trdgrp>
  <trd lng="pt">percal</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_k;&c_a;&c_l;&c_mol;</trd>
</drv>
</art>
