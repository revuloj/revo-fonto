<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>soft</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="soft.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Milde <ref tip="super" cel="lauxt.mal0a">malla&ubreve;ta</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>,
    <trd>&c_m;&c_ja;&c_k;&c_k;&c_ib; <klr tip="amb">(&c_g;&c_u;&c_k;, &c_g;&c_o;&c_l;&c_a;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="es">bajo<klr> (sonido)</klr></trd>
  <trd lng="fr">doux<klr> (son)</klr></trd>
  <trd lng="hu">l&aacute;gy hang&uacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_l;&c_u;&c_sh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_ja;&c_g;&c_k;&c_i;&c_j;<klr tip="amb"> (&c_z;&c_v;&c_u;&c_k;, &c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="soft.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="son.0i">Soni</ref> malla&ubreve;te, karese:
      <ekz>
        &leftquot;tio, kaj amuza kaj kortu&scirc;a&rightquot; <tld/>is &scirc;ia vo&ccirc;o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 289a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_h;&c_a; &c_g;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_a; &c_g;&c_a;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>doux</ind><klr> (son)</klr></trd>
  </trdgrp>
  <trd lng="hu">l&aacute;gy hangon sz&oacute;l</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_u;&c_ch;&c_a;&c_t;&c_mol; &c_t;&c_i;&c_h;&c_o;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol; &c_t;&c_i;&c_h;&c_o;</trd>
  </trdgrp>
</drv>
</art>
