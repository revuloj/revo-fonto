<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>soft</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="soft.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Milde <ref tip="super" cel="lauxt.mal0a">malla&ubreve;ta</ref>.
    </dif>
  </snc>
  <trd lng="es">bajo<klr> (sonido)</klr></trd>
  <trd lng="fr">doux<klr> (son)</klr></trd>
  <trd lng="hu">l&aacute;gy hang&uacute;</trd>
</drv>

<drv mrk="soft.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="son.0i">Soni</ref> malla&ubreve;te, karese:
      <ekz>
        &leftquot;tio, kaj amuza kaj kortu&scirc;a&rightquot; <tld/>is &scirc;ia vo&ccirc;o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 289a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>doux</ind><klr> (son)</klr></trd>
  </trdgrp>
  <trd lng="hu">l&aacute;gy hangon sz&oacute;l</trd>
</drv>
</art>

