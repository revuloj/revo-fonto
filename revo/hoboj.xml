<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hoboj</rad>/o
</kap>
<drv mrk="hoboj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika ligna <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>
      kun duobla langeto kaj klavoj.
    </dif>
  </snc>                   
  <trd lng="be">&c_g;&c_a;&c_b;&c_o;&c_j;</trd>
  <trd lng="br">oboell</trd>
  <trd lng="de">Oboe</trd>
  <trdgrp lng="el">
    <trd>&omikron_ton;&my;&pi;&omikron;&epsilon;</trd>,
    <trd>&omikron;&xi;&ypsilon_ton;&alfa;&ypsilon;&lambda;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="es">oboe</trd>
  <trd lng="fr">hautbois</trd>
  <trd lng="hu">oboa</trd>
  <trd lng="nl">hobo</trd>
  <trd lng="pt">obo&eacute;</trd>
  <trd lng="ru">&c_g;&c_o;&c_b;&c_o;&c_j;</trd>
</drv>
</art>
