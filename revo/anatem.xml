<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anatem</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="anatem.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Solena eklezia malbeno.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Bann</trd>,
      <trd>Kirchenbann</trd>
  </trdgrp>
  <trd lng="en">anathema</trd>
  <trd lng="fr">anath&egrave;me</trd>
  <trd lng="nl">banvloek</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_f;&c_je;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_k;&c_l;&c_ja;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">kliatba</trd>
</drv>

<drv mrk="anatem.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Solene malbeni iun a&ubreve; ian opinion.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd><klr>mit dem </klr>Bann belegen</trd>,
      <trd><klr>mit dem </klr>Kirchenbann belegen</trd>,
      <trd>verfluchen</trd>
  </trdgrp>
  <trd lng="en">anathematize</trd>
  <trd lng="fr">jeter l'<ind>anath&egrave;me</ind> (&agrave;, sur)</trd>
  <trd lng="nl">in de <ind>ban</ind> slaan</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_a;&c_n;&c_a;&c_f;&c_je;&c_m;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_k;&c_l;&c_i;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">preklia&tcaron;</trd>
</drv>
</art>
