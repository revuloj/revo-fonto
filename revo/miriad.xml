<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miriad.xml,v 1.21 2024/01/26 17:40:06 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>miriad</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="miriad.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="miriad.0o.nombro">
    <dif>
      Dek mil.
    </dif>
  </snc>
  <snc mrk="miriad.0o.KOMUNE">
    <dif>
      Nenombreble granda nombro:
      <ekz>
        <tld/>oj da steloj en la &ccirc;ielo;
      </ekz>
      <ekz>
        mi ne timas <tld/>ojn da kontra&ubreve;uloj, kiuj estas &ccirc;irka&ubreve; mi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de la homamaso kunkolekti&gcirc;is tiel, ke ili piedpremis
        unu la alian
        <fnt>
          <bib>NT</bib>
          <lok>Luko 12:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen la Sinjoro venis kun Siaj sanktaj <tld/>oj
        <fnt>
          <bib>NT</bib>
          <lok>Judas 1:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia nombro estis <tld/>oj da <tld/>oj kaj miloj da miloj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj da venenaj mu&scirc;oj kaj kuloj enflugadis al ili
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiaj malgrandaj insektoj <klr>[&#x2026;]</klr> povas en tri a&ubreve; kvar
        jaroj pretigi
        konstruon <klr>[&#x2026;]</klr> kun <tld/>oj da &ccirc;ambroj de diversa
        grandeco 
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amas.0o">amaso</ref>,
      <ref cel="abund.0o">abundo</ref>,
      <ref cel="mil.00o">milmilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_r;&c_y;&c_ja;&c_d;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_mol;&c_l;&c_ib;&c_ch;&c_o;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_b;&c_r;&c_o;&c_j;</trd>
  <trd lng="ca">mir&iacute;ada</trd>
  <trdgrp lng="cs">
    <trd>deset tis&iacute;c</trd>,
    <trd>myri&aacute;da</trd>,
    <trd>nes&ccaron;&iacute;sln&eacute; mno&zcaron;stv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Myriade</trd>,
     <trd>Unzahl</trd>
  </trdgrp>
  <trd lng="en">myriad</trd>
  <trd lng="fr">myriade</trd>
  <trdgrp lng="hu">
     <trd>sz&aacute;mtalan</trd>,
     <trd>miri&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x6570;  <pr>&#x3080;&#x3059;&#x3046;</pr></trd>,
    <trd>&#x4E00;&#x4E07;  <pr>&#x3044;&#x3061;&#x307E;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">myriade</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_i;&c_a;&c_d;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_m;&c_je;&c_t;&c_n;&c_o;&c_je; &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>myriada</trd>,
    <trd>nespo&ccaron;etn&eacute; mno&zcaron;stvo</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&#x435;&#x437;&#x43B;&#x456;&#x447;&#x435;&#x43D;&#x43D;&#x430; &#x43A;&#x456;&#x43B;&#x44C;&#x43A;&#x456;&#x441;&#x442;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E94;&#x82B1;&#x516B;&#x9580; <pr>w&#x1D4;hu&#x101;b&#x101;m&#xE9;n</pr></trd>,
    <trd>&#x4E94;&#x82B1;&#x516B;&#x95E8; <pr>w&#x1D4;hu&#x101;b&#x101;m&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>
