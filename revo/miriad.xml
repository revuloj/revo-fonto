<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>miriad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="miriad.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="miriad.0o.KOMUNE">
  <ref tip="dif" cel="mil.00o.KOMUNE">milmilo</ref>
    <ekz>
      <tld/>oj da steloj en la &ccirc;ielo;
    </ekz>
    <ekz>
      mi ne timas <tld/>ojn da kontra&ubreve;uloj<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_r;&c_y;&c_ja;&c_d;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_mol;&c_l;&c_ib;&c_ch;&c_o;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Myriade</trd>,
     <trd>Unzahl</trd>
  </trdgrp>
  <trd lng="en">myriad</trd>
  <trd lng="fr">myriade</trd>
  <trdgrp lng="hu">
     <trd>sz&aacute;mtalan</trd>,
     <trd>miri&aacute;d</trd>
  </trdgrp>
  <trd lng="nl">myriade</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_i;&c_a;&c_d;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_m;&c_je;&c_t;&c_n;&c_o;&c_je; &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>
