<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>fisk</rad>/o
</kap>
<drv mrk="fisk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="fisk.0o.kaso">
    <dif>
      &Scirc;tata trezorejo, regna kaso.
    </dif>
    <adm>
      Pola Radio havas uzon iom pli vastan de tiu senco. Gxi sxajne
      aplikas gxin al la tuto de sxtataj posedajxoj. Gxi uzas ankaux
      "fiskoministro", kiu sxajne ne identas kun "financoministro".
      <aut>MB</aut>
    </adm>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_a;&c_r;&c_b; <klr tip="amb">(&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_w;&c_n;&c_y;)</klr></trd>,
      <trd>&c_k;&c_a;&c_z;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Fiskus</trd>,
      <trd>Staatskasse</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="fisk.0o.administracio">
    <dif>
      &Scirc;tata administracio, kies tasko estas la enkasigo de la impostoj.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Fiskalbeh&ouml;rde</trd>,
      <trd>Finanzministerium</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </snc>
  <trd lng="es">fisco</trd>
  <trdgrp lng="fr">
    <trd>fisc</trd>,
    <trd><ind>Tr&eacute;sor</ind> Public</trd>,
    <trd><ind>Tr&eacute;sor</ind> de l'&Eacute;tat</trd>
  </trdgrp>
  <trd lng="nl">fiscus</trd>
</drv>

<drv mrk="fisk.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a le&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_s;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fiskalisch</trd>,
    <trd>Fiskal-</trd>
  </trdgrp>
  <trd lng="es">fiscal</trd>
  <trd lng="fr">fiscal</trd>
  <trd lng="nl">fiscaal</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_s;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
