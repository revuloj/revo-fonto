<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salamandr</rad>/o
</kap>
<drv mrk="salama.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salama.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="urodel.0oj">urodeloj</ref>,
      vosthava amfibio.
    </dif>
  </snc>
  <snc mrk="salama.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="salama.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Salamandra</trd>), idonaska surtere vivanta
      amfibio.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Salamander <klr>(kun ronda vosto)</klr></trd>,
    <trd>Molch <klr>(kun plata vosto)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_m;&a_n;&a_d;&a_r;</trd>
  <trd lng="fr">salamandre</trd>
  <trdgrp lng="hu">
    <trd>szalamandra</trd>,
    <trd>g&odblac;te</trd>
  </trdgrp>
  <trd lng="it">salamandra</trd>
  <trd lng="nl">salamander</trd>
  <trd lng="pl">salamandra</trd>
  <trdgrp lng="pt">
   <trd>salamandra</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
</drv>

<drv mrk="salama.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="urodel.0oj">urodeloj</ref>
      (<trd lng="la">Salamandridae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de"><klr tip="ind">(Echte)</klr> Salamander</trd>
  <trd lng="hu">szalamandraf&eacute;l&eacute;k</trd>
  <trd lng="it">salamandridi</trd>
  <trd lng="pl">salamandrowate</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y;<klr tip="ind">
    (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y;</ind> <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
