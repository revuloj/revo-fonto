<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salama.xml,v 1.27 2022/05/05 05:09:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salamandr</rad>/o
</kap>
<drv mrk="salama.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salama.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="urodel.0oj">urodeloj</ref>,
      vosthava amfibio.
    </dif>
  </snc>
  <snc mrk="salama.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Idonaska, surtere vivanta 
      <ref tip="lst" cel="amfibi.0o" lst="voko:amfibioj">amfibio</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="salama1.0oj">salamandredoj</ref>
      (<trd lng="la">Salamandra</trd>):
      <ekz>
        verajn <tld/>ojn, nigrajn kun oran&gcirc;aj makuloj, oni trovas en la ne&gcirc;monta
        regiono, &ccirc;efe post pluvoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007045.html">A&ubreve;strio: Et&scirc;puraj spuroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/21/Benny_Trapp_S%C3%BCdspanischer_Feuersalamander_Salamandra_longirostris.jpg/640px-Benny_Trapp_S%C3%BCdspanischer_Feuersalamander_Salamandra_longirostris.jpg" prm="CC BY 3.0"><tld/>o (Salamandra longirostris)
        <fnt>
          <aut>Benny Trapp, propra verko</aut>
          <lok><url ref="&WCW;/File:Benny_Trapp_S%C3%BCdspanischer_Feuersalamander_Salamandra_longirostris.jpg">&WCW;/File:Benny_Trapp_S&uuml;dspanischer_Feuersalamander_Salamandra_longirostris.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Feuersalamander</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>mlok</trd>,
    <trd>salamandr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Salamander <klr>(kun ronda vosto)</klr></trd>,
    <trd>Molch <klr>(kun plata vosto)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_m;&a_n;&a_d;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>salamandre</trd>,
    <trd>triton</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szalamandra</trd>,
    <trd>g&odblac;te</trd>
  </trdgrp>
  <trd lng="id">salamander</trd>
  <trd lng="it">salamandra</trd>
  <trd lng="nl">salamander</trd>
  <trd lng="pl">salamandra</trd>
  <trdgrp lng="pt">
   <trd>salamandra</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>mlok</trd>,
    <trd>salamandra</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x430;&#x43B;&#x430;&#x43C;&#x430;&#x43D;&#x434;&#x440;&#x430;</trd>
</drv>

</art>
