<vortaro>
<art mrk="$Id: salama.xml,v 1.33 2025/10/08 14:00:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salamandr</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="salama.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salama.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="urodel.0oj">urodeloj</ref>,
      vosthava amfibio.
    </dif>
  </snc>
  <snc mrk="salama.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Idonaska, surtere vivanta 
      <ref tip="lst" cel="amfibi.0o" lst="voko:amfibioj">amfibio</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="salama1.0oj">salamandredoj</ref>
      (<trd lng="la">Salamandra</trd>):
      <ekz>
        verajn <tld/>ojn, nigrajn kun oran&gcirc;aj makuloj, oni trovas en la ne&gcirc;monta
        regiono, &ccirc;efe post pluvoj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/007045.php">Et&scirc;puraj spuroj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/2/21/Benny_Trapp_S%C3%BCdspanischer_Feuersalamander_Salamandra_longirostris.jpg/640px-Benny_Trapp_S%C3%BCdspanischer_Feuersalamander_Salamandra_longirostris.jpg"
    prm="CC BY 3.0"><tld/>o (Salamandra longirostris)
        <fnt>
          <aut>Benny Trapp, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Benny_Trapp_S%C3%BCdspanischer_Feuersalamander_Salamandra_longirostris.jpg">&WCW;/File:Benny_Trapp_S&uuml;dspanischer_Feuersalamander_Salamandra_longirostris.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Feuersalamander</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>salamandra</trd>,
    <trd>salam&agrave;ndria</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mlok</trd>,
    <trd>salamandr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Salamander <klr>(kun ronda vosto)</klr></trd>,
    <trd>Molch <klr>(kun plata vosto)</klr></trd>
  </trdgrp>
  <trd lng="es">salamandra</trd>
  <trd lng="fa">&a_s;&a_m;&a_n;&a_d;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>salamandre</trd>,
    <trd>triton</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szalamandra</trd>,
    <trd>g&odblac;te</trd>
  </trdgrp>
  <trd lng="id">salamander</trd>
  <trd lng="it">salamandra</trd>
  <trdgrp lng="ja">
    <trd>&#12469;&#12531;&#12471;&#12519;&#12454;&#12454;&#12458;&#12398;&#39006;  <pr>&#12469;&#12531;&#12471;&#12519;&#12454;&#12454;&#12458;&#12398;&#12427;&#12356;</pr></trd>,
    <trd>&#12469;&#12521;&#12510;&#12531;&#12489;&#12521;&#12398;&#39006;  <pr>&#12469;&#12521;&#12510;&#12531;&#12489;&#12521;&#12398;&#12427;&#12356;</pr></trd>,
    <trd>&#28779;&#12392;&#12363;&#12370;  <pr>&#12402;&#12392;&#12363;&#12370;</pr></trd>
  </trdgrp>
  <trd lng="nl">salamander</trd>
  <trd lng="pl">salamandra</trd>
  <trdgrp lng="pt">
   <trd>salamandra</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>mlok</trd>,
    <trd>salamandra</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#34686; <pr>r&oacute;ng</pr></trd>,
    <trd>&#34833; <pr>r&oacute;ng</pr></trd>,
    <trd>&#39906; <pr>n&iacute;</pr></trd>,
    <trd>&#40117; <pr>n&iacute;</pr></trd>
  </trdgrp>
</drv>

