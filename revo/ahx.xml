<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&hcirc;</rad> <fnt>Z</fnt>
</kap>
<drv mrk="ahx.0">
  <kap><tld/></kap>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ahx.0.KOMUNE">
    <dif>
      Ekkrio pro forta emocio.
    </dif>
    <subsnc mrk="ahx.0.surprizo">
      <dif>
        Forta surprizo:
        <ekz>
          &#8212; <tld/> &#8212; ekkrias Injo, &#8212; kial do vi, pa&ccirc;jo, min distras per
          viaj parola&ccirc;oj
          <fnt><bib>Metrop</bib></fnt>?
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>ach</trd>,
        <trd>oha</trd>,
        <trd>was</trd>
      </trdgrp>
      <trd lng="en">oh</trd>
      <trdgrp lng="nl">
        <trd>ah</trd>,
        <trd>o</trd>,
        <trd>och</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="ahx.0.bedauxro">
      <dif>
        Forta beda&ubreve;ro:
        <ekz>
          <tld/>, pa&ccirc;jo, kiomfoje ni knaboj devis rea&ubreve;skulti vian
          rakontadon pri praavo
          <fnt>
            <bib>SkandalJozef</bib>,
            <lok>plendoj de Jozefo arestita</lok>
          </fnt>!
        </ekz>
      </dif>
      <trd lng="de">ach</trd>
      <trd lng="en">alas</trd>
      <trd lng="nl">ach</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_a;&c_h;</trd>
  <trdgrp lng="fr">
    <trd>ah</trd>,
    <trd>oh l&agrave; l&agrave;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_h;</trd>
  <trd lng="sk">ach</trd>
  <trd lng="vo">ag</trd>
</drv>

<drv mrk="ahx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ahx.0i.KOMUNE">
    <dif>
      Eldiri &leftquot;<tld/>&rightquot; pro ia emocio:
      <ekz>
        e&ccirc; iu ne konsekrita en misterojn de belo, nepre <tld/>us pro miro
        <fnt><bib>Mary</bib></fnt>;
      </ekz>
      <ekz>
        aparte interesa estas listo de <klr>...</klr> sonimitaj verboj
        <klr>...</klr> ekzemple <ctl><tld/>i</ctl>, <ctl>anheli</ctl>,
        <ctl>baci</ctl>
        <fnt>
        <aut>Ken Miner</aut>,
        <vrk><url
        ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">taglibro</url></vrk>,
        <lok>2003-10-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pousser des <ind>oh</ind> des ah</trd>
</drv>
</art>
