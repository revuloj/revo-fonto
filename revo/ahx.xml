<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ahx.xml,v 1.13 2020/12/17 14:40:00 revo Exp $">
<kap>
  <rad>a&hcirc;</rad> <fnt> <bib>ZR</bib></fnt>, 
  <var><kap><rad var="h">ah</rad></kap></var> 
</kap>
<drv mrk="ahx.0">
  <kap><tld/>, <var><kap><tld var="h"/></kap></var></kap>
 
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ahx.0.KOMUNE">
    <dif>
      <ref tip="super" cel="kri.ek0o">Ekkrio</ref> pro forta <ref cel="emoci.0o.KOMUNE">emocio</ref>.
    </dif>
    <subsnc mrk="ahx.0.surprizo">
      <dif>
	Forta <ref cel="surpri.0o">surprizo</ref>:
	<ekz>
	  &mdash; <tld/> &mdash; ekkrias Injo, &mdash; kial do vi, pa&ccirc;jo, min distras per
	  viaj parola&ccirc;oj
	  <fnt><bib>Metrop</bib></fnt>?
	</ekz>
      </dif>
      <trdgrp lng="de">
	<trd>ach</trd>,
	<trd>oha</trd>,
	<trd>was</trd>
      </trdgrp>
      <trd lng="en">oh</trd>
      <trdgrp lng="es">
        <trd>ah</trd>,
        <trd>oh</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>ah</trd>,
	<trd>o</trd>,
	<trd>och</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>ah</trd>,
        <trd>oh</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="ahx.0.bedauxro">
      <dif>
	Forta <ref cel="bedaux.0o">beda&ubreve;ro</ref>:
        <ekz>
          <tld var="h"/>, se mi scius, kio estas timo
          <fnt>
            <bib>ElektFab</bib>
            <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld var="h"/>, mi malfeli&ccirc;a kankro
          <fnt>
            <bib>ElektFab</bib>
            <lok>Doktoro &Ccirc;ioscia</lok>
          </fnt>!
        </ekz>
	<ekz>
	  <tld/>, pa&ccirc;jo, kiomfoje ni knaboj devis rea&ubreve;skulti vian
	  rakontadon pri praavo
	  <fnt>
	    <bib>SkandalJozef</bib>,
	    <lok>plendoj de Jozefo arestita</lok>
	  </fnt>!
	</ekz>
        <ekz>
          <tld/>, <tld/>! &dash; <klr>[&#8230;]</klr> langvori en mucida hum'
          <fnt>
            <aut>Raymond Schwartz</aut>
            <vrk><url ref="http://steloj.de/esperanto/butiko"
                >La Stranga Butiko</url></vrk>
            <lok>Lamentado</lok>
          </fnt>!
        </ekz>
      </dif>
      <trd lng="de">ach</trd>
      <trd lng="en">alas</trd>
      <trd lng="es">oh</trd>
      <trd lng="nl">ach</trd>
      <trd lng="pl">eh</trd>
    </subsnc>
  </snc>
  <ref tip="vid" cel="aha.0">aha</ref>
  <trd lng="be">&c_a;&c_h;</trd>
  <trdgrp lng="fr">
    <trd>ah</trd>,
    <trd>oh l&agrave; l&agrave;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_h;</trd>
  <trd lng="sk">ah</trd>
  <trd lng="vo">ag</trd>
</drv>

<drv mrk="ahx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ahx.0i.KOMUNE">
    <dif>
      Eldiri &leftquot;<tld/>&rightquot; pro ia emocio:
      <ekz>
	e&ccirc; iu ne konsekrita en misterojn de belo, nepre <tld/>us pro miro
	<fnt><bib>Mary</bib></fnt>;
      </ekz>
      <ekz>
	aparte interesa estas listo de <klr>[&#8230;]</klr> sonimitaj verboj
	<klr>[&#8230;]</klr> ekzemple <ctl><tld/>i</ctl>, <ctl>anheli</ctl>,
	<ctl>baci</ctl>
	<fnt>
	<aut>Ken Miner</aut>,
	<vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">taglibro</url></vrk>,
	<lok>2003-10-19</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">expresar emoci&oacute;n mediante oh y ah</trd>
  <trd lng="fr">pousser des <ind>oh</ind> des ah</trd>
  <trd lng="pl">wzdycha&cacute;</trd>
</drv>
</art>
