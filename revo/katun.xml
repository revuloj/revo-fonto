<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katun.xml,v 1.21 2023/09/01 13:58:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>katun</rad>/o
</kap>

<drv mrk="katun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Malpeza <ref tip="vid" cel="koton.0o">kotona</ref> teksa&jcirc;o,
      &gcirc;enerale surpresita per koloraj desegna&jcirc;oj:
      <ekz>
        <ref tip="sub" cel="indien.0o">indieno</ref>,
        <ref tip="sub" cel="kaliko.0o">kalikoto</ref>,
        <ref tip="sub" cel="kreton.0o">kretono</ref>,
        <ref tip="sub" cel="nanken.0o">nankeno</ref>,
        <ref tip="sub" cel="perkal.0o">perkalo</ref>
        estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_a;&c_w;&c_n;&c_ja;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_s;&c_m;&c_a;</trd>
  <trd lng="cs">katun</trd>
  <trdgrp lng="de">
    <trd>Kattun</trd>,
    <trd>Baumwollgewebe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cotton cloth</trd>,
    <trd>cotton fabric</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>toile de <ind>coton</ind></trd>,
    <trd>cotonnade</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>karton</trd>,
    <trd>pamutsz&ouml;vet</trd>
  </trdgrp>
  <trd lng="ja">&#x7DBF;&#x5E03;  [<ind>&#x3081;&#x3093;&#x3077;</ind>]</trd>
  <trd lng="nl">katoenen weefsel</trd>
  <trd lng="pl">katun</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_ch;&c_a;&c_t;&c_o;&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
  <trd lng="sk">kart&#xFA;n <klr>(text)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_v;&#x43E;&c_v;&#x43D;&#x44F;&#x43D;&c_a; &c_t;&#x43A;&c_a;&#x43D;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_s;&c_i;&c_t;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_b;&#x44F;&c_z;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
