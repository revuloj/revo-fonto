<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katun.xml,v 1.24 2023/12/03 08:00:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>katun</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="katun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Malpeza <ref tip="vid" cel="koton.0o">kotona</ref> teksa&jcirc;o,
      &gcirc;enerale surpresita per koloraj desegna&jcirc;oj:
      <ekz>
        <ref tip="sub" cel="indien.0o">indieno</ref>,
        <ref tip="sub" cel="kaliko.0o">kalikoto</ref>,
        <ref tip="sub" cel="kreton.0o">kretono</ref>,
        <ref tip="sub" cel="nanken.0o">nankeno</ref>,
        <ref tip="sub" cel="perkal.0o">perkalo</ref>
        estas <tld/>oj;
      </ekz>
      <ekz>
        kanapeto tegita per dis&scirc;irita <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fenestro estis kurtenigita per diverskolora <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> forigas oleomakulojn de silko, lintolo, <tld/>o
        <fnt>
          <bib>KrM</bib>
          <lok>32. Lupo Sopiras, Al Arbaro Sin Tiras.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_a;&c_w;&c_n;&c_ja;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_s;&c_m;&c_a;</trd>
  <trd lng="cs">katun</trd>
  <trdgrp lng="de">
    <trd>Kattun</trd>,
    <trd>Baumwollgewebe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cotton cloth</trd>,
    <trd>cotton fabric</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>toile de <ind>coton</ind></trd>,
    <trd>cotonnade</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>karton</trd>,
    <trd>pamutsz&ouml;vet</trd>
  </trdgrp>
  <trd lng="ja">&#x7DBF;&#x5E03;  <pr>&#x3081;&#x3093;&#x3077;</pr></trd>
  <trd lng="nl">katoenen weefsel</trd>
  <trd lng="pl">katun</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_ch;&c_a;&c_t;&c_o;&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
  <trd lng="sk">kart&uacute;n <klr>(text)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_v;&c_o;&c_v;&c_n;&c_ja;&c_n;&c_a; &c_t;&c_k;&c_a;&c_n;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_i;&c_t;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#x68C9;&#x5E03; <pr>mi&#xE1;nb&#xF9;</pr></trd>
</drv>
</art>
