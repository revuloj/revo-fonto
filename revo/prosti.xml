<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>prostitu</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="prosti.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="prosti.0i.AMOR">
    <dif>
      Devigi iun akiri monon per mal&ccirc;asta vivo:
      <ekz>
        ne senhonorigu vian
        filinon <tld/>ante &scirc;in<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>sin <tld/>i</ind>.
        <trd lng="fr">se <ind>prostituer</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">prostituieren</trd>
  <trd lng="es">prostituir</trd>
  <trd lng="fr">prostituer</trd>
  <trd lng="hu">prostitu&aacute;l</trd>
  <trd lng="it">prostituire</trd>
  <trd lng="nl">prostitueren</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">prostituovať sa</trd>
  <trd lng="sv">prostituera</trd>
</drv>

<drv mrk="prosti.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="prosti.0ado.AMOR">
    <dif>
      Vendado de mal&ccirc;astaj servoj:
      <ekz>
        le&gcirc;oj kontra&ubreve; la <tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">prostituci&oacute;n</trd>
  <trd lng="fr">prostitution</trd>
  <trd lng="hu">prostit&uacute;ci&oacute;</trd>
  <trd lng="nl">prostitutie</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">prostitúcia</trd>
</drv>

<drv mrk="prosti.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="prosti.0ejo.AMOR">
    <ref tip="dif" cel="bordel.0o">bordelo</ref>
  </snc>
  <trd lng="de">Bordell</trd>
  <trdgrp lng="es">
    <trd>prost&iacute;bulo</trd>,
    <trd>burdel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>claque<klr tip="ind"> (maison close)</klr></trd>,
    <trd>maison de prostitution</trd>,
    <trd>lupanar</trd>
  </trdgrp>
  <trd lng="hu">bord&eacute;ly(h&aacute;z)</trd>
  <trdgrp lng="it">
    <trd>bordello<klr> (casa chiusa)</klr></trd>, 
    <trd>lupanare</trd>,
    <trd>casa chiusa</trd>,
    <trd>casino<klr> (casa chiusa)</klr></trd>,
    <trd>casa di tolleranza</trd>, 
    <trd>casa d'appuntamenti</trd>, 
    <trd>casa di malaffare</trd>
  </trdgrp>
  <trd lng="nl">bordeel</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_u;&c_b;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; &c_d;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">nevestinec</trd>
  <trd lng="sv">bordell</trd>
</drv>

<drv mrk="prosti.0ito">
  <kap><tld/>ito</kap>
  <snc mrk="prosti.0ito.AMOR">
    <dif>
      Viro, kiun oni <tld/>as, a&ubreve; kiu sin <tld/>as
    </dif>
    <ref tip="vid" cel="amor.0isto">amoristo</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>prostitu&eacute;<klr> (subst.)</klr></trd>,
    <trd>gigolo</trd>,
    <trd>tapin<klr tip="ind"> (prostitu&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="it">gigol&ograve;</trd>
  <trd lng="es">prostituto</trd>
  <trd lng="nl">gigolo</trd>
</drv>

<drv mrk="prosti.0itino">
  <kap><tld/>itino</kap>
  <snc mrk="prosti.0itino.AMOR">
    <dif>
      Virino, kiun oni <tld/>as, a&ubreve; kiu sin <tld/>as;
      <ref tip="sin" cel="strat.0ulino.AMOR">stratulino</ref>;
      <ref tip="sin" cel="putin.0o.SEKS">putino</ref>.
    </dif>
    <ref tip="vid" cel="amor.0istino">amoristino</ref>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_a;&c_l;&c_y;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nutte</trd>,
    <trd>Hure</trd>,
    <trd>leichtes M&auml;dchen</trd>,
    <trd>Prostituierte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prostitute</trd>,
    <trd>hooker</trd>,
    <trd>whore</trd>,
    <trd>escort</trd>
  </trdgrp>
  <trd lng="es">prostituta</trd>
  <trdgrp lng="fr">
    <trd>prostitu&eacute;e<klr> (subst.)</klr></trd>,
    <trd>fille publique</trd>,
    <trd>fille de <ind>joie</ind></trd>,
    <trd>p&eacute;ripat&eacute;ticienne</trd>,
    <trd>tapineuse</trd>,
  </trdgrp>
  <trd lng="hu">prostitu&aacute;lt</trd>
  <trdgrp lng="it">
    <trd>prostituta</trd>,
    <trd>puttana</trd>,
    <trd>meretrice</trd>,
    <trd>marchettara</trd>
  </trdgrp>
  <trd lng="nl">prostituee</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>
  <trd lng="tr">orospu</trd>
  <trd lng="sk">prostit&uacute;tka</trd>
</drv>

<drv mrk="prosti.0isto">
  <kap><tld/>isto,
  <var><kap><tld/>istino</kap></var></kap>
  <snc mrk="prosti.0isto.AMOR">
    <dif>
      Homo kiu profesie ekspluatas la <tld/>adon.
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>proxeneta</trd>,
    <trd>rufi&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>maquereau<klr> (prox&eacute;n&egrave;te)</klr></trd>,
    <trd>souteneur</trd>,
    <trd>prox&eacute;n&egrave;te</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>futtat&oacute;</trd>,
    <trd>strici</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pooier</trd>,
    <trd>souteneur</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_t;&c_je;&c_n;&c_jo;&c_r;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_je;&c_n;&c_jo;&c_r;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">prostitútka</trd>
</drv>
</art>
