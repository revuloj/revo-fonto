<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>prostitu</rad>/i
</kap>
<drv mrk="prosti.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Devigi iun akiri monon per mal&ccirc;asta vivo: <ekz>ne senhonorigu vian
filinon <tld/>ante &scirc;in<fnt>Z</fnt>;
</ekz> 
<ekz>
  <ind>sin <tld/>i</ind>;
  <trd lng="fr">se <ind>prostituer</ind></trd>
</ekz> 
<ekz>
  <ind><tld/>it(in)o</ind>
  <klr>(publikul(in)o)</klr>;
  <trd lng="fr">prostitu&eacute;(e)</trd>
  <trd lng="hu">prostitu&aacute;lt</trd>
  <trd lng="nl">prostituee</trd>
</ekz> 
<ekz>
  <ind><tld/>ist(in)o</ind>
  <klr>(metie ekspluatanta la <tld/>it(in)ojn)</klr>;
  <trd lng="fr">prox&eacute;n&egrave;te</trd>
  <trdgrp lng="hu">
    <trd>futtat&oacute;</trd>,
    <trd>strici</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pooier</trd>,
    <trd>souteneur</trd>
  </trdgrp>
</ekz>  
<ekz>
  la le&gcirc;oj kontra&ubreve; la <ind><tld/>ado</ind>.
  <trd lng="fr">prostitution</trd>
  <trd lng="hu">prostit&uacute;ci&oacute;</trd>
  <trd lng="nl">prostitutie</trd>
</ekz>
</dif>
</snc>
<trd lng="de">prostituieren</trd>
<trd lng="fr">prostituer</trd>
<trd lng="hu">prostitu&aacute;l</trd>
<trd lng="nl">prostitueren</trd>
<trd lng="sv">prostituera</trd>
</drv>

    <drv mrk="prosti.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <ref tip="dif" cel="bordel.0o">bordelo.</ref>
      </snc>
      <trd lng="de">Bordell</trd>
      <trd lng="nl">bordeel</trd>
      <trd lng="hu">bord&eacute;ly(h&aacute;z)</trd>
      <trd lng="sv">bordell</trd>
    </drv>
    </art>
