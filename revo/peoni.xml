<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: peoni.xml,v 1.28 2024/02/17 06:43:06 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>peoni</rad>/o
</kap>
<drv mrk="peoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="peonia.0oj"><tld/>acoj</ref>
      (<trd lng="la">Paeonia</trd>), kun belaj grandaj multpetalaj
      floroj:
      <ekz>
        grandaj papavofloroj kaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub vitraj klo&scirc;oj staris delikataj hiacintoj kaj grandaj arbosimilaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ru&gcirc;i&gcirc;is kiel <tld/>o, mallevis la okulojn, preterpasis kvaza&ubreve; ne rekonante min
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/a/a3/Paeonia_officinalis_001.JPG/640px-Paeonia_officinalis_001.JPG" prm="CC BY-SA 3.0"><tld/>o (Paeonia officinalis)
       <fnt>
         <aut>H. Zell, propra verko</aut>
         <lok><url ref="&WCW;/File:Paeonia_officinalis_001.JPG">&WCW;/File:Paeonia_officinalis_001.JPG</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_p;&c_ib;&c_v;&c_o;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_b;&c_o;&c_zh;&c_u;&c_r;</trd>
  <trd lng="cs">pivo&ncaron;ka</trd>
  <trd lng="de">Pfingstrose</trd>
  <trd lng="en">peony</trd>
  <trdgrp lng="hu">
    <trd>bazsar&oacute;zsa</trd>,
    <trd>pe&oacute;nia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DC;&#x30BF;&#x30F3;  <pr>&#x307C;&#x305F;&#x3093;</pr></trd>,
    <trd>&#x30B7;&#x30E3;&#x30AF;&#x30E4;&#x30AF;</trd>
  </trdgrp>
  <trd lng="nl">pioenroos</trd>
  <trd lng="pl">piwonia</trd>
  <trd lng="pt">pe&ocirc;nia</trd>
  <trd lng="ro">bujor</trd>
  <trd lng="ru">&c_p;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">piv&oacute;nia</trd>
  <trd lng="uk">&#x43F;&#x456;&#x432;&#x43E;&#x43D;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x828D;&#x836F;&#x5C5E; <pr>sh&#xE1;oy&#xE0;osh&#x1D4;</pr></trd>,
    <trd>&#x828D;&#x85E5;&#x5C6C; <pr>sh&#xE1;oy&#xE0;osh&#x1D4;</pr></trd>,
    <trd>&#x828D; <pr>sh&#xE1;o</pr></trd>,
    <trd>&#x7261;&#x4E39; <pr>m&#x1D4;d&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
