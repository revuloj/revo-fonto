<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>peoni</rad>/o
</kap>
<drv mrk="peoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="peoni.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Paeonia</trd>), kun belaj grandaj multpetalaj
      floroj.
    </dif>
  </snc>
  <bld lok="../bld/peoni.jpg">&gcirc;ardene kulturata <tld/>o</bld>
  <trd lng="de">Pfingstrose</trd>
</drv>
<drv mrk="peoni.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Paeoniaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Pfingstrosengew&auml;chse</trd>
</drv>   
</art>
