<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>peoni</rad>/o
</kap>
<drv mrk="peoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="peonia.0oj"><tld/>acoj</ref>
      (<trd lng="la">Paeonia</trd>), kun belaj grandaj multpetalaj
      floroj.
    </dif>
  </snc>
  <bld lok="../bld/peoni.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="be">&c_p;&c_ib;&c_v;&c_o;&c_n;&c_ja;</trd>
  <trd lng="de">Pfingstrose</trd>
  <trdgrp lng="hu">
    <trd>bazsar&oacute;zsa</trd>,
    <trd>pe&oacute;nia</trd>
  </trdgrp>
  <trd lng="nl">pioenroos</trd>
  <trd lng="ru">&c_p;&c_i;&c_o;&c_n;</trd>
</drv>
</art>
