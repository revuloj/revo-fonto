<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grac.xml,v 1.20 2023/10/15 18:38:19 revo Exp $">
<kap>
  <rad>grac</rad>/o
</kap>
<drv mrk="grac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Spirita helpo senmerite donita de Dio al homo por savi&gcirc;o;
      Di<ref cel="favor.0o">favoro</ref>:
       <ekz>
        ne timu, Maria, &ccirc;ar vi trovis <tld/>on anta&ubreve; Dio
        <fnt>
          <bib>NT</bib>
          <lok>Luko 1:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o kaj la vero esti&gcirc;is per Jesuo Kristo
        <fnt>
          <bib>NT</bib>
          <lok>Johano 1:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        Stefano, plena de <tld/>o kaj potenco, faris grandajn mirinda&jcirc;ojn kaj signojn
        inter la popolo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 6:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        voja&gcirc;u al sanktejo, kie oni faradis pre&gcirc;ojn por kuracado kaj dia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2014/010918.php">Mirakloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o kaj beno, kiuj savas, kaj &ccirc;iam savos irlandanojn de totala malvenko,
        estas ilia nevenkebla humursento
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2011/010374.php">Plej optimisma popolo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_k;&c_a; <klr tip="amb">(&c_b;&c_o;&c_zh;&c_a;&c_ja;)</klr></trd>
  <trd lng="ca">gr&agrave;cia<klr tip="ind"> (divina)</klr></trd>
  <trd lng="cs">milost</trd>
  <trdgrp lng="de">
     <trd>Gnade</trd>,
     <trd>Huld</trd></trdgrp>
  <trdgrp lng="en">
    <trd>grace</trd>,
    <trd>mercy</trd>
  </trdgrp>
  <trd lng="es">gracia<klr tip="ind"> (divina)</klr></trd>
  <trd lng="fr">gr&acirc;ce<klr tip="ind"> (divine)</klr></trd>
  <trd lng="hu">kegyelem<klr tip="ind"> (isteni)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6069;&#x5BF5;  <pr>&#x304A;&#x3093;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x795E;&#x306E;&#x6075;&#x307F;  <pr>&#x304B;&#x307F;&#x306E;&#x3081;&#x3050;&#x307F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gratie<klr tip="ind"> (goedgunstigheid)</klr></trd>,
    <trd>gunst</trd>,
    <trd>goedgunstigheid</trd>
  </trdgrp>
  <trd lng="no">n&aring;de</trd>
  <trd lng="pt">gra&ccedil;a<klr tip="amb"> (favor de Deus)</klr></trd>
  <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">milos&tcaron;</trd>
  <trd lng="uk">&#x431;&#x43B;&#x430;&#x433;&#x43E;&#x434;&#x430;&#x442;&#x44C;</trd>
</drv>
</art>
