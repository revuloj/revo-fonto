<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salik</rad>/o
</kap>
<drv mrk="salik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="salikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Salix</trd>), arboj kaj arbedoj.
      La flekseblaj vergoj de kelkaj specioj estas uzataj en korbfarado.
    </dif>
  </snc>
  <trd lng="de">Weide</trd>
  <trd lng="nl">wilg</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_v;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>,
    <trd>&c_v;&c_je;&c_r;&c_b;&c_a; <klr>(kelkaj specioj)</klr></trd>
  </trdgrp>
</drv>
</art>
