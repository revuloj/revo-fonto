<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salik</rad>/o
</kap>
<drv mrk="salik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="salikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Salix</trd>), arboj kaj arbedoj.
      La flekseblaj vergoj de kelkaj specioj estas uzataj en korbfarado:
      <ekz>
        &scirc;ajnis al li, ke son&gcirc;e li
        vidis la junulinon sub la bran&ccirc;oj de la olda <tld/>o, kiu
        kreskis
        &ccirc;e la fonto<fnt><aut>Julian Modest</aut>: <vrk>La ikono</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 12a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_b;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_r;&c_b;&c_a;</trd>
  <trd lng="de">Weide</trd>
  <trd lng="en">willow</trd>
  <trd lng="fa"><klr>(&a_d;&a_r;&a_kh;&a_t;) </klr>&a_b;&f_y;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>saule</trd>
  </trdgrp>
  <trd lng="hu">f&udblac;z(fa)</trd>
  <trd lng="it">salice</trd>
  <trd lng="nl">wilg</trd>
  <trd lng="pl">wierzba</trd>
  <trdgrp lng="pt">
   <trd>salgueiro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_v;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>,
    <trd>&c_v;&c_je;&c_r;&c_b;&c_a; <klr>(kelkaj specioj)</klr></trd>
  </trdgrp>
</drv>
</art>
