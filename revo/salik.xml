<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>salik</rad>/o
</kap>
<drv mrk="salik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="salik.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Salix</trd>), arboj kaj arbedoj.
      La flekseblaj vergoj de kelkaj specioj estas uzataj en korbfarado.
    </dif>
  </snc>
  <trd lng="de">Weide</trd>
</drv>

</art>
