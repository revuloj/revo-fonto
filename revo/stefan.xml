<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>Stefan</rad>/o
</kap>
<drv mrk="stefan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo grekdevena:
      <ekz>
        mia frato diris al <tld/>o, ke li amas lin pli, ol sin mem
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>.
      </ekz>
    </dif>
  </snc>     
  <trd lng="br">Stefan</trd>
  <trdgrp lng="en">
    <trd>Stephen</trd>,
    <trd>Steven</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&Eacute;tienne</trd>,
    <trd>St&eacute;phane</trd>
  </trdgrp>
  <trd lng="hu">Istv&aacute;n</trd>
  <trd lng="it">Stefano</trd>
  <trdgrp lng="ru">
    <trd>&c_S;&c_t;&c_je;&c_p;&c_a;&c_n;</trd>,
    <trd>&c_S;&c_t;&c_je;&c_f;&c_a;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_E;&c_t;&c_mol;&c_je;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="stefan.in0o">
  <kap><tld/>ino</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Virina anta&ubreve;nomo grekdevena, kun la signifo de "la kronitino".
    </dif>
  </snc>
  <trd lng="fr">St&eacute;phanie</trd>
</drv>
</art>

