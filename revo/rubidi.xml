<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rubidi.xml,v 1.19 2022/03/26 19:19:20 revo Exp $">
<kap>
  <rad>rubidi</rad>/o
</kap>
<drv mrk="rubidi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Rb</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="37">elemento</ref> kun
      atomnumero 37, atompezo 85,47;
      alkala <ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <nom>Robert Bunsen</nom> <klr>[estas]</klr> malkovrinto
        de la kemiaj elementoj cezio kaj
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2904">Marie Curie</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas ege reagema, kaj povas ekbruli en aero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22434">Rubidio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_b;&c_ib;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_r;&c_u;&c_b;&c_i;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">rubidiom</trd>
  <trd lng="ca">rubidi</trd>
  <trdgrp lng="cs">
    <trd>Rb</trd>,
    <trd>rubidium</trd>
  </trdgrp>
  <trd lng="de">Rubidium</trd>
  <trd lng="en">rubidium</trd>
  <trd lng="es">rubidio</trd>
  <trd lng="fr">rubidium</trd>
  <trd lng="hu">rub&iacute;dium</trd>
  <trd lng="id">rubidium</trd>
  <trd lng="it">rubidio</trd>
  <trd lng="la">Rubidium</trd>
  <trd lng="nl">rubidium</trd>
  <trd lng="pl">rubid</trd>
  <trd lng="pt">rub&iacute;dio</trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_i;&c_d;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Rb</trd>,
    <trd>rub&iacute;dium</trd>
  </trdgrp>
  <trd lng="sv">rubidium</trd>
  <trd lng="uk">&#x440;&#x443;&#x431;&#x456;&#x434;&#x456;&#x439;</trd>
</drv>
</art>
