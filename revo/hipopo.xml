<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hipopotam</rad>/o
</kap>
<drv mrk="hipopo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hipopo.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Hippopotamus</trd>), granda dikha&ubreve;ta senhara
      mamulo, kun malaltaj kruroj, kiu vivas en riveroj kaj lagoj afrikaj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flusspferd</trd>,
    <trd>Nilpferd</trd>
  </trdgrp>
</drv>
<drv mrk="hipopo.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Hippopotamidae</trd>), vivantaj en riveroj, lagoj kaj
      mar&ccirc;oj.
    </dif>
  </snc>
  <trd lng="de">Flusspferde <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">hippos</trd>
</drv>
</art>
