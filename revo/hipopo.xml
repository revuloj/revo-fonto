<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>hipopotam</rad>/o
</kap>
<drv mrk="hipopo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hipopo1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Hippopotamus</trd>), granda dikha&ubreve;ta senhara
      mamulo, kun malaltaj kruroj, kiu vivas en riveroj kaj lagoj afrikaj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flusspferd</trd>,
    <trd>Nilpferd</trd>
  </trdgrp>
</drv>

</art>
