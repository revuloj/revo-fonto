<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hortul.xml,v 1.22 2022/04/30 08:42:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hortulan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hortul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hortul.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="emberi.0o">emberizo</ref>,
      kies karno estas tre delikata
      (<trd lng="la">Emberiza hortulana</trd>):
      <ekz>
        tiu <klr>[&#x2026;]</klr> popolo <klr>[&#x2026;]</klr>
        alveturigis &ccirc;iujn &ccirc;i fazanojn, <tld/>ojn,
        ostrojn kaj vinojn
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nesto de <tld/>o situas sur a&ubreve; &ccirc;egrunde
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=341651">Hortulano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fc/Ortolan_bunting_in_Sierra_de_Guara%2C_Aragon%2C_Spain.jpg/640px-Ortolan_bunting_in_Sierra_de_Guara%2C_Aragon%2C_Spain.jpg" prm="CC BY-SA 3.0"><tld/>o
        <fnt>
          <aut>Pierre Dalous, propra verko</aut>
          <lok><url ref="&WCW;/File:Embhor.jpg">&WCW;/File:Embhor.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_s;&c_ja;&c_n;&c_k;&c_a; &c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_s;&c_k;&c_a; &c_o;&c_v;&c_je;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">kilheri</trd>
  <trd lng="cs">strnad zahradn&iacute;</trd>
  <trdgrp lng="de">
    <trd>Ortolan</trd>,
    <trd>Gartenammer</trd>
  </trdgrp>
  <trd lng="fr">ortolan</trd>
  <trd lng="hu">kerti <ind>s&aacute;rm&aacute;ny</ind></trd>
  <trd lng="it">ortolano<klr> (zool.)</klr></trd>
  <trd lng="pl">ortolan</trd>
  <trd lng="pt">emberiz&iacute;deo</trd>
  <trd lng="ru">&c_o;&c_v;&c_s;&c_ja;&c_n;&c_k;&c_a; &c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;
  <klr>(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
  <trd lng="sk">strn&aacute;dka z&aacute;hradn&aacute;</trd>
  <trd lng="uk">&#x432;&#x456;&#x432;&#x441;&#x44F;&#x43D;&#x43A;&#x430; &#x441;&#x430;&#x434;&#x43E;&#x432;&#x430;</trd>
</drv>
</art>
