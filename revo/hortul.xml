<drv mrk="hortul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="emberi.0o">emberizo</ref>
      (<trd lng="la">Emberiza hortulana</trd>), kies karno estas tre delikata.
    </dif>
  </snc>
  <trd lng="de">Ortolan</trd>
  <trd lng="ru">&c_o;&c_v;&c_s;&c_ja;&c_n;&c_k;&c_a; &c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja; <klr>(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
</drv>
</art>
