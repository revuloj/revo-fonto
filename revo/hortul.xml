<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hortulan</rad>/o
</kap>
<drv mrk="hortul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="emberi.0o">emberizo</ref>
      (<trd lng="la">Emberiza hortulana</trd>), kies karno estas tre
      delikata.
    </dif>
  </snc>              
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_s;&c_k;&c_a; &c_o;&c_v;&c_je;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>       
  <trd lng="br">kilheri</trd>
  <trdgrp lng="de">
    <trd>Ortolan</trd>,
    <trd>Gartenammer</trd>
  </trdgrp>
  <trd lng="fr">ortolan</trd>
  <trd lng="hu">kerti <ind>s&aacute;rm&aacute;ny</ind></trd>
  <trd lng="it">ortolano<klr> (zool.)</klr></trd>
  <trd lng="pl">ortolan</trd>
  <trd lng="pt">emberiz&iacute;deo</trd>
  <trd lng="ru">&c_o;&c_v;&c_s;&c_ja;&c_n;&c_k;&c_a; &c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;
  <klr>(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
</drv>
</art>
