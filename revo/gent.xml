<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gent</rad>/o
</kap>
<drv mrk="gent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gent.0o.familio">
    <dif>
      Grupo da kelkaj familioj devenaj de unua sama prapatro, kune vivantaj
      kaj ordinare regataj de unu sama estro; subdivido de nacio; popoleto:
      <ekz>
        la sova&gcirc;aj <tld/>oj de Afriko;
      </ekz>
      <ekz>
        la <tld/>o de Jakob;
      </ekz>
      <ekz>
        la hebreaj <tld/>oj;
      </ekz>
      <ekz>
        la romanaj patriciaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili.0o">familio</ref>,
      <ref cel="klan.0o">klano</ref>,
      <ref cel="trib.0o">tribo</ref>,
      <ref cel="fratri.0o">fratrio</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_m;&c_ja;</trd>,
      <trd>&c_r;&c_o;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gent.0o.raso">
    <dif>
      <ref tip="dif" cel="ras.0o">Raso</ref>:
      <ekz>
        la homa <tld/>o;
      </ekz>
      <ekz>
        la germanaj <tld/>oj.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_l;&c_je;&c_m;&c_ja;</trd>
  </snc>
  <snc mrk="gent.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <ref tip="dif" cel="genr.0o.BIO">genro</ref>
  </snc>
  <adm>
  Neklaraj difinoj. Ankaux la difino de "raso" estas
  suspektinda. <aut>MB</aut>
  </adm>
  <trdgrp lng="fr">
    <trd>gens<klr tip="ind"> (ensemble de familles romaines)</klr></trd>,
    <trd>lign&eacute;e</trd>,
    <trd>peuplade</trd>,
    <trd>race</trd>,
    <trd>tribu</trd></trdgrp> 
</drv>
</art>

