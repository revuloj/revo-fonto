<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kapreol</rad>/o
</kap>
<drv mrk="kapreo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cerv.0edoj">cervedoj</ref>
      (<trd lng="la">Capreolus</trd>), rekonebla per la blanka posta&jcirc;o
      kaj la blankmakulita felo de la idoj.
    </dif>
  </snc>
  <trd lng="de">Reh</trd>
  <trd lng="en">roe</trd>
</drv>
</art>
