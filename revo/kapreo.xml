<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapreol</rad>/o
</kap>
<drv mrk="kapreo.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="huf.par0ulo"
      lst="voko:parhufuloj">Parhufulo</ref>
      rekonebla per la blanka posta&jcirc;o
      kaj la blankmakulita felo de la idoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="cerved.0oj">cervedoj</ref>
      (<trd lng="la">Capreolus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_u;&c_l;&c_ja;</trd>         
  <trd lng="br">yourc'h</trd>
  <trd lng="de">Reh</trd>
  <trdgrp lng="en">
    <trd>roe</trd>,
    <trd>roe buck</trd>,
    <trd>roe deer</trd>
  </trdgrp>
  <trd lng="es">corzo</trd>
  <trd lng="fa">&a_sh;&a_w;&f_k;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>chevreuil</trd>,
    <trd>chevrette<klr tip="ind"> (femelle du chevreuil)</klr></trd>
  </trdgrp>
  <trd lng="hu">&odblac;z</trd>
  <trd lng="nl">ree</trd>
  <trd lng="pl">sarna</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_u;&c_l;&c_ja;</trd>
</drv>
</art>
