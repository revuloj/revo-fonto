<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ulmac.xml,v 1.18 2017/04/17 16:10:32 revo Exp $">
<kap>
  <rad>ulmac</rad>/oj
</kap>
<drv mrk="ulmac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="ulmac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj"
      cel="famili.0o.BIO">Familio</ref> de lignoplantoj,
      multaj kreskantaj en tropikaj regionoj,
      el la ordo de rozoparencaj
      <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Ulmaceae</trd>) kun:
      folioj alternaj, nesimetriaj;
      kromfolioj frue defalantaj;
      floroj unuopaj a&ubreve; en faskoj;
      4 &gcirc;is 5 tepaloj;
      4 &gcirc;is 5 stamenoj;
      2 pistiloj;
      nuksoj alaj a&ubreve; drupoj:
      <ekz>
        la <ref tip="prt" cel="ulm.0o">ulmo</ref> estas 
        <klr>[...]</klr> genro el la familio de <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=20305'>Ulmo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_r;&c_ja;&c_s;&c_t;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">ulm&agrave;cies</trd>
  <trd lng="da">elme-familien</trd>
  <trd lng="de">Ulmengew&auml;chse</trd>
  <trd lng="en">ulmaceae</trd>
  <trd lng="es">ulm&aacute;ceas</trd>
  <trd lng="fi">jalavakasvit</trd>
  <trd lng="fr">ulmac&eacute;es</trd>
  <trd lng="hu">szil(fa)f&eacute;l&eacute;k</trd>
  <trd lng="it">ulmacee</trd>
  <trd lng="nl">iepenfamilie</trd>
  <trd lng="pl">wi&aogonek;zowate</trd>
  <trd lng="pt">ulm&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_l;&c_mol;&c_m;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_o;&c_v;&c_y;&c_je; <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sv">almv&auml;xter</trd>
</drv>
</art>
