<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skaben</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="skaben.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Alta urbestrara funkciulo en kelkaj landoj.
    </dif>
  </snc>
  <trd lng="ca">escab&iacute;</trd>
  <trdgrp lng="cs">
    <trd>konšel</trd>,
    <trd>radní</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Helfer des <ind>B&uuml;rgermeister</ind>s</trd>,
    <trd>Sch&ouml;ffe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>alderman</trd>,
    <trd>magistrate</trd>,
    <trd>sheriff</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;rosi <ind>tan&aacute;csnok</ind></trd>
  <trdgrp lng="nl">
    <trd>wethouder<klr tip="amb"> (Ned.)</klr></trd>,
    <trd>schepen<klr tip="amb"> (Belgi&euml;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_sh;&c_je;&c_v;&c_je;&c_n;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_o;&c_j; <ind>&c_s;&c_o;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</ind></trd>,
    <trd>&c_p;&c_o;&c_m;&c_o;&c_shch;&c_n;&c_i;&c_k; <ind>&c_m;&c_e;&c_r;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konšel</trd>,
    <trd>radný</trd>
  </trdgrp>
</drv>
</art>
