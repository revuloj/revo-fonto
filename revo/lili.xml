<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lili</rad>/o
</kap>
<drv mrk="lili.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="liliac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lilium</trd>), kun bulbo kaj grandaj bonodoraj
      floroj:
      <ekz>
        <tld/>o estas simbolo de pureco.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lilie</trd>
  <trd lng="el">&kappa;&rho;&jota_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">lily</trd>
  <trd lng="hu">liliom</trd>
  <trd lng="nl">lelie</trd>
  <trd lng="pl">lilia</trd>
  <trd lng="pt">l&iacute;rio</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sv">lilja</trd>
</drv>
</art>
