<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lili</rad>/o
</kap>
<drv mrk="lili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lili.0acoj"><tld/>acoj</ref> 
      (<trd lng="la">Lilium</trd>), kun bulbo, grandaj bonodoraj
      floroj: <ekz><tld/>o estas simbolo de pureco</ekz>.
    </dif> 
  </snc>
  <trd lng="de">Lilie</trd>
</drv>
<drv mrk="lili.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Liliaceae</trd>), al kiu apartenas
      ekzemple:
      <refgrp tip="sub">
        <ref cel="lili.0o">lilio</ref>,
        <ref cel="hiacin.0o">hiacinto</ref>,
        <ref cel="tulip.0o">tulipo</ref>,
        <ref cel="aspara.0o">asparago</ref>
      </refgrp> k.a.
    </dif>
  </snc>
  <trd lng="de">Liliengew&auml;chse</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>