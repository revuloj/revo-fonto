<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lili</rad>/o
</kap>
<drv mrk="lili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lili.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Lilium</trd>), kun bulbo kaj grandaj bonodoraj
      floroj:
      <ekz>
        <tld/>o estas simbolo de pureco.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lilie</trd>
</drv>
<drv mrk="lili.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Liliaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Liliengew&auml;chse</trd>
</drv>
</art>
