<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lili</rad>/o
</kap>
<drv mrk="lili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lili.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="liliac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lilium</trd>), kun bulbo kaj grandaj bonodoraj
      floroj:
      <ekz>
        <tld/>o estas simbolo de pureco;
      </ekz>
      <ekz>
        kial vi zorgas pri vesta&jcirc;o? Pripensu la <tld/>ojn de la kampo,
        kiel ili kreskas; ili ne laboras, nek &scirc;pinas
        <fnt><bib>NT</bib>, <lok>&Mat; 6:28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_l;&c_je;&c_ja;</trd>
  <trd lng="de">Lilie</trd>
  <trd lng="el">&kappa;&rho;&jota_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">lily</trd>
  <trdgrp lng="fr">
    <trd>lis</trd>,
    <trd>lys</trd>
  </trdgrp>
  <trd lng="hu">liliom</trd>
  <trd lng="nl">lelie</trd>
  <trd lng="pl">lilia</trd>
  <trd lng="pt">l&iacute;rio</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sv">lilja</trd>
</drv>

<drv mrk="lili.akvo0o">
  <kap><tld/>o</kap>
  <snc mrk="lili.akvo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="nufar.0o.BOT">nufaro</ref>
    <ekz>
      akvo<tld/>oj balanci&gcirc;adis sur la akvo
      <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda kristino</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Teichrose</trd>,
    <trd>Mummel</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;nuphar</trd>
  <trdgrp lng="hu">
    <trd>tavir&oacute;zsa</trd>,
    <trd>v&iacute;zit&ouml;k</trd>
  </trdgrp>
  <trd lng="nl">gele <ind>plomp</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_b;&c_y;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_jo;&c_l;&c_t;&c_a;&c_ja; <ind>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
</drv>
</art>
