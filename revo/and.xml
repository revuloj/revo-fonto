  <art mrk="$Id: and.xml,v 1.10 2023/10/28 08:33:06 afido Exp $">
    <kap>
      <rad>And</rad>/oj
    </kap>

<drv mrk="and.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="and.0oj.montaro">
    <dif>
      Granda <ref tip="lst" lst="voko:montaroj" cel="mont.0aro">montaro</ref>
      etendi&gcirc;anta la&ubreve;longe de la okcidenta bordo de <ref tip="malprt" cel="amerik.Sud0o">Sudameriko</ref>
      de <ref tip="malprt" cel="venezuel.V0o">Venezuelo</ref> &gcirc;is <ref tip="malprt" cel="cxili.CX0o">&Ccirc;ilio</ref>:
      <ekz>
        malvarmaj kaj sekaj zonoj en <tld/>oj, situantaj je alteco inter 3000 kaj 5000&#x202F;m
        super la marnivelo
        <fnt>
          <aut>Hose Marques</aut>
            <vrk><url ref="&Monato;/2000/007055.php">Voja&gcirc;o al la provinco Tukumano</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        unika fervojlinio <klr>[&#x2026;]</klr> komenci&gcirc;as en urbo <nom>Salta</nom>, supreniras &gcirc;is 4250
        metroj, trapasas <tld/>ojn kaj atingas fine la &ccirc;ilian havenurbon <nom>Antofagasta</nom>
        &ccirc;e la Pacifika Oceano
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php">Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz> 
      <ekz>
        <ref tip="prt" cel="alpak.0o.besto">alpako</ref> estas besto, kiu vivas en grandaj aroj en
        la altebena&jcirc;oj de la <tld/>oj,
        de 3500 &gcirc;is 5000 metrojn super marnivelo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010528.php">Bredi alpakojn</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Anden</trd>
    <trd lng="en">Andes</trd>
    <trd lng="fr">Cordill&egrave;re des Andes</trd>
    <trdgrp lng="zh">
    <trd>&#x5B89;&#x5730;&#x65AF;&#x5C71;&#x8108; <pr>&#x101;ndes&#x12B;sh&#x101;nm&#xE0;i</pr></trd>,
    <trd>&#x5B89;&#x5730;&#x65AF;&#x5C71;&#x8109; <pr>&#x101;ndes&#x12B;sh&#x101;nm&#xE0;i</pr></trd>,
    <trd>&#x5B89;&#x7B2C;&#x65AF;&#x5C71;&#x8108; <pr>&#x101;nd&#xEC;s&#x12B;sh&#x101;nm&#xE0;i</pr></trd>,
    <trd>&#x5B89;&#x7B2C;&#x65AF;&#x5C71;&#x8109; <pr>&#x101;nd&#xEC;s&#x12B;sh&#x101;nm&#xE0;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_A;&c_n;&c_d;&c_y;</trd>
  <trd lng="he">&#x5D0;&#x5E0;&#x5D3;&#x5D9;&#x5DD;</trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C7;&#x30B9;&#x5C71;&#x8108;  <pr>&#x30A2;&#x30F3;&#x30C7;&#x30B9;&#x3055;&#x3093;&#x307F;&#x3083;&#x304F;</pr></trd>
  <trd lng="uk">&c_g;&#x43E;&c_r;&c_i;&#xA0;&c_A;&#x43D;&c_d;&c_i;</trd>
