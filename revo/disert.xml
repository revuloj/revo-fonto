<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: disert.xml,v 1.25 2023/11/04 14:29:02 revo Exp $">
  <kap>
    <rad>disert</rad>/i <fnt><bib>PV</bib></fnt>
  </kap>

<drv mrk="disert.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="disert.0i.defendi">
    <dif>
      <ref tip="super" cel="verk.0i">Verki</ref> kaj prezenti al kompetenta
      faka instanco (universitato ktp)
      detalan studa&jcirc;on pri scienca, filozofia a&ubreve; literatura temo:
      <ekz>
        <klr>[<nom>Jouko Lindstedt</nom>]</klr> <tld/>is pri semantiko de la
        bulgara lingvo en 1985
        <fnt> <bib>Viki</bib>.
          <lok><url ref="&Viki;/Jouko_Lindstedt">Jouko Lindstedt</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="defend.0i.ordinare">defendi<sncref/></ref>,
      <ref cel="promoc.0igxi">promocii&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;
    <ind>&c_d;&c_y;&c_s;&c_e;&c_r;&c_t;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
  <trd lng="ca">dissertar</trd>
  <trdgrp lng="cs">
    <trd>obhajovat diserta&ccaron;n&iacute; pr&aacute;ci</trd>,
    <trd>p&rcaron;edlo&zcaron;it disertaci</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>dissertieren</trd>,
    <trd>seine Dissertation <ind>schreiben</ind></trd>,
    <trd>seine Doktorarbeit <ind>verfassen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dissert</trd>,
    <trd>dissertate</trd>,
    <trd>discuss</trd>,
    <trd>discourse</trd>
  </trdgrp>
  <trd lng="fr">disserter</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rtekezik</trd>,
    <trd>&eacute;rtekez&eacute;st &iacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#x8AD6;&#x3058;&#x308B;  <pr>&#x308D;&#x3093;&#x3058;&#x308B;</pr></trd>
  <trd lng="pl">przedstawia&cacute;
    <ind>dysertacj&eogonek;</ind></trd>
  <trd lng="pt">dissertar</trd>
  <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol; &c_d;&c_i;&c_s;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_i;&c_ju;</trd>
  <trd lng="sk">predlo&zcaron;i&tcaron; dizert&aacute;ciu</trd>
  <trd lng="uk">&c_z;&c_a;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_i; &c_d;&c_i;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_ib;&c_ju;</trd>
</drv>

<drv mrk="disert.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="disert.0ajxo.doktoriga">
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref> traktanta pri scienca,
      filozofia a&ubreve; literatura temo, kiun ties a&ubreve;toro prezentas (kaj publike defendas ties
      <ref tip="prt" cel="tez.0o.aserto">tezojn</ref>) &ccirc;e
      kompetenta instanco pruve pri sia faka kapablo por
      akiri sciencan gradon:
      <ekz>
        menciindas, ke la <tld/>a&jcirc;o de nia samideanino ricevis
        neniun <ctl>nigran globon</ctl> &ndash; &ccirc;iuj 15 ekzamenintoj
        estis <ctl>por</ctl>
        <fnt>
          <aut>B. Kondratjev</aut>: <vrk>Sukcesa defendo</vrk>.
          <bib>LOdE</bib>, <lok>2000:11</lok>
        </fnt>!
      </ekz><ekz>
        konsilanto de <nom>Byunghyun Oh</nom> <klr>[&#x2026;]</klr> verkis magistran <tld/>a&jcirc;on
        <fnt>
          <aut>Chr. Kiselman</aut>:
          <lok><url ref="http://www.math.uu.se/~kiselman/eo.html">Hejmpa&gcirc;o</url></lok>
        </fnt>;
      </ekz><ekz>
        la temo de lia doktora <tld/>a&jcirc;o estis:
        <ctl>Strofo-strukturo de la hungaraj popolkantoj</ctl>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Zolt%C3%A1n_Kod%C3%A1ly">Zolt&aacute;n Kod&aacute;ly</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="disert1.0o">disertacio</ref>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_e;&c_r;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">dissertaci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Dissertation</trd>,
    <trd>Doktorarbeit</trd>
  </trdgrp>
  <trd lng="en">dissertation</trd>
  <trd lng="fr">dissertation</trd>
  <trd lng="he">&tav;&zayin;&he;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rtekez&eacute;s</trd>,
    <trd>disszert&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8AD6;&#x6587;  <pr>&#x308D;&#x3093;&#x3076;&#x3093;</pr></trd>,
    <trd>&#x5B66;&#x4F4D;&#x8AD6;&#x6587;  <pr>&#x304C;&#x304F;&#x3044;&#x308D;&#x3093;&#x3076;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">dysertacja</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">dizert&aacute;cia</trd>
  <trd lng="uk">&c_d;&c_i;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>
  <trd lng="zh">&#x535A;&#x58EB;&#x8AD6;&#x6587; <pr>b&#xF3;sh&#xEC;l&#xF9;nw&#xE9;n</pr></trd>
</drv>
  </art>
