<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>disert</rad>/i
  </kap>
  <drv mrk="disert.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <snc>
      <dif>
        Prezenti detalan metodan studadon pri scienca, filozofia
        a&ubreve; literatura temo:
        <ekz>la profesoro <tld/>is pri Kant.</ekz>
      </dif>
    </snc>
    <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;
    <ind>&c_d;&c_y;&c_s;&c_e;&c_r;&c_t;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
    <trdgrp lng="de">
       <trd>seine Dissertation schreiben</trd>,
       <trd>seine Doktorarbeit verfassen</trd>
    </trdgrp>
    <trd lng="fr">disserter</trd>
    <trdgrp lng="hu">
       <trd>&eacute;rtekezik</trd>,
       <trd>&eacute;rtekez&eacute;st &iacute;r</trd>
    </trdgrp>
    <trd lng="pl">przedstawia&cacute;
    <ind>dysertacj&eogonek;</ind></trd>
    <trd lng="pt">dissertar</trd>
    <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;
    &c_d;&c_i;&c_s;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_i;&c_ju;</trd>
  </drv>
  <drv mrk="disert.0ajxo">
    <kap><tld/>a&jcirc;o</kap>
    <snc>
      <dif>
        Verko traktanta pri scienca, filozofia a&ubreve; literatura
        temo.  
      </dif>
    </snc>
    <trd
    lng="be">&c_d;&c_y;&c_s;&c_e;&c_r;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trdgrp lng="de">
       <trd>Dissertation</trd>,
       <trd>Doktorarbeit</trd>
    </trdgrp>
    <trd lng="fr">dissertation</trd>
    <trdgrp lng="hu">
       <trd>&eacute;rtekez&eacute;s</trd>,
       <trd>disszert&aacute;ci&oacute;</trd>
    </trdgrp>
    <trd lng="pl">dysertacja</trd>
    <trd
    lng="ru">&c_d;&c_i;&c_s;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
    <trd lng="sk">dizert&aacute;cia</trd>
  </drv>
</art>
