<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>akvifoli</rad>/o
</kap>
<drv mrk="akvifo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ileks.0o">ilekso</ref>
      (<trd lng="la">Ilex aquifolium</trd>),
      kiu havas ledajn, brilglatajn, pikrandajn foliojn kaj ru&gcirc;ajn
      berojn
      kaj kies folioj estas uzataj en tizanoj.
    </dif>
  </snc>
  <trd lng="de">Stechpalme</trd>
  <trd lng="fr">houx</trd>
</drv>
<drv mrk="akvifo.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      vegeta&jcirc;oj (<trd lng="la">Aquifoliaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Stechpalmengew&auml;chse</trd>
  <trd lng="fr">aquifoliac&eacute;es</trd>
</drv>
</art>
