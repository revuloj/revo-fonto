<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akvifoli</rad>/o
</kap>
<drv mrk="akvifo.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ileks.0o">ilekso</ref>
      (<trd lng="la">Ilex aquifolium</trd>),
      kiu havas ledajn, brilglatajn, pikrandajn foliojn kaj ru&gcirc;ajn
      berojn kaj kies folioj estas uzataj en tizanoj.
    </dif>
  </snc>
  <trd lng="de">Stechpalme</trd>
  <trd lng="fr">houx</trd>
  <trd lng="hu">magyal</trd>
  <trd lng="nl">bonte <ind>hulst</ind></trd>
  <trd lng="ru">&c_p;&c_a;&c_d;&c_u;&c_b;</trd>
  <trd lng="sv">j&auml;rnek</trd>
</drv>
</art>
