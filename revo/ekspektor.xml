<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekspektor.xml,v 1.9 2023/04/17 09:26:40 revo Exp $">
<kap>
  <rad>ekspektor</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekspektor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ekspektor.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      El<ref tip="vid" cel="sput.0i.FIZL">sputi</ref>
      embarasa&jcirc;ojn, 
      mukon el la spirtuboj:
      <ekz>
        kapitano van To&hcirc; <tld/>is. "Kaj ... kiel ili aspektas?"
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 1a, &ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_h;&c_a;&c_r;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">expectorar</trd>
  <trdgrp lng="cs">
    <trd>odka&scaron;lat <klr>(pr&#x16F;du&scaron;kov&#xFD; hlen)</klr></trd>,
    <trd>vyka&scaron;lat</trd>
  </trdgrp>
  <trd lng="fr">expectorer</trd>
  <trd lng="it">espettorare</trd>
  <trdgrp lng="pl">
    <trd>wykrztusi&cacute;</trd>,
    <trd>charka&cacute;<klr> (wulg.)</klr></trd>
  </trdgrp>
  <trd lng="pt">expectorar</trd>
  <trdgrp lng="sk">
    <trd>odka&scaron;la&tcaron;</trd>,
    <trd>vyka&scaron;la&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_d;&c_h;&c_a;&c_r;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>

