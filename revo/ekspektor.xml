<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekspektor.xml,v 1.7 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>ekspektor</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekspektor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ekspektor.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      El<ref tip="vid" cel="sput.0i.FIZL">sputi</ref>
      embarasa&jcirc;ojn, 
      mukon el la spirtuboj:
      <ekz>
        kapitano van To&hcirc; <tld/>is. "Kaj ... kiel ili aspektas?"
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 1a, &ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">expectorar</trd>
  <trdgrp lng="cs">
    <trd>odkašlat <klr>(průduškový hlen)</klr></trd>,
    <trd>vykašlat</trd>
  </trdgrp>
  <trd lng="fr">expectorer</trd>
  <trd lng="it">espettorare</trd>
  <trdgrp lng="pl">
    <trd>wykrztusi&cacute;</trd>,
    <trd>charka&cacute;<klr> (wulg.)</klr></trd>
  </trdgrp>
  <trd lng="pt">expectorar</trd>
  <trdgrp lng="sk">
    <trd>odkašlať</trd>,
    <trd>vykašlať</trd>
  </trdgrp>
</drv>
</art>

