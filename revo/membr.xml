<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: membr.xml,v 1.30 2021/05/29 14:32:54 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>membr</rad>/o
    </kap>

    <drv mrk="membr.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="membr.0o.ANA">
        <uzo tip="fak">ANA</uzo>
        <dif>
          &Ccirc;iu el la moveblaj pecoj de la
          <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>
          &#x2010; krom kapo &#x2010; kunigitaj kun la trunko per artiko:
          <ekz>
            la superaj (brakoj) kaj malsuperaj (kruroj) <tld/>oj de
            homo.
          </ekz>
        </dif>
        <trd lng="de">Glied</trd>
        <trd lng="es">miembro<klr> (del cuerpo)</klr></trd>
        <trd lng="id"><ind>anggota</ind> gerak</trd>
        <trd lng="nl">lidmaat</trd>
        <trd lng="pl">ko&nacute;czyna</trd>
        <trd lng="ro">membru</trd>
      </snc>
      <snc mrk="membr.0o.ano">
        <uzo tip="stl">FIG</uzo>
        <dif>
          <ref tip="dif" cel="an.0o">Ano</ref> de societo, partio,
          familio kc.
        </dif>
        <trdgrp lng="be">
          <trd>&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_ch;&c_a;&c_l;&c_je;&c_c;</trd>
        </trdgrp>
        <trd lng="de">Mitglied</trd>
        <trd lng="es">miembro<klr> (de un grupo humano)</klr></trd>
        <trd lng="id">anggota</trd>
        <trd lng="nl">lid</trd>
        <trd lng="pl">cz&lstroke;onek<klr> (grupy)</klr></trd>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Aparta difinita ero de tuta&jcirc;o:
          <ekz>
            <tld/>o de frazo.
          </ekz>
        </dif>
        <trd lng="de">Glied</trd>
        <trd lng="es">part&iacute;cula<klr> (de un todo)</klr></trd>
        <trd lng="nl">deel</trd>
        <trd lng="pl">cz&eogonek;&sacute;&cacute;</trd>
      </snc>
      <snc mrk="membr.0o.ekvacio">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>MatTerm</bib><lok>p. 13</lok></fnt>
        <dif>
          <klr>(de <ref cel="egal.0ajxo">egala&jcirc;o</ref>,
            <ref cel="egal.ne0ajxo">neegala&jcirc;o</ref> a&ubreve;
            <ref cel="ekvaci.0o">ekvacio</ref>)</klr>
          &Ccirc;iu el la du <ref tip="super" cel="esprim.0o.KOMP">esprimoj</ref>,
          situantaj amba&ubreve;flanke de la signo de egaleco a&ubreve; neegaleco:
          <ekz>
            &scirc;ovu la konstantan termon de la unua <tld/>o de la ekvacio 
            al la dua, &scirc;an&gcirc;inte ties signon;
          </ekz><ekz>
            dividi amba&ubreve; <tld/>ojn de la neegala&jcirc;o per la nekonato.
          </ekz>
        </dif>
        <rim>
          Kvankam tiu tre fakeca termino trovi&gcirc;as anka&ubreve; en
          modernaj fontoj kiel
          <fnt><bib>MatStokTerm</bib><lok>p. 34</lok></fnt>, ne estus
          absurde preferi la pli ordinaran kaj klaran
          <ref cel="flank.0o.ekvacio">flanko</ref>n.
        </rim>
        <trdgrp lng="de">
          <trd>Seite</trd>,
          <trd>Glied</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>member<klr tip="ind"> (of an equation)</klr></trd>,
          <trd>term<klr tip="ind"> (of an equation)</klr></trd>
        </trdgrp>
        <trd lng="es">miembro<klr> (a cada lado de una
        igualdad)</klr></trd>
        <trdgrp lng="fr">
          <trd>terme<klr tip="ind"> (d'une &eacute;galit&eacute;)</klr></trd>,
          <trd>membre<klr tip="ind"> (d'une &eacute;galit&eacute;)</klr></trd>
        </trdgrp>
        <trd lng="pl">strona<klr tip="ind"> (r&oacute;wnania)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;<klr tip="ind">
              (&c_u;&c_r;&c_a;&c_v;&c_n;&c_je;&c_n;&c_i;&c_ja;: &c_l;&c_je;&c_v;&c_a;&c_ja;, &c_p;&c_r;&c_a;&c_v;&c_a;&c_ja;)</klr>
          </trd>
        </trdgrp>
      </snc>
  <trd lng="be">&c_ch;&c_l;&c_je;&c_n;</trd>
  <trd lng="bo">&#xF62;&#xF90;&#xF44;&#xF0B;&#xF63;&#xF42;&#xF0B;</trd>
  <trdgrp lng="br">                                        
        <trd>ezel</trd>,                       
        <trd>mempr</trd>,                                   
      </trdgrp>
  <trdgrp lng="cs">
    <trd>&ccaron;len</trd>,
    <trd>kon&ccaron;etina</trd>,
    <trd>membrum</trd>,
    <trd>&uacute;d</trd>,
    <trd>&ccaron;l&aacute;nek</trd>
  </trdgrp>
  <trd lng="en">limb</trd>
  <trd lng="es">miembro</trd>
  <trd lng="fa">&a_ayn;&a_D;&a_w;</trd>
  <trd lng="fr">membre</trd>
  <trd lng="it">membro</trd>
  <trdgrp lng="pl">                                        
    <trd>cz&lstroke;onek</trd>,                       
    <trd>ko&nacute;czyna</trd>,                                   
  </trdgrp>
  <trd lng="pt">membro</trd>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>kon&ccaron;atina</trd>,
    <trd>&uacute;d</trd>,
    <trd>&ccaron;len</trd>,
    <trd>&ccaron;l&aacute;nok</trd>
  </trdgrp>
</drv>

    <drv mrk="membr.dis0igi">
      <kap>dis<tld/>igi</kap>
      <snc>
        <dif>
          Dividi la&ubreve; difinitaj partoj:
          <ekz>
            dis<tld/>igi beston, frazon, vorton;
          </ekz><ekz>
            mi aran&gcirc;is plenan dis<tld/>igon de la ideoj en memstarajn
            vortojn<fnt>Z</fnt>;
          </ekz><ekz>
            mi alkonformigis la dis<tld/>i&gcirc;on de la lingvo al la
            spirito de la lingvoj E&ubreve;ropaj<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="br">diempra&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;lit na &ccaron;leny</trd>,
    <trd>d&ecaron;lit na &ccaron;&aacute;sti</trd>,
    <trd>roz&ccaron;l&aacute;nkovat</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desmembrar</trd>,
    <trd>diseccionar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>analyser</trd>,
    <trd>d&eacute;membrer</trd>
  </trdgrp>
  <trd lng="id"><baz>kelompok</baz> mengelompokkan</trd>
  <trdgrp lng="it">
    <trd>smembrare</trd>,
    <trd>scomporre</trd>,
    <trd>scindere</trd>
  </trdgrp>
  <trd lng="nl">ontleden</trd>
  <trd lng="pl">rozcz&lstroke;onkowa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_ch;&c_l;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">roz&ccaron;l&aacute;nkova&tcaron;</trd>
</drv>
  </art>
