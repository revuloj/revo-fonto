  </trdgrp>
</drv>

<drv mrk="membr.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Esti <tld/>o en io; <ref cel="aparte.0i.partoDe">aparteni<sncref/></ref> al iu grupo:
      <ekz>
        li <tld/>is kaj aktivis en la dekstrega Progresa Partio, sed mem eksi&gcirc;is
        <klr>[poste]</klr>, &ccirc;ar li trovis &gcirc;in tro modera
        <fnt>
          <vrk><url ref="&Monato;/2011/010493.php">Floroj, ne armiloj</url></vrk>
        </fnt>;
      </ekz><ekz>
        en tiu &ccirc;i epoko ekzistis piedpilka teamo kies nomo est<klr>[i]</klr>s Esperanto
        F.C., en kiu <tld/>is studentoj de Universitato de &Ccirc;ilio
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/%C4%88ilia_Esperanto-Asocio">&Ccirc;ilia Esperanto-Asocio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><ind>Mitglied</ind> sein</trd>
  <trd lng="en">be a <ind>member</ind> of</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_ch;&c_l;&c_je;&c_n;&c_o;&c_m;</ind></trd>,
    <trd>&c_ch;&c_l;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="membr.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="al.0igxi">Ali&gcirc;i</ref> al iu organiza&jcirc;o kiel 
      <ref cel="membr.0o.ano">membro<sncref/></ref>:
      <ekz>
        <tld/>i&gcirc;i en E&ubreve;ropa Unio
        <fnt><aut>Zlatko Ti&scaron;ljar</aut>,
        <vrk><url ref="&Monato;/2000/006910.php">Post morto -- nova sorto</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/04, p. 10</lok>
        </fnt>;
      </ekz><ekz>
        homoj maldekstremaj preferis <tld/>i&gcirc;i en SAT anstata&ubreve; en aliaj Esperanto-asocioj
        <fnt><bib>EeP</bib> <lok>Kvara Parto</lok></fnt>;
      </ekz><ekz>
        &scirc;i beda&ubreve;rinde ne volas <tld/>i&gcirc;i en nia nove fondita junulara Esperanto-grupo
        <fnt><bib>FSI</bib> <lok>III</lok></fnt>;
      </ekz><ekz>
        se la ekssovetiaj respublikoj kaj satelitaj landoj sukcesos <tld/>i&gcirc;i en NATO,
        ili eble konstatos, ke ili liberi&gcirc;is de unu hegemonio sed subi&gcirc;is al alia
        <fnt>
	<aut>Garbhan MacAoidh</aut>
	<vrk><url ref="&Monato;/2000/007003.php">El flamo en fajron</url></vrk>
	<bib>Monato</bib>
	<lok>2000/06, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_l;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a; <klr>(&c_u; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_ch;&c_a;&c_l;&c_mol;&c_c;&c_a;, &c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a;)</klr></trd>,
    <trd>&c_u;&c_s;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol; <klr>(&c_u; &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ju;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eintreten <klr>(Verein)</klr></trd>,
    <trd><ind>Mitglied</ind> werden</trd>
  </trdgrp>
  <trd lng="ja">&#x5165;&#x4F1A;&#x3059;&#x308B;  <pr>&#x306B;&#x3085;&#x3046;&#x304B;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_t;&c_mol; <ind>&c_ch;&c_l;&c_je;&c_n;&c_o;&c_m;</ind></trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;<klr> (&c_v; &c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ju;)</klr></trd>
  </trdgrp>  
  <trdgrp lng="zh">
    <trd>&#x65B0;&#x8FDB; <pr>x&#x12B;nj&#xEC;n</pr></trd>,
    <trd>&#x65B0;&#x9032; <pr>x&#x12B;nj&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="membr.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc>
    <dif>
      <ref tip="super" cel="divid.0i.partigi">Dividi</ref> la&ubreve; difinitaj partoj:
      <ekz>
        dis<tld/>igi beston, frazon, vorton;
      </ekz><ekz>
        mi aran&gcirc;is plenan dis<tld/>igon de la ideoj en memstarajn
        vortojn
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        per tiu dis<tld/>igo de la lingvoelementoj <nom>Zamenhof</nom> donis al nova lingvo
        karakteron aglutinan
        <fnt>
          <bib>Zam</bib>
          <lok>Aglutina karaktero de esperanto</lok>
        </fnt>;
      </ekz><ekz>
        la dis<tld/>igo de &Ccirc;e&hcirc;oslovakio kaj la posta invado de tiu lando fare de nazia
        Germanio &ccirc;esigis <klr>[&#x2026;]</klr> gravan atingon en la sfero de perradia teatro
        <fnt><bib>EeP</bib> <lok>Tria Parto</lok></fnt>;
      </ekz><ekz>
        la kvar dama&gcirc;itaj reaktoroj estas plu malvarmigataj, sed ilia dis<tld/>igo
        ankora&ubreve; ne komenci&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2012/010852.php">Fandi&gcirc;is konsciencoj, fandi&gcirc;is
          reaktorkernoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_ch;&c_l;&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_l;&c_je;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">diempra&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;lit na &ccaron;leny</trd>,
    <trd>d&ecaron;lit na &ccaron;&aacute;sti</trd>,
    <trd>roz&ccaron;l&aacute;nkovat</trd>
  </trdgrp>
  <trd lng="de">zergliedern</trd>
  <trdgrp lng="es">
    <trd>desmembrar</trd>,
    <trd>diseccionar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>analyser</trd>,
    <trd>d&eacute;membrer</trd>
  </trdgrp>
  <trd lng="id"><baz>kelompok</baz> mengelompokkan</trd>
  <trdgrp lng="it">
    <trd>smembrare</trd>,
    <trd>scomporre</trd>,
    <trd>scindere</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89E3;&#x4F53;&#x3059;&#x308B;  <pr>&#x304B;&#x3044;&#x305F;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5206;&#x5272;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x304B;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5206;&#x89E3;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x304B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3070;&#x3089;&#x3070;&#x3089;&#x306B;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">ontleden</trd>
  <trd lng="pl">rozcz&lstroke;onkowa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_ch;&c_l;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">roz&ccaron;l&aacute;nkova&tcaron;</trd>
  <trd lng="uk">&c_r;&c_o;&c_z;&c_ch;&c_l;&c_je;&c_n;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x74DC;&#x5206; <pr>gu&#x101;f&#x113;n</pr></trd>
</drv>
