<drv mrk="pauxz.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pauxz.0ejo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Loko, korto, &ccirc;ambro uzata por <tld/>oj:
      <ekz>
        mi iras en la <tld/>ejon, halon konsistantan el du lar&gcirc;aj koridoroj, kie la
        lernantoj pasigu la <tld/>ojn <klr>[&#8230;]</klr> dum ekstere pluvas
        <fnt><bib>FSI</bib> <lok>I</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pausenraum</trd>,
    <trd>Pausenhof</trd>
  </trdgrp>
  <trdgrp lng="en">
