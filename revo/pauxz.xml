  </trdgrp>  
  <trd lng="bg">&c_p;&c_o;&c_ch;&c_i;&c_v;&c_k;&c_a;</trd>   
  <trd lng="bn">&#x9AC;&#x9BF;&#x9B0;&#x9A4;&#x9BF;</trd>
  <trdgrp lng="br">
