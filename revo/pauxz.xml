<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pa&ubreve;z</rad>/o
</kap>
<drv mrk="pauxz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nelonga interrompado de ago:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        <tld/>o dum legado, leciono, mar&scirc;ado;
      </ekz>
      <ekz>
        sen<tld/>e laboradi.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pause</trd>
  <trd lng="en">pause</trd>
  <trd lng="nl">pause</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_r;&c_y;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="pauxz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Halti momente dum agado.
    </dif>
    <refgrp tip="sin">
      <ref cel="ripoz.0i">ripozi</ref>,
      <ref cel="cxes.0i">&ccirc;esi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">pausieren</trd>
  <trd lng="nl">pauseren</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_u;&c_z;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
