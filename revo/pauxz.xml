<drv mrk="pauxz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pauxz.0o.KOMUNE">
    <dif>
      Nelonga interrompado de ago:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        <tld/>o dum legado, leciono, mar&scirc;ado.
      </ekz>
    </dif>
  </snc>        
  <trd lng="af">break</trd>
  <trd lng="am">&#x1230;&#x1260;&#x122D;</trd>
  <trd lng="ar">&#x627;&#x633;&#x62A;&#x631;&#x627;&#x62D;&#x629;</trd>
