<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pa&ubreve;z</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pauxz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pauxz.0o.KOMUNE">
    <dif>
      Nelonga interrompado de ago:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        <tld/>o dum legado, leciono, mar&scirc;ado;
      </ekz>
      <ekz>
        <ind>sen<tld/>e</ind> laboradi.
        <trd lng="br">dibaouez</trd>
      </ekz>
    </dif>
  </snc>        
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_a;&c_k;</trd>
  </trdgrp>  
  <trd lng="bg">&c_p;&c_o;&c_ch;&c_i;&c_v;&c_k;&c_a;</trd>   
  <trdgrp lng="br">
    <trd>ehan</trd>,
    <trd>paouez<klr tip="ind"> (ehan)</klr></trd>
  </trdgrp>
  <trd lng="de">Pause</trd> 
  <trd lng="en">pause</trd>
  <trd lng="fr">pause</trd>
  <trd lng="hu">sz&uuml;net</trd>
  <trd lng="nl">pause</trd>
  <trd lng="pl"><klr tip="ind">(przerwa) </klr>pauza</trd>
  <trd lng="pt">pausa</trd>
  <trd lng="ro">pauz&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_r;&c_y;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="pauxz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pauxz.0i.KOMUNE">
    <dif>
      Halti momente dum agado.
    </dif>
    <refgrp tip="vid">
      <ref cel="cxes.0i">&ccirc;esi</ref>,
      <ref cel="ripoz.0i">ripozi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;  <ind>&c_p;&c_a;&c_w;&c_z;&c_u;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>                 
  <trdgrp lng="br">
    <trd>ober un <ind>ehan</ind></trd>,   
    <trd>ober ur <ind>paouez</ind></trd>
  </trdgrp>
  <trd lng="de">pausieren</trd>   
  <trd lng="fr">faire une <ind>pause</ind></trd>
  <trd lng="hu">sz&uuml;netet tart</trd>
  <trd lng="nl">pauseren</trd>
  <trd lng="pl">pauzowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_u;&c_z;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pauxz.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pauxz.0ejo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Loko, korto, &ccirc;ambro uzata por pa&ubreve;zoj:
      <ekz>
        mi iras en la <tld/>ejon, halon konsistantan el du lar&gcirc;aj 
        koridoroj
        <fnt>
          <aut>U. Matthias</aut>,
          <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">
          Fajron sentas mi interne</url></vrk>,
          <lok>Pro Esperanto, 1990</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cour de r&eacute;cr&eacute;ation</trd>,
    <trd>salle de d&eacute;tente</trd>
  </trdgrp>
  <trd lng="nl">foyer</trd>
</drv>
</art>
