<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pa&ubreve;z</rad>/o
</kap>
<drv mrk="pauxz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nelonga interrompado de ago:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        <tld/>o dum legado, leciono, mar&scirc;ado;
      </ekz>
      <ekz>
        <ind>sen<tld/>e</ind> laboradi.
        <trd lng="br">dibaouez</trd>
      </ekz>
    </dif>
  </snc>         
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_a;&c_k;</trd>
  </trdgrp>      
  <trdgrp lng="br">
    <trd>ehan</trd>,
    <trd>paouez<klr tip="ind"> (ehan)</klr></trd>
  </trdgrp>
  <trd lng="de">Pause</trd>  
  <trd lng="en">pause</trd>
  <trd lng="fr">pause</trd>
  <trd lng="hu">sz&uuml;net</trd>
  <trd lng="nl">pause</trd>
  <trd lng="pt">pausa</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_r;&c_y;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="pauxz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Halti momente dum agado.
    </dif>
    <refgrp tip="sin">
      <ref cel="ripoz.0i">ripozi</ref>,
      <ref cel="cxes.0i">&ccirc;esi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_w;&c_z;&c_u;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>                  
  <trdgrp lng="br">
    <trd>ober un <ind>ehan</ind></trd>,    
    <trd>ober ur <ind>paouez</ind></trd>
  </trdgrp>
  <trd lng="de">pausieren</trd>    
  <trd lng="fr">faire une <ind>pause</ind></trd>
  <trd lng="hu">sz&uuml;netet tart</trd>
  <trd lng="nl">pauseren</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_u;&c_z;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
