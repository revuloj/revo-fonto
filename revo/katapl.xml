<art mrk="$Id: katapl.xml,v 1.23 2023/12/03 08:00:25 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kataplasm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katapl.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="katapl.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       <ref tip="super" cel="kurac.0ilo.MED">Kuracilo</ref> konsistanta 
       el varma, mola kaj malseka maso da medicinaj substancoj, 
       entenata en maldensa &scirc;tofo, kaj metata sur korpa parto:
       <ekz>
         Fernanda <klr>...</klr> aplikis al &scirc;i <tld/>ojn sur la ventro 
         kaj saketojn da glacio sur la kapo
         <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, 
         <vrk>Cent Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
       </ekz>
     </dif>
     <ref tip="vid" cel="foment.0ajxo">fomenta&jcirc;o</ref>
   </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_p;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_p;&c_a;</trd>
  <trd lng="ca">cataplasma</trd>
  <trdgrp lng="cs">
    <trd>ka&scaron;ovit&yacute; obklad</trd>,
    <trd>zm&ecaron;k&ccaron;uj&iacute;c&iacute; obklad</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Kataplasma</trd>,
       <trd>hei&szlig;er <ind>Breiumschlag</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>plaster</trd>,
     <trd>poultice</trd>
   </trdgrp>
  <trd lng="es">cataplasma</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_r;&a_h;&a_m;</trd>,
     <trd>&a_D;&a_m;&a_A;&a_d;</trd>
   </trdgrp>
  <trd lng="fr">cataplasme</trd>
  <trdgrp lng="hu">
       <trd>meleg <ind>borogat&aacute;s</ind></trd>,
       <trd>kataplazma</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30C3;&#x30D7;&#x5264;  <pr>&#x30D1;&#x30C3;&#x30D7;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x6E7F;&#x7F68;&#x6CD5;  <pr>&#x3057;&#x3081;&#x7F68;&#x6CD5;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>pap</trd>,
     <trd>warme <ind>omslag</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kataplazma</trd>,
    <trd>ok&lstroke;ad ciep&lstroke;y</trd>
  </trdgrp> 
  <trd lng="ro">cataplasm&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_p;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">ka&scaron;ovit&yacute; obklad</trd>
  <trd lng="uk">&#x43F;&#x440;&#x438;&#x43F;&#x430;&#x440;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x6CE5;&#x6577;&#x5242; <pr>n&#xED;f&#x16B;j&#xEC;</pr></trd>
</drv>
</art>
