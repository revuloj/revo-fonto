<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kataplasm</rad>/o
</kap>
<drv mrk="katapl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Kuracilo, konsistanta el varma, mola kaj malseka maso da
      medicinaj substancoj, entenita en maldensa &scirc;tofo, kaj metata
      sur korpa parto.  
    </dif>
    <ref tip="vid" cel="foment.0ajxo">fomenta&jcirc;o</ref>
  </snc>
  <trdgrp lng="de">
      <trd>Kataplasma</trd>,
      <trd>hei&szlig;er <ind>Breiumschlag</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>plaster</trd>,
    <trd>poultice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pap</trd>,
    <trd>warme <ind>omslag</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_p;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>
</art>
