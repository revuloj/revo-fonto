<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>kataplasm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katapl.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="katapl.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       <ref tip="super" cel="kurac.0ilo.MED">Kuracilo</ref> konsistanta 
       el varma, mola kaj malseka maso da medicinaj substancoj, 
       entenata en maldensa &scirc;tofo, kaj metata sur korpa parto:
       <ekz>
         Fernanda <klr>...</klr> aplikis al &scirc;i <tld/>ojn sur la ventro 
         kaj saketojn da glacio sur la kapo
         <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, 
         <vrk>Cent Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
       </ekz>
     </dif>
     <ref tip="vid" cel="foment.0ajxo">fomenta&jcirc;o</ref>
   </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_p;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_p;&c_a;</trd>
  <trd lng="ca">cataplasma</trd>
  <trdgrp lng="de">
       <trd>Kataplasma</trd>,
       <trd>hei&szlig;er <ind>Breiumschlag</ind></trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>plaster</trd>,
     <trd>poultice</trd>
   </trdgrp>
  <trd lng="es">cataplasma</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_r;&a_h;&a_m;</trd>,
     <trd>&a_D;&a_m;&a_A;&a_d;</trd>
   </trdgrp>
  <trd lng="fr">cataplasme</trd>
  <trdgrp lng="hu">
       <trd>meleg <ind>borogat&aacute;s</ind></trd>,
       <trd>kataplazma</trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>pap</trd>,
     <trd>warme <ind>omslag</ind></trd>
   </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_p;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">kašovitý obklad</trd>
</drv>
</art>
