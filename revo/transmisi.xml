<art mrk="$Id: transmisi.xml,v 1.16 2025/04/08 14:12:33 revo Exp $">
<kap>
  <rad>transmisi</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="transmisi.0i">
  <kap><tld/>i</kap>
  <snc mrk="transmisi.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="per.0i.KOMUNE">Peri</ref> movon, impulson,
      energion, informon al alia ma&scirc;ino, al alia loko:
      <ekz>
        la penso <tld/>i povumon de jetmotoro al helico ne estas nova
        <fnt>
          <aut>J. Kubat</aut>,
          <vrk><url ref="http://www.fw.hu/eventoj/steb/ingxenierado/ceteraj/aviado/saest82-15.htm">
          Nova erao de helicoj</url></vrk>,
          <lok>SAEST 1982, p. 62a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="propuls.0i">propulsi</ref>,
      <ref cel="trans.0igi">transigi</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_u;&c_h;, &c_ib;&c_m;&c_p;&c_u;&c_l;&c_mol;&c_s;&#8230;)</klr></trd>
    <trd lng="ca">transmetre</trd>
    <trdgrp lng="cs">
      <trd>p&rcaron;ev&aacute;d&ecaron;t energii</trd>,
      <trd>p&rcaron;ev&aacute;d&ecaron;t pohyb</trd>,
      <trd>p&rcaron;ev&aacute;d&ecaron;t s&iacute;lu</trd>
    </trdgrp>
    <trd lng="de">&uuml;bertragen <klr>(techn.)</klr></trd>
    <trd lng="en">to transmit</trd>
    <trd lng="fr">transmettre<klr> (m&eacute;canique etc.)</klr></trd>
    <trd lng="it">trasmettere<klr> (mecc. ecc.)</klr></trd>
    <trdgrp lng="pl">
      <trd>transmitowa&cacute;</trd>,
      <trd>przesy&lstroke;a&cacute;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>prev&aacute;dza&tcaron; energiu</trd>,
      <trd>prev&aacute;dza&tcaron; pohyb</trd>
    </trdgrp>
    <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>
    <trdgrp lng="zh">
      <trd>&#25773;&#36865; <pr>b&#333;s&#242;ng</pr></trd>,
      <trd>&#20253; <pr>y&#250;n</pr></trd>,
      <trd>&#20256; <pr>chu&#225;n</pr></trd>,
      <trd>&#20659; <pr>chu&#225;n</pr></trd>,
      <trd>&#20256;&#36798; <pr>chu&#225;nd&#225;</pr></trd>,
      <trd>&#20659;&#36948; <pr>chu&#225;nd&#225;</pr></trd>,
      <trd>&#23492;&#36865; <pr>j&#236;s&#242;ng</pr></trd>,
      <trd>&#36681;&#21578; <pr>zhu&#462;ng&#224;o</pr></trd>,
      <trd>&#36716;&#21578; <pr>zhu&#462;ng&#224;o</pr></trd>,
      <trd>&#36681;&#30332; <pr>zhu&#462;nf&amacron;</pr></trd>,
      <trd>&#36716;&#21457; <pr>zhu&#462;nf&amacron;</pr></trd>,
      <trd>&#20256;&#36755; <pr>chu&#225;nsh&#363;</pr></trd>,
      <trd>&#20659;&#36664; <pr>chu&#225;nsh&#363;</pr></trd>,
      <trd>&#20256;&#36882; <pr>chu&#225;nd&#236;</pr></trd>,
      <trd>&#20659;&#36958; <pr>chu&#225;nd&#236;</pr></trd>,
      <trd>&#20256;&#36865; <pr>chu&#225;ns&#242;ng</pr></trd>,
      <trd>&#20659;&#36865; <pr>chu&#225;ns&#242;ng</pr></trd>,
      <trd>&#25773;&#25918; <pr>b&#333;f&#224;ng</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="transmisi.0i.kontagxi">
    <uzo tip="stl">EVI</uzo><uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kontagx.0i">Konta&gcirc;i</ref>,
      <ref tip="dif" cel="trans.0igi.porti">transigi<sncref/></ref>:
      <ekz>
        virusa infektmalsano <tld/>ata de kuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/009243.php">Epidemiaj sekvoj en E&ubreve;ropo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>transigi</ke>, <ke>transsendi</ke>, <ke>peri</ke>, <ke>komuniki</ke>.
  </rim>

  <trd lng="ja">&#20253;&#21205;&#12377;&#12427;  <pr>&#12388;&#12383;&#12393;&#12358;&#12377;&#12427;</pr></trd>
</drv>

<drv mrk="transmisi.0o">
  <kap><tld/>o</kap>
  <snc mrk="transmisi.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Pero de movo, impulso, energio, informo ktp:
      <ekz>
        la trioblan hibridan <tld/>on kreas <klr>[&#8230;]</klr> multfuela motoro-generatoro
        <fnt>
          <bib>Monato</bib>
          <aut>Tam&aacute;s Slez&aacute;k</aut>
          <vrk><url ref="&Monato;/2008/009631.php">Pli ol a&ubreve;to: perspektivo kaj sinteno</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de informa&jcirc;oj per sendrata telegrafo
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011269.php">Pioniro de sendrata te&hcirc;nologio</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>
    <trd lng="ca">transmissi&oacute;</trd>
    <trdgrp lng="cs">
      <trd>p&rcaron;evod energie</trd>,
      <trd>p&rcaron;evod pohybu</trd>
    </trdgrp>
    <trd lng="de">&Uuml;bersetzung <klr>(Getriebe)</klr></trd>
    <trd lng="en">transmission</trd>
    <trd lng="fr">transmission<klr> (m&eacute;canique etc.)</klr></trd>
    <trd lng="it">trasmissione<klr> (mecc. ecc.)</klr></trd>
    <trd lng="pl">transmisja</trd>
    <trdgrp lng="sk">
      <trd>prenos</trd>,
      <trd>pren&aacute;&scaron;anie</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>,
      <trd>&c_s;&c_i;&c_l;&c_o;&c_v;&c_a; &c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="transmisi.0o.ilo">
    <dif>
      <ref tip="dif" cel="transmisi.0ilo"><tld lit="T"/>ilo</ref>:
      <ekz>
        rapide turni&gcirc;antaj radoj, klakantaj <tld/>oj, tondra bruo de gigantaj lameniloj
        <fnt>
          <bib>Tor</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        biciklo kun muntita <tld/>o
        <fnt>
          <aut>M. Cruz</aut>,
          <vrk><url ref="http://alinome.net/bici/con_bici01_bici.eo.html">
          Bicikle vivi</url></vrk>,
          <lok>2003-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        Andreo nervoze komutis la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2011/010294.php">Rusa vila&gcirc;o</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="de">Transmission</trd>
  <trd lng="ja">&#20253;&#21205;  <pr>&#12391;&#12435;&#12393;&#12358;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#36664;&#38651; <pr>sh&#363;di&#224;n</pr></trd>,
    <trd>&#36755;&#30005; <pr>sh&#363;di&#224;n</pr></trd>,
    <trd>&#36879;&#23556; <pr>t&#242;ush&#232;</pr></trd>,
    <trd>&#20256;&#36755; <pr>chu&#225;nsh&#363;</pr></trd>,
    <trd>&#20659;&#36664; <pr>chu&#225;nsh&#363;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="transmisi.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <uzo tip="fak">TEK</uzo><uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="mehxan1.0o.pecoj">Me&hcirc;anismo</ref> por <tld/>i movon
      de motoro al pelata ma&scirc;ino kun eventuala &scirc;an&gcirc;o de momanto:
      <ekz>
        la laborado estas vere dan&gcirc;era, &ccirc;ar povas okazi je iu momento ke unu el la
        <tld/>iloj <klr>(<tld/>aj rimenoj)</klr> eksaltus for de la rapide turni&gcirc;antaj radoj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="rapid.0umo">rapidumo</ref>
    <refgrp tip="vid">
      <ref cel="klucx.0ilo">klu&ccirc;ilo</ref>,
      <ref cel="rad.0aro">radaro</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/3/35/Stove_Erdholl&auml;nder_innen.JPG/640px-Stove_Erdholl&auml;nder_innen.JPG" prm="CC-BY-SA-3.0"><tld/>ilo de ventmuelilo el radoj sur komuna akso kaj rimeno
        <fnt>
          <aut>Ch. Pagenkopf, propra verko</aut>
          <lok><url ref="&WCW;/File:Stove_Erdholl%C3%A4nder_innen.JPG">&WCW;/File:Stove_Erdholl&auml;nder_innen.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_t;&c_a;&c_ch;&c_n;&c_y; <ind>&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_z;&c_m;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_t;&c_a;&c_ch;&c_n;&c_y; <ind>&c_r;&c_e;&c_m;&c_je;&c_n;&c_mol;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_t;&c_a;&c_ch;&c_n;&c_y; <ind>&c_v;&c_a;&c_l;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Getriebe</trd>,
    <trd>Transmission</trd>
  </trdgrp>
  <trd lng="en">propulsion transmission</trd>
  <trdgrp lng="ja">
    <trd>&#20253;&#21205;&#35013;&#32622;  <pr>&#12388;&#12383;&#12393;&#12358;&#12381;&#12358;&#12385;</pr></trd>,
    <trd>&#22793;&#36895;&#35013;&#32622;  <pr>&#12408;&#12435;&#12381;&#12367;&#12381;&#12358;&#12385;</pr></trd>,
    <trd>&#12488;&#12521;&#12531;&#12473;&#12511;&#12483;&#12471;&#12519;&#12531;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21464;&#36895;&#31665; <pr>bi&#224;ns&#249;xi&amacron;ng</pr></trd>,
    <trd>&#35722;&#36895;&#31665; <pr>bi&#224;ns&#249;xi&amacron;ng</pr></trd>,
    <trd>&#20256;&#36755; <pr>chu&#225;nsh&#363;</pr></trd>,
    <trd>&#20659;&#36664; <pr>chu&#225;nsh&#363;</pr></trd>,
    <trd>&#36664;&#38651; <pr>sh&#363;di&#224;n</pr></trd>,
    <trd>&#36755;&#30005; <pr>sh&#363;di&#224;n</pr></trd>,
    <trd>&#25490;&#25377; <pr>p&#225;id&#462;ng</pr></trd>,
    <trd>&#25490;&#25803; <pr>p&#225;id&#462;ng</pr></trd>,
    <trd>&#36215;&#33853;&#26550; <pr>q&#464;lu&#242;ji&#224;</pr></trd>,
    <trd>&#36879;&#23556; <pr>t&#242;ush&#232;</pr></trd>,
    <trd>&#20256;&#21160;&#35013;&#32622; <pr>chu&#225;nd&#242;ngzhu&amacron;ngzh&#236;</pr></trd>,
    <trd>&#20659;&#21205;&#35037;&#32622; <pr>chu&#225;nd&#242;ngzhu&amacron;ngzh&#236;</pr></trd>
  </trdgrp>
</drv>

</art>

