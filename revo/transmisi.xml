<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: transmisi.xml,v 1.9 2022/09/19 16:49:25 revo Exp $">
<kap>
  <rad>transmisi</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="transmisi.0i">
  <kap><tld/>i</kap>
  <snc mrk="transmisi.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="per.0i.KOMUNE">Peri</ref> movon, impulson, 
      energion, informon al alia ma&scirc;ino, al alia loko:
      <ekz>
        la penso <tld/>i povumon de jetmotoro al helico ne estas nova
        <fnt>
          <aut>J. Kubat</aut>,
          <vrk><url ref="http://www.fw.hu/eventoj/steb/ingxenierado/ceteraj/aviado/saest82-15.htm">
          Nova erao de helicoj</url></vrk>,
          <lok>SAEST 1982, p. 62a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="propuls.0i">propulsi</ref>,
      <ref cel="trans.0igi">transigi</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_u;&c_h;, &c_ib;&c_m;&c_p;&c_u;&c_l;&c_mol;&c_s;&#8230;)</klr></trd>
    <trd lng="ca">transmetre</trd>
    <trdgrp lng="cs">
      <trd>p&rcaron;ev&aacute;d&ecaron;t energii</trd>,
      <trd>p&rcaron;ev&aacute;d&ecaron;t pohyb</trd>,
      <trd>p&rcaron;ev&aacute;d&ecaron;t s&iacute;lu</trd>
    </trdgrp>
    <trd lng="de">&uuml;bertragen <klr>(techn.)</klr></trd>
    <trd lng="en">to transmit</trd>
    <trd lng="fr">transmettre<klr> (m&eacute;canique etc.)</klr></trd>
    <trd lng="it">trasmettere<klr> (mecc. ecc.)</klr></trd>
    <trdgrp lng="pl">
      <trd>transmitowa&cacute;</trd>,
      <trd>przesy&lstroke;a&cacute;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>prev&aacute;dza&tcaron; energiu</trd>,
      <trd>prev&aacute;dza&tcaron; pohyb</trd>
    </trdgrp>
    <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>
  </snc>
  <snc mrk="transmisi.0i.kontagxi">
    <uzo tip="stl">EVI</uzo><uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kontagx.0i">Konta&gcirc;i</ref>,
      <ref tip="dif" cel="trans.0igi.porti">transigi<sncref/></ref>:
      <ekz>
        virusa infektmalsano <tld/>ata de kuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2007/009243.php"
              >Epidemiaj sekvoj en E&ubreve;ropo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="transmisi.0o">
  <kap><tld/>o</kap>
  <snc mrk="transmisi.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Pero de movo, impulso, energio, informo ktp:
      <ekz>
        la trioblan hibridan <tld/>on kreas <klr>[&#8230;]</klr> multfuela motoro-generatoro
        <fnt>
          <bib>Monato</bib>
          <aut>Tam&aacute;s Slez&aacute;k</aut>
          <vrk><url ref="https://www.monato.be/2008/009631.php"
              >Pli ol a&ubreve;to: perspektivo kaj sinteno</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de informa&jcirc;oj per sendrata telegrafo
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2014/011269.php"
              >Pioniro de sendrata te&hcirc;nologio</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>
    <trd lng="ca">transmissi&oacute;</trd>
    <trdgrp lng="cs">
      <trd>p&rcaron;evod energie</trd>,
      <trd>p&rcaron;evod pohybu</trd>
    </trdgrp>
    <trd lng="de">&Uuml;bersetzung <klr>(Getriebe)</klr></trd>
    <trd lng="en">transmission</trd>
    <trd lng="fr">transmission<klr> (m&eacute;canique etc.)</klr></trd>
    <trd lng="it">trasmissione<klr> (mecc. ecc.)</klr></trd>
    <trd lng="pl">transmisja</trd>
    <trdgrp lng="sk">
      <trd>prenos</trd>,
      <trd>pren&aacute;&scaron;anie</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>,
      <trd>&c_s;&c_i;&c_l;&c_o;&c_v;&c_a; &c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="transmisi.0o.ilo">
    <dif>
      <ref tip="dif" cel="transmisi.0ilo"><tld lit="T"/>ilo</ref>:
      <ekz>
        rapide turni&gcirc;antaj radoj, klakantaj <tld/>oj, tondra bruo de gigantaj lameniloj
        <fnt>
          <bib>Tor</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        biciklo kun muntita <tld/>o
        <fnt>
          <aut>M. Cruz</aut>,
          <vrk><url ref="http://alinome.net/bici/con_bici01_bici.eo.html">
          Bicikle vivi</url></vrk>,
          <lok>2003-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        Andreo nervoze komutis la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="https://www.monato.be/2011/010294.php"
              >Rusa vila&gcirc;o</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="de">Transmission</trd>
</drv>

<drv mrk="transmisi.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <uzo tip="fak">TEK</uzo><uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="mehxan1.0o.pecoj">Me&hcirc;anismo</ref> por <tld/>i movon 
      de motoro al pelata ma&scirc;ino kun eventuala &scirc;an&gcirc;o de momanto:
      <ekz>
        la laborado estas vere dan&gcirc;era, &ccirc;ar povas okazi je iu momento ke unu el la
        <tld/>iloj <klr>(<tld/>aj rimenoj)</klr> eksaltus for de la rapide turni&gcirc;antaj radoj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="rapid.0umo">rapidumo</ref>
    <refgrp tip="vid">
      <ref cel="klucx.0ilo">klu&ccirc;ilo</ref>,
      <ref cel="rad.0aro">radaro</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/3/35/Stove_Erdholl&auml;nder_innen.JPG/640px-Stove_Erdholl&auml;nder_innen.JPG"
      prm="CC-BY-SA-3.0"
      ><tld/>ilo de ventmuelilo el radoj sur komuna akso kaj rimeno
        <fnt>
          <aut>Ch. Pagenkopf, propra verko</aut>
          <lok><url ref="&WCW;/File:Stove_Erdholl%C3%A4nder_innen.JPG"
              >https://commons.wikimedia.org/wiki/File:Stove_Erdholl&auml;nder_innen.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="de">
    <trd>Getriebe</trd>,
    <trd>Transmission</trd>
  </trdgrp>
  <trd lng="en">propulsion transmission</trd>
</drv>




</art>

