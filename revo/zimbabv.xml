<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zimbabv</rad>/o
</kap>

<drv mrk="zimbabv.0o">
  <kap>
    <tld lit="Z"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="zimbabv.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Harareo.
      Teritorio de Respubliko Zimbabvo.
      ISO-3166 kodo: ZW.
      Iam nomata Rodezio.
      &Gcirc;i limas kun
      <ref tip="vid" cel="bocvan.0o.GEOG">Bocvano</ref>,
      <ref tip="vid" cel="mozambik.0o.GEOG">Mozambiko</ref>,
      <ref tip="vid" cel="afrik.sud0o.GEOG">Sud-Afriko</ref>,
      kaj <ref tip="vid" cel="zambi.0o.GEOG">Zambio</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Zimbabwe</trd>,
      <trd>Republika <ind>Zimbabwe</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>