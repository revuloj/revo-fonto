<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvak.xml,v 1.25 2025/01/06 20:26:27 revo Exp $">
<kap>
  <rad>kvak</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="kvak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kvak.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel
      <ref tip="vid" cel="ran.0o">rano</ref> a&ubreve;
      <ref tip="vid" cel="anas.0o">anaso</ref>:
      <ekz>
        ne, tio ne estis la sonoriloj, tio estis ranoj, kiuj
        <tld/>is en la mar&ccirc;oj
        <fnt><bib>Fab4</bib>, <lok>Anneto</lok></fnt>;
      </ekz>
      <ekz>
        ili lernas sa&gcirc;econ: lingvon grekan, grekan, hebrean, <tld/>! sonis kvaza&ubreve; ranoj
        <tld/>as a&ubreve; iu kun akvobotoj iras en mar&ccirc;a akvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi tute ne komprenas iujn vo&ccirc;ojn, kiuj <ctl>sa&gcirc;e</ctl>
        blagas kaj <tld/>as en Monato pri terorismo
        <fnt>
          <aut>Jaroslav el &Ccirc;e&hcirc;io</aut>:
          <vrk>Al la intelektuloj de Monato</vrk>.
          <bib>Monato</bib>.
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e l' akvo <tld/>is nur <klr>[&#8230;]</klr> bufo
        <fnt>
          <bib>LSB</bib>
          <lok>Printempa&jcirc;oj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gak.0i">gaki</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(p.p. rano)</klr></trd>,
    <trd>&c_k;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(p.p. anaso)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ku&ncaron;kat</trd>,
    <trd>kv&aacute;kat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnattern</trd>,
    <trd>quaken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>quack <klr>(p.p. anaso)</klr></trd>,
    <trd>croak <klr>(p.p. rano)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancaner</trd>,
    <trd>coasser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>brekeg</trd>,
    <trd>h&aacute;pog</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12466;&#12525;&#12466;&#12525;&#40180;&#12367;  <pr>&#12466;&#12525;&#12466;&#12525;&#12394;&#12367;</pr></trd>,
    <trd>&#12460;&#12449;&#12460;&#12449;&#40180;&#12367;  <pr>&#12460;&#12449;&#12460;&#12449;&#12394;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">kwaken</trd>
  <trdgrp lng="pl">
    <trd>duka&cacute;</trd>,
    <trd>kumka&cacute;</trd>,
    <trd>kwaka&cacute;</trd>,
    <trd>rechota&cacute;</trd>,
    <trd>skrzecze&cacute;</trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>c&acirc;r&acirc;it</trd>,
    <trd>m&abreve;c&abreve;i</trd>,
    <trd>&tcomma;ip&abreve;t</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>coachar <klr>(r&atilde;, sapo)</klr></trd>,
    <trd>grasnar <klr>(pato)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. rano)</klr></trd>,
    <trd>&c_k;&c_r;&c_ja;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. anaso)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kv&aacute;ka&tcaron; <klr>(&zcaron;aby, ka&ccaron;ky)</klr></trd>,
    <trd>kv&aacute;ka&tcaron; <klr>(&zcaron;aby| ka&ccaron;ky)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_r;&c_ja;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#32993;&#21561; <pr>h&uacute;chu&imacron;</pr></trd>
</drv>

<drv mrk="kvak.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="kvak.0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      rana, bufa a&ubreve; anasa:
      <ekz>
        subite &scirc;i <klr>[&#8230;]</klr> ekkriis:
        &leftquot;<tld lit="K"/>! Estas malseke! Estas malseke!
        <klr>[&#8230;]</klr>&rightquot;, en momento &scirc;i fari&gcirc;is rano
        <fnt><bib>Fab2</bib>, <lok>Malgranda Tuk</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gak.gik0.SON">gik gak</ref>,
      <ref cel="kva.0.SON">kva</ref>,
      <ref cel="kuxaks.0.SON">k&ubreve;aks</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;!</trd>
  <trd lng="de">quak!</trd>
  <trd lng="fr">co&acirc; !</trd>
  <trd lng="pl">kwa, kwa</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;!</trd>
  <trd lng="zh">&#21617;&#21617; <pr>gu&amacron;gu&amacron;</pr></trd>
</drv>

</art>
