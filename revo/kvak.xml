<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvak</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kvak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kvak.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel <ref tip="vid"
      cel="ran.0o">rano</ref> a&ubreve; <ref tip="vid" cel="anas.0o">anaso</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(p.p. rano)</klr></trd>,
    <trd>&c_k;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(p.p. anaso)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnattern <klr>(pp anaso)</klr></trd>,
    <trd>quaken <klr>(pp rano)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>quack <klr>(p.p. anaso)</klr></trd>,
    <trd>croak <klr>(p.p. rano)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancaner</trd>,
    <trd>coasser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>brekeg</trd>,
    <trd>h&aacute;pog</trd>
  </trdgrp>  
  <trd lng="nl">kwaken</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. rano)</klr></trd>,
    <trd>&c_k;&c_r;&c_ja;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. anaso)</klr></trd>
  </trdgrp>
</drv>
</art>
