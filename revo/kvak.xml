<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvak</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kvak.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Bleki, pp ranoj a&ubreve; anasoj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>quack <klr>(p.p. anaso)</klr></trd>,
    <trd>croak <klr>(p.p. rano)</klr></trd>
  </trdgrp>
  <trd lng="nl">kwaken</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. rano)</klr></trd>,
    <trd>&c_k;&c_r;&c_ja;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. anaso)</klr></trd>
  </trdgrp>
</drv>
</art>
