<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>kvak</rad>/i
      <fnt>Z</fnt>
    </kap>

    <drv mrk="kvak.0">
      <kap><tld/></kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          <ref cel="imit.son0o">Sonimito</ref> de bleko rana, bufa
          a&ubreve; anasa:
          <ekz>
            subite &scirc;i <klr>(...)</klr> ekkriis:
            <ctl><tld lit="K"/>! Estas malseke! Estas malseke!
            (...)</ctl>. En momento &scirc;i fari&gcirc;is rano
            <fnt><bib>Fab2</bib>, <lok>Malgranda Tuk</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_v;&c_a;!</trd>
    </drv>

    <drv mrk="kvak.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="kvak.0i.ZOO">
        <dif>
          <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel
          <ref tip="vid" cel="ran.0o">rano</ref> a&ubreve;
          <ref tip="vid" cel="anas.0o">anaso</ref>:
          <ekz>
            ne, tio ne estis la sonoriloj, tio estis ranoj, kiuj 
            <tld/>is en la mar&ccirc;oj
	    <fnt><bib>Fab4</bib>, <lok>Anneto</lok></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            mi tute ne komprenas iujn vo&ccirc;ojn, kiuj <ctl>sa&gcirc;e</ctl>
            blagas kaj <tld/>as en MONATO pri terorismo
	    <fnt>
	      <aut>Jaroslav el &Ccirc;e&hcirc;io</aut>:
              <vrk>Al la intelektuloj de MONATO</vrk>.
	      <bib>Monato</bib>.
	    </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(p.p. rano)</klr></trd>,
        <trd>&c_k;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(p.p. anaso)</klr></trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>schnattern <klr>(pp anaso)</klr></trd>,
        <trd>quaken <klr>(pp rano)</klr></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>quack <klr>(p.p. anaso)</klr></trd>,
        <trd>croak <klr>(p.p. rano)</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>cancaner</trd>,
        <trd>coasser</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>brekeg</trd>,
        <trd>h&aacute;pog</trd>
      </trdgrp>  
      <trd lng="nl">kwaken</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. rano)</klr></trd>,
        <trd>&c_k;&c_r;&c_ja;&c_k;&c_a;&c_t;&c_mol; <klr>(p.p. anaso)</klr></trd>
      </trdgrp>
    </drv>
  </art>
