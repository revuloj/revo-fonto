<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: timbal.xml,v 1.12 2018/04/13 13:10:14 revo Exp $">
<kap>
  <rad>timbal</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="timbal.0o">
  <kap><tld/>o</kap>
  <snc mrk="timbal.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o"
      lst="voko:frapinstrumentoj">Frapinstrumento</ref>
      kun latuna sonkesto, kies tono estas regebla per pedalo:
      <ekz>
        Ili ludas <tld/>ojn / mane sen sova&gcirc;'. /
        Kelkaj flustras kaj tenas / lumojn &ccirc;e la viza&gcirc;'.
        <fnt>
          <aut>G. Heym, trad. Cezar</aut>,
          <vrk>&Ccirc;iuj pejza&gcirc;oj</vrk>,
          <lok><url ref="http://www.cezarpoezio.de/107.html">
          Cezarpoezio</url>, [2004?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        franca uverturo estas ofte aran&gcirc;ita kun trumpeto kaj
        <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;franca
        uverturo&rightquot;, 
        2007-03-13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_i;&c_m;&c_p;&c_a;&c_n;</trd>
  <trd lng="ca">timbala</trd>
  <trdgrp lng="cs">
    <trd>kotel</trd>,
    <trd>tymp&aacute;n</trd>
  </trdgrp>
  <trd lng="de">Pauke</trd>
  <trdgrp lng="en">
    <trd>timpani</trd>,
    <trd>kettle drum</trd>,
    <trd>kettledrum</trd>
  </trdgrp>
  <trd lng="es">timbal</trd>
  <trd lng="fr">timbale<klr> (musique)</klr></trd>
  <trd lng="hu">cimbalom</trd>
  <trdgrp lng="it">
    <trd>timballo</trd>,
    <trd>timpano</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pauk</trd>,
    <trd>keteltrom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kocio&lstroke;<klr tip="amb"> (instrument)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kotol</trd>,
    <trd>tympan</trd>
  </trdgrp>
</drv>
</art>

