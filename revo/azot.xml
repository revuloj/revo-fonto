<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <ofc>*</ofc>
       <rad>azot</rad>/o
     </kap>
     <mlg>N</mlg>
     <drv mrk="azot.0o">
       <kap><ofc>*</ofc><tld/>o</kap>
       <mlg>N</mlg>
       <uzo tip="fak">SCI</uzo>
       <uzo tip="fak">KEM</uzo>
       <snc mrk="azot.0o.elemento">
     <uzo tip="fak">KEM</uzo>
     <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 7, atompezo 14,0067, simbolo <frm>N</frm>.
      En scienco pli ofte uzata estas nomo
      <ref tip="sin" cel="nitrog.0o">nitrogeno</ref>:
       <ekz>
         <tld/>o estas grava parto de la proteinoj kaj nukleacidoj
         (16&ndash;17%), tamen el la tuta seka maso de biologiaj
         organismoj &gcirc;i konsistigas nur 1&ndash;3%;
       </ekz><ekz>
         <ref cel="salpet.0o">salpetro</ref> entenas <tld/>on.
       </ekz>
     </dif>
     <rim>
       En la &hcirc;emiaj nomoj de <tld/>havaj kombinoj ofte aperas
       terminformanta vortero: <ref cel="nitr.0">nitr...</ref>.
     </rim>
         <trd lng="sv">kv&auml;ve</trd>
       </snc>
       <snc mrk="azot.0o.N2">
     <uzo tip="fak">KEM</uzo>
     <uzo tip="fak">TEK</uzo>
     <dif>
       Simpla substanco el <tld/>o<sncref ref="azot.0o.elemento"/>
       <frm>(N<sub>2</sub>)</frm>, &ccirc;e la ordinaraj kondi&ccirc;oj
&#8208;
       senkolora, senodora, sengusta gaso, el kiu konsistas 78%
       de la aero; &gcirc;i ne brulas kaj ebligas nek
       bruladon nek spiradon:
       <ekz>
         mi scias, ke la aero konsistas el <tld/>o, oksigeno
         <fnt>
           <aut>F. Tuglas, tr. Hilda DRESEN</aut>:
           <vrk>El la pensotaglibro</vrk>.
           <lok>Nica literatura revuo, 2:3</lok>;
         </fnt>;
       </ekz><ekz>
         <ind>likva <tld/>o</ind> estas uzata en diversaj
         malvarmegigiloj.
         <trd lng="fi">nestetyppi</trd>
         <trd lng="pl">ciek&lstroke;y azot</trd>
         <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_i;&c_j;
		&c_a;&c_z;&c_o;&c_t;</trd>
         <trd lng="sv">flytande kv&auml;ve</trd>
       </ekz>
     </dif>
     <trd lng="br">dinitrogen</trd>
     <trd lng="sv">kv&auml;vgas</trd>
       </snc>
       <trd lng="be">&c_a;&c_z;&c_o;&c_t;</trd>
       <trd lng="bg">&c_a;&c_z;&c_o;&c_t;</trd>
       <trd lng="br">nitrogen</trd>
       <trd lng="ca">nitrogen</trd>
       <trd lng="de">Stickstoff</trd>
       <trd lng="en">nitrogen</trd>
       <trd lng="es">nitr&oacute;geno</trd>
       <trd lng="fi">typpi</trd>
       <trd lng="fr">azote</trd>
       <trd lng="hu">nitrog&eacute;n</trd>
       <trd lng="it">azoto</trd>
       <trd lng="la">Nitrogenium</trd>
       <trd lng="nl">stikstof</trd>
       <trd lng="pl">azot</trd>
       <trd lng="pt">azoto</trd>
       <trd lng="ru">&c_a;&c_z;&c_o;&c_t;</trd>
     </drv>

     <drv mrk="azot.0a">
       <kap><tld/>a</kap>
       <uzo tip="fak">KEM</uzo>
       <snc>
     <dif>
       Entenanta <tld/>on:
       <ekz>
         inter la <ind><tld/>aj sterkumoj</ind> iuj estas organikaj
         (ekz-e sterko, torfo), kaj aliaj, mineralaj;
         <trd lng="be">&c_a;&c_z;&c_o;&c_t;&c_n;&c_y;&c_ja; &c_w;&c_g;&c_n;&c_a;&c_je;&c_n;&c_mol;&c_n;&c_ib;</trd>
         <trd lng="ru">&c_a;&c_z;&c_o;&c_t;&c_n;&c_y;&c_je;
&c_u;&c_d;&c_o;&c_b;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
       </ekz><ekz>
         <ref cel="azot.0acido"><tld/>a acido</ref>.
       </ekz>
     </dif>
       </snc>
       <trd lng="be">&c_a;&c_z;&c_o;&c_t;&c_n;&c_y;</trd>
       <trd lng="ca">nitrogenat</trd>
       <trd lng="fi">typpi-</trd>
       <trd lng="pl">azotowy</trd>
       <trd lng="ru">&c_a;&c_z;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
     </drv>

     <drv mrk="azot.0acido">
       <kap><ofc>*</ofc><tld/>acido</kap>
       <uzo tip="fak">KEM</uzo>
       <snc>
     <dif>
       Tre morda flua&jcirc;o (la <ref tip="super" 
cel="acid.0o">acido</ref>
       <frm>HNO<sub>3</sub></frm>) uzata en industrio kaj gravurado:
       <ekz>
         en tiuj &ccirc;i boteletoj sin trovas diversaj acidoj:
         vinagro, sulfuracido, <tld/>acido kaj aliaj
         <fnt><bib>F</bib>, <lok>&FE;35.</lok></fnt>.
       </ekz>
     </dif>
     <ref tip="sin" cel="nitrat.0a.acido">nitrata acido</ref>
     <refgrp tip="vid">
       <ref cel="akvafo.0o">akvaforto</ref>.
     </refgrp>
       </snc>
       <trd lng="be">&c_a;&c_z;&c_o;&c_t;&c_n;&c_a;&c_ja;
<ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</ind></trd>
       <trd lng="br">trenkenn nitrek</trd>
       <trd lng="ca">&agrave;cid <ind>n&iacute;tric</ind></trd>
       <trdgrp lng="de">
     <trd>Salpeters&auml;ure</trd>,
     <trd>Scheidewasser</trd>
       </trdgrp>
       <trd lng="en"><ind>nitric</ind> acid</trd>
       <trd lng="es">&aacute;cido <ind>n&iacute;trico</ind></trd>
       <trd lng="fi">typpihappo</trd>
       <trd lng="fr">acide <ind>nitrique</ind></trd>
       <trdgrp lng="hu">
     <trd>sal&eacute;tromsav</trd>,
     <trd>v&aacute;laszt&oacute;v&iacute;z</trd>
       </trdgrp>
       <trd lng="nl">salpeterzuur</trd>
       <trd lng="pl">kwas azotowy</trd>
       <trd lng="ru">&c_a;&c_z;&c_o;&c_t;&c_n;&c_a;&c_ja;
&c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
       <trd lng="sv">salpetersyra</trd>
     </drv>
   </art>
