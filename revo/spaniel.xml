<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spaniel.xml,v 1.6 2023/10/05 18:31:46 revo Exp $">
<kap>
  <rad>spaniel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="spaniel.0o">
  <kap><tld/>o</kap>
  <snc mrk="spaniel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="hund.0o.dombesto">Hundo</ref> el raso
      mezgranda, kun longaj pendaj oreloj, &scirc;atata en birdo&ccirc;aso kaj kiel
      hejma besto:
      <ekz>
        sendormulo, vaganta meznokte kun sia <tld/>o, &ccirc;iupa&scirc;e a&ubreve;dus
        fluantajn tra malfermitaj fenestroj &gcirc;emetojn, murmurojn
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 22a, p. 127a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_n;&c_ib;&c_je;&c_l;&c_mol;</trd>
  <trd lng="cs">kokr&scaron;pan&ecaron;l</trd>
  <trd lng="de">Spaniel</trd>
  <trd lng="en">spaniel</trd>
  <trd lng="fr">&eacute;pagneul</trd>
  <trd lng="pl">spaniel</trd>
  <trd lng="sk">koker&scaron;paniel</trd>
  <trd lng="uk">&c_s;&#x43F;&c_a;&#x43D;&c_ib;&c_jeu;&#x43B;&#x44C; (&#x43F;&#x43E;&c_r;&#x43E;&c_d;&c_a; &c_s;&#x43E;&c_b;&c_a;&#x43A;)</trd>
</drv>
</art>

