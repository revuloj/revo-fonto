<drv mrk="cxevaled.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="cxevaled.0oj.ekvedoj">
    <dif>
      <ref tip="dif" cel="ekved.0oj">Ekvedoj</ref>:
      <ekz>
        la sova&gcirc;azeno (Equus hemionus) el la ordo de neparhufuloj kaj
        familio de <tld/>oj trovi&gcirc;as en centra kaj okcidenta Azio
        <fnt><vrk><url
           ref="&CRI;1/2005/06/20/1@25976.htm"
           >Sova&gcirc;azeno</url></vrk>.
           <lok>China Radio International, 2005-06-20</lok></fnt>;  
      </ekz><ekz>
        la sova&gcirc;&ccirc;evalo (<nom>Equus przewalskii</nom>) el la ordo de
        neparhufuloj kaj familio de
        <tld/>oj trovi&gcirc;as en &Gcirc;ungar-baseno de &Scirc;in&gcirc;jang, la baseno de
        Manasrivero kaj
        &Hcirc;ovd-baseno de Mongolio
        <fnt>
          <vrk><url
          ref="&CRI;1/2005/06/20/1@25977.htm"
              >Sova&gcirc;&ccirc;evalo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_o;&c_n;&c_je;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
    <trd lng="cs">Ko&ncaron;ovit&iacute;</trd>
    <trd lng="de">Pferde (Familie)</trd>
    <trd lng="en">Equidae</trd>
    <trd lng="la">Equidae</trd>
    <trd lng="pl">Koniowate</trd>
    <trd lng="ru">&c_L;&c_o;&c_sh;&c_a;&c_d;&c_i;&c_n;&c_y;&c_je;</trd>
  </snc>
</drv>

