<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: viaduk.xml,v 1.19 2022/03/28 11:08:54 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>viadukt</rad>/o
</kap>
<drv mrk="viaduk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Alta <ref tip="super" cel="pont.0o.ARKI">ponto</ref>,
      ofte el pluraj vicoj da arkadoj,
      konstruita super rivero, valo ktp:
      <ekz>
        la fervojo trapasas 31 pontojn, 13 <tld/>ojn kaj uzas 21 tunelojn <klr>[&#x2026;]</klr>
        post 15 km estas la plej alta punkto: <tld/>o <nom>La Polvorilla</nom>, kurba,
        224 metrojn longa kaj 70 metrojn alta, kun &scirc;tuparoj, ebligantaj al la vizitantoj
        malsupreniri &gcirc;is la fundo de la intermonto
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php">Unika fervojo en Andoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_ja;&c_d;&c_u;&c_k;</trd>
  <trd lng="ca">viaducte</trd>
  <trdgrp lng="cs">
    <trd>most</trd>,
    <trd>viadukt</trd>
  </trdgrp>
  <trd lng="de">Viadukt</trd>
  <trd lng="en">viaduct</trd>
  <trd lng="fr">viaduc</trd>
  <trdgrp lng="hu">
    <trd>viadukt</trd>,
    <trd>v&ouml;lgyh&iacute;d</trd>
  </trdgrp>
  <trd lng="nl">viaduct</trd>
  <trd lng="pl">wiadukt</trd>
  <trd lng="pt">viaduto</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_a;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_t;&c_je;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>most</trd>,
    <trd>viadukt</trd>
  </trdgrp>
  <trd lng="tr">viyad&uuml;k</trd>
  <trd lng="uk">&#x432;&#x456;&#x430;&#x434;&#x443;&#x43A;</trd>
</drv>
</art>
