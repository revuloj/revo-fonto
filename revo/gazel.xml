<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gazel.xml,v 1.30 2023/11/11 10:00:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gazel</rad>/o
</kap>
<drv mrk="gazel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> 
      el familio <ref tip="malprt" cel="boved.0oj">bovedoj</ref>,
      (<trd lng="la">Gazella</trd>),
      tre <ref cel="elegan.0a">eleganta</ref>, sveltkrura, rapidkura
      rema&ccirc;ulo; en pli vasta senco oni anka&ubreve; inkluzivas la parencajn genrojn 
      <trd lng="la">Eudorcas</trd>, <trd lng="la">Nanger</trd>,
      <trd lng="la">Antilope</trd>, <trd lng="la">Procapra</trd>:
      <ekz>
        man&gcirc;u tion tiel, kiel oni man&gcirc;as <tld/>on kaj cervon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 12:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        rapidpieda kiel <tld/>o sur la kampo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 2:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        rapidkuraj ili estis, kiel <tld/>oj sur la montoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 12:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsa&gcirc;a estas tiu, kiu forlasas la librojn tiel rapide, kiel forkuranta <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Przewalski</nom>-<tld/>o <klr>[&#x2026;]</klr> trovi&gcirc;as en <nom>&Ccirc;inghaj</nom>, <nom>Gansu</nom>,
        <nom>&Scirc;in&gcirc;jang</nom> kaj la
        Interna Mongolio de &Ccirc;inio <klr>[&#x2026;]</klr>, tiu specio aspekte similas al la
        tibeta <tld/>o, sed &gcirc;iaj kvar membroj kaj kornoj estas pli maldikaj ol tiuj
        de la tibeta <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49923">Przewalski-gazelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="antilo.0o.subfamiliano">antilopo</ref>
    <bld lok="&WCU;/thumb/2/2f/Gazella_gazella_gazella_2.jpg/640px-Gazella_gazella_gazella_2.jpg" prm="CC BY-SA 4.0"><tld/>o (Gazella gazella), Hevel Lakhish, Israelo
        <fnt>
          <aut>&mem;&yod;&nun;&vav;&zayin;&yod;&gimel; - MinoZig, propra verko</aut>
          <lok><url ref="&WCW;/File:Gazella_gazella_gazella_2.jpg">&WCW;/File:Gazella_gazella_gazella_2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bo">&#xF62;&#xF92;&#xF7C;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="ca">gasela</trd>
  <trd lng="cs">gazela</trd>
  <trd lng="de">Gazelle</trd>
  <trd lng="el">&gamma;&alfa;&zeta;&epsilon_ton;&lambda;&alfa;</trd>
  <trd lng="en">gazelle</trd>
  <trd lng="es">gacela</trd>
  <trd lng="fr">gazelle</trd>
  <trd lng="hu">gazella</trd>
  <trd lng="ja">&#x30AC;&#x30BC;&#x30EB;</trd>
  <trd lng="nl">gazelle</trd>
  <trd lng="pt">gazela</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">gazela</trd>
  <trd lng="sv">gasell</trd>
  <trd lng="tr">ceylan</trd>
  <trd lng="uk">&#x491;&#x430;&#x437;&#x435;&#x43B;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E00;&#x79CD;&#x5C0F;&#x7F9A;&#x7F8A; <pr>y&#x12B;zh&#x1D2;ngxi&#x1CE;ol&#xED;ngy&#xE1;ng</pr></trd>,
    <trd>&#x77AA;&#x7F9A; <pr>d&#xE8;ngl&#xED;ng</pr></trd>
  </trdgrp>
</drv>
</art>
