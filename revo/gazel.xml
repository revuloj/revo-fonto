<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>gazel</rad>/o
</kap>
<drv mrk="gazel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>,
      subfamilio <ref tip="super" cel="antilo.0o.1">antilopoj</ref>
      (<trd lng="la">Gazella</trd>), 
      tre eleganta, sveltkrura, rapidkura rema&ccirc;ulo.  
    </dif>
  </snc>
  <trd lng="de">Gazelle</trd>
  <trd lng="fr">gazelle</trd>
</drv>
</art>
