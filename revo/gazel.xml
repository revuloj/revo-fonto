<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gazel</rad>/o
</kap>
<drv mrk="gazel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="boved.0oj">bovedoj</ref>,
      (<trd lng="la">Gazella</trd>),
      tre eleganta, sveltkrura, rapidkura rema&ccirc;ulo.
    </dif>
    <ref tip="super" cel="antilo.0o.subfamiliano">antilopo</ref>
  </snc>
  <trd lng="de">Gazelle</trd>
  <trd lng="el">&gamma;&alfa;&zeta;&epsilon_ton;&lambda;&alfa;</trd>
  <trd lng="es">gacela</trd>
  <trd lng="fr">gazelle</trd>
  <trd lng="hu">gazella</trd>
  <trd lng="nl">gazelle</trd>
  <trd lng="pt">gazela</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sv">gasell</trd>
</drv>
</art>
