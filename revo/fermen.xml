<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fermen.xml,v 1.30 2026/01/14 18:00:21 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>ferment</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="fermen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Scirc;an&gcirc;i sian konsiston estigante alkoholon, vinagron, laktacidon kc,
      ne putrante:
      <ekz>
        el la pasto <klr>[&#8230;]</klr> ili bakis macajn platpanojn, &ccirc;ar &gcirc;i ankora&ubreve; ne
        <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 12:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KUI</uzo>
        <ind><tld/>inta</ind> trinka&jcirc;o;
        <trd lng="ca">fermentat</trd>
        <trd lng="es">fermentado</trd>
        <trd lng="fr">ferment&eacute;</trd>
        <trdgrp lng="hu">
          <trd>megerjedt</trd>,
          <trd>kelt<klr tip="amb"> (t&eacute;szta)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        la premitaj vinberoj <tld/>as nur tiom longe, &gcirc;is esti&gcirc;as la tipe roza koloro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008488.php">Kuk-urbo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kaze.0igxi">kazei&gcirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="fermen.0i.FIG">
    <dif>
      <klr>(pri sento, precipe malkontenta, protesta)</klr>
      Pliforti&gcirc;i interne minacante eksplodi balda&ubreve;:
      <ekz>
        gigantaj planoj <tld/>as en mia kreema kranio
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        forta kolero kontra&ubreve; la malagrabla&jcirc;oj de la vivo komencis <tld/>i en li
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malbona penso ku&scirc;is en lia sango, &ccirc;iam denove bolis kaj <tld/>is en li la
        sento de maldol&ccirc;eco
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fermentar</trd>,
    <trd>llevar</trd>
  </trdgrp>
  <trd lng="cs">kvasit</trd>
  <trdgrp lng="de">
     <trd>g&auml;ren</trd>,
     <trd>in <ind>G&auml;rung</ind> sein</trd></trdgrp>
  <trd lng="en">ferment</trd>
  <trd lng="es">fermentar</trd>
  <trdgrp lng="fr">
    <trd>fermenter</trd>,
    <trd>lever<klr tip="ind"> (fermenter)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>erjed</trd>,
    <trd>kel<klr tip="amb">(t&eacute;szta)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#30330;&#37237;&#12377;&#12427;  <pr>&#12399;&#12387;&#12371;&#12358;&#12377;&#12427;</pr></trd>
  <trd lng="nl">fermenteren</trd>
  <trd lng="pl">fermentowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_i;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;
    &c_t;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kvasi&tcaron;</trd>,
    <trd>kysn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_i;&c_s;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_f;&c_je;&c_r;&c_m;&c_je;&c_n;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o; &c_t;&c_ib;&c_s;&c_t;&c_o;)</trd>
  </trdgrp>
</drv>

<drv mrk="fermen.0o">
  <kap><tld/>o</kap>
  <snc mrk="fermen.0o.ago">
    <dif>
      Fermentado:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj ideoj faris certan servon al la lingvo kaj utilis kiel bona komenco por &gcirc;ia
        <tld/>o
        <fnt>
          <bib>Zam</bib>
          <lok>Rolo de Zamenhof en la internacilingva movado</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_s;&c_k;&c_ib;&c_s;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_f;&c_e;&c_r;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="ca">fermentaci&oacute;</trd>
    <trdgrp lng="cs">
      <trd>fermentace</trd>,
      <trd>kva&scaron;en&iacute;</trd>,
      <trd>zkva&scaron;ov&aacute;n&iacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>G&auml;rung</trd>,
       <trd>Fermentation</trd>
    </trdgrp>
    <trd lng="en">fermentation</trd>
    <trd lng="es">fermentaci&oacute;n</trd>
    <trd lng="fr">fermentation</trd>
    <trdgrp lng="hu">
      <trd>erjed&eacute;s</trd>,
      <trd>kel&eacute;s<klr tip="amb">(t&eacute;szta)</klr></trd>
    </trdgrp>
    <trd lng="nl">fermentatie</trd>
    <trd lng="pl">fermentacja</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_s;&c_k;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_f;&c_je;&c_r;&c_m;&c_je;&c_n;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
    <trd lng="sk">kvasenie</trd>
    <trdgrp lng="zh">
    <trd>&#21457;&#37237; <pr>f&#257;ji&agrave;o</pr></trd>,
    <trd>&#37247;&#36896; <pr>ni&agrave;ngz&agrave;o</pr></trd>
  </trdgrp>
</snc>  
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="F"/>ilo, enzimo:
      <ekz>
        tiuj molekuloj de RNA, diference de molekuloj de DNA, posedas memstaran <tld/>an
        aktivecon kaj povas reprodukti&gcirc;i sen helpo de aliaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009428.php">Lastaj opinioj pri la origino de vivo</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ja">
    <trd>&#30330;&#37237;&#20307;  <pr>&#12399;&#12387;&#12371;&#12358;&#12383;&#12356;</pr></trd>,
    <trd>&#30330;&#37237;&#24494;&#29983;&#29289;  <pr>&#12399;&#12387;&#12371;&#12358;&#12403;&#12379;&#12356;&#12406;&#12388;</pr></trd>,
    <trd>&#37237;&#32032;  <pr>&#12371;&#12358;&#12381;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fermen.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Malkomponi&gcirc;o per <tld/>o:
      <ekz>
        pulkvo do estas la alkohola trinka&jcirc;o <klr>[&#8230;]</klr> estigita dum la natura
        <tld/>ado de la miel-akvo &ccirc;erpita el tiu kolosa agavo
        <fnt>
          <aut>Fidel Figueroa</aut>
          <vrk><url ref="&Monato;/2000/007033.php">Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        mieleca gusto, ka&ubreve;zata de multe da alkoholo (9,5%), kun <ind>re<tld/>ado</ind> post
        enboteligo
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
        <trdgrp lng="ca">
          <trd>fermentaci&oacute; secund&agrave;ria</trd>,
          <trd>refermentaci&oacute;</trd>
        </trdgrp>
        <trd lng="en">secondary <ind>fermentation</ind></trd>
        <trdgrp lng="es">
          <trd>fermentaci&oacute;n secundaria</trd>,
          <trd>refermentaci&oacute;n</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>fermentation secondaire</trd>,
          <trd>refermentation</trd>
        </trdgrp>
      </ekz>
      <ekz>
        belgaj bierspecoj, ordigitaj la&ubreve; la <tld/>adprocedo: alta (a&ubreve; varma), malalta
        (a&ubreve; malvarma), spontanea kaj miksa fermentado
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_ib;&c_s;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_f;&c_e;&c_r;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">fermentaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>fermentace</trd>,
    <trd>kva&scaron;en&iacute;</trd>,
    <trd>zkva&scaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>G&auml;rung</trd>,
     <trd>Fermentation</trd></trdgrp>
  <trd lng="en">fermentation</trd>
  <trd lng="es">fermentaci&oacute;n</trd>
  <trd lng="fr">fermentation</trd>
  <trdgrp lng="hu">
    <trd>erjed&eacute;s</trd>,
    <trd>kel&eacute;s<klr tip="amb">(t&eacute;szta)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#30330;&#37237;  <pr>&#12399;&#12387;&#12371;&#12358;</pr></trd>
  <trd lng="nl">fermentatie</trd>
  <trd lng="pl">fermentacja</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_r;&c_m;&c_je;&c_n;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">kvasenie</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_o;&c_d;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_f;&c_je;&c_r;&c_m;&c_je;&c_n;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21457;&#37237; <pr>f&#257;ji&agrave;o</pr></trd>,
    <trd>&#37247;&#36896; <pr>ni&agrave;ngz&agrave;o</pr></trd>,
    <trd>&#21457;&#37237;&#20316;&#29992; <pr>f&#257;ji&agrave;ozu&ograve;y&ograve;ng</pr></trd>,
    <trd>&#22823;&#39479;&#20098; <pr>d&agrave;is&#257;olu&agrave;n</pr></trd>
  </trdgrp>
</drv>


<drv mrk="fermen.0ajxo">
  <kap>
    <tld/>a&jcirc;o,
    <var><kap><tld/>ilo</kap></var>
  </kap>
  <snc>
    <dif>
      Substanco, uzata por <tld/>i:
      <ekz>
        <ref cel="mac.0o">maco</ref>j estu man&gcirc;ataj dum la sep tagoj, kaj ne vidi&gcirc;u &ccirc;e vi <tld/>a&jcirc;o
        kaj ne
        vidi&gcirc;u &ccirc;e vi <tld/>inta pasto
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 13:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        malmulto da <tld/>a&jcirc;o fermentigas la tutan mason
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 5:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        alportinte hejmen la grenon, mi <klr>[&#8230;]</klr> bezonas muelilon por mueli &gcirc;in,
        kribrilon por kribri &gcirc;in, kaj <tld/>ilon por <tld/>i &gcirc;in, por fari panon el &gcirc;i
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la framasonismo servis kiel <tld/>ilo por la &leftquot;spiritaj&rightquot; tavoloj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="sod.bak0o">baksodo</ref>,
      <ref cel="enzim.0o">enzimo</ref>,
      <ref cel="gist.0o.KUI">gisto<sncref/></ref>,
      <ref cel="kaze.0igajxo">kazeiga&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_r;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_e;&c_n;&c_z;&c_y;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">ferment</trd>
  <trdgrp lng="cs">
    <trd>kvasnice</trd>,
    <trd>z&aacute;kvas</trd>
  </trdgrp>
  <trd lng="de">Triebmittel</trd>
  <trd lng="en">leavening agent</trd>
  <trd lng="es">fermento</trd>
  <trd lng="fr">ferment</trd>
  <trd lng="hu">&eacute;leszt&odblac;</trd>
  <trdgrp lng="nl">
    <trd>ferment</trd>,
    <trd>enzyme</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ferment</trd>,
    <trd>zaczyn</trd>
  </trdgrp>
  <trd lng="pt">fermento</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_r;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_v;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_h;&c_l;&c_i;&#769;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>enz&yacute;m</trd>,
    <trd>ferment</trd>,
    <trd>kvas</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_je;&c_r;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_v;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_ch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_r;&c_a;</trd>
  </trdgrp>
</drv>


<drv mrk="fermen.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Facile <tld/>anta.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_d;&c_a;
    <ind>&c_b;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>,
    <trd>&c_z;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_d;&c_a;
    <ind>&c_f;&c_e;&c_r;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="ca">fermentable</trd>
  <trd lng="en">fermentable</trd>
  <trd lng="fr">fermentescible</trd>
  <trdgrp lng="hu">
    <trd>erjeszthet&odblac;</trd>,
    <trd>keleszthet&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30330;&#37237;&#24615;&#12398;  <pr>&#12399;&#12387;&#12371;&#12358;&#12379;&#12356;&#12398;</pr></trd>,
    <trd>&#30330;&#37237;&#12375;&#12420;&#12377;&#12356;  <pr>&#12399;&#12387;&#12371;&#12358;&#12375;&#12420;&#12377;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">fermentowalny</trd>
</drv>
</art>
