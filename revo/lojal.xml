<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lojal</rad>/a
</kap>

<drv mrk="lojal.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Aganta konforme al la le&gcirc;oj de honoro, sincereco, fideleco,
      dank&scirc;uldo, a&ubreve; al siaj promesoj:
      <ekz>
        <tld/>a regnano, malamiko;
      </ekz>
      <ekz>
        <tld/>a konduto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fidel.0a">fidela</ref>,
      <ref cel="fid.0inda">fidinda</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_o;&c_ja;&c_l;&c_je;&c_n;</trd>
  <trd lng="br">leal</trd>
  <trdgrp lng="de">
    <trd>loyal</trd>,
    <trd>gesetzestreu</trd>,
    <trd>rechtschaffen</trd>,
    <trd>redlich</trd>,
    <trd>aufrichtig</trd>
  </trdgrp>
  <trd lng="en">loyal</trd>
  <trd lng="es">leal</trd>
  <trdgrp lng="fr">
    <trd>droit<klr tip="amb"> (loyal)</klr></trd>,
    <trd>loyal</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&udblac;s&eacute;ges</trd>,
    <trd>h&udblac;</trd>,
    <trd>loj&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loyaal</trd>,
    <trd>trouw</trd>
  </trdgrp>
  <trd lng="pt">leal</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_ja;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kladný</trd>,
    <trd>nestranný</trd>,
    <trd>súhlasný</trd>
  </trdgrp>
  <trd lng="sv">lojal</trd>
</drv>

<drv mrk="lojal.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="lojal.0eco.KOMUNE">
    <dif>
      Kvalito de homo a&ubreve; ago, kiu sinceras, fidelas, akordas kun
      anta&ubreve;a
      promeso:
      <ekz>
        <ind>&jcirc;uri <tld/>econ</ind> al la &scirc;tato
        <trd lng="hu">h&udblac;s&eacute;get esk&uuml;szik</trd>;
      </ekz>
      <ekz>
        la <tld/>eco al Esperanto estas tute privata afero
        <fnt>
        <aut>M. Lavallee</aut>,
        <vrk>Kune a&ubreve; malkune!</vrk>,
        <lok>soc.culture.esperanto, 1998-05-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_o;&c_ja;&c_l;&c_n;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">lealded</trd>
  <trd lng="de">Loyalit&auml;t</trd>
  <trd lng="en">loyalty</trd>
  <trd lng="es">lealtad</trd>
  <trd lng="fr">loyaut&eacute;</trd>
  <trdgrp lng="nl">
    <trd>loyaliteit</trd>,
    <trd>loyauteit</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(vernosť vládcovi...)</klr></trd>,
    <trd>lojalita</trd>
  </trdgrp>
</drv>

<drv mrk="lojal.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="lojal.mal0a.KOMUNE">
    <dif>
      Kiu ne konformi&gcirc;as al dirita promeso a&ubreve; al komuna regulo de
      honoro,
      fideleco:
      <ekz>
        suspektemo estas <klr>...</klr> maljusta kaj mal<tld/>a kontra&ubreve;
        Zalmuna
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 3a, Lemuel da&ubreve;rigas sian
        esploron</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a subulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="perfid.0a.KOMUNE">perfida</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_l;&c_o;&c_ja;&c_l;&c_je;&c_n;</trd>
  <trd lng="br">disleal</trd>
  <trd lng="en">disloyal</trd>
  <trd lng="es">desleal</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;loyal</trd>,
    <trd>fourbe</trd>,
    <trd>parjure</trd>,
    <trd>perfide</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trouweloos</trd>,
    <trd>ontrouw</trd>,
    <trd>niet <ind>loyaal</ind></trd>
  </trdgrp>
  <trd lng="sk">nelojálny</trd>
</drv>
</art>
