<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lojal.xml,v 1.32 2022/07/12 08:42:08 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>lojal</rad>/a
</kap>

<drv mrk="lojal.0a">
  <kap><tld/>a</kap>
  <snc mrk="lojal.0a.KOMUNE">
    <dif>
      Donanta firman kaj konstantan subtenon kaj fidelecon al alia persono, organizo a&ubreve; celo:
      <ekz>
        <tld/>a konduto, partnero, civitano;
      </ekz>
      <ekz>
        solvi &ccirc;iujn kolekti&gcirc;intajn disputojn a&ubreve; dubojn en <tld/>a interkonsento
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
          1911</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io bona kaj &ccirc;io efektive necesa povas facile esti farata <klr>[&#8230;]</klr> per vojo
        <tld/>a, en harmonio kaj paco
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iam <tld/>e agadis a&ubreve; almena&ubreve; penis agadi konforme al tiu deklaro
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;okite, kaj kiel <tld/>a subulo, mi donis la folion al palaca gvardiano
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011612.php">Vestoj kaj investoj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="asidu.0a">asidua</ref>,
      <ref cel="fidel.0a.iu">fidela<sncref/></ref>,
      <ref cel="fid.0inda">fidinda</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="akord.0a">akorda</ref>,
      <ref cel="akurat.0e">akurata</ref>,
      <ref cel="honest.0a">honesta</ref>,
      <ref cel="konsen.0a">konsenta</ref>,
      <ref cel="konsta.0a.anima">konstanta<sncref/></ref>,
      <ref cel="obe.0ema">obeema</ref>,
      <ref cel="serv.0ema">servema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_o;&c_ja;&c_l;&c_je;&c_n;</trd>
  <trd lng="br">leal</trd>
  <trdgrp lng="cs">
    <trd>loaj&aacute;ln&iacute;</trd>,
    <trd>oddan&yacute;</trd>,
    <trd>v&ecaron;rn&yacute; vl&aacute;d&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>loyal</trd>,
    <trd>gesetzestreu</trd>,
    <trd>rechtschaffen</trd>,
    <trd>redlich</trd>,
    <trd>aufrichtig</trd>
  </trdgrp>
  <trd lng="en">loyal</trd>
  <trd lng="es">leal</trd>
  <trdgrp lng="fr">
    <trd>droit<klr tip="amb"> (loyal)</klr></trd>,
    <trd>loyal</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&udblac;s&eacute;ges</trd>,
    <trd>h&udblac;</trd>,
    <trd>loj&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loyaal</trd>,
    <trd>trouw</trd>
  </trdgrp>
  <trd lng="pl">lojalny</trd>
  <trd lng="pt">leal</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_ja;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kladn&yacute;</trd>,
    <trd>nestrann&yacute;</trd>,
    <trd>s&uacute;hlasn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">lojal</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&#x44F;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&#x43D;&c_i;&c_j; (&#x43E;&c_b;&#x43E;&c_v;&#8217;&#x44F;&c_z;&#x43A;&#x43E;&c_v;&c_ib;)</trd>,
    <trd>&c_v;&c_ib;&c_r;&#x43D;&c_i;&c_j; (&#x43E;&c_b;&c_ib;&c_c;&#x44F;&#x43D;&c_c;&c_ib;)</trd>,
    <trd>&c_b;&#x43B;&c_a;&c_g;&#x43E;&#x43D;&c_a;&c_d;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="lojal.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="lojal.0eco.KOMUNE">
    <dif>
      Kvalito de homo <tld/>a:
      <ekz>
        <ind>&jcirc;uri <tld/>econ</ind> al la &scirc;tato;
        <trd lng="hu">h&udblac;s&eacute;get esk&uuml;szik</trd>
      </ekz>
      <ekz>
        esperantista <tld/>eco kaj unueco
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en Barcelona en la 6a de septembro
          1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco al Esperanto estas tute privata afero
        <fnt>
        <aut>M. Lavallee</aut>
        <vrk>Kune a&ubreve; malkune!</vrk>
        <lok>soc.culture.esperanto, 1998-05-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_ja;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_o;&c_ja;&c_l;&c_n;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">lealded</trd>
  <trdgrp lng="cs">
    <trd>loajalita</trd>,
    <trd>loaj&aacute;lnost</trd>
  </trdgrp>
  <trd lng="de">Loyalit&auml;t</trd>
  <trd lng="en">loyalty</trd>
  <trd lng="es">lealtad</trd>
  <trd lng="fr">loyaut&eacute;</trd>
  <trdgrp lng="nl">
    <trd>loyaliteit</trd>,
    <trd>loyauteit</trd>
  </trdgrp>
  <trd lng="pl">lojalno&sacute;&cacute;</trd>
  <trd lng="sk">lojalita <klr>(vernos&tcaron; vl&aacute;dcovi...)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&#x44F;&#x43B;&#x44C;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_b;&#x43B;&c_a;&c_g;&#x43E;&#x43D;&c_a;&c_d;&c_ib;&c_j;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="lojal.0ismo">
  <kap>
    <tld/>ismo
  </kap>
  <snc mrk="lojal.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Publika fideleco al elektita a&ubreve; ricevita a&ubreve;toritato, al gvidanto, al institucio:
      <ekz>
        niaj bru&gcirc;aj amikoj, unuanime, decidis resti fidelaj al la Zamenhofa Esperanto
        kaj da&ubreve;rigi sian fidon al la Lingva Komitato; ili atestis sian <tld/>ismon en
        fiera deklaracio, legita dum la &gcirc;enerala kunveno de la Ligo, la 20an de
        Septembro 1908
        <fnt>
          <aut>Amatus</aut>
          <vrk><url
          ref="https://vikiflandrio.alcl.be/_bezoek/rbee/belgaesp_017_1910mar.pdf">Esperanto en
          Bru&gcirc;o</url></vrk>
          <lok>Belga Esperantisto, 1910-03 (17)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_ja;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="fr">loyalisme</trd>
</drv>

<drv mrk="lojal.mal0a">
  <kap>mal<tld/>a, <var><kap>ne<tld/>a</kap></var></kap>
  <snc mrk="lojal.mal0a.KOMUNE">
    <dif>
      Al kiu mankas konstanta fideleco; kiu ne firme subtenas:
      <ekz>
        suspektemo estas <klr>[&#8230;]</klr> maljusta kaj mal<tld/>a kontra&ubreve;
        <nom>Zalmuna</nom>
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 3a</lok></fnt>;
      </ekz>
      <ekz>
        impresas malagrable kaj mal<tld/>e, ke la a&ubreve;toro ne preterlasas okazon por
        elkra&ccirc;i galon sur &ccirc;io, kio rilatas al la kristana doktrino
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9311-kaj-la-suno-haltis.txt">Kaj la Suno Haltis...
          </url></vrk>
          <lok>93/11</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estigis tute ne<tld/>an konkuradon kun la a&ubreve;tentikaj produktistoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007033.html">Centra Ameriko: Nepre &gcirc;ia nomo estas
          Meksiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili &ccirc;iam restis sub suspekto de ne<tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/007241.html">Aktoro
          malliberigita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="perfid.0a">perfida</ref>
    <refgrp tip="vid">
      <ref cel="insid.0a">insida</ref>,
      <ref cel="intrig.0i">intrigi</ref>,
      <ref cel="kompro1.0i">kompromiti</ref>,
      <ref cel="korupt.0i">korupti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_l;&c_ja;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="bg">&c_n;&c_je;&c_l;&c_o;&c_ja;&c_l;&c_je;&c_n;</trd>
  <trd lng="br">disleal</trd>
  <trd lng="cs">neloaj&aacute;ln&iacute;</trd>
  <trdgrp lng="de">
    <trd>illoyal</trd>,
    <trd>untreu</trd>,
    <trd>unredlich</trd>,
    <trd>unaufrichtig</trd>,
    <trd>t&uuml;ckisch</trd>,
    <trd>hinterh&auml;ltig</trd>,
    <trd>korrupt</trd>
  </trdgrp>
  <trd lng="en">disloyal</trd>
  <trd lng="es">desleal</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;loyal</trd>,
    <trd>fourbe</trd>,
    <trd>parjure</trd>,
    <trd>perfide</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trouweloos</trd>,
    <trd>ontrouw</trd>,
    <trd>niet <ind>loyaal</ind></trd>
  </trdgrp>
  <trd lng="pl">nielojalny</trd>
  <trd lng="sk">neloj&aacute;lny</trd>
  <trd lng="uk">&#x43D;&c_je;&#x43B;&#x43E;&#x44F;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>
