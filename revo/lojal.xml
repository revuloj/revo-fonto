<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lojal</rad>/a
</kap>
<drv mrk="lojal.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Aganta konforme al la le&gcirc;oj de honoro, sincereco, fideleco,
      dank&scirc;uldo, a&ubreve; al siaj promesoj:
      <ekz>
        <tld/>a regnano, malamiko;
      </ekz>
      <ekz>
        <tld/>a konduto;
      </ekz>
      <ekz>
        mal<tld/>a atako;
      </ekz>
      <ekz>
        &jcirc;uri <tld/>econ al la &scirc;tato.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fidel.0a">fidela</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>loyal</trd>,
    <trd>gesetzestreu</trd>,
    <trd>rechtschaffen</trd>,
    <trd>redlich</trd>,
    <trd>aufrichtig</trd>
  </trdgrp>
  <trd lng="en">loyal</trd>
  <trd lng="es">leal</trd>
  <trd lng="nl">loyaal</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_ja;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">lojal</trd>
</drv>
</art>
