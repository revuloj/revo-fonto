    <trd>trouw</trd>
  </trdgrp>
  <trd lng="pl">lojalny</trd>
  <trd lng="pt">leal</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_ja;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
