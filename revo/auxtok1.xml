<art mrk="$Id: auxtok1.xml,v 1.14 2019/06/30 07:47:08 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>a&ubreve;tokrati</rad>/o
</kap>
<drv mrk="auxtok1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="auxtok.0ismo">A&ubreve;tokratismo</ref>:
      <ekz>
        hodia&ubreve;, dum la mondo &gcirc;enerale turnas sin al demokratio, &scirc;ajnas, ke Britio
        direkti&gcirc;as al <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="https://www.monato.be/2005/008793.php">Iom da proporcio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Autokratie</trd>
  <trd lng="en">autocracy</trd>
  <trd lng="es">autocracia</trd>
  <trd lng="fr">autocratie</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nk&eacute;nyuralom</trd>,
    <trd>autokr&aacute;cia</trd>
  </trdgrp>
  <trd lng="it">autocrazia</trd>
  <trd lng="nl">autocratie</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_v;&c_i;&c_je;</trd>
</drv>
</art>
