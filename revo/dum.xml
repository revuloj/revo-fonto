<subart>
  <dif>
    Vortero kun la senco
    <ref cel="dum.0.prep"><tld/><sncref/></ref>
    a&ubreve; <ref cel="dum.0.konj_kontrast"><sncref/></ref>:
  </dif>

<drv mrk="dum.0a">
  <kap><tld/>a</kap>
  <snc mrk="dum.0a.samtempa">
    <dif>
      <ref tip="dif" cel="temp.sam0a">Samtempa</ref>:
      <ekz>
        legado povas esti same bona a&ubreve; same sensenca, kiel rigardado al la okcidentaj
        montoj kaj <tld/>a revado pri &ccirc;io kaj nenio
        <fnt>
          <aut>S. Engholm</aut>
          <vrk>Al Torento</vrk>
          <lok>1930</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
    <trd lng="fr">concomitant</trd>
    <trd lng="hu">egyidej&udblac;</trd>
    <trd lng="pt">simult&acirc;neo<klr tip="ind"> (adj.)</klr></trd>
  </snc>
  <snc mrk="dum.0a.provizora">
    <dif>
      Provizora, intertempa, &gcirc;isa.
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>
    <trdgrp lng="hu">
      <trd>ideiglenes</trd>,
      <trd>&aacute;tmeneti</trd>
    </trdgrp>
    <trd lng="pl">tymczasowy</trd>
    <trd lng="pt">provis&oacute;rio<klr tip="ind"> (adj.)</klr></trd>
  </snc>
  <trd lng="de">gleichzeitiger</trd>
  <trdgrp lng="en">
    <trd>concurrent</trd>,
    <trd>simultaneous</trd>,
    <trd>parallel</trd>
  </trdgrp>
  <trd lng="he">&zayin;&mem;&nun;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#x540C;&#x6642;&#x306E;  <pr>&#x3069;&#x3046;&#x3058;&#x306E;</pr></trd>,
    <trd>&#x4E00;&#x6642;&#x7684;&#x306A;  <pr>&#x3044;&#x3061;&#x3058;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_i;&c_m;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_d;&c_n;&c_o;&c_ch;&c_a;&c_s;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x540C;&#x65F6;&#x671F; <pr>t&#xF3;ngsh&#xED;q&#x12B;</pr></trd>,
    <trd>&#x76F8;&#x4F34; <pr>xi&#x101;ngb&#xE0;n</pr></trd>,
    <trd>&#x7B49;&#x65F6;&#x7EBF; <pr>d&#x11B;ngsh&#xED;xi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dum.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc mrk="dum.0e.kiam">
    <dif>
      <tld lit="D"/> tiu tempo,
      <ref tip="vid" cel="temp.inter0e.KOMUNE">intertempe</ref>,
      <ref tip="vid" cel="dum.inter0e.KOMUNE">inter<tld/>e</ref>:
      <ekz>
        ili sin pretigis, ni <tld/>e babilis;
      </ekz>
      <ekz>
        apena&ubreve; li eldiris la deziron, ekefikis la galo&scirc;oj, kiujn li
        <tld/>e estis metinta sur sin
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li devis mueli en la malliberejo, <tld/>e la haroj de lia kapo komencis rekreski
        post la fortondo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i devis pro alia laboro eliri el la kuirejo, <tld/>e la
        infanoj <klr>[&#x2026;]</klr> enkuris
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi rakontas tiel terure malrapide <klr>[&#x2026;]</klr> la ovoj povas
        <tld/>e malvarmi&gcirc;i
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_y;&c_m; &c_ch;&c_a;&c_s;&c_a;&c_m;</trd>
    <trdgrp lng="de">
      <trd>w&auml;hrenddessen</trd>,
      <trd>unterdessen</trd>,
      <trd>derweilen</trd>,
      <trd>derweil</trd>
    </trdgrp>
    <trd lng="en">meanwhile</trd>
    <trdgrp lng="fr">
      <trd>cependant<klr tip="ind"> (pendant ce temps)</klr></trd>,
      <trd>entre temps</trd>,
      <trd>pendant ce temps</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ek&ouml;zben</trd>,
      <trd>ezalatt</trd>,
      <trd>ak&ouml;zben</trd>,
      <trd>azalatt</trd>
    </trdgrp>
    <trd lng="pt">simultaneamente<klr tip="ind"> (adv.)</klr></trd>
    <trd lng="ru">&c_t;&c_je;&c_m;
    &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_je;&c_m;</trd>
  </snc>
  <snc mrk="dum.0e.provizore">
    <dif>
      Provizore, &gcirc;ise:
      <ekz>
        lasante <tld/>e flanke la demandon pri tio <klr>[&#x2026;]</klr>
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[estas pli prudente]</klr> akcepti <tld/>e la lingvon
        Esperanto en &gcirc;ia
        nuna formo
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_o;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_u;&c_l;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zun&auml;chst</trd>,
      <trd>erstmal</trd>,
      <trd>vorerst</trd>
    </trdgrp>
    <trd lng="en">for now</trd>
    <trd lng="fr">pour l'<ind>instant</ind></trd>
    <trd lng="hu">egyel&odblac;re</trd>
    <trdgrp lng="pl">
      <trd>na razie</trd>,
      <trd>tymczasem</trd>
    </trdgrp>
    <trd lng="pt">provisoriamente<klr tip="ind"> (adv.)</klr></trd>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="dum.0e.kontraste">
    <dif>
      Kontraste kun tio; <ref tip="sin" cel="kontraux.0e.opone">kontra&ubreve;e</ref>,
      <ref tip="sin" cel="malgra.0e">malgra&ubreve;e</ref>,
      <ref tip="sin" cel="tamen.0">tamen</ref>:
      <ekz>
        li kvaza&ubreve; ankora&ubreve; unu fojon travivis la feli&ccirc;an &gcirc;ojan tempon, &dash; kaj <tld/>e &scirc;i
        ku&scirc;is tie senviva en la apuda &ccirc;ambro
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>;
      </ekz>
      <ekz>
        principoj kaj postuloj pu&scirc;is kaj malhelpis unu la alian,
        <tld/>e en la teorio <klr>[&#x2026;]</klr> ili &scirc;ajnis al mi tute
        bonaj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e lingvo arta, anstata&ubreve; tiu &hcirc;aoso, donas
        al vi sole nur ses vortetojn
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
    <rim>
      Estas mal&gcirc;usta kaj nepre evitinda la uzado de la adverbo
      <em>dume</em> en la senco de la konjunkcio <em>dum</em>.
    </rim>
    <adm>
      La rimarko estas kripta. &Gcirc;i ver&scirc;ajne rilatas al la
      unua ekz-o, en kiu eblus kompreni "~e" kiel konjunkcion. Tio
      meritas
      pliajn klarigojn. <aut>MB</aut>
    </adm>
    <trd lng="be">&c_t;&c_a;&c_d;&c_y; &c_ja;&c_k;</trd>
    <trdgrp lng="de">
      <trd>w&auml;hrend</trd>,
      <trd>hingegen</trd>,
      <trd>unterdessen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>whereas</trd>,
      <trd>while</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cependant<klr tip="ind"> (toutefois)</klr></trd>,
      <trd>en <ind>revanche</ind></trd>,
      <trd>pourtant</trd>,
      <trd>toutefois</trd>
    </trdgrp>
    <trd lng="hu">ellenben</trd>
    <trd lng="ru">&c_t;&c_o;&c_g;&c_d;&c_a; &c_k;&c_a;&c_k;</trd>
  </snc>
  <trd lng="bo">&t_da;&t_drengbu;&t_-a;&t_gigu;&t_tsheg;&t_da;&t_shabchu;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
      <trd>zat&iacute;m</trd>,
      <trd>mezit&iacute;m</trd>
    </trdgrp>
    <trd lng="es">mientras</trd>
    <trd lng="he">&bet;&yod;&nun;&tav;&yod;&yod;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#x305D;&#x306E;&#x9593;  <pr>&#x305D;&#x306E;&#x3042;&#x3044;&#x3060;</pr></trd>,
    <trd>&#x4E00;&#x6642;&#x7684;&#x306B;  <pr>&#x3044;&#x3061;&#x3058;&#x3066;&#x304D;&#x306B;</pr></trd>,
    <trd>&#x3055;&#x3057;&#x3042;&#x305F;&#x308A;</trd>,
    <trd>&#x3068;&#x3053;&#x308D;&#x304C;&#x4E00;&#x65B9;  <pr>&#x3068;&#x3053;&#x308D;&#x304C;&#x3044;&#x3063;&#x307D;&#x3046;</pr></trd>,
    <trd>&#x53CD;&#x5BFE;&#x306B;  <pr>&#x306F;&#x3093;&#x305F;&#x3044;&#x306B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_i;&c_m; &c_ch;&c_a;&c_s;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_i; &c_shch;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_a;&c_z;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="dum.inter0e">
  <kap>inter<tld/>e</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dum.inter0e.KOMUNE">
    <dif>
      <ref tip="dif" cel="temp.inter0e.KOMUNE">Intertempe</ref>:
      <ekz>
        inter<tld/>e pli praktike mi konstatas, ke <klr>[&#x2026;]</klr>
        <fnt>
          <aut>R. Schwartz</aut>
          <vrk>Printempa Varianto</vrk>
          <lok>en: <url ref="&Steloj;butiko">La Stranga
          Butiko</url>, 1931</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>e Gab&ccirc;jo nerimarkeble malfermis sian historilibron
        kaj duonokule kun granda plezuro rigardadas
        <fnt>
          <aut>F. Karinthy, trad. J. Farkas</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr23/instruas_filetonu.html">
          Mi instruas mian fileton</url></vrk>,
          <lok>Nica Literatura Revuo, 1957-01 &gcirc;is 02, numero 2/3, p.
          83a-86a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_m; <ind>&c_ch;&c_a;&c_s;&c_a;&c_m;</ind></trd>,
    <trd>&c_m;&c_ib;&c_zh; &c_t;&c_y;&c_m;</trd>
  </trdgrp>
  <trd lng="de">zwischenzeitlich</trd>
  <trd lng="en">meanwhile</trd>
  <trd lng="fr">cependant<klr tip="ind"> (pendant ce
  temps)</klr></trd>
  <trd lng="zh">&#x4E2D;&#x9593;&#x9AD4; <pr>zh&#x14D;ngji&#xE0;nt&#x1D0;</pr></trd>
</drv>

<drv mrk="dum.0ke">
<kap><tld/> ke,
<var><kap><tld/> kiam</kap></var></kap>
<gra><vspec>konjunkcio</vspec></gra>
  <snc mrk="dum.0ke.konj_temp">
    <dif>
      <ref tip="dif" cel="dum.0.konj_temp">Dum<sncref/></ref>:
      <ekz>
        ni estis liberaj por promenadi, na&gcirc;i, sunbruni&gcirc;i kaj &gcirc;ui la &ccirc;irka&ubreve;a&jcirc;on
        diversmaniere <tld/> ke oni interkonati&gcirc;is
        <fnt>
          <aut>I. Mac Dowall</aut>
          <vrk><url ref="https://legacy.esperanto.org.uk/lbe/arkivo/944/01.html">
          Vizito al la Jubilea 20-a &Ccirc;ebalta Esperantista Printempo</url></vrk>
          <lok>La Brita Esperantisto, 1998-09 &gcirc;is 10</lok>
        </fnt>.
      </ekz>
   </dif>
  <trd lng="en">while</trd>
  </snc>
  <snc mrk="dum.0ke.konj_kontrast">
    <dif>
      <ref tip="dif" cel="dum.0.konj_kontrast">Dum<sncref/></ref>:
      <ekz>
        <tld/> kiam sur literatura kampo la unuaj esperantistoj praktikis la lingvon iom
        hezite inter la pli malpli fiksitaj limoj gramatikaj, <nom>Grabowski</nom> estis la
        unua, kiu donis fortan pu&scirc;on al plejeble vasta eluzado de la ebloj, kiujn
        prezentas la Esperanta gramatiko
        <fnt>
          <aut>G. J. Degenkamp</aut>
          <vrk>Esperanto 60-jara</vrk>
          <lok>1947</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Britujo studento legas, verkas, diskutas kaj pensas en sia
        unu gepatra lingvo, <tld/> ke en Danlando li devus eble legi en
        unu lingvo, verki en alia, kaj pensi iom en amba&ubreve;
        <fnt>
          <aut>V. Sadler</aut>
          <vrk><url ref="&UEA;informado/arkivo/63/ses.html">
          Azia Strategio</url></vrk>
          <bib>Esperanto</bib>
          <lok>1963-01, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">hingegen</trd>
  <trdgrp lng="en">
    <trd>while</trd>,
    <trd>whereas</trd>
  </trdgrp>
  </snc>
  <rim mrk="dum.0ke.RIM_GRAM">
    Same kiel <ref tip="vid" cel="gxis.0ke">&gcirc;is ke</ref>
    <em>dum ke</em> estas rigardata de pluraj gramatikistoj
    erara, &ccirc;ar tradicie <em>dum</em> havas
    amba&ubreve; funkciojn de dependiga konjunkcio kaj de prepozicio.
    Sistema uzo de <em>dum ke</em> en la dependiga funkcio
    la&ubreve; la modelo <em>anstata&ubreve; ke</em>, <em>sen ke</em> ktp.
    implicas ke oni agnoskas &gcirc;in nur prepozicio.
    <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_l;&c_mol;</trd>,
    <trd>&c_u; &c_t;&c_o;&c_j; &c_ch;&c_a;&c_s;, &c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="de">w&auml;hrend</trd>
  <trd lng="fr">cependant que</trd>
</drv>

</subart>

<subart>
  <dif>
    Prefikso kun la senco <ref cel="dum.0.prep"><tld/><sncref/></ref>:
  </dif>
  <snc mrk="dum.0.pref">
      <ekz>
        <tld/>voje
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:42</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>voja&gcirc;e
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 16:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>dorme
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="temp.dum0a"><tld/>tempe</ref> &scirc;i reakiris sian ekvilibron
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>kure li sursaltis &scirc;tupareton de tramo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        bruligi, kion vi adoris, signifas morti jam <tld/>vive
        <fnt>
          <bib>LSB</bib>
          <lok>Anta&ubreve;signoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        pasigante la <tld/>noktajn horojn sub granda ponto
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>noktaj vizitantoj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vespere post la <tld/>taga laboro
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="viv.dum0a"><tld/>viva</ref> rento da dek mil frankoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>hora mar&scirc;ado trans montojn kaj mar&ccirc;ojn
        <fnt>
          <bib>Tor</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>momente li staris hezite
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konversacioj <tld/>sekunde haltis
        <fnt>
          <bib>ChB</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne distingas halucinojn, kiajn ni &ccirc;iuj havas <tld/>son&gcirc;e, disde la realo
        <fnt>
          <bib>ChV</bib>
          <lok>1</lok>
        </fnt>.
      </ekz>
  <trd lng="en">during</trd>
  </snc>
  <trd lng="de"><klr>-</klr>lang</trd>
</subart>
