<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rabat.xml,v 1.33 2025/10/11 16:00:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rabat</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="rabat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Malpliigo de <ref cel="prez.0o">prezo</ref>, konsentita al a&ccirc;etanto:
      <ekz>
        peti, ricevi, &gcirc;ui <tld/>on;
      </ekz>
      <ekz>
        vendisto, kiu postulas tri fojojn pli multe, por ke li havu, de kio fari <tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio, kion vi petas valoras tridek mil frankojn, sen <tld/>o
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li turnas sin al libreja firmo kun la demando, &ccirc;u &gcirc;i ne donus <tld/>on, se &gcirc;i
        por tio &ccirc;i ricevos la garantion, ke &gcirc;i disvendos la donitajn librojn en la
        nombro de 20-30 miloj da ekzempleroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la magazeno komencos grandan <tld/>vendadon
        <klr>(kp <ref cel="vend.for0i.tute">forvendi</ref>)</klr>
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la registaro pliigis <ind>impost-<tld/>o</ind>jn por tiuj, kiuj ricevas
        malaltajn salajrojn
        <fnt><aut>D. E. Rogers</aut>: <vrk>Kolapsas
        koalicio</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 12a, p.
        8a</lok></fnt>.
        <trdgrp lng="de">
          <trd>Steuerverg&uuml;nstigung</trd>,
          <trd>Nachlass <klr>(Steuer)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pag.de0o">depago</ref>,
      <ref cel="diskon.0o.EKON_elcento">diskonto</ref>
    </refgrp>
  </snc>
  <ref tip="hom" cel="rabat1.0o">Rabato (&ccirc;efurbo)</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_ib;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#3906;&#3964;&#3908;&#3851;&#3926;&#3909;&#3906;&#3851;&#3924;&#3851;</trd>
  <trdgrp lng="ca">
    <trd>rebaixa</trd>,
    <trd>descompte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mno&zcaron;stevn&iacute; sleva</trd>,
    <trd>rabat</trd>,
    <trd>sleva</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rabatt</trd>,
    <trd>Preisnachlass</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>discount</trd>,
    <trd>rebate</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rebaja</trd>,
    <trd>descuento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rabais</trd>,
    <trd>remise<klr> (rabais)</klr></trd>,
    <trd>solde</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rengedm&eacute;ny</trd>
  <trdgrp lng="ja">
    <trd>&#21106;&#24341;  <pr>&#12431;&#12426;&#12403;&#12365;</pr></trd>,
    <trd>&#20516;&#24341;  <pr>&#12397;&#12403;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="nl">korting</trd>
  <trdgrp lng="pl">
    <trd>rabat</trd>,
    <trd>zni&zdot;ka</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>desconto</trd>,
    <trd>promo&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>rabat</trd>,
    <trd>reducere</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_i;&c_d;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>rabat</trd>,
    <trd>z&lcaron;ava</trd>
  </trdgrp>
  <trd lng="sv">rabatt <klr>(prisneds&auml;ttning)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_k;&c_a; &c_v; &c_c;&c_ib;&c_n;&c_ib;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25240;&#25187; <pr>zh&eacute;k&ograve;u</pr></trd>,
    <trd>&#25171;&#25240; <pr>d&#462;zh&eacute;</pr></trd>,
    <trd>&#25240; <pr>zh&eacute;</pr></trd>,
    <trd>&#38477;&#20215; <pr>ji&agrave;ngji&agrave;</pr></trd>,
    <trd>&#38477;&#20729; <pr>ji&agrave;ngji&agrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rabat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Konsenti <tld/>on de:
      <ekz>
        maljuna avino <klr>[&#8230;]</klr> petis <tld/>i dek kopekojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        por la specialaj verkoj la membroj devas anka&ubreve; elspezi 6-8 dolarojn &ccirc;iujare, sed
        tiujn &ccirc;i ili ricevas anka&ubreve; por <tld/>ita kosto
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_s;&c_k;&c_ib;&c_d;&c_k;&c_u;</ind></trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_z;&c_mol;&c_n;&c_ib;&c_zh;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="cs">slevit</trd>
  <trdgrp lng="de">
    <trd>rabattieren</trd>,
    <trd>Preis <ind>nachlassen</ind></trd>,
    <trd>Preisnachlass <ind>geben</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(give a) <ind>discount</ind></trd>
  </trdgrp>
  <trd lng="es">rebajar</trd>
  <trd lng="fr">faire une <ind>remise</ind></trd>
  <trd lng="hu">&aacute;rengedm&eacute;nyt ad</trd>
  <trdgrp lng="ja">
    <trd>&#21106;&#24341;&#12377;&#12427;  <pr>&#12431;&#12426;&#12403;&#12365;&#12377;&#12427;</pr></trd>,
    <trd>&#20516;&#24341;&#12365;&#12377;&#12427;  <pr>&#12397;&#12403;&#12365;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl"><ind>korting</ind> geven</trd>
  <trdgrp lng="pl">
    <trd>udziela&cacute; rabatu</trd>,
    <trd>udziela&cacute; zni&zdot;ki</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>baratear</trd>,
    <trd>descontar</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_s;&c_k;&c_i;&c_d;&c_k;&c_u;</trd>
  <trd lng="sk">z&lcaron;avi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_z;&c_n;&c_i;&c_zh;&c_k;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38477;&#20215; <pr>ji&agrave;ngji&agrave;</pr></trd>,
    <trd>&#38477;&#20729; <pr>ji&agrave;ngji&agrave;</pr></trd>,
    <trd>&#20943;&#20215; <pr>ji&#462;nji&agrave;</pr></trd>,
    <trd>&#28187;&#20729; <pr>ji&#462;nji&agrave;</pr></trd>,
    <trd>&#25171;&#25240; <pr>d&#462;zh&eacute;</pr></trd>
  </trdgrp>
</drv>
</art>
