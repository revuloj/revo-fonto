<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rabat</rad>/o
</kap>
<drv mrk="rabat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malpliigo de prezo, konsentita al a&ccirc;etanto:
      <ekz>
        peti, ricevi, &gcirc;ui <tld/>on;
      </ekz>
      <ekz>
        sen<tld/>e <klr>(la&ubreve; neta prezo)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="diskon.0o">diskonto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Rabatt</trd>,
    <trd>Preisnachlass</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rabais</trd>,
    <trd>remise</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rengedm&eacute;ny</trd>
  <trd lng="cs">sleva</trd>
  <trd lng="nl">korting</trd>
  <trd lng="ru">&c_s;&c_k;&c_i;&c_d;&c_k;&c_a;</trd>
  <trd lng="sv">rabatt <klr>(prisneds&auml;ttning)</klr></trd>
</drv>

<drv mrk="rabat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Konsenti <tld/>on de:
      <ekz>
        mi povas <tld/>i nur unu dolaron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>rabattieren</trd>,
    <trd>Preis nachlassen</trd>
  </trdgrp>
  <trd lng="fr">faire une remise</trd>
  <trd lng="hu">&aacute;rengedm&eacute;nyt ad</trd>
  <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_s;&c_k;&c_i;&c_d;&c_k;&c_u;</trd>
</drv>
</art>
