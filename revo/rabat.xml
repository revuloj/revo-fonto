<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rabat.xml,v 1.31 2024/04/13 20:15:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rabat</rad>/o
</kap>
<drv mrk="rabat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Malpliigo de <ref cel="prez.0o">prezo</ref>, konsentita al a&ccirc;etanto:
      <ekz>
        peti, ricevi, &gcirc;ui <tld/>on;
      </ekz>
      <ekz>
        vendisto, kiu postulas tri fojojn pli multe, por ke li havu, de kio fari <tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio, kion vi petas valoras tridek mil frankojn, sen <tld/>o
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li turnas sin al libreja firmo kun la demando, &ccirc;u &gcirc;i ne donus <tld/>on, se &gcirc;i
        por tio &ccirc;i ricevos la garantion, ke &gcirc;i disvendos la donitajn librojn en la
        nombro de 20-30 miloj da ekzempleroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la magazeno komencos grandan <tld/>vendadon
        <klr>(kp <ref cel="vend.for0i.tute">forvendi</ref>)</klr>
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la registaro pliigis <ind>impost-<tld/>o</ind>jn por tiuj, kiuj ricevas
        malaltajn salajrojn
        <fnt><aut>D. E. Rogers</aut>: <vrk>Kolapsas
        koalicio</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 12a, p.
        8a</lok></fnt>.
        <trdgrp lng="de">
          <trd>Steuerverg&uuml;nstigung</trd>,
          <trd>Nachlass <klr>(Steuer)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pag.de0o">depago</ref>,
      <ref cel="diskon.0o.EKON_elcento">diskonto</ref>
    </refgrp>
  </snc>
  <ref tip="hom" cel="rabat1.0o">Rabato (&ccirc;efurbo)</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_ib;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF7C;&#xF44;&#xF0B;&#xF56;&#xF45;&#xF42;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>rebaixa</trd>,
    <trd>descompte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mno&zcaron;stevn&iacute; sleva</trd>,
    <trd>rabat</trd>,
    <trd>sleva</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rabatt</trd>,
    <trd>Preisnachlass</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>discount</trd>,
    <trd>rebate</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rebaja</trd>,
    <trd>descuento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rabais</trd>,
    <trd>remise<klr> (rabais)</klr></trd>,
    <trd>solde</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rengedm&eacute;ny</trd>
  <trdgrp lng="ja">
    <trd>&#x5272;&#x5F15;  <pr>&#x308F;&#x308A;&#x3073;&#x304D;</pr></trd>,
    <trd>&#x5024;&#x5F15;  <pr>&#x306D;&#x3073;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">korting</trd>
  <trdgrp lng="pl">
    <trd>rabat</trd>,
    <trd>zni&zdot;ka</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>desconto</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>rabat</trd>,
    <trd>reducere</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_i;&c_d;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>rabat</trd>,
    <trd>z&lcaron;ava</trd>
  </trdgrp>
  <trd lng="sv">rabatt <klr>(prisneds&auml;ttning)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43D;&#x438;&#x436;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x43E;&#x441;&#x442;&#x443;&#x43F;&#x43A;&#x430; &#x432; &#x446;&#x456;&#x43D;&#x456;</trd>,
    <trd>&#x440;&#x430;&#x431;&#x430;&#x442;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6298;&#x6263; <pr>zh&#xE9;k&#xF2;u</pr></trd>,
    <trd>&#x6253;&#x6298; <pr>d&#x1CE;zh&#xE9;</pr></trd>,
    <trd>&#x6298; <pr>zh&#xE9;</pr></trd>,
    <trd>&#x964D;&#x4EF7; <pr>ji&#xE0;ngji&#xE0;</pr></trd>,
    <trd>&#x964D;&#x50F9; <pr>ji&#xE0;ngji&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rabat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Konsenti <tld/>on de:
      <ekz>
        maljuna avino <klr>[&#x2026;]</klr> petis <tld/>i dek kopekojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        por la specialaj verkoj la membroj devas anka&ubreve; elspezi 6-8 dolarojn &ccirc;iujare, sed
        tiujn &ccirc;i ili ricevas anka&ubreve; por <tld/>ita kosto
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_s;&c_k;&c_ib;&c_d;&c_k;&c_u;</ind></trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_z;&c_mol;&c_n;&c_ib;&c_zh;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="cs">slevit</trd>
  <trdgrp lng="de">
    <trd>rabattieren</trd>,
    <trd>Preis <ind>nachlassen</ind></trd>,
    <trd>Preisnachlass <ind>geben</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(give a) <ind>discount</ind></trd>
  </trdgrp>
  <trd lng="es">rebajar</trd>
  <trd lng="fr">faire une <ind>remise</ind></trd>
  <trd lng="hu">&aacute;rengedm&eacute;nyt ad</trd>
  <trdgrp lng="ja">
    <trd>&#x5272;&#x5F15;&#x3059;&#x308B;  <pr>&#x308F;&#x308A;&#x3073;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5024;&#x5F15;&#x304D;&#x3059;&#x308B;  <pr>&#x306D;&#x3073;&#x304D;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl"><ind>korting</ind> geven</trd>
  <trdgrp lng="pl">
    <trd>udziela&cacute; rabatu</trd>,
    <trd>udziela&cacute; zni&zdot;ki</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_s;&c_k;&c_i;&c_d;&c_k;&c_u;</trd>
  <trd lng="sk">z&lcaron;avi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x437;&#x43D;&#x438;&#x436;&#x43A;&#x443;</trd>,
    <trd>&#x43F;&#x43E;&#x441;&#x442;&#x443;&#x43F;&#x43A;&#x443;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x964D;&#x4EF7; <pr>ji&#xE0;ngji&#xE0;</pr></trd>,
    <trd>&#x964D;&#x50F9; <pr>ji&#xE0;ngji&#xE0;</pr></trd>,
    <trd>&#x51CF;&#x4EF7; <pr>ji&#x1CE;nji&#xE0;</pr></trd>,
    <trd>&#x6E1B;&#x50F9; <pr>ji&#x1CE;nji&#xE0;</pr></trd>,
    <trd>&#x6253;&#x6298; <pr>d&#x1CE;zh&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
