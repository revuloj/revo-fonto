<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&Hcirc;aron</rad>/o
</kap>

<drv mrk="hxaron.0o">
  <kap><tld/>o</kap>
  <snc mrk="hxaron.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <uzo tip="fak">BELE</uzo>
    <dif>
      En la helena kaj romia mito, ru&gcirc;okula maljunulo,
      kiu transveturigas la mortintojn per sia barko.
    </dif>
    <trd lng="ca">Caront (mit.)</trd>
  </snc>
  <snc mrk="hxaron.0o.AST">
    <fnt><url ref="http://at.komputilo.org/">Astronomia
    Terminaro</url></fnt>
    <uzo tip="fak">AST</uzo>
      <dif>
        <ref tip="vid" cel="sateli.0o.planedo">Satelito</ref> de 
        <ref tip="vid" cel="pluton.0o.AST">Plutono</ref>.
      </dif>
      <trd lng="ca">Caront (astr.)</trd>
    </snc>
  <trd lng="be">&c_H;&c_a;&c_r;&c_o;&c_n;</trd>
  <trd lng="it">Caronte</trd>
  <trd lng="en">Charon</trd>
  <trd lng="es">Caronte</trd>
  <trd lng="fa">&a_kh;&a_A;&a_r;&a_w;&a_n;</trd>
  <trd lng="fr">Charon</trd>
  <trd lng="grc">&Chi;&alfa_ton;&rho;&omega;&ny;</trd>
  <trd lng="nl">Charon</trd>
  <trd lng="pl">Charon</trd>
  <trd lng="ru">&c_H;&c_a;&c_r;&c_o;&c_n;</trd>
  <trd lng="sr">&c_H;&c_a;&c_r;&c_o;&c_n;</trd>
  <trd lng="uk">&c_H;&c_a;&c_r;&c_o;&c_n;</trd>
</drv>
</art>

