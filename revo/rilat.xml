    <trd>&#x4EA4;&#x6D41;  <pr>&#12371;&#12358;&#x308A;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">stosunek  <klr>(wzajemny)</klr></trd>
