<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vizit</rad>/i
</kap>
<drv mrk="vizit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Iri por vidi iun hejme:
      <ekz>
        anta&ubreve; tri tagoj mi <tld/>is vian kuzon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        veni <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la kuracisto <tld/>as siajn klientojn.
      </ekz>
    </dif>
    <trd lng="de">besuchen</trd>
    <trdgrp lng="fr">
      <trd>rendre <ind>visite</ind></trd>
    </trdgrp>
    <trd lng="pt">visitar</trd>
    <trd lng="ru">&c_n;&c_a;&c_n;&c_je;&c_s;&c_t;&c_i; <ind>&c_v;&c_i;&c_z;&c_i;&c_t;</ind></trd>
    <trd lng="sv">bes&ouml;ka</trd>
  </snc>
  <snc>
    <dif>
      Iri por vidi ion:
      <ekz>
        <tld/>i urbon, muzeon;
      </ekz>
      <ekz>
        la konferenco estis bone <tld/>ita;
      </ekz>
      <ekz>
        teatramanto ofte <tld/>as la teatron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>besuchen</trd>,
      <trd>besichtigen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>visiter</trd>
    </trdgrp>
    <trd lng="pt">visitar</trd>
    <trd lng="sv">bes&ouml;ka</trd>
  </snc>
  <snc>
    <dif>
      Iri regule ien:
      <ekz>
        <tld/>i kurson, klinikon<fnt>B</fnt>,
      </ekz>
      <ekz>
        lernejon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>adi gimnazion<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">besuchen</trd>
    <trd lng="pt">frequentar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_ib;&c_z;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">nav&scaron;t&iacute;vit</trd>
  <trd lng="el">&epsilon;&pi;&jota;&sigma;&kappa;&epsilon_ton;&pi;&tau;&omikron;&my;&alfa;&jota;</trd>
  <trd lng="en">visit</trd>
  <trd lng="es">visitar</trd>
  <trdgrp lng="fr">
    <trd>fr&eacute;quenter</trd>
  </trdgrp>
  <trd lng="he">&lamed;&bet;&qof;&resh;</trd>
  <trd lng="nl">bezoeken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">nav&scaron;t&iacute;vi&tcaron;</trd>
</drv>

<drv mrk="vizit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        mia <tld/>o faris al li grandan plezuron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mizero faras <tld/>on, ne atendante inviton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        peti de kuracisto la konton de liaj <tld/>oj;
      </ekz>
      <ekz>
        kondolenca <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_ib;&c_z;&c_ib;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Besuch</trd>
  <trd lng="el">&epsilon;&pi;&jota_ton;&sigma;&kappa;&epsilon;&psi;&eta;</trd>
  <trd lng="es">visita</trd>
  <trdgrp lng="fr">
    <trd>visite</trd>
  </trdgrp>
  <trd lng="he">&bet;&yod;&qof;&vav;&resh;</trd>
  <trd lng="nl">bezoek</trd>
  <trd lng="pt">visita</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_z;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">bes&ouml;k</trd>
</drv>
</art>
