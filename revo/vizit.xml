  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_d;&c_v;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_sh;&c_k;&c_o;&c_l;&c_u; &c_ib; &c_t;.&c_p;.)</trd>
