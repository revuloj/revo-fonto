<drv mrk="vizit.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="vizit.0inda.TUR">
    <dif>
      Tre vidinda, kurioza, impresa a&ubreve; interesa:
      <ekz>
        <tld/>indaj napolaj &ccirc;irka&ubreve;a&jcirc;oj: Pompejo kaj Herkulano, Vezuvio,
        kelkaj konataj
        insuloj kaj urboj
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2008/009566.php">Malkovru Napolon!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        Svislando estas lando bela, kiu estas <tld/>inda
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="https://tatoeba.org/eng/sentences/show/1954375">
          Frazo 1954375a</url></vrk>,
          <lok>Tatoeba, 2012-10-26</lok>
        </fnt>
      </ekz>
      <ekz>
        <tld/>indas deko da cisterciaj kaj benediktaj abatejoj, kiujn la
        historio
        postlasis
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_t;&c_y; &c_n;&c_a;&c_v;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="fr">curieux<klr> (qui m&eacute;rite une visite)</klr></trd>
  <trd lng="id"><ind>patut</ind> dikunjungi</trd>
  <trd lng="pl">wart<ind> odwiedzenia</ind></trd>
</drv>

<drv mrk="vizit.fantom0i">
  <kap>
    fantom<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="V"/>i kiel <ref tip="sin" cel="fantom.0i">fantomo</ref>; <ref tip="sin" cel="hant.0i">hanti</ref>:
      <ekz>
        neniu volas &gcirc;in lo&gcirc;i, &ccirc;ar <klr>[&#8230;]</klr> &gcirc;i estas fantom<tld/>ata
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi eble havas kontakton kun la Ministerio de Fantom<tld/>aj Aferoj
        <fnt>
          <bib>SatirRak</bib>
          <lok>La fluidumo de la &ccirc;iovido</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="de">heimsuchen <klr>(spuken)</klr></trd>
</drv>

<drv mrk="vizit.inter0i">
  <kap>
    inter<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="V"/>i sin reciproke:
      <ekz>
        la du familioj <klr>[&#8230;]</klr> sin inter<tld/>is
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[oni]</klr> aran&gcirc;os inter<tld/>ojn de dividitaj familianoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Besuche <ind>austauschen</ind></trd>
  </snc>
</drv>

<drv mrk="vizit.kontrol0i">
  <kap>
    kontrol<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="V"/>i por kontroli:
      <ekz>
        &ccirc;u estas rekomendinde, ke oni havu <klr>[&#8230;]</klr> sian fid-kardiologon? kaj kiom
        ofte oni devus fiksi fakan kontrol-<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk>Atentu la koron</vrk>
          <lok>2013</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inspek.0i">inspekti</ref>,
      <ref cel="revizi.0i.dogane">revizii<sncref/></ref>,
      <ref cel="rund.0i">rundi</ref>
    </refgrp>
  </snc>
  <trd lng="de">einen <ind>Kontrollbesuch</ind> abstatten</trd>
</drv>

<drv mrk="vizit.labor0o">
  <kap>
    labor<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="V"/>o kadre de sia laboro:
      <ekz>
        la &gcirc;enerala direktoro de UEA, <nom>Osmo Buller</nom>, faris labor<tld/>on al la Pariza
        sidejo de Unesko
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>;
      </ekz>
      <ekz>
        Zagreba vicurbestro <nom>Mladen Vilfan</nom> akceptis Konstantan Kongresan Sekretarion
        <nom>Nikola Ra&scirc;i&ccirc;</nom> okaze de ties labor<tld/>o al Zagrebo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Arbeitsbesuch</trd>
</drv>

<drv mrk="vizit.multe0i">
  <kap>
    mult<tld/>i, <var><kap>amas<tld/>i</kap></var>, <var><kap>multe<tld/>i</kap></var>
  </kap>
  <snc>
    <dif>
      Multnombre <tld/>i:
      <ekz>
        urbo multe<tld/>ata
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 62:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la litovoj amas<tld/>as siajn pre&gcirc;ejojn nur dum la tago de la mortintoj,
        Kristnasko kaj Pasko
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2009/009650.php"
              >&Ccirc;u vera katoliko?</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la placo]</klr> ornamita de multaj florbedoj, altaj arboj kaj dudeko da
        benkoj por la promenantoj, estas &ccirc;iam mult<tld/>ata, precipe de
        infanvartistoj kun infanoj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frekve2.0i">frekventi</ref>,
      <ref cel="vizit.0adi">vizitadi</ref>
    </refgrp>
  </snc>
  <trd lng="de">frequentieren</trd>
</drv>

<drv mrk="vizit.pet0i">
  <kap>
    pet<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="V"/>i por peti:
      <ekz>
