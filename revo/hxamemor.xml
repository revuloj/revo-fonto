<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&hcirc;amemor</rad>/o
  <fnt><vrk>Norsk-esperanto ordbok</vrk>, <lok>1963</lok></fnt>
</kap>
<drv mrk="hxamemor.0o">
  <kap><tld/>o</kap>
  <snc mrk="hxamemor.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara duonligneca vegeta&jcirc;o kreskanta
      precipe sur la norda duonglobo, en la muskaj mar&ccirc;oj de 
      lokoj tundraj kaj arbaraj, kun man&gcirc;indaj beroj
      el la genro <ref tip="super" cel="rubus.0o.genro">rubuso</ref>
      (<trd lng="la">Rubus chameaemorus</trd>).
    </dif>
  </snc>
  <snc mrk="hxamemor.0o.bero">
    <dif>
      La <ref cel="ber.0o.KOMUNE">bero</ref> de
      <tld/>o<sncref ref="hxamemor.0o.BOT"/>:
      <ekz>
        memoru anka&ubreve; la rostitan boaca&jcirc;on kaj la orflavajn
        <tld/>ojn kun &scirc;a&ubreve;mkremo
        <fnt>
          <aut>A. Lindh</aut>
          <vrk><url ref="http://www.student.uib.no/~st01490/esperanto/sezono.eo.html">
          Sezono dum la tuta jaro</url></vrk>,
          <lok>2002-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cloudberry</trd>
  <trdgrp lng="fr">
    <trd>chicout&eacute;e</trd>,
    <trd>m&ucirc;re arctique</trd>
  </trdgrp>
  <trd lng="no">molte</trd>
  <trd lng="pl"><klr>malina </klr>moroszka</trd>
  <trd lng="pt">framboesa</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sv">hjortron</trd>
</drv>
</art>
