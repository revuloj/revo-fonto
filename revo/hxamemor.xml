<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>&hcirc;amemor</rad>/o
      <fnt><vrk>Norsk-esperanto ordbok</vrk>, <lok>1963</lok></fnt>
    </kap>

    <drv mrk="hxamemor.0o">
      <kap><tld/>o</kap>
      <snc mrk="hxamemor.0o.BOT">
	<uzo tip="fak">BOT</uzo>
	<dif>
          Plurjara duonligneca vegeta&jcirc;o
          (<trd lng="la">Rubus chameaemorus</trd> el la familio
          <ref tip="super" cel="rozac.0oj">rozacoj</ref>), kreskanta
          precipe sur la norda duonglobo, en la muskaj mar&ccirc;oj de la
          zonoj tundra kaj arbara, kun man&gcirc;indaj beroj.
	</dif>
      </snc>
      <snc mrk="hxamemor.0o.bero">
        <dif>
          La <ref cel="ber.0o.KOMUNE">bero</ref> de
          <tld/>o<sncref ref="hxamemor.0o.BOT"/>:
          <ekz>
            memoru anka&ubreve; la rostitan boaca&jcirc;on kaj la orflavajn
            <tld/>ojn kun &scirc;a&ubreve;mkremo
            <fnt>
              <vrk><url
 ref="http://www.student.uib.no/~st01490/esperanto/sezono.eo.html"
                  >Sezono dum la tuta jaro</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">cloudberry</trd>
      <trd lng="pl">mroszka</trd>
      <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
    </drv>

  </art>

