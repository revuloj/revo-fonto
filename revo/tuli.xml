<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tuli.xml,v 1.21 2022/03/28 14:44:12 revo Exp $">
<kap>
   <rad>tuli</rad>/o
</kap>
<drv mrk="tuli.0o">
   <kap><tld/>o</kap>
   <mlg kod="IUPAC">Tm</mlg>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="69">elemento</ref> kun
      atomnumero 69, atompezo 168,934;
      malofta ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>, lantanoido:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas molega ar&gcirc;ent-kolora metalo,
        sinonimo foje uzata <klr>[&#x2026;]</klr> estas tuliumo por pli kongrui kun la simbolo Tm
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28696">Tulio</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_u;&c_l;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_t;&c_u;&c_l;&c_i;&c_j;</trd>
  <trd lng="br">tuliom</trd>
  <trd lng="ca">tuli</trd>
  <trdgrp lng="cs">
    <trd>Tm</trd>,
    <trd>thulium</trd>
  </trdgrp>
  <trd lng="de">Thulium</trd>
  <trd lng="en">thulium</trd>
  <trd lng="es">tulio</trd>
  <trd lng="fa">&a_t;&a_w;&a_l;&f_y;&a_m;</trd>
  <trd lng="fr">thulium</trd>
  <trd lng="hu">t&uacute;lium</trd>
  <trd lng="id">tulium</trd>
  <trd lng="it">tulio</trd>
  <trd lng="la">Thulium</trd>
  <trd lng="nl">thulium</trd>
  <trd lng="pl">tul</trd>
  <trd lng="pt">t&uacute;lio</trd>
  <trd lng="ru">&c_t;&c_u;&c_l;&c_l;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Tm</trd>,
    <trd>th&uacute;lium</trd>
  </trdgrp>
  <trd lng="sv">tulium</trd>
  <trd lng="uk">&#x442;&#x443;&#x43B;&#x456;&#x439;</trd>
</drv>
</art>
