<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apetit.xml,v 1.39 2023/08/30 18:23:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>apetit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="apetit.0o">
  <kap><tld/>o</kap>
  <snc mrk="apetit.0o.DIET">
    <dif>
      Deziro man&gcirc;i:
      <ekz>
        bona <tld/>o estas signo de sano;
      </ekz>
      <ekz>
        <ind>dum la man&gcirc;ado venas <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="fr">
          <trd>l'<ind>app&eacute;tit</ind> vient en mangeant</trd>
        </trdgrp>
        <trd lng="pl">apetyt ro&sacute;nie w miar&eogonek; jedzenia</trd>
      </ekz>
      <ekz>
        li man&gcirc;is kun plej granda <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; mi tute ne man&gcirc;us, mi nur ricevis pli grandan <tld/>on
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>bonan <tld/>on!</ind>
        <klr>(bondeziro dirita anta&ubreve; man&gcirc;o)</klr>
        <trdgrp lng="de">
          <trd>guten <ind>Appetit!</ind></trd>,
          <trd>gesegnete Mahlzeit!</trd>,
          <trd>Mahlzeit!</trd>
        </trdgrp>
        <trd lng="fa">&a_n;&a_w;&a_sh; &a_j;&a_A;&a_n;!</trd>
        <trd lng="fr">bon app&eacute;tit</trd>
        <trd lng="es">buen apetito</trd>
        <trd lng="hu">j&oacute; &eacute;tv&aacute;gyat</trd>
        <trd lng="id">selamat makan</trd>
        <trd lng="ms">selamat menjamu selara</trd>
        <trdgrp lng="lat">
          <trd>bene sapiat</trd>, <trd>bonam orexin</trd>
        </trdgrp>
        <trd lng="pl">smacznego!</trd>
        <trd lng="ro">poft&abreve; bun&abreve;!</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_t;&c_y;&c_t;</trd>
  <trd lng="bg">&c_a;&c_p;&c_je;&c_t;&c_i;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>apetyt</trd>,
    <trd>apet&yacute;t</trd>,
    <trd>chu&tcaron;</trd>
  </trdgrp>
  <trd lng="de">Appetit</trd>
  <trd lng="en">appetite</trd>
  <trd lng="es">apetito</trd>
  <trd lng="fa">&a_A;&a_sh;&a_t;&a_h;&a_A;</trd>
  <trd lng="fr">app&eacute;tit</trd>
  <trd lng="he">&#x5EA;&#x5B5;&#x5D0;&#x5B8;&#x5D1;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trd lng="hu">&eacute;tv&aacute;gy</trd>
  <trd lng="it">appetito</trd>
  <trd lng="ja">&#x98DF;&#x6B32;  [<ind>&#x3057;&#x3087;&#x304F;&#x3088;&#x304F;</ind>]</trd>
  <trd lng="pl">apetyt</trd>
  <trd lng="ro">apetit</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;</trd>
  <trd lng="ms">selara makan</trd>
  <trdgrp lng="sk">
    <trd>apet&iacute;t</trd>,
    <trd>chu&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x43F;&#x435;&#x442;&#x438;&#x442;</trd>
  <trd lng="zh">&#x80C3;&#x53E3;</trd>
</drv>

<drv mrk="apetit.0a">
  <kap><tld/>a</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif> 
      <ref tip="dif" cel="apetit.0veka"><tld lit="A"/>veka</ref>:
      <ekz>
        la batato pezis almena&ubreve; duonkilogramon, <tld/>a kaj dol&ccirc;a kiel
        mielo
        <fnt>
          <aut>LIU Houming</aut>,
          <vrk><url ref="&DonH;Revuoj/epc/epc8303/sago.html">
          Nigra Sago</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1983:03, p. 38a-41a</lok>
        </fnt>;
      </ekz>
       <ekz>
        <uzo tip="stl">FIG</uzo> &scirc;i anka&ubreve; estas tre <tld/>a, tute ne
        malbela
        <klr>(aminda, dezirveka)</klr>
        <fnt><bib>Revizoro</bib><lok>4:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5E6;&#x5D5;&#x5D3;&#x5D3;</trd>,
    <trd>&#x5DE;&#x5D5;&#x5E9;&#x5DA;</trd>
  </trdgrp>
  <trd lng="ja">&#x98DF;&#x6B32;&#x3092;&#x305D;&#x305D;&#x308B;  [<ind>&#x3057;&#x3087;&#x304F;&#x3088;&#x304F;&#x3092;&#x305D;&#x305D;&#x308B;</ind>]</trd>
</drv>

<drv mrk="apetit.0iga">
  <kap><tld/>iga</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="apetit.0veka"><tld lit="A"/>veka</ref>:
      <ekz>
        en la tradicia popolmedicino eruko estas konsiderata kiel
        <tld/>iga kaj uriniga    
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Kultiva_eruko">Kultiva
          eruko</url></vrk>
        </fnt>
      </ekz>
  </dif>
  </snc>
  <trd lng="cs">vzbuzuj&iacute;c&iacute; chu&tcaron; k j&iacute;dlu</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5E2;&#x5D5;&#x5B9;&#x5E8;&#x5B5;&#x5E8; &#x5EA;&#x5B5;&#x5D0;&#x5B8;&#x5D1;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trd lng="ja">&#x98DF;&#x6B32;&#x3092;&#x305D;&#x305D;&#x308B;  [<ind>&#x3057;&#x3087;&#x304F;&#x3088;&#x304F;&#x3092;&#x305D;&#x305D;&#x308B;</ind>]</trd>
  <trd lng="sk">vzbudzuj&uacute;ci chu&tcaron; k jedlu</trd>
</drv>

<drv mrk="apetit.0veka">
  <kap><tld/>veka</kap>
  <snc mrk="apetit.0veka.propre">
    <dif>
      Ekscitanta la <tld/>on:      
      <ekz>
        frititaj pecoj el kalmaro estas la plej populara <tld/>veka
        man&gcirc;a&jcirc;o de kantabroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=466567">Kantabra
          kuirarto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="apetit.0veka.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Alloga, dezirinda:
      <ekz>
        <tld/>veka virino;
      </ekz>
      <ekz>
        mi rememoris kelke el tiuj libroj, ili &ccirc;iam havis por mi tre
        <tld/>vekan aspekton
        <fnt>
          <bib>Fab3</bib>,
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_t;&c_y;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>appetitanregend</trd>, <trd>appetitlich</trd>
  </trdgrp>
  <trd lng="en">appetizing</trd>
  <trd lng="es">apetitoso</trd>
  <trd lng="fa">&a_A;&a_sh;&a_t;&a_h;&a_A;&a_A_madda;&a_w;&a_r;</trd>
  <trd lng="fr">app&eacute;tissant</trd>
  <trd lng="he">&#x5DE;&#x5E2;&#x5D5;&#x5E8;&#x5E8; &#x5EA;&#x5D0;&#x5D1;&#x5D5;&#x5DF;</trd>
  <trd lng="hu">&eacute;tv&aacute;gygerjeszt&odblac;</trd>
  <trdgrp lng="it">
    <trd>appetitoso,</trd>
    <trd>appetibile</trd>
  </trdgrp>
  <trd lng="ja">&#x98DF;&#x6B32;&#x3092;&#x305D;&#x305D;&#x308B;  [<ind>&#x3057;&#x3087;&#x304F;&#x3088;&#x304F;&#x3092;&#x305D;&#x305D;&#x308B;</ind>]</trd>
  <trd lng="pl">apetyczny</trd>
  <trd lng="ro">apetisant</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43F;&#x435;&#x442;&#x438;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x431;&#x443;&#x434;&#x436;&#x443;&#x44E;&#x447;&#x438;&#x439; &#x430;&#x43F;&#x435;&#x442;&#x438;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="apetit.0vekilo">
  <kap><tld/>vekilo</kap>
  <snc>
    <dif>
      &Ccirc;io ekscitanta la <tld/>on:
      <ekz>
        <klr>[aperitivo estas]</klr> trinka&jcirc;o, ordinare alkohola,
        trinkata kiel <tld/>vekilo anta&ubreve; man&gcirc;o
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        ili ludis kiel apetitvekilo en la koncertoj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Polarkreis_18">Polarkreis
          18</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Appetitanreger</trd>
  <trd lng="fr">ap&eacute;ritif</trd>
</drv>
</art>
