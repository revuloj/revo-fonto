<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>apetit</rad>/o
</kap>
<drv mrk="apetit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Deziro man&gcirc;i:
      <ekz>
        bona <tld/>o estas signo de sano;
      </ekz>
      <ekz>
        dum la man&gcirc;ado venas <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="cs">chu&tcaron;</trd>
  <trd lng="de">Appetit</trd>
  <trd lng="fr">app&eacute;tit</trd>
  <trd lng="it">appetito</trd>
  <trd lng="ru">&#1072;&#1087;&#1087;&#1077;&#1090;&#1080;&#1090;</trd>
</drv>
<drv mrk="apetit.0veka">
  <kap><tld/>(ig)a<fnt>Z</fnt>, <tld/>veka<fnt>Z</fnt></kap>
  <snc num="1">
    <dif>
      Ekscitanta la apetiton.  
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Alloga, dezirinda:
      <ekz>
        <tld/>a virino.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de"><trd>appetitanregend</trd>, <trd>appetitlich</trd></trdgrp>
  <trd lng="fr">app&eacute;tissant</trd>
  <trdgrp lng="it">
    <trd>appetitoso,</trd>
    <trd>appetibile;</trd>
  </trdgrp>
  <trd lng="ru">&#1072;&#1087;&#1087;&#1077;&#1090;&#1080;&#1090;&#1085;&#1099;&#1081;</trd>
</drv>
</art>
