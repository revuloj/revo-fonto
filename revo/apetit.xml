<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>apetit</rad>/o
</kap>
<drv mrk="apetit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Deziro man&gcirc;i:
      <ekz>
        bona <tld/>o estas signo de sano;
      </ekz>
      <ekz>
        dum la man&gcirc;ado venas <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>bonan <tld/>on!</ind>
        <klr>(bondeziro dirita anta&ubreve; man&gcirc;o)</klr>
        <trd lng="fr">bon app&eacute;tit</trd>
        <trd lng="es">buen apetito</trd>
        <trdgrp lng="lat">
          <trd>bene sapiat</trd>, <trd>bonam orexin</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">chu&tcaron;</trd>
  <trd lng="de">Appetit</trd>
  <trd lng="es">apetito</trd>
  <trd lng="fr">app&eacute;tit</trd>
  <trd lng="it">appetito</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;</trd>
  <trd lng="sk">chu&tcaron;</trd>
</drv>

<drv mrk="apetit.0veka">
  <kap><tld/>(ig)a<fnt>Z</fnt>, <tld/>veka<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ekscitanta la apetiton.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Alloga, dezirinda:
      <ekz>
        <tld/>a virino.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>appetitanregend</trd>, <trd>appetitlich</trd>
  </trdgrp>
  <trd lng="es">apetitoso</trd>
  <trd lng="fr">app&eacute;tissant</trd>
  <trdgrp lng="it">
    <trd>appetitoso,</trd>
    <trd>appetibile</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
