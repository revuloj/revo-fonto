<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>askalon</rad>/o
</kap>
<drv mrk="askalo.0">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de la genro <ref tip="super" cel="ajl.0o.genro">ajlo</ref>
      (<trd lng="la">Allium ascalonicum</trd>), uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja;-&c_sh;&c_a;&c_l;&c_o;&c_t;</trd>
  <trd lng="br">chalotez</trd>
  <trd lng="cs">cibule šalotka,</trd>
  <trd lng="de">Schalotte</trd>
  <trd lng="en">shalot</trd>
  <trd lng="fr">&eacute;chalote</trd>
  <trdgrp lng="hu">
    <trd>mogyor&oacute;hagyma</trd>,
    <trd>salotta</trd>
  </trdgrp>
  <trd lng="it">scalogno</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_u;&c_k;-&c_sh;&c_a;&c_l;&c_o;&c_t;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_l;&c_o;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">šalotka</trd>
</drv>
</art>
