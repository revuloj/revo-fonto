<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: askalo.xml,v 1.18 2022/04/25 15:11:00 revo Exp $">
<kap>
  <rad>askalon</rad>/o
</kap>
<drv mrk="askalo.0">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de la genro <ref tip="super" cel="ajl.0o.genro">ajlo</ref>
      (<trd lng="la">Allium ascalonicum</trd>), nuntempe konsiderata kiel varia&jcirc;o de <ref tip="super" cel="cep.0o">cepo</ref>
      (<trd lng="la">Allium cepa var. aggregatum</trd>), kiu formas bulbidojn kaj uzata kiel spica&jcirc;o:
      <ekz>
        liliacoj &dash; la plej konataj estas la ajlo, la <tld/>o, la cepo, la hiacinto, la
        lilio, la &scirc;enoprazo kaj la tulipo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18063"
              >Liliacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bearnezo estas sa&ubreve;co farita el batmiksita butero kaj ovoflavo kun aldono de
        vino, vinagro, drakunkolo kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=80660"
              >Bearnezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/69/6286Vegetables_Fruits_Baliuag_Bulacan_Philippines_14.jpg/320px-6286Vegetables_Fruits_Baliuag_Bulacan_Philippines_14.jpg"
      prm="CC0"
      >rikoltitaj <tld/>oj
        <fnt>
          <aut>Judgefloro, propra verko</aut>
          <lok><url ref="&WCW;/File:6286Vegetables_Fruits_Baliuag_Bulacan_Philippines_14.jpg"
              >&WCW;/File:6286Vegetables_Fruits_Baliuag_Bulacan_Philippines_14.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja;-&c_sh;&c_a;&c_l;&c_o;&c_t;</trd>
  <trd lng="br">chalotez</trd>
  <trd lng="cs">cibule &scaron;alotka,</trd>
  <trd lng="de">Schalotte</trd>
  <trd lng="en">shalot</trd>
  <trd lng="fr">&eacute;chalote</trd>
  <trdgrp lng="hu">
    <trd>mogyor&oacute;hagyma</trd>,
    <trd>salotta</trd>
  </trdgrp>
  <trd lng="it">scalogno</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_u;&c_k;-&c_sh;&c_a;&c_l;&c_o;&c_t;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_l;&c_o;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;alotka</trd>
</drv>
</art>
