<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klarion</rad>/o
</kap>
<drv mrk="klario.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Latuna <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>
      kun akuta sono, uzata ofte &ccirc;e infanterio.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_r;&c_n;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Zinke</trd>,
     <trd>Signalhorn</trd>
  </trdgrp>
  <trd lng="en">bugle</trd>
  <trd lng="es">clarin</trd>
  <trd lng="fr">clairon<klr tip="ind"> (instrument de mus.)</klr></trd>
  <trdgrp lng="hu">
    <trd>(jelz&odblac;)<ind>k&uuml;rt</ind></trd>,
    <trd>(katonai) <ind>trombita</ind></trd>
  </trdgrp>
  <trd lng="nl">bugel</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;</trd>
</drv>

<drv mrk="klario.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta sonon de <tld/>o:
      <ekz>
      <ind><tld/>a vo&ccirc;o</ind>.
      <trd lng="hu">k&uuml;rtsz&oacute;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">clarion</trd>
  <trdgrp lng="fr">
    <trd>de <ind>clairon</ind></trd>,
    <trd>claironnant</trd>
  </trdgrp>
  <trd lng="hu">k&uuml;rt-</trd>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="klario.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Produkti sonon per <tld/>o.
    </dif>
    <trd lng="hu">k&uuml;rtjelet ad</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Alvoki soldatojn signale per <tld/>o:
      <ekz>
        oni <tld/>is la elliti&gcirc;on <klr>(a&ubreve;: al la elliti&gcirc;o)</klr>,
        tagmezon, prepari&gcirc;on al formar&scirc;o, kolekti&gcirc;on,
        enkazerni&gcirc;on.
      </ekz>
    </dif>
    <trd lng="en">sound</trd>
    <trd lng="hu">trombit&aacute;l</trd>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>claironner</trd>,
    <trd>sonner<klr tip="ind"> (appeler par un signal de clairon)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="klario..0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Ludanto de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr"><klr tip="ind">(joueur de) </klr>clairon</trd>
  <trd lng="hu">k&uuml;rt&ouml;s</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>

</art>
