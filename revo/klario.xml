<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klarion</rad>/o
</kap>
<drv mrk="klario.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Latuna <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref> kun akuta sono, uzata ofte &ccirc;e infanterio. 
    </dif>
  </snc>
  <trd lng="en">bugle</trd>
  <trd lng="nl">bugel</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;</trd>
</drv>
<drv mrk="klario.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta sonon de <tld/>o:
      <ekz><tld/>a vo&ccirc;o.</ekz>
    </dif>
  </snc>
  <trd lng="en">clarion</trd>
</drv>
<drv mrk="klario.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Produkti sonon per <tld/>o. 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Alvoki soldatojn signale per <tld/>o:
      <ekz>
        oni <tld/>is la levi&gcirc;on, tagmezon,
        prepari&gcirc;on al formar&scirc;o, amasi&gcirc;on, enkazerni&gcirc;on.
      </ekz>
    </dif>
    <trd lng="en">sound</trd>
  </snc>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
