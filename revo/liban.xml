  <art mrk="$Id$">
   <kap>
    <ofc>8</ofc>
    <rad>Liban</rad>/o<fnt>Z</fnt>
   </kap>

   <drv mrk="liban.L0o">
    <kap><ofc>8</ofc><tld/>o
     <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Libano">Libano</url></lok></fnt>
    </kap>
    <fnt><bib>ZR</bib>,</fnt>
    <fnt><bib>PIV1</bib>,</fnt>
    <fnt><bib>KrED</bib></fnt>
    <snc>
    <uzo tip="fak">GEOG</uzo>
       <dif>
        &Scirc;tato &ccirc;e la orienta bordo de la Mediteraneo.
       </dif>
    </snc>
     <trd lng="be">&c_L;&c_ib;&c_v;&c_a;&c_n;</trd>
     <trd lng="cs">Libanon</trd>
     <trd lng="de">Libanon</trd>
     <trd lng="en">Lebanon</trd>
     <trdgrp lng="fr">
       <trd>Liban</trd>,
       <trd>R&eacute;publique libanaise</trd>
     </trdgrp>
     <trd lng="hu">Libanon</trd>
     <trd lng="id">Lebanon</trd>
     <trd lng="nl">Libanon</trd>
     <trd lng="pl">Liban</trd>
     <trd lng="pt">L&iacute;bano</trd>
     <trd lng="sk">Libanon</trd>
</drv>

 <drv mrk="liban.0ano">
  <kap><tld lit="l"/>ano,
  <var><kap><tld lit="l"/>anino</kap></var></kap>
    <fnt><bib>KrED</bib></fnt>
    <snc>
       <dif>
      Lo&gcirc;ant(in)o a&ubreve; civitan(in)o de Libano.
       </dif>
    </snc>
     <trd lng="be">&c_l;&c_ib;&c_v;&c_a;&c_n;&c_je;&c_c;</trd>
     <trd lng="de">Libanese</trd>
     <trd lng="en">Lebanese</trd>
     <trdgrp lng="fr"> 
       <trd>Libanais</trd>
       <trd>Libanaise<klr> (subst.)</klr></trd>
     </trdgrp>
     <trd lng="hu">libanoni</trd>
     <trd lng="nl">Libanees</trd>
     <trd lng="pl">liba&nacute;czyk</trd>
</drv>

<drv mrk="liban.0a">
  <kap><tld lit="l"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Libano.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>libanais </trd>
    <trd>libanaise<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

