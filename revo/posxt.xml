  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_sh;&c_t;&c_a; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;)</klr></trd>,
