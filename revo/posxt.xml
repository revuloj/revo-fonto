<drv mrk="posxt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <tezrad fak="POSX"/>
  <snc mrk="posxt.0ajxo.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      &Ccirc;ia <tld/>a senda&jcirc;o, kia <ref tip="sub" cel="leter.0o">letero</ref>,
      <ref tip="sub" cel="kart.posxt0o"><tld/>karto</ref>,
      <ref tip="sub" cel="pak.0ajxo">paka&jcirc;o</ref>:
      <ekz>
        &gcirc;i ekzemple ne havas funkciojn por a&ubreve;tomate ordigi la
        envenantajn <tld/>a&jcirc;ojn
        <fnt><aut>E. Grimley Evans</aut>, <vrk>Uniksulo
        ekspertas Unikodon</vrk>, <bib>Monato</bib>,
        <lok>2000:2, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_sh;&c_t;&c_o;&c_v;&c_a;&c_je; &c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_t;&c_a; <klr tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_s;&c_l;&c_a;&c_n;&c_y; &c_p;&c_r;&c_a;&c_d;&c_m;&c_je;&c_t;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Postsache</trd>,
     <trd>Postsendung</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>post</trd>,
     <trd>mail piece</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>correo<klr> (env&iacute;o postal)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>courrier<klr> (envoi postal)</klr></trd>
  </trdgrp>
  <trd lng="he">&dalet;&bet;&resh; &dalet;&vav;&alef;&resh;</trd>
  <trd lng="hu">postai k&uuml;ldem&eacute;ny</trd>
  <trd lng="id">pos</trd>
  <trd lng="ja">&#x90F5;&#x4FBF;&#29289;  <pr>&#12422;&#12358;&#12403;&#12435;&#12406;&#12388;</pr></trd>
  <trdgrp lng="nl">
