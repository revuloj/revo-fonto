  <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_a;&c_ja; <ind>&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x90AE;&#x7BB1;&#x5BA2;&#25143;&#x7AEF; <pr>y&#xF3;uxi&amacron;ngk&#xE8;h&#xF9;du&amacron;n</pr></trd>,
    <trd>&#x90F5;&#x7BB1;&#x5BA2;&#25142;&#x7AEF; <pr>y&#xF3;uxi&amacron;ngk&#xE8;h&#xF9;du&amacron;n</pr></trd>,
    <trd>&#30005;&#x5B50;&#x90AE;&#x4EF6;&#x8F6F;&#x4EF6;
    <pr>di&#xE0;nz&#x1D0;y&#xF3;uji&#xE0;nru&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x96FB;&#x5B50;&#x90F5;&#x4EF6;&#x8EDF;&#x4EF6;
    <pr>di&#xE0;nz&#x1D0;y&#xF3;uji&#xE0;nru&#x1CE;nji&#xE0;n</pr></trd>
  </trdgrp>
