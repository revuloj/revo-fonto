    <trd>poststuk</trd>
  </trdgrp>
  <trd lng="pl">przesy&lstroke;ka  <ind>pocztowa</ind></trd>
  <trd lng="pt">correspond&ecirc;ncia</trd>
  <trd lng="ro">pachet <ind>po&#537;tal</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_o;&c_je; &c_o;&c_t;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="zh">
