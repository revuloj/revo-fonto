<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>pionir</rad>/o
    </kap>
    <drv mrk="pionir.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="pionir.0o.MIL">
        <uzo tip="fak">MIL</uzo>
        <dif>
          Soldato, kies tasko estas konstrui pontojn, fari vojojn,
          tran&ccirc;eojn kc. 
        </dif>
        <trd lng="be">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
        <trd lng="hu">ut&aacute;sz</trd>
        <trdgrp lng="pt">
          <trd>sapador</trd>,
          <trd>bandeirante</trd>
        </trdgrp>
        <trd lng="ru">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
      </snc>
      <snc mrk="pionir.0o.koloniano">
        <dif>
          Koloniano plugebliganta senkulturan teron.  
        </dif>
        <trdgrp lng="be">
          <trd>&c_p;&c_ib;&c_ja;&c_n;&c_e;&c_r;</trd>,
          <trd>&c_p;&c_je;&c_r;&c_sh;&c_a;&c_p;&c_r;&c_a;&c_h;&c_o;&c_d;&c_z;&c_je;&c_c;</trd>,
          <trd>&c_p;&c_je;&c_r;&c_sh;&c_a;&c_p;&c_r;&c_a;&c_h;&c_o;&c_d;&c_c;&c_a;</trd>
        </trdgrp>
        <trd lng="hu">&uacute;tt&ouml;r&odblac;</trd>
       <trdgrp lng="pt">
        <trd>pioneiro</trd>,
        <trd>desbravador</trd>
       </trdgrp>
        <trd lng="ru">&c_p;&c_je;&c_r;&c_v;&c_o;&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_je;&c_c;</trd>
      </snc>
      <snc mrk="pionir.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Homo pretiganta la vojon kaj sukceson al novaj ideoj:
          <ekz>
            la <tld/>oj de la paca idealo;
          </ekz><ekz>
            la <tld/>oj de &ccirc;i tiuj ideoj devas multe batali<fnt>Z</fnt>;
          </ekz><ekz>
            fari <tld/>an laboron;
          </ekz><ekz>
            dedi&ccirc;i sin al <tld/>eco<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>e montri la vojon
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="aposto.0o">apostolo</ref>,
          <ref cel="propag.0isto">propagandisto</ref>,
          <ref cel="plug">plugisto</ref>,
          <ref cel="misi.0isto">misiisto</ref>,
          <ref cel="avanga.0o">avangardo</ref>.
        </refgrp>
        <trdgrp lng="be">
          <trd>&c_p;&c_ib;&c_ja;&c_n;&c_e;&c_r;</trd>,
          <trd>&c_p;&c_je;&c_r;&c_sh;&c_a;&c_p;&c_r;&c_a;&c_h;&c_o;&c_d;&c_z;&c_je;&c_c;</trd>,
          <trd>&c_p;&c_je;&c_r;&c_sh;&c_a;&c_p;&c_r;&c_a;&c_h;&c_o;&c_d;&c_c;&c_a;</trd>
        </trdgrp>
        <trd lng="hu">&uacute;tt&ouml;r&odblac;</trd>
       <trdgrp lng="pt">
         <trd>pioneiro</trd>,
         <trd>precursor</trd>
       </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_je;&c_c;</trd>,
          <trd>&c_p;&c_i;&c_o;&c_n;&c_je;&c_r;</trd></trdgrp>
      </snc>
      <trd lng="cs">pr&uring;kopn&iacute;k</trd>
      <trd lng="de">Pionier</trd>
      <trdgrp lng="nl">
        <trd>pionier</trd>,
        <trd>baanbreker</trd>
      </trdgrp>

    </drv>
  </art>
