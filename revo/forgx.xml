<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: forgx.xml,v 1.29 2025/12/13 16:38:39 revo Exp $">
<kap><ofc>*</ofc>
<rad>for&gcirc;</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="forgx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="forgx.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="labor.pri0i.KOMUNE">Prilabori</ref> metalon per fajro kaj martelo,
      precipe feron kaj &scirc;talon:
      <ekz>
        <tld/>i kradon, hufferon;
      </ekz>
      <ekz>
        kandelabro <tld/>ita el oro, de &gcirc;ia trunko &gcirc;is &gcirc;iaj floroj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 8:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru du kerubojn el oro per <tld/>a laboro
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>os el siaj glavoj plugilojn kaj el siaj lancoj rikoltilojn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 2:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anto de diversaj majstra&jcirc;oj el kupro kaj fero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 4:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        arestitoj en<tld/>itaj per katenoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u feron dum &gcirc;i estas varmega
        <klr>(uzu okazon &gcirc;ustatempe)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="forgx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pene <ref tip="super" cel="form.0i">formi</ref>:
      <ekz>
        vidu do tiun &ccirc;i kura&gcirc;an entrepreneman kapon, kiel &gcirc;i <tld/>as kaj plenumas
        planojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu kreas sian forton, &ccirc;iu <tld/>as sian sorton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <nom>Stalin</nom> nun &ccirc;esis toleri liberan evoluon de lingvoj kaj nacioj <klr>[&#8230;]</klr>
        <tld/>ante la karakteriza&jcirc;ojn de tutsoveta patriotismo
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">schmieden</trd>
  <trd lng="en">forge</trd>
  <trd lng="es">forjar</trd>
  <trd lng="fr">forger</trd>
  <trd lng="hu">kov&aacute;csol</trd>
  <trdgrp lng="id">
    <trd>tempa</trd>,
    <trd><baz>tempa</baz>menempa</trd>,
    <trd>gembleng</trd>,
    <trd><baz>gembleng</baz>menggembleng</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x935B;&#12360;&#x308B;  <pr>&#x304D;&#x305F;&#12360;&#x308B;</pr></trd>,
    <trd>&#x935B;&#36896;&#12377;&#x308B;  <pr>&#x305F;&#12435;&#x305E;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x935B;&#x932C;&#12377;&#x308B;  <pr>&#x305F;&#12435;&#x308C;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x935B;&#12360;&#x4E0A;&#12370;&#x308B;  <pr>&#x304D;&#x305F;&#12360;&#12354;&#12370;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">smeden</trd>
  <trdgrp lng="pl">
    <trd>ku&cacute;</trd>,
    <trd>wykuwa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>forjar</trd>,
    <trd>bater<klr> (ferro)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x935B; <pr>du&#xE0;n</pr></trd>,
    <trd>&#x935B;&#36896; <pr>du&#xE0;nz&#xE0;o</pr></trd>,
    <trd>&#37656;&#25171; <pr>chu&#xED;d&#x1CE;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="forgx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="forgx.0ejo.TEK">
    <dif>
      Loko ekipita por <tld/>ado:
      <ekz>
        jen ni estas anta&ubreve; la <tld/>ejo, kie la fajro brulas kaj la duonnudaj viroj
        frapas per la marteloj tiel, ke la fajreroj saltas malproksimen &ccirc;irka&ubreve;en
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> venis en <tld/>ejon, metis la tornistron <klr>[&#8230;]</klr> sur la
        <ref tip="prt" cel="ambos.0o.KOMUN">amboso</ref>n kaj petis <klr>[&#8230;]</klr>, ke ili martelu
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konstruis por si <tld/>ejon proksime al la maro,
	sed longdistance for de <nom>Borg</nom>
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>
          <vrk>Sagao de Egil</vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_z;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="de">Schmiede</trd>
  <trd lng="fr">forge</trd>
  <trd lng="id">apar</trd>
  <trd lng="ja">&#x935B;&#x51B6;&#x5C4B;  <pr>&#x304B;&#12376;&#12420;</pr></trd>
  <trd lng="pl">ku&zacute;nia</trd>
  <trd lng="uk">&#x43A;&c_u;&c_z;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="forgx.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="forgx.0isto.TEK">
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="meti.0isto">Metiisto</ref> kiu <tld/>as metalajn a&jcirc;ojn:
      <ekz>
        li forkondukis &ccirc;iujn <klr>[&#8230;]</klr> &ccirc;iujn artistojn kaj &ccirc;iujn <tld/>istojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 24:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto <ind>sub<tld/>i</ind>s la &ccirc;evalojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri fratoj</lok>
        </fnt>;
        <trd lng="de">beschlagen <klr>(Huf)</klr></trd>
        <trd lng="pl">podku&cacute; <klr>(kopyto)</klr></trd>
      </ekz>
      <ekz>
        la maljuna <tld/>isto batis la ambosorandon per gvidmarteleto
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Schmied</trd>
  <trd lng="es">herrero</trd>
  <trdgrp lng="fr">
    <trd>ferronnier</trd>,
    <trd>forgeron</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>tempa</baz>penempa</trd>,
    <trd><baz>gembleng</baz>penggembleng</trd>,
    <trd>pandai besi</trd>
  </trdgrp>
  <trd lng="ja">&#x935B;&#x51B6;&#x5C4B;  <pr>&#x304B;&#12376;&#12420;</pr></trd>
  <trdgrp lng="pl">
    <trd>kowal</trd>,
    <trd>&zdot;ele&zacute;nik</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&c_v;&c_a;&#x43B;&#x44C;</trd>
  <trd lng="zh">&#37941;&#21280; <pr>ti&#x11B;jiang</pr></trd>
</drv>

<drv mrk="forgx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="forgx.al0i.TEK">
    <dif>
      <ref tip="super" cel="fiks.0i.flagon">Fiksi</ref> al io per <tld/>ado:
      <ekz>
        al<tld/>i maningon al sabro;
      </ekz>
      <ekz>
        tiajn nesingardajn kanajlojn oni devus al<tld/>i al la galeroj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">r&aacute;kov&aacute;csol</trd>
  </snc>
  <snc mrk="forgx.al0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="mov.sen0igi">Senmovigi</ref> &ccirc;e iu loko,
      da&ubreve;re <ref tip="super" cel="rest.0igi">restigi</ref>:
      <ekz>
        li bone scias, ke mi lin amas <klr>[&#8230;]</klr> kion pli mi povus fari? &ccirc;u mi mem
        devas al<tld/>i lin per ia aparta atento?
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la konscienco, vekita de la infana rigardo, kvaza&ubreve; al<tld/>is lin al la tero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        personoj al<tld/>itaj al la lito de morto
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        rabisto <klr>[&#8230;]</klr> alfor&gcirc;ita al la malvirto per feraj ligiloj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj specoj de serpentoj per sia rigardo magie al<tld/>as la birdojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la virino, kiu staris anta&ubreve; &scirc;i, al<tld/>is &scirc;in al la loko per la forto de sia
        rigardo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu objekto <klr>[&#8230;]</klr> al<tld/>is al si la rigardon de Marta
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glu.al0i">alglui</ref>,
      <ref cel="katen.al0i">alkateni</ref>,
      <ref cel="fiks.0i">fiksi</ref>,
      <ref cel="firm">firmigi</ref>,
      <ref cel="rigid.0igxi">rigidi&gcirc;i</ref>,
      <ref cel="sxton.0igi.FIG">&scirc;tonigi<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anketten</trd>,
      <trd>fesseln</trd>,
      <trd>versteinern</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>attacher (&agrave;)</trd>,
      <trd>clouer (&agrave;)<klr tip="ind"> (immobiliser)</klr></trd>,
      <trd>immobiliser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>odak&ouml;t<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>odaszegez<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="id">
    <trd><baz>rantai</baz>merantai</trd>,
    <trd><baz>belenggu</baz>membelenggu</trd>,
    <trd><baz>ikat</baz>mengikat</trd>
  </trdgrp>
  <trd lng="ja">&#x304B;&#12375;&#12417;&#x308B;</trd>
  <trd lng="nl">vastsmeden</trd>
  <trdgrp lng="pl">
    <trd>przyku&cacute;</trd>,
    <trd>przykuwa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_t;&c_zh; &#x43F;&c_je;&c_r;&c_je;&#x43D;.)</trd>
</drv>

<drv mrk="forgx.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="forgx.kun0i.TEK">
    <dif>
      <ref tip="super" cel="kun.0igi">Kunigi</ref> per <tld/>ado.
    </dif>
  </snc>
  <snc mrk="forgx.kun0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Firme kaj da&ubreve;re kunigi:
      <ekz>
        Al la amiko sa&gcirc;e elektita / Kun<tld/>u vin en fera fideleco
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glu.al0i">alglui</ref>,
      <ref cel="lig.al0i">alligi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">zusammenschmieden</trd>
  <trd lng="hu">&ouml;sszekov&aacute;csol</trd>
  <trdgrp lng="id">
    <trd><baz>tempa</baz>menempa bersama</trd>,
    <trd><baz>gembleng</baz>menggembleng bersama</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x935B;&#x63A5;&#12377;&#x308B;  <pr>&#x305F;&#12435;&#x305B;&#12388;&#12377;&#x308B;</pr></trd>,
    <trd>&#x305F;&#x305F;&#12356;&#12390;&#x63A5;&#21512;&#12377;&#x308B;
    <pr>&#x305F;&#x305F;&#12356;&#12390;&#x305B;&#12388;&#12372;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x56FA;&#x304F;&#x7D50;&#12403;&#12388;&#12369;&#x308B;  <pr>&#x304B;&#x305F;&#x304F;&#12416;&#12377;&#12403;&#12388;&#12369;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">samensmeden</trd>
  <trdgrp lng="pl">
    <trd>skuwa&cacute;</trd>,
    <trd>&lstroke;&aogonek;czy&cacute; trwale</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_s;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x4F7F;&#x7D50;&#21512; <pr>sh&#x1D0;ji&#xE9;h&#xE9;</pr></trd>
</drv>
</art>
