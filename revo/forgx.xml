<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>for&gcirc;</rad>/i</kap>
<drv mrk="forgx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Prilabori metalon per fajro kaj martelo, precipe feron kaj &scirc;talon:
      <ekz>
        <tld/>i kradon, hufferon;
      </ekz>
      <ekz>
        <tld/>ejo<sup>Z</sup>;
      </ekz>
      <ekz>
        <tld/>isto<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pene formi:
      <ekz>
        &ccirc;iu kreas sian forton, &ccirc;iu <tld/>as sian
        sorton<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i vorton<fnt>Z</fnt>.
      </ekz>
      <ekz>
        la malfeli&ccirc;o <tld/>as la karakteron.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">schmieden</trd>
  <trd lng="en">forge</trd>
  <trd lng="es">forjar</trd>
  <trd lng="fr">forger</trd>
  <trd lng="hu">kov&aacute;csol</trd>
  <trd lng="nl">smeden</trd>
  <trdgrp lng="pt">
    <trd>forjar</trd>,
    <trd>bater<klr> (ferro)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="forgx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fiksi al io per <tld/>ado:
      <ekz>
        al<tld/>i maningon al sabro.
      </ekz>
    </dif>
    <trd lng="hu">r&aacute;kov&aacute;csol</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmovigi &ccirc;e iu loko, da&ubreve;re restigi:
      <ekz>
        la malsano al<tld/>as lin al la lito<fnt>Z</fnt>,
        al la &ccirc;ambro;
      </ekz>
      <ekz>
        pro surprizo li restis kvaza&ubreve;
        al<tld/>ita sur sia loko;
      </ekz>
      <ekz>
        diversaj monaj aferoj al<tld/>is lin al la urbo;
      </ekz>
      <ekz>
        al<tld/>i sian rigardon al a&ubreve; sur
        ion a&ubreve; iun<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al<tld/>i al si la rigardon de la publiko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="radik">enradikigi</ref>,
      <ref cel="glu.al0i">alglui</ref>,
      <ref cel="katen.al0i">alkateni</ref>,
      <ref cel="fiks.0i">fiksi</ref>,
      <ref cel="firm">firmigi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>anketten</trd>,
      <trd>fesseln</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>attacher (&agrave;)</trd>,
      <trd>clouer (&agrave;)<klr tip="ind"> (immobiliser)</klr></trd>,
      <trd>immobiliser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>odak&ouml;t<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>odaszegez<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">vastsmeden</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="forgx.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunigi per <tld/>ado.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Firme kaj da&ubreve;re kunigi:
      <ekz>
        kun<tld/>i sin al amiko<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glu.al0i">alglui</ref>,
      <ref cel="lig.al0i">alligi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">zusammenschmieden</trd>
  <trd lng="hu">&ouml;sszekov&aacute;csol</trd>
  <trd lng="nl">samensmeden</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
