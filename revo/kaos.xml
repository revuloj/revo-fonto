<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaos</rad>/o
</kap>
<drv mrk="kaos.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="hxaos.0o">&hcirc;aoso</ref>
    <ekz>
      plena <tld/>o regas en la hinda kalendaro
      <fnt><aut>Evgeni Georgiev</aut>: <vrk>&Ccirc;u kalendar-reformo estos
      bezonata?</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 2a, p.
11a</lok></fnt>.
    </ekz>
    <ekz>
     sen kapablo movi sin a&ubreve; vortigi la kaoson de sia interno.
    <fnt><aut>Stellan Engholm</aut>: <vrk>Al Torento</vrk>,<lok>&ccirc;apitro
    IV,
 1930</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_o;&c_s;</trd>
  <trd lng="ca">caos</trd>
  <trdgrp lng="cs">
    <trd>chaos</trd>,
    <trd>nepo&rcaron;&aacute;dek</trd>,
    <trd>zmatek</trd>
  </trdgrp>
  <trd lng="de">Chaos</trd>
  <trd lng="en">chaos</trd>
  <trd lng="es">caos</trd>
  <trd lng="fr">chaos</trd>
  <trd lng="hu">k&aacute;osz</trd>
  <trd lng="nl">chaos</trd>
  <trdgrp lng="pl">
    <trd>chaos</trd>,
    <trd>zamieszanie</trd>,
    <trd>rozgardiasz</trd>,
    <trd>ba&lstroke;agan</trd>,
    <trd>burdel <klr>(pot.,wulg.)</klr></trd>
  </trdgrp>   
  <trd lng="pt">caos</trd>
  <trdgrp lng="ro">
    <trd>haos</trd>,
    <trd>confuzie</trd>,
    <trd>dezordine</trd>,
    <trd>fream&abreve;t</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_a;&c_o;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>chaos</trd>,
    <trd>trma-vrma</trd>,
    <trd>zm&auml;tok</trd>
  </trdgrp>
  <trd lng="sv">kaos</trd>
</drv>

<drv mrk="kaos.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="hxaos.0a">&hcirc;aosa</ref>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">ca&ograve;tic</trd>
  <trdgrp lng="cs">
    <trd>chaotick&yacute;</trd>,
    <trd>zmaten&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>chaotisch</trd>,
    <trd>durcheinander</trd>,
    <trd>ungeordnet</trd>,
    <trd>unordentlich</trd>
  </trdgrp>
  <trd lng="en">chaotic</trd>
  <trd lng="es">ca&oacute;tico</trd>
  <trd lng="fr">chaotique</trd>
  <trd lng="hu">kaotikus</trd>
  <trd lng="nl">chaotisch<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">chaotyczny</trd>
  <trd lng="pt">ca&oacute;tico</trd>
  <trdgrp lng="ro">
    <trd>haotic </trd>,
    <trd>dezorganizat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">chaotick&yacute;</trd>
  <trd lng="sv">kaotisk</trd>
</drv>

<drv mrk="kaos.0e">
  <kap><tld/>e</kap>
  <snc>
    <ref tip="dif" cel="hxaos.0e">&hcirc;aose</ref>
    <ekz>
      en &ccirc;iu historieto la homoj, bestoj a&ubreve; objektoj 
      interparolas plimalpli <tld/>e
      <fnt><aut>Walter Klag</aut>:
      <vrk>Groteskumi fabele (Erwin Lazar, La sepkapa feino)</vrk>, 
      <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 24a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;</trd>
  <trd lng="ca">ca&ograve;ticament</trd>
  <trdgrp lng="cs">
    <trd>bez ladu a skladu</trd>,
    <trd>chaoticky</trd>,
    <trd>halabala</trd>
  </trdgrp>
  <trd lng="de">chaotischerweise</trd>
  <trd lng="en">chaotically</trd>
  <trd lng="es">ca&oacute;ticamente</trd>
  <trdgrp lng="fr">
    <trd>chaotiquement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;ssze-vissza</trd>,
    <trd>kaotikusan</trd>
  </trdgrp>
  <trd lng="nl">chaotisch<klr tip="ind"> (bw.)</klr></trd>
  <trd lng="pl">chaotycznie</trd>     
  <trd lng="ro">haotic</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;</trd>,
    <trd>&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">chaoticky</trd>
</drv>
</art>
