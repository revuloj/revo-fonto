<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>liberal</rad>/a
</kap>
<drv mrk="libera.0a">
  <kap><tld/>a</kap>
  <snc mrk="libera.0a.POL">
    <dif>
      Favora al libero kaj toleremo en sociaj, politikaj, religiaj
      aferoj:
      <ekz>
        <tld/>aj partioj, doktrinoj, opinioj, institucioj;
      </ekz>
      <ekz>
        <tld/>a spirito.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>szabadelv&udblac;</trd>,
      <trd>liber&aacute;lis</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Vd <ref tip="dif" cel="liber.0a.profesio">libera profesio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_b;&c_e;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">liberal</trd>
  <trdgrp lng="fr">
    <trd>lib&eacute;ral</trd>
  </trdgrp>
  <trd lng="es">liberal</trd>
  <trd lng="nl">liberaal</trd>
  <trd lng="pt">liberal</trd>
  <trd lng="ru">&c_l;&c_i;&c_b;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="libera.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="libera.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politiko celanta permesemon rilate al la individuoj kaj
      forigon de privilegioj.
    </dif>
  </snc>
  <snc mrk="libera.0ismo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ekonomika doktrino malaprobanta
      <ref cel="sxtat.0ismo">&scirc;tatismon</ref> kaj favoranta
      memregantan liberan merkaton kaj konkuradon.
    </dif>
    <refgrp tip="ant">
      <ref cel="protek.0ismo">protektismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_b;&c_e;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Liberalismus</trd>
  <trdgrp lng="fr">
    <trd>lib&eacute;ralisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szabadelv&udblac;s&eacute;g</trd>,
    <trd>liberalizmus</trd>
  </trdgrp>
  <trd lng="nl">liberalisme</trd>
  <trd lng="pt">liberalismo</trd>
  <trd lng="ru">&c_l;&c_i;&c_b;&c_je;&c_r;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
