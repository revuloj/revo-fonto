<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: retin.xml,v 1.19 2019/06/30 18:21:49 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>retin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="retin.0o">
  <kap><tld/>o</kap>
  <snc mrk="retin.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Impresebla <ref tip="super" cel="membra.0o.ANA">membrano</ref>
      de la interna fundo de la <ref tip="malprt" 
      cel="okul.0o.ANA">okulo</ref>, konsistigita de
      la ekstremo de la optika nervo kaj ricevanta la bildon de la
      eksteraj objektoj:
      <ekz>
        se la pupilo estas lar&gcirc;a, videblas la interna parto de la okulo, la <tld/>o,
        kies koloro, pro la densa reto de sangokapilaroj, estas ru&gcirc;a
        <fnt>
          <bib>Monato</bib>
          <aut>J&aacute;nos S&aacute;rk&ouml;zi</aut>
          <vrk><url ref="&Monato;/2005/008401.php"
              >La fenomeno de ru&gcirc;aj okuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        interplekti&gcirc;o de nervaj fibroj el <tld/>oj al cerbaj hemisferoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9501-sterogramoj.txt"
              >Stereogramoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_t;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_t;&c_k;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">retina</trd>
  <trdgrp lng="cs">
    <trd>retina</trd>,
    <trd>s&iacute;tnice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Netzhaut</trd>,
    <trd>Retina</trd>
  </trdgrp>
  <trd lng="en">retina</trd>
  <trd lng="es">retina</trd>
  <trd lng="fr">r&eacute;tine</trd>
  <trdgrp lng="hu">
    <trd>receh&aacute;rtya</trd>,
    <trd>retina</trd>
  </trdgrp>
  <trd lng="it">retina</trd>
  <trdgrp lng="nl">
    <trd>netvlies</trd>,
    <trd>retina</trd>
  </trdgrp>
  <trd lng="pl">siatk&oacute;wka <klr>(oka)</klr></trd>
  <trd lng="pt">retina</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_t;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>retina</trd>,
    <trd>sietnica</trd>
  </trdgrp>
</drv>
</art>
