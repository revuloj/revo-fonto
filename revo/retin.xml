<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>retin</rad>/o
</kap>
<drv mrk="retin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Impresebla membrano de la interna fundo de la okulo, konsistigita de
      la ekstremo de la optika nervo kaj ricevanta la bildon de la
      eksteraj objektoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_t;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_t;&c_k;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Netzhaut</trd>,
    <trd>Retina</trd>
  </trdgrp>
  <trd lng="en">retina</trd>
  <trd lng="fr">r&eacute;tine</trd>
  <trdgrp lng="hu">
    <trd>receh&aacute;rtya</trd>,
    <trd>retina</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>netvlies</trd>,
    <trd>retina</trd>
  </trdgrp>
  <trd lng="pl">siatk&oacute;wka <klr>(oka)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_t;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
