<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: font.xml,v 1.29 2022/04/27 17:19:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>font</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="font.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="font.0o.loko">
    <dif>
      Loko, kie <ref tip="prt" cel="akv.0o">akvo</ref> a&ubreve; alia 
      flua&jcirc;o <ref cel="ir.el0i.eksteren">eliras</ref> el la tero:
      <ekz>
        <tld/>oj, &scirc;prucigantaj akvon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 8:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i malsupreniris al la <tld/>o kaj &ccirc;erpis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen mi staras apud la <tld/>o da akvo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> staras &ccirc;e la kameloj apud la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        venos tamen vento orienta <klr>[&#8230;]</klr> kaj tiam elseki&gcirc;os lia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 13:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la komenci&gcirc;a <tld/>o de la rivero
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la subteraj <tld/>oj fluas varma akvo, iafoje malbonodora kaj abomene sala
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de natura gaso
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        miaj okuloj estus <tld/>o da larmoj
        <fnt><bib>MT</bib>, <lok>&Jer; 9:1</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="gejser.0o">gejsero</ref>
    <refgrp tip="vid">
       <ref cel="fontan.0o.konstruajxo">fontano<sncref/></ref>,
       <ref cel="put.0o">puto</ref>
    </refgrp>
    <trd lng="es">manantial</trd>
    <trdgrp lng="pt">
      <trd>fonte</trd>,
      <trd>nascente</trd>,
      <trd>olho-d'&aacute;gua</trd>,
      <trd>manancial</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_k;&c_l;&c_ju;&c_ch;</trd>,
      <trd>&c_s;&c_k;&c_v;&c_a;&c_zh;&c_i;&c_n;&c_a; <klr>(&c_n;&c_je;&c_f;&c_t;&c_ja;&c_n;&c_a;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="font.0o.elfluajxo">
    <dif>
      La elflua&jcirc;o mem:
      <ekz>
        pura <tld/>o
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        malklara <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="font.0o.originoDeForto">
    <dif>
      Origino de materia forto:
      <ekz>
        la unua radio de la suno trarompis la nubon, kaj la klara lum<tld/>o ka&ubreve;zis la
        aliformi&gcirc;on
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        trifosfata adenozino estas la precipa <tld/>o de energio en la korpo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007157.html">Vivo: Riboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        renovigeblaj energi<tld/>oj (suno, vento, biologia brula&jcirc;o)
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008164.html">La prienergiaj
          Oscar-premioj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      Trovi pli klarajn ekz-ojn por kontrastigi "~o de lumo"
      kaj "lum~o". <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="font.0o.origino">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="origin.0o.deveno">Origino</ref>, deveno, 
      <ref cel="kauxz.0o">ka&ubreve;zo</ref>:
      <ekz>
        <klr>[pro]</klr> elseki&gcirc;o de la materialaj <tld/>oj
        <klr>(rimedoj)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz> 
      <ekz>
        instruo de sa&gcirc;ulo estas <tld/>o de vivo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 13:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis denove forta kaj milda, <klr>[&#8230;]</klr> kaj de &scirc;i venadis nun ri&ccirc;a
        <tld/>o de konsolo
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksonis en &scirc;ia rememoro, vortoj <klr>[&#8230;]</klr> pri la &ccirc;efa <tld/>o de la amo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino estis senhalta <tld/>o de demandoj
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen kie estas la <tld/>o de la malfeli&ccirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vitrofarejo estis grava <tld/>o de enspezoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;erpi el komuna <tld/>o de e&ubreve;ropaj lingvoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de niaj informoj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        bovina lakto estas la plej bona <tld/>o por kalcio
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Steloj;monato2/007169.html">Lakto ne &scirc;atata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        homo malsani&gcirc;inta pro variolacio samtempe fari&gcirc;is
        <ind><tld/>o de infekto</ind> al aliaj
        <fnt>
          <aut>V. Lemelev</aut>
          <vrk>Homo, kiu savis milionojn</vrk>
          <bib>Monato</bib>
        </fnt>
        <klr>(kp <ref tip="sin" cel="kov.0ejo.FIG">kovejo<sncref/></ref>)</klr>.
        <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_k; &c_z;&c_a;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
  <trd>gener&aacute;tor</trd>,
  <trd>pramen</trd>,
  <trd>stud&aacute;nka</trd>,
  <trd>zdroj</trd>
  </trdgrp>
  <trd lng="de">Quelle</trd>
  <trd lng="en">spring</trd>
  <trd lng="es">fuente</trd>
  <trdgrp lng="fr">
    <trd>source</trd>,
    <trd>puits<klr tip="ind"> (de p&eacute;trole)</klr></trd>,
    <trd>cause<klr tip="ind"> (origine)</klr></trd>,
    <trd>origine<klr tip="ind"> (source)</klr></trd>
  </trdgrp>
  <trd lng="hu">forr&aacute;s</trd>
  <trd lng="nl">bron</trd>
  <trd lng="pl">&zacute;r&oacute;d&lstroke;o</trd>
  <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
  <trd>prame&ncaron;</trd>,
  <trd>zdroj</trd>,
  <trd>&zcaron;riedlo</trd>
  </trdgrp>
  <trd lng="sv">k&auml;lla</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_zh;&c_je;&c_r;&c_je;&#x43B;&#x43E;</trd>,
    <trd>&#x43A;&c_r;&c_i;&#x43D;&c_i;&c_ch;&#x43A;&c_a;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_r;&c_d;&#x43B;&#x43E;&c_v;&c_i;&#x43D;&c_a; (&#x43D;&c_a;&c_f;&c_t;&#x43E;&c_v;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="font.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="font.0i.eligxi">
    <dif>
      Eli&gcirc;i el <tld/>o, <ref tip="vid" cel="sxpruc.0i">&scirc;pruci</ref>:
      <ekz>
        neniam el<tld/>os lakto el mia mamo
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni vidas altan akvofalon, kiu <tld/>as el gla&ccirc;ero
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Argentino: Tondrulo &ndash;
          montego de la Andoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a; &c_v;&c_a;&c_d;&c_u; &c_z; &c_k;&c_r;&c_y;&c_n;&c_ib;&c_c;&c_y;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>quellen</trd>,
    </trdgrp>
    <trd lng="nl">ontspringen</trd>
  </snc>
  <snc mrk="font.0i.deveniDe">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="ven.de0i.kauxzo">Deveni</ref> de:
      <ekz>
        &ccirc;u &scirc;ia afablo <tld/>as el &gcirc;entilo, a&ubreve; &ccirc;u &scirc;i favoras min pro korinklino
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la hebrea estas la pralingvo, el kiu <tld/>is &ccirc;iuj postaj lingvoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        rimarkindaj estas la lingvaj eseoj kaj studoj, kiuj <tld/>as de la Budape&scirc;ta
        Skolo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        agresemo, kiu <tld/>is el malkontento pro la &gcirc;enerala stato
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        el tiu banala gesto <tld/>is gravaj konsekvencoj
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
    
    <trd lng="de">hervorgehen</trd>
    <trd lng="nl">ontspruiten</trd>
  </snc>
  <trdgrp lng="cs">
  <trd>pramenit</trd>,
  <trd>pr&yacute;&scaron;tit</trd>
  </trdgrp>
  <trd lng="de">entspringen</trd>
  <trdgrp lng="fr">
    <trd>sourdre</trd>,
    <trd>surgir<klr tip="ind"> (sourdre)</klr></trd>,
    <trd>jaillir<klr tip="ind"> (sourdre)</klr></trd>,
    <trd>d&eacute;couler</trd>,
    <trd>prendre sa <ind>source</ind></trd>,
    <trd>provenir</trd>
  </trdgrp>
  <trd lng="hu">ered</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">prameni&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &c_d;&c_zh;&c_je;&c_r;&c_je;&#x43B;&#x43E;)</trd>,
    <trd>&c_v;&c_i;&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&#8217;&#x44F;&c_v;&#x43B;&#x44F;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="font.varm0o">
  <kap>varm<tld/>o,
  <var><kap>varmo<tld/>o</kap></var></kap>
  <snc mrk="font.varm0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="super" cel="font.0o.loko"><tld lit="F"/>o</ref>
      kiu nature provizas varman akvon:
      <ekz>
        tie, en rojo, &scirc;prucas varm<tld/>oj, kie nun kamparanoj kaj maloftaj piedirantaj
        turistoj &gcirc;uante banas sin
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2003/007824.html">Tondrulo montego de la
          Andoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viva&jcirc;oj en japanaj varm<tld/>oj estas jam
        pli-malpli detale esploritaj de multaj biologoj
        <fnt>
          <aut>&Scirc;. Hori</aut>
          <vrk><url ref="&STEB;biologio/la-volvotigo-de-amikeco.pdf">
          La Volvotigo de Amikeco</url></vrk>
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Thermalquelle</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_a;&c_ch;&c_a;&c_ja; &c_k;&c_r;&c_y;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_t;&c_e;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_r;&c_y;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">source thermale</trd>
</drv>
</art>
