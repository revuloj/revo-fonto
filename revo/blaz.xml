<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>blaz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="blaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="blaz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="vezik.hauxt0o.MED">ha&ubreve;tveziko</ref>
      <ekz>
        kulturi siajn unuajn printempajn kalojn kaj <tld/>ojn
        <fnt>
          <aut>K. &Ccaron;apek, trad. -</aut>,
          <vrk>&Gcirc;ardenista marto</vrk>,
          <lok><url
          ref="http://www.volny.cz/aeh-ikeh/informilo/INFORMILO_2002_5.htm">
          </url>Informilo, 2002:5</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trd lng="en">blister</trd>
  <trdgrp lng="fr">
    <trd>ampoule<klr> (sur la peau)</klr></trd>,
    <trd>cloque<klr> (sur la peau)</klr></trd>,
    <trd>pustule</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&oacute;lyag</trd>,
    <trd>p&ouml;rsen&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>vescica<klr> (della pelle)</klr></trd>,
    <trd>vescichetta</trd>,
    <trd>vescicola</trd>
  </trdgrp>
  <trd lng="nl">blaar</trd>
  <trd lng="pl">odcisk</trd>
  <trdgrp lng="pt">
    <trd>ampola</trd>,
    <trd>empola</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j; &c_v;&c_o;&c_l;&c_d;&c_y;&c_r;&c_mol;</trd>
</drv>
</art>

