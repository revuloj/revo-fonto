  <rad>blaz</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="blaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="blaz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="vezik.hauxt0o.MED">Ha&ubreve;tveziko</ref>:
      <ekz>
        kulturi siajn unuajn printempajn kalojn kaj <tld/>ojn
        <fnt>
          <aut>K. &Ccaron;apek, trad. -</aut>,
          <vrk>&Gcirc;ardenista marto</vrk>,
          <lok><url ref="http://www.volny.cz/aeh-ikeh/informilo/INFORMILO_2002_5.htm">
          </url>Informilo, 2002:5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>(ha&#365;ta) vezik(et)o</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_d;&c_y;&c_r;</trd>,
    <trd>&c_p;&c_u;&c_h;&c_ib;&c_r; <klr>(&c_n;&c_a; &c_s;&c_k;&c_u;&c_r;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trd lng="cs">puch&yacute;&rcaron;</trd>
  <trd lng="de">Blase <klr>(Haut)</klr></trd>
  <trd lng="en">blister</trd>
  <trdgrp lng="fr">
    <trd>ampoule<klr> (sur la peau)</klr></trd>,
    <trd>cloque<klr> (sur la peau)</klr></trd>,
    <trd>pustule</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&oacute;lyag</trd>,
    <trd>p&ouml;rsen&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>vescica<klr> (della pelle)</klr></trd>,
    <trd>vescichetta</trd>,
    <trd>vescicola</trd>
  </trdgrp>
  <trd lng="nl">blaar</trd>
  <trd lng="pl">odcisk</trd>
  <trdgrp lng="pt">
    <trd>ampola</trd>,
    <trd>empola</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j; &c_v;&c_o;&c_l;&c_d;&c_y;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>1.bublina <klr>(vzduchov&aacute;)</klr></trd>,
    <trd>2.uhor</trd>,
    <trd>akn&eacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1087;&c_u;&c_h;&c_i;&c_r;&c_je;&c_c;&#1100;</trd>,
    <trd>&#1085;&c_a;&c_r;&c_i;&c_v;</trd>
  </trdgrp>
  <trd lng="zh">&#27700;&#30129; <pr>shu&#464;p&#224;o</pr></trd>
</drv>
