<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>absorb</rad>/i
</kap>
<drv mrk="absorb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Okupi ekskluzive la spiriton a&ubreve; la atenton:
      <ekz>
        lia laboro
        <tld/>as tutan lian penson;
      </ekz>
      <ekz>
        <tld/>ita per la legado li ne rimarkis mian eniron;
      </ekz>
      <ekz>
        la esprimo de lia viza&gcirc;o estis <tld/>ita<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="sorb.0i">sorbi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>absorbieren</trd>,
    <trd>in <ind>Bann</ind> <ind>schlagen</ind></trd>,
    <trd>auf sich <ind>ziehen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>absorber<klr tip="ind"> (l'esprit)</klr></trd>,
    <trd>accaparer<klr tip="ind"> (l'attention)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>absorber</trd>,
    <trd>ocupar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lefoglal</trd>,
    <trd>lek&ouml;t</trd>
  </trdgrp>
  <trd lng="nl">opslorpen</trd>
  <trdgrp lng="pt">
    <trd>absorver</trd>,
    <trd>preocupar</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>f&auml;ngsla</trd>,
    <trd>upptaga (sinnet)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol; &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;&c_m;</trd>
  </trdgrp>
</drv>
</art>
