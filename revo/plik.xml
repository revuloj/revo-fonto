<art mrk="$Id: plik.xml,v 1.23 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>plik</rad>/o
</kap>
<drv mrk="plik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plik.0o.haroj">
    <dif>
      Felteca kuna&jcirc;o de haroj:
      <ekz>
        perukon sian nomis li ora lanaro, la&ubreve; ni, kun <tld/>o estis pli &gcirc;usta komparo
        <fnt>
          <lok><url
          ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=pliko">edu.i-lo.tarnow.pl</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/b/bf/Hans_mit_Scheuklappe_und_Weichselzopf.jpg/205px-Hans_mit_Scheuklappe_und_Weichselzopf.jpg"
    prm="PD"><tld/>o &ccirc;e la kolhararo de &leftquot;La sa&gcirc;a Ha&ccirc;jo&rightquot;
        <fnt>
          <aut>Karl Krall: &leftquot;Denkende Tiere&rightquot;, Leipzig 1912, p. 6</aut>
          <lok><url
          ref="&WCW;/File:Hans_mit_Scheuklappe_und_Weichselzopf.jpg">&WCW;/File:Hans_mit_Scheuklappe_und_Weichselzopf.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <snc mrk="plik.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> de kraniha&ubreve;to,
      karakterizata per
      troa grasa sekrecio.
    </dif>
  </snc>
  <adm>
    Necesas kontroli la tradukojn post aldono de la &scirc;ajne pli fundamenta senco (hara&jcirc;o).
  </adm>
  <trd lng="be">&c_k;&c_a;&c_w;&c_t;&c_u;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>koldoun</trd>,
    <trd>zplst&ecaron;n&iacute; vlas&uring;</trd>
  </trdgrp>
  <trd lng="de">Weichselzopf</trd>
  <trdgrp lng="en">
    <trd>plica polonica</trd>,
    <trd>Polish plait</trd>
  </trdgrp>
  <trd lng="es">plica <klr>(med.)</klr></trd>
  <trdgrp lng="fr">
    <trd>plique<klr tip="amb"> polonaise</klr></trd>,
    <trd>trichoma</trd>
  </trdgrp>
  <trd lng="hu">lengyelf&uuml;rt</trd>
  <trd lng="ja">&#x7CFE;&#x9AEA;&#x75C7;  <pr>&#12365;&#12421;&#12358;&#12399;&#12388;&#12375;&#12423;&#12358;</pr></trd>
  <trd lng="nl">Poolse <ind>vlecht</ind></trd>
  <trd lng="pl">ko&lstroke;tun</trd>
  <trdgrp lng="pt">
    <trd>plica</trd>,
    <trd>tricoma</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_t;&c_u;&c_n;</trd>
  <trd lng="sk">mazotok lebe&ccaron;nej ko&zcaron;e</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_v;&c_t;&c_u;&#x43D;</trd>
</drv>
</art>
