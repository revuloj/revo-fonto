<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aboc.xml,v 1.35 2023/02/05 17:24:25 revo Exp $">
<kap>
  <rad>aboc</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aboc.0o">
  <kap><tld/>o</kap>
  <snc mrk="aboc.0o.alfabeto">
    <dif>
      <ref tip="dif" cel="aboc.0o">ABC, a&middot;bo&middot;co</ref>:
      <ekz>
        sur la monumento estas <tld/>e enskribitaj 50 familiaj nomoj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2012/010407.php"
              >Filantropa pomo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_l;&c_mol;&c_f;&c_a;&c_b;&c_e;&c_t;</trd>,
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_e;&c_c;&c_e;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>abec&eacute;</trd>,
      <trd>abecedari</trd>
    </trdgrp>
    <trd lng="da">abc</trd>
    <trdgrp lng="de">
      <trd>Abc</trd>,
      <trd>Alfabet</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ABC's</trd>,
      <trd>alphabet</trd>
    </trdgrp>
    <trd lng="es">abecedario</trd>
    <trd lng="fa">&a_A;&a_l;&a_f;&a_b;&a_A;</trd>
    <trd lng="fr">alphabet</trd>
    <trd lng="hr">abeceda</trd>
    <trd lng="hu">&aacute;b&eacute;c&eacute;</trd>
    <trd lng="nl">alfabet</trd>
    <trdgrp lng="oc">
      <trd>abec&eacute;</trd>,
      <trd>abecedari</trd>,
      <trd>be&ccedil;ar&ograve;las</trd>
    </trdgrp>    
    <trdgrp lng="pl">
     <trd>abecad&lstroke;o</trd>,
     <trd>alfabet</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>alfabeto</trd>,
      <trd>abeced&aacute;rio</trd>
    </trdgrp>
    <trd lng="ro">alfabet</trd>
    <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;</trd>
    <trd lng="sk">abeceda</trd>
    <trd lng="sl">abeceda</trd>
    <trd lng="sv">alfabet</trd>
    <trdgrp lng="th">
      <trd>&#3605;&#3633;&#3623;&#3629;&#3633;&#3585;&#3625;&#3619;
      (&#3627;&#3621;&#3634;&#3618;&#3605;&#3633;&#3623;)</trd>,
      <trd>&#3594;&#3640;&#3604;&#3605;&#3633;&#3623;&#3629;&#3633;&#3585;&#3625;&#3619;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aboc.0o.alfabetumo">
    <dif><ref tip="dif" cel="alfabe.0umo">Alfabetumo</ref>:
      <ekz>
        la malnova <tld/>olibro ku&scirc;is kun la unua pa&gcirc;o supre kaj &ccirc;i 
         tiu pa&gcirc;o estas en &gcirc;i la plej grava
         <fnt><bib>Fab4</bib>, <lok>Abocolibro</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="libr.aboco0o"><tld/>olibro</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
      <trd>&c_l;&c_je;&c_m;&c_a;&c_n;&c_t;&c_a;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>abecedari</trd>,
      <trd>beceroles <klr>(llibre)</klr></trd>
    </trdgrp>    
    <trd lng="da">l&aelig;rebog</trd>
    <trdgrp lng="de">
      <trd>Fibel</trd>,
      <trd>Lesebuch</trd>
    </trdgrp>
    <trd lng="fa">&f_k;&a_t;&a_A;&a_b; <ind>&a_A;&a_l;&a_f;&a_b;&a_A;</ind></trd>
    <trdgrp lng="fr">
      <trd>ab&eacute;c&eacute;daire</trd>,
      <trd>ABC<klr tip="ind"> (ab&eacute;c&eacute;daire)</klr></trd>
    </trdgrp>
    <trd lng="hu">&aacute;b&eacute;c&eacute;sk&ouml;nyv</trd>
    <trd lng="hr">abecedar</trd>
    <trd lng="pl">elementarz</trd>
    <trd lng="pt">cartilha</trd>
    <trd lng="ro">abecedar</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_u;&c_k;&c_v;&c_a;&c_r;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">&scaron;labik&aacute;r</trd>
    <trd lng="sl">abecednik</trd>
    <trd lng="sv">abc-bok</trd>
    <trd lng="th">&#3627;&#3609;&#3633;&#3591;&#3626;&#3639;&#3629; &#3585;&#3586;&#3588;</trd>
  </snc>
  <snc mrk="aboc.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elementa instruo pri iu scio:
      <ekz>
        <tld/>o de fiziko, de sennaciismo.
      </ekz>
    </dif>
    <ref tip="vid" cel="kompen.0o">kompendio</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_s;&c_n;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_ch;&c_a;&c_t;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="ca">beceroles <klr>(rudiments)</klr></trd>    
    <trdgrp lng="de">
      <trd>Anfangsgr&uuml;nde</trd>,
      <trd>kleines Einmaleins</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ABC's</trd>,
      <trd>primer</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>ABC</trd>,
      <trd>abec&eacute;</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_l;&a_f;&a_b;&a_A;</trd>,
      <trd>&a_m;&a_b;&a_A;&a_n;&f_y;</trd>,
      <trd>&a_m;&a_q;&a_d;&a_m;&a_A;&a_t;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ABC<klr tip="ind"> (rudiments)</klr></trd>,
      <trd>B-A-BA</trd>,
      <trd><ind>base</ind>s<klr tip="ind"> (d'un savoir)</klr></trd>,
      <trd><ind>rudiment</ind>s<klr tip="ind"> (d'un savoir)</klr></trd>
    </trdgrp>
    <trd lng="hu">&aacute;b&eacute;c&eacute;</trd>
    <trd lng="hr">osnove</trd>
    <trdgrp lng="nl">
      <trd>ABC</trd>,
      <trd>eerste beginselen</trd>
    </trdgrp>  
    <trdgrp lng="pt">
      <trd>ABC</trd>,
      <trd>abec&ecirc;</trd>
    </trdgrp>
    <trdgrp lng="pl">
       <trd>ABC</trd>,
       <trd>podstawa</trd>,
       <trd>minimum</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>abc</trd>,
      <trd>baz&abreve;</trd>,
      <trd>minimum</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="sk">z&aacute;klady</trd>
    <trd lng="sv">abc</trd>
    <trd lng="ms">abc huruf</trd>
  </snc>
  <trd lng="cs">abeceda</trd>
  <trd lng="he">&alef;&lamed;&fpe; &bet;&yod;&tav;</trd>
  <trd lng="it">abc</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_l;&c_f;&c_a;&c_v;&c_ib;&c_t;</trd>,
    <trd>&c_a;&c_b;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_k;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <adm>
     Diskuto en PIV-forumo (https://groups.io/g/piv-teamo/topic/aboc_o/96591204)
     traktas la demandon, &ccirc;u pro Z-a uzo abocolibro, aboco/a kaj Kabea ABC- kaj
     samakcentado de &ccirc;iu silabo, ne temas pri radiko aboc/ kiel prezentas &gcirc;in
     PV, sed pri elskribita mallongigo a-bo-co, kompareble kun "kotopo".
     Jam registri&gcirc;is tie aparta drv ABC, a/bo/co, sed necesas ankaora&ubreve; movi
     la duan, trian sencon tien.
  </adm>
</drv>
</art>
