<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalembur.xml,v 1.13 2023/08/29 17:27:25 revo Exp $">
<kap>
    <rad>kalembur</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kalembur.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalembur.0o.vortludo">
  <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="lud.vort0o.LIN">Vortludo</ref> per
      samskribaj a&ubreve; samsonaj diverssignifaj vortoj:
      <ekz>
        La stranga butiko &#8208; last-ranga butiko
        <fnt>
          <aut>Raymond Schwartz</aut>
        </fnt>;
      </ekz>
      <ekz>
         Insigne la &ccirc;apelo / Surportas bantornamon / Dum juna koko
         pruvas /
         Fervore sian amon / Kaj malgra&ubreve; diverssenco / Ne estas
         diferenco
         <klr>(En amba&ubreve; okazoj estas ... kokardo.)</klr>
         <fnt>
           <aut>Raymond Schwartz</aut>,
           <vrk><url ref="&Steloj;butiko/diferenco.html">La
           Stranga Butiko</url></vrk>,
           <lok>Ne estas diferenco</lok>.
         </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_m;&c_b;&c_u;&c_r;</trd>
  <trd lng="cs">kalamb&uacute;r</trd>
  <trd lng="de">Kalauer</trd>
  <trdgrp lng="en">
    <trd>pun</trd>,
    <trd>calembour</trd>
  </trdgrp>
  <trd lng="es">calambur</trd>
  <trd lng="fr">calembour</trd>
  <trd lng="io">kalemburo</trd>
  <trd lng="it">calembour</trd>
  <trd lng="ja">&#39364;&#27922;&#33853; [<ind>&#12384;&#12376;&#12419;&#12428;</ind>]</trd>
  <trd lng="ka">&#4313;&#4304;&#4314;&#4304;&#4315;&#4305;&#4323;&#4320;&#4312;</trd>
  <trd lng="lt">kalamb&#5691;ras</trd>
  <trdgrp lng="pl">
    <trd>kalambur</trd>,
    <trd>kalambur</trd>
  </trdgrp>
  <trd lng="pt">calembur</trd>
  <trdgrp lng="ro">
    <trd>calambur</trd>,
    <trd>calambur</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_a;&c_m;&c_b;&c_u;&c_r;</trd>
  <trd lng="sk">kalamb&uacute;r</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_l;&c_a;&c_m;&c_b;&c_u;&c_r;</trd>,
    <trd>&c_g;&c_r;&c_a; &c_s;&c_l;&c_ib;&c_v;</trd>
  </trdgrp>
</drv>
</art>

