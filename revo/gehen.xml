<vortaro>
<art mrk="$Id$">
<kap><rad>gehen</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="gehen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIB</uzo>
  <snc>
    <dif>
      Infero.
    </dif>
    <adm>
      La biblio uzas la formon &leftquot;Gehena&rightquot;, kaj &gcirc;in akuzativigas.
      <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>Gehenna</trd>,
      <trd>H&ouml;lle</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_m;&c_je;&c_t;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_k;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&eacute;henne</trd>,
    <trd>enfer</trd>
  </trdgrp>
  <trd lng="nl">hel</trd>
  <trdgrp lng="hu">
    <trd>gyehenna</trd>,
    <trd>pokol</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>geena</trd>, 
   <trd>inferno<klr tip="amb"> (b&iacute;blico)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_je;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_i;&c_s;&c_p;&c_o;&c_d;&c_n;&c_ja;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
