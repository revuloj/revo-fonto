<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gehen.xml,v 1.21 2023/11/11 10:00:39 revo Exp $">
<kap><rad>gehen</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="gehen.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      <ref tip="dif" cel="infer.0o.KRI">Infero</ref>:
      <ekz>
        se vi darfus, per pafil' / Lugubra ido de la Vermo / Vi min ekspedus al ekzil' /
        A&ubreve; al geheno, al ekstermo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Satano kiel prezentita en la Na&ubreve;a Cirklo de <tld lit="G"/>o en Fajrego
        de <nom>Dante Alighieri</nom>, ilustrita de <nom>Gustave Dor&eacute;</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4001">Satano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      La biblio uzas la formon &leftquot;Gehena&rightquot;, kaj
      &gcirc;in akuzativigas.
      <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>Gehenna</trd>,
      <trd>H&ouml;lle</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_m;&c_je;&c_t;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_k;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">gehenna</trd>
  <trdgrp lng="cs">
    <trd>gehenna</trd>,
    <trd>peklo</trd>
  </trdgrp>
  <trd lng="en">gehenna</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_h;&a_n;&a_m;</trd>,
    <trd>&a_d;&a_w;&a_z;&a_kh;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&eacute;henne</trd>,
    <trd>enfer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyehenna</trd>,
    <trd>pokol</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5730;&#x7344;  <pr>&#x3058;&#x3054;&#x304F;</pr></trd>,
    <trd>&#x82E6;&#x96E3;&#x306E;&#x5730;  <pr>&#x304F;&#x306A;&#x3093;&#x306E;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="nl">hel</trd>
  <trdgrp lng="pt">
   <trd>geena</trd>,
   <trd>inferno<klr tip="amb"> (b&iacute;blico)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_je;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_i;&c_s;&c_p;&c_o;&c_d;&c_n;&c_ja;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">peklo</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x435;&#x454;&#x43D;&#x430;</trd>,
    <trd>&#x43F;&#x435;&#x43A;&#x43B;&#x43E;</trd>
  </trdgrp>
  <trd lng="zh">&#x9634;&#x66F9; <pr>y&#x12B;nc&#xE1;o</pr></trd>
</drv>
</art>
