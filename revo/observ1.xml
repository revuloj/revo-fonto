<art mrk="$Id: observ1.xml,v 1.23 2024/01/30 16:09:13 revo Exp $">
<kap>
   <ofc>3</ofc>
   <rad>observatori</rad>/o  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="observ1.0o">
   <kap><tld/>o</kap>
   <snc mrk="observ1.0o.SCI">
     <uzo tip="fak">AST</uzo>
     <uzo tip="fak">MET</uzo>
     <dif>
       Scienca <ref tip="dif" cel="observ.0ejo.SCI">observejo</ref>,
       precipe veter- a&ubreve;
       <ref tip="vid" cel="observ.stel0ejo.AST">stelobservejo</ref>:
      <ekz>
        la maljuna avo kapsignis en la direkto al la <klr>[&#x2026;]</klr>
        kalendaro kun la Ronda Turo, la <tld/>o, <klr>[&#x2026;]</klr> kaj
        li diris: <nom>Tycho Brahe</nom>, li anka&ubreve; estis viro, kiu povosciis apliki
        glavon, ne por haki karnon kaj oston, sed por trahaki pli klaran 
        vojon inter la steloj de la firmamento
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;tiu vere estas planedo, la unua planedo ekster nia sunsistemo, 
        kiu estis bildigita&rightquot;, asertas <nom>Gael Chauvin</nom>, skipestro kaj astronomo
        de la E&ubreve;ropa Suda <tld lit="O"/>o (ESO)
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2006/008796.php">Unua fotografa&jcirc;o de ekstersunsistema planedo</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_e;&c_r;&c_v;&c_a;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>observato&rcaron;</trd>,
    <trd>observatorium</trd>,
    <trd>observato&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Observatorium</trd>
  <trd lng="en">observatory</trd>
  <trd lng="fr">observatoire<klr tip="ind"> (science)</klr></trd>
  <trdgrp lng="hu">
     <trd>csillagvizsg&aacute;l&oacute;</trd>,
     <trd>obszervat&oacute;rium</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89B3;&#x6E2C;&#x6240;  <pr>&#x304B;&#x3093;&#x305D;&#x304F;&#x3058;&#x3087;</pr></trd>,
    <trd>&#x5929;&#x6587;&#x53F0;  <pr>&#x3066;&#x3093;&#x3082;&#x3093;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x6C17;&#x8C61;&#x53F0;  <pr>&#x304D;&#x3057;&#x3087;&#x3046;&#x3060;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">observatorium</trd>
  <trd lng="pl">obserwatorium</trd>
  <trd lng="pt">observat&oacute;rio</trd>
  <trdgrp lng="ru">
     <trd>&c_o;&c_b;&c_s;&c_je;&c_r;&c_v;&c_a;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>,
     <trd>&c_m;&c_je;&c_t;&c_je;&c_o;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
   </trdgrp>
  <trd lng="sk">hvezd&aacute;re&ncaron;</trd>
  <trd lng="uk">&#x43E;&#x431;&#x441;&#x435;&#x440;&#x432;&#x430;&#x442;&#x43E;&#x440;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5929;&#x6587;&#x53F0; <pr>ti&#x101;nw&#xE9;nt&#xE1;i</pr></trd>,
    <trd>&#x77AD;&#x671B;&#x53F0; <pr>li&#xE0;ow&#xE0;ngt&#xE1;i</pr></trd>,
    <trd>&#x77AD;&#x671B;&#x81FA; <pr>li&#xE0;ow&#xE0;ngt&#xE1;i</pr></trd>,
    <trd>&#x89C0;&#x5BDF;&#x6240; <pr>gu&#x101;nch&#xE1;su&#x1D2;</pr></trd>,
    <trd>&#x89C2;&#x5BDF;&#x6240; <pr>gu&#x101;nch&#xE1;su&#x1D2;</pr></trd>,
    <trd>&#x89C0;&#x6E2C;&#x7AD9; <pr>gu&#x101;nc&#xE8;zh&#xE0;n</pr></trd>,
    <trd>&#x89C2;&#x6D4B;&#x7AD9; <pr>gu&#x101;nc&#xE8;zh&#xE0;n</pr></trd>,
    <trd>&#x76D1;&#x6D4B;&#x6240; <pr>ji&#x101;nc&#xE8;s&#x1D4;p</pr></trd>,
