<vortaro>
<art mrk="$Id$">
<kap><rad>Beograd</rad>/o</kap>
<drv mrk="beograd.B0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="urb.0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="serb.S0ujo">Serbujo</ref> 
      (20&#730;28'E, 44&#730;49'N), situanta
      &ccirc;e la riveroj <ref cel="sav1.0o">Savo</ref> kaj 
      <ref tip="vid" cel="danub.D0o">Danubo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_ja;&c_l;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="bg">&c_B;&c_je;&c_l;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="de">Belgrad</trd>
  <trd lng="en">Belgrade</trd>
  <trd lng="es">Belgrado</trd>
  <trd lng="fa">&a_b;&a_l;&f_g;&a_r;&a_A;&a_d;</trd>
  <trdgrp lng="hu">
    <trd>Belgr&aacute;d</trd>,
    <trd>N&aacute;ndorfeh&eacute;rv&aacute;r</trd>
  </trdgrp>
  <trd lng="lat">Belgradum</trd>
  <trd lng="nl">Belgrado</trd>
  <trd lng="pl">Belgrad</trd>
  <trd lng="ru">&c_B;&c_je;&c_l;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="sk">Belehrad</trd>
</drv>
</art>
