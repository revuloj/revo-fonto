<kap><ofc>10</ofc><rad>Beograd</rad>/o</kap>
<drv mrk="beograd.B0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="urb.0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="serb.S0ujo">Serbujo</ref> 
      (20&#x2DA;28'E, 44&#x2DA;49'N), situanta &ccirc;e la riveroj Savo kaj 
      <ref tip="vid" cel="danub.D0o">Danubo</ref>:
      <ekz>
        en <tld/>o, en la lastaj tagoj de junio 2004, fini&gcirc;is festivalo de sefarda
        kulturo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2004/008382.html">Sefardoj festivalis</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        registaroj en <tld/>o, Skopje kaj Ateno klopodis malhelpi &ccirc;iujn kontaktojn kaj
        lan&ccirc;is kontra&ubreve;albanan ofensivon en diplomatiaj rondoj en E&ubreve;ropo kaj Usono
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007989.html">Sen solvo neniu paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malsukcesis kelkaj provoj de Danuba Komisiono, organiza&jcirc;o en kiu membras &ccirc;iuj 11
        landoj tra kiuj fluas la rivero, konvinki <tld/>on forigi la ruinojn de la
        detruitaj pontoj
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006857.php">Danuba &scirc;iptrafiko paralizita</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="he">&#x5D1;&#x5DC;&#x5D2;&#x5E8;&#x5D3;</trd>
  <trd lng="ja">&#x30D9;&#x30AA;&#x30B0;&#x30E9;&#x30FC;&#x30C9;</trd>
  <trd lng="lat">Belgradum</trd>
  <trd lng="be">&c_B;&c_ja;&c_l;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="bg">&c_B;&c_je;&c_l;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="cs">B&ecaron;lehrad</trd>
  <trd lng="de">Belgrad</trd>
  <trd lng="en">Belgrade</trd>
  <trd lng="es">Belgrado</trd>
  <trd lng="fa">&a_b;&a_l;&f_g;&a_r;&a_A;&a_d;</trd>
  <trd lng="fr">Belgrade</trd>
  <trdgrp lng="hu">
    <trd>Belgr&aacute;d</trd>,
    <trd>N&aacute;ndorfeh&eacute;rv&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">Belgrado</trd>
  <trd lng="pl">Belgrad</trd>
  <trd lng="ru">&c_B;&c_je;&c_l;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="sk">Belehrad</trd>
  <trd lng="uk">&c_B;&c_je;&#x43B;&c_g;&c_r;&c_a;&c_d;</trd>
</drv>
