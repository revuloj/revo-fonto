<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaik.xml,v 1.5 2023/11/20 21:22:57 revo Exp $">
<kap>
  <rad>kaik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kaik.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaik.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Dumasta <ref tip="super" cel="sxip.vel0o.SHI">vel&scirc;ipeto</ref>
      iam komune uzata en Mezmaro:
      <ekz>
        grandare glitas surakve malpezaj, pla&ccirc;aj &leftquot;<tld/>oj&rightquot;
        <fnt>
          <bib>Lanti</bib>
          <lok>7:2</lok>
        </fnt>;
        </ekz>
      <ekz>
        la sultano alvenis en orkolora <tld/>o ornamita de eburo,
        ebono, perlamoto kaj oro
        <fnt>
          <aut>J. Warren</aut>
          <vrk>Al orgeno donacita...</vrk>
          <lok><url ref="http://www.esperanto-gb.org/lbe/arkivo/956/index.html">
          La Brita Esperantisto, 2004:956</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/5/51/Kaik.svg"
      prm="CC BY 2.5"
      lrg="342"
      >desegno de iu <tld/>o
        <fnt>
          <aut>ThoKay, propra verko</aut>
          <lok><url ref="&WCW;/File:Kaik.svg"
              >https://commons.wikimedia.org/wiki/File:Kaik.svg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="de">
    <trd>Kaik</trd>,
    <trd>Kajik</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ca&iuml;que</trd>,
    <trd>gulet</trd>
  </trdgrp>
  <trd lng="it">caicco</trd>
  <trd lng="pl">kaik</trd>
  <trd lng="ro">kaike</trd>
  <trd lng="tr">gulet</trd>
  <trd lng="uk">&c_k;&c_a;&c_ji;&c_k; (&c_l;&c_je;&c_g;&c_k;&c_i;&c_j; &c_t;&c_u;&c_r;&c_je;&c_c;&c_mol;&c_k;&c_i;&c_j; &c_ch;&c_o;&c_v;&c_je;&c_n;)</trd>
</drv>
</art>

