<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konzer.xml,v 1.14 2023/09/01 15:46:43 revo Exp $">
<kap>
  <rad>konzern</rad>/o
</kap>
<drv mrk="konzer.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="organi2.0o">Organizo</ref>
      de financa interligo inter memstaraj, sendepende
      restantaj entreprenoj:
      <ekz>
        temas pri akaparado de merkato, pri formado de monopoloj, de trustoj, <tld/>oj
        kiuj regas la sistemon kaj eliminas konkurencon
        <fnt>
       <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2000/006861.php">Milito kaj paco</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda plurnacia <tld/>o okupi&gcirc;anta pri merkat-esploroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2003/008062.html">Spektantoj spektataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la registaro reprezentu ne la interesojn de la elektro<tld/>oj, sed la
        interesojn de la popolo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2011/010418.php">Adia&ubreve;o al nukleo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        grandaj energi<tld/>oj, kiuj liveras nafton kaj tergason per multmilkilometraj
        duktoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010595.php">Novtipa ekologia turismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;stra energi<tld/>o <nom>OMV</nom> kaj <klr>[&#x2026;]</klr> la rusa gas<tld/>o
        <nom>Gazprom</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011363.php">Rusa tergaso por E&ubreve;ropa Unio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kartel.0o">kartelo</ref>,
      <ref cel="trust.0o">trusto</ref>.
    </refgrp>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="koncer.0o">koncerno</ref>.
  </rim>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_n;</trd>
  <trd lng="de">Konzern</trd>
  <trd lng="hu">konszern</trd>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30C4;&#x30A7;&#x30EB;&#x30F3;</trd>
  <trd lng="nl">concern</trd>
  <trd lng="pl">koncern</trd>
  <trd lng="pt">cons&oacute;rcio <klr tip="amb">(de empresas
  aut&ocirc;nomas)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_n;</trd>
</drv>
</art>
