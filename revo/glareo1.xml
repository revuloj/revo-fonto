<vortaro>
<art mrk="$Id: glareo1.xml,v 1.11 2019/01/27 17:10:18 revo Exp $">
<kap>
  <rad>glareoled</rad>/oj
</kap>
<drv mrk="glareo1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="hxarad.0oformaj">&hcirc;aradrioformaj
      birdoj</ref> (<trd lng="la">Glareolidae</trd>):
      <ekz>
        glareoledoj <klr>[&#8230;]</klr> trovi&gcirc;as en dezertoj kaj savanoj kaj kelkaj specioj
        rilatas al akvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=197217"
              >Glareoledoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">glare&ograve;lids</trd>
  <trd lng="cs">ouhorl&iacute;kovit&iacute;</trd>
  <trd lng="de">Brachschwalben<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">glar&eacute;olid&eacute;s</trd>
  <trd lng="hu">sz&eacute;kics&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">renvogels en vorkstaartplevieren<klr tip="ind">
  (fam.)</klr></trd>
  <trd lng="pt">glareol&iacute;deos</trd>
  <trd lng="ru">&c_t;&c_i;&c_r;&c_k;&c_u;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">prielo&zcaron;n&iacute;kovit&eacute;</trd>
</drv>
</art>
