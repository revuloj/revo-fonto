<vortaro>
  <art mrk="$Id: blez.xml,v 1.4 2023/08/31 08:48:52 revo Exp $">
    <kap>
      <rad>Blez</rad>/o
    </kap>

<drv mrk="blez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="blez.0o.urbo">
    <dif>
      <ref tip="malprt" cel="franc.F0ujo">Franca</ref>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> 
      &ccirc;e la rivero Luaro, inter Orleano kaj Turo:
      <ekz>
        liajn profitojn el la entrepreno li intencis elspezi por helpi al malri&ccirc;uloj en
        la urbo <tld/>o
        <fnt>
          <vrk><url ref="https://legacy.esperanto.org.uk/lbe/arkivo/982/14.html">Filozofo kaj
          busservo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en 1338 okaze de la geedzi&gcirc;o de Karolo de <tld/>o kaj de Johana de Pentievro,
        turniro <klr>[&#x2026;]</klr> estis organizita
        <fnt>
          <vrk><url ref="http://www.bertin.biz/index.php?titre=du_Guesclin.htm">Ek al la servo de
          Francio (1358)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_B;&c_l;&c_ju;&c_a; <klr>(&c_g;&c_o;&c_r;&c_a;&c_d; &c_u; &c_F;&c_r;&c_a;&c_n;&c_c;&c_y;&c_ib;)</klr></trd>
    <trd lng="de">Blois</trd>
    <trd lng="fr">Blois</trd>
  </snc>
  <trd lng="ja">&#x30D6;&#x30ED;&#x30EF;</trd>
  <trd lng="uk">&c_B;&#x43B;&c_u;&c_a;</trd>
</drv>

  </art>

