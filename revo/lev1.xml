<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lev</rad>/o
</kap>
<drv mrk="lev1.0o">
  <kap><tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="lev1.0o.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de
      <ref cel="bulgar.B0ujo">Bulgarujo</ref>.
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun
    <ref cel="lev.0o">levo</ref>.
  </refgrp>
  <trd lng="be">&c_l;&c_je;&c_w; <klr 
tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; 
&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_l;&c_je;&c_v;</trd>
  <trd lng="ca">lev</trd>
  <trd lng="de">Lev</trd>
  <trd lng="fr">lev</trd>
  <trd lng="hu">leva</trd>
  <trd lng="id">lev</trd>
  <trd lng="nl">lev</trd>
  <trd lng="pl">lewa<klr tip="amb"> (waluta Bu&lstroke;garii)</klr></trd>
  <trd lng="pt">lev</trd>
  <trdgrp lng="sk">
    <trd>zdvih</trd>,
    <trd>zvýšenie</trd>
  </trdgrp>
</drv>
</art>

