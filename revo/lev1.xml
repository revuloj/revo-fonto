<art mrk="$Id: lev1.xml,v 1.18 2023/10/19 05:15:35 revo Exp $">
<kap>
  <rad>lev</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lev1.0o">
  <kap><tld/>o</kap>
  <snc mrk="lev1.0o.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de <ref cel="bulgar.B0ujo">Bulgarujo</ref>:
      <ekz>
        la biblioteko <klr>[&#x2026;]</klr> tenas &gcirc;is nun konstantan prezon de po 5 <tld/>oj
        (egalvalore al 2,5 e&ubreve;roj) por unuopa volumo
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2007/009047.php">Gratulinda bulgara iniciato</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;efurbo estas produktata 33&#x202F;% de la MEP de la lando a&ubreve; &ccirc;irka&ubreve; 18,6
        miliardoj da <tld/>oj por la jaro 2007
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2009/009875.php">Obstrukco de la surstrata cirkulado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun
    <ref cel="lev.0o">levo</ref> (altigo).
  </refgrp>
  <trd lng="be">&c_l;&c_je;&c_w; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; 
&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_l;&c_je;&c_v;</trd>
  <trd lng="ca">lev</trd>
  <trd lng="de">Lev</trd>
  <trd lng="fr">lev</trd>
  <trd lng="hu">leva</trd>
  <trd lng="id">lev</trd>
  <trd lng="ja">&#x6301;&#x3061;&#x4E0A;&#x3052;&#x308B;&#x3053;&#x3068;  <pr>&#x3082;&#x3061;&#x3042;&#x3052;&#x308B;&#x3053;&#x3068;</pr></trd>
  <trd lng="nl">lev</trd>
  <trd lng="pl">lewa<klr tip="amb"> (waluta Bu&lstroke;garii)</klr></trd>
  <trd lng="pt">lev</trd>
  <trdgrp lng="sk">
    <trd>zdvih</trd>,
    <trd>zv&yacute;&scaron;enie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zdvih</trd>,
    <trd>zdvi&zcaron;en&iacute;</trd>,
    <trd>zv&yacute;&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x439;&#x43E;&#x43C;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x43D;&#x44F;&#x442;&#x442;&#x44F;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x43D;&#x456;&#x43C;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x432;&#x438;&#x449;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

