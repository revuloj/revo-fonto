<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: analit.xml,v 1.29 2022/03/12 10:18:06 revo Exp $">
<kap>
<rad>analitik</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="analit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 17</lok></fnt>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="matema.0o">matematiko</ref>,
      studanta
      <ref tip="prt" cel="funkci.0o.MAT">funkciojn</ref> per
      <ref tip="prt" cel="limes.0o">limesoj</ref>,
      <ref tip="prt" cel="deriv.0ajxo.MAT">deriva&jcirc;oj</ref> kaj
      <ref tip="prt" cel="integr2.0o">integraloj</ref>:
      <ekz>
        geometrio pristudas apartajn punktojn de kurboj, <tld/>o pristudas apartajn
        punktojn de la solvoj de diferencialaj ekvacioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3386">Punkto (matematiko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
  MatVort tradukas "analitiko" per "diferenciala kaj integrala
  kalkuloj". Gxi havas ankaux "matematika analizo" sxajne kun 
  la intencata senco de nia "analitiko"... Krome vd adm sub
  "geometrio (analitika)". <aut>MB</aut>
  </adm>
  <trd lng="be">&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_a;&c_n;&c_a;&c_l;&c_ib;&c_z;</trd>
  <trd lng="bg">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i; &c_a;&c_n;&c_a;&c_l;&c_i;&c_z;</trd>
  <trd lng="de">Analysis</trd>
  <trd lng="en">analysis</trd>
  <trd lng="es">anal&iacute;tica <klr tip="amb"> (matem&aacute;ticas)</klr></trd>
  <trd lng="fa">&a_A_madda;&a_n;&a_A;&a_l;&f_y;&a_z;</trd>
  <trdgrp lng="fr">
    <trd>analyse<klr tip="ind"> (math&eacute;matique)</klr></trd>,
    <trd>analyse math&eacute;matique</trd>
  </trdgrp>
  <trd lng="he">&#x5E0;&#x5D9;&#x5EA;&#x5D5;&#x5D7;</trd>
  <trd lng="hu">anal&iacute;zis</trd>
  <trdgrp lng="pl">
    <trd>analiza</trd>,
    <trd>analiza matematyczna</trd>
  </trdgrp> 
  <trd lng="nl">analytische <ind>wiskunde</ind></trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
  <ind>&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;</ind></trd>
  <trd lng="sk">anal&yacute;za</trd>
  <trd lng="sk">analytika</trd>
  <trdgrp lng="cs">
    <trd>diferenci&aacute;ln&iacute; a integr&aacute;ln&iacute; po&ccaron;et</trd>,
    <trd>matematick&aacute; anal&yacute;za</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x442;&#x435;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439; &#x430;&#x43D;&#x430;&#x43B;&#x456;&#x437;</trd>,
    <trd>&#x430;&#x43D;&#x430;&#x43B;&#x456;&#x442;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x430;&#x43D;&#x430;&#x43B;&#x456;&#x437; &#x43D;&#x435;&#x441;&#x43A;&#x456;&#x43D;&#x447;&#x435;&#x43D;&#x43D;&#x43E; &#x43C;&#x430;&#x43B;&#x438;&#x445;</trd>
  </trdgrp>
</drv>

<drv mrk="analit.0a">
  <kap><tld/>a</kap>
  <refgrp tip="vid">
    <ref cel="funkci.analitika0o">analitika funkcio</ref>,
    <ref cel="geomet1.analitika0o">analitika geometrio</ref>.
  </refgrp>
  <trd lng="be">&c_a;&c_n;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_l;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="de">analytisch</trd>
  <trd lng="en">analytical</trd>
  <trd lng="fa">&a_t;&a_H;&a_l;&f_y;&a_l;&f_y;</trd>
  <trd lng="hu">analitikus</trd>
  <trd lng="sk">analytick&yacute;</trd>
</drv>
</art>
