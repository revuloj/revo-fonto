<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>analitik</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="analit.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">MAT</uzo>
      <snc>
        <dif>
          Grupo de matematikaj sciencoj studantaj
          <ref tip="prt" cel="funkci.0o.mat">funkciojn</ref> per
          <ref tip="prt" cel="limes.0o">limesoj</ref>,
          <ref tip="prt" cel="deriv.0ajxo.mat">deriva&jcirc;oj</ref> kaj
          <ref tip="prt" cel="integr2.0o">integraloj</ref>.
        </dif>
      </snc>
      <trd lng="de">Analysis</trd>
      <trd lng="en">analysis</trd>
      <trd lng="fr">analyse</trd>
      <trd lng="ru">&#x43c;&#x430;&#x442;&#x435;&#x43c;&#x430;&#x442;&#x438;&#x447;&#x435;&#x441;&#x43a;&#x438;&#x439; <ind>&#x430;&#x43d;&#x430;&#x43b;&#x438;&#x437;</ind></trd>
    </drv>
    <drv mrk="analit.0a">
      <kap><tld/>a funkcio</kap>
      <uzo tip="fak">MAT</uzo>
      <snc>
        <dif>
          Kompleksnombra <ref tip="super" cel="funkci.0o.mat">funkcio</ref>
          kiu en &ccirc;iu punkto de esplorata regiono de sia fonta aro
          estas malkomponebla en potencoserion.
        </dif>
        <trd lng="en">analytic funktion</trd>
        <trd lng="ru">&#x430;&#x43d;&#x430;&#x43b;&#x438;&#x442;&#x438;&#x447;&#x435;&#x441;&#x43a;&#x430;&#x44f; <ind>&#x444;&#x443;&#x43d;&#x43a;&#x446;&#x438;&#x44f;</ind></trd>
      </snc>
    </drv>
  </art>
