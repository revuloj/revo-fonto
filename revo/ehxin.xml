<kap>
  <rad>e&hcirc;in</rad>/o,
  <var><kap><rad>ekin</rad>/o</kap></var>
</kap>
<drv mrk="ehxin.0o">
  <kap><tld/>o,
    <var><kap>ekino</kap></var>
  </kap>
  <snc mrk="ehxin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="kasxta.mar0o.ZOO">Marka&scirc;tano</ref>:
      <ekz>
        <tld/>o a&ubreve; mara erinaco estas <klr>[&#x2026;]</klr> proksima parenco de markukumo kaj
        marsteloj, marlutro man&gcirc;as grandan kvanton da <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57206">E&hcirc;ino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_i; <ind>&c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</ind></trd>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fa">&a_t;&a_w;&a_t;&f_y;&a_A;&f_y; &a_d;&a_r;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="he">&#x5E7;&#x5D9;&#x5E4;&#x5D5;&#x5D3; &#x5D9;&#x5DD;</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trd lng="nl">zee-egel</trd>
  <trd lng="pl">je&zdot;owiec</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j;
  &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x457;&#x436;&#x430;&#x43A;</trd>
</drv>
