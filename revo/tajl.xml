<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tajl.xml,v 1.25 2023/10/21 13:02:26 revo Exp $">
<kap>
   <ofc>8</ofc>
   <rad>tajl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tajl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tajl.0i.TEK">
  <uzo tip="fak">TEK</uzo>
    <dif>
      Doni formon al io, disigante pecetojn a&ubreve; erojn el
      &gcirc;i; <ref tip="sin" cel="trancx.el0i">eltran&ccirc;i</ref>:
      <ekz>
        <tld/>i &scirc;tonon, drapon, arbon, diamanton, fajfilon;
      </ekz>
      <ekz>
        sur la muroj de la groto ili &ccirc;izis belajn &ccirc;asbildojn per <tld/>ita fajro&scirc;tono
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;aron <klr>[&#x2026;]</klr> sekvis aliaj, portantaj fre&scirc;e <tld/>itajn fostojn
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> pantalono <klr>[estas]</klr> <tld/>ita el lintolo
        <fnt>
          <aut>Maxime Lechien</aut>
          <vrk><url ref="&Monato;/2000/006955.php">Fama karnavalo en Binche</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la zono <klr>[&#x2026;]</klr> sidas bone kvaza&ubreve; <tld/>ita por li
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu kostumo estis <tld/>ita la&ubreve; modo ege malsimila ol la nia
        <klr>(kp <ref cel="tajlor.0i">tajlori</ref>)</klr>
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="trancx.al0i">altran&ccirc;i</ref>,
       <ref cel="tond.cxirkaux0i">&ccirc;irka&ubreve;tondi</ref>,
       <ref cel="fason.0i">fasoni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_k;&c_r;&c_o;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_a;&c_b;&c_ch;&c_e;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>brou&scaron;en&iacute;m</trd>,
    <trd>drahokamy opracov&aacute;vat &rcaron;ez&aacute;n&iacute;m</trd>,
    <trd>l&aacute;tky st&rcaron;&iacute;hat</trd>,
    <trd>sek&aacute;n&iacute;m</trd>
  </trdgrp>
  <trd lng="de">zuschneiden</trd>
  <trdgrp lng="en">
     <trd>carve</trd>,
     <trd>sculpt</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>tallar</trd>,
     <trd>esculpir</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>tailler</trd>,
     <trd>couper</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>szab</trd>,
     <trd>kiszab</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x308B;  <pr>&#x304D;&#x308B;</pr></trd>,
    <trd>&#x524A;&#x308B;  <pr>&#x306F;&#x3064;&#x308B;</pr></trd>,
    <trd>&#x30AB;&#x30C3;&#x30C8;&#x3059;&#x308B;  <pr>&#x304B;&#x3063;&#x3068;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5207;&#x308A;&#x629C;&#x304F;  <pr>&#x304D;&#x308A;&#x306C;&#x304F;</pr></trd>,
    <trd>&#x88C1;&#x65AD;&#x3059;&#x308B;  <pr>&#x3055;&#x3044;&#x3060;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>fatsoeneren</trd>,
     <trd>slijpen</trd>,
     <trd>knippen</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>przycina&cacute;</trd>,
     <trd>wykrawa&cacute;</trd>,
     <trd>wyrzyna&cacute;</trd>,
     <trd>obciosywa&cacute;</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>talhar</trd>,
     <trd>esculpir</trd>,
     <trd>recortar</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_k;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_b;&c_t;&c_jo;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
     &c_i; &c_t;.&c_p;.
   </trdgrp>
  <trdgrp lng="sk">
    <trd>opracova&tcaron;</trd>,
    <trd>sformova&tcaron;</trd>,
    <trd>striha&tcaron;</trd>,
    <trd>vyreza&tcaron; <klr>(kraj&ccaron;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_r;&c_o;&c_ji;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_i;&c_g;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_t;&c_ib;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="tajl.en0o">
  <kap>
    en<tld/>o
  </kap>
  <snc mrk="tajl.en0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Arta <ref cel="trancx.en0o">entran&ccirc;o</ref>:
      <ekz>
        gravurita desegno, kies en<tld/>ojn oni plenigas per nigra emajlo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_r;&c_e;&c_z;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_z; <klr>(&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">Einschnitt</trd>
</drv>


</art>
