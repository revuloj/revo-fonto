<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>8</ofc>
   <rad>tajl</rad>/i
</kap>
<drv mrk="tajl.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">TEK</uzo>
   <snc>
     <dif>
       Doni formon al io, disigante pecetojn a&ubreve; erojn el 
&gcirc;i; eltran&ccirc;i:
       <ekz>
         <tld/>i &scirc;tonon, drapon, arbon, diamanton, fajfilon.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_r;&c_o;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_a;&c_b;&c_ch;&c_e;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="de">zuschneiden</trd>
   <trdgrp lng="en">
     <trd>carve</trd>,
     <trd>sculpt</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>szab</trd>,
     <trd>kiszab</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>fatsoeneren</trd>,
     <trd>slijpen</trd>,
     <trd>knippen</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>tailler</trd>,
     <trd>couper</trd>
       </trdgrp>
  <trdgrp lng="pt">
   <trd>talhar</trd>,
   <trd>esculpir</trd>,
   <trd>recortar</trd>
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_k;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_b;&c_t;&c_jo;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
     &c_i; &c_t;.&c_p;.
   </trdgrp>
</drv>
</art>
