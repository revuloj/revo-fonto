<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>tajl</rad>/i
</kap>
<drv mrk="tajl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Doni formon al io, disigante pecetojn a&ubreve; erojn el &gcirc;i; eltran&ccirc;i:
      <ekz>
        <tld/>i &scirc;tonon, drapon, arbon, diamanton, fajfilon.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zuschneiden</trd>
  <trdgrp lng="nl">
    <trd>fatsoeneren</trd>,
    <trd>slijpen</trd>,
    <trd>knippen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_t;&c_jo;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
    &c_i; &c_t;.&c_p;.
  </trdgrp>
</drv>
</art>
