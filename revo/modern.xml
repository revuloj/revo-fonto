<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>modern</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="modern.0a">
  <kap><tld/>a</kap>
  <snc mrk="modern.0a.KOMUNE">
    <dif>
      Apartenanta al la epoko nuna, al la tempo nuna
      a&ubreve; &jcirc;us pasinta,
      <ref tip="vid" cel="hodiau.0a.KOMUNE">hodia&ubreve;a</ref>;
      plej nova, plej fre&scirc;data:
      <ekz>
        li estis homo de la malnovaj moroj, ne aprobanta la
        <tld/>ajn ideojn
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kriterioj kaj metodoj de la <tld/>a lingvoscienco;
      </ekz>
      <ekz>
        la <tld/>a greka lingvo ne havas infinitivon
        <fnt>
          <aut>J. Wells</aut>,
          <vrk>Lingvistikaj aspektoj de Esperanto</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj mebloj, ideoj, inventoj;
      </ekz>
      <ekz>
        <tld/>a stilo;
      </ekz>
      <ekz>
        la progresoj de la <tld/>a scienco;
      </ekz>
      <ekz>
        hotelo kun <tld/>a komforto.
      </ekz>
    </dif>
    <trd lng="pl">nowoczesny</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="modern.0a.HIS">
    <dif>
      Apartenanta al la pli nova epoko, kontraste kun la antikva.
    </dif>
    <trd lng="pl">nowo&zdot;ytny</trd>
    <trdgrp lng="ru">
      <trd>&c_N;&c_o;&c_v;&c_o;&c_g;&c_o; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</trd>,
      <trd>&c_o;&c_t;&c_n;&c_o;&c_s;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_k; <ind>&c_N;&c_o;&c_v;&c_o;&c_m;&c_u; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
  <trd lng="ca">modern</trd>
  <trd lng="cs">modern&iacute;</trd>
  <trd lng="de">modern</trd>
  <trd lng="en">modern</trd>
  <trd lng="es">moderno</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_d;&a_r;&a_n;</trd>,
    <trd>&a_n;&a_w;&f_y;&a_n;</trd>,
    <trd>&a_A;&a_m;&a_r;&a_w;&a_z;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">moderne</trd>
  <trdgrp lng="hu">
    <trd>korszer&udblac;</trd>,
    <trd>modern</trd></trdgrp>
  <trd lng="id">modern</trd>
  <trd lng="nl">modern</trd>
  <trd lng="pt">moderno</trd>
  <trd lng="sv">modern</trd>
</drv>

<drv mrk="modern.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="modern.0igi.KOMUNE">
    <dif>
      Igi <tld/>a,
      <ref tip="vid" cel="nov.re0igi.KOMUNE">renovigi</ref>,
      <ref tip="vid" cel="dat.gxis0datigi.KOMUNE">&gcirc;isdatigi</ref>:
      <ekz>
        <tld/>igi sian hejmon, siajn metodojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">moderniser</trd>
  <trd lng="id">[<ind>modern</ind>] memodernkan</trd>
  <trd lng="pl">modernizowa&cacute;</trd>
</drv>

<drv mrk="modern.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="modern.0ismo.KOMUNE">
    <dif>
      Movado rigardata kiel <tld/>a pro iuj ecoj.
    </dif>
    <subsnc mrk="modern.0ismo.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Tuto de belartaj movadoj, kiuj ekde la
        fino de la 19a jarcento disi&gcirc;is de
        tradiciaj artoformoj, de realismo, de
        klasika harmonio, kaj kiuj emfazis novismon:
        <ekz>
          Jarl Martelmonto <klr>...</klr> konsideras sin
          ero de la <tld/>ismo
          <fnt>
            <aut>I. One&tcedil;</aut>,
            <vrk><url
            ref="http://esperanto.net/literaturo/uea/ceterceterrec.html">
            &Gcirc;uinda avangarda&jcirc;o</url></vrk>,
            <lok>[2005?]</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="pl">modernizm</trd>
    </subsnc>
    <subsnc mrk="modern.0ismo.FIL">
      <uzo tip="fak">FIL</uzo>
      <dif>
        Filozofia sinteno komenci&gcirc;anta en klasika E&ubreve;ropo,
        favora al raciismo, individuismo, progresismo,
        kontra&ubreve;a al tradicioj:
        <ekz>
          por varti <tld/>isman movadon oni devas montri progreson,
          &ccirc;u iluzian, &ccirc;u realan
          <fnt>
            <aut>M. Fettes</aut>,
            <vrk><url
            ref="http://www.helsinki.fi/~jslindst/raumo-mf.html">
            Ra&ubreve;mi transmoderne</url></vrk>,
            <lok>2000</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">modernit&eacute;</trd>
      <trd lng="pl">modernizm</trd>
    </subsnc>
    <subsnc mrk="modern.0ismo.KAT">
      <uzo tip="fak">KAT</uzo>
      <dif>
        Biblio-kritika kaj teologia movado el la komenco
        de la 20a jarcento, kiu enkalkulis la progresojn
        de historio kaj komence ricevis kondamnojn:
        <ekz>
          tiu estis la epoko kiam la diskutoj pri
          <tld/>ismo kaj kontra&ubreve;<tld/>ismo eniris
          plej akran fazon
          <fnt>
            <aut>-</aut>,
            <vrk><url
            ref="http://www.ikue.org/papoj/benediktoxv/biografio.htm">
            Papo Benedikto la 15a</url></vrk>,
            <lok>[2006?]</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="fa">&a_n;&a_w;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">modernisme</trd>
  <trd lng="id">modernisme</trd>
</drv>

<drv mrk="modern.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="modern.mal0a.KOMUNE">
    <dif>
      Ne plu valida, aktuala a&ubreve; uzata, malkonforma al la
      nunaj moroj a&ubreve; situacioj;
      <ref tip="vid" cel="arkaik.0a">arkaika</ref>,
      <ref tip="vid" cel="mod.eks0a.MOR">eksmoda</ref>,
      <ref tip="vid" cel="aktual.mal0a">malaktuala</ref>:
      <ekz>
        tio estas tro malvasta kaj mal<tld/>a difino
        <fnt>
          <aut>B. Wennergren</aut>,
          <vrk><url ref="http://bertilow.com/piv/">
          Kritikaj notoj pri la Nova PIV</url></vrk>,
          <lok>2002-2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la domo ne nur estas malnova, &gcirc;i estas mal<tld/>a
        <fnt>
          <aut>D. Broadribb</aut>,
          <vrk>Ne al &ccirc;iuj pla&ccirc;os &ccirc;i literatura gemeto</vrk>,
          <bib>Monato</bib>.
        </fnt>;
      </ekz>
      <ekz>
        du vicoj da domoj, &ccirc;e kiuj &ccirc;ie pendis mal<tld/>aj
        kadukaj balkonoj
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>unmodern</trd>,
    <trd>altmodisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>out-of-date</trd>,
    <trd>antiquated</trd>,
    <trd>obsolete</trd>,
    <trd>old-fashioned</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>archa&iuml;que</trd>,
    <trd>d&eacute;suet</trd>,
    <trd>surann&eacute;</trd>
  </trdgrp>
  <trd lng="id">kuno</trd>
  <trdgrp lng="pl">
    <trd>przestarza&lstroke;y</trd>,
    <trd>nienowoczesny</trd>,
    <trd>niedzisiejszy</trd>,
    <trd>staro&sacute;wiecki</trd>,
    <trd>staromodny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
