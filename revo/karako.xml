<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karako.xml,v 1.22 2024/09/07 18:33:47 revo Exp $">
<kap>
  <rad>karakol</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="karako.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <klr>(p.p. &ccirc;evalo a&ubreve; rajdanto)</klr>
      Fari fantaziajn saltetojn kaj turni&gcirc;ojn dum irado:
      <ekz>
        malanta&ubreve; ili senorde <tld/>is la aliaj edzinoj
        <fnt>
          <aut>Eugene De Zilah</aut>
          <vrk><url
          ref="https://books.google.de/books?id=zwjO3JIshdYC&amp;lpg=PA294&amp;ots=PEEts439w8&amp;pg=PA293#v=onepage&amp;q=karakolis&amp;f=false">La
          Princo Che La Hunoj </url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        postlasinte sian oficejon, li lasis senbride <tld/>i la noktan koboldon, kiun li
        ka&scirc;nutris en si
        <fnt>
          <aut>Ewa Grochowska</aut>
          <vrk><url
          ref="https://books.google.de/books?id=HsBDDwAAQBAJ&amp;lpg=PA91&amp;ots=d34K315mVH&amp;pg=PA91#v=onepage&amp;q=karakoli&amp;f=false">Belarta
          rikolto 2017: Premiitaj verkoj de la Belartaj Konkurso de UEA</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li decidis <klr>[&#8230;]</klr> senka&gcirc;igi siajn sentojn, gvidi ilin kien liaj inklinoj
        <tld/>is
        <fnt>
          <aut>Miguel Fern&aacute;ndez</aut>
          <vrk><url
          ref="https://books.google.de/books?id=vuLfCwAAQBAJ&amp;lpg=PA98&amp;ots=wIL9CPF-Ca&amp;pg=PA98#v=onepage&amp;q=karakolis&amp;f=false">La
          vorto kaj la vento. Rakonta koliero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaprio.0i">kaprioli</ref>,
      <ref cel="pranc.0i.RAJ">pranci<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">giravoltar<klr> (cavall)</klr></trd>
  <trd lng="cs">harcovat</trd>
  <trd lng="de">t&auml;nzeln</trd>
  <trd lng="en">caracole</trd>
  <trd lng="fr">caracoler</trd>
  <trdgrp lng="hu">
    <trd>kar&eacute;joz</trd>,
    <trd>t&aacute;ncol<klr tip="ind"> (l&oacute;)</klr></trd>,
    <trd>k&ouml;rben forog<klr tip="ind"> (l&oacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x534A;&#x56DE;&#x8EE2;&#12377;&#x308B;  <pr>&#x306F;&#12435;&#x304B;&#12356;&#12390;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x65CB;&#x56DE;&#12377;&#x308B;  <pr>&#x305B;&#12435;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">rondspringen</trd>
  <trdgrp lng="pl">
    <trd>karakolowa&cacute;</trd>,
    <trd>koniem toczy&cacute;</trd>
  </trdgrp>
  <trd lng="ro">rola de cal</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_zh;&c_i;&c_g;&c_i;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">samopa&scaron;ne tancova&tcaron; s ko&ncaron;om</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_c;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_r;&c_a;&c_v;&c_a;&c_t;&c_i; (&c_k;&c_o;&c_n;&c_je;&c_m;)</trd>,
    <trd>&c_d;&c_zh;&c_i;&c_gu;&c_ib;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x626D; <pr>ni&#x1D4;</pr></trd>
</drv>
</art>
