<drv mrk="asteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="asteri.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="stel.mar0o.ZOO">Marstelo</ref>:
      <ekz>
        novbu&scirc;uloj estas grupo de animaloj kiun konsistigas: ekinodermoj (e&hcirc;inoj,
        <tld/>oj, ktp), hemi&hcirc;orduloj, &hcirc;orduloj (vertebruloj kaj aliaj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=926">Novbu&scirc;ulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6D77;&#x76D8;&#x8F66; <pr>h&#x1CE;ip&#xE1;nch&#x113;</pr></trd>,
    <trd>&#x6D77;&#x76E4;&#x8ECA; <pr>h&#x1CE;ip&#xE1;nch&#x113;</pr></trd>,
    <trd>&#x6D77;&#x661F; <pr>h&#x1CE;ix&#x12B;ng</pr></trd>,
    <trd>&#x661F;&#x661F;&#x9B5A; <pr>x&#x12B;ngxingy&#xFA;</pr></trd>,
    <trd>&#x661F;&#x661F;&#x9C7C; <pr>x&#x12B;ngxingy&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mo&rcaron;sk&aacute; hv&ecaron;zda</trd>,
    <trd>mo&rcaron;sk&aacute; hv&ecaron;zdice</trd>
  </trdgrp>
  <trd lng="de">Seestern</trd>
  <trd lng="en">starfish</trd>
  <trd lng="es">estrella de mar</trd>
  <trdgrp lng="fr">
    <trd>ast&eacute;rie</trd>,
    <trd>&eacute;toile de mer</trd>
  </trdgrp>
  <trd lng="he">&kaf;&vav;&kaf;&bet;-&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>tengeri csillag</trd>,
    <trd>csillag&aacute;llatka</trd>
  </trdgrp>
  <trd lng="it"><ind>stella</ind> di mare</trd>
  <trd lng="ja">&#x30D2;&#x30C8;&#x30C7;</trd>
  <trd lng="pt">estrela do mar</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">morsk&aacute; hviezdica</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x430;&#xA0;&#x437;&#x456;&#x440;&#x43A;&#x430;</trd>
</drv>
