<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>asteri</rad>/o
</kap>
<drv mrk="asteri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el <ref tip="super" cel="ekinod.0oj">ekinodermoj</ref>
      (<trd lng="la">Asterias</trd>), stelforma, vivanta en maro.
    </dif>
    <ref tip="sin" cel="stel.mar0o">marstelo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Seestern</trd>
  <trd lng="en">starfish</trd>
  <trdgrp lng="fr">
    <trd>ast&eacute;rie</trd>,
    <trd>&eacute;toile de mer</trd>
  </trdgrp>
  <trd lng="he">&kaf;&vav;&kaf;&bet;-&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>tengeri <ind>csillag</ind></trd>,
    <trd>csillag&aacute;llatka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>
