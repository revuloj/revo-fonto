<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mang</rad>/o
</kap>
<drv mrk="mang.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref>
      de la <ref tip="malprt" cel="mang.0ujo"><tld/>ujo</ref>,
      dol&ccirc;a, suka kun flava karno.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="en">mango</trd>
  <trd lng="de">Mango</trd>
  <trd lng="hu">mang&oacute;</trd>
  <trd lng="nl">mango</trd>
  <trd lng="fr">mangue</trd>
  <trd lng="pl">mango</trd>
  <trd lng="pt">manga</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_g;&c_o; <klr
tip="amb">(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
  <trd lng="sv">mango</trd>
</drv>

<drv mrk="mang.0ujo">
  <kap>
    <tld/>ujo
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio
      <ref tip="super" cel="anakar1.0oj">anarkadiacoj</ref>
      (<trd lng="la">Mangifera</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="de">Mangobaum</trd>
  <trd lng="hu">mang&oacute;fa</trd>
  <trd lng="nl">mangoboom</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_g;&c_o; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
