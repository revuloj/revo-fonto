<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mang</rad>/o
</kap>
<drv mrk="mang.0o">
<kap><tld/>o</kap>
<uzo tip="fak">KUI</uzo>
<snc>
<dif>
Tropika, sukhava karneca frukto (<trd lng="la">Mangifera indica</trd>) de la <tld/>ujo.
</dif>
<trd lng="en">mango</trd>
<trd lng="de">Mango</trd>
<trd lng="nl">mango</trd>
<trd lng="fr">mangue</trd>
<trd lng="pl">mango</trd>
</snc>
</drv>
<drv mrk="mang.0ujo">
   <kap><tld/>ujo</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el la familio
       <ref tip="super" cel="kotile.0acoj">dukotiledonaj</ref>
       plantoj.
     </dif>
   <trd lng="nl">mangoboom</trd>
   </snc>
   </drv></art>

<!--
$Log$
