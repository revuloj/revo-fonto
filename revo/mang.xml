<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mang.xml,v 1.25 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>9</ofc><rad>mang</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mang.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="mang.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <ref tip="malprt" cel="mang.0arbo.BOT"><tld/>arbo</ref>,
      dol&ccirc;a, suka kun flava karno:
      <ekz>
        odoroj kaj gustoj de kareo kaj <tld/>o
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007156.php">Sutroj kaj sa&ubreve;dadoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 25</lok>
          </fnt>;
      </ekz>
      <ekz>
        la vendisto diris: &leftquot;&ccirc;iuj miaj <tld/>oj de tiu &ccirc;i arbo estas 
        dol&ccirc;aj kaj bongustaj&rightquot;
        <fnt>
          <aut>Miaohui</aut>,
          <vrk><url ref="http://www.ebudhano.cn/espero/index.php?/archives/214-70.-Gustumi-Mangojn.html">
          Gustumi Mangojn</url></vrk>,
          <lok>Rakontoj el Cent Paraboloj, 2006-11-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bo">&#xF68;&#xF58;&#xF0B;</trd>
  <trd lng="ca">mango</trd>
  <trd lng="en">mango</trd>
  <trd lng="de">Mango</trd>
  <trd lng="hu">mang&oacute;</trd>
  <trd lng="ja">&#x30DE;&#x30F3;&#x30B4;&#x30FC;</trd>
  <trd lng="nl">mango</trd>
  <trd lng="fr">mangue</trd>
  <trd lng="id">mangga</trd>
  <trd lng="pl">mango</trd>
  <trd lng="pt">manga</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_g;&c_o; <klr tip="amb">(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
  <trd lng="sv">mango</trd>
  <trd lng="uk">&#x43C;&c_a;&#x43D;&c_gu;&#x43E; (&#x43F;&#x43B;&c_ib;&c_d;)</trd>
  <trd lng="zh">&#x8292;&#x679C; <pr>m&#xE1;nggu&#x1D2;</pr></trd>
</drv>

<drv mrk="mang.0arbo">
  <kap>
    <ofc>9</ofc><tld/>arbo <fnt><bib>SPV</bib></fnt>,
    <var><kap><ofc>9</ofc><tld/>ujo <fnt><bib>PIV1</bib></fnt></kap></var>
  </kap>
  <snc mrk="mang.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Tropika <ref tip="super" cel="arb.0o.BOT">arbo</ref> el Sudazio kun diversaj specoj kultivataj
      pro la 
      fruktoj; <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el la
      familio
      <ref tip="malprt" cel="anakar1.0oj">anakardiacoj</ref>
      (<trd lng="la">Mangifera</trd>):
      <ekz>
        <klr>[la]</klr> bromeliojn <klr>[&#x2026;]</klr> mi plantis &ccirc;irka&ubreve; la trunko de
        <tld/>ujo en la malanta&ubreve;o de mia korto
        <fnt>
          <vrk><url ref="https://juliopedrosaesperanto.wordpress.com/tag/bromelio/">bromelio - Estas
          Mi Esperantisto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>arbo plantita en 2000 <klr>[&#x2026;]</klr> kiel arbo de monda solidareco
        <fnt>
          <vrk><url ref="http://www.recim.org/solidareco/mseo91.htm">Monda Solidareco n-ro
          90</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/8b/Mangga_apel_071214-2512_clya.jpg/384px-Mangga_apel_071214-2512_clya.jpg" prm="CC BY-SA 3.0">fruktoj &ccirc;e <tld/>ujo (Mangifera indica), Kuningan, Okcidenta Javo, Indonezio
        <fnt>
          <aut>W.A. Djatmiko (Wie146), propra verko</aut>
          <lok><url ref="&WCW;/File:Mangga_apel_071214-2512_clya.jpg">&WCW;/File:Mangga_apel_071214-2512_clya.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">mango</trd>
  <trdgrp lng="cs">
    <trd>mangovn&iacute;k</trd>,
    <trd>mangov&yacute; strom</trd>,
    <trd>slado&scaron;</trd>
  </trdgrp>
  <trd lng="de">Mangobaum</trd>
  <trd lng="fr">manguier</trd>
  <trd lng="hu">mang&oacute;fa</trd>
  <trd lng="ja">&#x30DE;&#x30F3;&#x30B4;&#x30FC;</trd>
  <trd lng="nl">mangoboom</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_g;&c_o; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">mangovn&iacute;k</trd>
  <trd lng="uk">&#x43C;&c_a;&#x43D;&c_gu;&#x43E; &c_ib;&#x43D;&c_d;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_je; (&c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;)</trd>
  <trd lng="zh">&#x6AA8; <pr>sh&#x113;</pr></trd>
</drv>
</art>
