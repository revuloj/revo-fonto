<art mrk="$Id: sanato.xml,v 1.21 2024/03/24 19:59:59 revo Exp $">
<kap>
  <rad>sanatori</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sanato.0o">
  <kap><tld/>o</kap>
  <snc mrk="sanato.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="san.mal0ulejo">Malsanulejo</ref>,
      en kiu oni kuracas malsanulojn a&ubreve; flegas malfortikulojn
      per longda&ubreve;ra severa higieno kaj per naturaj faktoroj pli ol per
      medikamentoj:
      <ekz>
        la <tld/>o vere helpis lin, kaj ni &ccirc;iuj estas esperplenaj
        pri lia estonta farto
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_a;&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>sanatorium</trd>,
    <trd>sanato&rcaron; <klr>(zast.)</klr></trd>
  </trdgrp>
  <trd lng="de">Sanatorium</trd>
  <trd lng="en">sanatorium</trd>
  <trd lng="es">sanatorio</trd>
  <trd lng="fr">sanatorium</trd>
  <trd lng="hu">szanat&oacute;rium</trd>
  <trd lng="it">sanatorio</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30CA;&#x30C8;&#x30EA;&#x30A6;&#x30E0;</trd>,
    <trd>&#x7642;&#x990A;&#x6240;  <pr>&#x308A;&#x3087;&#x3046;&#x3088;&#x3046;&#x3058;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="nl">sanatorium</trd>
  <trd lng="pl">sanatorium</trd>
  <trd lng="pt">sanat&oacute;rio</trd>
  <trd lng="ru">&c_s;&c_a;&c_n;&c_a;&c_t;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">sanat&oacute;rium</trd>
  <trd lng="uk">&c_s;&c_a;&#x43D;&c_a;&c_t;&#x43E;&c_r;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x7597;&#x517B;&#x9662; <pr>li&#xE1;oy&#x1CE;ngyu&#xE0;n</pr></trd>,
    <trd>&#x7642;&#x990A;&#x9662; <pr>li&#xE1;oy&#x1CE;ngyu&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
