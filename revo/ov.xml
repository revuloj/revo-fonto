<drv mrk="ov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ov.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o farita el <tld/>oj:
      <ekz>
        mi man&gcirc;is bongustan <tld/>a&jcirc;on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        prepari por vi tre bonan man&gcirc;a&jcirc;on: <tld/>a&jcirc;on kun &scirc;enoprazo
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne faras <tld/>a&jcirc;on sen rompi <tld/>ojn
        <fnt>
          <aut>Michel Marko</aut>
            <vrk><url ref="&Monato;/2000/007051.php">La stranga grupo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 29</lok>
          </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_je;&c_ch;&c_n;&c_ja;</trd>
  <trd lng="br">vio&ugrave;<klr tip="ind"> (boued)</klr></trd>
