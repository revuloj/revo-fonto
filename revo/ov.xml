<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc> <rad>ov</rad>/o
    </kap>

    <drv mrk="ov.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="ov.0o.gxermo">
        <dif>
          Organa korpo produktata kaj eligata de la inoj de diversaj klasoj da
          bestoj, kiu enhavas &gcirc;ermon de estonta samspeca besto, maturigonta
          ekster la patrina korpo:
          <ekz>
            <tld/>oj de birdo, lacerto, fi&scirc;o, insekto;
          </ekz><ekz>
            demeti, ellasi <tld/>ojn.
          </ekz>
        </dif>
      </snc>
      <snc mrk="ov.0o.nutrajxo">
        <dif>
          Kokina <tld/>o, uzata kiel nutra&jcirc;o:
          <ekz>
            en&scirc;ele bolitaj, duonkuiritaj, malmole kuiritaj <tld/>oj;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            doni <tld/>on por ricevi bovon
            <klr>(trompi la &scirc;an&gcirc;anton)</klr>
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            pli valoras <ind><mll tip="kom">tuj <tld/>o</mll></ind>
            ol poste bovo
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="ru">&c_l;&c_u;&c_ch;&c_sh;&c_je;
              <ind><mll tip="kom">&c_s;&c_i;&c_n;&c_i;&c_c;&c_a; &c_v; &c_r;&c_u;&c_k;&c_a;&c_h;</mll></ind>,
              &c_ch;&c_je;&c_m; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol; &c_v; &c_n;&c_je;&c_b;&c_je;</trd>
          </ekz>
        </dif>
      </snc>
      <snc mrk="ov.0o.cxelo">
        <uzo tip="fak">BIO</uzo>
        <dif>
          <ref tip="dif" cel="ov.0ocxelo"><tld lit="O"/>o&ccirc;elo</ref>:
          <ekz>
            nefekundigita, fekundigita <tld/>o.
          </ekz>
        </dif>
      </snc>                   
      <trd lng="be">&c_ja;&c_j;&c_k;&c_a;</trd>
      <trd lng="br">vi</trd>
      <trd lng="de">Ei</trd>
      <trd lng="el">&alfa;&ypsilon;&gamma;&omikron_ton;</trd>
      <trd lng="en">egg</trd>  
      <trd lng="fr">&oelig;uf</trd>
      <trd lng="he">&bet;&yod;&tsadi;&he;</trd>
      <trd lng="hu">toj&aacute;s</trd>
      <trd lng="nl">ei</trd>
      <trd lng="pl">jajko</trd>
      <trd lng="pt">ovo</trd>
      <trd lng="ru">&c_ja;&c_j;&c_c;&c_o;</trd>
      <trd lng="sv">&auml;gg</trd>
      <trd lng="tr">yumurta</trd>
    </drv>

    <drv mrk="ov.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <uzo tip="fak">KUI</uzo>
      <snc>
        <dif>
          Man&gcirc;a&jcirc;o farita el <tld/>oj.
        </dif>
        <refgrp tip="sub">
          <ref cel="omlet.0o">omleto</ref>.
        </refgrp>
      </snc>                       
      <trd lng="be">&c_ja;&c_je;&c_ch;&c_n;&c_ja;</trd>
      <trd lng="br">vio&ugrave;<klr tip="ind"> (boued)</klr></trd>
      <trdgrp lng="de">
        <trd>Eispeise</trd>,
        <trd>Eierspeise</trd>,
        <trd>Eiergericht</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>omelette</trd>,
        <trd>scrambled <ind>eggs</ind></trd>
      </trdgrp>    
      <trd lng="fr">des <ind>&oelig;ufs</ind><klr tip="ind"> (cuis.)</klr></trd>
      <trdgrp lng="he">
        <trd>&bet;&yod;&tsadi;&he; &mem;&qof;&vav;&shin;&qof;&shin;&tav;</trd>,
        <trd>&het;&bet;&yod;&tav;&he;</trd>
      </trdgrp>
      <trd lng="hu">toj&aacute;s&eacute;tel</trd>
      <trd lng="nl">eiergerecht</trd>
      <trd lng="pl">jajecznica</trd>
      <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
    </drv>

    <drv mrk="ov.0ingo">
      <kap><tld/>ingo</kap>
      <uzo tip="fak">KUI</uzo>
      <snc>
        <dif>
          Malgranda vazo en kiun oni metas <tld/>on, por &gcirc;in man&gcirc;i.
        </dif>
      </snc>                         
      <trd lng="br">dalc'her-vi</trd>
      <trd lng="de">Eierbecher</trd>
      <trd lng="en">eggcup</trd>    
      <trd lng="fr">coquetier</trd>
      <trd lng="he">&bet;&yod;&tsadi;&yod;&he;</trd>
      <trd lng="hu">toj&aacute;start&oacute; <klr tip="ind">(l&aacute;gy toj&aacute;shoz)</klr></trd>
      <trd lng="nl">eierdopje</trd>
      <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_ja;&c_i;&c_c;</trd>
      <trd lng="sv">&auml;ggkopp</trd>
    </drv>

    <drv mrk="ov.0ujo">
      <kap><ofc>*</ofc><tld/>ujo</kap>
      <snc mrk="ov.0ujo.ANA">
        <uzo tip="fak">ANA</uzo>
        <dif>
          Tiu parto de la inaj seksorganoj, kie maturi&gcirc;as
          la <tld/>o&ccirc;eloj.
        </dif>
        <trd lng="be">&c_ja;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>  
        <trd lng="br">vierez</trd>
        <trd lng="de">Eierstock</trd>
        <trd lng="el">&omega;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
        <trd lng="en">ovary</trd>  
        <trd lng="fr">ovaire</trd>
        <trd lng="he">&shin;&het;&lamed;&he;</trd>
        <trd lng="hu">petef&eacute;szek</trd>
        <trd lng="nl">eierstok</trd>
        <trd lng="pl">jajnik</trd>
        <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
        <trd lng="sv">&auml;ggstock</trd>
        <trdgrp lng="tr">
          <trd>yumurtal&inodot;k</trd>,
          <trd>mebiz</trd>
        </trdgrp>
      </snc>

      <snc mrk="ov.0ujo.skatolo">
        <uzo tip="stl">RAR</uzo>
        <dif>
          <tld lit="O"/>oskatolo.
        </dif>          
        <trd lng="br">boest vio&ugrave;</trd>
        <trd lng="de">Eierkarton</trd>  
        <trd lng="fr">bo&icirc;te &agrave; &oelig;ufs</trd>
      </snc>

      <rim>
        Nur la biologia senco <tld/>ujo<sncref ref="ov.0ujo.ANA"/>
        estas Fundamenta.
      </rim>

    </drv>

    <drv mrk="ov.0ocxelo">
      <kap><tld/>o&ccirc;elo</kap>
      <uzo tip="fak">BIO</uzo>
      <snc>
        <dif>
          Matura, fekundigebla inseksa
          <ref tip="super" cel="cxel.0o.BIO">&ccirc;elo<sncref/></ref>
          de animalo a&ubreve; vegeta&jcirc;o:
          <ekz>
            la fekundigita ovocelo evoluas generale al kronlarvo
            <klr>...</klr> kaj poste al vellarvo
            <fnt><vrk>Molusko</vrk>,
              <lok><url ref="http://eo.wikipedia.org/wiki/Molusko"
                  >Vikipedio</url></lok></fnt>;
          </ekz><ekz>
            <tld/>ujo: Organo de la ino, entenanta la
            <tld/>o&ccirc;elojn <fnt><bib>PV</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="ov.0ujo.ANA"><tld/>ujo<sncref/></ref>,
        <ref cel="spermatozo.o">spermatozoo</ref>
      </refgrp>                        
      <trd lng="be">&c_ja;&c_j;&c_c;&c_a;&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
      <trd lng="br">viell</trd>
      <trd lng="de">Eizelle</trd>
      <trd lng="el">&omega;&alfa_ton;&rho;&jota;&omikron;</trd>
      <trdgrp lng="en">
        <trd>ovum</trd>,
        <trd>ovule</trd>
      </trdgrp>  
      <trd lng="fr">ovule</trd>
      <trd lng="he">&bet;&yod;&tsadi;&yod;&tav;</trd>
      <trd lng="hu">petesejt</trd>
      <trd lng="nl">eitje</trd>
      <trd lng="pl">kom&oacute;rka jajowa</trd>
      <trd lng="pt">&oacute;vulo</trd>
      <trd lng="ru">&c_ja;&c_j;&c_c;&c_je;&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
      <trdgrp lng="tr">
        <trd>yumurtac&inodot;k</trd>,
        <trd>yumurtac&inodot;k&gbreve;&inodot;</trd>
      </trdgrp>
    </drv>

    <drv mrk="ov.el0igxi">
      <kap>el<tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Eliri el la <tld/>o:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            preska&ubreve; en &ccirc;iu nova libro el<tld/>i&gcirc;as kaj svarmas novaj vortoj
            tute nekompreneblaj por nepoliglotoj.
          </ekz>
        </dif>
      </snc>            
      <trdgrp lng="br">      
        <trd>diglora&ntilde;</trd>,
        <trd>dinodi&ntilde;</trd>
      </trdgrp>
      <trd lng="de"><klr>aus dem </klr>Ei <ind>schl&uuml;pfen</ind></trd>    
      <trd lng="en">hatch</trd>
      <trd lng="fr">&eacute;clore</trd>
      <trd lng="he">&lamed;&bet;&qof;&vav;&ayin; \<ind>&bet;&qof;&ayin;</ind>\</trd>
      <trd lng="hu">kikel <klr>a toj&aacute;sb&oacute;l</klr></trd>
      <trd lng="pl">klu&cacute; si&eogonek;</trd>
      <trd lng="ru">&c_v;&c_y;&c_l;&c_u;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>
  </art>
