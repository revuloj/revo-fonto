<drv mrk="ov.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="ov.0ingo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda vazo en kiun oni metas <tld/>on, por &gcirc;in
      man&gcirc;i:
      <ekz>
        mozaiko el Antio&hcirc;io el la jaro 40 anta&ubreve; Kristo montras homojn,
        kiuj man&gcirc;as <tld/>ojn el <tld/>ingoj per kuleretoj, en la romia
        urbo
        Pompejo trovi&gcirc;is ar&gcirc;enta <tld/>ingo kun <klr>[&#8230;]</klr>responda
        ar&gcirc;enta <tld/>oman&gcirc;a kulereto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215851">Ovingo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_z;&c_a;&c_ch;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_ja;&c_je;&c_k;</trd>
  <trd lng="br">dalc'her-vi</trd>
  <trd lng="ca">ouera<klr tip="ind"> (vaset per a un ou bullit)</klr></trd>
  <trd lng="cs">kal&iacute;&scaron;ek na va&rcaron;. vejce</trd>
  <trd lng="de">Eierbecher</trd>
  <trd lng="en">eggcup</trd>
  <trd lng="fr">coquetier</trd>
  <trd lng="he">&bet;&yod;&tsadi;&yod;&he;</trd>
  <trd lng="hu">toj&aacute;start&oacute; <klr tip="ind">(l&aacute;gy
  toj&aacute;shoz)</klr></trd>
  <trd lng="id"><ind>cangkir</ind> telur</trd>
  <trd lng="it">portauovo</trd>
  <trd lng="ja">&#x30A8;&#x30C3;&#x30B0;&#x30AB;&#x30C3;&#x30D7;</trd>
  <trd lng="nl">eierdopje</trd>
  <trd lng="pl"> jajecznik</trd>
  <trd lng="ro">suport pentru  <ind>ou&abreve;</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;
  &c_d;&c_l;&c_ja; &c_ja;&c_i;&c_c;</trd>
  <trd lng="sk">kal&iacute;&scaron;ok na vajce</trd>
  <trd lng="sv">&auml;ggkopp</trd>
