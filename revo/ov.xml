<drv mrk="ov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ov.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o farita el <tld/>oj:
      <ekz>
        mi man&gcirc;is bongustan <tld/>a&jcirc;on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        prepari por vi tre bonan man&gcirc;a&jcirc;on: <tld/>a&jcirc;on kun &scirc;enoprazo
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne faras <tld/>a&jcirc;on sen rompi <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007051.html">La
          stranga grupo</url></vrk>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_je;&c_ch;&c_n;&c_ja;</trd>
  <trd lng="br">vio&ugrave;<klr tip="ind"> (boued)</klr></trd>
  <trd lng="ca">truita</trd>
  <trdgrp lng="de">
    <trd>Eispeise</trd>,
    <trd>Eierspeise</trd>,
    <trd>Eiergericht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>omelette</trd>,
    <trd>scrambled <ind>eggs</ind></trd>,
    <trd>eggs<klr> (food)</klr></trd>
  </trdgrp>
  <trd lng="fr">des <ind>&oelig;ufs</ind><klr tip="ind">
  (cuis.)</klr></trd>
  <trdgrp lng="he">
    <trd>&bet;&yod;&tsadi;&he;
    &mem;&qof;&vav;&shin;&qof;&shin;&tav;</trd>,
    <trd>&het;&bet;&yod;&tav;&he;</trd>
  </trdgrp>
  <trd lng="hu">toj&aacute;s&eacute;tel</trd>
  <trdgrp lng="it">
    <trd>frittata</trd>,
    <trd>omelette</trd>,
    <trd><ind>uovo</ind> strapazzato</trd>
  </trdgrp>
  <trd lng="nl">eiergerecht</trd>
  <trdgrp lng="pl">
    <trd>omlet</trd>,
    <trd>jajecznica</trd>
  </trdgrp>
  <trd lng="ro">ou&abreve; jum&abreve;ri </trd>
  <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&c_ja;&c_jeu;&c_ch;&c_n;&c_ja;</trd>
</drv>

<drv mrk="ov.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="ov.0ingo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda vazo en kiun oni metas <tld/>on, por &gcirc;in
      man&gcirc;i:
      <ekz>
        mozaiko el Antio&hcirc;io el la jaro 40 anta&ubreve; Kristo montras homojn, 
        kiuj man&gcirc;as <tld/>ojn el <tld/>ingoj per kuleretoj, en la romia
        urbo
        Pompejo trovi&gcirc;is ar&gcirc;enta <tld/>ingo kun <klr>[&#8230;]</klr>responda
        ar&gcirc;enta <tld/>oman&gcirc;a kulereto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215851">Ovingo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">dalc'her-vi</trd>
  <trd lng="ca">ouera</trd>
  <trd lng="cs">kal&iacute;&scaron;ek na va&rcaron;. vejce</trd>
  <trd lng="de">Eierbecher</trd>
  <trd lng="en">eggcup</trd>
  <trd lng="fr">coquetier</trd>
  <trd lng="he">&bet;&yod;&tsadi;&yod;&he;</trd>
  <trd lng="hu">toj&aacute;start&oacute; <klr tip="ind">(l&aacute;gy
  toj&aacute;shoz)</klr></trd>
  <trd lng="id"><ind>cangkir</ind> telur</trd>
  <trd lng="it">portauovo</trd>
  <trd lng="nl">eierdopje</trd>
  <trd lng="pl"> jajecznik</trd>
  <trd lng="ro">suport pentru  <ind>ou&abreve;</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;
  &c_d;&c_l;&c_ja; &c_ja;&c_i;&c_c;</trd>
  <trd lng="sk">kal&iacute;&scaron;ok na vajce</trd>
  <trd lng="sv">&auml;ggkopp</trd>
  <trd lng="uk">&c_v;&c_a;&c_z;&c_o;&c_ch;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_ja;&c_j;&c_c;&c_ja;</trd>
</drv>

<drv mrk="ov.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="ov.0ujo.ANA">
  <uzo tip="fak">ANA</uzo>
  <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="ovari.0o.ANA">Ovario</ref>:
      <ekz>
        <tld/>ujo de salmo 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134307">Salmo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Gy&ouml;rgy Kese <klr>[&#8230;]</klr> esploris la rolon de sekshormonoj kaj
        la reciprokinfluon de utero kaj <tld/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=355411">Gy&ouml;rgy
          Kese</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Eierstock</trd>,
      <trd>Ovarium</trd>
    </trdgrp>
  </snc>
  <snc mrk="ov.0ujo.skatolo">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <tld lit="O"/>oskatolo:
      <ekz>
        la turdomego apud la landa parlamento de Turingio 
        (&scirc;erce anka&ubreve; nomita <tld/>ujo) estas multeta&gcirc;a publika domo en
        Erfurto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462477">Turdomego 
           apud la Landa parlamento de Turingio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">boest vio&ugrave;</trd>
    <trd lng="ca">closca <klr>(de l'ou)</klr></trd>
    <trd lng="de">Eierkarton</trd>
    <trd lng="es">huevera</trd>
    <trd lng="fr">bo&icirc;te &agrave; &oelig;ufs</trd>
    <trd lng="nl">eierdoosje<klr tip="ind"> (zeldz.)</klr></trd>
  </snc>
  <rim>
    Nur la anatomia senco <tld/>ujo <sncref ref="ov.0ujo.ANA"/>
    estas Fundamenta.
  </rim>
  <trdgrp lng="en">
    <trd>ovary</trd>,
    <trd>egg carton</trd>
  </trdgrp>
  <trd lng="sk">vaje&ccaron;n&iacute;k</trd>
  <trd lng="cs">vaje&ccaron;n&iacute;k</trd>
  <trd lng="pl">jajnik</trd>
  <trd lng="ro">ovar</trd>
  <trd lng="uk">&c_ja;&c_jeu;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="ov.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ov.el0igxi.KOMUNE">
    <dif>
      Eliri el <tld/>o:
      <ekz>
        la <klr>[turfalk]</klr>ino <klr>[&#8230;]</klr> meze de aprilo 
        demetas tri &gcirc;is ses <tld/>ojn, <klr>[&#8230;]</klr> la idoj
        el<tld/>i&gcirc;as post 28 tagoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter, Renate, Irene, Stefan Klag</aut>
          <vrk><url ref="&Monato;/2007/009167.php">Birdo de
          la jaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        preska&ubreve; en &ccirc;iu nova libro el<tld/>i&gcirc;as kaj
        svarmas novaj
        vortoj tute nekompreneblaj por nepoliglotoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>diglora&ntilde;</trd>,
    <trd>dinodi&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>descloure</trd>,
    <trd>sortir de l'ou</trd>
  </trdgrp>
  <trd lng="de"><klr>aus dem </klr>Ei <ind>schl&uuml;pfen</ind></trd>
  <trd lng="en">hatch</trd>
  <trd lng="es">salir del huevo</trd>
  <trd lng="fr">&eacute;clore</trd>
  <trd lng="he">&lamed;&bet;&qof;&vav;&ayin;
  \<ind>&bet;&qof;&ayin;</ind>\</trd>
  <trd lng="hu">kikel <klr>a toj&aacute;sb&oacute;l</klr></trd>
  <trd lng="id"><baz>tetas</baz>menetas</trd>
  <trdgrp lng="it">
    <trd>uscire<klr> dall'uovo</klr></trd>,
    <trd>schiudersi<klr> (di uova)</klr></trd>
  </trdgrp>
  <trd lng="nl">uit het <ind>ei</ind> komen</trd>
  <trd lng="pl">klu&cacute; si&eogonek;</trd>
  <trd lng="ro">trapa</trd>
  <trd lng="ru">&c_v;&c_y;&c_l;&c_u;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="uk">&c_v;&c_i;&c_l;&c_u;&c_p;&c_i;&c_t;&c_i;&c_s;&c_ja; (&c_z; &c_ja;&c_j;&c_c;&c_ja;)</trd>
