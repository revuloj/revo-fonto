<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ov</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ov.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Organa korpo produktata kaj eligata de la inoj de diversaj klasoj
      de bestoj, kiu enhavas &gcirc;ermon de estonta samspeca besto, 
      maturi&gcirc;onta ekster la patrina korpo:
      <ekz>
        <tld/>oj de birdo, lacerto, fi&scirc;o, insekto;
      </ekz>
      <ekz>
        demeti, ellasi <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ov.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="O"/>o, ofte kokina, uzata kiel nutra&jcirc;o:
      <ekz>
        en&scirc;ele bolitaj, duonkuiritaj, malmole kuiritaj <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>doni <tld/>on por ricevi bovon</ind>
        <klr>(trompi la &scirc;an&gcirc;anton)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="nl"><ind>appel</ind>en voor citroenen verkopen</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        pli valoras <ind><mll tip="kom">tuj <tld/>o</mll></ind>
        ol poste bovo
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">un <ind>tiens</ind> vaut deux tu l'auras</trd>
        <trd lng="ru">&c_l;&c_u;&c_ch;&c_sh;&c_je;
        <ind><mll tip="kom">&c_s;&c_i;&c_n;&c_i;&c_c;&c_a; &c_v;
        &c_r;&c_u;&c_k;&c_a;&c_h;</mll></ind>,
        &c_ch;&c_je;&c_m; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol; &c_v;
        &c_n;&c_je;&c_b;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="ov.0o.cxelo">
    <uzo tip="fak">BIO</uzo>
    <ref tip="dif" cel="cxel.ovo0o.FIZL"><tld/>o&ccirc;elo</ref>
    <ekz>
      nefekundigita, fekundigita <tld/>o.
    </ekz>
  </snc>
  <trd lng="be">&c_ja;&c_j;&c_k;&c_a;</trd>
  <trd lng="bg">&c_ja;&c_j;&c_c;&c_je;</trd>
  <trd lng="br">vi</trd>
  <trd lng="ca">ou</trd>
  <trd lng="de">Ei</trd>
  <trd lng="el">&alfa;&ypsilon;&gamma;&omikron_ton;</trd>
  <trd lng="en">egg</trd>
  <trd lng="es">huevo</trd>
  <trd lng="fr">&oelig;uf</trd>
  <trd lng="he">&bet;&yod;&tsadi;&he;</trd>
  <trd lng="hu">toj&aacute;s</trd>
  <trd lng="it">uovo</trd>
  <trd lng="nl">ei</trd>
  <trd lng="pl">jajko</trd>
  <trd lng="pt">ovo</trd>
  <trd lng="ru">&c_ja;&c_j;&c_c;&c_o;</trd>
  <trd lng="sv">&auml;gg</trd>
  <trd lng="tr">yumurta</trd>
</drv>

<drv mrk="ov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ov.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o farita el <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_je;&c_ch;&c_n;&c_ja;</trd>
  <trd lng="br">vio&ugrave;<klr tip="ind"> (boued)</klr></trd>
  <trd lng="ca">truita</trd>
  <trdgrp lng="de">
    <trd>Eispeise</trd>,
    <trd>Eierspeise</trd>,
    <trd>Eiergericht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>omelette</trd>,
    <trd>scrambled <ind>eggs</ind></trd>
  </trdgrp>
  <trd lng="es">tortilla</trd>
  <trd lng="fr">des <ind>&oelig;ufs</ind><klr tip="ind"> (cuis.)</klr></trd>
  <trdgrp lng="he">
    <trd>&bet;&yod;&tsadi;&he; &mem;&qof;&vav;&shin;&qof;&shin;&tav;</trd>,
    <trd>&het;&bet;&yod;&tav;&he;</trd>
  </trdgrp>
  <trd lng="hu">toj&aacute;s&eacute;tel</trd>
  <trdgrp lng="it">
    <trd>frittata</trd>,
    <trd>omelette</trd>,
    <trd><ind>uovo</ind> strapazzato</trd>
  </trdgrp>
  <trd lng="nl">eiergerecht</trd>
  <trd lng="pl">jajecznica</trd>
  <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="ov.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="ov.0ingo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda vazo en kiun oni metas <tld/>on, por &gcirc;in man&gcirc;i.
    </dif>
  </snc>
  <trd lng="br">dalc'her-vi</trd>
  <trd lng="ca">ouera</trd>
  <trd lng="de">Eierbecher</trd>
  <trd lng="en">eggcup</trd>
  <trd lng="fr">coquetier</trd>
  <trd lng="he">&bet;&yod;&tsadi;&yod;&he;</trd>
  <trd lng="hu">toj&aacute;start&oacute; <klr tip="ind">(l&aacute;gy
  toj&aacute;shoz)</klr></trd>
  <trd lng="it">portauovo</trd>
  <trd lng="nl">eierdopje</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_ja;&c_i;&c_c;</trd>
  <trd lng="sv">&auml;ggkopp</trd>
</drv>

<drv mrk="ov.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="ov.0ujo.BIO">
    <dif>
      Tiu parto de la inaj seksorganoj, kie maturi&gcirc;as
      <tld/>o&ccirc;eloj;
      <ref tip="sin" cel="ovari.0o.BIO">ovario</ref>.
    </dif>
    <subsnc mrk="ov.0ujo.ANA">
      <uzo tip="fak">ANA</uzo>
      <dif>
        &Ccirc;e bestoj, organo entenata la <tld/>o&ccirc;elojn.
      </dif>
      <trd lng="be">&c_ja;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>
      <trd lng="br">vierez</trd>
      <trd lng="ca">ovari</trd>
      <trd lng="de">Eierstock</trd>
      <trd lng="el">&omega;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
      <trd lng="en">ovary</trd>
      <trd lng="es">ovario</trd>
      <trd lng="he">&shin;&het;&lamed;&he;</trd>
      <trd lng="hu">petef&eacute;szek</trd>
      <trdgrp lng="it">
         <trd>ovario<klr> (anat.)</klr></trd>,
         <trd>ovaia</trd>,
         <trd>ovaio</trd>
       </trdgrp>
      <trd lng="nl">eierstok</trd>
      <trd lng="pl">jajnik</trd>
      <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
      <trd lng="sv">&auml;ggstock</trd>
      <trdgrp lng="tr">
        <trd>yumurtal&inodot;k</trd>,
        <trd>mebiz</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="ov.0ujo.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        &Ccirc;e vegeta&jcirc;oj, malsupra parto de la
        <ref tip="malprt" cel="pistil.0o">pistilo</ref>
        enhavanta la <tld/>o&ccirc;elojn.
      </dif>
    </subsnc>
    <trd lng="fr">ovaire</trd>
  </snc>
  <snc mrk="ov.0ujo.skatolo">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <tld lit="O"/>oskatolo.
    </dif>
    <trd lng="br">boest vio&ugrave;</trd>
    <trd lng="ca">closca <klr>(de l'ou)</klr></trd>
    <trd lng="de">Eierkarton</trd>
    <trd lng="fr">bo&icirc;te &agrave; &oelig;ufs</trd>
    <trd lng="nl">eierdoosje<klr tip="ind"> (zeldz.)</klr></trd>
  </snc>
  <rim>
    Nur la biologia senco <tld/>ujo<sncref ref="ov.0ujo.ANA"/>
    estas Fundamenta.
  </rim>
</drv>

<drv mrk="ov.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ov.el0igxi.KOMUNE">
    <dif>
      Eliri el <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        preska&ubreve; en &ccirc;iu nova libro el<tld/>i&gcirc;as kaj svarmas novaj
        vortoj tute nekompreneblaj por nepoliglotoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>diglora&ntilde;</trd>,
    <trd>dinodi&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>descloure</trd>,
    <trd>sortir de l'ou</trd>
  </trdgrp>
  <trd lng="de"><klr>aus dem </klr>Ei <ind>schl&uuml;pfen</ind></trd>
  <trd lng="en">hatch</trd>
  <trd lng="fr">&eacute;clore</trd>
  <trd lng="he">&lamed;&bet;&qof;&vav;&ayin; \<ind>&bet;&qof;&ayin;</ind>\</trd>
  <trd lng="hu">kikel <klr>a toj&aacute;sb&oacute;l</klr></trd>
  <trdgrp lng="it">
    <trd>uscire<klr> dall'uovo</klr></trd>,
    <trd>schiudersi<klr> (di uova)</klr></trd>
  </trdgrp>
  <trd lng="nl">uit het <ind>ei</ind> komen</trd>
  <trd lng="pl">klu&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_v;&c_y;&c_l;&c_u;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ov.kirl0ajxo">
  <kap>kirl<tld/>a&jcirc;o</kap>
  <snc mrk="ov.kirl0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o el ova&jcirc;oj
      <ref tip="vid" cel="kirl.0i.MOV">kirlitaj</ref>
      kaj kuiritaj
      sur pato kun eventualaj enmiksa&jcirc;oj;
      <ref tip="sin" cel="omlet.0o.KUI">omleto</ref>:
      <ekz>
        ni ricevis el &gcirc;i fine tiel nomatan kirl<tld/>a&jcirc;on
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_l;&c_je;&c_t;</trd>
  <trd lng="bg">&c_o;&c_m;&c_l;&c_je;&c_t;</trd>
  <trd lng="ca">truita</trd>
  <trd lng="de">Omelett</trd>
  <trd lng="el">&omikron;&my;&epsilon;&lambda;&epsilon_ton;&tau;&tau;&alfa;</trd>
  <trd lng="en">omelette</trd>
  <trdgrp lng="es">
    <trd>tortilla<klr> (de huevo)</klr></trd>,
    <trd>omelet</trd>
  </trdgrp>
  <trd lng="fr">omelette</trd>
  <trdgrp lng="hu">
    <trd>toj&aacute;slep&eacute;ny</trd>,
    <trd>omlett</trd></trdgrp>
  <trd lng="nl">omelet</trd>
  <trdgrp lng="pt">
    <trd>omelete</trd>,
    <trd>omeleta</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_m;&c_l;&c_je;&c_t;</trd>
  <trd lng="sv">omelett</trd>
</drv>
</art>
