<drv mrk="ov.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="ov.0ujo.ANA">
  <uzo tip="fak">ANA</uzo>
  <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="ovari.0o.ANA">Ovario</ref>:
      <ekz>
        <tld/>ujo de salmo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134307">Salmo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Gy&ouml;rgy Kese <klr>[&#8230;]</klr> esploris la rolon de sekshormonoj kaj
        la reciprokinfluon de utero kaj <tld/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=355411">Gy&ouml;rgy
          Kese</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ja;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_m;&c_a;&c_c;&c_ib;&c_n;&c_je;&c_c;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Eierstock</trd>,
      <trd>Ovarium</trd>
    </trdgrp>
  <trdgrp lng="id">
    <trd>ovarium</trd>,
    <trd><ind>indung</ind> telur</trd>
  </trdgrp>
  </snc>
  <snc mrk="ov.0ujo.skatolo">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <tld lit="O"/>oskatolo:
      <ekz>
        la turdomego apud la landa parlamento de Turingio
        (&scirc;erce anka&ubreve; nomita <tld/>ujo) estas multeta&gcirc;a publika domo en
        Erfurto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462477">Turdomego
           apud la Landa parlamento de Turingio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_n;&c_ja; &c_d;&c_l;&c_ja; &c_ja;&c_je;&c_k;</trd>
    <trd lng="br">boest vio&ugrave;</trd>
    <trd lng="ca">ouera <klr>(recipient per a guardar-hi ous)</klr></trd>
    <trd lng="de">Eierkarton</trd>
    <trd lng="es">huevera</trd>
    <trd lng="fr">bo&icirc;te &agrave; &oelig;ufs</trd>
    <trdgrp lng="id">
      <trd><ind>kotak</ind> telur</trd>,
      <trd><ind>rak</ind> telur</trd>
    </trdgrp>
    <trd lng="nl">eierdoosje<klr tip="ind"> (zeldz.)</klr></trd>
  </snc>
  <rim>
    Nur la anatomia senco <tld/>ujo <sncref ref="ov.0ujo.ANA"/>
    estas Fundamenta.
  </rim>
  <trdgrp lng="en">
    <trd>ovary</trd>,
    <trd>egg carton</trd>
  </trdgrp>
