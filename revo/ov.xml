<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>ov</rad>/o
</kap>
<drv mrk="ov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ov.0o.gxermo">
    <dif>
      Organa korpo produktata kaj eligata de la inoj de diversaj klasoj da
      bestoj, kiu enhavas &gcirc;ermon de estonta samspeca besto, maturigonta
      ekster la patrina korpo:
      <ekz>
        <tld/>oj de birdo, lacerto, fi&scirc;o, insekto;
      </ekz>
      <ekz>
        demeti, ellasi <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ov.0o.nutrajxo">
    <dif>
      Kokina <tld/>o, uzata kiel nutra&jcirc;o:
      <ekz>
        en&scirc;ele bolitaj, duonkuiritaj, malmole kuiritaj <tld/>oj;
      </ekz>
      <ekz>
        doni <tld/>on por ricevi bovon
        <klr>(trompi la &scirc;an&gcirc;anton)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pli valoras tuj <tld/>o ol poste bovo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ei</trd>
  <trd lng="el">&alfa;&ypsilon;&gamma;&omikron_ton;</trd>
  <trd lng="en">egg</trd>
  <trd lng="he">&bet;&yod;&tsadi;&he;</trd>
  <trd lng="nl">ei</trd>
  <trd lng="pl">jajka</trd>
  <trd lng="pt">ovo</trd>
  <trd lng="ru">&c_ja;&c_j;&c_c;&c_o;</trd>
  <trd lng="sv">&auml;gg</trd>
</drv>

<drv mrk="ov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Man&gcirc;a&jcirc;o farita el <tld/>oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="omlet.0o">omleto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Eispeise</trd>,
     <trd>Eierspeise</trd>,
     <trd>Eiergericht</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>omelette</trd>,
     <trd>scrambled <ind>eggs</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&bet;&yod;&tsadi;&he; &mem;&qof;&vav;&shin;&qof;&shin;&tav;</trd>,
    <trd>&het;&bet;&yod;&tav;&he;</trd>
  </trdgrp>
  <trd lng="nl">eiergerecht</trd>
  <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="ov.0ingo">
  <kap><tld/>ingo</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malgranda vazo en kiun oni metas <tld/>on, por &gcirc;in man&gcirc;i.
    </dif>
  </snc>
  <trd lng="de">Eierbecher</trd>
  <trd lng="en">eggcup</trd>
  <trd lng="he">&bet;&yod;&tsadi;&yod;&he;</trd>
  <trd lng="nl">eierdopje</trd>
  <trd lng="sv">&auml;ggkopp</trd>
</drv>

<drv mrk="ov.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Organo de la ino, entenanta la <tld/>o&ccirc;elojn.
    </dif>
  </snc>
  <trd lng="de">Eierstock</trd>
  <trd lng="en">ovary</trd>
  <trd lng="he">&shin;&het;&lamed;&he;</trd>
  <trd lng="nl">eierstok</trd>
  <trd lng="ru">&c_ja;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">&auml;ggstock</trd>
</drv>

<drv mrk="ov.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Eliri el la <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        preska&ubreve; en &ccirc;iu nova libro el<tld/>i&gcirc;as kaj svarmas novaj vortoj
        tute nekompreneblaj por nepoliglotoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">hatch</trd>
  <trd lng="he">&lamed;&bet;&qof;&vav;&ayin; \<ind>&bet;&qof;&ayin;</ind>\</trd>
  <trd lng="de"><klr>aus dem </klr>Ei <ind>schl&uuml;pfen</ind></trd>
  <trd lng="ru">&c_v;&c_y;&c_l;&c_u;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
