<drv mrk="ov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ov.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Organa korpo produktata kaj eligata de la inoj de diversaj klasoj
      de bestoj, kiu enhavas &gcirc;ermon de estonta samspeca besto, 
      maturi&gcirc;onta ekster la patrina korpo:
      <ekz>
        <tld/>oj de birdo, lacerto, fi&scirc;o, insekto;
      </ekz>
      <ekz>
        demeti, ellasi <tld/>ojn;
      </ekz>
      <ekz>
        &ccirc;io hele ekflamis kaj la maljuna birdo fenikso fari&gcirc;is cindro, 
        sed &gcirc;ia <tld/>o ku&scirc;is ru&gcirc;e ardante en la fajro, &gcirc;i krevis kun 
        granda krako, kaj la ido elflugis
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        krevis unu <tld/>o post la alia, &leftquot;Pip, pip!&rightquot; oni a&ubreve;dis,
        &ccirc;iuj <tld/>oflavoj vivi&gcirc;is kaj el&scirc;ovis la kapon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la formikoj sen&ccirc;ese kuradis kun sia <tld/>o sur la dorso
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda kiom <tld/>o de mar&ccirc;kokino
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de Kolumbo
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ov.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="O"/>o kortbirda, ofte kokina, uzata kiel nutra&jcirc;o:
      <ekz>
        en&scirc;ele boligitaj, duonkuiritaj, malmole kuiritaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>oblanko, <tld/>oflavo;
      </ekz>
      <ekz>
        unu <tld/>o malbona tutan man&gcirc;on difektas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>kara estas <tld/>o, kiam venas la Pasko</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="ru"><ind>&c_d;&c_o;&c_r;&c_o;&c_g;&c_o; &c_ja;&c_i;&c_ch;&c_k;&c_o;</ind> &c_k;
        &c_H;&c_r;&c_i;&c_s;&c_t;&c_o;&c_v;&c_u; &c_d;&c_n;&c_ju;</trd>
      </ekz><ekz>
        ekboligo de matenman&gcirc;a <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/007110.html">Atentokapta sed
          ru&gcirc;krajoninda</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>doni <tld/>on por ricevi bovon</ind>
        <klr>(trompi la &scirc;an&gcirc;anton)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="nl"><ind>appel</ind>en voor citroenen verkopen</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        pli valoras <ind><mll tip="kom">tuj <tld/>o</mll></ind>
        ol poste bovo
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">un <ind>tiens</ind> vaut deux tu l'auras</trd>
        <trd lng="ru">&c_l;&c_u;&c_ch;&c_sh;&c_je;
        <ind><mll tip="kom">&c_s;&c_i;&c_n;&c_i;&c_c;&c_a; &c_v;
        &c_r;&c_u;&c_k;&c_a;&c_h;</mll></ind>,
        &c_ch;&c_je;&c_m; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol; &c_v; &c_n;&c_je;&c_b;&c_je;</trd>
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="blank.ovo0o"><tld/>oblanko</ref>,
      <ref cel="flav.ov0o"><tld/>oflavo</ref>
    </refgrp>
  </snc>
  <snc mrk="ov.0o.cxelo">
    <uzo tip="fak">BIO</uzo>
    <dif>
      <ref tip="dif" cel="cxel.ovo0o.FIZL"><tld lit="O"/>o&ccirc;elo</ref>:
      <ekz>
        nefekundigita, fekundigita <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Ei<klr>(zelle)</klr></trd>
  </snc>
