<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ekstrem</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekstre.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="ekstre.0a.space">
    <dif>
      Estanta &ccirc;e la finfino, plej malproksima:
      <ekz>
        <tld/>a Oriento;
      </ekz>
      <ekz>
        deputito el <tld/>a maldekstro;
      </ekz>
      <ekz>
        homo de <tld/>a maljuneco.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ekstre.0a.kvante">
    <dif>
      Estanta &ccirc;e la plej alta grado:
      <ekz>
        mi sentas nek la <tld/>an
        varmon, nek la <tld/>an malvarmon;
      </ekz>
      <ekz>
        <tld/>a plezuro, mildeco, lerteco;
      </ekz>
      <ekz>
        <tld/>igi teorion;
      </ekz>
      <ekz>
        en <tld/>a okazo, en <tld/>a situacio
        <klr>(en okazo de <tld/>a bezono, supozante la plej malfavoran
        okazon)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_ib;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_a;&c_j;&c_n;&c_ib;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="es">extremo</trd>
  <trd lng="fr">extr&ecirc;me<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;ls&odblac;</trd>,
    <trd>sz&eacute;ls&odblac;s&eacute;ges</trd>,
    <trd>rendk&iacute;v&uuml;li</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstremalny</trd>,
    <trd>skrajny</trd>,
    <trd>kra&nacute;cowy</trd>
  </trdgrp>
  <trd lng="pt">extremo</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_r;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>,
      
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">extrem</trd>
</drv>

<drv mrk="ekstre.0e">
  <kap><tld/>e</kap>
  <snc mrk="ekstre.0e.kvante">
    <dif>
      Treege, plej:
      <ekz>
        kordo <tld/>e stre&ccirc;ita;
      </ekz>
      <ekz>
        ni <tld/>e bezonas plenan vortaregon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&auml;u&szlig;erst</trd>,
    <trd>extrem</trd>
  </trdgrp>
  <trd lng="fr">extr&ecirc;mement</trd>
  <trdgrp lng="hu">
    <trd>rendk&iacute;v&uuml;l</trd>,
    <trd>szerfelett</trd>,
    <trd>nagyon-nagyon</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstremalnie</trd>,
    <trd>skrajnie</trd>,
    <trd>kra&nacute;cowo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_j;&c_n;&c_je;</trd>
  <trd lng="sv">extremt</trd>
</drv>

<drv mrk="ekstre.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekstre.0o.ajxo">
  <ref tip="dif" cel="ekstre.0ajxo"><tld/>a&jcirc;o</ref>
    <ekz>
      <tld/>o de membroj, de &scirc;ipo.
    </ekz>
    <rim mrk="ekstre.0o.RIM_ARK">
      La formo <em>ekstremiteto</em> aperas en malnova verko:
      <ekz>
        malgranda hom-esta&jcirc;o kun kvar ekstremitetoj kiuj baraktis
        en la
        aero
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>du blinduloj, sceno el la
        lando de
        Donaj kozakoj</lok></fnt>.
      </ekz>
    </rim>
  </snc>
  <snc mrk="ekstre.0o.eco">
  <ref tip="dif" cel="ekstre.0eco"><tld/>eco</ref>
      <ekz>
        malaj <tld/>oj estas proksimaj;
      </ekz>
      <ekz>
        peli, pu&scirc;i siajn postulojn &gcirc;is <tld/>o;
      </ekz>
      <ekz>
        la dioj gardu nin de tia <tld/>o!
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 25a</lok></fnt>.
      </ekz>
      <trdgrp lng="pl">
        <trd>kraniec</trd>,
        <trd>skraj</trd>
      </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>extr&ecirc;me<klr> (subst.)</klr></trd>,
    <trd>extr&ecirc;mit&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="ekstre.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ekstre.0ajxo.space">
    <dif>
      La plej malproksima fino, parto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_m;&c_ja;&c_zh;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_m;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spitze</trd>,
    <trd>Ende</trd>
  </trdgrp>
  <trd lng="es">extremo</trd>
  <trd lng="fr">extr&eacute;mit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;l</trd>,
    <trd>v&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstremum</trd>,
    <trd>kraniec</trd>,
    <trd>skraj</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="ekstre.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="ekstre.0eco.kvante">
    <dif>
      La plej alta grado:
      <ekz>
        la &gcirc;is <tld/>eco stre&ccirc;ita kordo krevis<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_ja;&c_ja;
    &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&Auml;u&szlig;erstes</trd>,
    <trd>Extrem</trd>
  </trdgrp>
  <trd lng="fr">extr&ecirc;me<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>v&eacute;glet</trd>,
    <trd>extremit&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kraniec</trd>,
    <trd>skraj</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_ja;&c_ja;
    &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="ekstre.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="ekstre.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Emo al <tld/>aj opinioj, al drastaj agoj por atingi siajn celojn,
      kontraste al <ref tip="ant" cel="moder.0ismo.POL">moderismo</ref>:
      <ekz>
        ajna <tld/>ismo ne povas kongrui kun demokratia idearo,
        &ccirc;ar &gcirc;i
        malebligas la trovadon de kompromiso akceptebla por &ccirc;iuj
        <fnt>
          <aut>I. De Coster</aut>,
          <vrk><url
          ref="http://www.helsinki.fi/~jslindst/raumo-id.html">
          Esperanto-komunumo kaj la civila socio</url></vrk>,
          <lok>en: seminario en Helsinko, 2000-07-22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fanati.0eco">fanatikeco</ref>,
      <ref cel="radika.0ismo.POL">radikalismo</ref>
    </refgrp>
  </snc>
  <trd lng="es">extremismo</trd>
  <trd lng="fr">extr&eacute;misme</trd>
  <trd lng="pl">ekstremizm</trd>
</drv>

<drv mrk="ekstre.0umo">
  <kap><tld/>umo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="bild.0igo.MAT">bildigo</ref> al
      <ref cel="ar.orda0o">orda aro</ref>)</klr>
      <ref tip="sub" cel="minimu.0o.MAT">Minimumo</ref> a&ubreve;
      <ref tip="sub" cel="maksim1.0o.MAT">maksimumo</ref> de &gcirc;ia
      bildaro:
      <ekz>
        la nombro <frm>1</frm> estas <ind>absoluta <tld/>umo</ind> de
        funkcio sinuso <klr>(<tld/>umo)</klr>;
        <trd lng="be">&c_a;&c_b;&c_s;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;
        <ind>&c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_u;&c_m;</ind></trd>
        <trd lng="de">absolutes <ind>Extremum</ind></trd>
        <trd lng="en">absolute <ind>extremum</ind></trd>
        <trd lng="fr">extr&eacute;mum absolu</trd>
        <trd lng="hu">abszol&uacute;t
        <ind>sz&eacute;ls&odblac;&eacute;rt&eacute;k</ind></trd>
        <trd lng="pl">ekstremum absolutne</trd>
        <trd
        lng="ru">&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;
        <ind>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_u;&c_m;</ind></trd>
      </ekz>
      <ekz>
        la nombro <frm>1</frm> estas <ind>loka <tld/>umo</ind> de
        funkcio <frm><k>x</k><sup>3</sup>-3.<k>x</k>)</frm> &ccirc;e
        punkto
        <frm>1</frm> <klr>(<tld/>umo de la malvastiga&jcirc;o de tiu
        funkcio al iu &ccirc;irka&ubreve;a&jcirc;o de
        <frm>1</frm>)</klr>.
        <trd lng="be">&c_l;&c_ja;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;
        <ind>&c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_u;&c_m;</ind></trd>
        <trd lng="de">lokales <ind>Extremum</ind></trd>
        <trd lng="en">local <ind>extremum</ind></trd>
        <trd lng="fr">extr&eacute;mum local</trd>
        <trd lng="hu">helyi
        <ind>sz&eacute;ls&odblac;&eacute;rt&eacute;k</ind></trd>
        <trd lng="pl">ekstremum lokalne</trd>
        <trd lng="ru">&c_l;&c_o;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
        <ind>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_u;&c_m;</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_u;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Extremum</trd>,
    <trd>Extremwert</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extremum</trd>,
    <trd>extreme value</trd>
  </trdgrp>
  <trd lng="fr">extr&eacute;mum</trd>
  <trd lng="hu">sz&eacute;ls&odblac;&eacute;rt&eacute;k</trd>
  <trd lng="pl">ekstremum</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_u;&c_m;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
    <ind>&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="ekstre.0umejo">
  <kap><tld/>umejo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <ref tip="dif" cel="ekstre.0umiganto"><tld/>umiganto</ref>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_n;&c_k;&c_t;
  &c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_u;&c_m;&c_u;</trd>
  <trd lng="hu">sz&eacute;ls&odblac;&eacute;rt&eacute;khely</trd>
</drv>

<drv mrk="ekstre.0umiganto">
  <kap><tld/>umiganto</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="bild.0igo.MAT">bildigo</ref> al
      <ref cel="ar.orda0o">orda aro</ref>)</klr>
      Punkto de la fonto-aro, &ccirc;e kiu &gcirc;i atingas
      <tld/>umon.
    </dif>
    <rim>
      Ial <fnt><bib>EKV</bib>, <lok>&para;112</lok></fnt>
      nomas tion <ctl><tld/>umo</ctl>, kio ne ebligas distingi inter la
      alprenata minimuma a&ubreve; maksimuma valoro kaj aliflanke la
      punkto,
      &ccirc;e kiu la funkcio alprenas tiun valoron. Koncerne la uzon de
      sufikso <ctl>-ejo</ctl> &ccirc;i-cele vidu rimarkon sub
      <ref cel="maksim1.0iganto">maksimumiganto</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_n;&c_k;&c_t;
  &c_e;&c_k;&c_s;&c_t;&c_r;&c_e;&c_m;&c_u;&c_m;&c_u;</trd>
  <trdgrp lng="de">
    <trd>Extremstelle</trd>,
    <trd>Extremalstelle</trd>
  </trdgrp>
  <trd lng="en">position of an extremum</trd>
  <trd lng="fr">point o&ugrave; la fonction admet un
  extr&eacute;mum</trd>
  <trd lng="hu">sz&eacute;ls&odblac;&eacute;rt&eacute;khely</trd>
  <trd lng="pl">punkt ekstremalny</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_k;&c_a;, &c_v;
  &c_k;&c_o;&c_t;&c_o;&c_r;&c_o;&c_j;
  &c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;
  &c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_je;&c_t;
  &c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_u;&c_m;</trd>
</drv>
</art>
