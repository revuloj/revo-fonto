<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ekstrem</rad>/a
</kap>
<drv mrk="ekstre.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Estanta &ccirc;e la finfino, plej malproksima:
      <ekz>
        <tld/>a Oriento;
      </ekz>
      <ekz>
        deputito el <tld/>a maldekstro;
      </ekz>
      <ekz>
        homo de <tld/>a maljuneco.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Estanta &ccirc;e la plej alta grado:
      <ekz>
        mi sentas nek la <tld/>an
        varmon, nek la <tld/>an malvarmon;
      </ekz>
      <ekz>
        <tld/>a plezuro,
        mildeco, lerteco;
      </ekz>
      <ekz>
        <tld/>igi teorion;
      </ekz>
      <ekz>
        en <tld/>a okazo, en <tld/>a situacio
        <klr>(en okazo de ekstrema bezono, supozante la plej malfavoran
        okazon)</klr>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="fr">extr&ecirc;me<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_r;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">extrem</trd>
</drv>
<drv mrk="ekstre.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Treege, plej:
      <ekz>
        kordo <tld/>e stre&ccirc;ita;
      </ekz>
      <ekz>
        ni <tld/>e bezonas plenan vortaregon<fnt>Z</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>&auml;u&szlig;erst</trd>,
    <trd>extrem</trd>
  </trdgrp>
  <trd lng="fr">extr&ecirc;mement</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_j;&c_n;&c_je;</trd>
  <trd lng="sv">extremt</trd>
</drv>
<drv mrk="ekstre.0o">
  <kap><tld/>o, <tld/>a&jcirc;o</kap>
  <snc mrk="ekstre.0o.KOMUNE">
    <dif>
      La plej malproksima fino, parto:
      <ekz>
        <tld/>o de membroj, de &scirc;ipo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Spitze</trd>,
      <trd>Ende</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ekstre.0o.segmento">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de <ref cel="segmen.0o.linio">segmento</ref>
      a&ubreve; <ref cel="interv.0o.MAT">intervalo</ref>)</klr>
      &Ccirc;iu el la du <ref tip="super" cel="punkt.0o.spaco">punktoj</ref> 
      a&ubreve; <ref tip="super" cel="reel.0o.MAT">reeloj</ref>, kiuj limas 
      &gcirc;in.
    </dif>
  </snc>
  <trd lng="fr">extr&eacute;mit&eacute;</trd>
</drv>
<drv mrk="ekstre.0eco">
  <kap><tld/>o, <tld/>eco</kap>
  <snc>
    <dif>
      La plej alta grado:
      <ekz>
        malaj <tld/>oj estas proksimaj;
      </ekz>
      <ekz>
        peli, pu&scirc;i siajn postulojn &gcirc;is <tld/>o;
      </ekz>
      <ekz>
        la dioj gardu nin de tia <tld/>o
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 25</lok>)</fnt>!
      </ekz>
      <ekz>
        la &gcirc;is <tld/>eco stre&ccirc;ita kordo
        krevis<fnt>Z</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>&Auml;u&szlig;erstes</trd>,
    <trd>Extrem</trd>
  </trdgrp>
  <trd lng="fr">extr&ecirc;me<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_ja;&c_ja; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
