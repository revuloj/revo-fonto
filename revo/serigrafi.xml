<vortaro>
  <art mrk="$Id: serigrafi.xml,v 1.4 2024/03/24 20:00:01 afido Exp $">
    <kap>
      <rad>serigrafi</rad>/o
      <fnt><bib>KrDE</bib></fnt>
    </kap>

<drv mrk="serigrafi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pres.0arto">Pres</ref>te&hcirc;niko, &ccirc;e kiu oni premas la
      farbon tra delikata teksa&jcirc;o, blokante nekolorigendajn lokojn per &scirc;ablono:
      <ekz>
        la Marilina diptiko (1962) estas <tld/>a pentra&jcirc;o de usona popartisto
        <klr>[&#x2026;]</klr> <nom>Andy Warhol</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=521309">Marilina diptiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;ar iam oni uzis silkon kiel teksa&jcirc;o, oni trovas anka&ubreve; la sinonimon silkpresado.
    </rim>
    <trd lng="be">&c_d;&c_r;&c_u;&c_k; &c_t;&c_r;&c_a;&c_f;&c_a;&c_r;&c_e;&c_t;&c_n;&c_y;</trd>
    <trd lng="de">Siebdruck</trd>
    <trd lng="en">screen printing</trd>
    <trd lng="fr">s&eacute;rigraphie</trd>
    <trd lng="pl">sitodruk</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_f;&c_a;&c_r;&c_je;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x4E1D;&#x5370; <pr>s&#x12B;y&#xEC;n</pr></trd>,
    <trd>&#x7D72;&#x5370; <pr>s&#x12B;y&#xEC;n</pr></trd>
  </trdgrp>
</drv>
  </art>

