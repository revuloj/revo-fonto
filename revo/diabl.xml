<drv mrk="diabl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="diabl.0o.Satano">
    <uzo tip="fak">REL</uzo>
    <dif>
      Estro de la malbonaj spiritoj,
      de la <ref tip="malprt" cel="infer.0o.KRI">infero<sncref/></ref>,
      kontra&ubreve;ulo de dio:
      <ekz>
        la <tld/>o lin prenu!
        <klr>(lian animon)</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas ruza kiel la <tld/>o, &scirc;i estas sor&ccirc;istino
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj li ekkaptis la drakon, la antikvan serpenton,
        kiu estas la <tld lit="D"/>o kaj
        <ref tip="sin" cel="satan.0o">Satano</ref>
        <fnt><bib>NT</bib>, <lok>&Apo; 20:2</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Luzifer</trd>,
      <trd>der <ind>Teufel</ind></trd>,
      <trd>Satan</trd>,
      <trd>Beelzebub</trd>,
      <trd>der <ind>Antichrist</ind></trd>
    </trdgrp>
    <trd lng="grc">&delta;&jota;&alfa_acute;&beta;&omikron;&lambda;&omikron;&sigma_fina;</trd>
    <trdgrp lng="pl">
      <trd>Antychryst</trd>,
      <trd>Belzebub</trd>,
      <trd>Lucyfer</trd>,
      <trd>Szatan</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;</trd>
    <trdgrp lng="zh">
    <trd>&#x8DEF;&#x897F;&#x5F17; <pr>l&#xF9;x&#x12B;f&#xFA;</pr></trd>,
    <trd>&#x8DEF;&#x897F;&#x6CD5; <pr>l&#xF9;x&#x12B;f&#x1CE;</pr></trd>,
    <trd>&#x6492;&#x4F46; <pr>s&#x101;d&#xE0;n</pr></trd>,
    <trd>&#x6492;&#x65E6; <pr>s&#x101;d&#xE0;n</pr></trd>,
    <trd>&#x5A11;&#x6B9A; <pr>su&#x14D;d&#x101;n</pr></trd>,
    <trd>&#x6492;&#x6B9A; <pr>s&#x101;d&#x101;n</pr></trd>,
    <trd>&#x654C;&#x57FA;&#x7763; <pr>d&#xED;j&#x12B;d&#x16B;</pr></trd>,
    <trd>&#x53CD;&#x57FA;&#x7763; <pr>f&#x1CE;nj&#x12B;d&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="diabl.0o.demono">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="dif" cel="demon.0o.diablo">Demono<sncref/></ref>,
      unu el la lo&gcirc;antoj de la <ref tip="malprt" cel="infer.0o.KRI">infero<sncref/></ref>:
      <ekz>
        la <tld/>oj &gcirc;in prenu
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne pentru <tld/>on sur la muro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sep <tld/>oj kaj unu <tld/>ino batu al vi la dentojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        li frotis la okulojn kaj ekvidis na&ubreve; malbelajn <tld/>ojn, kiuj rondire dancis &ccirc;irka&ubreve; li
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        per &ccirc;iuj <tld/>oj, ni ne igu la aferon ankora&ubreve; pli malsimpla!
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2000/006937.php">Genroj en la franca: Sinjorino "Le secr&eacute;taire"
          (la sekretario)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Teufel</trd>,
      <trd>D&auml;mon</trd>,
    </trdgrp>
    <trdgrp lng="pl">
      <trd>diabe&lstroke;</trd>,
      <trd>bies</trd>,
      <trd>czart</trd>,
      <trd>czort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_jo;&c_r;&c_t;</trd>,
      <trd>&c_b;&c_je;&c_s;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x9B45; <pr>m&#xE8;i</pr></trd>,
    <trd>&#x6076;&#x9B54; <pr>&#xE8;m&#xF3;</pr></trd>,
    <trd>&#x90AA;&#x7075; <pr>xi&#xE9;l&#xED;ng</pr></trd>,
    <trd>&#x5996; <pr>y&#x101;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="diabl.0o.homo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre malica homo a&ubreve; aganto:
      <ekz>
        li estas vera <tld/>o kun la virinoj!<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ino</ind>;
        <trd lng="fr">diablesse</trd>
        <trd lng="nl">duivelin</trd>
        <trdgrp lng="pl">
          <trd>dialica</trd>,
          <trd>wied&zacute;ma</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_ch;&c_je;&c_r;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>,
          <trd>&c_v;&c_je;&c_d;&c_mol;&c_m;&c_a;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        &scirc;ajnis normala ke ni pensu ke granda <tld/>o de la
        mezepoka E&ubreve;ropo estis Katolikismo
        <fnt>
        <aut>Be&ccirc;jo</aut>,
        <vrk><url ref="http://becxjo.blogspot.com">La mondo el mia
        vidpunkto</url></vrk>,
        <lok>&leftquot;Pri toleremo&rightquot;, 2005-11-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_d;&c_ja;&c_v;&c_o;&c_l;</trd>
    <trdgrp lng="de">
      <trd>Teufel</trd>,
      <trd>Satan</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>diabe&lstroke;</trd>,
      <trd>czort</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_jo;&c_r;&c_t;</trd>,
      <trd>&c_ch;&c_je;&c_r;&c_t;&c_ja;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="zh">&#x8863;&#x51A0;&#x79BD;&#x517D; <pr>y&#x12B;gu&#x101;nq&#xED;nsh&#xF2;u</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_d;'&c_ja;&c_b;&c_a;&c_l;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_sh;&c_a;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xF51;&#xF74;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ert</trd>,
    <trd>&dcaron;&aacute;bel</trd>,
    <trd>&dcaron;&aacute;bel</trd>
  </trdgrp>
  <trd lng="de">Teufel</trd>
  <trd lng="en">devil</trd>
  <trd lng="es">diablo</trd>
  <trd lng="fr">diable</trd>
  <trd lng="he">&shin;&tet;&fnun;</trd>
  <trd lng="hu">&ouml;rd&ouml;g</trd>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x9B54;  <pr>&#x3042;&#x304F;&#x307E;</pr></trd>,
    <trd>&#x9B54;&#x738B;  <pr>&#x307E;&#x304A;&#x3046;</pr></trd>,
    <trd>&#x6975;&#x60AA;&#x4EBA;  <pr>&#x3054;&#x304F;&#x3042;&#x304F;&#x306B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">duivel</trd>
  <trd lng="pt">diabo</trd>
  <trdgrp lng="sk">
    <trd>&ccaron;ert</trd>,
    <trd>diabol</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&#x44F;&c_v;&#x43E;&#x43B;</trd>,
    <trd>&c_ch;&#x43E;&c_r;&c_t;</trd>,
    <trd>&c_d;&c_ib;&c_d;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&c_b;&c_ib;&c_s;</trd>,
    <trd>&#x43A;&c_u;&c_c;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="diabl.0a">
  <kap><tld/>a</kap>
  <snc mrk="diabl.0a.REL">
    <dif>
      De <tld/>o, havanta ties ecojn; inda je <tld/>o; grandefekta:
      <ekz>
        <tld/>a kulto;
      </ekz>
      <ekz>
        <tld/>a ruzo, kaptilo;
      </ekz>
      <ekz>
        kia <tld/>a petolulo!
      </ekz>
      <ekz>
        viza&gcirc;o agrabla kaj ungo <tld/>a
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        estis <tld/>a cirklo, el kiu ne ekzistis fu&gcirc;o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantomurbo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie <klr>[...]</klr> <tld/>a malamo a&ubreve; malica detruemo okazigas kruela&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2000/007074.php">Al pli justa mondo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pro kiu <tld/>a ka&ubreve;zo devas havi Esperanto simplegan gramatikon, se neniu alia havas?
        <fnt>
          <aut>Ott&oacute; Haszpra</aut>
          <vrk><url ref="&Monato;/2000/006803.php">REAGO:
            Supersignoj, pro kiu diabla ka&ubreve;zo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/03, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvieti&gcirc;u, <tld/>aj monstroj, li kriis, sed ili estis pli kaj pli trudemaj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;'&c_ja;&c_b;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_t;&c_o;&c_w;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_t;&c_a;&c_w;</trd>,
    <trd>&c_sh;&c_a;&c_t;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>diabolick&yacute;</trd>,
    <trd>zl&yacute;</trd>,
    <trd>&ccaron;ertovsk&yacute;</trd>,
    <trd>&dcaron;&aacute;belsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>teuflisch</trd>,
    <trd>satanisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>diabolic</trd>,
    <trd>diabolical</trd>,
    <trd>devilish</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>diab&oacute;lico</trd>,
    <trd>diab&oacute;lica</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diabolique</trd>,
    <trd>d&eacute;moniaque</trd>
  </trdgrp>
  <trd lng="he">&shin;&tet;&nun;&yod;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;rd&ouml;gi</trd>,
    <trd>&ouml;rd&ouml;ng&odblac;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x9B54;&#x306E;  <pr>&#x3042;&#x304F;&#x307E;&#x306E;</pr></trd>,
    <trd>&#x60AA;&#x9B54;&#x306E;&#x3088;&#x3046;&#x306A;  <pr>&#x3042;&#x304F;&#x307E;&#x306E;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x60AA;&#x8FA3;&#x306A;  <pr>&#x3042;&#x304F;&#x3089;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x3072;&#x3069;&#x3044;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>duivels</trd>,
    <trd>verduiveld</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>diabelski</trd>,
    <trd>diaboliczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_t;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&ccaron;ertovsk&yacute;</trd>,
    <trd>diabolsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd> &c_d;&c_i;&#x44F;&c_v;&#x43E;&#x43B;&#x44C;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_ch;&#x43E;&c_r;&c_t;&#x44F;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_ch;&#x43E;&c_r;&c_t;&c_ib;&c_v;</trd>,
    <trd>&c_b;&c_ib;&c_s;&c_ib;&c_v;</trd>
  </trdgrp>
  <trd lng="zh">&#x9B54;&#x9B3C;&#x4F3C; <pr>m&#xF3;gu&#x1D0;s&#xEC;</pr></trd>
</drv>

<drv mrk="diabl.0e">
  <kap><tld/>e</kap>
  <snc mrk="diabl.0e.REL">
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e trompi.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>diaboliquement</trd>,
      <trd>diablement</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>diabelsko</trd>,
      <trd>diabolicznie</trd>
    </trdgrp>
  </snc>
  <snc mrk="diabl.0e.FIG">
    <gra><vspec>ekkrio</vspec></gra>
    <dif>
      Kia malbona surprizo!:
      <ekz>
        <tld/>e! mi forgesis la monujon!
      </ekz>
      <ekz>
        kio, <tld/>e, estas poezio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
        </fnt>?
      </ekz>
      <ekz>
        kio, <tld/>e, okazas
        <fnt><aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">KOTA AKVO
           </url></vrk>
           <bib>Monato</bib>
           <lok>2000/03, p. 28</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>diable !</trd>,
      <trd>diantre !</trd>,
      <trd>fichtre !</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>do <ind>diab&lstroke;a</ind>!</trd>,
      <trd>do <ind>czarta</ind>!</trd>,
      <trd>do <ind>licha</ind>!</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;'&c_ja;&c_b;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_t;&c_o;&c_w;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">&dcaron;&aacute;belsky</trd>
  <trdgrp lng="de">
    <trd>Mist</trd>,
    <trd>verdammt</trd>,
    <trd>Schei&szlig;e</trd>
  </trdgrp>
  <trd lng="en">diabolically</trd>
  <trd lng="es">&#xA1;diablos!</trd>
  <trd lng="he">&bet;&alef;&vav;&pe;&fnun;
  <ind>&shin;&tet;&nun;&yod;</ind></trd>
  <trdgrp lng="hu">
    <trd>&ouml;rd&ouml;gien</trd>,
    <trd>&ouml;rd&ouml;ng&odblac;sen</trd>,
    <trd>&ouml;rd&ouml;gi m&oacute;don</trd>,
    <trd>az <ind>&ouml;rd&ouml;gbe!</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x9B54;&#x306E;&#x3088;&#x3046;&#x306B;  <pr>&#x3042;&#x304F;&#x307E;&#x306E;&#x3088;&#x3046;&#x306B;</pr></trd>,
    <trd>&#x60AA;&#x8FA3;&#x306B;  <pr>&#x3042;&#x304F;&#x3089;&#x3064;&#x306B;</pr></trd>,
    <trd>&#x3082;&#x306E;&#x3059;&#x3054;&#x304F;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verduiveld!</trd>,
    <trd>verdomme!</trd>,
    <trd>verdorie!</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_t;&c_o;&c_v;&c_s;&c_k;&c_i;</trd>,
    <trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&ccaron;ertovsky</trd>,
    <trd>diabolsky</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8BE5;&#x6B7B; <pr>g&#x101;is&#x1D0;</pr></trd>,
    <trd>&#x5988;&#x7684; <pr>m&#x101;de</pr></trd>,
    <trd>&#x6211;&#x65E5; <pr>w&#x1D2;r&#xEC;</pr></trd>,
    <trd>&#x4ED6;&#x5988;&#x7684; <pr>t&#x101;m&#x101;de</pr></trd>
  </trdgrp>
</drv>

