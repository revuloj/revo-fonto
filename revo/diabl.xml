<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>diabl</rad>/o
</kap>
<drv mrk="diabl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">REL</uzo>
    <dif>
      Estro de malbonaj an&gcirc;eloj, kiuj ribelis kontra&ubreve; Dio:
      <ekz>ne pentru <tld/>on sur la muro<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="satan.0o">satano</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Luzifer</trd>,
      <trd>der <ind>Teufel</ind></trd>,
      <trd>Satan</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Unu el la ribelintaj an&gcirc;eloj:
      <ekz>
        la <tld/>oj vin prenu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al &ccirc;iuj <tld/>oj<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="demon.0o">demono</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Teufel</trd>,
      <trd>D&auml;mon</trd>,
    </trdgrp>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre malica homo:
      <ekz>
        li estas vera <tld/>o kun la virinoj!<fnt>B</fnt>;
      </ekz>  
      <ekz>
        <ind><tld/>ino</ind>.
        <trd lng="fr">diablesse</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Teufel</trd>,
      <trd>Satan</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>&dcaron;&aacute;bel</trd>,
    <trd>&ccaron;ert</trd>
  </trdgrp>
  <trd lng="fr">diable</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;</trd>,
    <trd>&c_ch;&c_jo;&c_r;&c_t;</trd>
  </trdgrp>
</drv>
<drv mrk="diabl.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, havanta ties ecojn; inda je <tld/>o; grandefekta:
      <ekz>
        <tld/>a kulto;
      </ekz>
      <ekz>
        <tld/>a ruzo, kaptilo;
      </ekz>
      <ekz>
        kia <tld/>a petolulo!
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>teuflisch</trd>,
      <trd>satanisch</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>diabolique</trd>,
    <trd>d&eacute;moniaque</trd></trdgrp> 
</drv>
<drv mrk="diabl.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero; ekkrio esprimanta malbonan surprizon:
      <ekz>
        <tld/>e trompi;
      </ekz>
      <ekz>
        <tld/>e! mi forgesis la monujon!
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>Mist</trd>,
      <trd>verdammt</trd>,
      <trd>Schei&szlig;e</trd>
    </trdgrp>
<trdgrp lng="fr">
  <trd>diaboliquement</trd>,
  <trd>diablement</trd>,
  <trd>diable!</trd>,
  <trd>diantre!</trd>,
  <trd>fichtre!</trd></trdgrp> 
</drv>
<drv mrk="diabl.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Ago inda je <tld/>o:
      <ekz>
        fari <tld/>a&jcirc;ojn hejme, sankta&jcirc;ojn oficeje.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>Teufelei</trd>
    </trdgrp>
  <trd lng="fr">diablerie</trd>
</drv>
<drv mrk="diabl.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Malgranda, komika <tld/>o.
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>Teufelchen</trd>
    </trdgrp>
  <trd lng="fr">diablotin</trd>
</drv>
</art>
