<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><ofc>*</ofc>
      <rad>diabl</rad>/o</kap>

    <drv mrk="diabl.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
	<uzo tip="fak">REL</uzo>
	<dif>
	  Estro de malbonaj an&gcirc;eloj, kiuj ribelis kontra&ubreve; Dio:
	  <ekz>
	    kaj li ekkaptis la drakon, la antikvan serpenton,
	    kiu estas la <tld lit="D"/>o kaj
	    <ref tip="sin" cel="satan.0o">Satano</ref>
	    <fnt><bib>NT</bib>, <lok>&Apo; 20:2</lok></fnt>.
	  </ekz>
	</dif>
	<trdgrp lng="de">
	  <trd>Luzifer</trd>,
	  <trd>der <ind>Teufel</ind></trd>,
	  <trd>Satan</trd>,
	  <trd>Beelzebub</trd>,
	  <trd>der <ind>Antichrist</ind></trd>
	</trdgrp>
	<trdgrp lng="ru">
	  <trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;</trd>,
	  <trd>&c_S;&c_a;&c_t;&c_a;&c_n;&c_a;</trd>
	</trdgrp>
      </snc>
      <snc>
	<uzo tip="fak">KRI</uzo>
	<dif>
	  Unu el la ribelintaj an&gcirc;eloj:
	  <ekz>
	    la <tld/>oj vin prenu<fnt>Z</fnt>;
	  </ekz><ekz>
	    ne pentru <tld/>on sur la muro
	    <fnt><bib>PrV</bib></fnt>
	  </ekz><ekz>
	    al &ccirc;iuj <tld/>oj<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="demon.0o">demono</ref>.
	</refgrp>
	<trdgrp lng="de">
	  <trd>Teufel</trd>,
	  <trd>D&auml;mon</trd>,
	</trdgrp>
	<trdgrp lng="ru">
	  <trd>&c_ch;&c_jo;&c_r;&c_t;</trd>,
	  <trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;</trd>
	</trdgrp>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Tre malica homo:
	  <ekz>
	    li estas vera <tld/>o kun la virinoj!<fnt>B</fnt>;
	  </ekz><ekz>
	    <ind><tld/>ino</ind>.
	    <trd lng="fr">diablesse</trd>
	    <trd lng="nl">duivelin</trd>
	  </ekz>
	</dif>
	<trdgrp lng="de">
	  <trd>Teufel</trd>,
	  <trd>Satan</trd>
	</trdgrp>
	<trd lng="ru">&c_ch;&c_jo;&c_r;&c_t;</trd>
      </snc>
      <trdgrp lng="cs">
	<trd>&dcaron;&aacute;bel</trd>,
	<trd>&ccaron;ert</trd>
      </trdgrp>
      <trd lng="es">diablo</trd>
      <trd lng="fr">diable</trd>
      <trd lng="hu">&ouml;rd&ouml;g</trd>
      <trd lng="nl">duivel</trd>
      <trdgrp lng="sk">
	<trd>&ccaron;ert</trd>,
	<trd>diabol</trd>
      </trdgrp>
    </drv>

    <drv mrk="diabl.0a">
      <kap><tld/>a</kap>
      <snc>
	<dif>
	  De <tld/>o, havanta ties ecojn; inda je <tld/>o; grandefekta:
	  <ekz>
	    <tld/>a kulto;
	  </ekz><ekz>
	    <tld/>a ruzo, kaptilo;
	  </ekz><ekz>
	    kia <tld/>a petolulo!
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="de">
	<trd>teuflisch</trd>,
	<trd>satanisch</trd>
      </trdgrp>
      <trd lng="es">diab&oacute;lico</trd>
      <trdgrp lng="fr">
	<trd>diabolique</trd>,
	<trd>d&eacute;moniaque</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>duivels</trd>,
	<trd>verduiveld</trd>
      </trdgrp>
      <trdgrp lng="hu">
	<trd>&ouml;rd&ouml;gi</trd>,
	<trd>&ouml;rd&ouml;ng&odblac;s</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
	<trd>&c_ch;&c_je;&c_r;&c_t;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
      </trdgrp>
      <trdgrp lng="sk">
	<trd>&ccaron;ertovsk&yacute;</trd>,
	<trd>diabolsk&yacute;</trd>
      </trdgrp>
    </drv>

    <drv mrk="diabl.0e">
      <kap><tld/>e</kap>
      <snc>
	<dif>
	  En <tld/>a maniero; ekkrio esprimanta malbonan surprizon:
	  <ekz>
	    <tld/>e trompi;
	  </ekz><ekz>
	    <tld/>e! mi forgesis la monujon!
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="de">
	<trd>Mist</trd>,
	<trd>verdammt</trd>,
	<trd>Schei&szlig;e</trd>
      </trdgrp>
      <trdgrp lng="fr">
	<trd>diaboliquement</trd>,
	<trd>diablement</trd>,
	<trd>diable!</trd>,
	<trd>diantre!</trd>,
	<trd>fichtre!</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>verduiveld!</trd>,
	<trd>verdomme!</trd>,
	<trd>verdorie!</trd>
      </trdgrp>
      <trdgrp lng="hu">
	<trd>&ouml;rd&ouml;gien</trd>,
	<trd>&ouml;rd&ouml;ng&odblac;sen</trd>,
	<trd>&ouml;rd&ouml;gi m&oacute;don</trd>,
	<trd>az <ind>&ouml;rd&ouml;gbe!</ind></trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_ch;&c_je;&c_r;&c_t;&c_o;&c_v;&c_s;&c_k;&c_i;</trd>,
	<trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;</trd>
      </trdgrp>
      <trdgrp lng="sk">
	<trd>&ccaron;ertovsky</trd>,
	<trd>diabolsky</trd>
      </trdgrp>
    </drv>

    <drv mrk="diabl.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
	<dif>
	  Ago inda je <tld/>o:
	  <ekz>
	    fari <tld/>a&jcirc;ojn hejme, sankta&jcirc;ojn oficeje.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="de">
	<trd>Teufelei</trd>
      </trdgrp>
      <trd lng="es">diablura</trd>
      <trd lng="fr">diablerie</trd>
      <trd lng="hu">&ouml;rd&ouml;ng&odblac;ss&eacute;g</trd>
      <trd lng="nl">iets <ind>duivels</ind></trd>
      <trd lng="ru">&c_ch;&c_je;&c_r;&c_t;&c_o;&c_v;&c_shch;&c_i;&c_n;&c_a;</trd>
      <trd lng="sk">&ccaron;ertovina</trd>
    </drv>

    <drv mrk="diabl.0eto">
      <kap><tld/>eto</kap>
      <snc>
	<dif>
	  Malgranda, komika <tld/>o.
	</dif>
      </snc>
      <trdgrp lng="de">
	<trd>Teufelchen</trd>
      </trdgrp>
      <trd lng="fr">diablotin</trd>
      <trd lng="hu">kis&ouml;rd&ouml;g</trd>
      <trd lng="nl">duiveltje</trd>
      <trd lng="ru">&c_ch;&c_jo;&c_r;&c_t;&c_i;&c_k;</trd>
      <trd lng="sk">&ccaron;ert&iacute;k</trd>
    </drv>
  </art>
