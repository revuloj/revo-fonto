<art mrk="$Id: basketbal.xml,v 1.17 2021/04/07 07:01:20 revo Exp $">
<kap>
  <rad>basketbal</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="basketbal.0o">
  <kap><tld/>o</kap>
  <snc mrk="basketbal.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="pilk.korbo0ado">Korbopilkado</ref>:
      <ekz>
        &scirc;i &scirc;atas sportojn <klr>[&#8230;]</klr> aparte tenison, <tld/>on
        <fnt>
          <aut>E. Ellemberg</aut>,
          <vrk>Novembro 13a, Esperanto Koresponda Servo</vrk>,
          <lok><url
          ref="http://groups.google.com/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
          1998-11-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; tio ke la plej gravaj usonaj sportoj evoluis el e&ubreve;ropaj praktikoj,
        <tld/>o, flugpilko <klr>[&#8230;]</klr> estas usonaj inventoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4287">Usono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_k;&c_je;&c_t;&c_b;&c_o;&c_l;</trd>
  <trd lng="bg">&c_b;&c_a;&c_s;&c_k;&c_je;&c_t;&c_b;&c_o;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>b&agrave;squet</trd>,
    <trd>basquetbol</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>basketbal</trd>,
    <trd>ko&scaron;&iacute;kov&aacute;</trd>
  </trdgrp>
  <trd lng="de">Basketball</trd>
  <trd lng="fa">&a_b;&a_s;&f_k;&a_t;&a_b;&a_A;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>basket</trd>,
    <trd>basketball</trd>
  </trdgrp>
  <trd lng="he">&#x5DB;&#x5D3;&#x5D5;&#x5E8;&#x5E1;&#x5DC;</trd>
  <trd lng="hu">kos&aacute;rlabda</trd>
  <trdgrp lng="it">
    <trd>pallacanestro</trd>,
    <trd>basket</trd>,
    <trd>basketball</trd>
  </trdgrp>
  <trd lng="nl">basketbal</trd>
  <trd lng="pl">koszyk&oacute;wka</trd>
  <trdgrp lng="pt">
    <trd>basquetebol</trd>,
    <trd>basquete</trd>
    <trd>bola-ao-cesto</trd>,
    <trd>bola-no-cesto</trd>
  </trdgrp>
  <trd lng="ro">baschet</trd>
  <trd lng="sk">basketbal</trd>
  <trd lng="tr">basketball</trd>
</drv>
</art>

