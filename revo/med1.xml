<vortaro>
  <art mrk="$Id: med1.xml,v 1.4 2023/10/19 15:13:16 afido Exp $">
    <kap>
      <rad>med</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="med1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="vin.miel0o">Mielvino</ref>
      (fermentinta <ref cel="miel.trink0o">trinkmielo</ref>
      a&ubreve; <ref cel="akv.miel0o">mielakvo</ref>):
      <ekz>
        vespere kun varma <tld/>o en bele aran&gcirc;ita restoracio oni plu konati&gcirc;is
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#x2116; 4 (114)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_jo;&c_d; <klr tip="amb">(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
    <trd lng="de">Met</trd>
    <trd lng="en">mead</trd>
    <trd lng="fr">hydromel</trd>
  </snc>
  <ref tip="hom" cel="med.0o">medo (irana popolo)</ref>
  <trd lng="ja">&#x8702;&#x871C;&#x9152;  <pr>&#x306F;&#x3061;&#x307F;&#x3064;&#x3057;&#x3085;</pr></trd>
</drv>

  </art>

