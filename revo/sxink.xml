<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxink.xml,v 1.25 2024/03/25 07:28:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ink</rad>/o
</kap>

<drv mrk="sxink.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Salita a&ubreve; fuma&jcirc;ita femuro de <ref tip="malprt" cel="pork.0o">porko</ref>;
      peco da &gcirc;i:
      <ekz>
        sur la tablo staras malplena glaseto kaj telero kun duono da pantran&ccirc;o kun
        <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la provizejo <klr>[&#x2026;]</klr> <tld/>oj pendas sub la plafono
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu ankora&ubreve; du panojn kaj unu <tld/>on, por ke vi ne bezonu malsati
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelke da rostitaj terpomoj kaj bongustan grasan pecon da <tld/>o &scirc;i ricevis por
        la malsanulino
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la plafono pendis kolbasoj kaj <tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Marta a&ccirc;etis funton da fuma&jcirc;ita <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tran&ccirc;a&jcirc;oj de <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni &ccirc;iam man&gcirc;is <klr>[&#x2026;]</klr> sabate <tld/>on kun brasiko
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_n;&c_d;&c_l;&c_ib;&c_n;&c_a;</trd>, 
    <trd>&c_sh;&c_y;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_sh;&c_u;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">pernil</trd>
  <trd lng="en">ham</trd>
  <trd lng="es">jam&oacute;n</trd>
  <trd lng="fa">&f_zh;&a_A;&a_m;&a_b;&a_w;&a_n;</trd>
  <trd lng="fr">jambon</trd>
  <trd lng="de">Schinken</trd>
  <trd lng="hu">sonka</trd>
  <trd lng="it">prosciutto</trd>
  <trd lng="ja">&#x30CF;&#x30E0;  <pr>&#x306F;&#x3080;</pr></trd>
  <trd lng="nl">ham</trd>
  <trd lng="pl">szynka</trd>
  <trdgrp lng="pt">
    <trd>fiambre</trd>,
    <trd>presunto</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_t;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">skinka</trd>
  <trdgrp lng="uk">
    <trd>&#x448;&#x438;&#x43D;&#x43A;&#x430;</trd>,
    <trd>&#x43E;&#x43A;&#x456;&#x441;&#x442;</trd>
  </trdgrp>
  <trd lng="zh">&#x706B;&#x817F; <pr>hu&#x1D2;tu&#x1D0;</pr></trd>
</drv>
</art>
