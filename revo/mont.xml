  </snc>
  <trd lng="be">&c_u;&c_z;&c_v;&c_y;&c_sh;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_ib;&c_b;&c_y; &c_g;&c_a;&c_r;&c_a;)</klr></trd>
  <trdgrp lng="de">
