<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mont</rad>/o
</kap>
<drv mrk="mont.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda natura amaso da roko kaj tero, kiu tre alti&gcirc;as super la
      nivelo de la &ccirc;irka&ubreve;anta grundo:
      <ekz>
        la ondegoj levi&gcirc;as kiel <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>jen staras la bovoj anta&ubreve; la <tld/>o
        <klr>(f: jen la malfacila&jcirc;o!)</klr>
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Amasego, multego:
      <ekz>
        <tld/>oj da kadavroj, da ruba&jcirc;oj, da papera&jcirc;oj;
      </ekz>
      <ekz>promesi orajn <tld/>ojn
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        ni ne povas senfine toleri tiun <tld/>on da mizero;
      </ekz>
      <ekz>inter faro kaj rakonto staras meze granda <tld/>o
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">hora</trd>
  <trd lng="de">Berg</trd>
  <trd lng="en">mountain</trd>
  <trd lng="hu">hegy</trd>
  <trd lng="nl">berg</trd>
  <trd lng="os">&c_h;&c_o;&c_h;</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_a;</trd>
  <trd lng="sv">berg</trd>
</drv>

<drv mrk="mont.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Masivo a&ubreve; &ccirc;eno da kunigitaj <tld/>oj formantaj tuton:
      <ekz>
        la Ka&ubreve;kaza, Pirenea <tld/>aro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gebirge</trd>
  <trd lng="hu">hegys&eacute;g</trd>
  <trd lng="nl">gebergte</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_y;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_n;&c_y;&c_j; &c_m;&c_a;&c_s;&c_s;&c_i;&c_v;</trd>
  </trdgrp>
  <trd lng="sv">bergskedja</trd>
</drv>

<drv mrk="mont.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Alti&gcirc;i kvaza&ubreve; <tld/>o:
      <ekz>
        de via kolera blovo <tld/>i&gcirc;is akvo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="mont.inter0o0pasejo">
  <kap>inter<tld/>o, <tld/>pasejo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Malpli alta trairejo inter du <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bergpass</trd>,
    <trd>Pass</trd>
  </trdgrp>
  <trd lng="hu">h&aacute;g&oacute;</trd>
  <trd lng="nl">bergpas</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_a;&c_l;</trd>
</drv>

<drv mrk="mont.0bastono">
  <kap><tld/>bastono</kap>
  <snc>
    <dif>
      Longa bastono kun fera pinto uzata en <tld/>oj por faciligi
      grimpadon.
    </dif>
  </snc>
  <trd lng="nl">bergstok</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_p;&c_je;&c_n;&c_sh;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="mont.0fendo">
  <kap><tld/>fendo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Interkrutejo.
    </dif>
  </snc>
  <trd lng="nl">bergkloof</trd>
  <trd lng="ru">&c_u;&c_shch;&c_je;&c_l;&c_mol;&c_je;</trd>
</drv>
</art>
