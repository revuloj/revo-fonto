<drv mrk="mont.0fendo">
      <kap><tld/>fendo</kap>
      <snc mrk="mont.0fendo.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Interkrutejo:
      <ekz>la levi&gcirc;anta suno trovis nin torde rampantaj la&ubreve;
      interkruta&jcirc;oj kaj <tld/>fendoj &ccirc;iam pli kaj pli supren
        <fnt>
          <aut>S. Bergman, trad. B. Gerdman</aut>
          <vrk>Tra sova&gcirc;a Kam&ccirc;atko</vrk>
          <lok>1932</lok>
        </fnt>;
      </ekz>
      <ekz>&Ccirc;iu <tld/>fendo havis sian tonon, kaj &ccirc;iu <tld/>a deklivo
      propran sonon
        <fnt>
          <aut>Selma Lagerl&ouml;f</aut>
          <vrk>Uzino 2</vrk>
          <lok>La senpaculoj</lok>
        </fnt>.
      </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">horsk&aacute; str&zcaron;</trd>
  <trd lng="fr">crevasse</trd>
  <trd lng="nl">bergkloof</trd>
  <trd lng="ru">&c_u;&c_shch;&c_je;&c_l;&c_mol;&c_je;</trd>
  <trd lng="sk">horsk&aacute; str&zcaron;</trd>
</drv>

<drv mrk="mont.0oprediko">
      <kap><tld/>oprediko</kap>
      <snc mrk="mont.0oprediko.KRI">
        <uzo tip="fak">KRI</uzo>
        <dif>
          Prediko farita de <ref cel="jesu.0o">Jesuo</ref> sur
          (la&ubreve; Mateo) a&ubreve; apud (la&ubreve; Luko) <tld/>o al
          siaj dis&ccirc;iploj kaj granda homamaso:
          <ekz>
            min mem trafis, ekzemple, la paragrafoj 612&ndash;621
            kvaza&ubreve; e&hcirc;oj de l' t.n. <tld/>oprediko de Jesuo en la
            evangelio la&ubreve; Mateo
            <fnt>
              <aut>G. Berveling</aut>
              <vrk>
                <url ref="http://www.esperanto.be/fel/mon/rec/dvoj.html">
                  Sankta Skribo de Oomoto en Esperanto
                </url>
              </vrk>.
              <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Bergpredigt</trd>
      <trd lng="en">Sermon on the Mount</trd>
      <trd lng="fr">sermon sur la montagne</trd>
      <trd lng="ru">&c_n;&c_a;&c_g;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_p;&c_o;&c_v;&c_je;&c_d;&c_mol;</trd>
</drv>
