    <trd>sich <ind>aufh&auml;ufen</ind></trd>
  </trdgrp>
  <trd lng="fr">s'<ind>amonceler</ind></trd>
  <trd lng="id"><baz>gunung</baz>menggunung</trd>
  <trdgrp lng="ja">
    <trd>&#x9AD8;&#x304F;&#x306A;&#x308B;  <pr>&#x305F;&#x304B;&#x304F;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x6301;&#x3061;&#x4E0A;&#x304C;&#x308B;  <pr>&#x3082;&#x3061;&#x3042;&#x304C;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">g&oacute;rowa&cacute;</trd>
