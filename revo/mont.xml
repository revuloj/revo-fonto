<drv mrk="mont.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="mont.0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Granda natura amaso da roko kaj tero, kiu tre alti&gcirc;as
          super la nivelo de la &ccirc;irka&ubreve;anta grundo:
          <ekz>
            la ondegoj levi&gcirc;as kiel <tld/>oj<fnt>Z</fnt>;
          </ekz><ekz>
            jen staras la bovoj anta&ubreve; la <tld/>o
            <klr>(jen la malfacila&jcirc;o!)</klr>
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="mont.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Amasego, multego:
          <ekz>
            <tld/>oj da kadavroj, da ruba&jcirc;oj, da papera&jcirc;oj;
          </ekz>
          <ekz>
            ni ne povas senfine toleri tiun <tld/>on da mizero;
          </ekz>
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>promesi orajn <tld/>ojn</ind>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="ru">&c_s;&c_u;&c_l;&c_i;&c_t;&c_mol;
              <ind>&c_z;&c_o;&c_l;&c_o;&c_t;&c_y;&c_je; &c_g;&c_o;&c_r;&c_y;</ind>
            </trd>
          </ekz>
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>inter faro kaj rakonto staras meze granda <tld/>o</ind>
            <fnt><bib>PrV</bib></fnt>
            <trd lng="es">del dicho al hecho, hay mucho trecho</trd>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="abism.0o">abismo</ref>
        </refgrp>
      </snc>
