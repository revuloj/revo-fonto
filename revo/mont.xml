  <trd lng="zu">igquma</trd>
</drv>

<drv mrk="mont.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="alt.0igxi">Alti&gcirc;i</ref> kvaza&ubreve; <tld/>o:
      <ekz>
        de via kolera blovo <tld/>i&gcirc;is akvo
        <fnt><bib>MT</bib><lok>&Eli; 15:8</lok></fnt>;
      </ekz>
      <ekz>
        la &ccirc;ifita papero <tld/>i&gcirc;is kaj &scirc;veli&gcirc;is kiel ondo sur la
        pagtablo pli kaj pli
        <fnt>
          <aut>H. G. Wells, trad. Raoul Bricard</aut>
          <vrk>Uzino 5</vrk>
          <lok>La magia butiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la batalo la manoj de la triumfanta Napoleono tremis same mizere, kiel tiuj
        de Henriko, kaj la cigarstumpoj same <tld/>i&gcirc;is anta&ubreve; ili amba&ubreve;
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="amas.0igxi">amasi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_v;&c_y;&c_sh;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_ib;&c_b;&c_y; &c_g;&c_a;&c_r;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>sich <ind>anh&auml;ufen</ind></trd>,
    <trd>sich <ind>aufh&auml;ufen</ind></trd>
  </trdgrp>
  <trd lng="fr">s'<ind>amonceler</ind></trd>
  <trd lng="pl">g&oacute;rowa&cacute;</trd>
  <trd lng="ro">superioar&abreve;</trd>
</drv>

<drv mrk="mont.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="mont.antaux0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
       Malpli alta parto de monto a&ubreve; montaro, kiu apudas ebena&jcirc;on:
      <ekz>
        fora pina arbaro la&ubreve;longe de la rando de anta&ubreve;<tld/>oj
        <fnt>
          <vrk><url ref="https://www.rulit.me/books/tais-el-ateno-read-292394-64.html">Tais el
          Ateno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Katedrala Vojeto krucis la &scirc;oseon en la anta&ubreve;<tld/>oj
        <fnt>
          <vrk><url ref="https://mir-knig.com/read_116022-17">&c_Ch;&c_i;&c_t;&c_a;&c_t;&c_mol; Naski?o de la Rustimuna
          ?talrato &c_o;&c_n;&c_l;&c_a;&c_j;&c_n; (&c_p;&c_o;&c_l;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;...)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
         por solvi la problemon pri akvomanko en anta&ubreve;<tld/>oj de Soaro
         estas decidite fosi specialan kanalon de la rivero &gcirc;is la
         <tld/>aro
        <fnt>
          <aut>K. Buli&ccirc;jov, trad. P. Mo&jcirc;aev</aut>
          <vrk><url ref="http://esperantio.narod.ru/ekster/silentu.htm">
          Silentu, simpla marsano!</url></vrk>
          <lok>vidita en 2018]</lok>
        </fnt>.
       </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="pied.0o.FIG">piedo<sncref/></ref>,
       <ref cel="promon.0o">promontoro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_g;&c_o;&c_r;&#8217;&c_je;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_g;&c_o;&c_r;&#8217;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausl&auml;ufer</trd>,
    <trd>Bergvorland</trd>,
    <trd>Fu&szlig; <klr>(Berg)</klr></trd>
  </trdgrp>
  <trd lng="fr">contrefort<klr> (montagneux)</klr></trd>
  <trd lng="pl">przedg&oacute;rze</trd>
  <trd lng="ro">poalele muntelui</trd>
</drv>

<drv mrk="mont.inter0o">
  <kap>inter<tld/>o, <var><kap><tld/>pasejo</kap></var></kap>
  <snc mrk="mont.inter0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Malpli alta trairejo inter du <tld/>oj:
      <ekz>
        en &ccirc;iuj inter<tld/>oj de la &ccirc;irka&ubreve;a&jcirc;o sin ka&scirc;as egiptaj
        regimentoj
        <fnt><bib>Far2</bib> <lok>&ccirc;apitro 17a</lok></fnt>;
      </ekz>
      <ekz>
        por trapasi pitoreskan inter<tld/>on plurkoloran, la trajno alproksimi&gcirc;as al la
        altebena&jcirc;o <klr>[&#8230;]</klr> je 3500 metroj
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php">Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda armeo estis sendita sudokcidenten por transiri la
        altajn <tld/>pasejojn
        <fnt>
          <aut>A. Lundkvist, trad. B. Nilsson</aut>
          <vrk>La volo de l' &ccirc;ielo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_l;</trd>
  <trdgrp lng="de">
    <trd>Bergpass</trd>,
    <trd>Pass</trd>
  </trdgrp>
  <trd lng="en">(mountain) <ind>pass</ind></trd>
  <trd lng="es">paso</trd>
  <trd lng="fr">col<klr> (de montagne)</klr></trd>
  <trd lng="hu">h&aacute;g&oacute;</trd>
  <trd lng="nl">bergpas</trd>
  <trd lng="pl">prze&lstroke;&eogonek;cz</trd>
  <trdgrp lng="ro">
    <trd>trec&abreve;toare</trd>,
    <trd>pasul</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_a;&c_l;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_ib;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j; &#x43F;&c_r;&#x43E;&c_h;&c_ib;&c_d;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_v;&c_a;&#x43B;</trd>
  </trdgrp>
</drv>

<drv mrk="mont.tablo0o">
  <kap>tablo<tld/>o <fnt>PIV2</fnt></kap>
  <snc mrk="mont.tablo0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="M"/>o kun ebena supra&jcirc;o:
      <ekz>
        la tablo<tld/>o de Kaburbo situas en la norda parto de monto&ccirc;eno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=263426">Tablomonto (Sudafriko)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la 448 metrojn alta tablo<tld/>o <nom>Gohrisch</nom>
        situas <klr>[&#8230;]</klr> maldekstraflanke de la rivero Elbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40738">Gohrisch (monto)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_g;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;-&c_s;&c_t;&c_o;&c_l;</trd>
  </trdgrp>
  <trd lng="de">Tafelberg</trd>
  <trd lng="en">mesa</trd>
  <trdgrp lng="fr">
    <trd>mesa</trd>,
    <trd>plateau<klr tip="ind"> (g&eacute;og.)</klr></trd>
  </trdgrp>
  <trd lng="pl">g&oacute;ra sto&lstroke;owa</trd>
  <trd lng="ro">munte de mas&abreve;</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_g;&c_o;&c_r;&c_a;</trd>
  <trd lng="uk">&c_s;&c_t;&#x43E;&#x43B;&#x43E;&c_v;&c_a; &c_g;&#x43E;&c_r;&c_a;</trd>
</drv>

<drv mrk="mont.Tablo0o">
  <kap>Tablo<tld/>o
  <fnt>
    <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
    <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
    universo</vrk>.
    <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <snc mrk="mont.Tablo0o.AST">
    <uzo tip="fak">AST</uzo>
    <mlg>Men</mlg>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">Stelfiguro</ref>
      en la suda duonsfero (<trd lng="la">Mensa</trd>):
      <ekz>
        NGC 1754 estas globa stelamaso en konstelacio Tablo<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=111411">NGC 1754</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_t;&c_a;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_G;&c_a;&c_r;&c_a; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="de">Tafelberg</trd>
  <trd lng="en">Mensa</trd>
  <trd lng="es">Mesa</trd>
  <trd lng="fr">Table<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="pl">G&oacute;ra Sto&lstroke;owa<klr> (gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Platoul  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="ru">&c_S;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_G;&c_o;&c_r;&c_a;<klr>
  (&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="mont.tomb0eto">
  <kap>tomb<tld/>eto</kap>
  <snc mrk="mont.tomb0eto.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Tera a&ubreve; &scirc;tona amaso super tombo, &gcirc;in
      &scirc;irmanta kontra&ubreve; besta a&ubreve; homa rabado;
      <ref tip="sin" cel="tumul.0o.ARKE">tumulo</ref>:
      <ekz>
        diference de la anta&ubreve;historia tomb<tld/>eto, havanta
        konusan formon, la vikingaj tomb<tld/>etoj estas kutime
        plataj
        <fnt><bib>IntArkeol</bib>, <lok>p. 41a</lok></fnt>;
      </ekz>
      <ekz>sub la tomb<tld/>eton estis enterigita la trovito
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>
          <vrk>Surprizitaj nuptofestantoj</vrk>
          <lok>&ccirc;apitro 12a</lok>
        </fnt>;
      </ekz>
      <ekz>Ili sidas tie kaj ludas, kvaza&ubreve; ili sidus sur sia propra
      tombo<tld/>eto
        <fnt><bib>Mortul&Scirc;ip</bib> <lok>dua libro, &ccirc;apitro
        44a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trd lng="cs">rov</trd>
  <trd lng="de">Grabh&uuml;gel</trd>
  <trd lng="en">(burial) <ind>mound</ind></trd>
  <trd lng="es">t&uacute;mulo</trd>
  <trd lng="fr">tumulus</trd>
  <trd lng="pl">kurchan</trd>
  <trd lng="ro">movil&abreve; funerar&abreve; </trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>hrob</trd>,
    <trd>rov</trd>
  </trdgrp>
</drv>

