<drv mrk="mont.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mont.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda natura <ref tip="super" cel="amas.0o">amaso</ref>
      da roko, tero, glacio, kiu tre alti&gcirc;as
      super la nivelo de la &ccirc;irka&ubreve;anta grundo:
      <ekz>
        en nia lando sin ne trovas <tld/>oj, sed nur <tld/>etoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 38</lok>
        </fnt>;
      </ekz>
      <ekz>
        alte sur la pintoj de la <tld/>oj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        Moseo malsupreniris de la <tld/>o al la popolo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        fumo levi&gcirc;adis <klr>[&#x2026;]</klr> kaj la tuta <tld/>o forte tremis
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen al vi pla&ccirc;is <tld/>o, jen <ref tip="ant" cel="val.0o">valo</ref>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kaj Aftanido sidis enpensi&gcirc;inte &ccirc;e <ref tip="prt" cel="dekliv.0o">deklivo</ref> de
        <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;eno da <tld/>oj elstaris pli alte ol alia
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sidis trankvile sur sia na&gcirc;anta glacia <tld/>o kaj vidis, kiel la fajraj
        fulmoj zigzage sin &jcirc;etadis en la &scirc;a&ubreve;man maron
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel grandaj nigraj <tld/>oj sin levadis la &scirc;a&ubreve;manta akvo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen staras la bovoj anta&ubreve; la <tld/>o
        <klr>(jen la malfacila&jcirc;o!)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alt.0ajxo">alta&jcirc;o</ref>,
      <ref cel="krest.0o.GEOG">kresto<sncref/></ref>
    </refgrp>
  <trd lng="en">mountain <klr>(geography)</klr></trd>
  </snc>
  <snc mrk="mont.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="amas.0o">Amaso</ref>, <ref tip="sin" cel="mult.0ego">multego</ref>:
      <ekz>
        kolektoj kaj <tld/>oj da plenaskribitaj negocaj paperoj kaj frajtleteroj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        muziko efektive povas elprofundigi diversajn pli noblajn emojn de lia naturo,
        surpremitajn de <tld/>oj da egoismo, dibo&ccirc;emo kaj krimoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>promesi orajn <tld/>ojn</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="be">&c_a;&c_b;&c_ja;&c_c;&c_a;&c_c;&c_mol; &c_z;&c_a;&c_l;&c_a;&c_t;&c_y;&c_ja; <ind>&c_g;&c_o;&c_r;&c_y;</ind></trd>
        <trd lng="ru">&c_s;&c_u;&c_l;&c_i;&c_t;&c_mol;
          <ind>&c_z;&c_o;&c_l;&c_o;&c_t;&c_y;&c_je; &c_g;&c_o;&c_r;&c_y;</ind>
        </trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>inter faro kaj rakonto staras meze granda <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="es">del dicho al hecho, hay mucho trecho</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>,
      <ref cel="stak.0o.fojna">stako</ref>
    </refgrp>
