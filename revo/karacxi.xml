  </snc>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30C1;</trd>
  <trd lng="pl">Karaczi</trd>
  <trd lng="uk">&#x41A;&c_a;&c_r;&c_a;&c_ch;&c_ib;</trd>
