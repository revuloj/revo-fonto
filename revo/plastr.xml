<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plastr</rad>/o
</kap>
<drv mrk="plastr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Medikamento el glueca substanco, per kiu oni &scirc;miras pecon de tolo,
      kiun oni metas sur la kuracota korpa parto.
    </dif>
    <refgrp tip="sin">
      <ref cel="katapl.0o">kataplasmo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Pflaster</trd>
  <trd lng="nl">pleister</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;</trd>
</drv>
</art>
