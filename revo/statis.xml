<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc> <rad>statistik</rad>/o</kap>
<drv mrk="statis.0o">
  <kap><tld/>o</kap>
  <snc mrk="statis.0o.matematika">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="matema.0o">matematiko</ref> kiu esploras grandkvantajn
      aferojn per distingo de parametroj kaj kalkulo de &scirc;ancoj.
    </dif>
  </snc>
  <snc mrk="statis.0o.socia">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Scienco celanta metodan klasigon de tiuj el la sociaj faktoj, kiuj
      estas mezureblaj kaj nombreblaj.
    </dif>
  </snc>
  <snc mrk="statis.0o.tabelo">
    <dif>
      Tabelo starigita la&ubreve; la metodoj de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Statistik</trd>
  <trdgrp lng="fr">
    <trd>statistique</trd>
  </trdgrp>
  <trd lng="nl">statistiek</trd>
  <trd lng="sv">statistik</trd>
</drv>
</art>
