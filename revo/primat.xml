<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: primat.xml,v 1.20 2022/05/28 17:34:44 revo Exp $">
<kap>
  <rad>primat</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="primat.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Iu el <tld/>oj:
      <ekz>
        longa digesto-tubo de frukto-man&gcirc;anta <tld/>o
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>nehetnatec</trd>,
    <trd>prim&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Primat</trd>,
    <trd>Herrentier</trd>
  </trdgrp>
  <trd lng="en">primate</trd>
  <trd lng="fr">primate</trd>
  <trd lng="hu">f&odblac;eml&odblac;s</trd>
  <trd lng="nl">primaat</trd>
  <trd lng="pl">ma&lstroke;pa cz&lstroke;ekokszta&lstroke;tna</trd>
  <trd lng="pt">primata</trd>
  <trd lng="ro">goril&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_m;&c_a;&c_t;</trd>
  <trd lng="sk">prim&aacute;t</trd>
</drv>


<drv mrk="primat.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="primat.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref> de <ref tip="malprt"
      cel="mam.0uloj">mamuloj</ref>,
      al kiu apartenas i.a. <ref tip="sub" cel="simi.0o">simio</ref>j kaj <ref tip="sub"
      cel="hom.0o">homo</ref>
      havanta nur ok incizivojn kaj anta&ubreve;enrigardantajn okulojn:
      <ekz>
        la terciaro komenci&gcirc;is anta&ubreve; sesdeko da jarmilionoj kaj en tiu epoko vaste
        evoluis la mamuloj, inter kiuj la unuaj plej primitivaj <tld/>oj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        anatomie la homo, kune kun la prasimioj kaj simioj, apartenas al la grupo de
        <tld/>oj
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="pl">naczelne</trd>
  <trd lng="ro">primate</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&#x43C;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
