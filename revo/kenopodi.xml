<vortaro>
<art mrk="$Id$">
<kap>
<rad>kenopodi</rad>/o
</kap>
<drv mrk="kenopodi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kenopodi.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Chenopodium</trd>).
    </dif>
  </snc>
  <trd lng="de">G&auml;nsefu&szlig;</trd>
</drv>
<drv mrk="kenopodi.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Chenopodiaceae</trd>).
    </dif>
    <refgrp tip="sub">
      <ref cel="bet.0o">beto</ref>,
      <ref cel="spinac.0o">spinaco</ref>
    </refgrp>
  </snc>
  <trd lng="de">G&auml;nsefu&szlig;gew&auml;chse</trd>
</drv>
</art>

