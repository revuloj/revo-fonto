<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flar.xml,v 1.35 2024/03/05 01:19:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flar</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="flar.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flar.0i.SEN">
    <dif>
      <ref tip="super" cel="sent.0i.sensi">Senti</ref> odorojn per la
      nazo:
      <ekz>
        la hundo <tld/>is leporon;
      </ekz><ekz>
        Okulojn ili havas, sed ne vidas,
        Orelojn ili havas, sed ne a&ubreve;das,
        Nazon ili havas, sed ne <tld/>as
        <fnt><bib>MT</bib>, <lok>&Psa; 115:5,6</lok></fnt>;
      </ekz><ekz>
         Kaj li <klr>(Jakobo) </klr> alproksimi&gcirc;is kaj kisis lin,
         kaj tiu <klr>(Isaako) </klr> <tld/>is la odoron de liaj vestoj
	 <fnt><bib>MT</bib>, <lok>&Gen; 27:27</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol;
      <ind>&c_p;&c_a;&c_h;</ind></trd>
    <trdgrp lng="de">
      <trd>riechen</trd>,
      <trd>wittern</trd>
    </trdgrp>
    <trd lng="hu">szagol</trd>
    <trd lng="pt">cheirar</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_p;&c_a;&c_h;</trd>,
      <trd>&c_ch;&c_u;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="zh">&#38395; <pr>w&eacute;n</pr></trd>
  </snc>
  <snc mrk="flar.0i.esplore">
    <dif>
      Esplori odorojn per la nazo, <tld/>esplori, pri<tld/>i:
      <ekz>
        hundo <tld/>as sur la vojo;
      </ekz><ekz>
        La rozoj ta&ubreve;gas nur por rigardi ilin kaj <tld/>i ilin
        <fnt><bib>Fab2</bib>, <lok>Najbaraj familioj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_ju;&c_h;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_n;&c_ju;&c_h;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>riechen an</trd>,
      <trd>hinriechen</trd>
    </trdgrp>
    <trd lng="hu">szagl&aacute;szik</trd>
    <trd lng="pt">farejar</trd>
    <trd lng="ru">&c_n;&c_ju;&c_h;&c_a;&c_t;&c_mol;</trd>
    <trd lng="zh">&#21957; <pr>xi&ugrave;</pr></trd>
  </snc>
  <trd lng="cs">&ccaron;ichat</trd>
  <trd lng="en">smell (something)</trd>
  <trd lng="es">oler</trd>
  <trdgrp lng="fr">
    <trd>flairer</trd>,
    <trd>renifler<klr tip="ind"> (une odeur)</klr></trd>,
    <trd>sentir<klr tip="ind"> (flairer)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24863;&#12376;&#12427;  <pr>&#12363;&#12435;&#12376;&#12427;</pr></trd>,
    <trd>&#21957;&#12368;  <pr>&#12363;&#12368;</pr></trd>,
    <trd>&#21957;&#12366;&#20998;&#12369;&#12427;  <pr>&#12363;&#12366;&#12431;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">ruiken</trd>
  <trdgrp lng="sk">
    <trd>vo&ncaron;a&tcaron;</trd>,
    <trd>&ncaron;ucha&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_ju;&c_h;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_u;&c_t;&c_i; (&c_n;&c_o;&c_s;&c_o;&c_m;)</trd>,
    <trd>&c_ch;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="flar.0o">
  <kap><tld/>o</kap>
  <snc mrk="flar.0o.sentado">
    <ref tip="dif" cel="flar.0ado.SEN"><tld/>ado</ref>
  </snc>
  <snc mrk="flar.0o.povo">
    <ref tip="dif" cel="flar.0povo.SEN"><tld/>povo</ref>
  </snc>
  <trd lng="be">&c_n;&c_ju;&c_h;</trd>
  <trd lng="cs">&ccaron;ich</trd>
  <trd lng="en">act of smelling, sense of smell</trd>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#12395;&#12362;&#12356;&#12434;&#21957;&#12368;&#12371;&#12392;  <pr>&#12395;&#12362;&#12356;&#12434;&#12363;&#12368;&#12371;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="sk">&ccaron;uch</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_ju;&c_h;</trd>,
    <trd>&c_ch;&c_u;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="flar.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="flar.0ado.SEN">
    <dif>
      Ago <tld/>i:
      <ekz>
        per mallonga <tld/>ado blinda hundo tuj rekonas sian mastron.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_n;&c_ju;&c_h;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">&ccaron;ich&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Riechen</trd>,
    <trd>Wittern</trd>
  </trdgrp>
  <trd lng="en">act of smelling</trd>
  <trd lng="hu">szagol&aacute;s</trd>
  <trd lng="ja">&#12395;&#12362;&#12356;&#12434;&#21957;&#12368;&#12371;&#12392;  <pr>&#12395;&#12362;&#12356;&#12434;&#12363;&#12368;&#12371;&#12392;</pr></trd>
  <trd lng="nl">ruiken<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="ru">&c_o;&c_b;&c_n;&c_ju;&c_h;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">&ccaron;uchanie</trd>
</drv>

<drv mrk="flar.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anticipe <tld/>i:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi anta&ubreve;<tld/>as lin &ccirc;i tie
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_ju;&c_h;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_n;&c_ju;&c_h;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">smell beforehand</trd>
  <trdgrp lng="fr">
    <trd>flairer d'avance</trd>,
    <trd>suspecter</trd>,
    <trd>subodorer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_ja;&c_t;&c_mol; &c_z;&c_a;&c_g;&c_o;&c_d;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_n;&c_ju;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="flar.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i &ccirc;iuflanke:
      <ekz>
        <klr>[la musoj]</klr> &ccirc;irka&ubreve;<tld/>is la abion kaj
        traglitis inter &gcirc;iaj bran&ccirc;oj
        <fnt><bib>Fab2</bib>, <lok>Abio</lok></fnt>;
      </ekz>
      <ekz>
        la princo profunde klini&gcirc;is, la hundo
        &ccirc;irka&ubreve;<tld/>is lin kaj ree ku&scirc;i&gcirc;is
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_n;&c_ju;&c_h;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">smell all around</trd>
  <trdgrp lng="fr">
    <trd>flairer<klr tip="ind"> (de toutes parts)</klr></trd>,
    <trd>renifler<klr tip="ind"> (tr., de toutes parts)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&resh;&het;&resh;&het; &mem;&samekh;&bet;&yod;&bet;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_n;&c_ju;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_n;&c_ju;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="flar.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ennazigi:
      <ekz>
        en<tld/>i tabakon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_y;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol; &c_n;&c_o;&c_s;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nas&aacute;t nosem</trd>,
    <trd>v&ccaron;ichnouti</trd>,
    <trd>&scaron;&ncaron;upat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnuppern</trd>,
    <trd>schn&uuml;ffeln</trd>
  </trdgrp>
  <trd lng="en">breathe in (as a smell)</trd>
  <trdgrp lng="fr">
    <trd>renifler<klr tip="ind"> (aspirer qc par le nez)</klr></trd>,
    <trd>prendre une prise<klr tip="ind"> (de tabac)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felszippant</trd>,
    <trd>beszippant</trd>
  </trdgrp>
  <trd lng="ja">&#12395;&#12362;&#12356;&#12434;&#21560;&#12356;&#36796;&#12416;  <pr>&#12395;&#12362;&#12356;&#12434;&#12377;&#12356;&#12371;&#12416;</pr></trd>
  <trd lng="ru">&c_v;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol; &c_n;&c_o;&c_s;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>nad&yacute;chnu&tcaron; <klr>(nosom)</klr></trd>,
    <trd>zavo&ncaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_d;&c_i;&c_h;&c_a;&c_t;&c_i; &c_n;&c_o;&c_s;&c_o;&c_m;</trd>,
    <trd>&c_n;&c_ju;&c_h;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#21957; <pr>xi&ugrave;</pr></trd>
</drv>

<drv mrk="flar.0povo">
  <kap><tld/>povo, <var><kap><tld/>kapablo</kap></var></kap>
  <snc mrk="flar.0povo.SEN">
    <dif>
      <ref tip="super" cel="sens.0o.FIZL">Senso</ref>,
      per kiu oni perceptas odorojn:
      <ekz>
        vi estas por mi kiel hundino, kiu perdis la <tld/>kapablon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
      </ekz>
      <ekz>
        ekestis multaj soifantoj nuntempe, kaj &ccirc;iuj havis bonan
        <tld/>povon <klr>(por trovi trinka&jcirc;on)</klr>
        <fnt>
          <aut>S. Engholm</aut>,
          <vrk>Homoj sur la tero</vrk>,
          <lok>1932</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ju;&c_h;</trd>
  <trdgrp lng="de">
    <trd>Riechsinn</trd>,
    <trd>Geruchssinn</trd>
  </trdgrp>
  <trd lng="en">sense of smell</trd>
  <trd lng="es">olor</trd>
  <trdgrp lng="fr">
    <trd>flair</trd>,
    <trd>odorat</trd>
  </trdgrp>
  <trd lng="hu">szagl&aacute;s</trd>
  <trd lng="nl">reukzin</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_ju;&c_h;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_n;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_ch;&c_u;&c_t;&c_mol;&c_jo;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21957;&#35258; <pr>xi&ugrave;ju&eacute;</pr></trd>,
    <trd>&#21957;&#35258; <pr>xi&ugrave;ju&eacute;</pr></trd>
  </trdgrp>
</drv>
</art>
