<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>enjekci</rad>/o
</kap>
<drv mrk="enjekc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <ref tip="dif" cel="jxet.dis0o">dis&jcirc;eto.</ref>
  </snc>
  <trd lng="en">injection</trd>
  <trd lng="de">Injektion</trd>
  <trdgrp lng="fr">
    <trd>injection</trd>,
    <trd>application injective</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_malmol;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
</drv>
<drv mrk="enjekc.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>)</klr>
      Havanta la ecojn de <tld/>o:
      <ekz>
        <ref cel="homomo.0o">homomorfio</ref> inter vektoraj spacoj estas 
        <tld/>a, nur se &gcirc;ia <ref cel="kern.0o.homomorfio">kerno</ref> 
        redukti&gcirc;as al {0}.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">injective</trd>
  <trd lng="de">injektiv</trd>
  <trd lng="fr">injectif</trd>
  <trd lng="ru">&c_i;&c_n;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
</art>

