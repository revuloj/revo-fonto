<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>tret</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="tret.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Premi per piedoj:
      <ekz>
        <tld/>i vinberojn;
      </ekz>
      <ekz>
        <tld/>i en vinpremejoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <ind>sur<tld/>i</ind>s la piedojn de la virino<fnt>Z</fnt>;
        <trd lng="hu">r&aacute;l&eacute;p</trd>
      </ekz>
      <ekz>
        tenu vin proksime de mi, por ke neniu <tld/>u sur
        vin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        liaj &scirc;uoj estis <ind><tld/>platigita</ind>j<fnt>Z</fnt>.
        <trd lng="hu">kitaposott</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_p;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>xafar</trd>,
    <trd>trepitjar</trd>,
    <trd>follar <klr>(treti vinberojn)</klr></trd>
  </trdgrp>
  <trd lng="de">treten</trd>
  <trdgrp lng="en">
    <trd>tread</trd>,
    <trd>trample</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pisar</trd>,
    <trd>pisotear</trd>,
    <trd>hollar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tapos</trd>,
    <trd>tipor</trd>
  </trdgrp>
  <trd lng="nl">trappen op</trd>
  <trd lng="pt">pisar</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_p;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_n;&c_o;&c_g;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
</drv>
</art>
