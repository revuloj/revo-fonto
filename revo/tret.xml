<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tret</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="tret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Premi per piedoj:
      <ekz>
        <tld/>i vinberojn;
      </ekz>
      <ekz>
        <tld/>i en vinpremejoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <ind>sur<tld/>i</ind>s la piedojn de la virino<fnt>Z</fnt>;
        <trd lng="hu">rálép</trd>
      </ekz>
      <ekz>
        tenu vin proksime de mi, por ke neniu <tld/>u sur vin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        liaj ŝuoj estis <ind><tld/>platigita</ind>j<fnt>Z</fnt>.
        <trd lng="hu">kitaposott</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>таптаць</trd>,
    <trd>мяць</trd>,
    <trd>расьціскаць</trd>
  </trdgrp>
  <trd lng="de">treten</trd>
  <trdgrp lng="en">
    <trd>tread</trd>,
    <trd>trample</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tapos</trd>,
    <trd>tipor</trd>
  </trdgrp>
  <trd lng="nl">trappen op</trd>
  <trd lng="pt">pisar</trd>
  <trdgrp lng="ru">
    <trd>топтать</trd>,
    <trd>давить ногами</trd>
  </trdgrp>
</drv>
</art>
