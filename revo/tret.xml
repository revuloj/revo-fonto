<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tret.xml,v 1.23 2024/01/25 15:26:33 revo Exp $">
<kap>
  <ofc>9</ofc><rad>tret</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tret.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tret.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="prem.0i.pusxi">Premi</ref>, pu&scirc;i per piedoj:
      <ekz>
        oni <tld/>as vinpremilojn
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 13:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i pu&scirc;is la knabon je la flanko, <tld/>is lin per la piedoj kaj forpelis lin el
        sia domo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi petis ilin krie, ke ili ne <tld/>u &scirc;in morta
        <fnt>
          <bib>VaK</bib>
          <lok>Na&ubreve;a &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        Li tiel <tld/>is mian piedon, ke mi eksiblis pro la doloro
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordigis siajn pantoflojn, &ccirc;ar tio estis &scirc;uoj, kiujn li estis <tld/>platiginta
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tret.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mistrakti, <ref cel="respek.mal0i">malrespekti</ref> morojn, ies rajtojn ktp:
      <ekz>
        en la koton ili <tld/>os &ccirc;ion, kio la homon distingas de la sova&gcirc;a besto
        <fnt>
          <bib>VaK</bib>
          <lok>Deksepa &Ccirc;apitro</lok>
        </fnt>!
      </ekz>
      <ekz>
        en &Ccirc;inio la re&gcirc;imo da&ubreve;re <tld/>as homajn rajtojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009366.php">Problemoj ne solvataj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tret.0i.stamfi">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Peze <ref tip="super" cel="pasx.0i">pa&scirc;i</ref>, <ref tip="dif" cel="stamf.0i">stamfi</ref>:
      <ekz>
        preska&ubreve; dance, iom fajfetante li <tld/>is tra la flankstrato
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;dis la bovinon <tld/>i trans la ponteton
        <fnt>
          <bib>Kiso</bib>
          <lok>La Bovinoj De La Paro&hcirc;estro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_p;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>xafar</trd>,
    <trd>trepitjar</trd>,
    <trd>follar <klr>(treti vinberojn)</klr></trd>
  </trdgrp>
  <trd lng="de">treten</trd>
  <trdgrp lng="en">
    <trd>tread</trd>,
    <trd>trample</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pisar</trd>,
    <trd>pisotear</trd>,
    <trd>hollar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fouler<klr> (aux pieds)</klr></trd>,
    <trd>pi&eacute;tiner</trd>,
    <trd>&eacute;craser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tapos</trd>,
    <trd>tipor</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8E0F;&#12416;  <pr>&#12405;&#12416;</pr></trd>,
    <trd>&#x8E0F;&#x307F;&#12388;&#12369;&#x308B;  <pr>&#12405;&#x307F;&#12388;&#12369;&#x308B;</pr></trd>,
    <trd>&#x8E0F;&#x307F;&#12388;&#12406;&#12377;  <pr>&#12405;&#x307F;&#12388;&#12406;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">trappen op</trd>
  <trdgrp lng="pl">
    <trd>depta&cacute;</trd>,
    <trd>podepta&cacute;</trd>,
    <trd>tratowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">pisar</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_p;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_n;&c_o;&c_g;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_p;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&#8217;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="tret.sur0i">
  <kap>sur<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tret.sur0i.KOMUNE">
    <dif>
      Preme surpa&scirc;i sur:
      <ekz>
        &scirc;i dolorige sur<tld/>is la piedojn de la bona maljuna virino
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pordisto, &jcirc;us elirinta <klr>[&#8230;]</klr> por fumi cigaredon, &gcirc;in sur<tld/>is
        <fnt>
          <bib>MkM</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        buldogino preta sen multaj skrupuloj sur<tld/>i vin plata
        <fnt>
          <bib>ChV</bib>
          <lok>12</lok>
        </fnt>;
      </ekz>
      <ekz>
        infanoj sur<tld/>is la plantojn kaj plukis la florojn
        <fnt>
          <bib>StU</bib>
          <lok>La Nazaretanoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_t;&c_a;&c_c;&c_c;&c_a; <klr>(&c_p;&c_a; &c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd>darauf <ind>treten</ind></trd>,
    <trd>niederdr&uuml;cken</trd>
  </trdgrp>
  <trd lng="fr">marcher sur</trd>
  <trd lng="hu">r&aacute;l&eacute;p</trd>
  <trdgrp lng="ja">
    <trd>&#x8E0F;&#x307F;&#12388;&#12369;&#x308B;  <pr>&#12405;&#x307F;&#12388;&#12369;&#x308B;</pr></trd>,
    <trd>&#x8E0F;&#x307F;&#12388;&#12406;&#12377;  <pr>&#12405;&#x307F;&#12388;&#12406;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nadepn&aogonek;&cacute;</trd>,
    <trd>rozdepta&cacute;</trd>,
    <trd>zadepta&cacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_i;</trd>
</drv>
</art>
