<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lept</rad>/o
</kap>
<drv mrk="lept.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lept.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona
      subunuo</ref>,
      egala al centono de
      <ref tip="malprt" cel="drahxm.0o">dra&hcirc;mo</ref>;
      tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        vi neniel el tie eliros, &gcirc;is vi pagos la lastan <tld/>on
        <fnt><bib>NT</bib>, <lok>&Luk; 12:59</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_p;&c_t;&c_a;</trd>
  <trd lng="de">Lepton<klr> [pl. Lepta]</klr></trd>
  <trd lng="el">&lambda;&epsilon;&pi;&tau;&omikron_ton;</trd>
  <trd lng="fr">lepton<klr tip="ind"> (ancienne monnaie
  grecque)</klr></trd>
  <trd lng="hu">lepta</trd>
  <trd lng="nl">lepton</trd>
  <trd lng="pl">grosz <klr>(rel.)</klr></trd>
  <trd lng="pt">leptae</trd>
  <trd lng="ro">penny</trd>
  <trd lng="ru">&c_l;&c_je;&c_p;&c_t;&c_a;</trd>
</drv>
</art>
