<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sentenc</rad>/o
</kap>
<drv mrk="senten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Mallonga frazo enhavanta &gcirc;eneralan moralan principon
    </dif>
    <refgrp tip="sin">
      <ref cel="aforis.0o">aforismo</ref>,
      <ref cel="maksim.0o">maksimo</ref>,
      <ref cel="deviz.0o">devizo</ref>,
      <ref cel="prover.0o">proverbo</ref>,
      <ref cel="mot.0o">moto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_n;&c_t;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Sentenz</trd>
  <trd lng="en">maxim</trd>
  <trdgrp lng="hu">
    <trd>szentencia</trd>,
    <trd>b&ouml;lcs mond&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">spreuk</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_t;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
