<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sentenc</rad>/o
</kap>
<drv mrk="senten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">BELE</uzo>
    <dif>
      Mallonga frazo enhavanta &gcirc;eneralan moralan principon.
    </dif>
    <refgrp tip="vid">
      <ref cel="mot.0o">moto</ref>,
      <ref cel="prover.0o">proverbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_n;&c_t;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Sentenz</trd>
  <trdgrp lng="en">
    <trd>maxim</trd>,
    <trd>pithy saying</trd>
  </trdgrp>
  <trd lng="es">sentencia</trd>
  <trdgrp lng="fr">
    <trd>aphorisme</trd>,
    <trd>sentence</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szentencia</trd>,
    <trd>b&ouml;lcs mond&aacute;s</trd>
  </trdgrp>
  <trd lng="it">sentenza<klr> (motto)</klr></trd>
  <trd lng="nl">spreuk</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_t;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
