<kap>
  <ofc>*</ofc>
  <rad>kadr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kadr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kadr.0o.borderajxo">
    <dif>
      Bordera&jcirc;o, ordinare kvadrata, sed anka&ubreve; diversforma,
      el iu ajn materialo, per kiu oni &ccirc;irka&ubreve;igas
      pentra&jcirc;on, spegulon, fenestron ktp:
      <ekz>
        la orita <tld/>o mirinde reliefigis la nigran figuron
        de la maljunulino;
      </ekz>
      <ekz>
        malgranda insekto estas sub vitro kaj en <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pentra&jcirc;oj sur la muro estis bone koloritaj litografa&jcirc;oj
        <klr>[&#8230;]</klr>
        ili povis kosti po tri &gcirc;is kvin &scirc;ilingoj kaj la <tld/>oj po unu
        dolaro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        fenestro, kiu havis nigran ebonan <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_m;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_m;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_r;&c_a;&c_m;&c_k;&c_a;</trd>
    <trd lng="ca">marc</trd>
    <trd lng="de">Rahmen</trd>
    <trd lng="en">frame</trd>
    <trdgrp lng="fi">
      <trd>kehys</trd>,
      <trd>kehykset</trd>,
      <trd>raami(t)</trd>,
      <trd>puitteet</trd>
    </trdgrp>
    <trd lng="nl">lijst</trd>
    <trdgrp lng="pl">
      <trd>rama</trd>,
      <trd>ramka</trd>,
      <trd>obramowanie</trd>,
      <trd>kadr</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>quadro</trd>,
      <trd>moldura</trd>,
      <trd>caixilho</trd>
    </trdgrp>
    <trd lng="ro">cadru</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_m;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_m;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>marco</trd>,
      <trd>cuadro</trd>
    </trdgrp>
    <trd lng="tr">&ccedil;er&ccedil;eve</trd>
  </snc>
   <snc mrk="kadr.0o.medio">
     <dif>
       Vida&jcirc;o, kiu &ccirc;irka&ubreve;as ion:
       <ekz>
         la urbo ku&scirc;is en <tld/>o el fre&scirc;a verda&jcirc;o;
       </ekz>
       <ekz>
         la palaco liveris bonegan <tld/>on al nia kongreso
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="fon.0o">fono</ref>.
     </refgrp>
     <trd lng="be">
     &c_a;&c_b;&c_r;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
     </trd>
     <trdgrp lng="ca">
       <trd>escenari</trd>,
       <trd>fons</trd>
     </trdgrp>
     <trd lng="de">Umgebung</trd>
     <trdgrp lng="en">
       <trd>frame</trd>,
       <trd>setting</trd>,
       <trd>backdrop</trd>
     </trdgrp>
     <trd lng="es">marco</trd>
     <trdgrp lng="fi">
       <trd>ymp&auml;rist&ouml;</trd>,
       <trd>tausta</trd>
     </trdgrp>
     <trd lng="nl">kader</trd>
     <trdgrp lng="pl">
       <trd>obramowanie</trd>,
       <trd>otoczenie</trd>
     </trdgrp>
     <trd lng="ru">
     &c_o;&c_b;&c_r;&c_a;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;
     </trd>
   </snc>
  <snc mrk="kadr.0o.kondicxoj">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;io, kio &ccirc;irka&ubreve;as ian okaza&jcirc;on:
      cirkonstancoj, kutimoj, reguloj, organizo, doktrino:
      <ekz>
         la disdonado de la diplomoj okazis samtage, en la <tld/>o
         de adia&ubreve;a vespero;
      </ekz>
      <ekz>
         la historia <tld/>o de tiu cita&jcirc;o estas sufi&ccirc;e
         konata;
      </ekz>
      <ekz>
         establi novan organizon en la <tld/>o de nia asocio;
      </ekz>
      <ekz>
         tiu agado okazas ekster la <tld/>o de nia ofico;
      </ekz>
      <ekz>
        Ili &scirc;atis nur la <tld/>on eklezian
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili nur profitas &gcirc;ian <tld/>on eksteran
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        nenia nova vorto en tiu &ccirc;i lingvo devas esti kreata el sonoj,
        kiuj sin ne trovas en la <tld/>o de &gcirc;ia alfabeto
        <fnt>
          <bib>LR</bib>
          <lok>2. Transskribo</lok>
        </fnt>;
      </ekz>
      <ekz>
         ni proponas nur krei ne&ubreve;tralan eksteran <tld/>on,
         kiu povus unuigi inter si &ccirc;iujn memstare pensantajn
         homojn
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tonoma instituto en <tld/>o de UN
        <fnt>
       <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2000/006881.php">Universala Retlingvo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        subvencioj en la <tld/>o de apogprogramoj
        <fnt>
       <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2001/007258.html">Post la kunsido de financistoj en
          Prago</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
         krevis la malnovaj <tld/>oj de la sciencoj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tekst.kun0o">kunteksto</ref>,
      <ref cel="cirkon1.0aro">cirkonstancaro</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>
     &c_a;&c_b;&c_r;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
      </trd>,
      <trd>&c_r;&c_a;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_m;&c_k;&c_ib;</trd>,
       <trd>&c_m;&c_ja;&c_zh;&c_a;</trd>,
       <trd>&c_m;&c_je;&c_zh;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="ca">
       <trd>&agrave;mbit</trd>,
       <trd>quadre</trd>,
       <trd>esquema</trd>
     </trdgrp>
     <trd lng="de">Rahmen</trd>
     <trd lng="en">framework</trd>
     <trdgrp lng="fi">
       <trd>puitteet <klr tip="amb"> (kuv.)</klr></trd>,
       <trd>olosuhteet</trd>
     </trdgrp>
     <trd lng="nl">kader</trd>
     <trdgrp lng="pl">
       <trd>t&lstroke;o</trd>,
       <trd>warunki</trd>,
       <trd>ramy</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_o;&c_b;&c_r;&c_a;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
       <trd>&c_r;&c_a;&c_m;&c_k;&c_a;</trd>,
       <trd>&c_g;&c_r;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
     </trdgrp>
     <trd lng="es">marco</trd>
   </snc>
  <trd lng="ca">marc</trd>
  <trdgrp lng="cs">
    <trd>r&aacute;mec</trd>,
    <trd>r&aacute;m <klr>(p&rcaron;edm&ecaron;t)</klr></trd>
  </trdgrp>
  <trd lng="fr">cadre</trd>
  <trd lng="he">&mem;&samekh;&gimel;&resh;&tav;</trd>
  <trd lng="hu">keret</trd>
  <trd lng="ja">&#x30D5;&#x30EC;&#x30FC;&#x30E0;</trd>
  <trd lng="sk">k&aacute;der <klr>(osoba)</klr></trd>
  <trd lng="sv">ram</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&#x43C;&#x43A;&c_a;</trd>,
    <trd>&c_r;&c_a;&#x43C;&c_a;</trd>,
    <trd>&#x43E;&#x43F;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_r;&c_a;&#x43C;&#x43A;&c_a;</trd>,
    <trd>&#x43E;&c_t;&#x43E;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43E;&c_b;&c_r;&c_a;&#x43C;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E27; <pr>zh&#xE8;ng</pr></trd>,
    <trd>&#26694; <pr>ku&#xE0;ng</pr></trd>,
    <trd>&#26694;&#x67B6; <pr>ku&#xE0;ngji&#xE0;</pr></trd>,
    <trd>&#x755B;&#x57DF; <pr>zh&#x11B;ny&#xF9;</pr></trd>,
    <trd>&#x7BC4;&#x570D; <pr>f&#xE0;nw&#xE9;i</pr></trd>,
    <trd>&#x8FB9;&#26694; <pr>bi&amacron;nku&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kadr.0ede">
   <kap><tld/>e de,
   <var><kap>en<tld/>e de</kap></var></kap>
   <gra><vspec>prepozicia&jcirc;o</vspec></gra>
   <snc mrk="kadr.0ede.KOMUNE">
     <dif>
       <ref tip="super" cel="en.0.kie">En</ref>
       la donitaj cirkonstancoj,
       la&ubreve; la koncerna organizo:
       <ekz>
         ili trovus trafan esprimon, <tld/>e de la kutima lingvo
         <fnt><bib>BonaLingvo</bib>, <lok>p. 70a</lok></fnt>;
       </ekz>
     <ekz>
         La interkonsento estis farita en<tld/>e de la laborgrupo
         <fnt><aut>E. Georgiev</aut>, <vrk>Interkonsento pri komerca
         liberigo</vrk>, <bib>Monato</bib>, <lok>2002:2, p.
         13a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_u; <ind>&c_r;&c_a;&c_m;&c_k;&c_a;&c_h;</ind></trd>
  <trd lng="ca">dins el <ind>marc</ind> de</trd>
  <trd lng="cs">v r&aacute;mci</trd>
  <trd lng="de">im <ind>Rahmen</ind> von</trd>
  <trd lng="en">in the <ind>framework</ind> of</trd>
  <trd lng="es">en el <ind>marco</ind> de</trd>
  <trdgrp lng="fi">
     <trd><klr tip="amb">(jonkin) </klr> puitteissa <klr> (t.
     &#x201D;puitteissa&#x201D;)</klr></trd>
   </trdgrp>
  <trd lng="fr">dans le <ind>cadre</ind> de</trd>
  <trd lng="he">&bet;<ind>&mem;&samekh;&gimel;&resh;&tav;</ind></trd>
  <trd lng="hu"><klr>(vminek a )</klr>keret&eacute;ben</trd>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x67A0;&#20869;&#12391;
  </ind><pr>&#x306E;&#x308F;&#x304F;&#x306A;&#12356;&#12391;</pr></trd>
  <trd lng="nl">in het <ind>kader</ind> van</trd>
  <trd lng="pl">w <ind>ramach</ind></trd>
  <trd lng="ru">&c_v; <ind>&c_r;&c_a;&c_m;&c_k;&c_a;&c_h;</ind></trd>
  <trd lng="sk">v r&aacute;mci</trd>
  <trdgrp lng="zh">
    <trd>&#22312;&#26694;&#x67B6;&#20839; <pr>z&#xE0;iku&#xE0;ngji&#xE0;n&#xE8;i</pr></trd>,
    <trd>&#26694;&#x67B6;&#x4E0B; <pr>ku&#xE0;ngji&#xE0;xi&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kadr.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="kadr.0i.iu">
     <uzo tip="klr">(iu)</uzo>
     <dif>
       <ref tip="dif" cel="kadr.en0igi.iu">En<tld/>igi<sncref/></ref>:
       <ekz>
         <klr>[ve al tiu, kiu]</klr> trahakas al si fenestrojn
         kaj <tld/>as ilin per cedro kaj kolorigas per cinabro
         <fnt><bib>MT</bib><lok>&Jer; 22:14</lok></fnt>;
       </ekz>
       <ekz>
         &scirc;i havis sur si ne&gcirc;oblankan kolumon,
         butonumitan per granda ore <tld/>ita kameo
         <klr>(&ccirc;irka&ubreve;igita per ora <tld/>o)</klr>
         <fnt><bib>Marta</bib></fnt>;
       </ekz>
       <ekz>
         li plu konservis sur la viza&gcirc;o <tld/>ita per
         malgrandaj vangoharoj rideton de ravo kaj sindonemo
         <fnt><bib>MkM</bib><lok>&ccirc;ap. 21</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="de">rahmen</trd>
     <trd lng="he">&lamed;<ind>&mem;&samekh;&gimel;&resh;</ind></trd>
     <trdgrp lng="hu">
       <trd>bekeretez</trd>,
       <trd>keretbe foglal</trd>
     </trdgrp>
     <trd lng="nl">inlijsten</trd>
     <trdgrp lng="pl">
       <trd>obramowywa&cacute;</trd>
     </trdgrp>
   </snc>
   <snc mrk="kadr.0i.io">
     <uzo tip="klr">(io)</uzo>
     <dif>
       <ref tip="dif" cel="kadr.en0igi.io">En<tld/>igi<sncref/></ref>:
       <ekz>
         el &scirc;iaj profundi&gcirc;intaj kaj blue <tld/>itaj
         okuloj <klr>(okuloj, &ccirc;irka&ubreve;itaj de blua&jcirc;o)
         </klr>
         fluis silenta, sed korpenetra plendo
         <fnt><bib>Marta</bib></fnt>;
       </ekz>
       <ekz>
         malaperis la cipresoj <tld/>antaj la supran terason
         <fnt><bib>MkM</bib><lok>&ccirc;ap. 2</lok></fnt>.
       </ekz>
     </dif>
     <rim>
       &Ccirc;i tiu senco ne estas agnoskita de
       <fnt><bib>PIV2</bib></fnt>, kiu citas la zamenhofan
       ekzemplon kadre de senco <sncref ref="kadr.0i.iu"/>,
       kio estas allasebla interpreto pro la pasiva formo
       de la verbo kaj la adverba formo de la adjekto.
     </rim>
     <trd lng="de">umgeben</trd>
     <trdgrp lng="hu">
       <trd>keretez</trd>,
       <trd>&ouml;vez <klr tip="ind">(&aacute;tv.)</klr></trd>
     </trdgrp>
     <trd lng="nl">omkaderen</trd>
     <trdgrp lng="pl">
       <trd>obramowywa&cacute;</trd>
     </trdgrp>
   </snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="ca">emmarcar</trd>
  <trdgrp lng="cs">
    <trd>r&aacute;movat</trd>,
    <trd>zar&aacute;movat</trd>
  </trdgrp>
  <trd lng="en">frame</trd>
  <trd lng="es">enmarcar</trd>
  <trdgrp lng="fi">
     <trd>kehyst&auml;&auml;</trd>,
     <trd>ymp&auml;r&ouml;id&auml;</trd>
   </trdgrp>
  <trd lng="fr">encadrer</trd>
  <trdgrp lng="ja">
    <trd>&#x7E01;&#x53D6;&#x308B;  <pr>&#12405;&#12385;&#12393;&#x308B;</pr></trd>,
    <trd>&#x56F2;&#12416;  <pr>&#x304B;&#12371;&#12416;</pr></trd>,
    <trd>&#x67A0;&#x306B;&#x306F;&#12417;&#x308B;
    <pr>&#x308F;&#x304F;&#x306B;&#x306F;&#12417;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_o;&c_b;&c_r;&c_a;&c_m;&c_l;&c_ja;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_k;&c_a;&c_j;&c_m;&c_l;&c_ja;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>r&aacute;mova&tcaron;</trd>,
    <trd>zar&aacute;mova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">rama in</trd>
  <trd lng="tr">&ccedil;er&ccedil;evelemek</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_r;&c_a;&#x43C;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_b;&#x43B;&#x44F;&#x43C;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kadr.al0igi">
   <kap>al<tld/>igi
      <fnt><bib>PIV2</bib></fnt>
   </kap>
   <snc>
     <uzo tip="fak">TIP</uzo>
     <dif>
       Aran&gcirc;i tekston tiel, ke amba&ubreve; randoj estas glataj.
     </dif>
     <ref tip="vid" cel="rekt.0igi">rektigi</ref>
     <ref tip="vid" cel="rembur.0i.TIP">remburi</ref>
     <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_k;&c_u;</ind> &c_t;&c_e;&c_k;&c_s;&c_t;&c_u; &c_p;&c_a; &c_a;&c_b;&c_o;&c_d;&c_v;&c_u;&c_h; &c_b;&c_a;&c_k;&c_o;&c_h;</trd>
     <trd lng="ca">justificar <klr>(tipogr.)</klr></trd>
     <trd lng="es">justificar <klr>(tipogr.)</klr></trd>
     <trd lng="nl">blokken<klr tip="ind"> (v.tekst)</klr></trd>
     <trdgrp lng="pl">
       <trd>kadrowa&cacute;</trd>,
       <trd>justowa&cacute;</trd>
     </trdgrp>
   </snc>
  <trd lng="ja">&#x4E21;&#x7AEF;&#x63C3;&#12360;&#12377;&#x308B;
  <pr>&#x308A;&#12423;&#12358;&#x305F;&#12435;&#x305D;&#x308D;&#12360;&#12377;&#x308B;</pr></trd>
  <trd lng="uk">&c_v;&c_i;&c_r;&c_ib;&c_v;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i; &c_z; &#x43E;&c_b;&#x43E;&c_h; &#x43A;&c_r;&c_a;&c_ji;&c_v;</trd>
</drv>

<drv mrk="kadr.brod0o">
  <kap>brod<tld/>o</kap>
  <snc mrk="kadr.brod0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="K"/>o fermebla &ccirc;irka&ubreve; la randoj de
      kanvaso, kiu tiel restas stre&ccirc;ata dum prilaboro:
      <ekz>
        juna fra&ubreve;lino <klr>...</klr> sidis kun sia
        brod<tld/>o apud fenestro
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_l;&c_mol;&c_c;&c_y;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_u;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">r&aacute;m na vy&scaron;&iacute;v&aacute;n&iacute;</trd>
  <trd lng="es">bastidor</trd>
  <trd lng="fr">cadre de broderie</trd>
  <trd lng="ja">&#x523A;&#x7E4D;&#x67A0;  <pr>&#12375;&#12375;&#12421;&#12358;&#x308F;&#x304F;</pr></trd>
  <trd lng="sk">r&aacute;m na vy&scaron;&iacute;vanie</trd>
  <trdgrp lng="uk">
