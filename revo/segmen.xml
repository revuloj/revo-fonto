<drv mrk="segmen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="segmen.0i.KOMUNE">
    <dif>
      Dividi en <tld/>ojn:
      <ekz>
        ni povas la kontinuan kampon de koloroj en Esperanto
        <tld/>i tiel &ccirc;i: UV - viola - blua - verda - flava
        - oran&gcirc;a - ru&gcirc;a - IR 
        <fnt>
          <aut>J. Werner</aut>
          <vrk><url ref="http://dvd.ikso.net/faka/scienco/Terminologiaj_konsideroj.pdf">
          Terminologiaj konsideroj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">segment</trd>
  <trd lng="fr">segmenter</trd>
  <trdgrp lng="ja">
    <trd>&#x5206;&#x7BC0;&#x306B;&#x5206;&#x3051;&#x308B;  <pr>&#x3076;&#x3093;&#x305B;&#x3064;&#x306B;&#x308F;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x5206;&#x5272;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x304B;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_ib;&#x43B;&c_i;&c_t;&c_i; &#x43D;&c_a; &c_s;&c_je;&c_gu;&#x43C;&c_je;&#x43D;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D30;&#x5206; <pr>x&#xEC;f&#x113;n</pr></trd>,
    <trd>&#x7EC6;&#x5206; <pr>x&#xEC;f&#x113;n</pr></trd>,
    <trd>&#x5206;&#x6BB5; <pr>f&#x113;ndu&#xE0;n</pr></trd>
  </trdgrp>
</drv>
