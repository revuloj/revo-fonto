<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>segment</rad>/o
</kap>
<drv mrk="segmen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="segmen.0o.gxenerale">
    <dif>
      Iu parto de
      <ref tip="malprt" cel="figur.geometria0o">geometria figuro</ref>.
    </dif>
    <subsnc mrk="segmen.0o.linio">
      <dif>
        <klr>(de <ref tip="malprt" cel="lini.0o.MAT">linio</ref>)</klr>
        Parto de &gcirc;i, entenata inter du &gcirc;iaj punktoj:
        <ekz>
          <tld/>o de hiperbolo;
        </ekz>
        <ekz>
          la du punktojn, limantajn <tld/>on, oni nomas &gcirc;iaj
          <ref tip="prt" cel="rand.0o.figuro">randoj</ref>.
        </ekz>
      </dif>
      <adm>
        Fonto trovenda. <aut>MB</aut>
      </adm>
      <rim>
        La vorto <ctl>inter</ctl> en la difino supozigas, ke oni difinis
        iun ordon, ekz-e la manieron trairi la linion, sed la <tld/>o
        mem ne dependas de &gcirc;i.
      </rim>
      <trd lng="de">Kurvenst&uuml;ck</trd>
      <trd lng="en">segment<klr tip="amb"> (of a curve)</klr></trd>
      <trd lng="fr">segment<klr tip="amb"> (de courbe)</klr></trd>
      <trd lng="hu">szakasz<klr tip="amb"> (g&ouml;rb&eacute;&eacute;)</klr></trd>      
      <trd lng="pl">odcinek<klr tip="amb"> (krzywej)</klr></trd>
      <trd lng="ru">&c_k;&c_u;&c_s;&c_o;&c_k;<klr tip="amb"> (&c_k;&c_r;&c_i;&c_v;&c_o;&c_j;)</klr></trd>
    </subsnc>
    <subsnc mrk="segmen.0o.rekto">
      <fnt><bib>MatTerm</bib><lok>p.. 22</lok></fnt>
      <dif>
        <klr>(de <ref tip="malprt" cel="rekt.0o.MAT">rekto</ref>)</klr>
      </dif>
      <ref tip="dif" cel="strek.0o.segmento">streko.</ref>
      <trd lng="en">line <ind>segment</ind></trd>
      <trd lng="de">Strecke</trd>
      <trd lng="fr">segment de droite</trd>
      <trd lng="hu">szakasz<klr tip="amb"> (egyenes&eacute;)</klr></trd>      
      <trd lng="pl">odcinek<klr tip="ind"> (prostej)</klr></trd>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_z;&c_o;&c_k;</trd>
    </subsnc>
    <subsnc mrk="segmen.0o.cirklo">
      <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
      <dif>
        <klr>(de <ref cel="cirkl.0o.surfaco">cirklo <sncref/></ref>)</klr>
        Parto de &gcirc;i, entenata inter <tld/>o
        <sncref ref="segmen.0o.linio"/> de &gcirc;ia
        <ref cel="rand.0o.figuro">rando</ref> kaj la responda
        <ref cel="sxnur.0o.cirklo">&scirc;nuro</ref>.
      </dif>
      <rim>
        Eble pro la eventuala konfuzo kun <ref cel="ark.0o.segmento">arko</ref>
        &ccirc;i-sence trovi&gcirc;as <ctl>disko<tld/>o</ctl>
        en <fnt><bib>MatVort</bib></fnt>.
      </rim>
      <trd lng="de">Kreisabschnitt</trd>
      <trd lng="en">circular <ind>segment</ind></trd>
      <trd lng="pl">odcinek<klr tip="ind"> (ko&lstroke;owy)</klr></trd>
      <trd lng="fr">segment<klr tip="amb"> (de cercle)</klr></trd>
      <trd lng="hu">k&ouml;rszelet</trd>      
      <trd lng="ru">&c_k;&c_r;&c_u;&c_g;&c_o;&c_v;&c_o;&c_j; <ind>&c_s;&c_je;&c_g;&c_m;&c_je;&c_n;&c_t;</ind></trd>
    </subsnc>
    <subsnc mrk="segmen.0o.sfero">
      <fnt><bib>MatVort</bib><lok>3065</lok></fnt>
      <dif>
        <klr>(de 
        <ref tip="malprt" cel="sfer.0o.solido">sfero <sncref/></ref>)</klr>
        &Ccirc;iu el &gcirc;iaj partoj situantaj amba&ubreve;flanke de
        &gcirc;in sekcanta ebeno.
      </dif>
      <refgrp tip="super">
        <ref cel="tavol.glob0o">globtavolo</ref>.
      </refgrp>
      <rim>
        &Ccirc;i-sence aperas anka&ubreve; <ctl>glob<tld/>o</ctl> en
        <fnt><bib>MatVort</bib></fnt>.
      </rim>
      <trd lng="de">Kugelabschnitt</trd>
      <trd lng="en">spherical <ind>segment</ind></trd>
      <trd lng="fr">segment<klr tip="amb"> (sph&eacute;rique)</klr></trd>
      <trd lng="hu">g&ouml;mbszelet</trd>            
      <trdgrp lng="pl">
        <trd>odcinek kulisty</trd>,
        <trd>odcinek kuli</trd></trdgrp> 
      <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; <ind>&c_s;&c_je;&c_g;&c_m;&c_je;&c_n;&c_t;</ind></trd>
    </subsnc>
  </snc>
  <snc mrk="segmen.0o.reela">
    <dif>
      Fermita reela <ref cel="interv.0o">intervalo</ref>.
    </dif>
    <adm>
      Fonto trovenda. <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>abgeschlossenes <ind>Intervall</ind></trd>,
      <trd>Segment<klr tip="ind"> (Intervall)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>closed <ind>interval</ind></trd>,
      <trd>segment<klr tip="ind"> (interval)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>intervalle ferm&eacute;</trd>,
      <trd>segment<klr tip="ind"> (intervalle)</klr></trd>
    </trdgrp>
    <trd lng="hu">z&aacute;rt <ind>intervallum</ind></trd>
    <trd lng="pl">przedzia&lstroke; domkni&eogonek;tny</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_y;&c_j; <ind>&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;</ind></trd>,
      <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_o;&c_k;<klr tip="ind"> (&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="nl">segment</trd>
  <trd lng="ru">&c_s;&c_je;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
