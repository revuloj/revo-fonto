<drv mrk="segmen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="segmen.0i.KOMUNE">
    <dif>
      Dividi en <tld/>ojn:
      <ekz>
        ni povas la kontinuan kampon de koloroj en Esperanto
        <tld/>i tiel &ccirc;i: UV - viola - blua - verda - flava
        - oran&gcirc;a - ru&gcirc;a - IR
        <fnt>
          <aut>J. Werner</aut>
          <vrk><url ref="http://dvd.ikso.net/faka/scienco/Terminologiaj_konsideroj.pdf">
          Terminologiaj konsideroj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">segment</trd>
  <trd lng="fr">segmenter</trd>
  <trdgrp lng="ja">
    <trd>&#20998;&#31680;&#12395;&#20998;&#12369;&#12427;  <pr>&#12406;&#12435;&#12379;&#12388;&#12395;&#12431;&#12369;&#12427;</pr></trd>,
    <trd>&#20998;&#21106;&#12377;&#12427;  <pr>&#12406;&#12435;&#12363;&#12388;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_ib;&c_l;&c_i;&c_t;&c_i; &c_n;&c_a; &c_s;&c_je;&c_gu;&c_m;&c_je;&c_n;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#32048;&#20998; <pr>x&igrave;f&emacron;n</pr></trd>,
    <trd>&#32454;&#20998; <pr>x&igrave;f&emacron;n</pr></trd>,
    <trd>&#20998;&#27573; <pr>f&emacron;ndu&agrave;n</pr></trd>
  </trdgrp>
