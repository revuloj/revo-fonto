<art mrk="$Id: segmen.xml,v 1.109 2025/04/16 22:29:26 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>segment</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="segmen.0o">
  <kap><tld/>o</kap>
  <snc mrk="segmen.0o.gxenerale">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Iu parto de
      <ref tip="malprt" cel="figur.geometria0o">geometria figuro</ref>.
    </dif>
    <subsnc mrk="segmen.0o.linio">
      <dif>
        <klr>(de <ref tip="malprt" cel="lini.0o.MAT">linio</ref>)</klr>
        Parto de &gcirc;i, entenata inter du &gcirc;iaj punktoj:
        <ekz>
          <tld/>o de hiperbolo;
        </ekz>
        <ekz>
          la du punktojn, limantajn <tld/>on, oni nomas &gcirc;iaj
          <ref tip="prt" cel="rand.0o.figuro">randoj</ref>.
        </ekz>
      </dif>
      <adm>
        Fonto trovenda. <aut>MB</aut>
      </adm>
      <rim>
        La vorto <ctl>inter</ctl> en la difino supozigas, ke oni difinis
        iun ordon, ekz-e la manieron trairi la linion, sed la <tld/>o
        mem ne dependas de &gcirc;i.
      </rim>
      <trd lng="de">Kurvenst&uuml;ck</trd>
      <trd lng="ca">segment</trd>
      <trd lng="en">segment<klr tip="amb"> (of a curve)</klr></trd>
      <trd lng="es">segmento</trd>
      <trd lng="fr">segment<klr tip="amb"> (de courbe)</klr></trd>
      <trd lng="it">segmento</trd>
      <trd lng="hu">szakasz<klr tip="amb"> (g&ouml;rb&eacute;&eacute;)</klr></trd>
      <trd lng="pl">odcinek<klr tip="amb"> (krzywej)</klr></trd>
      <trd lng="ru">&c_k;&c_u;&c_s;&c_o;&c_k;<klr tip="amb">
      (&c_k;&c_r;&c_i;&c_v;&c_o;&c_j;)</klr></trd>
    </subsnc>
    <subsnc mrk="segmen.0o.rekto">
      <fnt><bib>MatTerm</bib><lok>p. 22</lok></fnt>
      <dif>
        <klr>(de <ref tip="malprt" cel="rekt.0o.MAT">rekto</ref>)</klr>
        <ref tip="dif" cel="strek.0o.segmento">Streko</ref>.
      </dif>
      <trd lng="en">line <ind>segment</ind></trd>
      <trd lng="de">Strecke</trd>
      <trd lng="es">segmento de recta</trd>
      <trd lng="fr">segment de droite</trd>
      <trd lng="hu">szakasz<klr tip="amb"> (egyenes&eacute;)</klr></trd>
      <trd lng="it">segmento</trd>
      <trd lng="pl">odcinek<klr tip="ind"> (prostej)</klr></trd>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_z;&c_o;&c_k;</trd>
    </subsnc>
    <subsnc mrk="segmen.0o.cirklo">
      <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
      <dif>
        <klr>(de <ref cel="cirkl.0o.surfaco">cirklo
        <sncref/></ref>)</klr>
        Parto de &gcirc;i, entenata inter <tld/>o
        <sncref ref="segmen.0o.linio"/> de &gcirc;ia
        <ref cel="rand.0o.figuro">rando</ref> kaj la responda
        <ref cel="sxnur.0o.cirklo">&scirc;nuro</ref>.
      </dif>
      <rim>
        Eble pro la eventuala konfuzo kun <ref cel="ark.0o.segmento">arko</ref>
        &ccirc;i-sence trovi&gcirc;as <ctl>disko<tld/>o</ctl>
        en <fnt><bib>MatVort</bib></fnt>.
      </rim>
      <trd lng="de">Kreisabschnitt</trd>
      <trd lng="en">circular <ind>segment</ind></trd>
      <trd lng="pl">odcinek<klr tip="ind"> (ko&lstroke;owy)</klr></trd>
      <trd lng="fr">segment<klr tip="amb"> (de cercle)</klr></trd>
      <trd lng="hu">k&ouml;rszelet</trd>
      <trd lng="it"><ind>segmento</ind> circolare</trd>
      <trd lng="ru">&c_k;&c_r;&c_u;&c_g;&c_o;&c_v;&c_o;&c_j;
      <ind>&c_s;&c_je;&c_g;&c_m;&c_je;&c_n;&c_t;</ind></trd>
    </subsnc>
    <subsnc mrk="segmen.0o.sfero">
      <fnt><bib>MatVort</bib><lok>3065</lok></fnt>
      <dif>
        <klr>(de
        <ref tip="malprt" cel="sfer.0o.solido">sfero
        <sncref/></ref>)</klr>
        &Ccirc;iu el &gcirc;iaj partoj situantaj amba&ubreve;flanke de
        &gcirc;in sekcanta ebeno.
      </dif>
      <refgrp tip="super">
        <ref cel="tavol.glob0o">globtavolo</ref>.
      </refgrp>
      <rim>
        &Ccirc;i-sence aperas anka&ubreve; <ctl>glob<tld/>o</ctl> en
        <fnt><bib>MatVort</bib></fnt>.
      </rim>
      <trd lng="de">Kugelabschnitt</trd>
      <trd lng="en">spherical <ind>segment</ind></trd>
      <trd lng="fr">segment<klr tip="amb"> (sph&eacute;rique)</klr></trd>
      <trd lng="hu">g&ouml;mbszelet</trd>
      <trd lng="it"><ind>segmento</ind> sferico</trd>
      <trdgrp lng="pl">
        <trd>odcinek kulisty</trd>,
        <trd>odcinek kuli</trd></trdgrp>
      <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;
      <ind>&c_s;&c_je;&c_g;&c_m;&c_je;&c_n;&c_t;</ind></trd>
    </subsnc>
  <trd lng="en">segment</trd>
  </snc>
  <snc mrk="segmen.0o.reela">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Fermita reela <ref cel="interv.0o">intervalo</ref>.
    </dif>
    <adm>
      Fonto trovenda. <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>abgeschlossenes <ind>Intervall</ind></trd>,
      <trd>Segment<klr tip="ind"> (Intervall)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>closed <ind>interval</ind></trd>,
      <trd>segment<klr tip="ind"> (interval)</klr></trd>
    </trdgrp>
    <trd lng="es">intervalo cerrado</trd>
    <trdgrp lng="fr">
      <trd>intervalle ferm&eacute;</trd>,
      <trd>segment<klr tip="ind"> (intervalle)</klr></trd>
    </trdgrp>
    <trd lng="hu">z&aacute;rt <ind>intervallum</ind></trd>
    <trd lng="it">segmento<klr> (intervallo)</klr></trd>
    <trd lng="pl">przedzia&lstroke; domkni&eogonek;tny</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_y;&c_j;
      <ind>&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;</ind></trd>,
      <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_o;&c_k;<klr tip="ind">
      (&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="segmen.0o.KOMUNE">
    <dif>
      Nete aparta <ref tip="super" cel="part.0o.KOMUNE">parto</ref>
      de tuto, komparebla kun aliaj:
      <ekz>
        pordegoj el betonaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj
          progreso</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la landa ekonomio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2013/010975.php">La &ccirc;e&hcirc;a elektrodistribuanto CEZ mise
          taksis la albanan situacion</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="segmen.0o.ZOO">
      <uzo tip="fak">ZOO</uzo>
      <dif>
        Iu el la sinsekvaj, inter si tre similaj korpopartoj
        de diversaj artikuloj, ringovermoj kaj ceteraj:
        <ekz>
          la artropoda korpoformo konsistas el <tld/>oj,
          kiuj havas po paron de membroj
          <fnt>
            <bib>Viki</bib>
            <vrk><url ref="&Viki;/Artropodoj">
            artropodoj</url>, 2017-06-25</vrk>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">segment<klr> (biol.)</klr></trd>
    </subsnc>
    <subsnc mrk="segmen.0o.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        En sociologio, demografio, merkatiko,
        subparto de studata homgrupo distingita
        la&ubreve; geografia, a&gcirc;a, profesia a&ubreve; alia karakterizo:
        <ekz>
          oni dividas merkaton en <tld/>oj
          <fnt>
            <aut>D. Keefe</aut>
            <vrk><url ref="&LOdE;Historio/237keefe.htm">
            La subfakoj de merkatiko, aplikataj al la
            Esperanto-movadoj</url></vrk>
            <lok>La Ondo de Esperanto, 2014:4-7</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="segmen.0o.BELE">
      <uzo tip="fak">BELE</uzo>
      <dif>
        Seninterrompa, apartigita parto de parolado, de teksto:
        <ekz>
          skriba <tld/>o
          <fnt>
            <aut>P. Dasgupta</aut>
            <vrk>Lo&gcirc;i en homaj lingvoj: la substancisma
            perspektivo</vrk>
            <lok>2011</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
       <ref cel="sektor.0o.EKON">sektoro<sncref/></ref>,
       <ref cel="brancx.0o.FIG">bran&ccirc;o<sncref/></ref>
    </refgrp>
  <trd lng="en">segment</trd>
  </snc>
