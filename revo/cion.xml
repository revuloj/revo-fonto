<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Cion</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="cion.0o">
  <kap><tld/>o</kap>
  <snc mrk="cion.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Nomo de antikva fortika&jcirc;o en <ref tip="malprt"
      cel="jerusa.0o.GEOG">Jerusalemo</ref>, ofte aludanta la tutan
      urbon rigardatan kiel sanktan:
      <ekz>
        Dio helpos <tld/>on kaj konstruos la urbojn de Judujo
        <fnt><bib>MT</bib>, <lok>&amp;Psa 69:36</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zion</trd>
  <trd lng="en">Zion</trd>
  <trd lng="fr">Sion</trd>
  <trd lng="it">Sion</trd>
  <trd lng="pl">Syjon</trd>
  <trd lng="ru">&c_S;&c_i;&c_o;&c_n;</trd>
</drv>

<drv mrk="cion.0ismo">
  <kap><tld lit="c"/>ismo <fnt><bib>SPV</bib></fnt></kap>
  <snc mrk="cion.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Movado por la fondo de propra regno de la judoj,
      kian realigis <ref tip="vid" cel="israel.0o">Israelo</ref>:
      <ekz>
        <tld/>ismo &ndash; la&ubreve; la opinio de Zamenhof &ndash;
        havas
        antinomiojn, estas neefektivigebla utopio
        <fnt>
        <aut>J. Meisl , trad. N. Z. Maimon</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr54/hilelismo.html">Hilelismo</url></vrk>,
        <lok>la Nica Literatura Revuo, 1960-03 &gcirc;is 04, numero 5/4,
        p.
        131a-133a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zionismus</trd>
  <trd lng="en">Zionism</trd>
  <trd lng="fr">sionisme</trd>
  <trd lng="it">sionismo</trd>
  <trd lng="pl">syjonizm</trd>
  <trd lng="ru">&c_s;&c_i;&c_o;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="cion.0isto">
  <kap><tld lit="c"/>isto <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="cion.0isto.POL">
    <dif>
      <tld/>ismano:
      <ekz>
        la <tld/>ista movado renkontis ekde sia fondi&gcirc;o fervorajn
        adeptojn <klr>...</klr> unuflanke, <klr>...</klr> oponantojn
        &ccirc;e
        la alia ekstremo
        <fnt>
        <aut>T. L&ouml;wenkopf</aut>,
        <vrk><url
        ref="http://www.geocities.com/Athens/Cyprus/5318/8/Cionismo.html">Kio
        estas cionismo ?</url></vrk>,
        <lok>[vidita je 2006-05-11]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zionist</trd>
  <trd lng="en">Zionist</trd>
  <trd lng="fr">sioniste</trd>
  <trd lng="it">sionista</trd>
  <trd lng="pl">syjonista</trd>
  <trd lng="ru">&c_s;&c_i;&c_o;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>
</art>

