<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cereal</rad>/o
</kap>
<drv mrk="cereal.0o">
  <kap><tld/>o</kap>
  <snc mrk="cereal.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="sub" cel="gren.0o.planto">Greno</ref> a&ubreve;
      grensimila vegeta&jcirc;o:
      <ekz>
        sova&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        hordeo, aveno, sekalo, tritiko estas kulturaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_b;&c_o;&c_zh;&c_zh;&c_a;</trd>,
    <trd>&c_z;&c_b;&c_a;&c_zh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Getreide</trd>
  <trd lng="el">&delta;&eta;&my;&eta;&tau;&rho;&jota;&alfa;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">cereal</trd>
  <trd lng="es">cereal</trd>
  <trd lng="fr">c&eacute;r&eacute;ale</trd>
  <trdgrp lng="hu">
    <trd>gabonan&ouml;v&eacute;ny</trd>,
    <trd>gabonaf&eacute;le</trd></trdgrp>
  <trd lng="pt">cereal</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;</trd>
  <trd lng="sk">obilie</trd>
</drv>
</art>
