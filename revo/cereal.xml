<drv mrk="cereal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      <ref tip="sub" cel="gren.0o.planto">Greno</ref> a&ubreve;
      grensimila vegeta&jcirc;o:
      <ekz>
        sova&gcirc;a <tld/>o;
      </ekz>
