        hordeo, aveno, sekalo, tritiko estas kulturaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Getreide</trd>
  <trd lng="en">cereal</trd>
  <trd lng="fr">c&eacute;r&eacute;ale</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;</trd>
</drv>
