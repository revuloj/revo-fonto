<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxspi.xml,v 1.20 2022/06/14 08:37:00 revo Exp $">
<kap>
  <rad>a&ubreve;spici</rad>/o
</kap>

<drv mrk="auxspi.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxspi.0o.DIV">
    <dif>
      Spontanee aperanta
      <ref tip="super" cel="auxgur.0o.DIV">a&ubreve;guro</ref>,
      ofte favora:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la afero sin prezentas sub favora <tld/>o
	<klr>(la kondi&ccirc;oj estas bonaj por sukceso de la afero)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_je;</trd>
    <trd lng="de">Vorzeichen</trd>
    <trdgrp lng="en">
      <trd>auspice</trd>,
      <trd>good <ind>omen</ind></trd>,
      <trd>harbinger</trd>,
      <trd>precursor</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>auspice<klr tip="ind"> (pr&eacute;sage)</klr></trd>,
      <trd>pr&eacute;sage</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>el&odblac;jel<klr tip="ind"> (kil&aacute;t&aacute;s)</klr></trd>,
      <trd>kil&aacute;t&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">voorteken</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="auxspi.0o.POL">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="sin" cel="egid.0o.protekto">Egido<sncref/></ref>, alta
      <ref tip="sin" cel="protek.0ado">protektado</ref>:
      <ekz>
        <tld/>ata de Unui&gcirc;intaj Nacioj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2004/008291.html">Kial fieri pri iu patrujo kaj ne pri
          alia?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ef<tld/>into de la zagreba Universala Kongreso de Esperanto
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2004/008453.html">Prezidanto postulas, prezidanto
          rifuzas</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia eldonejo nur <tld/>is tiun laboron, liverante senpagajn ekzemplerojn al la
        fidelaj vo&ccirc;legantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008572.php">A&ubreve;dantoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
     <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
     <trd>&c_a;&c_p;&c_ja;&c_k;&c_u;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Schirmherrschaft</trd>
    <trd lng="en">auspices</trd>
    <trdgrp lng="fr">
      <trd>auspice <klr tip="ind">(&eacute;gide)</klr></trd>,
      <trd>&eacute;gide</trd>
    </trdgrp>
    <trd lng="hu">v&eacute;dn&ouml;ks&eacute;g</trd>
    <trd lng="nl">auspici&euml;n</trd>
  </snc>
  <trdgrp lng="he">
    <trd>&#x5D0;&#x5D5;&#x5EA; &#x5DC;&#x5D1;&#x5D0;&#x5D5;&#x5EA;</trd>,
    <trd>&#x5EA;&#x5E7;&#x5D5;&#x5D4;</trd>
  </trdgrp>
  <trd lng="it">auspicio</trd>
</drv>

<drv mrk="auxspi.0a">
  <kap><tld/>a</kap>
  <snc mrk="auxspi.0a.bonauxgura">
    <dif>
      <ref tip="dif" cel="auxgur.bon0a">Bona&ubreve;gura</ref>:
      <ekz>
        &scirc;i pretis a&ccirc;eti novan domon, meblojn kaj aliajn necesa&jcirc;ojn kaj
        elekti <tld/>an nuptotagon
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 67a, volumo 2a, p.
             429a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y; <klr>(&c_a;&c_b; &c_p;&c_r;&c_a;&c_d;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="en">auspicious</trd>
  <trdgrp lng="fr">
    <trd>auspicieux</trd>,
    <trd>faste<klr> (signe)</klr></trd>,
    <trd>favorable<klr> (signe)</klr></trd>
  </trdgrp>
  <trd lng="it">augurante</trd>
</drv>
</art>
