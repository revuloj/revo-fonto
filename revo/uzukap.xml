<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>uzukap</rad>/i
</kap>
<drv mrk="uzukap.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Akiri, la&ubreve; le&gcirc;a rajto, propra&jcirc;on per ne
interrompita posedo:
      <ekz>
        &scirc;tela&jcirc;oj ne estas <tld/>eblaj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ersitzen</trd>
  <trd lng="es">usucapir</trd>
  <trd lng="hu">elbirtokol</trd>
  <trd lng="nl">verwerven<klr tip="ind"> (door verjaring)</klr></trd>
  <trdgrp lng="pt">
   <trd>usucapir</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;
&c_p;&c_r;&c_a;&c_v;&c_o;
&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_i;
&c_p;&c_o; &c_d;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_i;
&c_v;&c_l;&c_a;&c_d;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="uzukap.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      La le&gcirc;a rajto akiri la propra&jcirc;on per neinterrompita
posedo.
    </dif>
    <refgrp tip="vid">
      <ref cel="preskr1.0o">preskripto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Usukapion</trd>,
    <trd>Ersitzung</trd>
  </trdgrp>
  <trd lng="es">usucapi&oacute;n</trd>
  <trd lng="fr">usucapion</trd>
  <trd lng="hu">elbirtokl&aacute;s</trd>
  <trd lng="nl">verwerving<klr tip="ind"> (door verjaring)</klr></trd>
</drv>
</art>
