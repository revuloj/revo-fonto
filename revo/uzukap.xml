<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uzukap.xml,v 1.21 2024/03/25 23:24:25 revo Exp $">
<kap>
   <rad>uzukap</rad>/i
</kap>
<drv mrk="uzukap.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">JUR</uzo>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Akiri, la&ubreve; le&gcirc;a rajto, propra&jcirc;on per ne
interrompita posedo:
       <ekz>
         &scirc;tela&jcirc;oj ne estas <tld/>eblaj.
       </ekz>
     </dif>
   </snc>
  <trd lng="cs">vydr&zcaron;et <klr>(pr&aacute;vo)</klr></trd>
  <trd lng="de">ersitzen</trd>
  <trd lng="en">usucapt</trd>
  <trd lng="es">usucapir</trd>
  <trd lng="fi">saada omistusoikeus pitk&auml;aikaisen nautinnan
nojalla</trd>
  <trd lng="hu">elbirtokol</trd>
  <trd lng="it">usucapire</trd>
  <trd lng="ja">&#26178;&#x52B9;&#x53D6;&#x5F97;&#12377;&#x308B;  <pr>&#12376;&#12371;&#12358;&#12375;&#12421;&#12392;&#x304F;&#12377;&#x308B;</pr></trd>
  <trd lng="nl">verwerven<klr tip="ind"> (door verjaring)</klr></trd>
  <trd lng="pl">naby&cacute; przez <ind>zasiedzenie</ind></trd>
  <trdgrp lng="pt">
    <trd>usucapir</trd>
   </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;
&c_p;&c_r;&c_a;&c_v;&c_o;
&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_i;
&c_p;&c_o; &c_d;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_i;
&c_v;&c_l;&c_a;&c_d;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">z&aacute;konite z&iacute;ska&tcaron; nie&ccaron;o nepretr&zcaron;itou dr&zcaron;bou</trd>
  <trd lng="uk">&#x44E;&c_r;. &#x43D;&c_a;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43F;&c_r;&c_a;&c_v;&#x43E; &c_v;&#x43B;&c_a;&c_s;&#x43D;&#x43E;&c_s;&c_t;&c_ib;
  &c_v;&#x43D;&c_a;&c_s;&#x43B;&c_ib;&c_d;&#x43E;&#x43A; &c_t;&c_r;&c_i;&c_v;&c_a;&#x43B;&#x43E;&c_g;&#x43E;
  &c_v;&c_i;&#x43A;&#x43E;&c_r;&c_i;&c_s;&c_t;&c_a;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="uzukap.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       La le&gcirc;a rajto akiri la propra&jcirc;on per neinterrompita
posedo.
     </dif>
     <refgrp tip="vid">
       <ref cel="preskr1.0o">preskripto</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="cs">
    <trd>usucapio</trd>,
    <trd>vydr&zcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Usukapion</trd>,
     <trd>Ersitzung</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>usucaption</trd>,
     <trd>usucapion <klr>(Roman law)</klr></trd>
   </trdgrp>
  <trd lng="es">usucapi&oacute;n</trd>
  <trd lng="fi">nauttimussaanto</trd>
  <trd lng="fr">usucapion</trd>
  <trd lng="hu">elbirtokl&aacute;s</trd>
  <trd lng="it">usucapione</trd>
  <trd lng="ja">&#26178;&#x52B9;&#x306B;&#12424;&#x308B;&#25152;&#26377;&#x6A29;&#x53D6;&#x5F97; 
  <pr>&#12376;&#12371;&#12358;&#x306B;&#12424;&#x308B;&#12375;&#12423;&#12422;&#12358;&#12369;&#12435;&#12375;&#12421;&#12392;&#x304F;</pr></trd>
  <trd lng="nl">verwerving<klr tip="ind"> (door verjaring)</klr></trd>
  <trdgrp lng="pl">
    <trd>zasiedzenie</trd>,
    <trd>usucapio</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pr&aacute;vo z&iacute;ska&tcaron; nie&ccaron;o nepretr&zcaron;itou dr&zcaron;bou</trd>,
    <trd>&uacute;zukapia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_z;&c_u;&#x43A;&c_a;&#x43F;&c_ib;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_b;&c_u;&c_t;&c_t;&#x44F; &#x43F;&c_r;&c_a;&c_v;&c_a; &c_v;&#x43B;&c_a;&c_s;&#x43D;&#x43E;&c_s;&c_t;&c_ib;
    &c_v;&#x43D;&c_a;&c_s;&#x43B;&c_ib;&c_d;&#x43E;&#x43A; &c_t;&c_r;&c_i;&c_v;&c_a;&#x43B;&#x43E;&c_g;&#x43E;
    &c_v;&c_i;&#x43A;&#x43E;&c_r;&c_i;&c_s;&c_t;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
