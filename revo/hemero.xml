<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemero.xml,v 1.20 2022/04/30 08:42:41 revo Exp $">
<kap>
  <rad>hemerokalid</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="piv">hemerokal</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="hemero.0o">
  <kap><tld/>o, <var><kap><tld var="piv"/>o</kap></var></kap>
  <snc mrk="hemero.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="lili.tago0o.BOT">Tagolilio</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> sufi&ccirc;e diversaj de koloro kaj formo,
        plejofte kultivita
        de amantoj de &ccirc;i tiuj plantoj, pro tio la nombro da kultiva&jcirc;oj
        nuntempe estas
        kelkmiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=454731">Hemerokalo</url></lok>
        </fnt>.
      </ekz>    
      <ekz>
        <tld var="piv"/>o ne nur estas dekoracia planto, sed anka&ubreve; &gcirc;ia florbutono estas
        man&gcirc;ebla
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url ref="http://es.chinabroadcast.cn/1/2004/05/07/1@8333.htm">Panjo estas
        plej amata...</url></vrk>,
        <lok>2004-05-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/23/Hemerocallis_fulva_2012_G1.jpg/320px-Hemerocallis_fulva_2012_G1.jpg" prm="PD"><tld/>o, ver&scirc;ajne hibrido, Ukrainujo
        <fnt>
          <aut>George Chernilevsky, propra verko</aut>
          <lok><url ref="&WCW;/File:Hemerocallis_fulva_2012_G1.jpg">&WCW;/File:Hemerocallis_fulva_2012_G1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_s;&c_a;&c_d;&c_z;&c_jo;&c_n;</trd>
  <trd lng="cs">denivka</trd>
  <trd lng="de">Taglilie</trd>
  <trd lng="fr">h&eacute;m&eacute;rocalle</trd>
  <trd lng="hu">s&aacute;sliliom</trd>
  <trd lng="nl">daglelie</trd>
  <trd lng="pt">hemerocale</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_o;&c_d;&c_n;&c_je;&c_v;</trd>,
    <trd>&c_l;&c_i;&c_l;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>denivka</trd>,
    <trd>&lcaron;aliovka</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x456;&#x43B;&#x456;&#x439;&#x43D;&#x438;&#x43A;</trd>
</drv>
</art>
