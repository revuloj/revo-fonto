<drv mrk="sving.man0i">
  <kap>man<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sving.man0i.KOMUNE">
    <dif>
      <tld lit="S"/>i manon a&ubreve; la manojn por de malproksime
      atentigi iun, saluti, adia&ubreve;i...:
      <ekz>
        ekmovi&gcirc;as la trajno, oni man<tld/>as
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>;
      </ekz>
      <ekz>
        mi senesperi&gcirc;e man<tld/>is, el kio li konkludis, ke mi devas esti tre laca
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> kolere man<tld/>is
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili per na&ubreve;za man<tld/>o silentigis min
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li per man<tld/>o &scirc;ovis librojn de sur se&gcirc;o sur la plankon
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> flankenmetis per man<tld/>o la demandon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007991.html">Espero</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&aacute;vat na pozdrav</trd>
  <trdgrp lng="de">
    <trd>winken</trd>,
    <trd>abwinken</trd>,
