<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sving.xml,v 1.24 2021/06/06 07:08:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sving</rad>/i
</kap>
<drv mrk="sving.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sving.0i.skui">
    <dif>
      Moveti ion rapide kaj ripete, &ccirc;iudirekten rapide skuadi:
      <ekz>
        <tld/>i vipon, bastonon, la pugnon, la remilojn;
      </ekz>
      <ekz>
        hundo <klr>[&#8230;]</klr> <tld/>as la voston kaj atendas
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 4a</lok></fnt>;
      </ekz>
      <ekz>
        la grandega papilio <klr>[&#8230;]</klr> <tld/>is siajn flugilojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i la &ccirc;apelon por saluti;
      </ekz>
      <ekz>
        <nom>Anna Andrejevna</nom> <klr>[&#8230;]</klr> <tld/>as per la naztuko
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 1a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ante la penikojn de sia velura vesto<fnt>B</fnt>;
      </ekz>
      <ekz>
        ek<tld/>eti la fingron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o de &scirc;ia mano estis tro forta kaj la birdoj
        forflugis<fnt>B</fnt>;
      </ekz>
      <ekz>
        ribela deziro el<tld/>i el la jugo sian nukon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balanc1.0i">balanci</ref>,
      <ref cel="kirl.0i">kirli</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>schwingen</trd>,
      <trd>schwenken</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&aacute;z</trd>,
      <trd>lenget</trd>,
      <trd>l&oacute;b&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>agitare</trd>,
      <trd>dimenare</trd>,
      <trd>brandire</trd>, 
      <trd>battere<klr> (di ali)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>zwaaien</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_a;&c_h;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_z;&c_m;&c_a;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_a;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sving.0i.jxeti">
    <dif>
      Forte stre&ccirc;i la muskolojn kaj subite &jcirc;eti ion
      anta&ubreve;en:
      <ekz>
        <tld/>i pilkon, martelon;
      </ekz>
      <ekz>
        forte <tld/>inte la ar&gcirc;entan pelvon, li &jcirc;etis
        &gcirc;in en
        Rejnon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>is la piedojn kaj eksaltis sur la sofo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i alte sian piedon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu tro forte la manon <tld/>as, nenion atingas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ili dancis <tld/>ante sin tien kaj reen<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">wegschnellen</trd>
    <trdgrp lng="hu">
      <trd>lend&iacute;t</trd>,
      <trd>lenget</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>lanciare</trd>,
      <trd>scoccare</trd>
    </trdgrp>
    <trd lng="nl">slingeren</trd>
    <trd lng="ru">&c_v;&c_z;&c_m;&c_a;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sving.0i.vibrigi">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Vibrigi:
      <ekz>
        <tld/>i kaj tremigi kordon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio estas nur <tld/>i&gcirc;oj kaj movi&gcirc;oj de la
        nervoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">rezegtet</trd>    
    <trd lng="it"><klr>far </klr>vibrare</trd>
    <trd lng="nl">doen <ind>trillen</ind></trd>
    <trd lng="ru">&c_t;&c_r;&c_ja;&c_s;&c_t;&c_i;</trd>
  </snc>
  <snc mrk="sving.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekmovi, ekimpulsi, instigi:
      <ekz>
        lango nenion atingas, se &gcirc;in sa&gcirc;o ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        puno pekon <tld/>as, favorkoreco &gcirc;in estingas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>mozgat</trd>,
      <trd>&ouml;szt&ouml;n&ouml;z</trd>
    </trdgrp>
    <trd lng="it">scuotere</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>m&aacute;vat</trd>,
    <trd>mr&scaron;tit</trd>,
    <trd>&scaron;vihnout</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>to wave</trd>, <trd>swing</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>agiter</trd>,
    <trd>balancer</trd>,
    <trd>brandir</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>m&aacute;va&tcaron;</trd>,
    <trd>zam&aacute;va&tcaron;</trd>,
    <trd>&scaron;viha&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="sving.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="sving.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        per unu ek<tld/>o &scirc;i transflugis sur la transan bordon
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        per unu <tld/>o de via glavo vi finis la militon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> flankenmetis per man<tld/>o la demandon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2003/007991.html"
              >Espero</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>osono de la flugiloj superis la bruon de la a&ubreve;to&scirc;oseo
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007052.html"
              >Verda nature, science, socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la batanto do devas komenci sian anta&ubreve;en-<tld/>on anta&ubreve; ol la pilko komencas
        montri multan horizontalan movi&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Vus-Tit Zik</aut>
          <vrk><url ref="https://www.monato.be/2009/009769.php"
              >Iom da historio kaj iom da fiziko</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schwung</trd>
  </snc>
  <snc mrk="sving.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Stilvario de <ref tip="super" cel="jxaz.0o">&jcirc;azo</ref> kaj dancstilo la&ubreve; 
      tia muziko esti&gcirc;inta en Usono je la 1920aj jaroj:
      <ekz>
        okcidenta <tld/>o estas &jcirc;az-subspeco, en kiu intermiksi&gcirc;is ritmoj de tradiciaj
        <tld/>o, bluso, diksilando kaj e&ccirc; polko-danco
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="https://www.monato.be/2012/010439.php"
              >Svingante la &scirc;tatan flagon</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="rokenrol.0o">rokenrolo</ref>
    </refgrp>
    <trd lng="de">Swing</trd>
    <trd lng="en">swing</trd>
    <trd lng="fr">swing</trd>
  </snc>
</drv>




<drv mrk="sving.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sving.0igxi.KOMUNE">
    <dif>
      Sin rapide demovi tien a&ubreve; reen:
      <ekz>
        la infanoj rigardas la gaje <tld/>i&gcirc;antajn ru&gcirc;ajn
        fi&scirc;ojn;
      </ekz>
      <ekz>
        junuloj kaj knabinoj <tld/>i&gcirc;as en danco
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>;
      </ekz>
      <ekz>
        la glitkurantoj <tld/>i&gcirc;adis sur la <klr>[&#8230;]</klr> lagoj
        <fnt><bib>Fab3</bib>, <lok>historio de la jaro</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la insekto]</klr> per oblikva <tld/>i&gcirc;o saltis rekte
        sur
        la
        bruston al la re&gcirc;idino
        <fnt><bib>Fab2</bib>, <lok>konkurso de saltado</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>slingeren</trd>,
      <trd>zwaaien</trd>,
      <trd>zwiepen</trd>,
      <trd>zwieren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>to wave</trd>,
    <trd>swing</trd>, <trd>flutter</trd>, <trd>oscillate</trd>,
    <trd>undulate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>agiter</ind></trd>,
    <trd>se <ind>balancer</ind></trd>,
    <trd>s'<ind>&eacute;lancer</ind></trd>,
    <trd>gigoter</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dimenarsi</trd>,
    <trd>guizzare</trd>,
    <trd>agitarsi<klr> (dimenarsi)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sving.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="sving.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      &Ccirc;ia penda parto de ilo a&ubreve; ma&scirc;ino, fiksita je
      unu ekstremo kaj
      povanta <tld/>i&gcirc;i:
    </dif>
    <subsnc mrk="sving.0ilo.sonorila">
      <dif>
        Frapilo pendanta en sonorilo.
      </dif>
      <trd lng="de">Schwengel</trd>
      <trd lng="en">clapper</trd>
      <trd lng="hu">harangnyelv</trd>
      <trd lng="it">batacchio</trd>
      <trd lng="nl">klepel</trd>
      <trd lng="ru">&c_ja;&c_z;&c_y;&c_k;
      &c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_a;</trd>
    </subsnc>
    <subsnc mrk="sving.0ilo.horlogxa">
      <dif>
        Pendolo de horlo&gcirc;o.
      </dif>
      <trd lng="de">Pendel</trd>
      <trd lng="en">pendulum</trd>
      <trd lng="hu">inga</trd>
      <trd lng="it">pendolo<klr> (di orologio)</klr></trd>
      <trd lng="nl">slinger</trd>
      <trd lng="ru">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>
    </subsnc>
    <subsnc mrk="sving.0ilo.drasxila">
      <dif>
        Batilo de dra&scirc;ilo.
      </dif>
      <trd lng="de">Kl&ouml;ppel</trd>
      <trd lng="en">swipple</trd>
      <trd lng="hu">cs&eacute;phadar&oacute;</trd>
      <trd lng="it">vetta<klr> (agr. mecc.)</klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>bij&aacute;k cepu</trd>,
    <trd>cepovka</trd>,
    <trd>kyvadlo</trd>,
    <trd>p&aacute;ka pumpy</trd>,
    <trd>srdce zvonu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balancier</trd>,
    <trd>battant</trd>,
    <trd>marteau<klr tip="amb"> (de cloche)</klr></trd>
  </trdgrp>
  <trd lng="it">battente</trd>
  <trdgrp lng="sk">
    <trd>kyvadlo</trd>,
    <trd>p&aacute;ka pumpy</trd>,
    <trd>srdce zvonu</trd>
  </trdgrp>
</drv>

<drv mrk="sving.man0i">
  <kap>man<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sving.man0i.KOMUNE">
    <dif>
      <tld lit="S"/>i manon a&ubreve; la manojn por de malproksime
      atentigi
      iun,
      saluti, adia&ubreve;i...:
      <ekz>
        ekmovi&gcirc;as la trajno, oni man<tld/>as
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&aacute;vat na pozdrav</trd>
  <trd lng="de">winken</trd>
  <trd lng="en">to wave <klr>(the hand)</klr></trd>
  <trd lng="fr">faire signe de la main</trd>
  <trdgrp lng="it">
    <trd><ind>dimenare</ind> la mano</trd>,
    <trd><ind>agitare</ind> la mano</trd>,
    <trd>salutare<klr> (con la mano)</klr></trd>
  </trdgrp>
  <trd lng="nl">zwaaien</trd>
  <trdgrp lng="sk">
    <trd>k&yacute;va&tcaron;</trd>,
    <trd>m&aacute;va&tcaron; rukou</trd>
  </trdgrp>
</drv>

<drv mrk="sving.kur0igxi">
  <kap>kur<tld/>i&gcirc;i</kap>
  <snc mrk="sving.kur0igxi.KOMUNE">
    <dif>
      Sin anta&ubreve;en&jcirc;eti por <ref tip="vid"
      cel="salt.0i.sin_levi">salti</ref>.
    </dif>
  </snc>
  <trd lng="fr">s'<ind>&eacute;lancer</ind></trd>
  <trd lng="it">slanciarsi<klr> (nel salto)</klr></trd>
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  <klr tip="amb">(&c_d;&c_l;&c_ja;
  &c_p;&c_r;&c_y;&c_zh;&c_k;&c_a;)</klr></trd>
</drv>
</art>
