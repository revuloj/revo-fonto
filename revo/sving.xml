<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sving</rad>/i
</kap>
<drv mrk="sving.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Moveti ion rapide kaj ripete, &ccirc;iudirekten rapide skuadi:
      <ekz>
        <tld/>i vipon, bastonon, la pugnon, sian &ccirc;apelon, la remilojn;
      </ekz>
      <ekz>
        <tld/>i la flugilojn, la voston<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la &ccirc;apelon, la manon por saluti;
      </ekz>
      <ekz>
        <tld/>i per la naztuko<fnt>Z</fnt>
        <klr>(por adia&ubreve;i)</klr>;
      </ekz>
      <ekz>
        <tld/>ante la penikojn de sia velura vesto<fnt>B</fnt>;
      </ekz>
      <ekz>
        ek<tld/>eti la fingron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la infanoj rigardas la gaje <tld/>i&gcirc;antajn ru&gcirc;ajn fi&scirc;ojn;
      </ekz>
      <ekz>
        la <tld/>o de &scirc;ia mano estis tro forta kaj la birdoj
        forflugis<fnt>B</fnt>;
      </ekz>
      <ekz>
        ribela deziro el<tld/>i el la jugo sian nukon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kirl.0i">kirli</ref>,
      <ref cel="balanc1.0i">balanci</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>schwingen</trd>,
      <trd>schwenken</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&aacute;z</trd>,
      <trd>lenget</trd>,
      <trd>l&oacute;b&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>zwaaien</trd>,
      <trd>doen <ind>trillen</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_a;&c_h;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_z;&c_m;&c_a;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_a;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Forte stre&ccirc;i la muskolojn kaj subite &jcirc;eti ion anta&ubreve;en:
      <ekz>
        <tld/>i pilkon, martelon;
      </ekz>
      <ekz>
        forte <tld/>inte la ar&gcirc;entan pelvon, li &jcirc;etis &gcirc;in en
        Rejnon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>is la piedojn kaj eksaltis sur la sofo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i alte sian piedon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu tro forte la manon <tld/>as, nenion atingas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ili dancis <tld/>ante sin tien kaj reen<fnt>Z</fnt>;
      </ekz>
      <ekz>
        junuloj kaj knabinoj <tld/>i&gcirc;as en danco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la glitkurantoj <tld/>i&gcirc;adis sur la lagoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        per oblikva <tld/>i&gcirc;o la insekto saltis rekte sur la bruston al la
        re&gcirc;idino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>pordo.
      </ekz>
    </dif>
    <trd lng="de">wegschnellen</trd>
    <trdgrp lng="hu">
      <trd>lend&iacute;t</trd>,
      <trd>lenget</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_z;&c_m;&c_a;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Vibrigi:
      <ekz>
        <tld/>i kaj tremigi kordon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio estas nur <tld/>i&gcirc;oj kaj movi&gcirc;oj de la nervoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">rezegtet</trd>
    <trd lng="ru">&c_t;&c_r;&c_ja;&c_s;&c_t;&c_i;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekmovi, ekimpulsi, instigi:
      <ekz>
        lango nenion atingas, se &gcirc;in sa&gcirc;o ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        puno pekon <tld/>as, favorkoreco &gcirc;in estingas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>mozgat</trd>,
      <trd>&ouml;szt&ouml;n&ouml;z</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">m&aacute;vat</trd>
  <trd lng="en">wave</trd>
</drv>

<drv mrk="sving.kur0igxi">
  <kap>kur<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin anta&ubreve;en&jcirc;eti por salti.
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_p;&c_r;&c_y;&c_zh;&c_k;&c_a;)</klr></trd>
</drv>

<drv mrk="sving.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Frapilo pendanta en sonorilo.
    </dif>
    <trd lng="de">Schwengel</trd>
    <trd lng="hu">harangnyelv</trd>
    <trd lng="nl">klepel</trd>
    <trd lng="ru">&c_ja;&c_z;&c_y;&c_k; &c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Pendolo de horlo&gcirc;o.
    </dif>
    <trd lng="de">Pendel</trd>
    <trd lng="hu">inga</trd>
    <trd lng="nl">slinger</trd>
    <trd lng="ru">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc>
    <dif>
      Batilo de dra&scirc;ilo.
    </dif>
    <trd lng="de">Kl&ouml;ppel</trd>
    <trd lng="hu">cs&eacute;phadar&oacute;</trd>
  </snc>
</drv>
</art>
