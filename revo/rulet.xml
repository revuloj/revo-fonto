<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rulet.xml,v 1.7 2023/09/03 07:53:12 revo Exp $">
<kap>
  <ofc>9</ofc><rad>rulet</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="rulet.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="rulet.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.hazard0o.SPO">Hazardludo</ref>, en kiu 
      ludanto gajnas, se globeto &jcirc;etita sur turni&gcirc;antan
      diskon haltas en fako kun la numero a&ubreve; koloro, kiun li
      anta&ubreve;elektis:
      <ekz>
        Tobbogan ali&gcirc;is al amaso da ludantoj, &ccirc;irka&ubreve;antaj <tld/>on
        sub markezo
        <fnt><aut>A. Grin, trad. J. Finkel</aut>, <vrk><url ref="http://www.esperanto.mv.ru/Grin/kuranta.html">Kuranta sur 
        ondoj</url></vrk>, <lok>2008</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">roulette<klr> (game)</klr></trd>
  <trd lng="es">ruleta<klr> (juego)</klr></trd>
  <trd lng="fr">roulette<klr> (jeu)</klr></trd>
  <trd lng="it">roulette</trd>
  <trd lng="ja">&#x30EB;&#x30FC;&#x30EC;&#x30C3;&#x30C8;</trd>
  <trdgrp lng="pl">
    <trd>ruleta</trd>,
    <trd>ruletka</trd>
  </trdgrp>
  <trd lng="sv">roulette</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_l;&c_je;&c_t;&c_k;&c_a; (&c_g;&c_r;&c_a;)</trd>,
    <trd>&c_c;&c_i;&c_r;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_a; &c_p;&c_i;&c_l;&c_a;</trd>
  </trdgrp>
</drv>
</art>

