<art mrk="$Id: rulet.xml,v 1.11 2025/10/30 14:00:22 revo Exp $">
<kap>
  <ofc>9</ofc><rad>rulet</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="rulet.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="rulet.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.hazard0o.SPO">Hazardludo</ref>, en kiu
      ludanto gajnas, se globeto &jcirc;etita sur turni&gcirc;antan
      diskon haltas en fako kun la numero a&ubreve; koloro, kiun li
      anta&ubreve;elektis:
      <ekz>
        Tobbogan ali&gcirc;is al amaso da ludantoj, &ccirc;irka&ubreve;antaj <tld/>on
        sub markezo
        <fnt><aut>A. Grin, trad. J. Finkel</aut>, <vrk><url
        ref="http://www.esperanto.mv.ru/Grin/kuranta.html">Kuranta sur
        ondoj</url></vrk>, <lok>2008</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_l;&c_je;&c_t;&c_k;&c_a; <klr
  tip="amb">(&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="en">roulette<klr> (game)</klr></trd>
  <trd lng="es">ruleta<klr> (juego)</klr></trd>
  <trd lng="fr">roulette<klr> (jeu)</klr></trd>
  <trd lng="it">roulette</trd>
  <trd lng="ja">&#12523;&#12540;&#12524;&#12483;&#12488;</trd>
  <trdgrp lng="pl">
    <trd>ruleta</trd>,
    <trd>ruletka</trd>
  </trdgrp>
  <trd lng="pt">roleta</trd>
  <trd lng="sv">roulette</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_l;&c_je;&c_t;&c_k;&c_a; (&c_g;&c_r;&c_a;)</trd>,
    <trd>&c_c;&c_i;&c_r;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_a; &c_p;&c_i;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32599;&#30424;&#24230; <pr>lu&oacute;p&aacute;nd&ugrave;</pr></trd>,
    <trd>&#32645;&#30436;&#24230; <pr>lu&oacute;p&aacute;nd&ugrave;</pr></trd>,
    <trd>&#36650;&#30436;&#36077; <pr>l&uacute;np&aacute;nd&#468;</pr></trd>,
    <trd>&#36718;&#30424;&#36172; <pr>l&uacute;np&aacute;nd&#468;</pr></trd>
  </trdgrp>
</drv>
</art>

