<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>sat</rad>/a <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="sat.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="sat.0a.KUI">
        <dif>
          Kies man&gcirc;obezono estas plene kvietigita:
          <ekz>
            <tld/>a mal<tld/>an ne povas kompreni
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            &gcirc;is<tld/>e man&gcirc;i, trinki
          </ekz>
          <sup>D</sup>;
          <ekz>
            havi <tld/>e (sufi&ccirc;e) da pano<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="hu">j&oacute;llakott</trd>
      </snc>
      <snc mrk="sat.0a.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Kies deziro estas plene kontentigita:
          <ekz>
            li mortis profunda&gcirc;a kaj <tld/>a de vivo<fnt>Z</fnt>;
          </ekz><ekz>
            <ind><tld/>dormi</ind>;
            <trd lng="hu">kialussza mag&aacute;t</trd>
            <trd lng="nl">uitslapen</trd>
          </ekz><ekz>
            li estas <tld/>a je vulupto, per plezuroj
          </ekz>
        </dif>
        <trd lng="hu">elege <ind>van vmib&odblac;l</ind></trd>
      </snc>
      <trd lng="be">&c_s;&c_y;&c_t;&c_y;</trd>
      <trd lng="cs">syt&yacute;</trd>
      <trd lng="de">satt</trd>
      <trd lng="en">satisfied</trd>
      <trd lng="es">satisfecho</trd>
      <trdgrp lng="fr">
        <trd>combl&eacute;<klr> (satisfait)</klr></trd>,
        <trd>repus</trd>,
        <trd>satisfait</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>verzadigd</trd>,
        <trd>voldaan</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_y;&c_t;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="sat.0.prefikso">
      <kap><tld/>-</kap>
      <snc>
        <dif>
          Prefikso de verboj, indikanta plenumon de la ago &gcirc;is plena
          kontento, satureco ktp:
          <ekz>
            <refgrp>
              <ref cel="dorm.sat0i"><tld/>dormi</ref>,
              <ref cel="manx.sat0i"><tld/>man&gcirc;i</ref>,
              <ref cel="parol.sat0i"><tld/>paroli</ref>,
              <ref cel="plor.sat0i"><tld/>plori</ref>,
              <ref cel="rid.sat0i"><tld/>ridi</ref>,
              <ref cel="trink.sat0i"><tld/>trinki</ref>
            </refgrp>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_n;&c_a;...&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="sat.0igi">
      <kap><tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="sat.0igi.KUI">
        <dif>
          Kontentigi la man&gcirc;obezonon sufi&ccirc;e <ref tip="vid" 
            cel="nutr.0i.BIO">nutrante</ref>:
          <ekz>
            kuiriston vaporo <tld/>igas
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            promeso ne <tld/>igas
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <trd lng="hu">j&oacute;llakat</trd>
      </snc>
      <snc mrk="sat.0igi.FIG">
        <dif>
          Kontentigi la deziron:
          <ekz>
            <tld/>igi siajn bezonojn<fnt>B</fnt>, instinktojn<fnt>B</fnt>,
            sian revantan animon, sian ambicion;
          </ekz><ekz>
            vi ekscitas sed ne <tld/>igas nian scivolemon;
          </ekz><ekz>
            liaj okuloj ne povis <tld/>i&gcirc;i je la vida&jcirc;o;
          </ekz><ekz>
            <tld/>i&gcirc;inta de la vivo<fnt>Z</fnt>;
          </ekz><ekz>
            ne<tld/>igebla sciavido.
          </ekz>
        </dif>
        <trd lng="hu">kiel&eacute;g&iacute;t</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_n;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_s;&c_y;&c_c;&c_ib;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>verzadigen</trd>,
        <trd>bevredigen</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_y;&c_t;&c_i;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="sat.mal0a">
      <kap>mal<tld/>a</kap>
      <snc mrk="sat.mal0a.KUI">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Sentanta man&gcirc;obezonon:
          <ekz>
            mal<tld/>a kiel lupo<fnt>B</fnt>;
          </ekz><ekz>
            &ccirc;e stomako mal<tld/>a ne kapricas palato
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            en mal<tld/>a familio mankas harmonio
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            mal<tld/>e morti.
            <trd lng="hu">&eacute;henhal</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_d;&c_n;&c_y;</trd>
      <trd lng="de">hungrig</trd>
      <trd lng="es">hambriento</trd>
      <trd lng="en">hungry</trd>
      <trd lng="fr">affam&eacute;</trd>
      <trd lng="hu">&eacute;hes</trd>
      <trdgrp lng="nl">
        <trd>hongerig</trd>
      </trdgrp>
      <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="sat.mal0i">
      <kap>mal<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="sat.mal0i.KUI">
        <dif>
          Senti la bezonon man&gcirc;i:
          <ekz>
            tuta jam <tld/>a, sed okulo mal<tld/>as
            <klr>(avidas)</klr>
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="fast.0i">fasti</ref>
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_g;&c_a;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_g;&c_a;&c_l;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
      </trdgrp>
      <trd lng="nl">honger hebben</trd>
      <trdgrp lng="es">
        <trd>tener hambre</trd>,
        <trd>estar hambriento</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>&ecirc;tre <ind>affam&eacute;</ind></trd>,
        <trd>avoir <ind>faim</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>&eacute;hes</trd>,
        <trd>&eacute;hezik</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_g;&c_o;&c_l;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_g;&c_o;&c_l;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
      </trdgrp>
    </drv>

    <drv mrk="sat.super0eco">
      <kap>super<tld/>eco, <var><kap>tro<tld/>i&gcirc;o</kap></var></kap>
      <snc mrk="sat.super0eco.KUI">
        <dif>
          Stato de tiu, kiu estas pli ol <tld/>a.
        </dif>
      </snc>
      <trd
       
lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      <trdgrp lng="fr">
        <trd>repl&eacute;tion</trd>,
        <trd>sati&eacute;t&eacute;</trd>
      </trdgrp>
      <trd lng="nl">oververzadigdheid</trd>
      <trdgrp lng="hu">
        <trd>eltelt</trd>,
        <trd>t&uacute;llakott</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </drv>
  </art>
