<drv mrk="sat.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sat.mal0i.KUI">
    <dif>
      Senti la bezonon man&gcirc;i:
      <ekz>
        la elpelito mal<tld/>as jam la trian tagon
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;telas por sin <tld/>igi, kiam li mal<tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 6:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen prenu ankora&ubreve; du panojn kaj unu &scirc;inkon, por ke vi ne bezonu mal<tld/>i
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>!
      </ekz>
      <ekz>
        estro ne mal<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tuta jam <tld/>a, sed okulo mal<tld/>as
        <klr>(avidas)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fast.0i">fasti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hladov&ecaron;t</trd>,
    <trd>m&iacute;t hlad</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hungern</trd>,
    <trd><ind>Hunger</ind> leiden</trd>,
    <trd><ind>Knast</ind> haben</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tener hambre</trd>,
    <trd>estar hambriento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>affam&eacute;</ind></trd>,
    <trd>avoir <ind>faim</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
