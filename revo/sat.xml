<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sat.xml,v 1.38 2022/05/05 05:09:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sat</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sat.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sat.0a.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kies man&gcirc;obezono estas plene kvietigita:
      <ekz>
        <tld/>a mal<tld/>an ne povas kompreni
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;is<tld/>e man&gcirc;i, trinki
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        havi <tld/>e <klr>(sufi&ccirc;e)</klr> da pano<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">j&oacute;llakott</trd>
  </snc>
  <snc mrk="sat.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kies deziro estas plene kontentigita:
      <ekz>
        li mortis profunda&gcirc;a kaj <tld/>a de vivo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>dormi</ind>;
        <trd lng="hu">kialussza mag&aacute;t</trd>
        <trd lng="nl">uitslapen</trd>
      </ekz>
      <ekz>
        li estas <tld/>a je volupto, per plezuroj.
      </ekz>
    </dif>
    <trd lng="hu">elege <ind>van vmib&odblac;l</ind></trd>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_t;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>nasycen&yacute;</trd>,
    <trd>syt&yacute;</trd>
  </trdgrp>
  <trd lng="de">satt</trd>
  <trd lng="en">satisfied</trd>
  <trd lng="es">satisfecho</trd>
  <trdgrp lng="fr">
    <trd>combl&eacute;<klr> (satisfait)</klr></trd>,
    <trd>repus</trd>,
    <trd>satisfait</trd>
  </trdgrp>
  <trd lng="it">sazio</trd>
  <trdgrp lng="nl">
    <trd>verzadigd</trd>,
    <trd>voldaan</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_y;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">nas&yacute;ten&yacute;</trd>
  <trd lng="sv">m&auml;tt</trd>
  <trd lng="uk">&c_s;&c_i;&c_t;&c_i;&c_j;</trd>
</drv>

<drv mrk="sat.0">
  <kap><tld/>-</kap>
  <snc mrk="sat.0.prefikso">
    <dif>
      Prefikso de verboj, indikanta plenumon de la ago &gcirc;is plena
      kontento, satureco ktp:
      <ekz>
        <refgrp>
          <ref cel="dorm.sat0i"><tld/>dormi</ref>,
          <ref cel="mangx.sat0i"><tld/>man&gcirc;i</ref>,
          <ref cel="parol.sat0i"><tld/>paroli</ref>,
          <ref cel="plor.sat0i"><tld/>plori</ref>,
          <ref cel="trink.sat0i"><tld/>trinki</ref>
        </refgrp>.
      </ekz>
    </dif>
  </snc><trd lng="fr">... son content
  <klr> (jusqu'&agrave; pleine satisfaction)</klr></trd>
  <trd lng="ru">&c_n;&c_a;...&c_s;&c_ja;</trd>
</drv>

<drv mrk="sat.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sat.0igi.KUI">
    <dif>
      Kontentigi la man&gcirc;obezonon sufi&ccirc;e <ref tip="vid" cel="nutr.0i.BIO">nutrante</ref>:
      <ekz>
        kuiriston vaporo <tld/>igas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        promeso ne <tld/>igas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">j&oacute;llakat</trd>
  </snc>
  <snc mrk="sat.0igi.FIG">
    <dif>
      Kontentigi la deziron:
      <ekz>
        <tld/>igi siajn bezonojn<fnt>B</fnt>, instinktojn<fnt>B</fnt>,
        sian revantan animon, sian ambicion;
      </ekz>
      <ekz>
        vi ekscitas sed ne <tld/>igas nian scivolemon.
      </ekz>
    </dif>
    <trd lng="hu">kiel&eacute;g&iacute;t</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_y;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t dosyta naj&iacute;st</trd>,
    <trd>nasytit</trd>
  </trdgrp>
  <trd lng="fr">satisfaire</trd>
  <trd lng="it">saziare</trd>
  <trdgrp lng="nl">
    <trd>verzadigen</trd>,
    <trd>bevredigen</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_y;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>da&tcaron; dos&yacute;ta najes&tcaron;</trd>,
    <trd>nas&yacute;ti&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">m&auml;tta</trd>
  <trd lng="uk">&#x43D;&c_a;&c_s;&c_i;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sat.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sat.0igxi.KUI">
    <dif>
      Preni sufi&ccirc;an man&gcirc;on, ne plu senti mal<tld/>on.
    </dif>
  </snc>
  <snc mrk="sat.0igxi.FIG">
    <dif>
      Kontenti&gcirc;i, plenumi sian deziron:
      <ekz>
        liaj okuloj ne povis <tld/>i&gcirc;i je la vida&jcirc;o;
      </ekz>
      <ekz>
        <tld/>i&gcirc;inta de la vivo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">se <ind>rassasier</ind></trd>
  <trd lng="uk">&#x43D;&c_a;&c_s;&c_i;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="sat.gxis0e">
  <kap>&gcirc;is<tld/>e</kap>
  <snc mrk="sat.gxis0e.KOMUNE">
    <dif>
      &Gcirc;is atingo de <tld/>eco, la&ubreve;dezire,
      <ref tip="vid" cel="vol.laux0e">la&ubreve;vole</ref>:
      <ekz>
        vi povas man&gcirc;i vinberojn kiom vi volos, &gcirc;is<tld/>e  
        <fnt><bib>MT</bib>, <lok>&Rea; 23:24</lok></fnt>;  
      </ekz>;
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi kaj mi &gcirc;is<tld/>e konversacios
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/tais.html">
          Tais el Ateno</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_s;&c_y;&c_t;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>&agrave; sati&eacute;t&eacute;</trd>,
    <trd>jusqu'&agrave; plus soif</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&c_s;&c_i;&c_t;&c_a;</trd>,
    <trd>&c_d;&#x43E;&c_s;&c_h;&#x43E;&c_ch;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="sat.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sat.mal0a.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sentanta man&gcirc;obezonon:
      <ekz>
        mal<tld/>a kiel lupo<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;e stomako mal<tld/>a ne kapricas palato
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en mal<tld/>a familio mankas harmonio
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>e morti.
        <trd lng="hu">&eacute;henhal</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="bo">&t_ga;&t_rata;&t_naro;&t_da;&t_tsheg;&t_kha;&t_naro;&t_ga;&t_tsheg;&t_la;&t_tata;&t_naro;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hladov&yacute;</trd>,
    <trd>la&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">hungrig</trd>
  <trd lng="en">hungry</trd>
  <trd lng="es">hambriento</trd>
  <trd lng="fr">affam&eacute;</trd>
  <trd lng="hu">&eacute;hes</trd>
  <trd lng="it">affamato</trd>
  <trdgrp lng="nl">
    <trd>hongerig</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hladn&yacute;</trd>,
    <trd>la&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="sv">hungrig</trd>
  <trd lng="uk">&c_g;&#x43E;&#x43B;&#x43E;&c_d;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="sat.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sat.mal0i.KUI">
    <dif>
      Senti la bezonon man&gcirc;i:
      <ekz>
        tuta jam <tld/>a, sed okulo mal<tld/>as
        <klr>(avidas)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fast.0i">fasti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hladov&ecaron;t</trd>,
    <trd>m&iacute;t hlad</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tener hambre</trd>,
    <trd>estar hambriento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>affam&eacute;</ind></trd>,
    <trd>avoir <ind>faim</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;hes</trd>,
    <trd>&eacute;hezik</trd>
  </trdgrp>
  <trd lng="it">essere <ind>affamato</ind></trd>
  <trd lng="nl">honger hebben</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_l;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_g;&c_o;&c_l;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladova&tcaron;</trd>,
    <trd>ma&tcaron; hlad</trd>
  </trdgrp>
  <trd lng="sv">hungra</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_g;&#x43E;&#x43B;&#x43E;&c_d;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&c_g;&#x43E;&#x43B;&#x43E;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&#x43E;&c_t;&c_ib;&c_t;&c_i; &c_ji;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x997F; [<ind>&egrave;</ind>]</trd>
</drv>

<drv mrk="sat.ne0igebla">
  <kap>ne<tld/>igebla</kap>
  <snc mrk="sat.ne0igebla.KUI">
    <dif>
      Kiun nenio <tld/>igas, nenio sufi&ccirc;e nutras:
      <ekz>
        la ne<tld/>igebla Maltano ensorbis, &ccirc;iutage post la vesperman&gcirc;o, grandegan
        pladon da aveloj
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la
          Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sat.ne0igebla.FIG">
    <dif>
      Kiun nenio kontentigas:
      <ekz>
        ne<tld/>igebla sciavido.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_n;&c_a;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_n;&c_a;&c_s;&c_y;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">nenasytn&yacute;</trd>
  <trdgrp lng="fr">
    <trd>inassouvissable</trd>,
    <trd>insatiable</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nen&aacute;sytn&yacute;</trd>,
    <trd>pa&zcaron;rav&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&c_je;&#x43D;&c_a;&c_s;&c_i;&c_t;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="sat.super0eco">
  <kap>super<tld/>eco, <var><kap>tro<tld/>i&gcirc;o</kap></var></kap>
  <snc mrk="sat.super0eco.KUI">
    <dif>
      Stato de tiu, kiu estas pli ol <tld/>a:
      <ekz>
        oni diras ke per <klr>[malsato]</klr> la spirito
        lerti&gcirc;as kaj male per la tro<tld/>i&gcirc;o
        <fnt>
          <aut>E. de Bonaero</aut>,
          <vrk>La Friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>repl&eacute;tion</trd>,
    <trd>sati&eacute;t&eacute;</trd>
  </trdgrp>
  <trd lng="nl">oververzadigdheid</trd>
  <trdgrp lng="hu">
    <trd>eltelt</trd>,
    <trd>t&uacute;llakott</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>satollo</trd>,
    <trd>rimpinzato</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;verm&auml;tt</trd>,
    <trd>d&auml;st</trd>
  </trdgrp>
</drv>
</art>
