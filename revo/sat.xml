<drv mrk="sat.ne0igebla">
  <kap>ne<tld/>igebla</kap>
  <snc mrk="sat.ne0igebla.KUI">
    <dif>
      Kiun nenio <tld/>igas, nenio sufi&ccirc;e nutras:
      <ekz>
        nesatigebla soifo &scirc;in konsumis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ne<tld/>igebla Maltano ensorbis, &ccirc;iutage post la vesperman&gcirc;o, grandegan
        pladon da aveloj
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la
          Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="id">tidak pernah kenyang</trd>
  </snc>
  <snc mrk="sat.ne0igebla.FIG">
    <dif>
      Kiun nenio kontentigas:
      <ekz>
        ne<tld/>igebla ambicio
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ekkonis ilian ne<tld/>igeblan fieron kaj deziron regni
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>igeblaj sendankuloj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIX</lok>
        </fnt>!
      </ekz>
      <ekz>
        ne<tld/>igebla estas mia deziro &ccirc;ion ekvidi, &ccirc;ion trapalpi, nenion preterlasi
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        indignigis nin la ne<tld/>igebla postulemo de la edzino
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="id">
    <trd><baz>puas</baz>tak terpuaskan</trd>,
    <trd>tak pernah <ind>puas</ind></trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_n;&c_a;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_n;&c_a;&c_s;&c_y;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">nenasytn&yacute;</trd>
  <trd lng="de">uners&auml;ttlich</trd>
  <trdgrp lng="fr">
    <trd>inassouvissable</trd>,
    <trd>insatiable</trd>
  </trdgrp>
  <trdgrp lng="ja">
