<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sat</rad>/a
</kap>
<drv mrk="sat.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <dif>
    Karakterizas iun:
  </dif>
  <snc>
    <dif>
      Kies man&gcirc;obezono estas plene kvietigita:
      <ekz>
        <tld/>a mal<tld/>an ne povas kompreni
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;is<tld/>e man&gcirc;i, trinki
      </ekz>
      <sup>D</sup>;
      <ekz>
        havi <tld/>e (sufi&ccirc;e) da pano<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Kies deziro estas plene kontentigita:
      <ekz>
        li mortis profunda&gcirc;a kaj <tld/>a de vivo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>dormi</ind>;
        <trd lng="nl">uitslapen</trd>
      </ekz>
      <ekz>
        li estas <tld/>a je vulupto, per plezuroj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="glat">glate</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fort.0o">forto</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">syt&yacute;</trd>
  <trd lng="de">satt</trd>
  <trd lng="en">satisfied</trd>
  <trd lng="es">satisfecho</trd>
  <trdgrp lng="nl">
    <trd>verzadigd</trd>,
    <trd>voldaan</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_y;&c_t;&c_y;&c_j;</trd>
</drv>

<drv mrk="sat.0igi">
  <kap><tld/>igi</kap>
  <dif>
    Kontentigi la man&gcirc;obezonon a&ubreve; la deziron:
  </dif>
  <snc>
    <ekz>
      kuiriston vaporo <tld/>igas
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      promeso ne <tld/>igas
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
  </snc>
  <snc>
    <ekz>
      <tld/>igi siajn bezonojn<fnt>B</fnt>, instinktojn<fnt>B</fnt>,
      sian revantan animon, sian ambicion;
    </ekz>
    <ekz>
      vi ekscitas sed ne <tld/>igas nian scivolemon;
    </ekz>
    <ekz>
      liaj okuloj ne povis <tld/>i&gcirc;i je la vida&jcirc;o;
    </ekz>
    <ekz>
      <tld/>i&gcirc;inta de la vivo<fnt>Z</fnt>;
    </ekz>
    <ekz>
      ne<tld/>igebla sciavido.
    </ekz>
  </snc>
  <trdgrp lng="nl">
    <trd>verzadigen</trd>,
    <trd>bevredigen</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_y;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sat.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Sentanta man&gcirc;obezonon:
      <ekz>
        mal<tld/>a kiel lupo<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;e stomako mal<tld/>a ne kapricas palato
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en mal<tld/>a familio mankas harmonio
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>e morti.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">hungrig</trd>
  <trd lng="es">hambriento</trd>
  <trd lng="en">hungry</trd>
  <trdgrp lng="nl">
    <trd>hongerig</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sat.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti la bezonon man&gcirc;i:
      <ekz>
        tuta jam <tld/>a, sed okulo mal<tld/>as
        <klr>(avidas)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fast.0i">fasti</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">honger hebben</trd>
  <trdgrp lng="es">
     <trd>tener hambre</trd>,
     <trd>estar hambriento</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_l;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_g;&c_o;&c_l;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sat.super0ecotro0igxo">
  <kap>super<tld/>eco, tro<tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Stato de tiu, kiu estas pli ol <tld/>a.
    </dif>
  </snc>
  <trd lng="nl">oververzadigdheid</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
