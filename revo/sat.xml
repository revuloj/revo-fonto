    <trd>&eacute;hezik</trd>
  </trdgrp>
  <trd lng="id">lapar</trd>
  <trd lng="it">essere <ind>affamato</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x98E2;&#x3048;&#x3066;&#x3044;&#x308B;  <pr>&#x3046;&#x3048;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x8179;&#x3092;&#x3059;&#x304B;&#x305B;&#x3066;&#x3044;&#x308B;  <pr>&#x306F;&#x3089;&#x3092;&#x3059;&#x304B;&#x305B;&#x3066;&#x3044;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">honger hebben</trd>
