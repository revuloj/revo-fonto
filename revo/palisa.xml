<art mrk="$Id: palisa.xml,v 1.26 2024/08/20 15:27:28 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>palisandr</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="palisa.0o">
  <kap><tld/>o</kap>
  <snc mrk="palisa.0o.ligno">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Amerika valora, viole nigra <ref tip="super" cel="lign.0o">ligno</ref>,
      el arboj de la genro <trd lng="la">Dalbergia</trd>,
      uzata por luksaj mebloj:
      <ekz>
        li turnis sin al la <tld/>a tablo, sur kiu ku&scirc;is la tapi&scirc;oj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        kilogramo da <tld/>o kostas milojn da e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2012/010816.php">Restas nur espero</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mahago.0o">mahagono</ref>
    <bld lok="&WCU;/thumb/e/e3/Rio-Palisander%2C_dunkel_Holz.JPG/640px-Rio-Palisander%2C_dunkel_Holz.JPG" prm="CC-BY-SA-3.0">surfaco de <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Rio-Palisander,_dunkel_Holz.JPG">&WCW;/File:Rio-Palisander,_dunkel_Holz.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_s;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>palisandr</trd>,
    <trd>palisandrov&eacute; d&rcaron;evo</trd>,
    <trd>rosewood</trd>
  </trdgrp>
  <trd lng="de">Palisanderholz</trd>
  <trd lng="en">rosewood</trd>
  <trd lng="es">palisandro</trd>
  <trd lng="fr">palissandre</trd>
  <trd lng="hu">paliszander</trd>
  <trdgrp lng="ja">
    <trd>&#32043;&#27264;&#26448; <pr>&#12375;&#12383;&#12435;&#12374;&#12356;</pr></trd>,
    <trd>&#12471;&#12479;&#12531;</trd>
  </trdgrp>
  <trd lng="nl">palissanderhout</trd>
  <trd lng="pl">palisander</trd>
  <trdgrp lng="pt">
    <trd>jacarand&aacute;</trd>,
    <trd>carobagua&ccedil;u</trd>,
    <trd>palissandra</trd>
  </trdgrp>
  <trd lng="ro">palisandru</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_i;&c_s;&c_a;&c_n;&c_d;&c_r;</trd>
  <trd lng="sk">palisandrov&eacute; drevo</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_s;&c_a;&c_n;&c_d;&c_r;&c_o;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_ja;&c_k;&c_a;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_i;&c_n;&c_a; &c_p;&c_a;&c_l;&c_ib;&c_s;&c_a;&c_n;&c_d;&c_r;&c_o;&c_v;&c_o;&c_g;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_s;&c_a;&c_n;&c_d;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32005;&#26408; <pr>h&oacute;ngm&ugrave;</pr></trd>,
    <trd>&#32418;&#26408; <pr>h&oacute;ngm&ugrave;</pr></trd>
  </trdgrp>
</drv>
