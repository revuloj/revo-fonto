<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: palisa.xml,v 1.19 2021/01/13 18:11:02 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>palisandr</rad>/o
</kap>
<drv mrk="palisa.0o">
  <kap><tld/>o</kap>
  <snc mrk="palisa.0o.ligno">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Amerika valora, viole nigra <ref tip="super"
      cel="lign.0o">ligno</ref>, 
      el arboj de la genro <trd lng="la">Dalbergia</trd>, 
      uzata por luksaj mebloj:
      <ekz>
        li turnis sin al la <tld/>a tablo, sur kiu ku&scirc;is la tapi&scirc;oj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        kilogramo da <tld/>o kostas milojn da e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="https://www.monato.be/2012/010816.php"
              >Restas nur espero</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mahago.0o">mahagono</ref>
    <bld
      lok="&WCU;/thumb/e/e3/Rio-Palisander%2C_dunkel_Holz.JPG/640px-Rio-Palisander%2C_dunkel_Holz.JPG"
      prm="CC-BY-SA-3.0"
      >surfaco de <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Rio-Palisander,_dunkel_Holz.JPG"
              >https://commons.wikimedia.org/wiki/File:Rio-Palisander,_dunkel_Holz.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_s;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>palisandr</trd>,
    <trd>palisandrov&eacute; d&rcaron;evo</trd>,
    <trd>rosewood</trd>
  </trdgrp>
  <trd lng="de">Palisanderholz</trd>
  <trd lng="en">rosewood</trd>
  <trd lng="es">palisandro</trd>
  <trd lng="fr">palissandre</trd>
  <trd lng="hu">paliszander</trd>
  <trd lng="nl">palissanderhout</trd>
  <trdgrp lng="pt">
    <trd>jacarand&aacute;</trd>,
    <trd>carobagua&ccedil;u</trd>,    
    <trd>palissandra</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_i;&c_s;&c_a;&c_n;&c_d;&c_r;</trd>
  <trd lng="sk">palisandrov&eacute; drevo</trd>
</drv>
</art>
