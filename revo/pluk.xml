      <ekz>
        <uzo tip="stl">FIG</uzo>
        se post <tld/>ado de &ccirc;iuj &scirc;iaj pensoj li ne estos kapabla faligi &scirc;in en liton,
        nu, bone, li estus nemezureble stupida
        <fnt>
          <bib>Monato</bib>
          <vrk>Alberto Garc&iacute;a Fumero</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_r;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
