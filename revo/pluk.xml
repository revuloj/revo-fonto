<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pluk</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pluk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fingre kapti, eventuale &scirc;ire, per kunprema, pin&ccirc;a movo:
      <ekz>
        ili pli multe <tld/>us fruktojn a&ubreve; &scirc;irus florojn en la nomo de iu
        mastrino
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 246a</lok></fnt>;
      </ekz>
    </dif>
    <ekz>
      <uzo tip="stl">FIG</uzo><tld/>i el longa teksto a&ubreve; parolado unu
      solan frazon<fnt><aut>Stefan Maul</aut>: <vrk>Supra&jcirc;eco super
      &ccirc;io</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 5a, p.
      5a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">pfl&uuml;cken</trd>
  <trdgrp lng="fr">
    <trd>cueillir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szak&iacute;t<klr tip="ind"> (sz&uuml;retel)</klr></trd>,
    <trd>szed</trd>,
    <trd>felcs&iacute;p</trd>
  </trdgrp>
</drv>

<drv mrk="pluk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fingra kaptado, permana rikoltado.
    </dif>
  </snc>
  <trd lng="de">Pfl&uuml;cken</trd>
  <trdgrp lng="fr">
    <trd>cueillette</trd>
  </trdgrp>
</drv>
</art>

