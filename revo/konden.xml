<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kondensator</rad>/o
</kap>
<drv mrk="konden.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="konden.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Aparato, konsistanta el du 
      <ref tip="prt" cel="armatu.0o.de_kondensatoro">armaturoj</ref>, kaj uzata 
      por koncentrigi elektron.
    </dif>
    <refgrp tip="vid">
      <ref cel="akumul1.0o">akumulatoro</ref>,
      <ref cel="kapaci1.0o">kapacitanco</ref>.
    </refgrp>
    <adm>
      Necesas kontroli, &ccirc;u "koncentrigi" estas la &gcirc;usta vorto kaj
      doni pli fakan difinon. <aut>MB</aut>
    </adm>
    <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_e;&c_n;&c_s;&c_a;&c_t;&c_a;&c_r; <klr tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
    <trd lng="de">Kondensator</trd>
    <trd lng="en">capacitor</trd>
    <trd lng="fr">condensateur<klr tip="ind"> (&eacute;lectrique)</klr></trd>
    <trd lng="hu">kondenz&aacute;tor</trd>
    <trd lng="nl">condensator</trd>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_je;&c_n;&c_s;&c_a;&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc mrk="konden.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Simpla a&ubreve; duobla lenso, per kiu oni 
      <ref cel="konver.0igi">konver&gcirc;igas</ref> la lumon sur
      malgrandan surfacon.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_d;&c_e;&c_n;&c_s;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_a;&c_n;&c_d;&c_e;&c_n;&c_s;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_l;&c_ib;&c_n;&c_z;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>Brennglas</trd>,
     <trd>Sammellinse</trd>,
     <trd>Kondensor</trd>
    </trdgrp>
    <trd lng="en">condenser lens</trd>
    <trd lng="fr">condenseur<klr tip="ind"> (optique)</klr></trd>
    <trd lng="hu">kondenzorlencse</trd>
    <trd lng="nl">condensor</trd>
</snc>
</drv>
</art>
