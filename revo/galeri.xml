<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>galeri</rad>/o</kap>
<drv mrk="galeri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="galeri.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Longa kovrita ekstera ejo de domo, uzata kiel promenejo
      a&ubreve; rigardejo:
      <ekz>
        <tld/>o de mona&hcirc;ejo;
      </ekz>
      <ekz>
        korto kun graciaj arkadaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="korido.0o">koridoro</ref>,
      <ref cel="arkad.0o">arkado</ref>,
      <ref cel="klostr.0o">klostro</ref>,
      <ref cel="verand.0o">verando</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>torn&aacute;c</trd>;
      <trd>kereng&odblac;</trd>,
      <trd>&aacute;rk&aacute;d</trd>
    </trdgrp>
    <trd lng="nl">galerij</trd>
  </snc>
  <snc mrk="galeri.0o.ekspoziciejo">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Tre longa salono plej ofte uzata por elmontri arta&jcirc;ojn a&ubreve;
      kurioza&jcirc;ojn.
    </dif>
    <ref tip="super" cel="kabine.0o.ekspozicio">kabineto</ref>
    <ref tip="vid" cel="muze.0o.ejo">muzeo</ref>
    <trd lng="nl">galerij</trd>
  </snc>
  <snc mrk="galeri.0o.kolekto">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Kolekto da arta&jcirc;oj a&ubreve; kurioza&jcirc;oj:
      <ekz>
        <tld/>o de pentra&jcirc;oj
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o pri naturscienco.
      </ekz>
    </dif>
    <ref tip="vid" cel="kabine.0o.ekspozicio">kabineto</ref>
    <ref tip="vid" cel="muze.0o.kolekto">muzeo</ref>
    <trdgrp lng="hu">
      <trd>m&udblac;csarnok</trd>;
      <trd>gal&eacute;ria</trd>,
      <trd>m&udblac;gy&udblac;jtem&eacute;ny</trd>
    </trdgrp>
    <trd lng="nl">galerij</trd>
  </snc>
  <snc mrk="galeri.0o.mineja">
    <dif>
      Subtera koridoro, trairejo:
      <ekz>
        <tld/>o de minejo<fnt>B</fnt>,
      </ekz>
      <ekz>
        <tld/>o de subtera fervojo.
      </ekz>
    </dif>
    <ref tip="sin" cel="tunel.0o">tunelo</ref>
    <trdgrp lng="hu">
      <trd>v&aacute;gat</trd>;
      <trd>t&aacute;rna</trd>,
      <trd>j&aacute;rat</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd><klr>&c_p;&c_o;&c_d;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;</klr> &c_h;&c_o;&c_d;</trd>;
      <trd>&c_sh;&c_t;&c_r;&c_je;&c_k;</trd>,
      <trd>&c_sh;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_ja;</trd>
    </trdgrp>
    <trd lng="nl">tunnel</trd>
  </snc>
  <snc mrk="galeri.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      &Ccirc;irka&ubreve;loko por la publiko &ccirc;e altaj eta&gcirc;oj en
      teatro.
    </dif>
    <trdgrp lng="hu">
      <trd>karzat</trd>;
      <trd>erk&eacute;ly</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>balkon<klr tip="ind"> (in theater)</klr></trd>,
      <trd>galerij</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_a;&c_l;&c_jo;&c_r;&c_k;&c_a;</trd>
  </snc>
  <trd lng="de">Galerie</trd>
  <trd lng="en">gallery</trd>
  <trd lng="es">galer&iacute;a</trd>
  <trd lng="fr">galerie<klr tip="ind"> (t.s. sauf
  porte-bagages)</klr></trd>
  <trd lng="pt">galeria</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_je;&c_r;&c_je;&c_ja;</trd>
</drv>

<drv mrk="galeri.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Balustradeto ornamanta la supron de meblo:
      <ekz>
        mahagona liteto, &ccirc;irka&ubreve;ita de <tld/>eto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Supozeble temas pri plia senco de ~o kaj la sufikso -et
    konservas sian normalan sencon. <aut>MB</aut>
  </adm>
  <trd lng="fr">galerie<klr tip="ind"> (sur un meuble)</klr></trd>
</drv>
</art>
