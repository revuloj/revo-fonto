<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>galeri</rad>/o
    </kap>
    <drv mrk="galeri.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="galeri.0o.ARKI">
        <uzo tip="fak">ARKI</uzo>
        <dif>
          Longa kovrita ekstera ejo de domo, uzata kiel promenejo
          a&ubreve; rigardejo:
          <ekz>
            <tld/>o de mona&hcirc;ejo;
          </ekz><ekz>
            korto kun graciaj arkadaj <tld/>oj.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="korido.0o">koridoro</ref>.
        </refgrp>
      </snc>
      <snc mrk="galeri.0o.ekspoziciejo">
        <uzo tip="fak">BELA</uzo>
        <dif>
          Tre longa salono plej ofte uzata por elmontri arta&jcirc;ojn a&ubreve;
          kurioza&jcirc;ojn.
        </dif>
        <ref tip="sin" cel="muze.0o.ejo">muzeo</ref>
      </snc>
      <snc mrk="galeri.0o.kolekto">
        <uzo tip="fak">BELA</uzo>
        <dif>
          Kolekto da arta&jcirc;oj a&ubreve; kurioza&jcirc;oj:
          <ekz>
            <tld/>o de pentra&jcirc;oj
            <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
                Kupra porko</vrk>)</fnt>;
          </ekz><ekz>
            <tld/>o pri naturscienco.
          </ekz>
        </dif>
        <ref tip="sin" cel="muze.0o.kolekto">muzeo</ref>
      </snc>
      <snc mrk="galeri.0o.mineja">
        <dif>
          Subtera koridoro, trairejo:
          <ekz>
            <tld/>o de minejo<fnt>B</fnt>,
          </ekz><ekz>
            <tld/>o de subtera fervojo.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd><klr>&c_p;&c_o;&c_d;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;</klr> &c_h;&c_o;&c_d;</trd>;
          <trd>&c_sh;&c_t;&c_r;&c_je;&c_k;</trd>,
          <trd>&c_sh;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_ja;</trd></trdgrp>
      </snc>
      <snc mrk="galeri.0o.TEA">
        <uzo tip="fak">TEA</uzo>
        <dif>
          &Ccirc;irka&ubreve;loko por la publiko &ccirc;e altaj eta&gcirc;oj en
          teatro.
        </dif>
        <trd lng="ru">&c_g;&c_a;&c_l;&c_jo;&c_r;&c_k;&c_a;</trd>
      </snc>
      <trd lng="de">Galerie</trd>
      <trd lng="ru">&c_g;&c_a;&c_l;&c_je;&c_r;&c_je;&c_ja;</trd>
    </drv>
    <drv mrk="galeri.0eto">
      <kap><tld/>eto</kap>
      <snc>
        <dif>
          Balustradeto ornamanta la supron de meblo:
          <ekz>
            mahagona liteto, &ccirc;irka&ubreve;ita de <tld/>eto<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
    </drv>
  </art>

