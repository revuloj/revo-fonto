<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>granat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="granat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="granat.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <ref tip="malprt" cel="granat.0arbo.BOT"><tld/>arbo</ref>,
      kun multaj internaj rozkoloraj &ccirc;eloj kaj abunda dol&ccirc;a suko.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="de">Granatapfel</trd>
  <trd lng="es">granada</trd>
  <trd lng="fa">&a_A;&a_n;&a_A;&a_r;</trd>
  <trd lng="fr">grenade<klr tip="ind"> (fruit)</klr></trd>
  <trdgrp lng="hu">
    <trd>gr&aacute;n&aacute;talma</trd>,
    <trd>pomagr&aacute;n&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>granaat<klr tip="ind"> (fruit)</klr></trd>,
    <trd>granaatappel</trd>
  </trdgrp>
  <trd lng="pt">rom&atilde;</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="tr">nar</trd>
</drv>

<drv mrk="granat.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="granat.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbusto el familio <ref tip="super"
      cel="granat1.0oj"><tld/>ujacoj</ref>
      (<trd lng="la">Punica</trd>):
      <ekz>
        eleganta <tld/>arbo moketas figarbon,
        misforman pro nukdoloro
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Granatapfelbaum</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_A;&a_n;&a_A;&a_r;</ind></trd>
  <trd lng="fr">grenadier<klr tip="ind"> (arbre)</klr></trd>
  <trd lng="hu">gr&aacute;n&aacute;talmafa</trd>
  <trdgrp lng="nl">
    <trd>granaat<klr tip="ind"> (boom)</klr></trd>,
    <trd>granaatboom</trd>
  </trdgrp>
  <trd lng="pt">rom&atilde;zeira</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="tr">nar a&gbreve;ac&inodot;</trd>
</drv>
</art>
