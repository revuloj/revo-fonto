<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: granat.xml,v 1.34 2023/11/11 11:36:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>granat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="granat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="granat.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <ref tip="malprt" cel="granat.0arbo.BOT"><tld/>arbo</ref>,
      kun multaj internaj rozkoloraj &ccirc;eloj kaj abunda dol&ccirc;a
      suko:
      <ekz>
        ili lasas la bebon Jesuo ludi per <tld/>o
        <fnt>
          <aut>M. Proust, trad. J.-L. Tortel</aut>
          <vrk>Unu amo de Swann</vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE17;&#xE31;&#xE1A;&#xE17;&#xE34;&#xE21; (&#xE1C;&#xE25;&#xE44;&#xE21;&#xE49;)</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;
  <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bo">&t_sa;&t_drengbu;&t_tsheg;&t_-a;&t_ba;&t_rata;&t_shabchu;&t_tsheg;</trd>
  <trd lng="ca">magrana</trd>
  <trd lng="cs">gran&aacute;tov&eacute; jablko</trd>
  <trd lng="de">Granatapfel</trd>
  <trd lng="en">pomegranate</trd>
  <trd lng="es">granada</trd>
  <trd lng="fa">&a_A;&a_n;&a_A;&a_r;</trd>
  <trd lng="fr">grenade<klr tip="ind"> (fruit)</klr></trd>
  <trdgrp lng="hu">
    <trd>gr&aacute;n&aacute;talma</trd>,
    <trd>pomagr&aacute;n&aacute;t</trd>
  </trdgrp>
  <trd lng="id">delima</trd>
  <trd lng="ja">&#x30B6;&#x30AF;&#x30ED;</trd>
  <trdgrp lng="nl">
    <trd>granaat<klr tip="ind"> (fruit)</klr></trd>,
    <trd>granaatappel</trd>
  </trdgrp>
  <trd lng="pl">granat <klr>(owoc)</klr></trd>
  <trd lng="pt">rom&atilde;</trd>
  <trd lng="ro">rodie</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;
  <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">gran&aacute;tov&eacute; jablko</trd>
  <trd lng="tr">nar</trd>
  <trd lng="uk">&#x491;&#x440;&#x430;&#x43D;&#x430;&#x442;&#xA0;(&#x43F;&#x43B;&#x456;&#x434;)</trd>
  <trd lng="zh">&#x77F3;&#x69B4; <pr>sh&#xED;li&#xFA;</pr></trd>
</drv>

<drv mrk="granat.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="granat.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbusto, <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="litrum1.0oj">litrumacoj</ref>
      (<trd lng="la">Punica</trd>):
      <ekz>
        eleganta <tld/>arbo moketas figarbon,
        misforman pro nukdoloro
        <fnt>
          <aut>J. Ribillard</aut>
          <vrk>Vagado sub palmoj</vrk>
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;
    <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_a;&c_je;
    &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">magraner</trd>
  <trd lng="de">Granatapfelbaum</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;
  <ind>&a_A;&a_n;&a_A;&a_r;</ind></trd>
  <trd lng="fr">grenadier<klr tip="ind"> (arbre)</klr></trd>
  <trd lng="hu">gr&aacute;n&aacute;talmafa</trd>
  <trd lng="ja">&#x30B6;&#x30AF;&#x30ED;</trd>
  <trdgrp lng="nl">
    <trd>granaat<klr tip="ind"> (boom)</klr></trd>,
    <trd>granaatboom</trd>
  </trdgrp>
  <trd lng="pl">granat w&lstroke;a&sacute;ciwy <klr>(drzewo)</klr></trd>
  <trd lng="pt">rom&atilde;zeira</trd>
  <trd lng="ro">rodie</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="tr">nar a&gbreve;ac&inodot;</trd>
  <trdgrp lng="uk">
    <trd>&#x491;&#x440;&#x430;&#x43D;&#x430;&#x442;</trd>,
    <trd>&#x491;&#x440;&#x430;&#x43D;&#x430;&#x442;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x491;&#x440;&#x430;&#x43D;&#x430;&#x442;&#x43E;&#x432;&#x435; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x77F3;&#x69B4;&#x6811; <pr>sh&#xED;li&#xFA;sh&#xF9;</pr></trd>,
    <trd>&#x69B4; <pr>li&#xFA;</pr></trd>,
    <trd>&#x5B89;&#x77F3;&#x69B4; <pr>&#x101;nsh&#xED;li&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
