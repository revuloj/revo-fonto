<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>granat</rad>/o
</kap>
<drv mrk="granat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="granat.0ujo"><tld/>ujo</ref>,
      kun multaj internaj rozkoloraj &ccirc;eloj kaj abunda dol&ccirc;a suko.
    </dif>
  </snc>
  <trd lng="de">Granatapfel</trd>
</drv>
<drv mrk="granat.0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Arbusto el familio <ref tip="super"
      cel="granat.0ujacoj"><tld/>ujacoj</ref>
      (<trd lng="la">Punica</trd>).
    </dif>
  </snc>
  <trd lng="de">Granatapfelbaum</trd>
</drv>
<drv mrk="granat.0ujacoj">
  <kap><tld/>ujacoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Punicaceae</trd>),
      kreskantaj en mediteraneaj kaj aziaj regionoj.
    </dif>
  </snc>
  <trd lng="de">Granatapfelgew&auml;chse</trd>
</drv>
</art>
