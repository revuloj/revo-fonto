<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>granat</rad>/o
</kap>
<drv mrk="granat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="granat.0ujo"><tld/>ujo</ref>,
      kun multaj internaj rozkoloraj &ccirc;eloj kaj abunda dol&ccirc;a suko.
    </dif>
  </snc>
  <trd lng="de">Granatapfel</trd>
  <trd lng="fr">grenade<klr tip="ind"> (fruit)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
</drv>
<drv mrk="granat.0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Arbusto el familio <ref tip="super"
      cel="granat1.0oj"><tld/>ujacoj</ref>
      (<trd lng="la">Punica</trd>).
    </dif>
  </snc>
  <trd lng="de">Granatapfelbaum</trd>
  <trd lng="fr">grenadier<klr tip="ind"> (arbre)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>

</art>
