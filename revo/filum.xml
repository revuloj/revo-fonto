<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filum.xml,v 1.24 2025/11/08 22:29:51 revo Exp $">
<kap>
  <rad>filum</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="filum.0o">
  <kap><tld/>o</kap>
  <tezrad fak="ZOO"/>
  <snc mrk="filum.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Scirc;tupo de la zoologia klasifiko,
      super <ref tip="prt" cel="klas.0o.BIO">klaso</ref>
      (<trd lng="la">phylum</trd>).
    </dif>
    <refgrp tip="sin">
      <ref cel="filon.0o.BIO">filono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_p; <klr
  tip="amb">(&c_b;&c_ib;&c_ja;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>f&iacute;lum</trd>,
    <trd>embrancament <klr>(zoologia)</klr></trd>,
    <trd>divisi&oacute; <klr>(bot&agrave;nica)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fylum</trd>,
    <trd>kmen</trd>
  </trdgrp>
  <trd lng="de">Stamm</trd>
  <trd lng="en">phylum</trd>
  <trdgrp lng="fr">
    <trd>embranchement<klr> (biol.)</klr></trd>,
    <trd>phylum</trd>
  </trdgrp>
  <trd lng="hu">t&ouml;rzs<klr tip="ind"> (n&ouml;v&eacute;nyi, &aacute;llati)</klr></trd>
  <trd lng="pl">typ<klr tip="ind"> (biologia)</klr></trd>
  <trd lng="pt">filo</trd>
  <trd lng="ru">&c_t;&c_i;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>fylum</trd>,
    <trd>kme&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_ib;&c_d;&c_d;&c_ib;&c_l;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_t;&c_i;&c_p;</trd>
  </trdgrp>
  <trd lng="zh">&#38272; <pr>m&eacute;n</pr></trd>
</drv>

<drv mrk="filum.sub0o">
  <kap>subfilumo</kap>
  <snc mrk="filum.sub0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="filum.0o.BIO"><tld/>o</ref>,
      grupo de klasoj:
      <ekz>
        la miriapodoj estas sub<tld/>o de la artropodoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Miriapodo">
          Miriapodo</url>, 2010-11-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_t;&c_y;&c_p; <klr
  tip="amb">(&c_b;&c_ib;&c_ja;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>subembrancament <klr>(zoologia)</klr></trd>,
    <trd>subdivisi&oacute; <klr>(bot&agrave;nica)</klr></trd>
  </trdgrp>
  <trd lng="en">sybphylum</trd>
  <trd lng="fr">sous-embranchement</trd>
  <trd lng="pl">podtyp</trd>
</drv>

<drv mrk="filum.super0o">
  <kap>superfilumo</kap>
  <snc mrk="filum.super0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Grupo de <ref tip="malprt" cel="filum.0o.BIO"><tld/>oj</ref>,
      parto de regno:
      <ekz>
        la super<tld/>o de <klr>...</klr> vaskulaj plantoj
        <klr>...</klr> havas proksimume 250 000 speciojn
        <fnt>
          <aut>K. Lendon</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/per-esperanto-bazascienco/message/538">
          La Siluria Periodo</url></vrk>,
          <lok>per-esperanto-bazascienco, 2003-10-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_t;&c_y;&c_p; <klr
  tip="amb">(&c_b;&c_ib;&c_ja;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="en">superphylum</trd>
  <trd lng="fr">superembranchement</trd>
  <trd lng="pl">nadtyp</trd>
</drv>
</art>
