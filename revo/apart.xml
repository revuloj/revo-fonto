<drv mrk="apart.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="apart.0ajxo.rezervate">
    <dif>
      Io <tld/>a, disigita de la tuto:
      <ekz>
        <tld/>a&jcirc;ojn faru en la arkeo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>a&jcirc;o <klr>[de la regimento]</klr> konsistis el dudek kvar mil
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 27:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiujn]</klr> ideojn <klr>[&#8230;]</klr> li dividis en 3 &ccirc;efajn <tld/>a&jcirc;ojn kaj
        poste ankora&ubreve; en klasojn kaj subklasojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda malliberejo, en kiu ekzistas multe da <tld/>a&jcirc;oj, &ccirc;eloj kaj kavoj
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Sara</nom> supreniris unu &scirc;tuparon kaj venis supre en la <tld/>a&jcirc;on de la virinoj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aparta.0o">apartamento</ref>,
      <ref cel="cxambr.0o">&ccirc;ambro</ref>,
      <ref cel="divid.0ajxo">divida&jcirc;o</ref>,
      <ref cel="fak.0o">fako</ref>,
      <ref cel="part.0o">parto</ref>,
      <ref cel="sekci.0o">sekcio</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Abteil</trd>,
      <trd>Unterteilung</trd>,
      <trd>Separee</trd>
    </trdgrp>
    <trd lng="es">lugar <ind>reservado</ind></trd>
    <trd lng="sk">vyhraden&eacute; <ind>miesto</ind></trd>
    <trd lng="ru">&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="apart.0ajxo.proprajxo">
    <dif>
      Karakteriza <ref cel="propr.0ajxo.KOMUNE">propra&jcirc;o</ref>, speciala&jcirc;o:
      <ekz>
        la diversaj homaj gentoj povus pace kaj frate interkomuniki&gcirc;adi, ne altrudante
        al si reciproke siajn gentajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ofende sin amuzi pro eksteraj stranga&jcirc;oj kaj <tld/>a&jcirc;oj <klr>[de aliaj]</klr>
        homoj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj de la diversaj juraj sistemoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto ebligas adekvatan esprimadon de &ccirc;iuj <tld/>a&jcirc;oj, entenataj en &ccirc;iu
        unuopa nacia lingvo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karakt1.0o">karakteriza&jcirc;o</ref>,
      <ref cel="signatur.0o">signaturo</ref>,
      <ref cel="specif.0o.ajxo">specifa&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>characteristic</trd>,
      <trd>pecularity</trd>,
      <trd>particularity</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>caracter&iacute;stica</trd>,
      <trd>peculiaridad</trd>,
      <trd>particularidad</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><ind>ciri</ind> khas</trd>,
      <trd>karakteristik</trd>
    </trdgrp>
    <trd lng="be">&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>Besonderheit</trd>,
      <trd>Charakteristikum</trd>,
      <trd>Spezialit&auml;t</trd>
    </trdgrp>
    <trd lng="fr">particularit&eacute;</trd>
    <trd lng="ru">&c_o;&c_s;&c_o;&c_b;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    <trd lng="sk">zvl&aacute;&scaron;tnos&tcaron;</trd>
  </snc>
  <trd lng="he">&dalet;&bet;&resh; &nun;&pe;&resh;&dalet;</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_s;&c_o;&c_b;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;&nbsp;(&c_shch;&c_o;&c_s;&c_mol; &c_o;&c_s;&c_o;&c_b;&c_l;&c_i;&c_v;&c_je;)</trd>,
