<drv mrk="apart.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="apart.0ajxo.rezervate">
    <dif>
      <tld lit="A"/>a rezervata loko:
      <ekz>
        &scirc;i venis supre en la <tld/>a&jcirc;on
        de la virinoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="es">lugar <ind>reservado</ind></trd>
    <trd lng="sk">vyhraden&eacute; <ind>miesto</ind></trd>
    <trd lng="ru">&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="apart.0ajxo.proprajxo">
    <dif>
      Karakteriza propra&jcirc;o, speciala&jcirc;o:
      <ekz>
        tio estas <tld/>a&jcirc;o de la slavaj lingvoj.
      </ekz>
    <trdgrp lng="en">
      <trd>characteristic</trd>,
      <trd>pecularity</trd>,
      <trd>particularity</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>caracter&iacute;stica</trd>,
      <trd>peculiaridad</trd>,
      <trd>particularidad</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><ind>ciri</ind> khas</trd>,
      <trd>karakteristik</trd>
    </trdgrp>
    </dif>
    <trd lng="be">&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="de">Besonderheit</trd>
    <trd lng="fr">particularit&eacute;</trd>
    <trd lng="ru">&c_o;&c_s;&c_o;&c_b;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    <trd lng="sk">zvl&aacute;&scaron;tnos&tcaron;</trd>
