  <trd lng="af">afsonderlike</trd>
  <trd lng="am">&#x1218;&#x1208;&#x12E8;&#x1275;</trd>
  <trd lng="ar">&a_m;&a_n;&a_f;&a_S;&a_l;&a_t_marbuta;</trd>
