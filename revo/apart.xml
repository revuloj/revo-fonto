    <trdgrp lng="th">
      <trd>&#3650;&#3604;&#3618;&#3648;&#3593;&#3614;&#3634;&#3632;</trd>,
      <trd>&#3649;&#3618;&#3585;&#3629;&#3629;&#3585;&#3592;&#3634;&#3585;&#3585;&#3633;&#3609;</trd>
    </trdgrp>
  </snc>
  <snc mrk="apart.0a.speciala">
    <dif>
      <ref cel="ordina.ne0a.KOMUNE">Neordinara</ref>, 
      <ref cel="star.el0a.eminenta">elstara</ref>,
      <ref cel="rezerv.0i">rezervi</ref>ta nur por certa okazo:
      <ekz>
        per <tld/>a favoro oni elpelis min malhonore el la limoj de la lando
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        malnovaj melodioj eksonis, <tld/>aj taktoj, bildoj, ravantaj bildoj disruli&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis <tld/>an talenton kaj inklinon por eltrovi ka&scirc;itajn krimojn
        <fnt>
          <bib>ChV</bib>
          <lok>14</lok>
        </fnt>;
      </ekz>
      <ekz>
        por &ccirc;iu horo de la tago li havis <tld/>an surtuton
        <fnt><bib>Fab1</bib>, <lok>La novaj vestoj de la
        re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        mi ne akceptis <klr>[&ccirc;ar kiel vidvino]</klr> 
        &scirc;i certe postulus por si iajn <tld/>ajn konsiderojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ervin</nom> ludis <tld/>an rolon en &scirc;ia vivo
        <fnt>
          <bib>ChC</bib>
          <lok>28</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distin.0iga">distingiga</ref>,
      <ref cel="ordina.ekster0a">eksterordinara</ref>,
      <ref cel="banal.mal0a">malbanala</ref>,
      <ref cel="sxablo.mal0a">mal&scirc;ablona</ref>,
      <ref cel="origin1.0a">originala</ref>,
      <ref cel="partikular.0a">partikulara</ref>,
      <ref cel="spec.sia0a">siaspeca</ref>,
      <ref cel="specif.0a.aparta">specifa<sncref/></ref>,
      <ref cel="unik.0a.eksterordinara">unika</ref>
    </refgrp>
    <rim>
      Pri sinonimoj vidu sub <ref cel="specia.0a.SIN">speciala</ref>.
    </rim>
    <trd lng="be">&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_y;</trd>
    <trdgrp lng="de">
      <trd>besonders <klr tip="amb">(adj.)</klr></trd>,
      <trd>von <klr>(ganz)</klr> <ind>eigener Art</ind></trd>,
      <trd>ungew&ouml;hnlich</trd>,
      <trd>au&szlig;ergew&ouml;hnlich</trd>,
      <trd>apart</trd>
