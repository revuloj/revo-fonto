<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaliforni.xml,v 1.37 2024/12/04 08:38:01 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Kaliforni</rad>/o <fnt><bib>FK</bib></fnt>

</kap>
<drv mrk="kaliforni.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <mlg>US&#8209;CA</mlg>
  <snc mrk="kaliforni.0o.GEOG">
    <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> de
      <ref tip="super" cel="uson.U0o">Usono</ref>, situanta &ccirc;e la
      <ref tip="super" cel="pacifik.P0o">Pacifika</ref>
      marbordo de
      <ref tip="super" cel="amerik.Nord0o">Nordameriko</ref> kun
      &ccirc;efurbo Sakramento, kaj plej grava urbo <ref cel="losangxeles.0o">Losan&gcirc;eleso</ref>:
      <ekz>
        kiel oni scias, <tld lit="K"/>o apartenas al la lokoj tre ri&ccirc;aj
        kiel per mineraloj, tiel anka&ubreve; per produkta&jcirc;oj de la regno
        kreska&jcirc;a
         <fnt>
           <aut>L. L. Zamenhof</aut>
           <vrk>Fundamenta Krestomatio</vrk>
         </fnt>;
       </ekz>
      <ekz>
        39,4 km longis la tiel nomata &leftquot;Kuranta barilo&rightquot;, kiu ekestis en 1994 en <tld lit="K"/>o
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007013.php">Ekspozicio de famaj artistoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la universitato de <tld lit="K"/>o en Berkelio
        <fnt>
          <bib>Monato</bib>
          <aut>Barry Friedman</aut>
          <vrk><url ref="&Monato;/2008/009591.php">Ohiaj &scirc;tataj Hipoka&scirc;tanoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        el 50 usonaj &scirc;tatoj, <tld lit="K"/>o kaj Teksaso enhavas la plej grandan nombron
        de prizonuloj atendantaj mortopunon
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2011/010384.php">Barbara minoritato</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_ja;</trd>
    <trd lng="ca">Calif&ograve;rnia</trd>
    <trd lng="de">Kalifornien</trd>
    <trd lng="id">Kalifornia</trd>
    <trd lng="it">California</trd>
    <trd lng="en">California</trd>
    <trd lng="pl">Kalifornia</trd>
    <trd lng="sv">Kalifornien</trd>
    <trdgrp lng="zh">
    <trd>&#21152;&#21033;&#31119;&#23612;&#20122; <pr>Ji&amacron;l&igrave;f&uacute;n&iacute;y&agrave;</pr></trd>,
    <trd>&#21152;&#21033;&#31119;&#23612;&#20122; <pr>ji&amacron;l&igrave;f&uacute;n&iacute;y&agrave;</pr></trd>,
    <trd>&#21152;&#21033;&#31119;&#23612;&#20122;&#24030; <pr>Ji&amacron;l&igrave;f&uacute;n&iacute;y&agrave;zh&omacron;u</pr></trd>,
    <trd>&#21152;&#24030; <pr>Ji&amacron;zh&omacron;u</pr></trd>,
    <trd>&#21152;&#24030; <pr>ji&amacron;zh&omacron;u</pr></trd>
  </trdgrp>
</snc>
  <ref tip="hom" cel="kalifornium.0o">kaliforniumo, kalifornio (elemento)</ref>
  <trd lng="ja">&#12459;&#12522;&#12501;&#12457;&#12523;&#12491;&#12450;&#24030;  <pr>&#12363;&#12426;&#12405;&#12361;&#12427;&#12395;&#12354;&#12375;&#12421;&#12358;</pr></trd>
</drv>

<drv mrk="kaliforni.Malsupra_0o">
  <kap>
    <ofc>10</ofc>Malsupra <tld/>o,
    <var><kap>Malalta <tld/>o</kap></var>
  </kap>
  <mlg>MX&#8209;BCN</mlg>
  <snc>
    <dif>
      Meksika sub&scirc;tato en la nordo de la <tld/>a Duoninsulo:
      <ekz>
        la Internacia Akademio de la Sciencoj-RSM-Meksikio kunlabore kun Universitato
        A&ubreve;tonoma de Malalta Kalifornio (UABC) invitas al la dua Internacia Kongreso
        de la 3 Kalifornioj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_ib;&c_zh;&c_n;&c_ja;&c_ja; <ind>&c_K;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_ja;</ind></trd>
  <trdgrp lng="de">
    <trd>Baja California</trd>,
    <trd>Niederkalifornien</trd>
  </trdgrp>
  <trd lng="en">Baja California</trd>
  <trd lng="fr">Basse-Californie</trd>
</drv>

<drv mrk="kaliforni.Suda_Malsupra_0o">
  <kap>
    <ofc>10</ofc>Suda Malsupra <tld/>o
  </kap>
  <mlg>MX&#8209;BCS</mlg>
  <snc>
    <dif>
      Meksika sub&scirc;tato en la sudo de la <tld/>a Duoninsulo.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_N;&c_ib;&c_zh;&c_n;&c_ja;&c_ja; <ind>&c_K;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_ja;</ind></trd>
  <trdgrp lng="de">
    <trd>Baja California Sur</trd>,
    <trd>S&uuml;dniederkalifornien</trd>
  </trdgrp>
  <trd lng="en">Baja California Sur</trd>
  <trd lng="fr">Basse-Californie du Sud</trd>
</drv>

</art>
