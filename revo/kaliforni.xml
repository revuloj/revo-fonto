<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaliforni</rad>/o
</kap>
<drv mrk="kaliforni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 98, atompezo 251,0796, simbolo Cf. <ref tip="super"
     cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
     estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Californium</trd>
  <trd lng="en">californium</trd>
  <trd lng="hu">kalifornium</trd>
  <trd lng="nl">californium</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
</drv>
</art>

