<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaliforni.xml,v 1.25 2020/06/01 15:10:38 revo Exp $">
<kap>
  <rad>Kaliforni</rad>/o <fnt><bib>FK</bib></fnt>

</kap>
<drv mrk="kaliforni.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaliforni.0o.GEOG">
    <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> de
      <ref tip="super" cel="uson.U0o">Usono</ref>, situanta &ccirc;e la
      <ref tip="super" cel="pacifik.P0o">Pacifika</ref> 
      marbordo de 
      <ref tip="super" cel="amerik.Nord0o">Nordameriko</ref> kun
      &ccirc;efurbo Sakramento, kaj plej grava urbo <ref cel="losangxeles.0o">Losan&gcirc;eleso</ref>:
      <ekz>
        kiel oni scias, <tld lit="K"/>o apartenas al la lokoj tre ri&ccirc;aj
        kiel per mineraloj, tiel anka&ubreve; per produkta&jcirc;oj de la regno
        kreska&jcirc;a
         <fnt>
           <aut>L. L. Zamenhof</aut>
           <vrk>Fundamenta Krestomatio</vrk>
         </fnt>;
       </ekz>
      <ekz>
        39,4 km longis la tiel nomata &leftquot;Kuranta barilo&rightquot;, kiu ekestis en 1994 en <tld
        lit="K"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007013.html"
              >A&ubreve;strio: Ekspozicio de famaj artistoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la universitato de <tld lit="K"/>o en Berkelio
        <fnt>
          <bib>Monato</bib>
          <aut>Barry Friedman</aut>
          <vrk><url ref="https://www.monato.be/2008/009591.php"
              >Ohiaj &scirc;tataj Hipoka&scirc;tanoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        el 50 usonaj &scirc;tatoj, <tld lit="K"/>o kaj Teksaso enhavas la plej grandan nombron
        de prizonuloj atendantaj mortopunon
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="https://www.monato.be/2011/010384.php"
              >Barbara minoritato</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Calif&ograve;rnia</trd>
    <trd lng="de">Kalifornien</trd>
    <trd lng="id">Kalifornia</trd>
    <trd lng="it">California</trd>
    <trd lng="en">California</trd>
    <trd lng="pl">Kalifornia</trd>
    <trd lng="sv">Kalifornien</trd>
  </snc>
</drv>

<drv mrk="kaliforni.k0o">
  <kap>
    <tld lit="k"/>o <fnt><bib>PIV1</bib></fnt>
  </kap>
  <snc mrk="kaliforni.0o.KEM">    
    <mlg>Cf</mlg>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="kalifornium.0o">kaliforniumo</ref>
    </dif>
    <trd
  lng="be">&c_k;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_j;</trd>
    <trd lng="br">kaliforniom</trd>
    <trd lng="ca">californi</trd>
    <trd lng="cs">kalifornium</trd>
    <trd lng="de">Californium</trd>
    <trd lng="en">californium</trd>
    <trd lng="es">californio</trd>
    <trd lng="fr">californium</trd>
    <trd lng="hu">kalifornium</trd>
    <trd lng="id">kalifornium</trd>
    <trd lng="it">californio</trd>
    <trd lng="la">Californium</trd>
    <trd lng="nl">californium</trd>
    <trd lng="pl">kaliforn</trd>
    <trd lng="pt">calif&oacute;rnio</trd>
    <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
    <trd lng="sk">kalifornium</trd>
    <trd lng="sv">californium</trd>
  </snc>
</drv>


</art>
