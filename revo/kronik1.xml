    <trd lng="de">Chronik</trd>
  </snc>
  <snc>
    <dif>
      Parto de gazeto dedi&ccirc;ita al difinita speco da nova&jcirc;oj:
      <ekz>
        politika, teatra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Tagesberichterstattung</trd>,
     <trd>Rundschau</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">kr&ouml;nika</trd>
</drv>
