    <kap>
      <rad>Okcitani</rad>/o
    </kap>

<drv mrk="okcitani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref> norde de Pireneoj,
      kie tradicie lo&gcirc;as okcitanoj:
      <ekz>
        konati&gcirc;i kun muzikaj tradicioj de Katalunio kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2005/008811.php">Katalunio, Okcitanio Kaj Tiel Plu</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        reprezentantoj de kulturaj organiza&jcirc;oj de la tuta <tld/>o, kunvokitaj de la
        federacio de okcitanlingvaj lernejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ferri&ograve;l Macip i Bonet</aut>
          <vrk><url ref="&Monato;/2011/010257.php">Ni konservos la afi&scirc;ojn!</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Okzitanien</trd>
    <trd lng="en">Occitania</trd>
    <trd lng="fr">Occitanie</trd>
  </snc>
  <trd lng="be">&c_A;&c_k;&c_s;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#22885;&#x514B;&#x897F;&#22612;&#x5C3C; <pr>&#xC0;ok&#xE8;x&#x12B;t&#x1CE;n&#xED;</pr></trd>,
    <trd>&#22887;&#x514B;&#x897F;&#22612;&#x5C3C; <pr>&#xC0;ok&#xE8;x&#x12B;t&#x1CE;n&#xED;</pr></trd>
  </trdgrp>
</drv>

