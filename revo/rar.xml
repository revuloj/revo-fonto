  </trdgrp>
  <trd lng="pl">rarytas</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_d;&#1082;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31232;&#32597; <pr>x&#299;han</pr></trd>,
    <trd>&#23588;&#29289; <pr>y&#243;uw&#249;</pr></trd>,
