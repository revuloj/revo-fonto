<drv mrk="rar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="rar.0ajxo.valorajxo">
    <dif>
      <ref tip="super" cel="valor.0ajxo">Valora&jcirc;o</ref> malabunda, malfacile akirebla a&ubreve; trovebla:
      <ekz>
        amba&ubreve; po&scirc;tmarkoj jam el&ccirc;erpi&gcirc;is kaj i&gcirc;is <tld/>a&jcirc;oj intertempe
        <fnt>
          <aut>Bert Boon</aut>
          <vrk><url ref="&Monato;/2005/008538.php">Pli ol ri-mark-inde</url></vrk>
          <bib>Monato</bib>
          <lok>2005/02, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        jes, tio estas <tld/>ega&jcirc;o, la obstetriko mencias nur sep a&ubreve; ok tiajn kazojn de
        vira gravedeco
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        la biblioteko enhavas 30&#8239;000 <tld/>a&jcirc;ojn el entute 300&#8239;000 posedataj libroj
        <fnt>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html">Impresoj pri Portugalio</url></vrk>
          <bib>Monato</bib>
          <lok>2003/07, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; nur 10&#8239;-&#8239;15 jaroj tiaspeca literaturo
        estis <tld/>a&jcirc;o en la rusa
        <fnt>
        <aut>Nikolao Gudskov</aut>:
          <vrk><url ref="&Monato;/1999/monato199908.pdf#page=24">Por efektivigo de la homaj rajtoj
          kaj evoluo de 
          Esperanto</url> (Leah Levin, Homaj rajtoj)</vrk>, 
          <bib>Monato</bib>
          <lok>1999/08, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="unik.0a">unika</ref>&jcirc;o
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_y;&c_t;&c_e;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
