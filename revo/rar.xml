<drv mrk="rar.0e">
  <kap><tld/>e</kap>
  <snc mrk="rar.0e.malofte">
    <dif>
      <ref tip="sin" cel="oft.mal0e">Malofte</ref>, <ref cel="abund.mal0a">malabunde</ref>:
      <ekz>
        <tld/>e emas kune kreski en unu persono <klr>[&#8230;]</klr> kapablo facile lerni,
        memori, esti lerta, rapide reagi
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>&ccirc;u vi trinkas alkoholon?</ctl> &ndash; <ctl>tre
        <tld/>e</ctl>
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>e bela &leftquot;Banejo de Mirzo&rightquot;, konstruita de <nom>Ulugbek</nom>, funkciis dum pli ol
        500 jaroj
        <fnt>
          <aut>Vitalij kaj Anatolij Ionesov</aut>
          <vrk><url ref="&Monato;/2010/009996.php">Mirzo Ulugbek: granda re&gcirc;o de
          astronomio</url></vrk>
          <bib>Monato</bib>
          <lok>2010/07, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apenau.0">apena&ubreve;</ref>,
      <ref cel="escept.0e">escepte</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_r;&c_e;&c_d;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ojedin&ecaron;le</trd>,
    <trd>z&rcaron;&iacute;dka</trd>
  </trdgrp>
