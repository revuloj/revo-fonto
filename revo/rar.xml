    <trd>&#24076;&#23569;&#12394;  <pr>&#12414;&#12428;&#12377;&#12367;&#12394;</pr></trd>,
    <trd>&#29645;&#12375;&#12356;  <pr>&#12417;&#12378;&#12425;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">zeldzaam<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pt">raro</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_i;&c_t;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>
