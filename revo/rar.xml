<drv mrk="rar.0a">
  <kap><tld/>a</kap>
  <snc mrk="rar.0a.valora">
    <dif>
      <ref tip="super" cel="valor.alt0a">Altvalora</ref> pro sia malabundo:
      <ekz>
        plej <tld/>a po&scirc;tmarko
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/007065.php">VIPE 2000, a&ubreve; &ccirc;u la po&scirc;tmarkoj
          malaperos?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 18</lok>
          </fnt>;
      </ekz>
      <ekz>
        li posedis la <tld/>an econ ne sentigi al subulo lian subecon
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        vasta lando kun almena&ubreve; 25 urboj kaj <tld/>aj bela&jcirc;oj
        <fnt><aut>Bardhyl Selimi</aut>: 
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas plisita per <tld/>a ma&scirc;ino, kiu helpas la laboriston dum la plenumo de
        tiu malfacila tasko
        <fnt>
          <aut>Maxime Lechien</aut>
          <vrk><url ref="&Monato;/2000/006955.php">Fama karnavalo en Binche</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ekzistanta man&ccirc;ura estas rigardata de la fakuloj kiel <tld/>a primara
        materialo por esploro, vivanta fosilio
        <fnt>
          <aut>Sabeo Fu/Yamasaki Seik&ocirc;</aut>
            <vrk><url ref="&Monato;/2001/007025.php">Formortanta lingvo de malplimulto en
            &Ccirc;inio</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 22</lok>
          </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>ritka</trd>,
      <trd>becses</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rzadki</trd>,
      <trd>unikalny</trd>
    </trdgrp>
  </snc>
  
  <snc mrk="rar.0a.malofta">
    <dif>
      Malofte trovebla:
      <ekz>
        li sidas en rulse&gcirc;o hodia&ubreve;; povas mem iri, sed devas ne tro
        vigli pro iu <tld/>a sindromo
        <fnt>
        <aut>Anne Tilmont</aut>: 
        <vrk><url ref="&Monato;/1998/005932.php">Arthur
        C. Clarke, kosmopolita civitano kun okulo al la universo</url></vrk>
        <bib>Monato</bib>
        <lok>1998/04, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la traduko ne uzas neologismojn, kaj obskuraj vortoj <tld/>as
        <fnt>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2007/009029.php">Irigu vin tra intrigo vintra</url></vrk>
          <bib>Monato</bib>
          <lok>2007/02, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">ritk&aacute;n el&odblac;fordul&oacute;</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="ordina.ekster0a">eksterordinara</ref>,
    <ref cel="sporad.0a">sporada</ref>,
    <ref cel="unik.0a.eksterordinara">unika</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_d;&c_k;&c_ib;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_y;&c_t;&c_e;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF40;&#xF7C;&#xF53;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>rar</trd>,
    <trd>selten</trd>,
    <trd>kostbar</trd>
  </trdgrp>
  <trd lng="en">rare</trd>
  <trdgrp lng="es">
    <trd>rara</trd>,
    <trd>infrecuente</trd>
  </trdgrp>
  <trd lng="fr">rare</trd>
  <trdgrp lng="ja">
    <trd>&#x307E;&#x308C;&#x306A;</trd>,
    <trd>&#x5E0C;&#x5C11;&#x306A;  <pr>&#x307E;&#x308C;&#x3059;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x73CD;&#x3057;&#x3044;  <pr>&#x3081;&#x305A;&#x3089;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">zeldzaam<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pt">raro</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_i;&c_t;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vz&aacute;cny</trd>,
    <trd>zriedkav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nev&iacute;dan&yacute;</trd>,
    <trd>&rcaron;&iacute;dk&yacute; v&yacute;skytem</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_d;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_d;&#x43A;&c_ib;&c_s;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F55;&#x898B; <pr>h&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x7F55;&#x89C1; <pr>h&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x7A00;&#x5C11; <pr>x&#x12B;sh&#x1CE;o</pr></trd>,
    <trd>&#x7A00;&#x7F55; <pr>x&#x12B;han</pr></trd>,
    <trd>&#x7F55; <pr>h&#x1CE;n</pr></trd>,
    <trd>&#x96BE;&#x5F97;&#x4E00;&#x89C1; <pr>n&#xE1;nd&#xE9;y&#x12B;ji&#xE0;n</pr></trd>,
    <trd>&#x96E3;&#x5F97;&#x4E00;&#x898B; <pr>n&#xE1;nd&#xE9;y&#x12B;ji&#xE0;n</pr></trd>,
    <trd>&#x96BE;&#x5F97; <pr>n&#xE1;nd&#xE9;</pr></trd>,
    <trd>&#x96E3;&#x5F97; <pr>n&#xE1;nd&#xE9;</pr></trd>,
    <trd>&#x73AE; <pr>w&#x11B;i</pr></trd>,
    <trd>&#x744B; <pr>w&#x11B;i</pr></trd>,
    <trd>&#x7A00;&#x6709; <pr>x&#x12B;y&#x1D2;u</pr></trd>,
    <trd>&#x540D;&#x8CB4; <pr>m&#xED;nggu&#xEC;</pr></trd>,
    <trd>&#x540D;&#x8D35; <pr>m&#xED;nggu&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rar.0e">
  <kap><tld/>e</kap>
  <snc mrk="rar.0e.malofte">
    <dif>
      <ref tip="sin" cel="oft.mal0e">Malofte</ref>, <ref cel="abund.mal0a">malabunde</ref>:
      <ekz>
        <tld/>e emas kune kreski en unu persono <klr>[&#x2026;]</klr> kapablo facile lerni,
        memori, esti lerta, rapide reagi
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>&ccirc;u vi trinkas alkoholon?</ctl> &ndash; <ctl>tre
        <tld/>e</ctl>
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>e bela &leftquot;Banejo de Mirzo&rightquot;, konstruita de <nom>Ulugbek</nom>, funkciis dum pli ol
        500 jaroj
        <fnt>
          <aut>Vitalij kaj Anatolij Ionesov</aut>
          <vrk><url ref="&Monato;/2010/009996.php">Mirzo Ulugbek: granda re&gcirc;o de
          astronomio</url></vrk>
          <bib>Monato</bib>
          <lok>2010/07, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apenau.0">apena&ubreve;</ref>,
      <ref cel="escept.0e">escepte</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_r;&c_e;&c_d;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ojedin&ecaron;le</trd>,
    <trd>z&rcaron;&iacute;dka</trd>
  </trdgrp>
  <trd lng="de">selten</trd>
  <trd lng="en">rarely</trd>
  <trd lng="es">raramente</trd>
  <trdgrp lng="fr">
    <trd>rarement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ritk&aacute;n</trd>,
    <trd>elv&eacute;tve</trd>
  </trdgrp>
  <trd lng="ja">&#x307E;&#x308C;&#x306B;</trd>
  <trd lng="nl">zeldzaam<klr tip="ind"> (bw.)</klr></trd>
  <trd lng="pl">rzadko</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_d;&c_k;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_r;&c_je;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ojedinele</trd>,
    <trd>zriedka</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x96BE;&#x5F97;&#x4E00;&#x89C1; <pr>n&#xE1;nd&#xE9;y&#x12B;ji&#xE0;n</pr></trd>,
    <trd>&#x96E3;&#x5F97;&#x4E00;&#x898B; <pr>n&#xE1;nd&#xE9;y&#x12B;ji&#xE0;n</pr></trd>,
    <trd>&#x9BAE;&#x5C11; <pr>xi&#x1CE;nsh&#x1CE;o</pr></trd>,
    <trd>&#x9C9C;&#x5C11; <pr>xi&#x1CE;nsh&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="rar.0ajxo.valorajxo">
    <dif>
      <ref tip="super" cel="valor.0ajxo">Valora&jcirc;o</ref> malabunda, malfacile akirebla a&ubreve; trovebla:
      <ekz>
        amba&ubreve; po&scirc;tmarkoj jam el&ccirc;erpi&gcirc;is kaj i&gcirc;is <tld/>a&jcirc;oj intertempe
        <fnt>
          <aut>Bert Boon</aut>
          <vrk><url ref="&Monato;/2005/008538.php">Pli ol ri-mark-inde</url></vrk>
          <bib>Monato</bib>
          <lok>2005/02, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        jes, tio estas <tld/>ega&jcirc;o, la obstetriko mencias nur sep a&ubreve; ok tiajn kazojn de
        vira gravedeco
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        la biblioteko enhavas 30&#x202F;000 <tld/>a&jcirc;ojn el entute 300&#x202F;000 posedataj libroj
        <fnt>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html">Impresoj pri Portugalio</url></vrk>
          <bib>Monato</bib>
          <lok>2003/07, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; nur 10&#x202F;-&#x202F;15 jaroj tiaspeca literaturo
        estis <tld/>a&jcirc;o en la rusa
        <fnt>
        <aut>Nikolao Gudskov</aut>:
          <vrk><url ref="&Monato;/1999/monato199908.pdf#page=24">Por efektivigo de la homaj rajtoj
          kaj evoluo de 
          Esperanto</url> (Leah Levin, Homaj rajtoj)</vrk>, 
          <bib>Monato</bib>
          <lok>1999/08, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="unik.0a">unika</ref>&jcirc;o
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_y;&c_t;&c_e;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Seltenheit</trd>,
    <trd>seltenes St&uuml;ck</trd>,
    <trd>Rarit&auml;t</trd>
  </trdgrp>
  <trd lng="en">rarity</trd>
  <trd lng="es">rareza</trd>
  <trdgrp lng="fr">
    <trd>chose <ind>rare</ind></trd>,
    <trd>raret&eacute;<klr> (chose rare)</klr></trd>
  </trdgrp>
  <trd lng="hu">ritkas&aacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x5E0C;&#x5C11;&#x54C1;  <pr>&#x304D;&#x3057;&#x3087;&#x3046;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x73CD;&#x54C1;  <pr>&#x3061;&#x3093;&#x3074;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rariteit</trd>,
    <trd>zeldzaamheid</trd>,
    <trd>iets <ind>zeldzaam</ind>s</trd>
  </trdgrp>
  <trd lng="pl">rarytas</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_d;&#x43A;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_r;&c_a;&c_r;&c_i;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A00;&#x7F55; <pr>x&#x12B;han</pr></trd>,
    <trd>&#x5C24;&#x7269; <pr>y&#xF3;uw&#xF9;</pr></trd>,
    <trd>&#x7470;&#x5B9D; <pr>gu&#x12B;b&#x1CE;o</pr></trd>,
    <trd>&#x7470;&#x5BF6; <pr>gu&#x12B;b&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
