<art mrk="$Id: bu.xml,v 1.21 2023/10/14 08:39:48 revo Exp $">
<kap><rad>bu</rad>/o</kap>

<drv mrk="bu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Flosanta ilo alfiksita &ccirc;e la fundo, en maro
      a&ubreve; rivero, kaj uzata por signali ian dan&gcirc;eran lokon a&ubreve;
      por alligi &scirc;ipon;
      <ref tip="sin" cel="barel.nagx0o">na&gcirc;barelo</ref>:
      <ekz>
        la kutro iris proksime de arbustoj, kiuj prikreskis la moleon ekde la kvina
        <tld/>o
        <fnt>
          <aut>Aleksandro Grin, trad. Jurij Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora_17.html">La ora &ccirc;eno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni aliris kun la duonmallevita &ccirc;efvelo, kaj &leftquot;Tufanaso&rightquot; &jcirc;etis la ankron apud fera
        <tld/>o, en kies ringon estis haste traigita la po&ubreve;pa ha&ubreve;sero
        <fnt>
          <aut>Aleksandro Grin, trad. Jurij Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/kuranta_21.html">Kuranta sur ondoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis trovita, el&ccirc;erpita kaj ekstreme malvarma, dum &scirc;i alkro&ccirc;i&gcirc;is al <tld/>o
        meze de maro
        <fnt>
          <vrk><url ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=buo">Delfeno ne volas halti ludon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_n;&c_d;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">boia</trd>
  <trd lng="cs">b&oacute;je</trd>
  <trd lng="de">Boje</trd>
  <trd lng="en">buoy</trd>
  <trd lng="es">boya</trd>
  <trd lng="fr">bou&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>b&oacute;ja</trd>,
    <trd>&uacute;sz&oacute;</trd>
  </trdgrp>
  <trd lng="it">boa</trd>
  <trdgrp lng="ja">
    <trd>&#x30D6;&#x30A4;</trd>,
    <trd>&#x6D6E;&#x6A19;  <pr>&#x3075;&#x3072;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boei</trd>,
    <trd>baken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>boja</trd>,
    <trd>p&lstroke;awa</trd>,
    <trd>baka</trd>
  </trdgrp>
  <trd lng="ro">baliz&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&oacute;ja</trd>,
    <trd>b&oacute;ja</trd>,
    <trd>plav&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x439;</trd>,
    <trd>&#x431;&#x430;&#x43A;&#x435;&#x43D;</trd>
  </trdgrp>
</drv>
