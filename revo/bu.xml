    <trd>baken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
