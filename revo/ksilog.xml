<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ksilograf</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="ksilog.0i">
  <kap>
    <tld/>i
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Fari desegnojn sur ligno per speciala &scirc;tala instrumento, por
      reprodukti ilin per presado.
    </dif>
  </snc>
  <trd lng="de">(k&uuml;nstlerisch in) <ind>Holz schneiden</ind></trd>
</drv>

<drv mrk="ksilog.0isto">
  <kap>
    <tld/>(ist)o
  </kap>
  <snc>
    <dif>
      Homo, kiu <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Holzschneider</trd>,
     <trd>Holzschnittk&uuml;nstler</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r; &c_p;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_u;</trd>
  </trdgrp>
</drv>
</art>

