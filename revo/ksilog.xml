<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ksilograf</rad>/i
  <fnt>Z</fnt>
</kap>

<drv mrk="ksilog.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ksilog.0i.TIP">
    <dif>
      Prese reprodukti bildon a&ubreve; skriba&jcirc;on reliefe faritan en ligna
      tabulo.
    </dif>
  </snc>
  <trd lng="cs">vyrývat do dřeva</trd>
  <trd lng="de">(k&uuml;nstlerisch in) <ind>Holz schneiden</ind></trd>
  <trd lng="fr">xylographier</trd>
  <trd lng="hu">fametszetet k&eacute;sz&iacute;t</trd>
  <trd lng="nl">xylografi&euml;ren</trd>
  <trdgrp lng="pt">
    <trd>xilogravar</trd>,
    <trd>xilografar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">vyrývať do dreva</trd>
</drv>

<drv mrk="ksilog.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ksilog.0ajxo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Bilda a&ubreve; teksta verko, kiun oni faris per <ref tip="vid"
      cel="ksilog1.0o.TIP">ksilografio</ref>.
    </dif>
  </snc>
  <trd lng="de">Holzschnitt</trd>
  <trd lng="fr">xylographie<klr tip="amb"> (&oelig;uvre)</klr></trd>
  <trd lng="nl">xylografie<klr tip="ind"> (het gedrukte)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a; &c_p;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="ksilog.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ksilog.0isto.TIP">
    <dif>
      Tiu, kiu profesie preparas <tld/>a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="cs">dřevorytec</trd>
  <trdgrp lng="de">
    <trd>Holzschneider</trd>,
    <trd>Holzschnittk&uuml;nstler</trd>
  </trdgrp>
  <trd lng="fr">xylographe</trd>
  <trdgrp lng="hu">
    <trd>fametsz&odblac;</trd>,
    <trd>xilogr&aacute;fus</trd>
  </trdgrp>
  <trd lng="nl">xylograaf</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r; &c_p;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drevorytec</trd>,
    <trd>xylograf</trd>
  </trdgrp>
</drv>
</art>
