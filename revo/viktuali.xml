<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: viktuali.xml,v 1.12 2024/08/04 21:25:45 revo Exp $">
<kap>
  <rad>viktuali</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="viktuali.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="proviz.mangx0o">Man&gcirc;provizo</ref>, <ke><ref tip="dif"
      cel="provia.0o">provianto</ref></ke>:
      <ekz>
        ne tre valora <tld/>o por tiel brava kavaliro
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 10a, p.
        79a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_s; &c_je;&c_zh;&c_y;</trd>
  <trd lng="ca">vituallas</trd>
  <trdgrp lng="cs">
    <trd>potraviny</trd>,
    <trd>viktu&aacute;lie</trd>,
    <trd>z&aacute;soby potravin</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proviant</trd>,
    <trd>Viktualie<klr tip="amb"> (veraltet)</klr></trd>
  </trdgrp>
  <trd lng="es">vituallas</trd>
  <trd lng="fa">&a_A_madda;&a_dh;&a_w;&a_q;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>provision de bouche</trd>,
    <trd>victuaille</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;lelmiszerk&eacute;szlet</trd>,
    <trd>&uacute;traval&oacute;</trd>,
    <trd>elem&oacute;zsia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proviand</trd>,
    <trd>mondvoorraad</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prowiant</trd>,
    <trd>zaopatrzenie</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>potraviny</trd>,
    <trd>viktu&aacute;lie</trd>
  </trdgrp>
</drv>
</art>

