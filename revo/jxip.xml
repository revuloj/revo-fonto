<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jxip.xml,v 1.11 2010/08/09 16:30:20 revo Exp $">
<kap>
  <rad>&jcirc;ip</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jxip.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxip.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Malgranda <ref tip="super" cel="auxtom1.0o">a&ubreve;tomobilo</ref>,
      origine milita, destinita por veturi sur plej malebenaj
      grundoj:
      <ekz>
        li ja miris ke sur la viadukto ne venis e&ccirc; unu <tld/>o de la 
        okupanta armeo 
        <fnt>
          <aut>M. Kuroda</aut>,
          <vrk>Ambur&gcirc;ono</vrk>,
          <lok>Animo drivas, 1990</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="auxt.0o">a&ubreve;to</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_zh;&c_y;&c_p;</trd>,
    <trd>&c_u;&c_s;&c_je;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;
    </trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gel&auml;ndewagen</trd>,
     <trd>K&uuml;belwagen<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">jeep</trd>
  <trd lng="es">jeep</trd>
  <trd lng="fr">jeep</trd>
  <trd lng="hu">dzsip</trd>
  <trd lng="it">jeep</trd>
  <trdgrp lng="nl">
    <trd>jeep</trd>,
    <trd>terreinwagen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>d&zdot;ip</trd>,
    <trd>jeep</trd>,
    <trd>teren&oacute;wka<klr> (pot.)</klr></trd>,
    <trd>samoch&oacute;d terenowy</trd>,
    <trd>&lstroke;azik<klr> (pot.)</klr></trd>,
    <trd>gazik<klr> (pot. o aucie GAZ)</klr></trd>
  </trdgrp>
  <trd lng="pt">jipe</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_p;</trd>
</drv>
</art>

