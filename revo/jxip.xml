<vortaro>
<art mrk="$Id$">
<kap>
<rad>&jcirc;ip</rad>/o
</kap>
<drv mrk="jxip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Malgranda <ref tip="super" cel="auxtom1.0o">a&ubreve;tomobilo</ref>
      (origine milita), destinita por veturi sur plej malebenaj grundoj.
    </dif>
    <ref tip="super" cel="auxt.0o">a&ubreve;to</ref>
  </snc>
  <trd lng="de">Gel&auml;ndewagen</trd>
  <trd lng="en">jeep</trd>
  <trdgrp lng="nl">
    <trd>jeep</trd>,
    <trd>terreinwagen</trd>
  </trdgrp>
<trd lng="ru">&c_d;&c_zh;&c_i;&c_p;</trd>
</drv>
</art>

