     <trd>K&uuml;belwagen<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">jeep</trd>
  <trd lng="es">jeep</trd>
  <trd lng="fa">&a_j;&f_y;&f_p;</trd>
  <trd lng="fr">jeep</trd>
  <trd lng="hu">dzsip</trd>
  <trd lng="it">jeep</trd>
  <trd lng="ja">&#x30B8;&#x30FC;&#x30D7;</trd>
  <trdgrp lng="nl">
    <trd>jeep</trd>,
    <trd>terreinwagen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>d&zdot;ip</trd>,
    <trd>jeep</trd>,
    <trd>teren&oacute;wka<klr> (pot.)</klr></trd>,
    <trd>samoch&oacute;d terenowy</trd>,
    <trd>&lstroke;azik<klr> (pot.)</klr></trd>,
