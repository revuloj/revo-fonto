<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: patric.xml,v 1.17 2023/11/27 09:28:00 revo Exp $">
<kap>
  <rad>patric</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="patric.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="patric.0o.formo">
    <dif>
      Metala peco reliefe gravurita, por formi fand-, moner- a&ubreve; medal-
      matricojn kaj presliterojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="matric.0o">matrico</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_a;&c_n;&c_s;&c_o;&c_n;</trd>,
      <trd>&c_p;&c_a;&c_z;&c_y;&c_t;&c_y;&c_t;&c_y;&c_w;&c_n;&c_a;&c_ja; &c_f;&c_o;&c_r;&c_m;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Patrize</trd>,
      <trd>Pr&auml;gestock</trd>,
      <trd>Stempel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>male mold</trd>,
      <trd>male mould</trd>,
      <trd>male part</trd>,
      <trd>positive part</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>apaminta</trd>,
      <trd>patrica</trd>
    </trdgrp>
    <trd lng="pl">patryca</trd>
    <trd lng="ro">patricea</trd>
    <trd lng="ru">&c_p;&c_u;&c_a;&c_n;&c_s;&c_o;&c_n;</trd>
  </snc>
  <snc mrk="patric.0o.signo">
    <dif>
      <ref tip="super" cel="sign.0o.KOMUNE">Signo</ref> stampata sur orajn kaj ar&gcirc;entajn objektojn
      por garantii la
      proporciecon de la uzitaj metaloj:
      <ekz>
        oficiala patrico indikanta la fajnecon
        <fnt>
          <vrk><url ref="https://lernu.net/en/forumo/temo/26910/1">10 Sm monero / News / Forum -
          lernu.net</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        militoj, eksterlandaj kaj enlanda, estis la <tld/>o de lia re&gcirc;a regado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=546335">Aleksandro Janeo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="sigel.0o">sigelo</ref>
    <trd lng="be">&c_p;&c_r;&c_o;&c_b;&c_a; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_u;)</klr></trd>
    <trdgrp lng="de">
      <trd>Pr&auml;gung</trd>,
      <trd>Gepr&auml;ge</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>imprint</trd>,
      <trd>embossing</trd>
    </trdgrp>
    <trd lng="hu">pr&oacute;ba<klr tip="amb"> (nemesf&eacute;men)</klr></trd>
    <trd lng="pl">znak pr&oacute;bny</trd>
    <trd lng="ro">marca de prob&abreve;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_a; <klr>(&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_h; &c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_o;&c_v;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>razidlo</trd>,
    <trd>raznice</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25171;&#12385;&#x578B;  <pr>&#12358;&#12385;&#x304B;&#x305F;</pr></trd>,
    <trd>&#x91D1;&#x578B;  <pr>&#x304B;&#x306A;&#x304C;&#x305F;</pr></trd>,
    <trd>&#x96C4;&#x578B;  <pr>&#x305F;&#12369;&#12375;&#x304B;&#x305F;</pr></trd>,
    <trd>&#26997;&#21360;  <pr>&#12372;&#x304F;&#12356;&#12435;</pr></trd>,
    <trd>&#x523B;&#21360;  <pr>&#12371;&#x304F;&#12356;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">muntstempel</trd>
  <trd lng="pt">pun&ccedil;&atilde;o <klr>(subst.masc.)</klr></trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>
  <trd lng="sk">patrica <klr>(na vyr&aacute;banie matr&iacute;c)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>,
    <trd>&c_p;&c_u;&c_n;&c_s;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>
  </trdgrp>
</drv>
</art>
