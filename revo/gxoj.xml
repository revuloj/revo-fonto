  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&#x43C;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_ch;&c_a;&#x43B;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>
