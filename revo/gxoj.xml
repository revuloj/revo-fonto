<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&gcirc;oj</rad>/i
</kap>
<drv mrk="gxoj.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti vivan plezuron en la animo:
      <ekz>
        mi <tld/>as vin vidi;
      </ekz>
      <ekz>
        mi <tld/>as, ke vi restas;
      </ekz>
      <ekz>
        <tld/>i pri ies sukceso<fnt>B</fnt>,
      </ekz>
      <ekz>
        malfeli&ccirc;o;
      </ekz>
      <ekz>
        naskinto de sa&gcirc;ulo <tld/>os pro li
        <fnt><bib>MT</bib>, <lok>&Sen; 23:24</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxu.0i">&gcirc;ui</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_c;&c_je;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">radovat se</trd>
  <trd lng="de">freuen</trd>
  <trdgrp lng="en">
    <trd>rejoice</trd>,
    <trd>be <ind>glad</ind></trd>
  </trdgrp>
  <trd lng="fr">se <ind>r&eacute;jouir</ind></trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&uuml;l</trd>,
    <trd>&ouml;rvendezik</trd>
  </trdgrp>
  <trd lng="nl">blij zijn</trd>
  <trdgrp lng="pt">
    <trd>alegrar-se</trd>,
    <trd>regozijar-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="vo">g&auml;l&ouml;n</trd>
</drv>

<drv mrk="gxoj.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Sentanta a&ubreve; montranta <tld/>on:
      <ekz>
        <tld/>a homo, koro, viza&gcirc;o, rideto, krio, lumo, festo;
      </ekz>
      <ekz>
        <tld/>aj petoluloj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gaj.0a">gaja</ref>,
      <ref cel="konten.0a">kontenta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="de">froh</trd>
  <trdgrp lng="en">
    <trd>joyful</trd>,
    <trd>glad</trd>
  </trdgrp>
  <trd lng="fr">joyeux</trd>
  <trdgrp lng="hu">
    <trd>&ouml;rvendez&odblac;</trd>,
    <trd>&ouml;rvendetes</trd>,
    <trd>vid&aacute;m</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bevreugd</trd>,
    <trd>blij</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alegre</trd>,
    <trd>contente</trd>,
    <trd>feliz</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="vo">g&auml;lik</trd>
</drv>

<drv mrk="gxoj.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Viva plezuro de la animo:
      <ekz>
        havi <tld/>on de io, pro io, pri io;
      </ekz>
      <ekz>
        ka&ubreve;zi <tld/>on al iu;
      </ekz>
      <ekz>
        difekti, fu&scirc;i ies <tld/>on;
      </ekz>
      <ekz>
        amiko en <tld/>o kaj ploro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivi en silko kaj veluro, en <tld/>o kaj plezuro
        (<ref tip="sin" cel="viv">gajvivi</ref>)<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sa&gcirc;a filo estas <tld/>o
        <klr>(<tld/>iga&jcirc;o)</klr> por sia patro
        <fnt><bib>MT</bib>, <lok>&Sen; 10:1</lok></fnt>;
      </ekz>
      <ekz>
        por malsa&gcirc;ulo estas <tld/>o fari malbonon
        <fnt><bib>MT</bib>, <lok>&Sen; 10:23</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Freude</trd>
  <trdgrp lng="en">
    <trd>joy</trd>
  </trdgrp>
  <trd lng="fr">joie</trd>
  <trd lng="hu">&ouml;r&ouml;m</trd>
  <trd lng="nl">vreugde</trd>
  <trdgrp lng="pt">
    <trd>alegria</trd>,
    <trd>contentamento</trd>,
    <trd>felicidade</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="vo">g&auml;l</trd>
</drv>

<drv mrk="gxoj.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        laboro lacigas, sed akiro <tld/>igas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sa&gcirc;a filo <tld/>igas la patron
        <fnt><bib>MT</bib>, <lok>&Sen; 15:20</lok></fnt>;
      </ekz>
      <ekz>
        suna vetero <tld/>igas la koron;
      </ekz>
      <ekz>
        <tld/>iga sciigo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">erfreuen</trd>
  <trdgrp lng="en">
    <trd>make <ind>joyful</ind></trd>,
    <trd>make <ind>glad</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meg&ouml;rvendeztet</trd>,
    <trd>&ouml;r&ouml;met okoz neki</trd>
  </trdgrp>
  <trd lng="nl">blij maken</trd>
  <trd lng="fr">r&eacute;jouir</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gxoj.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti vivan malplezuron en la animo, aflikti&gcirc;i:
      <ekz>
        mal<tld/>i pro la morto de amiko<fnt>B</fnt>;
      </ekz>
      <ekz>
        koro scias sian propran mal<tld/>on, kaj en &gcirc;ia mal<tld/>o ne
        partoprenas fremdulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nek <tld/>o nek mal<tld/>o da&ubreve;ras eterne;
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        akiri la mal<tld/>an <klr>(ne enviindan)</klr> gloron de subfosanto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo anta&ubreve;
        la Sesa Kongreso Esperantista en Washington, 1910</vrk>)</fnt>;
      </ekz>
      <ekz>
        profitemulo mal<tld/>igas sian domon
        <fnt><bib>MT</bib>, <lok>&Sen; 15:27</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="funebr">funebri</ref>,
      <ref cel="esper.mal0i">malesperi</ref>,
      <ref cel="lum.mal0a">malluma</ref>,
      <ref cel="nub.0a">nuba</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">trauern</trd>
  <trdgrp lng="hu">
    <trd>b&aacute;nk&oacute;dik</trd>,
    <trd>b&uacute;slakodik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>treuren</trd>,
    <trd>bedroefd zijn</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>triste</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gxoj.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Sentanta fortan malplezuron.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">triste</trd>
  <trdgrp lng="hu">
    <trd>b&aacute;natos</trd>,
    <trd>b&uacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="gxoj.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Sento malplezura, aflikta.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_m;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">tristesse</trd>
  <trdgrp lng="hu">
    <trd>b&aacute;nat</trd>,
    <trd>b&uacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
