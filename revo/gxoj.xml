<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro> 
<art mrk="$Id$"> 
<kap> 
  <ofc>*</ofc> 
  <rad>&gcirc;oj</rad>/i 
</kap> 

<drv mrk="gxoj.0i"> 
  <kap><ofc>*</ofc><tld/>i</kap> 
  <gra><vspec>ntr</vspec></gra> 
  <snc> 
    <dif> 
      <ref tip="super" cel="sent.0i">Senti</ref> vivan plezuron en la animo: 
      <ekz> 
        mi <tld/>as vin vidi; 
      </ekz> 
      <ekz> 
        mi <tld/>as, ke vi restas; 
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> kore <tld/>is vidante lin kaj a&ubreve;dante la rakonton pri lia sukceso
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri oraj haroj de la diablo</lok>
        </fnt>;
      </ekz> 
      <ekz> 
        <tld/>i pri ies malfeli&ccirc;o; 
      </ekz> 
      <ekz>
        mi <tld/>as pro via helpo
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia koro <tld/>as pro via savo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 13:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as, ke en &ccirc;io mi povas fidi al vi
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 7:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as kaj kun<tld/>as kun vi
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 2:17</lok>
        </fnt>;
      </ekz>
      <ekz> 
        naskinto de sa&gcirc;ulo <tld/>os pro li 
        <fnt><bib>MT</bib>, <lok>&Sen; 23:24</lok></fnt>;
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="gxu.0i">&gcirc;ui</ref>. 
    </refgrp> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_v;&c_je;&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_c;&c_je;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd> 
  </trdgrp> 
  <trd lng="cs">radovat se</trd> 
  <trd lng="de">freuen</trd> 
  <trd lng="es">estar feliz</trd> 
  <trdgrp lng="en"> 
    <trd>rejoice</trd>, 
    <trd>be <ind>glad</ind></trd> 
  </trdgrp> 
  <trd lng="fa">&a_kh;&a_w;&a_sh;&a_H;&a_A;&a_l; &a_b;&a_w;&a_d;&a_n;</trd>
  <trd lng="fr">se <ind>r&eacute;jouir</ind></trd> 
  <trdgrp lng="hu"> 
    <trd>&ouml;r&uuml;l</trd>, 
    <trd>&ouml;rvendezik</trd> 
  </trdgrp> 
  <trd lng="it">gioire</trd> 
  <trd lng="nl">blij zijn</trd> 
  <trdgrp lng="pl"> 
    <trd>radowa&cacute; si&eogonek;</trd>, 
    <trd>cieszy&cacute; si&eogonek;</trd> 
  </trdgrp> 
  <trdgrp lng="pt"> 
    <trd>alegrar-se</trd>, 
    <trd>regozijar-se</trd> 
  </trdgrp> 
  <trdgrp lng="ru"> 
    <trd>&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>, 
    <trd>&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd> 
  </trdgrp> 
  <trd lng="vo">g&auml;l&ouml;n</trd> 
  <trd lng="sv">gl&auml;djas</trd>
</drv> 

<drv mrk="gxoj.0a"> 
  <kap><ofc>*</ofc><tld/>a</kap> 
  <snc> 
    <dif> 
      Sentanta a&ubreve; montranta <tld/>on: 
      <ekz> 
        <tld lit="&Gcirc;"/>an feston! (mi deziras al vi) &dash; kia &gcirc;oja festo!
        (estas hodia&ubreve;)
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>
      </ekz> 
      <ekz> 
        <tld/>a homo, koro, viza&gcirc;o, rideto, krio, lumo; 
      </ekz> 
      <ekz> 
        <tld/>aj petoluloj; 
      </ekz> 
      <ekz>
        en gajeco kaj kun <tld/>a koro, kiam vi &ccirc;ion havis abunde
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:47</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a koro faras la viza&gcirc;on <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:13</lok>
        </fnt>.
      </ekz>
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="gaj.0a">gaja</ref>, 
      <ref cel="konten.0a">kontenta</ref>. 
    </refgrp> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_y;</trd>, 
    <trd>&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd> 
  </trdgrp> 
  <trd lng="de">froh</trd> 
  <trdgrp lng="en"> 
    <trd>joyful</trd>, 
    <trd>glad</trd> 
  </trdgrp> 
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_sh;&a_H;&a_A;&a_l;</trd>,
    <trd>&a_sh;&a_A;&a_d;&a_m;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">joyeux</trd> 
  <trdgrp lng="hu"> 
    <trd>&ouml;rvendez&odblac;</trd>, 
    <trd>&ouml;rvendetes</trd>, 
    <trd>vid&aacute;m</trd> 
  </trdgrp> 
  <trdgrp lng="it"> 
    <trd>gioioso</trd>, 
    <trd>felice</trd>,  
    <trd>giulivo</trd>, 
    <trd>gaudioso</trd> 
  </trdgrp> 
  <trdgrp lng="nl"> 
    <trd>bevreugd</trd>, 
    <trd>blij</trd> 
  </trdgrp> 
  <trdgrp lng="pl"> 
    <trd>radosny</trd>, 
    <trd>ucieszony</trd> 
  </trdgrp> 
  <trdgrp lng="pt"> 
    <trd>alegre</trd>, 
    <trd>contente</trd>, 
    <trd>feliz</trd> 
  </trdgrp> 
  <trdgrp lng="es"> 
    <trd>alegre</trd>, 
    <trd>contento, -a</trd>, 
    <trd>feliz</trd> 
  </trdgrp> 
  <trd lng="ru">&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd> 
  <trd lng="vo">g&auml;lik</trd>
  <trdgrp lng="sv">
    <trd>glad</trd>,
    <trd>gl&auml;djefull</trd>
  </trdgrp>
</drv> 

<drv mrk="gxoj.0o"> 
  <kap><tld/>o</kap> 
  <snc> 
    <dif> 
      Viva <ref tip="lst" lst="voko:sentoj" cel="plezur.0o">plezuro</ref> de la animo: 
      <ekz> 
        havi <tld/>on de io, pro io, pri io; 
      </ekz> 
      <ekz> 
        ka&ubreve;zi <tld/>on al iu; 
      </ekz> 
      <ekz> 
        difekti, fu&scirc;i ies <tld/>on; 
      </ekz> 
      <ekz> 
        amiko en <tld/>o kaj ploro<fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        vivi en silko kaj veluro, en <tld/>o kaj plezuro 
        <klr>(kp <ref tip="sin" cel="viv">gajvivi</ref>)</klr><fnt><bib>PrV</bib> 
        </fnt>; 
      </ekz> 
      <ekz> 
        sa&gcirc;a filo estas <tld/>o 
        <klr>(<tld/>iga&jcirc;o)</klr> por sia patro 
        <fnt><bib>MT</bib>, <lok>&Sen; 10:1</lok></fnt>; 
      </ekz> 
      <ekz> 
        por malsa&gcirc;ulo estas <tld/>o fari malbonon 
        <fnt><bib>MT</bib>, <lok>&Sen; 10:23</lok></fnt>. 
      </ekz> 
    </dif> 
  </snc> 
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd> 
  <trd lng="de">Freude</trd> 
  <trdgrp lng="en"> 
    <trd>joy</trd> 
  </trdgrp> 
  <trd lng="es">alegr&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_sh;&a_H;&a_A;&a_l;&f_y;</trd>,
    <trd>&a_sh;&a_A;&a_d;&a_m;&a_A;&a_n;&f_y;</trd>
  </trdgrp> 
  <trd lng="fr">joie</trd> 
  <trd lng="hu">&ouml;r&ouml;m</trd>
  <trd lng="it">gioia</trd> 
  <trd lng="nl">vreugde</trd> 
  <trdgrp lng="pt"> 
    <trd>alegria</trd>, 
    <trd>contentamento</trd>, 
    <trd>felicidade</trd> 
  </trdgrp> 
  <trdgrp lng="pl"> 
    <trd>rado&sacute;&cacute;</trd>, 
    <trd>uciecha</trd> 
  </trdgrp> 
  <trd lng="ru">&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_mol;</trd> 
  <trd lng="vo">g&auml;l</trd>
  <trd lng="sv">gl&auml;dje</trd>
</drv> 

<drv mrk="gxoj.0igi"> 
  <kap><tld/>igi</kap> 
  <snc> 
    <dif> 
      Igi <tld/>a: 
      <ekz> 
        laboro lacigas, sed akiro <tld/>igas<fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        sa&gcirc;a filo <tld/>igas la patron 
        <fnt><bib>MT</bib>, <lok>&Sen; 15:20</lok></fnt>; 
      </ekz> 
      <ekz> 
        suna vetero <tld/>igas la koron; 
      </ekz> 
      <ekz> 
        <tld/>iga sciigo. 
      </ekz> 
    </dif> 
  </snc> 
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd> 
  <trd lng="de">erfreuen</trd> 
  <trdgrp lng="en"> 
    <trd>make <ind>joyful</ind></trd>, 
    <trd>make <ind>glad</ind></trd> 
  </trdgrp>
  <trd lng="fa">&a_kh;&a_w;&a_sh;&a_H;&a_A;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="hu"> 
    <trd>meg&ouml;rvendeztet</trd>, 
    <trd>&ouml;r&ouml;met okoz neki</trd> 
  </trdgrp>
  <trdgrp lng="it"> 
    <trd>rallegrare</trd>, 
    <trd>ingioiare</trd>, 
    <trd>allietare</trd> 
  </trdgrp>
  <trd lng="nl">blij maken</trd> 
  <trd lng="fr">r&eacute;jouir</trd> 
  <trd lng="es">alegrar</trd> 
  <trdgrp lng="pl"> 
    <trd>cieszy&cacute;<klr> (kogo&sacute;)</klr></trd>, 
    <trd>radowa&cacute;<klr> (kogo&sacute;)</klr></trd> 
  </trdgrp> 
  <trdgrp lng="ru"> 
    <trd>&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>, 
    <trd>&c_o;&c_b;&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd> 
  </trdgrp>
  <trd lng="sv">gl&auml;dja</trd>
</drv> 

<drv mrk="gxoj.mal0i"> 
  <kap>mal<tld/>i</kap> 
  <gra><vspec>ntr</vspec></gra> 
  <snc> 
    <dif> 
      Senti vivan malplezuron en la animo, aflikti&gcirc;i: 
      <ekz>
        kiel homo, kiu funebras pri sia patrino, mi profunde mal<tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 35:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ploris kaj mal<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i certe sopiras pri mi, mal<tld/>as pri mi
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fratinoj mal<tld/>is, kiel mal<tld/>as junaj koroj, tu&scirc;itaj precipe de la
        doloro de la gepatroj: la patro estis malgaja, sed plej profunde premita
        estis la koro de la patrino
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post sep tagoj li vidis, ke la rezultato estas nula; li mal<tld/>is kaj
        malesperis
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o ekmal<tld/>is, ke li estas perdonta &ccirc;iujn siajn fidelajn servistojn pro
        unu
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>.
      </ekz>
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="funebr">funebri</ref>, 
      <ref cel="esper.mal0i">malesperi</ref>, 
      <ref cel="lum.mal0a">malluma</ref>, 
      <ref cel="nub.0a">nuba</ref>. 
    </refgrp> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_s;&c_u;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd> 
    <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_zh;&c_u;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd> 
  </trdgrp> 
  <trd lng="de">trauern</trd> 
  <trd lng="es">estar triste</trd> 
  <trdgrp lng="fa">
    <trd>&a_A;&a_n;&a_d;&a_w;&a_h;&f_g;&f_y;&a_n; &a_b;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_gh;&a_m;&f_g;&f_y;&a_n; &a_b;&a_w;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>triste</ind></trd> 
  <trdgrp lng="hu"> 
    <trd>b&aacute;nk&oacute;dik</trd>, 
    <trd>b&uacute;slakodik</trd> 
  </trdgrp> 
  <trdgrp lng="it"> 
    <trd>addolorarsi</trd>, 
    <trd>rattristarsi</trd>, 
    <trd>essere <ind>triste</ind></trd>, 
    <trd>contristarsi</trd>
  </trdgrp>
  <trdgrp lng="nl"> 
    <trd>treuren</trd>, 
    <trd>bedroefd zijn</trd> 
  </trdgrp> 
  <trdgrp lng="pl"> 
    <trd>smuci&cacute; si&eogonek;</trd>, 
    <trd>martwi&cacute; si&eogonek;</trd>, 
    <trd>troska&cacute; si&eogonek;</trd>, 
    <trd>by&cacute; markotnym</trd>, 
    <trd>frasowa&cacute; si&eogonek;<klr> (przest.)</klr></trd> 
  </trdgrp> 
  <trdgrp lng="ru"> 
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>, 
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd> 
  </trdgrp> 
</drv> 

<drv mrk="gxoj.mal0a"> 
  <kap>mal<tld/>a</kap> 
  <snc mrk="gxoj.mal0a.mem"> 
    <dif> 
      Sentanta a&ubreve; esprimanta fortan malplezuron, kiel post perdo, adia&ubreve;o de proksimulo:
      <ekz>
        &scirc;i estis tre mal<tld/>a <klr>[&#8230;]</klr> kaj forte ploris
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 1:10</lok>
        </fnt>;
      </ekz> 
      <ekz>
        la re&gcirc;o de <nom>Izrael</nom> iris hejmen mal<tld/>a kaj afliktita
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 20:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vokis per mal<tld/>a vo&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun mal<tld/>a mieno
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 6:16</lok>
        </fnt>.
      </ekz>
    </dif> 
    <trdgrp lng="be"> 
      <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>, 
      <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_n;&c_y;</trd>, 
      <trd>&c_zh;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_y;</trd> 
    </trdgrp> 
    <trd lng="de">traurig</trd> 
    <trd lng="es">triste</trd> 
    <trdgrp lng="fa">
      <trd>&a_gh;&a_m;&f_g;&f_y;&a_n;</trd>,
      <trd>&a_A;&a_n;&a_d;&a_w;&a_h;&f_g;&f_y;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">triste</trd> 
    <trdgrp lng="hu"> 
      <trd>b&aacute;natos</trd>, 
      <trd>b&uacute;s</trd> 
    </trdgrp> 
   <trd lng="it">triste</trd>
    <trdgrp lng="pl"> 
      <trd>smutny</trd>, 
      <trd>zmartwiony</trd>, 
      <trd>zatroskany</trd>, 
      <trd>markotny</trd>, 
      <trd>zafrasowany<klr> (przest.)</klr></trd> 
    </trdgrp>
    <trd lng="ro">trist</trd> 
    <trdgrp lng="ru"> 
      <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>, 
      <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>, 
      <trd>&c_b;&c_je;&c_z;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd> 
    </trdgrp> 
  </snc> 
  <snc mrk="gxoj.mal0a.iun"> 
    <dif> 
      Ka&ubreve;zanta tian fortan malplezuron:
      <ekz>
        &ccirc;iu puno portempe &scirc;ajnas esti <klr>[&#8230;]</klr> mal&gcirc;oja
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 12:11</lok>
        </fnt>;
      </ekz> 
      <ekz>
        estas mal<tld/>a afero, kiam oni devas vivi kiel fremdulo en fremda lando, kie
        oni neniun interesas
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la mal<tld/>a rememoro ekfluis &scirc;iaj larmoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif> 
    <trd lng="fa">&a_gh;&a_m;&zwnj;&a_A;&a_n;&f_g;&f_y;&a_z;</trd>
    <trdgrp lng="pl"> 
      <trd>smuci&cacute;</trd>, 
      <trd>martwi&cacute;</trd> 
    </trdgrp> 
  </snc> 
</drv> 

<drv mrk="gxoj.mal0o"> 
  <kap>mal<tld/>o</kap> 
  <snc> 
    <dif> 
      <ref tip="lst" lst="voko:sentoj" cel="sent.0o.emocio">Sento</ref> malplezura,
      kiel post perdo, adia&ubreve;o de proksimulo k.s.:
      <ekz>
        amiko en <tld/>o kaj mal<tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz> 
      <ekz>
        kiun mal<tld/>o ne turmentis, tiu <tld/>on ne sentas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni &jcirc;etis la terbulojn sur la &ccirc;erkon <klr>[&#8230;]</klr> kaj li havis la senton,
        kvaza&ubreve; pro mal<tld/>o tuj krevos al li la koro
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
