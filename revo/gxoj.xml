<drv mrk="gxoj.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Sentanta a&ubreve; montranta <tld/>on:
      <ekz>
        <tld lit="&Gcirc;"/>an feston! (mi deziras al vi) &dash; kia &gcirc;oja festo!
        (estas hodia&ubreve;)
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a patrino de infanoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 113:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        en gajeco kaj kun <tld/>a koro, kiam vi &ccirc;ion havis abunde
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:47</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a koro faras la viza&gcirc;on <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        estu kore unuigitaj <klr>[&#x2026;]</klr> en espero <tld/>aj, en aflikto paciencaj, en
        pre&gcirc;ado persistaj
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 12:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la rendevuoj ili havis <tld/>an tempon
         <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        varia elekto de amuzaj, tristaj, <tld/>aj <klr>[&#x2026;]</klr> kantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Wolfram Rohloff</aut>
          <vrk><url ref="&Monato;/2004/008428.html">Kantaro kun fluaj
          tekstoj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gaj.0a">gaja</ref>,
      <ref cel="konten.0a">kontenta</ref>.
    </refgrp>
  </snc>
