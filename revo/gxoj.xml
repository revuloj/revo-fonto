<drv mrk="gxoj.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        laboro lacigas, sed akiro <tld/>igas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sa&gcirc;a filo <tld/>igas la patron
        <fnt><bib>MT</bib>, <lok>&Sen; 15:20</lok></fnt>;
      </ekz>
      <ekz>
        amika vorto <klr>[&#x2026;]</klr> <tld/>igas
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 12:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kor<tld/>iga vido kaj bonodoro de &scirc;ar&gcirc;ita fruktarbo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la opinio de la gasto flatis kaj <tld/>igis lin
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dioj donis la <tld/>igan sorton
        <fnt>
          <bib>Ifigenio</bib>
