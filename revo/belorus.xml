<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>belorus</rad>/o
</kap>

<drv mrk="belorus.0o">
  <kap><tld/>o</kap>
  <snc mrk="belorus.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="belorus.B0ujo"><tld lit="B"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="belorus.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="pt">bielorrusso <klr tip="ind"> (subst.)</klr></trd>
</drv>

<drv mrk="belorus.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="pt">bielorrusso <klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="belorus.B0ujo">
  <kap>
    <tld lit="B"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="belorus.B0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en orienta E&ubreve;ropo kun &ccirc;efurbo Minsko.
      Teritorio de Respubliko Belorusujo.
      ISO-3166 kodo: BY.
      &Gcirc;i limas kun
      <ref tip="vid" cel="latv.L0ujo">Latvujo</ref>,
      <ref tip="vid" cel="litov.L0ujo.GEOG">Litovujo</ref>,
      <ref tip="vid" cel="pol.P0lando">Pollando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>
      kaj <ref tip="vid" cel="ukrain.U0ujo">Ukrainujo</ref>.
    </dif>
    <trd lng="bg">&c_B;&c_je;&c_l;&c_a;&c_r;&c_u;&c_s;</trd>
    <trd lng="de">Wei&szlig;russland</trd>
    <trd lng="en">Belarus</trd>
    <trd lng="fa">&a_b;&a_l;&a_A;&a_r;&a_w;&a_s;</trd>
    <trd lng="nl">Wit-Rusland</trd>
    <trdgrp lng="pl">
      <trd>Bia&lstroke;oru&sacute;</trd>,
      <trd>Republika <ind>Bia&lstroke;orusi</ind></trd>
    </trdgrp>
    <trd lng="pt">Bielorr&uacute;ssia</trd>
  </snc>
</drv>

</art>

