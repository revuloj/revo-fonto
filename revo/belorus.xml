<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: belorus.xml,v 1.24 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>belorus</rad>/o,
  <var><kap><rad>belarus</rad>/o</kap></var>
</kap>

<drv mrk="belorus.0o">
  <kap><tld/>o,
    <var><kap>belaruso</kap></var>
  </kap>
  <snc mrk="belorus.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de <ref
      tip="malprt" cel="belorus.B0ujo"><tld lit="B"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="belorus.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        350 000 nelitovoj &ndash; precipe poloj, rusoj kaj <tld/>oj &ndash; scias la
        litovan
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url
          ref="&Monato;/2003/008013.html">Parolema
          popolo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        inter la litovoj en la parlamento estis elektitaj tri rusoj, po
        du <tld/>oj kaj poloj, kaj unu judo
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url
          ref="&Monato;/2005/008504.php">Parlamento
          multpartia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_a;&c_r;&c_u;&c_s;</trd>
  <trd lng="cs">B&ecaron;lorus</trd>
  <trdgrp lng="de">
    <trd>Wei&szlig;russe</trd>,
    <trd>Belarusse</trd>
  </trdgrp>
  <trd lng="en">Belarusian</trd>
  <trd lng="es">bielorruso</trd>
  <trd lng="fr">Bi&eacute;lorusse<klr> (subst.)</klr></trd>
  <trd lng="fy">Wyt-Rus</trd>
  <trdgrp lng="io">
    <trd>Bielorusiano</trd>,
    <trd>Bieloruso</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Wit-Rus</trd>,
    <trd>Belarus</trd>
  </trdgrp>
  <trd lng="pl">Bia&lstroke;orusin</trd>
  <trd lng="pt">bielorrusso <klr tip="ind"> (subst.)</klr></trd>
  <trd lng="sk">Bielorus</trd>
  <trd lng="tok">jan pi ma Pelalusi</trd>
  <trd lng="vo">Belarus&auml;nan</trd>
  <trd lng="zh">&#30333;&#20420;&#32599;&#26031;&#20154; <pr>b&aacute;i &eacute; lu&oacute; sī r&eacute;n</pr></trd>
</drv>

<drv mrk="belorus.0a">
  <kap><tld/>a,
    <var><kap>belarusa</kap></var>
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>aj infanoj malsanigitaj pro la nuklea katastrofo en
        &Ccirc;ernobilo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2001/007319.php">Ne tiom negative ...</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        la landlimo litova-<tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2008/009413.php">Vizoj
          reviziitaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_a;&c_r;&c_u;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>wei&szlig;russisch</trd>,
    <trd>belarussisch</trd>
  </trdgrp>
  <trd lng="en">Belarusian</trd>
  <trd lng="fr">bi&eacute;lorusse<klr> (adj.)</klr></trd>
  <trd lng="fy">Wyt-Russysk</trd>
  <trdgrp lng="io">
    <trd>Bielorusiana</trd>,
    <trd>Bielorusa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Wit-Russisch</trd>,
    <trd>Belarussisch</trd>
  </trdgrp>
  <trd lng="pl">bia&lstroke;oruski</trd>
  <trd lng="pt">bielorrusso <klr tip="ind"> (adj.)</klr></trd>
  <trd lng="tok">pi ma Pelalusi</trd>
  <trd lng="vo">Belarus&auml;nik</trd>
</drv>

<drv mrk="belorus.B0ujo">
  <kap>
    <tld lit="B"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>,
    <var><kap>Belarusujo</kap></var>
  </kap>
  <mlg kod="ISO-3166">BY</mlg>
  <snc mrk="belorus.B0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en orienta <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Minsko; teritorio de Respubliko <tld lit="B"/>ujo, 
      limanta kun <ref tip="vid" cel="latv.L0ujo">Latvujo</ref>, 
      <ref tip="vid" cel="litov.L0ujo.GEOG">Litovujo</ref>, <ref
      tip="vid" cel="pol.P0lando">Pollando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref> kaj <ref tip="vid"
      cel="ukrajn.U0ujo">Ukrainujo</ref>:
      <ekz>
        la patrino, <nom>Elsie Chomsky</nom> (naski&gcirc;inta
        <nom>Simonofsky</nom>), venis de kio nun estas <tld lit="B"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29193">Noam
          Chomsky</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="belorusi.B0o">Belorusio</ref>
    <trd lng="be">&c_B;&c_je;&c_l;&c_a;&c_r;&c_u;&c_s;&c_mol;</trd>
    <trd lng="bg">&c_B;&c_je;&c_l;&c_a;&c_r;&c_u;&c_s;</trd>
    <trdgrp lng="de">
      <trd>Wei&szlig;russland</trd>,
      <trd>Belarus</trd>
    </trdgrp>
    <trd lng="en">Belarus</trd>
    <trd lng="fa">&a_b;&a_l;&a_A;&a_r;&a_w;&a_s;</trd>
    <trdgrp lng="fr">
      <trd>Bi&eacute;lorussie</trd>,
      <trd>R&eacute;publique de Bi&eacute;lorussie</trd>
    </trdgrp>
    <trd lng="fy">Wyt-Rusl&acirc;n</trd>
    <trd lng="id">Belarus</trd>
    <trd lng="io">Bielorusia</trd>
    <trdgrp lng="nl">
      <trd>Wit-Rusland</trd>,
      <trd>Belarus</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Bia&lstroke;oru&sacute;</trd>,
      <trd>Republika <ind>Bia&lstroke;orusi</ind></trd>
    </trdgrp>
    <trd lng="pt">Bielorr&uacute;ssia</trd>
    <trd lng="tok">ma Pelalusi</trd>
    <trd lng="vo">Belarus&auml;n</trd>
    <trd lng="zh">&#30333;&#20420;&#32599;&#26031; <pr>b&aacute;i &eacute; lu&oacute; sī</pr></trd>
  </snc>
</drv>
</art>

