<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>belorus</rad>/o
</kap>

<drv mrk="belorus.0ujo">
  <kap>
    <tld lit="B"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="belorus.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la orienta E&ubreve;ropo kun &ccirc;efurbo Minsko.
      Teritorio de Respubliko Belorusujo.
      ISO-3166 kodo: BY.
      &Gcirc;i limas kun
      <ref tip="vid" cel="latv.0ujo">Latvujo</ref>
      <ref tip="vid" cel="litov.0ujo.GEOG">Litovujo</ref>
      <ref tip="vid" cel="pol.P0lando">Pollando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>
      kaj <ref tip="vid" cel="ukrain.U0ujo">Ukrainujo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Bia&lstroke;oru&sacute;</trd>,
      <trd>Republika <ind>Bia&lstroke;orusi</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

