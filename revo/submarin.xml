<kap>
  <rad>submarin</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="submarin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="submarin.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="dif" cel="sxip.submar0o">Submar&scirc;ipo</ref>:
      <ekz>
        sub la &scirc;ipbordo mem trana&gcirc;is longa kaj mallar&gcirc;a <tld/>o
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        iru al <nom>Vasilevskij</nom>-insulo, kie atendas vin la vera <tld/>o-muzeo
        <fnt>
          <vrk><url ref="https://stanobelov.blogspot.com/2018/01/submarino-d-2-sinko-en-la-pasintecon.html">Submarino
          D-2: sinko en la pasintecon</url></vrk>
          <lok>stanobelov.blogspot.com</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>submar&#349;ipo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_b;&c_m;&c_a;&c_r;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_l;&c_o;&c_d;&c_k;&c_a;</trd>.
    <trd>&c_p;&c_a;&c_d;&c_v;&c_o;&c_d;&c_n;&c_y; &c_ch;&c_o;&c_v;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">ponorka</trd>
  <trd lng="de">U-Boot</trd>
  <trdgrp lng="pl">
    <trd>okr&eogonek;t podwodny</trd>,
    <trd>&lstroke;&oacute;d&zacute; podwodna</trd>
  </trdgrp>
  <trd lng="sk">ponorka</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&c_ib;&c_d;&c_v;&#1086;&c_d;&#1085;&c_i;&c_j; &c_ch;&#1086;&c_v;&c_je;&#1085;</trd>,
    <trd>&c_s;&c_u;&c_b;&#1084;&c_a;&c_r;&c_i;&#1085;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28507;&#33351; <pr>qi&#225;nl&#464;ng</pr></trd>,
    <trd>&#28508;&#33351; <pr>qi&#225;nl&#464;ng</pr></trd>,
    <trd>&#28507;&#33351; <pr>qi&#225;nt&#464;ng</pr></trd>,
    <trd>&#28508;&#33351; <pr>qi&#225;nt&#464;ng</pr></trd>,
    <trd>&#28507;&#27700;&#33351; <pr>qi&#225;nshu&#464;t&#464;ng</pr></trd>,
    <trd>&#28508;&#27700;&#33351; <pr>qi&#225;nshu&#464;t&#464;ng</pr></trd>
  </trdgrp>
</drv>
