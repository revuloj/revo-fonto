<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ser&gcirc;ent</rad>/o
</kap>
<drv mrk="sergxe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.sub0o">suboficiro</ref>
      de plej malalta rango.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_r;&c_zh;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Sergeant</trd>,
    <trd>Wachtmeister</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sergent<klr tip="ind"> (sous-officier)</klr></trd>,
    <trd>mar&eacute;chal des logis</trd>
  </trdgrp>
  <trd lng="hu">&odblac;rmester</trd>
  <trd lng="nl">sergeant</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_t;</trd>
</drv>

<drv mrk="sergxe.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.sub0o">suboficiro</ref>, kiu rangas inter
      <ref cel="sergxe.0o"><tld/>o</ref> kaj
      <ref cel="adjuda.0o">adjudanto</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>sergent-chef</trd>,
    <trd>mar&eacute;chal des logis-chef</trd>
  </trdgrp>
</drv>
</art>
