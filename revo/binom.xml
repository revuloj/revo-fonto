<art mrk="$Id: binom.xml,v 1.27 2023/10/14 08:39:48 revo Exp $">
<kap><rad>binom</rad>/o</kap>
<drv mrk="binom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="polino.0o">Polinomo</ref> kun du
      <ref cel="term.0o.polinomo">termoj</ref>;
      <klr>alidire, en elementa algebro:</klr>
      algebra <ref cel="esprim.0o.KOMP">esprimo</ref>, konsistanta 
      el du <ref cel="term.0o.sumo">termoj</ref>
      kunligitaj per <ref cel="adici.0o">adicio</ref> a&ubreve; 
      <ref cel="subtra.0o">subtraho</ref>:
      <ekz>
        <tld/>a koeficiento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=221512">Binoma koeficiento</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a distribuo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=207736">Probabla distribuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="term.n0o">n-termo.</ref>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_n;&c_o;&c_m;</trd>
  <trd lng="bg">&c_b;&c_i;&c_n;&c_o;&c_m;</trd>
  <trd lng="ca">binomi</trd>
  <trdgrp lng="cs">
    <trd>binom</trd>,
    <trd>dvoj&ccaron;len</trd>
  </trdgrp>
  <trd lng="de">Binom</trd>
  <trd lng="en">binomial</trd>
  <trd lng="es">binomio</trd>
  <trd lng="fa">&a_d;&a_w;&a_j;&a_m;&a_l;&a_h;&zwnj;&a_A;&f_y;</trd>
  <trd lng="fr">bin&ocirc;me</trd>
  <trd lng="he">&#x5D1;&#x5D9;&#x5E0;&#x5D5;&#x5DE;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;ttag&uacute; <ind>kifejez&eacute;s</ind></trd>,
    <trd>binom</trd>
  </trdgrp>
  <trd lng="it">binomio</trd>
  <trd lng="ja">&#x4E8C;&#x9805;&#x5F0F;  <pr>&#x306B;&#x3053;&#x3046;&#x3057;&#x304D;</pr></trd>
  <trd lng="nl">binomium</trd>
  <trd lng="pl">dwumian</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_u;&c_ch;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_b;&c_i;&c_n;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bin&oacute;m</trd>,
    <trd>dvoj&ccaron;len</trd>,
    <trd>mnoho&ccaron;len</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x432;&#x43E;&#x447;&#x43B;&#x435;&#x43D;</trd>,
    <trd>&#x431;&#x456;&#x43D;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>
