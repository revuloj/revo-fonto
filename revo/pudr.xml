<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pudr</rad>/o
</kap>
<drv mrk="pudr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pulvoro el parfumita kaj kolorigita amelo, uzata por tualeto:
      <ekz>
        riz<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_d;&c_r;&c_a;</trd>
  <trd lng="de">Puder</trd>
  <trd lng="en">powder <klr>(cosmetic facial)</klr></trd>
  <trd lng="fr">poudre<klr> (cosm&eacute;tique)</klr></trd>
  <trd lng="hu">p&uacute;der</trd>
  <trdgrp lng="nl">
    <trd>poeder</trd>,
    <trd>toiletpoeder</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>talco</trd>,  
    <trd>p&oacute;-de-arroz</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>púder</trd>,
    <trd>zásyp</trd>
  </trdgrp>
</drv>

<drv mrk="pudr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sur&scirc;uti per <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ne&gcirc;e <tld/>itaj kampoj<fnt>Z</fnt>
        (<ref tip="vid" cel="prujn.0o">prujno</ref>);
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sennombraj astroj, kiuj <tld/>as la vastegon.
      </ekz>
    </dif>
    <ref tip="sin" cel="polv.0o">polvo</ref>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_d;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="de">pudern</trd>
  <trd lng="fr">poudrer</trd>
  <trd lng="hu">p&uacute;derez</trd>
  <trd lng="nl">poederen</trd>
  <trd lng="ru">&c_p;&c_u;&c_d;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>napudrovať</trd>,
    <trd>zapudrovať</trd>,
    <trd>zasypať púdrom</trd>
  </trdgrp>
</drv>
</art>
