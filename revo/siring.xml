<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>siring</rad>/o
</kap>
<drv mrk="siring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Syringa</trd>), malgranda arbo devena el
      Persujo kaj havanta grapolojn da blankaj a&ubreve;
      violkoloraj bonodoraj floroj; lilako.
    </dif>
  </snc>
  <bld lok="../bld/siring.jpg">
    floraroj kaj folioj de <tld/>o
  </bld>
  <trd lng="de">Flieder</trd>
  <trd lng="nl">sering</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_je;&c_n;&c_mol;</trd>
</drv>
</art>
