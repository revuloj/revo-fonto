<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>siring</rad>/o
</kap>
<drv mrk="siring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Syringa</trd>), malgranda arbo devena el
      Persujo kaj havanta grapolojn da blankaj a&ubreve;
      violkoloraj bonodoraj floroj; lilako.
    </dif>
  </snc>
  <bld lok="../bld/siring.jpg">
    floraroj kaj folioj de <tld/>o
  </bld>
  <trd lng="be">&c_b;&c_e;&c_z;</trd>
  <trd lng="ca">lil&agrave;</trd>
  <trd lng="de">Flieder</trd>
  <trdgrp lng="en">
    <trd>Syringa</trd>,
    <trd>lilac</trd>
  </trdgrp>
  <trd lng="es">lila</trd>
  <trd lng="fr">lilas</trd>
  <trd lng="hu">orgona<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="nl">sering</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_je;&c_n;&c_mol;</trd>
</drv>
</art>
