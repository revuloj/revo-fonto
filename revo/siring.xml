<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: siring.xml,v 1.23 2018/10/07 19:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>siring</rad>/o
</kap>
<drv mrk="siring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Syringa</trd>), malgranda 
      <ref tip="lst" cel="arb.0o" lst="voko:lamioparencoj">arbo</ref> 
      devena el Persujo kaj havanta grapolojn da blankaj a&ubreve;
      violkoloraj bonodoraj floroj:
      <ekz>
        arbeta&jcirc;oj de <tld/>o kaj de sova&gcirc;aj rozoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o staris en plena florado kaj odoris
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj lonicero
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la floranta <tld/>o klinis siajn fre&scirc;ajn bonodorajn flora&jcirc;ojn 
        super la kapon de la mortinto
        <fnt><bib>Fab3</bib>, <lok>Muta libro</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="lilak.0o">lilako</ref>    
    <bld
       lok="&WCU;/thumb/a/a0/Syringa_reflexa_A.jpg/640px-Syringa_reflexa_A.jpg"
       prm="PD"
       >floraroj kaj folioj de <tld/>o
         <fnt>
           <aut>Wouter Hagens, propra verko</aut>
           <lok><url ref="&WCW;/File:Syringa_reflexa_A.jpg"
               >&WCW;/File:Syringa_reflexa_A.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_z;</trd>
  <trd lng="ca">lil&agrave;</trd>
  <trd lng="de">Flieder</trd>
  <trdgrp lng="en">
    <trd>Syringa</trd>,
    <trd>lilac</trd>
  </trdgrp>
  <trd lng="es">lila</trd>
  <trd lng="fr">lilas</trd>
  <trd lng="hu">orgona<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="nl">sering</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">orgov&aacute;n</trd>
  <trdgrp lng="cs">
    <trd>barevn&aacute; forma &scaron;e&rcaron;&iacute;ku</trd>,
    <trd>bez modr&yacute;</trd>,
    <trd>lila</trd>,
    <trd>&scaron;e&rcaron;&iacute;k</trd>
  </trdgrp>
</drv>
</art>
