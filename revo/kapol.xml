  <trd lng="ja">&#x30AB;&#x30DC;&#x30B7;&#x30E7;&#x30F3;</trd>
  <trd lng="nl">cabochon<klr tip="ind"> (edelsteen)</klr></trd>
  <trd lng="pl">kaboszon</trd>     
  <trd lng="pt">cabuch&atilde;o</trd>
  <trd lng="ro">cabo&#x219;oane</trd>  
  <trd lng="ru">&c_k;&c_a;&c_b;&c_o;&c_sh;&c_o;&c_n;</trd>
  <trd lng="uk">&#x43A;&#x430;&#x431;&#x43E;&#x448;&#x43E;&#x43D;</trd>
