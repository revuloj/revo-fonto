<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sekret</rad>/o
</kap>
<drv mrk="sekret.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Io, kio restas a&ubreve; devas resti ka&scirc;ita kaj ne de &ccirc;iu ajn konata:
      <ekz>
        tenado de <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        (el)perfidi<fnt>Z</fnt>,
      </ekz>
      <ekz>
        babili <tld/>on<fnt>B</fnt>;
      </ekz>
      <ekz>
        eltiri de iu la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        teni ien en <tld/>o<fnt>Z</fnt>
        <klr>(teni <tld/>a)</klr>;
      </ekz>
      <ekz>
        teni fabrikprocedon en plej granda <tld/>o;
      </ekz>
      <ekz>
        publika <tld/>o<fnt>Z</fnt>
        <klr>(de &ccirc;iuj konata afero)</klr>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Geheimnis</trd>
  <trd lng="en">secret</trd>
  <trd lng="nl">geheim</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Restanta a&ubreve; devanta resti ka&scirc;ita:
      <ekz>
        <tld/>a donaco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a societo;
      </ekz>
      <ekz>
        vundo <tld/>a doloras plej multe
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a amo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mister.0a">mistera</ref>,
      <ref cel="enigm.0a">enigma</ref>.
    </refgrp>
  </snc>
  <trd lng="de">geheim</trd>
  <trd lng="nl">geheim</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e diri
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kasx">ka&scirc;e</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="plank.0o">planko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>heimelijk</trd>,
    <trd>in het <ind>geheim</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_o; <ind>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_u;</ind></trd>,
    <trd>&c_v; <ind>&c_t;&c_a;&c_j;&c_n;&c_je;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Diri al iu, petante, ke li ne rediru:
      <ekz>
        vi <tld/>os al edzino, &scirc;i <tld/>os al fratino kaj tiel la <tld/>o
        promenadas sen fino<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">toevertrouwen</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_p;&c_o; <ind>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_u;</ind></trd>
</drv>

<drv mrk="sekret.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Teni <tld/>e.
    </dif>
  </snc>
  <trd lng="de">geheim halten</trd>
  <trd lng="nl">geheim houden</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_v; &c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Diskreta.
    </dif>
  </snc>
  <trd lng="nl">discreet</trd>
  <trd lng="ru">&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sekret.mal0igi">
  <kap>mal<tld/>igi</kap>
  <snc>
    <dif>
      Malka&scirc;i, disfamigi ion <tld/>an.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>verklappen</trd>,
    <trd>ruchtbaar maken</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
