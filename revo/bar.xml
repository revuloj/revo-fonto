<drv mrk="bar.el0i">
  <kap>
    el<tld/>i,
    <var><kap>for<tld/>i</kap></var>
  </kap>
  <snc>
    <dif>
      Malhelpi, malpermesi la eniron:
      <ekz>
        &scirc;talmuro tri metrojn alta <klr>[&#8230;]</klr> tute el<tld/>is la publikon
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008369.html">Kontra&ubreve; Bush, ne kontra&ubreve; Usono</url></vrk>
              <bib>Monato</bib>
          <lok>2004/10, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        for<tld/>i fu&scirc;ulojn
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2006/008988.php">Grava lingvo estas tiu, en kiu oni trovas multajn
          informojn</url></vrk>
              <bib>Monato</bib>
          <lok>2006/08, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la regularo de la societo <klr>[&#8230;]</klr> severe el<tld/>is virseksulojn el la
        kunvenoj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        lar&gcirc;a di&gcirc;ita disdivido for<tld/>as multajn malri&ccirc;ajn landojn de la rimedoj de
        informteknologio
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;8-9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apart.0igi">apartigi</ref>,
      <ref cel="eksklud.0i">ekskludi</ref>,
      <ref cel="eksklu.0i">ekskluzivi</ref>,
      <ref cel="inkluz.mal0i">malinkluzivi</ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="las.al0i">allasi</ref>,
      <ref cel="las.en0i">enlasi</ref>,
      <ref cel="konduk.en0i.enirigi">enkonduki<sncref/></ref>,
      <ref cel="invit.0i.ien">inviti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; <ind>&c_b;&c_a;&c_r;&#8217;&c_je;&c_r;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aussperren</trd>,
    <trd>ausschlie&szlig;en</trd>
  </trdgrp>
</drv>

