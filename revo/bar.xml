<drv mrk="bar.0ilo">
  <kap><ofc>*</ofc><tld/>ilo
  <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-4980">bar'il'</url></lok></fnt></kap>
  <snc mrk="bar.0ilo.HOR">
    <dif>
      <ref tip="sub" cel="palis.0aro">Palisaro</ref>, <ref tip="sub"
      cel="krad.0o.KOMUNE">krado</ref>,
      <ref cel="mur.0o.ARKI">muro</ref> apartiganta lokon de alia:
      <ekz>
        vojeto inter vinber&gcirc;ardenoj, kie estis <tld/>ilo sur unu flanko kaj <tld/>ilo
        sur la dua flanko
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 22:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        pika&jcirc;a <tld/>ilo
        <fnt>
          <bib>MT</bib>
          <lok>Mi&hcirc;a 7:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>palisara <tld/>ilo</ind>
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
        <trd lng="de">Palisadenzaun</trd>
      </ekz>
      <ekz>
        densa <tld/>ilo el traboj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        alta <ind>lata <tld/>ilo</ind>
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
        <trd lng="de">Lattenzaun</trd>
      </ekz>
      <ekz>
        la &gcirc;ardenon &ccirc;irka&ubreve;is fera <ind>krada <tld/>ilo</ind>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
        <trd lng="de">Gitterzaun</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        aerveturo <klr>[&#8230;]</klr>, helpite de la lingvo internacia, malaperigos la
        <tld/>ilojn, kiuj ankora&ubreve; staras inter popoloj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        malanta&ubreve; <tld/>ilo kura&gcirc;o estas facila
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barier.0o">bariero</ref>,
      <ref cel="lim.0o">limo</ref>,
      <ref cel="barika.0o">barikado</ref>
    </refgrp>
    <trdgrp lng="be">
