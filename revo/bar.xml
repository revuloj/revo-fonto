<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bar.xml,v 1.79 2024/01/06 21:24:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bar</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="bar.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bar.0i.vojon">
    <dif>
      <ref tip="super" cel="ferm.0i.eniro">Fermi</ref> vojon por
      malhelpi la trairon:
      <ekz>
        kiam li venis al la gardistoj, ili volis <tld/>i al li la vojon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li devis <tld/>i al la atakantoj la revenon al ilia urbo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi kuntrenis kun vi en tiun mondon ian pekon, kiu <tld/>as al vi la eniron en
        la paradizon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>?
      </ekz>
      <ekz>
        <tld/>i la eliradon kaj eniradon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 15:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        krio de malespero, tiel la&ubreve;ta, ke nenia mano povis <tld/>i la orelon, estis
        a&ubreve;dita
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        koto levi&gcirc;is tre alte <klr>[&#8230;]</klr> kaj <tld/>is la rivereton, kiu disfluante
        komencis formi brikokoloran lagon
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="br">stanka&ntilde;<klr tip="ind"> (un hent)</klr></trd>
    <trd lng="de">sperren</trd>
    <trd lng="es">bloquear</trd>
    <trdgrp lng="fr">
      <trd>barrer<klr tip="ind"> (un passage)</klr></trd>,
      <trd>bloquer<klr tip="ind"> (un passage)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elker&iacute;t</trd>,
      <trd>eltorlaszol</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>grodzi&cacute;</trd>,
      <trd>zagradza&cacute;</trd>,
      <trd>odgradza&cacute;</trd>,
      <trd>zastawia&cacute;</trd>,
      <trd>tarasowa&cacute;</trd>,
      <trd>zatarasowa&cacute;</trd>,
      <trd>stawia&cacute; <ind>przeszkod&eogonek;</ind></trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x95BC; <pr>&#xE8;</pr></trd>,
      <trd>&#x5C01;&#37782; <pr>f&emacron;ngsu&#x1D2;</pr></trd>,
      <trd>&#x95A1; <pr>h&#xE9;</pr></trd>,
      <trd>&#31105;&#x6B62;&#x901A;&#x884C; <pr>j&#xEC;nzh&#x1D0;t&#x14D;ngx&#xED;ng</pr></trd>,
      <trd>&#25233;&#21046; <pr>y&#xEC;zh&#xEC;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="bar.0i.malhelpi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="help.mal0i">Malhelpi</ref>:
      <ekz>
        <tld/>i la progreson al la konkurenco, al la kalumnio;
      </ekz>
      <ekz>
        kio <tld/>as la re&gcirc;an ordonon
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>?
      </ekz>
      <ekz>
        mi volas &ccirc;ion &ccirc;irka&ubreve; mi ekstermi, kio <tld/>as al mi la eblon esti mastro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as al si la sanan naturon per sensencaj konvencioj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        de unu flanko <tld/>as al vi la vojon via propra ne sufi&ccirc;a prepariteco, de la
        alia flanko la kutimo, la manko de iniciativo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;uste tiu &ccirc;armo <tld/>is la profesian reagon
        <fnt>
          <bib>ChM</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iom post iom persvadis &scirc;in pri &scirc;ia misio <tld/>i ilian progreson
        <fnt>
          <bib>ChC</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu principo ne <tld/>as kunlaboron kun diverstendencaj organiza&jcirc;oj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io, kio nun povis <tld/>i tiun &ccirc;i preparon, devis fori&gcirc;i el &scirc;ia vivo
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>skoilha&ntilde;</trd>,
      <trd>mirout ouzh</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>versperren</trd>, <trd>verhindern</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>impedir</trd>,
      <trd>obstaculizar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>barrer <klr tip="ind">(emp&ecirc;cher)</klr></trd>,
      <trd>emp&ecirc;cher</trd>,
      <trd>faire <ind>obstacle</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>akad&aacute;lyoz</trd>, <trd>korl&aacute;toz</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>stawia&cacute; <ind>przeszkod&eogonek;</ind></trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x95A1; <pr>h&#xE9;</pr></trd>,
      <trd>&#x5C01;&#37782; <pr>f&emacron;ngsu&#x1D2;</pr></trd>,
      <trd>&#x59A8;&#x788D; <pr>f&#x1CE;ng&#xE0;i</pr></trd>,
      <trd>&#x50BA; <pr>ch&#xEC;</pr></trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="embara.0i">embarasi</ref>,
    <ref cel="akcel">malakceli</ref>,
    <ref cel="obstru.0i">obstrukci</ref>,
    <ref cel="sxtop.0i">&scirc;topi</ref>,
    <ref cel="brid.0i">bridi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">hradit</trd>
  <trdgrp lng="en">
    <trd>obstruct</trd>,
    <trd>block</trd>,
    <trd>bar</trd>
  </trdgrp>
  <trd lng="he">&lamed;&het;&samekh;&vav;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#36947;&#12434;&#12405;&#12373;&#12368;  <pr>&#12393;&#12358;&#12434;&#12405;&#12373;&#12368;</pr></trd>,
    <trd>&#x59A8;&#12370;&#x308B;  <pr>&#12373;&#x307E;&#x305F;&#12370;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hati&tcaron;</trd>,
    <trd>oploti&tcaron;</trd>,
    <trd>prehradi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blokovat</trd>,
    <trd>oplocovat</trd>,
    <trd>p&rcaron;ehradit</trd>,
    <trd>p&rcaron;ek&aacute;&zcaron;et</trd>,
    <trd>tarasit</trd>,
    <trd>zastoupit <klr>(cestu)</klr></trd>,
    <trd>zatarasit</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_i;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_i;&c_n;&c_i;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_sh;&c_k;&c_o;&c_d;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="bar.0o">
  <kap><tld/>o</kap>
  <snc mrk="bar.0o.KOMUNE">
    <dif>
      Io, kio <tld/>as a&ubreve; malhelpas:
      <ekz>
        ili trarompos la <tld/>on, trapasos la pordegon
        <fnt>
          <bib>MT</bib>
          <lok>Mi&hcirc;a 2:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi renkontis preska&ubreve; nevenkeblan <tld/>on: mi devas dungi bonan advokaton kaj mi
        tute ne havas monon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi faras <tld/>on al via propra liberi&gcirc;o, se vi ka&scirc;as vian doloron anta&ubreve; via
        amiko
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brika <tld/>o disfaladis, <klr>[&#8230;]</klr> apud la templo estis neniu
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mallar&gcirc;a fenestreto provizita je feraj <tld/>oj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        li krampi&gcirc;is al la veturilo <klr>[sed]</klr> liaj fingroj malkuni&gcirc;is kaj la
        malfeli&ccirc;ulo, lasante la apogan <tld/>on, falis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio faligis &ccirc;iujn sociajn <tld/>ojn, <klr>[&#8230;]</klr> anta&ubreve; la altaro de Bak&hcirc;o
        &ccirc;iuj estis egalaj
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Viktoro koleris kontra&ubreve; la <tld/>o, kaj ne akceptis, ke volado alia ol la lia
        ordonu
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de l' komenco
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Al la &leftquot;Esperantisto&rightquot;</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Fundamento <klr>[&#8230;]</klr> ne prezentas ian <tld/>on kontra&ubreve; la evolucio de la
        lingvo
        <fnt>
          <bib>LR</bib>
          <lok>Tria Parto &ndash; Jurisprudenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        lingvajn <tld/>ojn oni povas venki nur per lernado de fremdaj lingvoj
        <fnt>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2007/009137.php">Helpi financi edukadon por e&ubreve;ropa
          konscio</url></vrk>
              <bib>Monato</bib>
          <lok>2007/03, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro limoj kaj <tld/>oj malpacas
        najbaroj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        gardu vin du <tld/>oj, lipoj kaj dentaroj
        <klr>(parolu nur singarde)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barika.0o">barikado</ref>,
      <ref cel="bar.0ilo">barilo</ref>,
      <ref cel="blokad.0o">blokado</ref>,
      <ref cel="embara.0o">embaraso</ref>,
      <ref cel="kontraux.0ajxo">kontra&ubreve;a&jcirc;o</ref>,
      <ref cel="help.mal0ajxo">malhelpa&jcirc;o</ref>,
      <ref cel="mur.0o">muro</ref>,
      <ref cel="obstak.0o">obstaklo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>
    <trdgrp lng="cs">
      <trd>hradba</trd>,
      <trd>p&rcaron;ek&aacute;&zcaron;ka</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>skoilh</trd>,
      <trd>harz<klr tip="ind"> (skoilh)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sperre</trd>,
      <trd>Hindernis</trd>,
      <trd>Schranke</trd>,
      <trd>Barriere</trd>,
      <trd>Hemmnis</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>obstruction</trd>,
      <trd>barrier</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>obst&aacute;culo</trd>,
      <trd>barrera<klr tip="ind"> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>obstacle</trd>,
      <trd>barri&egrave;re<klr tip="ind"> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>akad&aacute;ly</trd>,
      <trd>korl&aacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>przeszkoda</trd>,
      <trd>ograniczenie</trd>,
      <trd>przegroda</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>
    <trdgrp lng="zh">
      <trd>&#x58C1;&#22418; <pr>b&#xEC;l&#x11B;i</pr></trd>,
      <trd>&#x680F;&#26629; <pr>l&#xE1;nsh&amacron;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="bar.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de subaro <frm am="bb A"><g>A</g></frm> de
      <ref cel="ar.orda0o">orda aro</ref>
      <frm am="(bb E,le)">(<g>E</g>,&#8804;)</frm>)</klr>
      Tia elemento <frm am="b"><k>b</k></frm> en
      <frm am="bb E"><g>E</g></frm>, ke por
      &ccirc;iu <frm am="x"><k>x</k></frm> en <frm am="bb A"><g>A</g></frm>
      veras <frm am="x le b"><k>x</k> &#8804; <k>b</k></frm>:
      <ekz>
        <ref tip="sub" cel="bar.suba0o">suba <tld/>o</ref>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="bar.supera0o">supera <tld/>o</ref>;
      </ekz>
      <ekz>
        la aro de reeloj akceptas nek superan, nek suban <tld/>on;
      </ekz>
      <ekz>
        <tld/>o de bildigo <klr>(t.e. de &gcirc;ia bildaro)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="suprem.0o">supremo</ref>,
      <ref cel="infim.0o">infimo</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="maksim1.0o">maksimumo</ref>,
      <ref cel="minimu.0o">minimumo</ref>
    </refgrp>
    <trd lng="de">Schranke</trd>
    <trd lng="en">bound</trd>
    <trdgrp lng="es">
      <trd>mayorante</trd>,
      <trd>minorante</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>majorant</trd>,
      <trd>minorant</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>korl&aacute;t</trd>,
      <trd>hat&aacute;r</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>element ograniczaj&aogonek;cy</trd>
      <trd>granica</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
    <trd lng="zh">&#x754C; <pr>ji&#xE8;</pr></trd>
  </snc>
  <ref tip="hom" cel="bar1.0o">baro (mezurunuo)</ref>
  <trdgrp lng="cs">
    <trd>hranice</trd>,
    <trd>ohrani&ccaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="he">&mem;&het;&samekh;&vav;&fmem;</trd>
  <trd lng="ja">&#x30D0;&#x30FC;&#x30EB;</trd>
  <trdgrp lng="sk">
    <trd>hranica</trd>,
    <trd>ohrani&ccaron;enie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="bar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bar.0ajxo.KOMUNE">
    <dif>
      <tld lit="B"/>o, a&jcirc;o kiu <tld/>as a&ubreve; malhelpas:
      <ekz>
        <tld/>a&jcirc;ojn por &scirc;afoj <klr>(kp <ref tip="malprt" cel="kral.0o">kralo</ref>)</klr>
        ni konstruos &ccirc;i tie
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 32:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        junulo alkuris al mi &gcirc;oje saltante trans miloj da <tld/>a&jcirc;oj sur la bordo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Unua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj lingve maturaj esperantistoj tuj komprenos vin, <klr>[&#8230;]</klr> ne staras
        <tld/>a&jcirc;o
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>6. Ies Malfido, Fido Mia</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ekestas]</klr> akvomanko &ccirc;irka&ubreve; la lago Viktorio <klr>[&#8230;]</klr> pro la
        konstruo de novaj <tld/>a&jcirc;oj
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2010/010051.php">Monda akvo fluas en malbonaj direktoj</url></vrk>
              <bib>Monato</bib>
          <lok>2010/07, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam falis la fifama berlina muro <klr>[&#8230;]</klr> oni forigis ne nur la
        <tld/>a&jcirc;ojn sed intertempe anka&ubreve; &ccirc;iujn minojn
        <fnt>
