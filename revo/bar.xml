<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bar</rad>/i
</kap>

<drv mrk="bar.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bar.0i.vojon">
    <dif>
      Fermi vojon por malhelpi la trairon:
      <ekz>
        <tld/>i straton, komunikon, la eniradon al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la vojon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la revenon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la riveron<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la fluon de la rivero.
      </ekz>
    </dif>
    <trd lng="br">stanka&ntilde;<klr tip="ind"> (un hent)</klr></trd>
    <trd lng="de">sperren</trd>
    <trdgrp lng="fr">
      <trd>barrer<klr tip="ind"> (un passage)</klr></trd>,
      <trd>bloquer<klr tip="ind"> (un passage)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elker&iacute;t</trd>,
      <trd>eltorlaszol</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>grodzi&cacute;</trd>,
      <trd>zagradza&cacute;</trd>,
      <trd>odgradza&cacute;</trd>,
      <trd>zastawia&cacute;</trd>,
      <trd>tarasowa&cacute;</trd>,
      <trd>zatarasowa&cacute;</trd>,
      <trd>stawia&cacute; <ind>przeszkod&eogonek;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="bar.0i.malhelpi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhelpi:
      <ekz>
        <tld/>i la progreson al la konkurenco, al la kalumnio;
      </ekz>
      <ekz>
        kio <tld/>as la re&gcirc;an ordonon?<fnt>Z</fnt>
      </ekz>
      <ekz>
        kontra&ubreve;<tld/>i la vojon al &ccirc;iuj reformoj.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>skoilha&ntilde;</trd>,
      <trd>mirout ouzh</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>versperren</trd>, <trd>verhindern</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>barrer <klr tip="ind">(emp&ecirc;cher)</klr></trd>,
      <trd>emp&ecirc;cher</trd>,
      <trd>faire <ind>obstacle</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>akad&aacute;lyoz</trd>, <trd>korl&aacute;toz</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>stawia&cacute; <ind>przeszkod&eogonek;</ind></trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="embara.0i">embarasi</ref>,
    <ref cel="akcel">malakceli</ref>,
    <ref cel="obstru.0i">obstrukci</ref>,
    <ref cel="sxtop.0i">&scirc;topi</ref>,
    <ref cel="brid.0i">bridi</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">hradit</trd>
  <trdgrp lng="en">
    <trd>obstruct</trd>,
    <trd>block</trd>,
    <trd>bar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hatiť</trd>,
    <trd>oplotiť</trd>,
    <trd>prehradiť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blokovat</trd>,
    <trd>oplocovat</trd>,
    <trd>přehradit</trd>,
    <trd>překážet</trd>,
    <trd>tarasit</trd>,
    <trd>zastoupit <klr>(cestu)</klr></trd>,
    <trd>zatarasit</trd>
  </trdgrp>
</drv>

<drv mrk="bar.0o">
  <kap><tld/>o</kap>
  <snc mrk="bar.0o.KOMUNE">
    <dif>
      Io, kio <tld/>as a&ubreve; malhelpas:
      <ekz>
        pro limoj kaj <tld/>oj malpacas
        najbaroj<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        gardu vin du <tld/>oj, lipoj kaj dentaroj
        <klr>(parolu nur singarde)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        Esperanto forigas la lingvajn <tld/>ojn;
      </ekz>
      <ekz>
        la Fundamento ne prezentas ian <tld/>on kontra&ubreve; la
        evolucio de la lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pro multaj materiaj malhelpoj kaj <tld/>oj la laboro haltis.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mur.0o">muro</ref>.
    </refgrp>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>
    <trdgrp lng="cs">
      <trd>hradba</trd>,
      <trd>p&rcaron;ek&aacute;&zcaron;ka</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>skoilh</trd>,
      <trd>harz<klr tip="ind"> (skoilh)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sperre</trd>, <trd>Hindernis</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>obstruction</trd>,
      <trd>barrier</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>obstacle</trd>,
      <trd>barri&egrave;re<klr tip="ind"> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>akad&aacute;ly</trd>,
      <trd>korl&aacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>przeszkoda</trd>,
      <trd>ograniczenie</trd>,
      <trd>przegroda</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="bar.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de subaro <frm><g>A</g></frm> de
      <ref cel="ar.orda0o">orda aro</ref>
      <frm>(<g>E</g>,&#8804;)</frm>)</klr>
      Tia elemento <frm><k>b</k></frm> en <frm><g>E</g></frm>, ke por
      &ccirc;iu <frm><k>x</k></frm> en <frm><g>A</g></frm>
      veras <frm><k>x</k> &#8804; <k>b</k></frm>:
      <ekz>
        <ref tip="sub" cel="bar.suba0o">suba <tld/>o</ref>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="bar.supera0o">supera <tld/>o</ref>;
      </ekz>
      <ekz>
        la aro de reeloj akceptas nek superan, nek suban <tld/>on;
      </ekz>
      <ekz>
        <tld/>o de bildigo <klr>(t.e. de &gcirc;ia bildaro)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="suprem.0o">supremo</ref>,
      <ref cel="infim.0o">infimo</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="maksim1.0o">maksimumo</ref>,
      <ref cel="minimu.0o">minimumo</ref>.
    </refgrp>
    <trd lng="de">Schranke</trd>
    <trd lng="en">bound</trd>
    <trdgrp lng="fr">
      <trd>majorant</trd>,
      <trd>minorant</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>korl&aacute;t</trd>,
      <trd>hat&aacute;r</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>element ograniczaj&aogonek;cy</trd>
      <trd>granica</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hranice</trd>,
    <trd>ohraničení</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hranica</trd>,
    <trd>ohraničenie</trd>
  </trdgrp>
</drv>

<drv mrk="bar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bar.0ajxo.KOMUNE">
    <dif>
      <tld lit="B"/>o, a&jcirc;o kiu baras a&ubreve; malhelpas:
      <ekz>
        konstrui <tld/>a&jcirc;on en la rivero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0ilo">barilo</ref>,
      <ref cel="baston.0o">bastono</ref>,
      <ref cel="trab.0o">trabo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_t;</trd>,
      <trd>&c_b;&c_a;&c_r;'&c_je;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>stankell</trd>,
      <trd>bardell</trd>,
      <trd>fardell</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sperre</trd>,
      <trd>Damm</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>obstruction</trd>,
      <trd>blockage</trd>
    </trdgrp>
    <trd lng="fr">barrage</trd>
    <trd lng="hu">torlasz</trd>
    <trdgrp lng="pl">
      <trd>przeszkoda</trd>,
      <trd>ograniczenie</trd>,
      <trd>przegroda</trd>,
      <trd>bariera</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_b;&c_o;&c_r;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_b;&c_a;&c_r;&c_mol;&c_je;&c_r;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="bar.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="bar.0ilo.HOR">
    <dif>
      Palisaro, krado, muro apartiganta lokon de alia:
      <ekz>
        malanta&ubreve; <tld/>ilo kura&gcirc;o estas facila
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barier.0o">bariero</ref>,
      <ref cel="lim.0o">limo</ref>,
      <ref cel="barika.0o">barikado</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_t;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_l;&c_je;&c_t;</trd>
    <trdgrp lng="br">
      <trd>draf</trd>,
      <trd>kloued</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Absperrung</trd>,
      <trd>Barriere</trd>,
      <trd>Zaun</trd>,
      <trd>Schranke</trd>
      <trd>Sperre</trd>,
    </trdgrp>
    <trdgrp lng="en">
      <trd>obstruction</trd>,
      <trd>barrier</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cl&ocirc;ture</trd>,
      <trd>barri&egrave;re</trd>
    </trdgrp>
    <trd lng="hu">ker&iacute;t&eacute;s</trd>
    <trdgrp lng="pl">
      <trd>p&lstroke;ot</trd>,
      <trd>ogrodzenie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_b;&c_o;&c_r;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="bar.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="bar.0ita.KOMUNE">
    <dif>
      <ekz>
        &ccirc;iuj alirejoj al la palaco estis <tld/>itaj de la
        soldatoj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>bounded</trd>
      <trd>cordoned off</trd>
      <trd>fenced off</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lez&aacute;rt</trd>
      <trd>elker&iacute;tett</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ogrodzony</trd>,
      <trd>ograniczony</trd>,
      <trd>zagrodozny</trd>,
      <trd>odgrodzony</trd>,
      <trd>zastawiony</trd>,
      <trd>zatarasowany</trd>,
      <trd>zastawiony</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_g;&c_o;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bar.0ita.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;44</lok></fnt>
    <subsnc mrk="bar.0ita.subaro">
      <dif>
        <klr>(p.p. subaro <frm><g>A</g></frm> de
        <ref cel="ar.orda0o">orda aro</ref>)</klr>
        Tia, ke &gcirc;i akceptas <ref cel="bar.0o.MAT"><tld/>on</ref>:
        <ekz>
          &ccirc;iu <tld/>ita subaro de la aro de naturaj entjeroj
          akceptas
          maksimumon;
        </ekz>
        <ekz>
          la aro de &ccirc;iuj reeloj inter <frm>0</frm> kaj
          <frm>1</frm>
          estas <tld/>ita.
        </ekz>
      </dif>
      <rim>
        Se la konsiderata ordo-rilato estas
        <ref cel="tut.0eca.rilato">tuteca</ref> kaj signata per
        <frm>&#8804;</frm>, oni kutime postulas, ke la subaro akceptu
        kaj
        superan kaj suban <tld/>ojn.
      </rim>
    </subsnc>
    <subsnc mrk="bar.0ita.bildigo">
      <dif>
        <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref> al
        <ref cel="ar.orda0o">orda aro</ref>)</klr>
        Tia, ke &gcirc;ia bildaro estas <tld/>ita
        <sncref ref="bar.0ita.subaro"/>.
      </dif>
    </subsnc>
    <subsnc mrk="bar.0ita.metrikaspaco">
      <dif>
        <klr>(p.p. subaro de <ref cel="spac.metrika0o">metrika
        spaco</ref>)</klr>
        Tia, ke &gcirc;in inkluzivas
        <ref cel="glob.0o.metrikaspaco">globo</ref>.
      </dif>
    </subsnc>
    <trd lng="br">bonnet<klr tip="ind"> (mat.)</klr></trd>
    <trd lng="de">beschr&auml;nkt</trd>
    <trd lng="en">bounded</trd>
    <trd lng="fr">born&eacute;<klr tip="ind"> (math.)</klr></trd>
    <trd lng="hu">korl&aacute;tos<klr tip="ind"> (pl.
    halmaz)</klr></trd>
    <trd lng="pl">ograniczony</trd>
    <trd
    lng="ru">&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd
  lng="be">&c_a;&c_b;&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trd lng="cs">ohraničený</trd>
  <trd lng="sk">ohraničený</trd>
</drv>

<drv mrk="bar.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bar.cxirkaux0i.fermi">
    <dif>
      &Ccirc;irka&ubreve;igi per <tld/>iloj:
      <ekz>
        ili ludis en la flanka aleo kaj &ccirc;irka&ubreve;<tld/>is
        per palisetoj sur la tero malgrandan &gcirc;ardenon
        <fnt><bib>Fab2</bib><lok>Sambuka virineto</lok></fnt>;
      </ekz>
      <ekz>
        Li &ccirc;irka&ubreve;<tld/>is min, ke mi ne povu eliri
        <fnt><bib>MT</bib><lok>&Plo; 3:7</lok></fnt>;
      </ekz>
      <ekz>
        oni devas kiel eble plej rapide &ccirc;irka&ubreve;<tld/>i
        <klr>[la &ccirc;asa&jcirc;on]</klr>, &ccirc;ar &gcirc;i povus
        formalaperi
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        en dol&ccirc;incita malkvieto nin &ccirc;irka&ubreve;<tld/>is
        kvaza&ubreve; sor&ccirc;a ring'
        <fnt><aut>P. Peneter</aut><vrk>Sekretaj
        Sonetoj</vrk><lok>4</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ferm.cxirkaux0i">&ccirc;irka&ubreve;fermi</ref>
    <trd lng="de">umz&auml;unen</trd>
    <trd lng="hu">k&ouml;rbeker&iacute;t</trd>
    <trdgrp lng="pl">
      <trd>ogradza&cacute;</trd>,
      <trd>odgradza&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bar.cxirkaux0i.malhelpi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;iuflanke haltigi, &ccirc;iumaniere malhelpi:
      <ekz>
        la viro regas, sed la virinon &ccirc;io
        &ccirc;irka&ubreve;<tld/>as<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>surround</trd>
    </trdgrp>
    <trd lng="hu">minden oldalr&oacute;l
    <ind>akad&aacute;lyoz</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>mogeria&ntilde;</trd>,
    <trd>bac'ha&ntilde;</trd>,
    <trd>kelc'hia&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ceindre<klr tip="ind"> (entourer d'une
    barri&egrave;re)</klr></trd>,
    <trd>confiner<klr tip="ind"> (entourer d'une
    barri&egrave;re)</klr></trd>
    <trd>enceindre</trd>,
    <trd>encercler</trd>,
    <trd>enclore</trd>,
    <trd>enfermer</trd>,
    <trd>enserrer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="bar.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bar.de0i.KOMUNE">
    <dif>
      Apartigi per <tld/>ilo:
      <ekz>
        lia patra domo staris ankora&ubreve; kiel anta&ubreve;e,
        sed la &gcirc;ardeno estis parte de<tld/>ita
        <fnt><bib>Fab3</bib><lok>Nokta &ccirc;apo de
        fra&ubreve;lo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="krad.de0i">dekradi</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;limiter<klr tip="ind"> (par une
      cl&ocirc;ture)</klr></trd>,
      <trd>enclore<klr tip="ind"> (d&eacute;limiter)</klr></trd>,
      <trd>clore<klr tip="ind"> (d&eacute;limiter)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ogradza&cacute;</trd>,
      <trd>grodzi&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_g;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>odhradit</trd>,
    <trd>odstranit překážku</trd>
  </trdgrp>
  <trd lng="sk">odstrániť prekážku</trd>
</drv>

<drv mrk="bar.dis0i">
  <kap>dis<tld/>i<fnt>Z</fnt></kap>
  <snc mrk="bar.dis0i.KOMUNE">
    <dif>
      Apartigi per <tld/>o.
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>segregate</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;limiter<klr tip="ind"> (par des
    cl&ocirc;tures)</klr></trd>
    <trd lng="hu">elker&iacute;t<klr tip="ind">
    (sz&eacute;tv&aacute;laszt)</klr></trd>
    <trdgrp lng="pl">
      <trd>ogradza&cacute;</trd>,
      <trd>zastawia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">odhradit <klr>(čím| co)</klr></trd>
  <trdgrp lng="sk">
    <trd>oddeliť</trd>,
    <trd>prehradiť</trd>
  </trdgrp>
</drv>

<drv mrk="bar.ne0ita">
  <kap>ne<tld/>ita</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="bar.ne0ita.MAT">
    <ref tip="ant" cel="bar.0ita.MAT"><tld/>ita.</ref>
    <trd
    lng="be">&c_n;&c_je;&c_a;&c_b;&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
    <trd lng="br">divonn<klr tip="ind"> (mat.)</klr></trd>
    <trd lng="de">unbeschr&auml;nkt</trd>
    <trd lng="en">unbounded</trd>
    <trd lng="fr">non born&eacute;<klr tip="ind"> (math.)</klr></trd>
    <trdgrp lng="hu">
      <trd>korl&aacute;tlan<klr tip="ind"> (pl. halmaz)</klr></trd>,
      <trd>hat&aacute;rtalan<klr tip="ind"> (pl. halmaz)</klr></trd>
    </trdgrp>
    <trd lng="pl">nieograniczony</trd>
    <trd
    lng="ru">&c_n;&c_je;&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="cs">neohraničený</trd>
  <trd lng="sk">neohraničený</trd>
</drv>

<drv mrk="bar.plekto0ilo">
  <kap>plekto<tld/>ilo</kap>
  <snc mrk="bar.plekto0ilo.HOR">
    <dif>
      <tld lit="B"/>ilo el densaj vivantaj arbetoj, a&ubreve;
      detran&ccirc;itaj bran&ccirc;etoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="hegx.0o">he&gcirc;o</ref>
    </refgrp>
    <ref tip="vid" cel="fascxi.0o">fas&ccirc;ino</ref>
    <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_mol;</trd>
    <trd lng="br">treilh</trd>
    <trd lng="de">Hecke</trd>
    <trdgrp lng="en">
      <trd>hegderow</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>treillis<klr tip="ind"> (cl&ocirc;ture)</klr></trd>,
      <trd>haie</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vessz&odblac;ker&iacute;t&eacute;s</trd>,
      <trd>s&ouml;v&eacute;nyker&iacute;t&eacute;s</trd>,
    </trdgrp>
    <trdgrp lng="pl">
      <trd>p&lstroke;ot</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_mol;</trd>
  </snc>
  <trd lng="cs">proplétaná překážka</trd>
  <trd lng="sk">prepletaná prekážka</trd>
</drv>

<drv mrk="bar.tabul0ilo">
  <kap>tabul<tld/>ilo</kap>
  <snc mrk="bar.tabul0ilo.HOR">
    <dif>
      <tld lit="B"/>ilo el lignaj tabuloj:
      <ekz>
        la kokina korto per tabul<tld/>ilo estis apartigita de alia
        korto
        <fnt><bib>Fab4</bib>, <lok>Korta koko kaj ventkoko</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="br">kael peulio&ugrave;</trd>
    <trd lng="de">Lattenzaun</trd>
    <trdgrp lng="en">
      <trd>wooden <ind>fence</ind></trd>
    </trdgrp>
    <trd lng="fr">palissade<klr tip="ind"> (de planches)</klr></trd>
    <trd lng="hu">deszkaker&iacute;t&eacute;s</trd>
    <trdgrp lng="pl">
      <trd>parkan</trd>,
      <trd>palisada</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_mol;<klr tip="amb">
      (&c_i;&c_z;
      &c_sh;&c_t;&c_a;&c_k;&c_je;&c_t;&c_n;&c_i;&c_k;&c_a;)</klr></trd>,
      <trd>&c_z;&c_a;&c_b;&c_o;&c_r;</trd>,
      <trd>&c_p;&c_a;&c_l;&c_i;&c_s;&c_a;&c_d;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">prkenný plot</trd>
  <trd lng="sk">doskový plot</trd>
</drv>

<drv mrk="bar.suba0o">
  <kap>suba <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de subaro <frm><g>A</g></frm> de
      <ref cel="ar.orda0o">orda aro</ref>
      <frm>(<g>E</g>,&#8804;)</frm>)</klr>
      <ref tip="super" cel="bar.0o.MAT"><tld lit="B"/>o</ref> de la sama
      aro, provizita per la inversa rilato:
      <ekz>
        la aro de &ccirc;iuj subaj baroj de la reela intervalo
        <frm>[0,1[</frm> estas la aro de strikte negativaj reeloj.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="bar.supera0o">supera <tld/>o</ref>.
    </rim>
    <trd lng="br">minorer</trd>
    <trd lng="de">untere <ind>Schranke</ind></trd>
    <trd lng="en">lower <ind>bound</ind></trd>
    <trd lng="fr">minorant</trd>
    <trdgrp lng="pl">
      <trd>element ograniczaj&aogonek;cy z do&lstroke;u</trd>
      <trd>granica dolna</trd>
    </trdgrp>
    <trd lng="hu">als&oacute; <ind>korl&aacute;t</ind></trd>
    <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja;
    <ind>&c_g;&c_r;&c_a;&c_n;&c_mol;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dolní limita</trd>,
    <trd>dolní závora</trd>,
    <trd>mez</trd>,
    <trd>spodní hranice</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dolná limita</trd>,
    <trd>medza</trd>,
    <trd>spodná hranica</trd>
  </trdgrp>
</drv>

<drv mrk="bar.supera0o">
  <kap>supera <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de subaro <frm><g>A</g></frm> de
      <ref cel="ar.orda0o">orda aro</ref>
      <frm>(<g>E</g>,&#8804;)</frm>)</klr>
      <ref tip="super" cel="bar.0o.MAT"><tld lit="B"/>o</ref> de
      &gcirc;i:
      <ekz>
        &ccirc;iu pozitiva reelo estas supera <tld/>o de la aro de
        negativaj reeloj.
      </ekz>
    </dif>
    <rim>
      La epitetoj <ctl>supera</ctl> a&ubreve; <ctl>suba</ctl> rilatas al
      ia
      intuicia prezento de la koncerna aro kaj de &gcirc;ia ordo.
      La&ubreve;
      la kunteksto eblas inter&scirc;an&gcirc;i ilian signifon,
      a&ubreve;
      paroli pri <ctl>dekstra <tld/>o</ctl>, <ctl>maldekstra
      <tld/>o</ctl> ks.
      Cetere, kelkaj opinias, ke al la paro supera/suba preferindus
      supra/malsupra. Bricard parolas pri <ctl>superrando</ctl> kaj
      <ctl>malsuperrando</ctl> en sufi&ccirc;e konfuza artikoleto
      <fnt><bib>MatTerm</bib><lok>p. 17-18</lok></fnt>,
      titolita <ctl>limoj kaj randoj</ctl>, kiu miksas la nociojn
      rilatajn al
      baroj (maksimumoj, minimumoj, supremoj, infimoj) kun nocioj
      rilataj
      al limesoj.
    </rim>
    <trd lng="br">majorer</trd>
    <trd lng="de">obere <ind>Schranke</ind></trd>
    <trd lng="en">upper <ind>bound</ind></trd>
    <trd lng="fr">majorant</trd>
    <trd lng="hu">fels&odblac; <ind>korl&aacute;t</ind></trd>
    <trdgrp lng="pl">
      <trd>element ograniczaj&aogonek;cy z g&oacute;ry</trd>,
      <trd>granica g&oacute;na</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;&c_ja;
    <ind>&c_g;&c_r;&c_a;&c_n;&c_mol;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>horní hranice</trd>,
    <trd>horní limita</trd>,
    <trd>horní závora</trd>
  </trdgrp>
  <trd lng="sk">horná hranica</trd>
</drv>
