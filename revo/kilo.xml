<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kilo.xml,v 1.6 2024/12/02 08:35:52 revo Exp $">
<kap>
  <rad>kilo</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kilo.0">
  <kap><tld/>-</kap>
  <mlg>k</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="kilo.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Mil&rightquot; (1000)
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        <ref cel="kilome.0o"><tld/>metro</ref>;
      </ekz>
      <ekz>
        <ref cel="kilovo.0o"><tld/>volto</ref>;
      </ekz>
      <ekz>
        ili povas atingi 90-<tld/>metran rapidon
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        1 mino = 1,5 <tld/>gramo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arta lago <klr>[&#8230;]</klr> okupis 300 <tld/>metrojn kvadratajn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        transformiloj je kvindek mil <tld/>vatoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;-</trd>
  <trd lng="bg">&c_k;&c_i;&c_l;&c_o;-</trd>
  <trdgrp lng="ca">
    <trd>kilo-</trd>
    <trd>quilo-</trd>,
  </trdgrp>
  <trd lng="de">Kilo-</trd>
  <trd lng="en">kilo-</trd>
  <trd lng="es">kil&oacute;-</trd>
  <trd lng="fa">&f_k;&f_y;&a_l;&a_w;</trd>
  <trd lng="fr">kilo-</trd>
  <trd lng="hu">kilo-</trd>
  <trd lng="it">chilo-</trd>
  <trd lng="ja">&#12461;&#12525;&#65374;  <pr>&#12365;&#12429;&#65374;</pr></trd>
  <trd lng="nl">kilo-</trd>
  <trd lng="no">kilo-</trd>
  <trd lng="pl">kilo-</trd>
  <trd lng="pt">quilo-</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;-</trd>
  <trd lng="sv">kilo-</trd>
</drv>

<drv mrk="kilo.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="mil.0o">Milo</ref> da, el subkomprenata mezuro (kutime gramoj);
      <tld/>gramo:
      <ekz>
        <tld/>o da gramoj, dolaroj, bitokoj;
      </ekz>
      <ekz>
        la <tld/>a <klr>(t.e. <tld/>grama)</klr> prezo estas 2 e&ubreve;roj;
      </ekz>
      <ekz>
        po kiom, la mandarinoj? &ndash; po tridek kopekoj por <tld/>o, &ndash; &scirc;i respondis
        <fnt>
          <aut>M. Bulgakov</aut>
          <vrk><url ref="http://miresperanto.com/biblioteko/margarita/28.htm"
              >Majstro kaj Margarita</url></vrk>
          <lok>&ccirc;ap. 28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kilo</trd>
</drv>

</art>

