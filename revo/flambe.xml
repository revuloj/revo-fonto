<vortaro>
<art mrk="$Id$">
<kap>
  <rad>flamberg</rad>/o
</kap>
<drv mrk="flambe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Speco de longa amba&ubreve;mana <ref tip="super" cel="glav.0o">glavo</ref>,
      kies klingo ofte havis ondan formon de flamo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flamberg</trd>,
    <trd>Landsknechtschwert mit geflammter Klinge</trd>
  </trdgrp>
  <trd lng="fr">flamberge</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_b;&c_je;&c_r;&c_g;</trd>
</drv>
</art>
