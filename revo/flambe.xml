<art mrk="$Id: flambe.xml,v 1.21 2023/08/31 13:11:29 revo Exp $">
<kap>
  <rad>flamberg</rad>/o
  <fnt>
    <bib>PV</bib>
  </fnt>
</kap>
<drv mrk="flambe.0o">
  <kap><tld/>o</kap>
  <snc mrk="flambe.0o.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
      Speco de longa amba&ubreve;mana <ref tip="super" cel="glav.0o.MIL">glavo</ref>,
      kies klingo ofte havis ondan formon de flamo:
      <ekz>
        en pluraj muzeoj de la svisa urbo <nom>Chur</nom> oni trovas diversajn <tld/>ojn el la jaroj
        1200 &gcirc;is
        1550.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/e0/Anciens_gardes_suisses_pontificaux_&agrave;_Lausanne_3.jpg/640px-Anciens_gardes_suisses_pontificaux_&agrave;_Lausanne_3.jpg" prm="CC BY-SA 3.0">Svisaj gardistoj kun <tld/>oj
        <fnt>
          <aut>Abaddon1337, propra verko</aut>
          <lok><url ref="&WCW;/File:Anciens_gardes_suisses_pontificaux_%C3%A0_Lausanne_3.jpg">&WCW;/File:Anciens_gardes_suisses_pontificaux_&agrave;_Lausanne_3.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_b;&c_e;&c_r;&c_g;</trd>
  <trd lng="cs">flamberg</trd>
  <trdgrp lng="de">
    <trd>Flamberg</trd>,
    <trd>Landsknechtschwert mit geflammter Klinge</trd>
  </trdgrp>
  <trd lng="fr">flamberge</trd>
  <trdgrp lng="hu">
    <trd>pallos</trd>,
    <trd>l&aacute;ngpallos</trd>,
    <trd>flamberzs</trd>
  </trdgrp>
  <trd lng="it">flamberga</trd>
  <trd lng="ja">&#x30D5;&#x30E9;&#x30F3;&#x30D9;&#x30EB;&#x30B8;&#x30E5;</trd>
  <trdgrp lng="pt">
    <trd>durindana</trd>,
    <trd>espadag&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_b;&c_je;&c_r;&c_g;</trd>
  <trd lng="sk">dlh&yacute; kord</trd>
  <trd lng="uk">&#x448;&#x43F;&#x430;&#x433;&#x430; &#x437; &#x491;&#x43E;&#x444;&#x440;&#x43E;&#x432;&#x430;&#x43D;&#x438;&#x43C; &#x43A;&#x43B;&#x438;&#x43D;&#x43A;&#x43E;&#x43C;</trd>
</drv>
