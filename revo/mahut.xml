<kap>
  <rad>mahut</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mahut.0o">
  <kap><tld/>o</kap>
  <snc mrk="mahut.0o.RAJ">
    <dif>
      Gvidisto kaj flegisto de malsova&gcirc;a elefanto:
      <ekz>
        &scirc;i estis... <tld/>o sur die ornamselita
        elefanto trairanta dezertojn &gcirc;is atingo
        de oazo
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">mahut</trd>
  <trd lng="fr">cornac</trd>
  <trdgrp lng="pl">
    <trd>kornak</trd>,
    <trd>poganiacz <ind>s&lstroke;oni</ind></trd>
  </trdgrp> 
  <trd lng="pt">cornaca</trd>
  <trd lng="ro">cornac</trd>
  <trd lng="sk">mahut</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x433;&#x43E;&#x43D;&#x438;&#x447; &#x441;&#x43B;&#x43E;&#x43D;&#x456;&#x432;</trd>
</drv>
</art>

