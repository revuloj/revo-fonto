<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konjun2.xml,v 1.18 2019/06/30 18:21:03 revo Exp $">
<kap>
  <rad>konjunktivit</rad>/o
  <fnt>FK</fnt>
</kap>
<drv mrk="konjun2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref cel="konjun4.0o">konjunktivo</ref>.
      <ekz>
        kronika katara <tld/>o
        <fnt>
          <bib>LR</bib>
          <lok>2. La verkaro de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e maljunuloj &gcirc;i estas preska&ubreve; regulo, ke ni trovas facilan gradon da kronika <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu ajn printempe spertas fortajn kaj da&ubreve;rajn malvarmumojn, <tld/>on, kapdoloron, ver&scirc;ajne estas alergiulo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010953.php">Zorga gardisto nenion preteratentas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_ju;&c_n;&c_k;&c_t;&c_y;&c_v;&c_ib;&c_t;</trd>
  <trd lng="ca">conjuntivitis</trd>
  <trdgrp lng="cs">
    <trd>konjunktivitida</trd>,
    <trd>konjunktivitis</trd>,
    <trd>z&aacute;n&ecaron;t spojivek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bindehautentz&uuml;ndung</trd>,
    <trd>Konjunktivitis</trd>
  </trdgrp>
  <trd lng="en">conjunctivitis</trd>
  <trd lng="es">conjuntivitis</trd>
  <trd lng="fr">conjonctivite</trd>
  <trd lng="id">konjungtivitis</trd>
  <trd lng="nl">oogvliesontsteking</trd>
  <trd lng="pl">spoj&oacute;wek zapalenie</trd>
  <trd lng="pt">conjuntivite</trd>
  <trd lng="ro">conjunctivit&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_malmol;&c_ju;&c_n;&c_k;&c_t;&c_i;&c_v;&c_i;&c_t;</trd>
  <trd lng="sk">z&aacute;pal spojiviek</trd>
</drv>
</art>
