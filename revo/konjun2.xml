<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konjunktivit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konjun2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref cel="konjun4.0o">konjunktivo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_ju;&c_n;&c_k;&c_t;&c_y;&c_v;&c_ib;&c_t;</trd>
  <trd lng="ca">conjuntivitis</trd>
  <trdgrp lng="de">
    <trd>Bindehautentz&uuml;ndung</trd>,
    <trd>Konjunktivitis</trd>
  </trdgrp>
  <trd lng="en">conjunctivitis</trd>
  <trd lng="es">conjuntivitis</trd>
  <trd lng="fr">conjonctivite</trd>
  <trd lng="id">konjungtivitis</trd>
  <trd lng="nl">oogvliesontsteking</trd>
  <trd lng="pt">conjuntivite</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_malmol;&c_ju;&c_n;&c_k;&c_t;&c_i;&c_v;&c_i;&c_t;</trd>
</drv>
</art>
