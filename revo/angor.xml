<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>angor</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="angor.0o">
   <kap><tld/>o</kap>
   <snc num="1">
     <dif>
       Malsaneca sento de sufoko kun korpremateco:
       <ekz>
         <tld/>oj  de la agonio.
       </ekz>
     </dif>
   </snc>
   <trd lng="it">angoscia</trd>
   <snc num="2">
     <dif>
       Sento de sufoko pro timego, maltrankvilego:
       <ekz>
         en horo de <tld/>o<fnt>Z</fnt>;
       </ekz>
       <ekz>
         plena de <tld/>o kaj teruro
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         Najbaraj familioj</vrk>)</fnt>;
       </ekz>
       <ekz>
         la morto de mia patro naskis en mi doloran <tld/>on.
       </ekz>
     </dif>
   </snc>
<trd lng="fr">angoisse</trd>
</drv>
<drv mrk="angor.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Senti <tld/>on.
     </dif>
   </snc>
   <trd lng="fr">&ecirc;tre <ind>angoiss&eacute;</ind></trd>
   <trd lng="it">angosciarsi</trd>
</drv>
<drv mrk="angor.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Timplena, premsenta.
     </dif>
   </snc>
   <trd lng="fr">anxieux</trd>
   <trd lng="it">angoscioso</trd>
</drv>
</art>
