<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>angor</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="angor.0o">
   <kap><tld/>o</kap>
   <snc num="1">
     <dif>
       Malsaneca sento de sufoko kun korpremateco:
       <ekz>
         <tld/>oj  de la agonio.
       </ekz>
     </dif>
   </snc>
   <trd lng="it">angoscia</trd>
   <snc num="2">
     <dif>
       Sento de sufoko pro timego, maltrankvilego:
       <ekz>
         en horo de <tld/>o<fnt>Z</fnt>;
       </ekz>
       <ekz>
         plena de <tld/>o kaj teruro
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         Najbaraj familioj</vrk>)</fnt>;
       </ekz>
       <ekz>
         la morto de mia patro naskis en mi doloran <tld/>on.
       </ekz>
     </dif>
   </snc>
<trdgrp lng="be">
  <trd>&c_s;&c_m;&c_u;&c_t;&c_a;&c_k;</trd>, 
  <trd>&c_t;&c_u;&c_g;&c_a;</trd>, 
  <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
</trdgrp>
<trd lng="fr">angoisse</trd>
</drv>
<drv mrk="angor.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Senti <tld/>on.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_m;&c_u;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
     <trd>&c_t;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>, 
     <trd>&c_n;&c_je;&c_p;&c_a;&c_k;&c_o;i&c_c;&c_c;&c_a;</trd>
   </trdgrp>
   <trd lng="fr">&ecirc;tre <ind>angoiss&eacute;</ind></trd>
   <trd lng="it">angosciarsi</trd>
</drv>
<drv mrk="angor.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Timplena, premsenta.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>, 
     <trd>&c_t;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>, 
     <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="fr">anxieux</trd>
   <trd lng="it">angoscioso</trd>
</drv>
</art>
