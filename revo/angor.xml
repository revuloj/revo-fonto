<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: angor.xml,v 1.25 2022/09/06 16:46:32 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>angor</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="angor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsaneca sento de sufoko kun 
      <ref tip="sin" cel="prem.kor0ateco">korpremateco</ref>:
      <ekz>
        tago de mal&gcirc;ojo kaj de <tld/>o, tago de teruro
        <fnt>
          <bib>MT</bib>
          <lok>Cefanja 1:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> portadis mem en la longa nokto sian malsanan infanon
        <klr>[&#8230;]</klr> kaj pre&gcirc;is por &gcirc;i al Dio, kiel patrino pre&gcirc;as en la horo de
        <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Plej bela rozo de la mondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        virina figuro kun morta <tld/>o sur la viza&gcirc;o <klr>[&#8230;]</klr> kun manoj
        interfingrigitaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        neniam &scirc;ia viza&gcirc;o estis <klr>[&#8230;]</klr> tiel flave pala kiel nun, kaj &gcirc;iaj
        trajtoj montris tiom da <tld/>o <klr>[&#8230;]</klr>
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia koro estis dis&scirc;irita kaj plenigita de la plej maldol&ccirc;a <tld/>o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;dan&gcirc;ero!&rightquot; &ndash; ekkriis subite la longnaza <nom>Stern</nom>
        kun gestoj de morta <tld/>o
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        freneziga <tld/>o <klr>[&#8230;]</klr> premegis &scirc;ian koron
        <fnt>
          <aut>Hans Weinhengst</aut>,
          <vrk>Tur-Strato 4</vrk>,
          <lok><url ref="http://www.fadedpage.com/showbook.php?pid=20131216"
          >Fadedpage</url>
          </lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="tim.0ego">timego</ref>
    <trdgrp lng="de">
      <trd>Beklemmung</trd>,
      <trd>Enge</trd>,
      <trd>Angina pectoris</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_u;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="bo">&t_zha;&t_drengbu;&t_da;&t_tsheg;&t_sa;&t_nata;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>strach</trd>,
    <trd>t&iacute;se&ncaron;</trd>,
    <trd>&uacute;zkost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bedr&auml;ngnis</trd>,
    <trd>Drangsal</trd>,
    <trd>Pein</trd>,
    <trd>beklemmende <ind>Angst</ind></trd>
  </trdgrp>
  <trd lng="en">distress</trd>
  <trd lng="es">angustia</trd>
  <trd lng="fr">angoisse</trd>
  <trd lng="he">&het;&#1458;&resh;&#1464;&dalet;&#1464;&he;</trd>
  <trd lng="it">angoscia</trd>
  <trdgrp lng="nl">
    <trd>angor</trd>,
    <trd>benauwdheid</trd>,
    <trd>angst</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_h;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepokoj</trd>,
    <trd>obava</trd>,
    <trd>tiese&ncaron; <klr>(pocit)</klr></trd>,
    <trd>tiese&ncaron; <klr>(tri stupne rovnak&eacute;ho stavu)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_v;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_je;&c_p;&c_o;&c_k;&c_o;&c_jeu;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&c_n;&c_je;&c_n;&c_n;&c_ja;&nbsp;(&c_u; &c_g;&c_r;&c_u;&c_d;&c_ja;&c_h;)</trd>
  </trdgrp>
</drv>

<drv mrk="angor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti <tld/>on:
      <ekz>
        lia tuta vivo nun orienti&gcirc;os al radikala &scirc;an&gcirc;o, <klr>[&#8230;]</klr> kaj anstata&ubreve;
        <tld/>i, tremi, suferi, li plenas per nekomprenebla sereneco
        <fnt>
          <bib>CKv</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro cerbumas, &scirc;vitas, <tld/>as
        <fnt>
          <bib>KrB</bib>
          <lok>Mastro en vojo &ndash; servantoj en &gcirc;ojo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kun&gcirc;ojas kun tiuj lastaj pri ilia liberigo, 
        sed iom <tld/>as pri &gcirc;ia estonta
        evoluo
        <fnt>
          <bib>Monato</bib>
          <aut>Guido Van Damme</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007000.html"
              >Atento por etaj kaj monda lingvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_u;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>c&iacute;tit &uacute;zkost</trd>,
    <trd>trnout &uacute;zkost&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>Beklemmungen</ind> haben</trd>,
    <trd><ind>Drangsal</ind> sp&uuml;ren</trd>,
    <trd>gro&szlig;e <ind>Pein</ind> empfinden</trd>,
  </trdgrp>
  <trd lng="en">feel distressed</trd>
  <trd lng="es">sentir angustia</trd>
  <trd lng="fr">&ecirc;tre <ind>angoiss&eacute;</ind></trd>
  <trd lng="it">angosciarsi</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">c&iacute;ti&tcaron; &uacute;zkos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_zh;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_u;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="angor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizita de <tld/>o:
      <ekz>
        miaj dentoj klakas, mi singultas kaj min konkeras la <tld/>a timo
        <fnt>
          <bib>LGA</bib>
          <lok>VI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi alvenis <klr>[&#8230;]</klr> &ccirc;e la Beringa markolo <klr>[&#8230;]</klr> 
        kaj pasigis tie
        kelkajn ege <tld/>ajn monatojn de mia certe aventurplena vivo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>1. Kakume.</lok>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a <tld/>a kriado de la birdoj
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;das kriojn de alarmo, mort<tld/>ajn vokojn pri helpo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia sceno ne forgesi&gcirc;as, &gcirc;i ne allasas forgeson,
        &gcirc;i revenas nokton post nokto en
        son&gcirc;oj, en premaj, <tld/>aj son&gcirc;oj
        <fnt>
          <bib>ChM</bib>
          <lok>14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skl&iacute;&ccaron;en&yacute;</trd>,
    <trd>st&iacute;sn&ecaron;n&yacute;</trd>,
    <trd>t&iacute;sniv&yacute;</trd>,
    <trd>&uacute;zkostliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beklommen</trd>,
    <trd>bek&uuml;mmert</trd>,
    <trd>verzweifelt</trd>,
  </trdgrp>
  <trd lng="en">distressed</trd>
  <trd lng="es">angustioso</trd>
  <trd lng="fr">anxieux</trd>
  <trd lng="it">angoscioso</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_m;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sk&lcaron;u&ccaron;uj&uacute;ci</trd>,
    <trd>strachupln&yacute;</trd>,
    <trd>tiesniv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_zh;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_v;&c_o;&c_zh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_m;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
