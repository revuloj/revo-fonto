<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: angor.xml,v 1.23 2021/05/28 12:11:18 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>angor</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="angor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsaneca sento de sufoko kun korpremateco:
      <ekz>
        <tld/>oj de la agonio;
      </ekz>
            <ekz>
      freneziga <tld/>o kiu premegis &scirc;ian koron
        <fnt>
          <aut>Hans Weinhengst</aut>,
          <vrk>Tur-Strato 4</vrk>,
          <lok><url ref="http://www.fadedpage.com/showbook.php?pid=20131216">
        Fadedpage</url>
          </lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Beklemmung</trd>,
      <trd>Enge</trd>,
      <trd>Angina</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_u;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5E;&#xF7A;&#xF51;&#xF0B;&#xF66;&#xFA3;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>strach</trd>,
    <trd>t&iacute;se&ncaron;</trd>,
    <trd>&uacute;zkost</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Bedr&auml;ngnis</trd>,
      <trd>Drangsal</trd>,
      <trd>Pein</trd>,
    </trdgrp>
  <trd lng="en">distress</trd>
  <trd lng="es">angustia</trd>
  <trd lng="fr">angoisse</trd>
  <trd lng="he">&#x5D7;&#x5B2;&#x5E8;&#x5B8;&#x5D3;&#x5B8;&#x5D4;</trd>
  <trd lng="it">angoscia</trd>
  <trdgrp lng="nl">
    <trd>angor</trd>,
    <trd>benauwdheid</trd>,
    <trd>angst</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_h;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepokoj</trd>,
    <trd>obava</trd>,
    <trd>tiese&ncaron; <klr>(pocit)</klr></trd>,
    <trd>tiese&ncaron; <klr>(tri stupne rovnak&eacute;ho stavu)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="angor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_u;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>c&iacute;tit &uacute;zkost</trd>,
    <trd>trnout &uacute;zkost&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Beklemmungen <ind>haben</ind></trd>,
      <trd>Drangsal <ind>sp&uuml;ren</ind></trd>,
      <trd>gro&szlig;e Pein <ind>empfinden</ind></trd>,
  </trdgrp>
  <trd lng="en">feel distressed</trd>
  <trd lng="es">sentir angustia</trd>
  <trd lng="fr">&ecirc;tre <ind>angoiss&eacute;</ind></trd>
  <trd lng="it">angosciarsi</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">c&iacute;ti&tcaron; &uacute;zkos&tcaron;</trd>
</drv>

<drv mrk="angor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Timplena, premsenta.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skl&iacute;&ccaron;en&yacute;</trd>,
    <trd>st&iacute;sn&ecaron;n&yacute;</trd>,
    <trd>t&iacute;sniv&yacute;</trd>,
    <trd>&uacute;zkostliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>beklommen</trd>,
      <trd>bek&uuml;mmert</trd>,
      <trd>verzweifelt</trd>,
  </trdgrp>
  <trd lng="en">distressed</trd>
  <trd lng="es">angustioso</trd>
  <trd lng="fr">anxieux</trd>
  <trd lng="it">angoscioso</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_m;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sk&lcaron;u&ccaron;uj&uacute;ci</trd>,
    <trd>strachupln&yacute;</trd>,
    <trd>tiesniv&yacute;</trd>
  </trdgrp>
</drv>
</art>
