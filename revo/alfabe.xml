<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>alfabet</rad>/o
</kap>
<drv mrk="alfabe.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Literaro, literlisto:
      <ekz>
        la esperanta <tld/>o enhavas 28 literojn;
      </ekz>
      <ekz>
        <ind>li neniam venkis la <tld/>on</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="fr">
          <trd>il ne <ind>sait</ind> ni a ni b</trd>,
          <trd>c'est un <ind>ignorant</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>li &scirc;vitas ankora&ubreve; super la <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="fr">
          <trd>il ne <ind>sait</ind> ni a ni b</trd>,
          <trd>c'est un <ind>ignorant</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aboc.0o.alfabeto">aboco <sncref/></ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_mol;&c_f;&c_a;&c_b;&c_e;&c_t;</trd>,
    <trd>&c_a;&c_b;&c_e;&c_c;&c_e;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">abeceda</trd>
  <trdgrp lng="de">
     <trd>Alphabet</trd>,
     <trd>Alfabet</trd>,
     <trd>Abc</trd>,
     <trd>Buchstabenfolge</trd>
  </trdgrp>
  <trd lng="en">alphabet</trd>
  <trd lng="es">alfabeto</trd>
  <trd lng="fr">alphabet</trd>
  <trd lng="nl">alfabet</trd>
  <trd lng="pt">alfabeto</trd>
  <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;</trd>
  <trd lng="sk">abeceda</trd>
  <trd lng="sv">alfabet</trd>
</drv>

<drv mrk="alfabe.0umo">
  <kap><tld/>umo</kap>
  <snc>
    <dif>
      Libro por lernigi <tld/>on al infanoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="libr.aboco0o">abocolibro</ref>,
      <ref cel="aboc.0o.alfabetumo">aboco <sncref/></ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_m;&c_a;&c_n;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Abc-Buch</trd>,
     <trd>Fibel</trd>
  </trdgrp>
  <trd lng="en">book of ABC's</trd>
  <trdgrp lng="fr">
    <trd>ab&eacute;c&eacute;daire</trd>,
    <trd>ABC<klr tip="ind"> (ab&eacute;c&eacute;daire)</klr></trd>
  </trdgrp>
  <trd lng="nl">ABC-boek</trd>
  <trd lng="pt">cartilha</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_k;&c_v;&c_a;&c_r;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;labik&aacute;r</trd>
</drv>
</art>
