<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>alfabet</rad>/o
</kap>
<drv mrk="alfabe.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Literaro, literlisto:
       <ekz>
         la esperanta <tld/>o enhavas 28 literojn;
       </ekz>
       <ekz>
         li neniam venkis la <tld/>on
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         li &scirc;vitas ankora&ubreve; super la <tld/>o
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="cs">abeceda</trd>
   <trd lng="de">Alphabet</trd>
   <trd lng="fr">alphabet</trd>
   <trd lng="ru">&#1072;&#1083;&#1092;&#1072;&#1074;&#1080;&#1090;</trd>
   <trd lng="sv">alfabet</trd>
</drv>
<drv mrk="alfabe.0umo">
   <kap><tld/>umo</kap>
   <snc>
     <dif>
       Libro por lernigi <tld/>on al infanoj.
     </dif>
     <refgrp tip="sin">
       <ref cel="aboc.0o">aboco</ref>.
     </refgrp>
   </snc>
   <trd lng="fr">ab&eacute;c&eacute;daire</trd>
   <trdgrp lng="ru">
     <trd>&#1073;&#1091;&#1082;&#1074;&#1072;&#1088;&#1100;</trd>,
     <trd>&#1072;&#1079;&#1073;&#1091;&#1082;&#1072;</trd>
   </trdgrp>
</drv>
</art>
