<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kamen</rad>/o
</kap>
<drv mrk="kamen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Parto de &ccirc;ambro, kie oni aran&gcirc;is por hejtado, kuirado a&ubreve; aliaj celoj,
      masonitan a&ubreve; metalan aparaton, konsistantan el fajrujo kaj fumtubo:
      <ekz>
        fari fajron sur la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        du sinjoroj en unu bieno, du mastrinoj &ccirc;e unu <tld/>o
        neniam vivas sen reciproka malbeno
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="forn.0o">forno</ref>,
      <ref cel="kapucx.kamen0o"><tld/>kapu&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_ib;&c_n;</trd>
  <trd lng="de">Kamin</trd>
  <trd lng="en">fireplace</trd>
  <trd lng="fr">chemin&eacute;e<klr tip="ind"> (dans une pi&egrave;ce)</klr></trd>
  <trd lng="hu">kandall&oacute;</trd>
  <trd lng="nl">schoorsteen <klr tip="ind">(stookplaats)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_i;&c_n;</trd>
</drv>

<drv mrk="kamen.0skrapisto">
  <kap><tld/>skrapisto</kap>
  <snc>
    <dif>
      Homo, kies metio estas purigi <tld/>ojn kaj fumtubojn je fulgo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_r;</trd>
  <trd lng="de">Schornsteinfeger</trd>
  <trd lng="en">chimney sweep</trd>
  <trd lng="fr">ramoneur</trd>
  <trd lng="hu">k&eacute;m&eacute;nysepr&odblac;</trd>
  <trd lng="nl">schoorsteenveger</trd>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_o;&c_ch;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="kamen.0tubo">
  <kap><ofc>*</ofc><tld/>tubo</kap>
  <snc>
    <dif>
      Fumtubo por <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_m;&c_ib;&c_n;</trd>
  <trd lng="de">Schornstein</trd>
  <trd lng="en">chimney</trd>
  <trdgrp lng="fr">
    <trd>conduit de <ind>chemin&eacute;e</ind></trd>,
    <trd>chemin&eacute;e<klr tip="ind"> (sur le toit)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&eacute;m&eacute;ny</trd>
  <trd lng="nl">schoorsteen<klr tip="ind">(pijp)</klr></trd>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_o;&c_v;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_a;</trd>
</drv>
</art>
