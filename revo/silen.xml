<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: silen.xml,v 1.6 2023/12/02 21:23:15 afido Exp $">
    <kap>
      <rad>silen</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="silen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="silen.0o.genro">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="kariof1.0oj">kariofilacoj</ref>
      (<trd lng="la">Silene</trd>), kun pli ol 600 specioj
      precipe en la norda hemisfero; unu- a&ubreve; dujaraj herboj kun kvinnombraj floroj
      kies kaliko ofte estas tub-, klo&scirc;- a&ubreve; ovoforma:
      <ekz>
        kvankam la grundo de <nom>Ailsa Craig</nom> estas sala, humida kaj ofte malvarma, diversaj
        specioj de plantoj kreskas sur la insulo: ekzemple sova&gcirc;a hiacinto, liknido,
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008289.html"
              >Mistera insulo Ailsa Craig</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        pendola sileno <klr>[&#8230;]</klr> originas en la mediteranea regiono, kaj transvenis
        al Japanio fine de la 19-a jarcento
        <fnt>
          <aut>Vastalto</aut>
          <vrk><url ref="https://www.vastalto.com/blog/?p=646"
              >Pendola sileno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/41/Silene-vulgaris-170704-800-1.jpg/296px-Silene-vulgaris-170704-800-1.jpg"
      prm="CC BY-SA 2.5"
      >floro de ordinara <tld/>o (Silene vulgaris), Gerstrubental, Alpoj
        <fnt>
          <aut>Michael H. Lemmer, http://www.naturkamera.de propra verko</aut>
          <lok><url ref="&WCW;/File:Silene-vulgaris-170704-800-1.jpg"
              >https://commons.wikimedia.org/wiki/File:Silene-vulgaris-170704-800-1.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="bg">&c_p;&c_l;&c_ju;&c_s;&c_k;&c_a;&c_v;&c_i;&c_ch;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Leimkraut</trd>,
      <trd>Lichtnelke</trd>
    </trdgrp>
    <trd lng="en">Silene</trd>
    <trd lng="fr">Sil&egrave;ne</trd>
    <trd lng="pl">lepnica  <klr>(ro&sacute;l.)</klr></trd>
    <trd lng="ru">&c_S;&c_m;&c_o;&c_l;&c_jo;&c_v;&c_k;&c_a;</trd>
  </snc>
  <ref tip="hom" cel="silen1.0o">Sileno (dio)</ref>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_s;&c_k;&c_a;&c_t;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_w;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
</drv>
  </art>

