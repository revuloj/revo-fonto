<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sociol1.xml,v 1.23 2024/03/25 06:40:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sociologi</rad>/o
</kap>
<drv mrk="sociol1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sociol1.0o.SCI">
    <uzo tip="fak">POL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri fenomenoj kaj le&gcirc;oj de socia vivo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">sociologia</trd>
  <trd lng="de">Soziologie</trd>
  <trd lng="en">sociology</trd>
  <trd lng="es">sociolog&iacute;a</trd>
  <trd lng="fa">&a_j;&a_A;&a_m;&a_ayn;&a_h;&#x200C;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">sociologie</trd>
  <trdgrp lng="hu">
    <trd>szociol&oacute;gia</trd>,
    <trd>t&aacute;rsadalomtudom&aacute;ny</trd>
  </trdgrp>
  <trd lng="ja">&#x793E;&#x4F1A;&#x5B66;  <pr>&#x3057;&#x3083;&#x304B;&#x3044;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">sociologie</trd>
  <trd lng="pl">socjologia</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43E;&c_c;&c_ib;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>,
    <trd>&c_s;&c_u;&c_s;&#x43F;&c_ib;&#x43B;&#x44C;&c_s;&c_t;&c_v;&#x43E;&c_z;&#x43D;&c_a;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x793E;&#x4F1A;&#x5B66; <pr>sh&#xE8;hu&#xEC;xu&#xE9;</pr></trd>,
    <trd>&#x793E;&#x6703;&#x5B78; <pr>sh&#xE8;hu&#xEC;xu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sociol1.0a">
  <kap><tld/>a</kap>
  <snc mrk="sociol1.0a.SCI">
    <dif>
      Rilata al <tld/>o, al &gcirc;iaj metodoj, teorioj:
      <ekz>
        Pliparto de la artikoloj traktas diversajn <tld/>ajn facetojn de
        la Esperanto-komunumo
        <fnt>
        <aut>Boris Kolker</aut>,
        <vrk>Pensoj pri la esperantista popolo (Zlatko Ti&scirc;ljar,
        Esperanto vivos malgra&ubreve; la
        esperantistoj)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 1a, p. 19a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_ja;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;</trd>
  <trd lng="ca">sociol&ograve;gic</trd>
  <trd lng="en">sociological</trd>
  <trd lng="es">sociol&oacute;gico</trd>
  <trd lng="fa">&a_j;&a_A;&a_m;&a_ayn;&a_h;&#x200C;&a_sh;&a_n;&a_A;&a_kh;&a_t;&f_y;</trd>
  <trd lng="fr">sociologique</trd>
  <trdgrp lng="hu">
    <trd>szociol&oacute;giai</trd>,
    <trd>t&aacute;rsadalomtudom&aacute;nyi</trd>
  </trdgrp>
  <trd lng="ja">&#x793E;&#x4F1A;&#x5B66;&#x306E;  <pr>&#x3057;&#x3083;&#x304B;&#x3044;&#x304C;&#x304F;&#x306E;</pr></trd>
  <trd lng="nl">sociologisch</trd>
  <trd lng="pl">socjologiczny</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&c_s;&#x43E;&c_c;&c_ib;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="sociol1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sociol1.0isto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="sociol1.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="sociol.0o">sociologo</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">soci&ograve;leg</trd>
  <trd lng="en">sociologist</trd>
  <trd lng="es">soci&oacute;logo</trd>
  <trd lng="fa">&a_j;&a_A;&a_m;&a_ayn;&a_h;&#x200C;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">sociologue</trd>
  <trdgrp lng="hu">
    <trd>szociol&oacute;gus</trd>,
    <trd>t&aacute;rsadalomtud&oacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x793E;&#x4F1A;&#x5B66;&#x8005;  <pr>&#x3057;&#x3083;&#x304B;&#x3044;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">socioloog</trd>
  <trd lng="pl">socjolog</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x793E;&#x4F1A;&#x5B66;&#x5BB6; <pr>sh&#xE8;hu&#xEC;xu&#xE9;ji&#x101;</pr></trd>,
    <trd>&#x793E;&#x6703;&#x5B78;&#x5BB6; <pr>sh&#xE8;hu&#xEC;xu&#xE9;ji&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
