  <rad>sociologi</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="sociol1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sociol1.0o.SCI">
    <uzo tip="fak">POL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri fenomenoj kaj le&gcirc;oj de socia vivo.
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>sociscienco</ke>.
  </rim>

  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">sociologia</trd>
  <trd lng="de">Soziologie</trd>
  <trd lng="en">sociology</trd>
  <trd lng="es">sociolog&iacute;a</trd>
  <trd lng="fa">&a_j;&a_A;&a_m;&a_ayn;&a_h;&#8204;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">sociologie</trd>
  <trdgrp lng="hu">
    <trd>szociol&oacute;gia</trd>,
    <trd>t&aacute;rsadalomtudom&aacute;ny</trd>
  </trdgrp>
  <trd lng="ja">&#31038;&#20250;&#23398;  <pr>&#12375;&#12419;&#12363;&#12356;&#12364;&#12367;</pr></trd>
  <trd lng="nl">sociologie</trd>
  <trd lng="pl">socjologia</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#1086;&c_c;&c_ib;&#1086;&#1083;&#1086;&c_g;&c_ib;&#1103;</trd>,
    <trd>&c_s;&c_u;&c_s;&#1087;&c_ib;&#1083;&#1100;&c_s;&c_t;&c_v;&#1086;&c_z;&#1085;&c_a;&c_v;&c_s;&c_t;&c_v;&#1086;</trd>
  </trdgrp>
