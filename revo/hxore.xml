   <trdgrp lng="ca">
     <trd>corea</trd>,
     <trd>ball de Sant Vito</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Chorea</trd>,
     <trd>Veitstanz</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>(Sydenham's) <ind>chorea</ind></trd>,
     <trd>St. Vitus' dance</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>corea</trd>,
     <trd>baile de San Vito</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>tanssitauti</trd>,
     <trd>chorea</trd>
