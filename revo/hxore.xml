    <trd>vitust&aacute;nc</trd>
  </trdgrp>
  <trd lng="nl">St.-Vitusdans</trd>
  <trd lng="pt">cor&eacute;ia<klr tip="amb"> (medicina)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_je;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a; &c_s;&c_v;&c_ja;&c_t;&c_o;&c_g;&c_o; &c_V;&c_i;&c_t;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
