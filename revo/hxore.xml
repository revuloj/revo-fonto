  <trdgrp lng="ca">
    <trd>corea</trd>,
    <trd>ball de Sant Vito</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Chorea</trd>,
    <trd>Veitstanz</trd>
  </trdgrp>
