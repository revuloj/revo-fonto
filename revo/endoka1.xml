<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>endokarp</rad>/o
</kap>
<drv mrk="endoka1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Plej interna parto de la <ref tip="malprt" 
      cel="perika1.0o">perikarpo</ref>,
      membrano, kiu envolvas la grajnojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="epikar.0o">epikarpo</ref>,
      <ref cel="mezoka.0o">mezokarpo</ref>
    </refgrp>
  </snc>
  <trd lng="en">endocarp</trd>
</drv>
</art>
