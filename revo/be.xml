<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>be</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="be.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel &scirc;afo.
    </dif>
  </snc>
  <trd lng="fr">b&ecirc;ler</trd>
  <trd lng="hu">b&eacute;get</trd>
</drv>

<drv mrk="be.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Bruo simila al &scirc;afbleko:
      <ekz>
        mi a&ubreve;das nur multajn <tld/>ojn de &scirc;afoj kaj
        &scirc;afinoj<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 18a, p.
        128a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">b&ecirc;lement</trd>
  <trd lng="hu">b&eacute;get&eacute;s</trd>
</drv>
</art>

