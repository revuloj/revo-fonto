<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apiac.xml,v 1.14 2023/10/13 16:46:48 revo Exp $">
<kap>
<rad>apiac</rad>/oj
</kap>
<drv mrk="apiac.0oj">
  <kap>
  <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref> plantoj
      (<trd lng="la">Apiaceae</trd>), kun umbelaj a&ubreve;
      duoble umbelaj infloreskoj:
      <ekz>
        la cerefolio botanike apartenas al la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=756">Cerefolio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="celeri.0o">celerio</ref>,
      <ref cel="anet.0o">aneto</ref>,
      <ref cel="aniz.0o">anizo</ref>,
      <ref cel="cerefo.0o">cerefolio</ref>,
      <ref cel="pastin.0o">pastinako</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_o;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trdgrp lng="ca">
    <trd>api&agrave;cies</trd>,
    <trd>umbel&middot;l&iacute;feres</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Doldenbl&uuml;ter</trd>,
    <trd>Doldengew&auml;chse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Apiaceae</trd>,
    <trd>Umbelliferae</trd>,
    <trd>celery, carrot and parsley family</trd>
  </trdgrp>
  <trd lng="fr">ombellif&egrave;res</trd>
  <trdgrp lng="hu">
    <trd>erny&odblac;s&ouml;k</trd>,
    <trd>zellerf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="ja">&#x30BB;&#x30EA;&#x79D1;  <pr>&#x30BB;&#x30EA;&#x304B;</pr></trd>
  <trdgrp lng="pl">
    <trd>baldaszkowate</trd>,
    <trd>selerowate</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_mol;&c_d;&c_je;&c_r;&c_je;&c_j;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">flockblommiga v&auml;xter</trd>
</drv>
</art>
