<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apiac</rad>/oj
</kap>
<drv mrk="apiac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unujaraj <ref tip="super"
      cel="kotile.du0aj">dukotiledonaj</ref> plantoj
      (<trd lng="la">Apiaceae</trd>), kun umbelaj a&ubreve;
      duoble umbelaj infloreskoj.
    </dif>
  </snc>
  <trd lng="de">Doldenbl&uuml;ter</trd>
  <trd lng="fr">ombellif&egrave;res</trd>
  <trdgrp lng="hu">
    <trd>erny&odblac;s&ouml;k</trd>,
    <trd>zellerf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_je;</trd>,
 
<trd>&c_s;&c_je;&c_l;&c_mol;&c_d;&c_je;&c_r;&c_je;&c_j;&c_n;&c_y;&c_je;</trd
>
  </trdgrp>
  <trd lng="sv">flockblommiga v&auml;xter</trd>
</drv>
</art>
