<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glugl.xml,v 1.14 2023/12/09 16:29:33 revo Exp $">
<kap>
  <rad>glugl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glugl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glugl.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel agite fluanta,
      bobelanta akvo:
      <ekz>
        en la tubo siblis kaj singulte <tld/>is aero
        <fnt>
          <aut>Aleksandro Grin, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Grin/kuranta_04.html"
              >Kuranta sur ondoj</url></vrk>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gargar.0i.FIG">gargari<sncref/></ref>,
      <ref cel="lirl.0i.SON">lirli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">clapotejar</trd>
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>&zcaron;blu&ncaron;kat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>glucksen</trd>,
     <trd>gluckern</trd>,
     <trd>gurgeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>burble</trd>,
    <trd>gurgle</trd>
  </trdgrp>
  <trd lng="es">gorgotear</trd>
  <trdgrp lng="fr">
    <trd>gargouiller</trd>,
    <trd>glouglouter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bugyog</trd>,
    <trd>bugybor&eacute;kol</trd>,
  </trdgrp>
  <trd lng="pt">gluglurejar</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>klokota&tcaron;</trd>,
    <trd>&zcaron;blnkota&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#30262;&#21475; <pr>s&ograve;uk&#466;u</pr></trd>,
    <trd>&#27753;&#27753; <pr>g&#468;g&#468;</pr></trd>,
    <trd>&#22013;&#21475; <pr>s&ograve;uk&#466;u</pr></trd>,
    <trd>&#28465;&#21475; <pr>sh&ugrave;k&#466;u</pr></trd>,
    <trd>&#30262; <pr>s&ograve;u</pr></trd>,
    <trd>&#22013; <pr>s&ograve;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glugl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="bru.0o.sonaro">Bruo</ref> simila al tiu de skuata akvo:
      <ekz>
        mi petole demandis al la infaneto &gcirc;ian nomon, kaj la patrino kontente tradukis
        &gcirc;ian <tld/>on
        <fnt>
          <bib>KrM</bib>
          <lok>40. Kaj Rapide Kreskas La Afero.</lok>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; li prenis duonon el taso da teo, en&scirc;vebis ridaj <tld/>oj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 333a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;</trd>
  <trd lng="ca">clapoteig</trd>
  <trd lng="cs">&zcaron;blu&ncaron;knut&iacute;</trd>
  <trdgrp lng="de">
     <trd>Glucksen</trd>,
     <trd>Gluckern</trd>,
     <trd>Gurgeln</trd>
  </trdgrp>
  <trd lng="es">gorgoteo</trd>
  <trdgrp lng="fr">
    <trd>gargouillement</trd>,
    <trd>glouglou</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bugyog&aacute;s</trd>,
    <trd>bugybor&eacute;kol&aacute;s</trd>,
  </trdgrp>
  <trd lng="pt">gluglu</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_n;&c_mol;&c_je;</trd>
  <trd lng="sk">&zcaron;blnknutie</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_k;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

