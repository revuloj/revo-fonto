<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glugl.xml,v 1.12 2022/03/19 19:18:23 revo Exp $">
<kap>
  <rad>glugl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glugl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glugl.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel agite fluanta,
      bobelanta akvo.
    </dif>
    <refgrp tip="vid">
      <ref cel="lirl.0i.SON">lirli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">clapotejar</trd>
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>&#x17E;blu&#x148;kat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>glucksen</trd>,
     <trd>gluckern</trd>,
     <trd>gurgeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>burble</trd>,
    <trd>gurgle</trd>
  </trdgrp>
  <trd lng="es">gorgotear</trd>
  <trdgrp lng="fr">
    <trd>gargouiller</trd>,
    <trd>glouglouter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bugyog</trd>,
    <trd>bugybor&eacute;kol</trd>,
  </trdgrp>
  <trd lng="pt">gluglurejar</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>klokota&#x165;</trd>,
    <trd>&#x17E;blnkota&#x165;</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x443;&#x43B;&#x44C;&#x43A;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="glugl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Bruo simila al tiu de skuata akvo:
      <ekz>
        apena&ubreve; li prenis duonon el taso da teo, en&scirc;vebis ridaj <tld/>oj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 333a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;</trd>
  <trd lng="ca">clapoteig</trd>
  <trd lng="cs">&#x17E;blu&#x148;knut&#xED;</trd>
  <trdgrp lng="de">
     <trd>Glucksen</trd>,
     <trd>Gluckern</trd>,
     <trd>Gurgeln</trd>
  </trdgrp>
  <trd lng="es">gorgoteo</trd>
  <trdgrp lng="fr">
    <trd>gargouillement</trd>,
    <trd>glouglou</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bugyog&aacute;s</trd>,
    <trd>bugybor&eacute;kol&aacute;s</trd>,
  </trdgrp>
  <trd lng="pt">gluglu</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_n;&c_mol;&c_je;</trd>
  <trd lng="sk">&#x17E;blnknutie</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x43B;&#x44C;&#x43A;&#x456;&#x442;</trd>,
    <trd>&#x431;&#x443;&#x43B;&#x44C;&#x43A;&#x43E;&#x442;&#x456;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

