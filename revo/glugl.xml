<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>glugl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glugl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel agite fluanta,
      bobelanta akvo.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>glucksen</trd>,
     <trd>gluckern</trd>,
     <trd>gurgeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>burble</trd>,
    <trd>gurgle</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gargouiller</trd>,
    <trd>glouglouter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bugyog</trd>,
    <trd>bugybor&eacute;kol</trd>,
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="glugl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Bruo simila al tiu de skuata akvo:
      <ekz>
        apena&ubreve; li prenis duonon el taso da teo, en&scirc;vebis ridaj <tld/>oj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 333a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Glucksen</trd>,
     <trd>Gluckern</trd>,
     <trd>Gurgeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gargouillement</trd>,
    <trd>glouglou</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bugyog&aacute;s</trd>,
    <trd>bugybor&eacute;kol&aacute;s</trd>,
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_n;&c_mol;&c_je;</trd>
</drv>
</art>

