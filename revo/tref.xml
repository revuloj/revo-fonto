<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tref.xml,v 1.23 2022/03/28 14:44:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tref</rad>/o
</kap>
<drv mrk="tref.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <mlg>&#x2663;</mlg>
  <snc>
    <dif>
      Ludkarta
      <ref tip="super" cel="emblem.0o.karta">emblemo</ref>,
      signata per nigra <ref tip="vid" cel="foli.tri0o">trifolio</ref>: 
      <ekz>
        trefo a&ubreve; trifolio estas <klr>[&#x2026;]</klr> la plej malalta emblemo en bri&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27891">Trefo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ker.0o">kero</ref>,
      <ref cel="karo.0o">karoo</ref>,
      <ref cel="pik.0o.emblemo">piko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_u;&c_d;&c_y; <klr>(&c_u;
  &c_k;&c_a;&c_r;&c_t;&c_a;&c_h;)</klr></trd>
  <trd lng="cs">k&rcaron;&iacute;&zcaron; <klr>(v kart&aacute;ch)</klr></trd>
  <trd lng="de">Kreuz</trd>
  <trd lng="en">clubs</trd>
  <trd lng="es">tr&eacute;boles <klr>(palo de la baraja de naipes
  francesa)</klr></trd>
  <trd lng="fr">tr&egrave;fle<klr tip="ind"> (jeu de cartes)</klr></trd>
  <trd lng="hu">treff</trd>
  <trd lng="nl">klaver<klr tip="ind"> (in kaartspel)</klr></trd>
  <trdgrp lng="pl">
    <trd>trefl</trd>,
    <trd>&zdot;o&lstroke;&aogonek;d&zacute;<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">paus</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_f;&c_y;</trd>
  <trd lng="sk">kr&iacute;&zcaron; <klr>(kart)</klr></trd>
  <trd lng="sv">kl&ouml;ver</trd>
  <trdgrp lng="uk">
    <trd>&#x436;&#x438;&#x440;</trd>,
    <trd>&#x442;&#x440;&#x435;&#x444;&#x430;</trd>,
    <trd>&#x442;&#x440;&#x435;&#x444;&#x438;</trd>
  </trdgrp>
</drv>
</art>
