<art mrk="$Id: kameled.xml,v 1.13 2023/12/02 21:20:05 revo Exp $">
<kap>
  <rad>kameled</rad>/oj
</kap>
<drv mrk="kameled.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Camelidae</trd>):
      <ekz>
        <ref tip="prt" cel="lam1.0o">lamo</ref>
        a&ubreve; ljamo estas sudamerika hejmbestigita <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16821">Lamo</url></lok>
        </fnt>.
      </ekz>      
    </dif>
    <ref tip="prt" cel="kamel.0o">kamelo</ref>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_b;&c_l;&c_ju;&c_d;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cam&egrave;lids</trd>
  <trd lng="de">Kamele</trd>
  <trd lng="fr">cam&eacute;lid&eacute;s</trd>
  <trd lng="hu">tevef&eacute;l&eacute;k</trd>
  <trd lng="nl">kamelenfamilie</trd>
  <trd lng="pl">wielb&lstroke;&aogonek;dowate</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;&c_o;&c_v;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x432;&#x435;&#x440;&#x431;&#x43B;&#x44E;&#x434;&#x43E;&#x432;&#x456;</trd>
  <trd lng="zh">&#x99F1;&#x99DD;&#x79D1; <pr>lu&#xF2;tu&#xF3;k&#x113;</pr></trd>
</drv>
</art>
