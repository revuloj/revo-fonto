<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompresor.xml,v 1.10 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>kompresor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kompresor.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompresor.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Gaspremilo:
      <ekz>
        <tld/>o de jetmotoro
        <fnt>
          <aut>J. Pospi&scaron;il</aut>,
          <vrk><url ref="&STEB;ingxenierado/ceteraj/aviado/saest82-17.htm">
          Komunaj klarigoj al priaviadaj prelegoj...</url></vrk>,
          <lok>SAEST-1982</lok>
        </fnt>;
      </ekz>
      <ekz>
        susuris ar&gcirc;entkoloraj serpentaj tuboj, zumzumis motoroj, bruegis <tld/>oj,
        ekbrilis signallumoj
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pump.aer0ilo">aerpumpilo</ref>
    </refgrp>
  </snc>
  <adm>
    La&ubreve; PIV nur *aer*premilo.
  </adm>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_r;&c_e;&c_s;&c_a;&c_r;</trd>
  <trd lng="cs">kompresor</trd>
  <trdgrp lng="de">
    <trd>Verdichter</trd>,
    <trd>Kompressor</trd>
  </trdgrp>
  <trd lng="fr">compresseur</trd>
  <trdgrp lng="pl">
    <trd>spr&eogonek;&zdot;arka</trd>,
    <trd>kompresor</trd>
  </trdgrp>
  <trd lng="ro">compresor </trd>
  <trd lng="sk">kompresor</trd>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_p;&c_r;&c_je;&c_s;&c_o;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x7A7A;&#x538B;&#x673A; <pr>k&#x14D;ngy&#x101;j&#x12B;</pr></trd>,
    <trd>&#x538B;&#x7F29;&#x673A; <pr>y&#x101;su&#x14D;j&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

