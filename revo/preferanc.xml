<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: preferanc.xml,v 1.8 2024/02/17 07:52:35 revo Exp $">
<kap>
  <rad>preferanc</rad>/o
</kap>

<drv mrk="preferanc.0o">
  <kap><tld/>o</kap>
  <snc mrk="preferanc.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="kart.0ludo">Kartludo</ref> por tri a&ubreve;
      kvar personoj, populara en Ruslando kaj &ccirc;irka&ubreve;e,
      bazita sur <ref cel="vist.0o">visto</ref>, ludata per 32
      kartoj:
      <ekz>
        <nom>Anna Sergeevna</nom>, kiu plejparte parolis kun <nom>Bazarov</nom>,
        demandis E&ubreve;genon, &ccirc;u li deziras fari kun ili batalon
        je <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>, <lok>&ccirc;ap. 16, p. 88.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiun rilaton havas granda teniso kaj <tld/>o al evoluigo de Esperanto
        <fnt>
          <vrk><url ref="https://www.google.com/url?sa=t&amp;rct=j&amp;q=&amp;esrc=s&amp;source=web&amp;cd=&amp;cad=rja&amp;uact=8&amp;ved=2ahUKEwjGq8y-6eiAAxWh8LsIHe1MBgc4ChAWegQIDhAB&amp;url=https%3A%2F%2Freu.ru%2Fprenu%2Fprotokolo_de_la_31_a_konferenco.pdf&amp;usg=AOvVaw0ijR6zWu2HIRWuugCA1gMe&amp;opi=89978449">Protokolo de la 31-a Konferenco de REU, 21-a &ndash; 22-a de a&ubreve;gusto 2021, Dagestano</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_f;&c_e;&c_r;&c_a;&c_n;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Pr&eacute;f&eacute;rence</trd>,
    <trd>Preferanzen</trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;f&eacute;rence<klr> (jeu de cartes)</klr></trd>
  <trd lng="hu">prefer&aacute;nsz</trd>
  <trdgrp lng="nl">
    <trd>preferans<klr tip="ind"> (kaartspel)</klr></trd>,
    <trd>preference<klr tip="ind"> (kaartspel)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
  <trd lng="zh">&#x504F;&#x597D; <pr>pi&#x101;nh&#xE0;o</pr></trd>
</drv>

<drv mrk="preferanc.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Persono iom serioze interesita en <tld/>o, regule ludanta
      <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_f;&c_e;&c_r;&c_a;&c_n;&c_s;&c_ib;&c_s;&c_t;</trd>
  <trd lng="hu">prefer&aacute;nszj&aacute;t&eacute;kos</trd>
  <trdgrp lng="nl">
    <trd>preferansspeler</trd>,
    <trd>preferencespeler</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_n;&c_s;&c_i;&c_s;&c_t;</trd>
</drv>

</art>

