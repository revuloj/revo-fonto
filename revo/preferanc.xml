<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>preferanc</rad>/o
    </kap>

    <drv mrk="preferanc.0o">
      <kap><tld/>o</kap>
      <snc>
    	<dif>
    	  <ref tip="super" cel="kart.0ludo">Kartludo</ref> por tri a&ubreve;
    	  kvar personoj, populara en Ruslando kaj &ccirc;irka&ubreve;e,
          bazita sur <ref cel="vist.0o">visto</ref>, ludata sur 32
    	  kartoj:
          <ekz>
            Anna Sergeevna, kiu plejparte parolis kun Bazarov,
            demandis E&ubreve;genon, &ccirc;u li deziras fari kun ili batalon
            je <tld/>o
            <fnt>
              <bib>PatrojFiloj</bib>, <lok>&ccirc;ap. 16, p. 88.</lok>
            </fnt>
          </ekz>
    	</dif>
      </snc>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
    </drv>

    <drv mrk="preferanc.0isto">
      <kap><tld/>isto</kap>
      <snc>
    	<dif>
          Persono iom serioze interesita en <tld/>o, regule ludanta <tld/>on.
    	</dif>
      </snc>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_n;&c_s;&c_i;&c_s;&c_t;</trd>
    </drv>

  </art>

<!--
$Log$
-->
</vortaro>