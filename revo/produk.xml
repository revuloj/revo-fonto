<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>produkt</rad>/i
</kap>
<drv mrk="produk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Estigi viva&jcirc;ojn a&ubreve; senviva&jcirc;ojn per natura evoluipovo:
      <ekz>
        la &scirc;afinoj <tld/>as ordinare nur unu &scirc;afidon;
      </ekz>
      <ekz>
        tiuj arboj <tld/>as pomojn;
      </ekz>
      <ekz>
        Ameriko <tld/>as multon da greno<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu grundo <tld/>as oron;
      </ekz>
      <ekz>
        la lando <tld/>is grandajn homojn;
      </ekz>
      <ekz>
        fruktoj <tld/>itaj de tero;
      </ekz>
      <ekz>
        la pluvo malsoifigas la teron kaj <tld/>igas &gcirc;in<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="nask.0i">naski</ref>,
      <ref cel="kresk">elkreskigi</ref>.
    </refgrp>
    <trd lng="de">hervorbringen</trd>
  </snc>
  <snc>
    <dif>
      Fari el krudaj materialoj substancojn a&ubreve; objektojn utilajn por la
      homo:
      <ekz>
        batado de lakto <tld/>as buteron<fnt>Z</fnt>
        <klr>(oni ne devas miri pri la neeviteblaj konsekvencoj de siaj
        faroj)</klr>;
      </ekz>
      <ekz>
        <tld/>i komerca&jcirc;ojn;
      </ekz>
      <ekz>
        la fabriko <tld/>as bonan tolon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ejo de sukero.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tir.el0i">eltiri</ref>,
      <ref cel="el.0igi">eligi</ref>,
      <ref cel="kauxz.0i">ka&ubreve;zi</ref>,
      <ref cel="don.el0i">eldoni</ref>,
      <ref cel="fabrik.0i">fabriki</ref>.
    </refgrp>
  </snc>
  <trd lng="de">produzieren</trd>
  <trd lng="fr">produire</trd>
  <trd lng="en">produce</trd>
  <trd lng="nl">produceren</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="produk.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        stimuli la <tld/>adon per doganimpostoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Produktion</trd>
  <trd lng="en">production</trd>
  <trd lng="fr">production</trd>
  <trd lng="nl">productie</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="produk.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Io <tld/>ita:
      <ekz>
        ter<tld/>a&jcirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la porcelan-industrio, kies <tld/>oj estas eksportataj al;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj de la homa penso fari&gcirc;us akireblaj
        por &ccirc;iu el ni<fnt>Z</fnt>;
      </ekz>
      <ekz>
        literaturaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>malneta enlanda <tld/>o</ind> (MEP)
        <trdgrp lng="en">
          <trd>gross domestic <ind>product</ind></trd>,
          <trd>GDP</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>bruto nationaal <ind>product</ind></trd>,
          <trd>BNP</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_v;&c_a;&c_l;&c_o;&c_v;&c_o;&c_j; &c_n;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;</trd>,
          <trd>&c_V;&c_N;&c_P;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="frukt.0o">frukto</ref>
  </snc>
  <trd lng="de">Produkt</trd>
  <trd lng="en">product</trd>
  <trd lng="fr">produit</trd>
  <trd lng="nl">product</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;</trd>
</drv>

<drv mrk="produk.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Multe <tld/>anta:
      <ekz>
        <tld/>ema tero
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fekund.0a">fekunda</ref>,
      <ref cel="frukt.0odona0oporta">fruktoporta</ref>.
    </refgrp>
  </snc>
  <trd lng="de">produktiv</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="produk.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i novajn esta&jcirc;ojn similajn al la anta&ubreve;aj:
      <ekz>
        tiu vegeta&jcirc;o facile re<tld/>as sin;
      </ekz>
      <ekz>
        re<tld/>i rozujon per bran&ccirc;planto.
      </ekz>
    </dif>
    <trd lng="de">reproduzieren</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="P"/>i objekton similan al alia:
      <ekz>
        re<tld/>i pentra&jcirc;on per gravurado, per fotografado;
      </ekz>
      <ekz>
        re<tld/>i en sia revuo interesan artikolon;
      </ekz>
      <ekz>
        la pentristo re<tld/>as la naturon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="imit.0i">imiti</ref>,
      <ref cel="kopi.0i">kopii</ref>,
      <ref cel="prezen1.re0i">reprezenti</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>reproduzieren</trd>,
      <trd>nachbilden</trd>,
      <trd>kopieren</trd>,
      <trd>abdrucken</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">reproduceren</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_s;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_r;&c_o;&c_d;&c_u;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
