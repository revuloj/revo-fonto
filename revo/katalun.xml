<drv mrk="katalun.K0ujo">
   <kap><tld lit="K"/>ujo</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      A&ubreve;tonoma <ref tip="lst" lst="voko:regionoj"
      cel="region.0o.land">regiono</ref>
      situanta en la nordokcidento de <ref tip="malprt"
      cel="hispan.H0ujo">Hispanujo</ref>:
      <ekz>
        <nac>Els Segadors</nac> (&leftquot;La Agristoj&rightquot;) estas la nuntempa
        oficiala himno de <tld lit="K"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21234">Els
          Segadors</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Catharina J. Roskes-Dirksen</nom> <klr>[&#8230;]</klr>
        estas premiita kaj menciita de la Internaciaj Floraj Ludoj en
        <tld lit="K"/>ujo en 1926, 1927 kaj 1928, i.a. per &scirc;iaj
        poemoj Simile al la mar' kaj Kristnasko
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=27174">Catharina J.
          Roskes-Dirksen</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kataluni.A0o">Katalunio</ref>
  </snc>
   <trd lng="af">Kataloni&euml;</trd>
   <trd lng="ca">Catalunya</trd>
   <trd lng="de">Katalonien</trd>
   <trd lng="en">Catalonia</trd>
   <trd lng="es">Catalu&ntilde;a</trd>
   <trd lng="eu">Katalunia</trd>
   <trd lng="fa">&f_k;&a_A;&a_t;&a_A;&a_l;&a_w;&a_n;&f_y;&a_A;</trd>
   <trd lng="fr">Catalogne</trd>
   <trd lng="fy">Kataloanje</trd>
   <trd lng="gl">Catalu&ntilde;a</trd>
   <trd lng="io">Katalunia</trd>
   <trd lng="it">Catalogna</trd>
   <trd lng="ko">&#52852;&#53448;&#47336;&#45264; &#51648;&#48169;</trd>
   <trd lng="nl">Cataloni&euml;</trd>
   <trd lng="no">Catalonia</trd>
   <trd lng="oc">Catalonha</trd>
   <trd lng="pl">Katalonia</trd>
   <trd lng="pt">Catalunha</trd>
   <trd lng="ro">Catalonia</trd>
   <trd lng="ru">&c_K;&c_a;&c_t;&c_a;&c_l;&c_o;&c_n;&c_i;&c_ja;</trd>
   <trd lng="sk">Katal&aacute;nsko</trd>
   <trd lng="sv">Katalonien</trd>
   <trd lng="tp">ma Katala</trd>
   <trd lng="tr">Katalonya</trd>
   <trd lng="vo">Katalon&auml;n</trd>
</drv>
