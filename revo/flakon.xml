<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>flakon</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="flakon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="botel.0o">Boteleto</ref> por parfumoj, medikamentoj,
      likvoroj, plej ofte fermita per &gcirc;ustege adaptita &scirc;topilo el 
      sama materio. 
    </dif>
    <refgrp tip="vid">
      <ref cel="karaf.0o">karafo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Flakon</trd>,
     <trd>Parf&uuml;mfl&auml;schchen</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>flacon</trd>,
    <trd>fiole</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_f;&c_l;&c_a;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_u;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
  </trdgrp>
</drv>
</art>
