<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flakon.xml,v 1.22 2023/11/09 20:48:33 revo Exp $">
<kap>
  <rad>flakon</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="flakon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="botel.0o">Boteleto</ref> por parfumoj,
      medikamentoj, likvoroj, plej ofte fermita per &gcirc;ustege
      adaptita &scirc;topilo el sama materialo:
      <ekz>
        facetitaj <tld/>oj de parfumo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la supra breto <klr>[&#x2026;]</klr> vi trovos <tld/>eton kun la &gcirc;usta dozo da
        kuraca&jcirc;o
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>7. La nesolvebla problemo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        karikaturistoj pentris lin <tld/>forma, kun apoteksurskribo &leftquot;amoniako&rightquot;
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>2. Ligi-oravilli.</lok>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;iuj li senpage disdonadis po du <tld/>ojn da eksterlanda parfumo
        <fnt>
          <bib>MkM</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li metis la manon en sian kompleton kaj eltiris etan vitran <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La Helpanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KEM</uzo>
        konusa <tld/>o de <nom>Erlenmeyer</nom>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balon.0o.KEM">balono<sncref/></ref>,
      <ref cel="karaf.0o">karafo</ref>,
<ref cel="retort.0o">retorto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_k;&c_o;&c_n;</trd>
  <trd lng="cs">flakon</trd>
  <trdgrp lng="de">
     <trd>Flakon</trd>,
     <trd>Parf&uuml;mfl&auml;schchen</trd></trdgrp>
  <trdgrp lng="en">
    <trd>flacon</trd>,
    <trd>flask</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flacon</trd>,
    <trd>fiole</trd></trdgrp>
  <trdgrp lng="hu">
     <trd>flakon</trd>,
     <trd>&uuml;vegcse</trd>,
     <trd>fiola</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x3073;&#x3093;  <pr>&#x3053;&#x3073;&#x3093;</pr></trd>,
    <trd>&#x30D5;&#x30E9;&#x30B9;&#x30B3;</trd>
  </trdgrp>
  <trd lng="nl">flacon</trd>
  <trd lng="pl">flakon</trd>
  <trdgrp lng="pt">
    <trd>frasco</trd>,
    <trd>vidro</trd>,
    <trd>garrafinha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_l;&c_a;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_u;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>flak&oacute;n</trd>,
    <trd>f&lcaron;a&scaron;ti&ccaron;ka na vo&ncaron;avku</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_l;&c_a;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_sh;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EFB;&#x4F55;&#x74F6;&#x72B6;&#x5BB9;&#x5668; <pr>r&#xE8;nh&#xE9;p&#xED;ngzhu&#xE0;ngr&#xF3;ngq&#xEC;</pr></trd>,
    <trd>&#x9577;&#x9838;&#x74F6; <pr>ch&#xE1;ngj&#x1D0;ngp&#xED;ng</pr></trd>,
    <trd>&#x5C0F;&#x74F6; <pr>xi&#x1CE;op&#xED;ng</pr></trd>,
    <trd>&#x918B;&#x74F6;&#x5B50; <pr>c&#xF9;p&#xED;ngzi</pr></trd>,
    <trd>&#x53CC;&#x8F6E;&#x7684;&#x624B;&#x8F66; <pr>shu&#x101;ngl&#xFA;ndesh&#x1D2;uch&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
