    <kap>
      <rad>provincialism</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="provincialism.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Esprimo, lingva aparta&jcirc;o, komuna nur en iu provinco;
      <ref tip="sin" cel="provin.0ismo.LIN">provincismo</ref>:
      <ekz>
        tiuj &leftquot;novaj&rightquot; vortoj prezentos nenion alian ol <tld/>ojn de la unu lingvo
        internacia
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen ia kondukanto a&ubreve; le&gcirc;donanto la lingvo <klr>[&#8230;]</klr> &ccirc;iam pli unuformi&gcirc;is,
        la dialektoj kaj <tld/>oj iom post iom perdi&gcirc;is
        <fnt>
          <bib>Zam</bib>
          <lok>Kolektivismo en la kreado de la lingvo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Provinzialismus</trd>
    <trd lng="en">provincialism</trd>
    <trd lng="fr">provincialisme</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_c;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_m;</trd>,
  <trd>&c_d;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;&c_y;&c_z;&c_m;</trd>
</trdgrp>
</drv>

  </art>

