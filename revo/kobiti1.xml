<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kobiti1.xml,v 1.14 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>kobitided</rad>/oj
</kap>
<drv mrk="kobiti1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="ciprin.0oformaj">ciprinoformaj
      fi&scirc;oj</ref> (<trd lng="la">Cobitidae</trd>), de nesalakvaj fi&scirc;oj:
      <ekz>
        <ref tip="prt" cel="kobiti.0o">kobitido</ref> <klr>[&#x2026;]</klr>
        el la familio de <tld/>oj, estas e&ubreve;ropa fi&scirc;o nutranta
        sin per ma&ccirc;ado de sablo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=25243">Kobitido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_ju;&c_n;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Schmerlen</trd>,
    <trd>Grundeln</trd>
  </trdgrp>
  <trd lng="en">loach</trd>
  <trd lng="fr">cobitid&eacute;s</trd>
  <trd lng="hu">cs&iacute;kf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30C9;&#x30B8;&#x30E7;&#x30A6;&#x79D1; 
  <pr>&#x30C9;&#x30B8;&#x30E7;&#x30A6;&#x304B;</pr></trd>
  <trd lng="nl">modderkruipers<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">piskorzowate</trd>
  <trd lng="ro">cobitide</trd>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_v;&#x2019;&#x44E;&#x43D;&#x43E;&c_v;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x9C0D; <pr>qi&#x16B;</pr></trd>,
    <trd>&#x9C3C; <pr>x&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
