<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tauxr.xml,v 1.19 2019/10/18 17:10:22 revo Exp $">
<kap>
  <rad>ta&ubreve;r</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tauxr.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="tauxr.0o.ZOO">
    <dif>
      <ref tip="dif" cel="bov.vir0o">Virbovo</ref>:
      <ekz>
        du sova&gcirc;ajn <tld/>ojn ni arkpafe &ccirc;asis
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url
          ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
          rakontoj</url>. - 2005</vrk>
        </fnt>;
      </ekz>
      <ekz>
        dum la portugalaj <tld/>-bataloj oni ne mortigas <tld/>ojn kontraste kun la
        hispanaj
        <fnt>
          <bib>Monato</bib>
          <aut>Tatjana Loskutova</aut>
          <vrk><url ref="https://www.monato.be/2007/009164.php"
              >Voja&gcirc;o en Iberio</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la 1960aj jaroj stari&gcirc;is apud hispanaj &scirc;oseoj <tld/>o-figuroj &gcirc;is 14 metrojn
        altaj, kiuj reklamis brando-markon
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2005/008533.php"
              >Pri kato, rato, &scirc;tato kaj azeno</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        krom plenkreskuloj aktoris anka&ubreve; infanoj kaj <tld/>ido
        <fnt>
          <bib>Monato</bib>
          <aut>Tatjana Loskutova</aut>
          <vrk><url ref="https://www.monato.be/2007/009164.php"
              >Voja&gcirc;o en Iberio</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> en tiu &ccirc;i verko kaptas per la kornoj sian <tld/>on: li rezolute
        donas al ni romanon pri lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Probal Dasgupta</aut>
          <vrk><url ref="https://www.monato.be/2010/009859.php"
              >Pri homoj kaj lingvoj romanece</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_k;</trd>
  <trd lng="br">tarv</trd>
  <trd lng="ca">bou <klr>(brau)</klr></trd>
  <trd lng="cs">b&yacute;k</trd>
  <trd lng="de">Stier</trd>
  <trd lng="en">bull</trd>
  <trd lng="es">toro</trd>
  <trd lng="fa">&f_g;&a_A;&a_w; &a_n;&a_r;</trd>
  <trd lng="fr">taureau</trd>
  <trdgrp lng="he">
    <trd>&shin;&vav;&resh;</trd>,
    <trd>&pe;&resh;</trd>
  </trdgrp>
  <trd lng="hu">bika</trd>
  <trd lng="io">bovulo</trd>
  <trd lng="it">toro</trd>
  <trd lng="nl">stier</trd>
  <trd lng="no">okse</trd>
  <trdgrp lng="pl">
    <trd>byk</trd>,
    <trd>buhaj</trd>
  </trdgrp>
  <trd lng="pt">touro</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">b&yacute;k</trd>
  <trdgrp lng="vo">
    <trd>tor</trd>,
    <trd>hibub</trd>
  </trdgrp>
</drv>
</art>
