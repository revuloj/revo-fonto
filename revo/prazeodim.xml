<art mrk="$Id: prazeodim.xml,v 1.20 2024/02/17 07:52:35 revo Exp $">
<kap>
  <rad>prazeodim</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prazeodim.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Pr</mlg>
  <snc mrk="prazeodim.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="59">elemento</ref>,
      malofta ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>
      kun atomnumero 59, atompezo 140,907; lantanoido:
      <ekz>
        la elemento trovita de <nom>Berzelius</nom> kaj <nom>Hisinger</nom> estis, verdire, ia miksa&jcirc;o de
        cerio, lantano kaj <klr>[&#x2026;]</klr> ia kombina&jcirc;o de la elementoj <tld/>o kaj
        neodimo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=493762">Wilhelm von Hisinger</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_e;&c_a;&c_d;&c_y;&c_m;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_z;&c_je;&c_o;&c_d;&c_i;&c_m;</trd>
  <trd lng="br">prazeodim</trd>
  <trdgrp lng="cs">
    <trd>Pr</trd>,
    <trd>prazeodym</trd>
  </trdgrp>
  <trd lng="de">Praseodym</trd>
  <trd lng="en">praseodymum</trd>
  <trd lng="es">praseodimio</trd>
  <trd lng="fa">&f_p;&a_r;&a_A;&a_z;&a_y_hamza;&a_w;&a_d;&f_y;&a_m;&f_y;&a_damma;&a_m;</trd>
  <trd lng="fr">pras&eacute;odyme</trd>
  <trd lng="hu">prazeod&iacute;mium</trd>
  <trd lng="id">praseodimium</trd>
  <trd lng="it">praseodimio</trd>
  <trd lng="la">Praseodymium</trd>
  <trd lng="nl">praseodymium</trd>
  <trd lng="pl">prazeodym</trd>
  <trd lng="pt">praseod&iacute;mio</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_je;&c_o;&c_d;&c_i;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>Pr</trd>,
    <trd>prazeod&yacute;m</trd>
  </trdgrp>
  <trd lng="sv">praseodym</trd>
  <trd lng="uk">&#x43F;&#x440;&#x430;&#x437;&#x435;&#x43E;&#x434;&#x438;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x9420; <pr>p&#x1D4;</pr></trd>,
    <trd>&#x9568; <pr>p&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
