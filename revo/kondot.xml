<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kondotier</rad>/o
</kap>
<drv mrk="kondot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En mezepoko kaj renesanco, itala aventurema bandestro de dungataj
      soldatoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_a;&c_c;&c_mol;&c_je;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Condottiere</trd>,
    <trd>italienischer Freischarf&uuml;hrer</trd>
  </trdgrp>
  <trd lng="fr">condottiere</trd>
  <trd lng="nl">condottiere</trd>
  <trdgrp lng="pt">
    <trd>condottiero</trd>,
    <trd>condottiere</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_o;&c_t;&c_mol;&c_je;&c_r;</trd>
</drv>
</art>
