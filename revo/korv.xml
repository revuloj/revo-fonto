<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korv.xml,v 1.46 2023/12/03 09:58:59 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>korv</rad>/o
</kap>
<drv mrk="korv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="korv.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="korved.0oj"><tld/>edoj</ref>
      (<trd lng="la">Corvus</trd>) de grandaj pli malpli nigraj birdoj.
    </dif>
    <refgrp tip="sub">
      <ref cel="frugil.0o">frugilego</ref>,
      <ref cel="kornik.0o">korniko</ref>,
      <ref cel="moned.0o">monedo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/b/bc/Corvus_caurinus_-Stanley_Park%2C_Vancouver%2C_British_Columbia%2C_Canada-8.jpg/640px-Corvus_caurinus_-Stanley_Park%2C_Vancouver%2C_British_Columbia%2C_Canada-8.jpg" prm="CC BY-SA 2.0">markola <tld/>o (Corvus caurinus), Stanley Parko, Vancouver, Kanado
         <fnt>
           <aut>tgreyfox, stanley park - northwestern crowUploaded by Snowmanradio</aut>
           <lok><url ref="&WCW;/File:Corvus_caurinus_-Stanley_Park,_Vancouver,_British_Columbia,_Canada-8.jpg">&WCW;/File:Corvus_caurinus_-Stanley_Park,_Vancouver,_British_Columbia,_Canada-8.jpg</url></lok>
         </fnt>
    </bld>
    <bld lok="&WCU;/thumb/8/8d/Hooded_Crow_%28Corvus_cornix%29_%2811%29.jpg/480px-Hooded_Crow_%28Corvus_cornix%29_%2811%29.jpg" prm="CC BY-SA 3.0">korniko (Corvus cornix), Bolle di Magadino, Ticino, Svislando
         <fnt>
           <aut>Ken Billington, propra verko</aut>
           <lok><url ref="&WCW;/File:Hooded_Crow_(Corvus_cornix)_(11).jpg">&WCW;/File:Hooded_Crow_(Corvus_cornix)_(11).jpg</url></lok>
         </fnt>
    </bld>
    <trd lng="be">&c_v;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
    <trd lng="bg">&c_g;&c_a;&c_r;&c_v;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>corb</trd>,
      <trd>cornella</trd>,
      <trd>gralla</trd>,
      <trd>graula</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Rabe <klr>(la grandaj specioj)</klr></trd>,
      <trd>Kr&auml;he <klr>(la pli malgrandaj specioj)</klr></trd>
    </trdgrp>
    <trd lng="en">crow</trd>
    <trd lng="es">cuervo</trd>
    <trd lng="fa">&f_k;&a_l;&a_A;&a_gh;</trd>
    <trd lng="fr">corbeau</trd>
    <trd lng="nl">kraai</trd>
    <trd lng="hu">varj&uacute;</trd>
    <trd lng="pl">kruk</trd>
    <trd lng="zh">&#x4E4C; <pr>w&#x16B;</pr></trd>
</snc>
  <snc mrk="korv.0o.korako">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La plej granda <ref cel="speci.0o.BIO" tip="lst" lst="voko:zoologiaj_specioj">specio</ref> el
      tiu 
      <ref tip="super" cel="korv.0o">genro</ref>
      (<trd lng="la">Corvus corax</trd>):
      <ekz><ind><tld/>o al <tld/>o okulon ne pikas</ind>
        <klr>(friponoj ne interbatalas)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="be">
          <trd>&c_k;&c_r;&c_u;&c_m;&c_k;&c_a;&c_ch; &c_k;&c_r;&c_u;&c_m;&c_k;&c_a;&c_ch;&c_u; &c_v;&c_o;&c_k;&c_a; &c_n;&c_ja; &c_v;&c_y;&c_d;&c_z;&c_ja;&c_w;&c_b;&c_je;</trd>,
          <trd>&c_k;&c_r;&c_u;&c_k; &c_k;&c_r;&c_u;&c_k;&c_a; &c_z;&c_a; &c_ch;&c_u;&c_b; &c_n;&c_je; &c_b;&c_ja;&c_r;&c_e; </trd>
        </trdgrp>
        <trd lng="pl">kruk krukowi oka nie wykole</trd>
      </ekz>
      <ekz>
        haroj buklitaj, nigraj kiel korvo
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        venis korvoj kaj kornikoj, unuj pli nigraj ol la aliaj, en
        grandaj svarmoj
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
          filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="korak.0o.korvo">korako</ref>
    <bld lok="&WCU;/thumb/4/42/Female_adult_raven.jpg/480px-Female_adult_raven.jpg" prm="CC BY-SA 3.0"><tld/>ino (Corvus corax)

         <fnt>
           <aut>Bombtime, propra verko</aut>
           <lok><url ref="&WCW;/File:Female_adult_raven.jpg">&WCW;/File:Female_adult_raven.jpg</url></lok>
         </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_u;&c_g;&c_a;&c_n;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_m;&c_k;&c_a;&c_ch;</trd>
    </trdgrp>
    <trd lng="bg">&c_g;&c_a;&c_r;&c_v;&c_a;&c_n;
    &c_g;&c_r;&c_o;&c_b;&c_a;&c_r;</trd>
    <trd lng="ca">corb</trd>
    <trd lng="cs">krkavec velk&yacute;</trd>
    <trd lng="de">Kolkrabe</trd>
    <trd lng="en">raven</trd>
    <trd lng="fa">&a_z;&a_A;&a_gh;</trd>
    <trd lng="fr">grand corbeau</trd>
    <trd lng="hu">holl&oacute;</trd>
    <trd lng="id">gagak</trd>
    <trd lng="nl">raaf</trd>
    <trd lng="pl">kruk</trd>
    <trd lng="pt">corvo</trd>
    <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>havran</trd>,
    <trd>krkavec</trd>
  </trdgrp>
  <trd lng="ro">corb</trd>
  <trd lng="sk">krkavec</trd>
</drv>

<drv mrk="korv.K0o">
  <kap><tld lit="K"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Crv</mlg>
  <snc mrk="korv.K0o.AST">
    <uzo tip="fak">AST</uzo> 
    <dif>
      Sud-hemisfera
      <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> 
      (<trd lng="la">Corvus</trd>):
      <ekz>
        NGC 4361 estas planeduma nebulozo en konstelacio <tld lit="K"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=117193">NGC 4361</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_r;&c_u;&c_g;&c_a;&c_n; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="ca">el <ind>Corb</ind></trd>
  <trdgrp lng="cs">
    <trd>havran</trd>,
    <trd>krkavec</trd>
  </trdgrp>
  <trd lng="de">Rabe</trd>
  <trd lng="en">Corvus</trd>
  <trd lng="es">Cuervo</trd>
  <trd lng="fa">&a_gh;&a_r;&a_A;&a_b;</trd>
  <trd lng="fr">Corbeau<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30B9;  <pr>&#x304B;&#x3089;&#x3059;</pr></trd>
  <trd lng="nl">Raaf<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Kruk  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Corbul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="sk">krkavec</trd>
  <trd lng="zh">&#x4E4C;&#x9E26;&#x5EA7; <pr>W&#x16B;y&#x101;zu&#xF2;</pr></trd>
</drv>

<drv mrk="korv.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="korv.0eto.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <fnt><bib>Bird</bib></fnt>
    <dif>
      <ref tip="dif" cel="moned.0o">Monedo</ref>:
      <ekz>
        rideton sendu al la <tld/>eto / al vi fluganta el maro dista
        <fnt>
          <vrk><url ref="https://www.stihi.ru/2013/11/04/10214">Se vin lacigos malica vento...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d8/CROP._Corvus_monedula%2C_naakka%2C_Naantalin_hautausmaa%2C_Naantali%2C_8.11.2011..jpg/640px-CROP._Corvus_monedula%2C_naakka%2C_Naantalin_hautausmaa%2C_Naantali%2C_8.11.2011..jpg" prm="CC BY-SA 3.0"><tld/>eto  (Corvus monedula) Naantali, Finlando
         <fnt>
           <aut>Markus Rantala (Makele-90), propra verko</aut>
           <lok><url ref="&WCW;/File:CROP._Corvus_monedula,_naakka,_Naantalin_hautausmaa,_Naantali,_8.11.2011..jpg">&WCW;/File:CROP._Corvus_monedula,_naakka,_Naantalin_hautausmaa,_Naantali,_8.11.2011..jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="korvet.0o">korveto (vel&scirc;ipo)</ref>
  <trd lng="be">&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="ca">gralla</trd>
  <trdgrp lng="cs">
    <trd>kavka</trd>,
    <trd>kavka obecn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Dohle</trd>
  <trd lng="en">jackdaw</trd>
  <trd lng="fa">&a_z;&a_A;&a_gh;&f_ch;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>choucas</trd>
  </trdgrp>
  <trd lng="hu">cs&oacute;ka</trd>
  <trd lng="ja">&#x30B3;&#x30EB;&#x30D9;&#x30C3;&#x30C8;</trd>
  <trd lng="nl">torenkraai</trd>
  <trd lng="pl">kawka</trd>
  <trd lng="ro">st&abreve;ncu&#x21B;&abreve;</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">&lcaron;ahk&aacute; vojensk&aacute; plachetnica</trd>
  <trdgrp lng="zh">
    <trd>&#x5BD2;&#x9D09; <pr>h&#xE1;ny&#x101;</pr></trd>,
    <trd>&#x7A74;&#x9CE5; <pr>xu&#xE9;ni&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
</art>
