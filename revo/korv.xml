<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>korv</rad>/o
</kap>
<drv mrk="korv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="korv.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Corvus</trd>) de grandaj pli malpli nigraj birdoj.
    </dif>
    <refgrp tip="sub">
      <ref cel="frugil.0o">frugilego</ref>,
      <ref cel="kornik.0o">korniko</ref>,
      <ref cel="moned.0o">monedo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Rabe <klr>(la grandaj specioj)</klr></trd>,
      <trd>Kr&auml;he <klr>(la pli malgrandaj specioj)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      La plej granda specio el tiu genro
      (<trd lng="la">Corvus corax</trd>):
      <ekz>
        <tld/>o al <tld/>o okulon ne pikas<fnt>Z</fnt>
        <klr>(friponoj ne interbatalas)</klr>;
      </ekz>
      <ekz>
        &scirc;iaj <tld/>enigraj haroj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="cs">krkavec velk&yacute;</trd>
    <trd lng="de">Kolkrabe</trd>
    <trd lng="en">raven</trd>
    <trd lng="hu">holl&oacute;</trd>
    <trd lng="pl">kruk</trd>
    <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_n;</trd>
  </snc>
</drv>
<drv mrk="korv.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="paser.0oformaj">paseroformaj birdoj</ref>
      (<trd lng="la">Corvidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Rabenv&ouml;gel</trd>
</drv>
</art>
