<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>korv</rad>/o
</kap>
<drv mrk="korv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="korv.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="korved.0oj"><tld/>edoj</ref>
      (<trd lng="la">Corvus</trd>) de grandaj pli malpli nigraj birdoj.
    </dif>
    <refgrp tip="sub">
      <ref cel="frugil.0o">frugilego</ref>,
      <ref cel="kornik.0o">korniko</ref>,
      <ref cel="moned.0o">monedo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Rabe <klr>(la grandaj specioj)</klr></trd>,
      <trd>Kr&auml;he <klr>(la pli malgrandaj specioj)</klr></trd>
    </trdgrp>
    <trd lng="en">crow</trd>
    <trd lng="es">cuervo</trd>
    <trd lng="fr">corbeau</trd>
    <trd lng="hu">varj&uacute;</trd>
  </snc>
  <snc mrk="korv.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La plej granda specio el tiu genro
      (<trd lng="la">Corvus corax</trd>):
      <ekz><tld/>o al <tld/>o okulon ne pikas
        <klr>(friponoj ne 
interbatalas)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        &scirc;iaj <tld/>enigraj haroj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/korv.jpg">
      <tld/>o <klr>(Corvus corax)</klr>
    </bld>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_u;&c_g;&c_a;&c_n;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_m;&c_k;&c_a;&c_ch;</trd>
    </trdgrp>
    <trd lng="cs">krkavec velk&yacute;</trd>
    <trd lng="de">Kolkrabe</trd>
    <trd lng="en">raven</trd>
    <trd lng="fr">grand corbeau</trd>
    <trd lng="hu">holl&oacute;</trd>
    <trd lng="pl">kruk</trd>
    <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_n;</trd>
  </snc>
</drv>

<drv mrk="korv.K0o">
  <kap><tld lit="K"/>o</kap>
  <mlg>Crv</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="super" 
cel="stel.0figuro">stelfiguro</ref> (<trd lng="la">Corvus</trd>).
    </dif>
  </snc>
  <trd lng="de">Rabe</trd>
  <trd lng="en">Corvus</trd>
  <trd lng="es">Cuervo</trd>
</drv>

<drv mrk="korv.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="korv.0eto.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <fnt><bib>Bird</bib></fnt>
    <ref tip="dif" cel="moned.0o">monedo</ref>
  </snc>
  <trd lng="cs">kavka obecn&aacute;</trd>
  <trd lng="de">Dohle</trd>
  <trd lng="en">jackdaw</trd>
  <trdgrp lng="fr">
    <trd>choucas</trd>
  </trdgrp>
  <trd lng="hu">cs&oacute;ka</trd>
  <trd lng="nl">torenkraai</trd>
  <trd lng="pl">kawka</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
