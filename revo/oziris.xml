<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oziris.xml,v 1.14 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>Oziris</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="oziris.0o">
  <kap><tld/>o</kap>
  <snc mrk="oziris.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Antikva egipta <ref tip="super" cel="di.0o.politeisma">dio</ref>
      viviga, multaspekta: de vegeta&jcirc;aro, de luno a&ubreve; suno,
      de mortintoj..., edzo de
      <ref tip="vid" cel="izis.0o.MIT">Iziso</ref>:
      <ekz>
        la dio Set, kvankam li estas la vera frato de la suna <tld/>o,
        malamas lin
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_z;&c_y;&c_r;&c_y;&c_s;</trd>,
    <trd>&c_A;&c_s;&c_y;&c_r;&c_y;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">Osiris</trd>
  <trd lng="cs">Osiris</trd>
  <trd lng="de">Osiris</trd>
  <trd lng="fr">Osiris</trd>
  <trd lng="hu">Ozirisz</trd>
  <trd lng="ja">&#x30AA;&#x30B7;&#x30EA;&#x30B9;</trd>
  <trd lng="nl">Osiris</trd>
  <trd lng="pl">Ozyrys</trd>
  <trd lng="pt">Os&iacute;ris</trd>
  <trd lng="ru">&c_O;&c_s;&c_i;&c_r;&c_i;&c_s;</trd>
  <trd lng="sk">Osiris</trd>
  <trd lng="uk">&#x41E;&c_s;&c_i;&c_r;&c_i;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x6B27;&#x897F;&#x91CC;&#x65AF; <pr>&#x14D;ux&#x12B;l&#x1D0;s&#x12B;</pr></trd>,
    <trd>&#x6B50;&#x897F;&#x88E1;&#x65AF; <pr>&#x14D;ux&#x12B;l&#x1D0;s&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

