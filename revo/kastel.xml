<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kastel</rad>/o
</kap>
<drv mrk="kastel.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fortikigita lo&gcirc;loko, defendita  per fosa&jcirc;o, remparoj kaj turoj
    </dif>
    <refgrp tip="sin">
      <ref cel="citade.0o">citadelo</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>castle</trd>,
      <trd>citadel</trd>,
      <trd>stronghold</trd>,
      <trd>fort</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Luksa sinjora domego en la kamparo: (f)
      <ekz>
        konstrui <tld/>ojn en aero<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sur glacio<fnt>Z</fnt>
        <klr>(revi &hcirc;imerajn projektojn)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>castle</trd>,
      <trd>manse</trd>,
      <trd>mansion</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">hrad</trd>
  <trdgrp lng="nl">
    <trd>kasteel</trd>,
    <trd>slot <klr tip="ind">(kasteel)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_k; <klr>(&c_s;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
