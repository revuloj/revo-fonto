<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kastel</rad>/o
</kap>
<drv mrk="kastel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kastel.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Lo&gcirc;loko, defendata kiel <ref tip="super"
      cel="fortik.0ajxo">fortika&jcirc;o</ref> per fosa&jcirc;o, remparoj kaj turoj.
    </dif>
    <trd lng="de">Burg</trd>
    <trdgrp lng="en">
      <trd>castle</trd>,
      <trd>citadel</trd>,
      <trd>stronghold</trd>,
      <trd>fort</trd>
    </trdgrp>
  </snc>
  <snc mrk="kastel.0o.palaco">
    <dif>
      <ref tip="super" cel="konstr.0ajxo">Konstrua&jcirc;o</ref> servanta kiel
      luksa sinjora domego en la kamparo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konstrui <tld/>ojn en aero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konstrui <tld/>on sur glacio
        <klr>(revi &hcirc;imeran projekton)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ctl><tld lit="K"/>o de Prelongo</ctl>
        <klr>(unua originala romano en Esperanto de Henri Vallienne)</klr>
        <fnt><aut>Vallienne Henri</aut>, <vrk>Kastelo de Prelongo. -
        Parizo: Hachette, 1907</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="palac.0o">palaco</ref>
    </refgrp>
    <trd lng="de">Schloss</trd>
    <trdgrp lng="en">
      <trd>castle</trd>,
      <trd>manse</trd>,
      <trd>mansion</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_k;</trd>
  <trd lng="cs">hrad</trd>
  <trd lng="fr">ch&acirc;teau</trd>
  <trdgrp lng="nl">
    <trd>kasteel</trd>,
    <trd>slot <klr tip="ind">(kasteel)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_k; <klr>(&c_s;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
