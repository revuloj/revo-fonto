<vortaro>
<art mrk="$Id$">
<kap>
   <rad>klepsidr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klepsidr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KAL</uzo>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <ref tip="dif" cel="horlog.akvo0o">akvohorlo&gcirc;o</ref>
     <ekz>
       <tld/>a akvo balda&ubreve; &ccirc;esos guti
       <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 76a, p. 606a</lok></fnt>.
     </ekz>
   </snc>
   <trd lng="be">&c_k;&c_l;&c_je;&c_p;&c_s;&c_ib;&c_d;&c_r;&c_a;</trd>
   <trd lng="pt">clepsidra</trd>
   <trdgrp lng="fr">
     <trd>clepsydre</trd>,
     <trd>horloge &agrave; eau</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>v&iacute;z&oacute;ra</trd>,
     <trd>klepszidra</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>klepsydra</trd>,
     <trd>zegar wodny</trd>
   </trdgrp>
   <trd lng="ro">clepsidr&#x103;</trd>
   <trd lng="ru">&c_k;&c_l;&c_je;&c_p;&c_s;&c_i;&c_d;&c_r;&c_a;</trd>
</drv>
</art>

