<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>&scirc;tof</rad>/o
</kap>
<drv mrk="sxtof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxtof.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="teks.0ajxo.TEKS">teksa&jcirc;o <sncref/></ref>
    <ekz>
      kvar metroj da tiu &ccirc;i <tld/>o kostas na&ubreve; frankojn
      <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
    </ekz>
    <ekz>
      lana, silka, kotona <tld/>o;
    </ekz>
    <ekz>
      blankaj, ru&gcirc;aj kaj bluaj <tld/>oj pendis sur marmoraj
      kolonoj<fnt>Z</fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
      <trd>&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="el">&ypsilon_ton;&phi;&alfa;&sigma;&my;&alfa;</trd>
    <trd lng="es">tela</trd>
    <trd lng="fr">&eacute;toffe</trd>
    <trdgrp lng="pt">
      <trd>estofo</trd>,
      <trd>pano</trd>,
      <trd>tecido</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ja;<klr tip="ind"> (&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
    </trdgrp>
    <trd lng="sv">tyg</trd>
  </snc>
  <snc mrk="sxtof.0o.materialo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Konsistiga materia substanco havanta proprajn ecojn;
      <ref tip="sin" cel="materi1.0o.substanco">materialo</ref>:
      <ekz>
        la <tld/>o, el kiu estas farataj la son&gcirc;oj.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;</trd>
  </snc>
  <snc mrk="sxtof.0o.histo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Da&ubreve;ra kunmeta&jcirc;o da difinitaj anatomiaj elementoj kompareblaj al
      teksa&jcirc;o; <ref tip="sin" cel="hist.0o">histo</ref>.
    </dif>
    <trd lng="be">&c_t;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
    <trd lng="es">tejido</trd>
    <trd lng="ru">&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
  </snc>
  <trd lng="de">Stoff</trd>
  <trd lng="en">material</trd>
  <trd lng="fr">&eacute;toffe</trd>
  <trdgrp lng="hu">
    <trd>anyag<klr tip="ind"> (sz&ouml;vet)</klr></trd>,
    <trd>sz&ouml;vet</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stof<klr tip="ind"> (weefsel)</klr></trd>,
    <trd>weefsel</trd>
  </trdgrp>
</drv>
</art>
