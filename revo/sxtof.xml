<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>&scirc;tof</rad>/o
</kap>
<drv mrk="sxtof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksa&jcirc;o:
      <ekz>
        4 metroj de tiu &ccirc;i <tld/>o kostas 9 frankojn<fnt>Z</fnt>,
      </ekz>
      <ekz>
        lana, silka, kotona <tld/>o;
      </ekz>
      <ekz>
        blankaj, ru&gcirc;aj kaj bluaj <tld/>oj pendis sur marmoraj
        kolonoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o, el kiu estas farataj la son&gcirc;oj.
      </ekz>
    </dif>
    <trd lng="el">&ypsilon_ton;&phi;&alfa;&sigma;&my;&alfa;</trd>
  <trd lng="es">tela</trd>
    <trd lng="fr">&eacute;toffe</trd>
    <trdgrp lng="pt">
      <trd>estofo</trd>,
      <trd>tecido</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ja;<klr tip="ind"> (&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
    </trdgrp>
    <trd lng="sv">tyg</trd>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Konsistiga materia substanco havanta proprajn ecojn;
      <ref tip="sin" cel="materi1.0o.substanco">materialo</ref>.
    </dif>
    <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;</trd>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Da&ubreve;ra kunmeta&jcirc;o da difinitaj anatomiaj elementoj kompareblaj al
      teksa&jcirc;o; <ref tip="sin" cel="hist.0o">histo</ref>.
    </dif>
    <trd lng="es">tejido</trd>
    <trd lng="ru">&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
  </snc>
  <trd lng="de">Stoff</trd>
  <trd lng="en">material</trd>
  <trdgrp lng="nl">
    <trd>stof<klr tip="ind"> (weefsel)</klr></trd>,
    <trd>weefsel</trd>
  </trdgrp>
</drv>
</art>
