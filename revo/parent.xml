<vortaro>
   <art mrk="$Id$">
     <kap>
       <ofc>*</ofc>
       <rad>parentez</rad>/o
     </kap>
     <drv mrk="parent.0o">
       <kap><ofc>*</ofc><tld/>o</kap>
       <snc mrk="parent.0o.subfrazo">
         <uzo tip="fak">GRA</uzo>
         <dif>
           Klariga kromparto de teksto (vorto, subfrazo a&ubreve; frazoj),
           apartigita per specialaj interpunkciaj signoj, kutime
           <ref cel="kramp.0o.TIP">krampoj</ref> a&ubreve; haltostrekoj.
         </dif>
         <trdgrp lng="de">
            <trd>Parenthese</trd>,
            <trd>Anmerkung</trd>,
            <trd>Einschub</trd>
         </trdgrp>
	 <trdgrp lng="es">
	   <trd>par&eacute;ntesis</trd>,
	   <trd>inciso<klr> (interrupci&oacute;n para aclarar)</klr></trd>
	 </trdgrp>
         <trd lng="hu">z&aacute;r&oacute;jeles megjegyz&eacute;s</trd>         
         <trd lng="nl">parenthese</trd>
         <trd lng="pt">par&ecirc;ntese</trd>
         <trd lng="ru">&c_t;&c_je;&c_k;&c_s;&c_t; &c_v; 
<ind>&c_s;&c_k;&c_o;&c_b;&c_k;&c_a;&c_h;</ind></trd>
       </snc>
       <snc mrk="parent.0o.krampo">
         <uzo tip="fak">TIP</uzo>
         <dif>
           <ref tip="dif" cel="kramp.0o.TIP">Krampo</ref>.
         </dif>
         <trd lng="be">&c_d;&c_u;&c_zh;&c_k;&c_a;</trd>
         <trd lng="de">Klammer</trd>
         <trd lng="hu">z&aacute;r&oacute;jel</trd>         
         <trd lng="nl">haakje</trd>
         <trd lng="ru">&c_s;&c_k;&c_o;&c_b;&c_k;&c_a;</trd>
         <trd lng="es">par&eacute;ntesis</trd>
       </snc>
       <trd lng="ca">par&egrave;ntesi</trd>
       <trd lng="en">parenthesis</trd>
       <trd lng="fr">parenth&egrave;se</trd>
       <trd lng="pt">par&ecirc;ntese <klr>(sinal gr&aacute;fico)</klr></trd>
     </drv>
   </art>
