<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>amalgam</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="amalga.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Kunmiksi hidrargon kun metaloj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>mit Quecksilber <ind>legieren</ind></trd>,
    <trd>amalgamieren</trd>
  </trdgrp>
  <trd lng="fr">amalgamer</trd>
  <trd lng="nl">amalgameren</trd>
  <trd lng="ru">&c_a;&c_m;&c_a;&c_l;&c_mol;&c_g;&c_a;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="amalga.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kunmiksa&jcirc;o de hidrargo kun metaloj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Quecksilberlegierung</trd>,
    <trd>Amalgam</trd>
  </trdgrp>
  <trd lng="fr">amalgame</trd>
  <trd lng="nl">amalgaam</trd>
  <trd lng="ru">&c_a;&c_m;&c_a;&c_l;&c_mol;&c_g;&c_a;&c_m;&c_a;</trd>
</drv>
</art>
