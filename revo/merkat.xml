    <trd>&#x43F;&#x43E;&#x43F;&c_i;&c_t; &c_t;&c_a; &#x43F;&c_r;&#x43E;&#x43F;&#x43E;&c_z;&c_i;&c_c;&c_ib;&#x44F; &#x43D;&c_a;
