<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>merkat</rad>/o
</kap>
<drv mrk="merkat.0o">
  <kap><tld/>o</kap>
  <snc mrk="merkat.0o.EKON">
    <tezrad fak="EKON"/>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Stato de a&ccirc;etkapablo kaj a&ccirc;etemo pri iu <ref tip="prt" cel="komerc.0ajxo">komerca&jcirc;o</ref>
      a&ubreve; borsa valoro:
      <ekz>
        la koton<tld/>o;
      </ekz>
      <ekz>
        tiu varo havas vastan <tld/>on;
      </ekz>
      <ekz>
        ser&ccirc;i al si novajn <tld/>ojn;
      </ekz>
      <ekz>
        la &ccirc;ina bilo<tld/>o funkcias kiel barometro de la
        ekonomio
        <fnt>
        <aut>Huang Tingsheng</aut>,
        <vrk>La &ccirc;ina bilomerkato</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 8a, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="ekonom1.0o.situacio">ekonomio</ref>
    <rim>
      <em>merkato</em> neniam signifas placon a&ubreve; ejon.
    </rim>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_n;&c_a;&c_k; <klr>(&c_ja;&c_k; &c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_t;&c_e;&c_g;&c_o;&c_r;&c_y;&c_ja;)</klr></trd>
  <trd lng="de">Markt</trd>
  <trd lng="en">market</trd>
  <trd lng="es">mercado</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;bouch&eacute; commercial</trd>,
    <trd>march&eacute;</trd>
  </trdgrp>
  <trd lng="hu">piac</trd>
  <trdgrp lng="nl">
    <trd>markt<klr tip="ind"> (afzetgebied)</klr></trd>,
    <trd>afzetgebied</trd>
  </trdgrp>
  <trd lng="pt">mercado <klr>(econ.)</klr></trd>
  <trd lng="ru">&c_r;&c_y;&c_n;&c_o;&c_k; <klr>(&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_p;&c_o;&c_n;&c_ja;&c_t;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="merkat.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="merkat.en0igi.EKON">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="merkat.sur0igi">sur<tld/>igi</ref>
    <ekz>
      malperfekta&jcirc;oj ekzistas nur en la en<tld/>igo
      <fnt>
      <aut>Evgeni Georgiev</aut>,
      <vrk>A&ubreve;strio: lando de ekologia kultivado</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 1997a, numero 9a, p. 19a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="es">comercializar</trd>
  <trdgrp lng="fr">
    <trd>commercialiser</trd>,
    <trd>lancer<klr> (un produit)</klr></trd>,
    <trd>mettre sur le <ind>march&eacute;</ind></trd>
  </trdgrp>
   <trdgrp lng="nl">
    <trd>commercialiseren</trd>,
    <trd>op de <ind>markt</ind> brengen</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_r;&c_y;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="merkat.sur0igi">
  <kap>sur<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="merkat.sur0igi.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ekvendi novan produkton, aran&gcirc;ante &gcirc;ian prireklamadon, liveradon,
      uzhelpon:
      <ekz>
        eta kompanio, kiu sur<tld/>igis komputilan programon, kiu tuj
        tutmonde furoras, i&gcirc;as ege valora
        <fnt>
        <aut>Roland Rotsaert</aut>,
        <vrk>Piramidaj investoj: ne nur en Albanio!</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1997a, numero 7a, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="merkat.en0igi">en<tld/>igi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lancx.0i.sociajxon">lan&ccirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="es">comercializar</trd>
  <trdgrp lng="fr">
    <trd>commercialiser</trd>,
    <trd>lancer<klr> (un produit)</klr></trd>,
    <trd>mettre sur le <ind>march&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>commercialiseren</trd>,
    <trd>op de <ind>markt</ind> brengen</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_r;&c_y;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="merkat.labor0o">
  <kap>labor<tld/>o</kap>
  <snc mrk="merkat.labor0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Momenta kaj loka stato de dungemo, senlaboreco, salajroj:
      <ekz>
        elastaj labortempoj kreis en la lastaj dek jaroj pli flekseblan
        labor<tld/>on
        <fnt>
        <aut>Evgeni Georgiev</aut>,
        <vrk>Pli maljunaj, pli sa&gcirc;aj, pli flekseblaj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 3a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_n;&c_a;&c_k; &c_p;&c_r;&c_a;&c_c;&c_y;</trd>
  <trd lng="es"><ind>mercado</ind> de trabajo</trd>
  <trdgrp lng="fr">
    <trd>march&eacute; du travail</trd>
  </trdgrp>
  <trd lng="nl">arbeidsmarkt</trd>
  <trd lng="ru">&c_r;&c_y;&c_n;&c_o;&c_k; &c_t;&c_r;&c_u;&c_d;&c_a;</trd>
</drv>

<drv mrk="merkat.nigra0o">
  <kap>nigra <tld/>o</kap>
  <snc mrk="merkat.nigra0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Kontra&ubreve;le&gcirc;a <tld/>o sen vendimposta pago:
      <ekz>
        ekzistas la mondo de la rapida mono, drogo-vendado, nigra <tld/>o
        <fnt>
        <aut>J. Schram</aut>,
        <vrk>Re: Mortpuno</vrk>,
        <lok>soc.culture.esperanto, 1998-04-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">march&eacute; noir</trd>
  <trd lng="nl">zwarte <ind>markt</ind></trd>
</drv>
</art>
