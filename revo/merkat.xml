  <trd lng="pt">mercado <klr>(econ.)</klr></trd>
  <trd lng="ru">&c_r;&c_y;&c_n;&c_o;&c_k;
  <klr>(&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  &c_p;&c_o;&c_n;&c_ja;&c_t;&c_i;&c_je;)</klr></trd>
