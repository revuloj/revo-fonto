<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>merkat</rad>/o
</kap>
<drv mrk="merkat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Stato de a&ccirc;etkapablo kaj a&ccirc;etemo pri iu komerca&jcirc;o
      a&ubreve; borsa valoro:
      <ekz>
        la koton<tld/>o;
      </ekz>
      <ekz>
        tiu varo havas vastan <tld/>on;
      </ekz>
      <ekz>
        ser&ccirc;i al si novajn <tld/>ojn.
      </ekz>
    </dif>
    <rim>
      <em>merkato</em> neniam signifas placon a&ubreve; ejon.
    </rim>
  </snc>
  <trd lng="de">Markt</trd>
  <trd lng="en">market</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;bouch&eacute; commercial</trd>,
    <trd>march&eacute;<klr> (d&eacute;bouch&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">piac</trd>
  <trdgrp lng="nl">
    <trd>markt<klr tip="ind"> (afzetgebied)</klr></trd>,
    <trd>afzetgebied</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_y;&c_n;&c_o;&c_k; <klr>(&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
&c_p;&c_o;&c_n;&c_ja;&c_t;&c_i;&c_je;)</klr></trd>
</drv>
</art>
