<drv mrk="merkat.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="merkat.en0igi.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="merkat.sur0igi">Sur<tld/>igi</ref>:
      <ekz>
        malgranda firmao <klr>[&#x2026;]</klr> provis en<tld/>igi kurson sur kompakta disko pri aviada
        lingvo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007159.php">Flugilhavaj vortoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        malperfekta&jcirc;oj ekzistas nur en la en<tld/>igo
        <fnt>
        <aut>Evgeni Georgiev</aut>,
        <vrk>A&ubreve;strio: lando de ekologia kultivado</vrk>,
        <bib>Monato</bib>,
        <lok>1997/09, p. 19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol; &c_n;&c_a; <ind>&c_r;&c_y;&c_n;&c_a;&c_k;</ind></trd>,
    <trd>&c_v;&c_y;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib; &c_n;&c_a; <ind>&c_r;&c_y;&c_n;&c_a;&c_k;</ind></trd>
