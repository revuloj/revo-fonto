<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc><rad>ruman</rad>/o
        <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ruman.0o">
  <kap><tld/>o,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="ruman.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="ruman.R0ujo"><tld lit="R"/>ujo</ref>:
      <ekz>
        akra malla&ubreve;do kontra&ubreve; Poloj, Rusoj, a&ubreve; <tld lit="R"/>oj malpla&ccirc;is al li
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj volis, ke la ponto staru tiel, ke trafiko inter Grekio kaj okcidenta E&ubreve;ropo pasu tra Rumanio
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni GEORGIEV</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007150.html">Anseroj kaj azeno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ruman.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="cs">Rumun</trd>
  <trd lng="de">Rum&auml;ne</trd>
  <trd lng="en">Romanian <klr> (subst.)</klr></trd>
  <trdgrp lng="fr"> 
    <trd>Roumain</trd>
    <trd>Roumaine<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="pt">romeno<klr> (subst.)</klr></trd>
  <trd lng="sk">Rumun</trd>
</drv>

<drv mrk="ruman.0a">
  <kap><tld/>a
     <fnt>
      <bib>Ifigenio</bib>
      <lok>pa&gcirc;o 109, Verkaro de Dro L. L. Zamenhof</lok>
    </fnt>
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
         la rumana &scirc;tato
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9309-noticoj.txt">Esperanto sen peno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tata <tld/>a riverkompanio deklaris, ke se la &scirc;ipveturado la&ubreve; Danubo longtempe ne funkcios
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006857.html">Trafiko: Danuba &scirc;iptrafiko paralizita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pli ol unu miliono da senlaboruloj, kio egalas al 9,3 centonoj de la <tld/>a lo&gcirc;antaro
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9309-noticoj.txt">Esperanto sen peno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
   <trd lng="en">Romanian<klr> (adj.)</klr></trd>
   <trdgrp lng="fr"> 
    <trd>roumain</trd>
    <trd>roumaine<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc mrk="ruman.0a.LING">
    <dif>
      La <tld/>a <ref tip="super" cel="lingv.0o">lingvo</ref>:
      <ekz>
        inter la e&ubreve;ropaj lingvoj, mi trovas pli klare a&ubreve;deblaj kaj rekoneblaj la normigitan germanan kaj la <tld/>an
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MACAOIDH</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007159.html">Flugilhavaj vortoj</url></vrk>
        </fnt>.
      </ekz>
   </dif>
   <trd lng="en">Romanian<klr> (language)</klr></trd>
   <trd lng="fr">roumain <klr>(langue)</klr></trd>
  </snc>
  <trd lng="de">rum&auml;nisch</trd>
  <trd lng="pt">romeno<klr> (adj.)</klr></trd>
</drv>

<drv mrk="ruman.R0ujo">
  <kap>
    <tld lit="R"/>ujo
     <fnt>
      <bib>Ifigenio</bib>
      <lok>pa&gcirc;o 110, La Revuo</lok>
    </fnt>
  </kap>
  <mlg kod="ISO-3166">RO</mlg>
  <snc mrk="ruman.R0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la centra <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>;
      limanta kun
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>,
      <ref tip="vid" cel="moldav.M0ujo.GEOG">Moldavujo</ref>,
      <ref tip="vid" cel="ukrajn.U0ujo">Ukrajnujo</ref>
      kaj <ref cel="mar.NigraM0o">Nigra Maro</ref>,
      kaj kun la &ccirc;efurbo Bukare&scirc;to:
      <ekz>
        &ccirc;ar la plimulto jam de longe uzas &leftquot;<tld lit="R"/>ujo&rightquot; kaj &leftquot;Bukare&scirc;to&rightquot;, 
        kaj &ccirc;ar ekzistas nenia grava ka&ubreve;zo por tion &scirc;an&gcirc;i, tial mi konsilas lasi la formojn &gcirc;isnunajn
        <fnt>
          <bib>LR</bib>
          <lok>2. Transskribo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hunor Kelemen</nom> <klr>[estis]</klr> deputito de RMDSZ en parlamento de <tld lit="R"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=249409">Hunor Kelemen</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rumani.R0o.GEOG">Rumanio</ref>
  </snc>
  <trd lng="bg">&c_R;&c_u;&c_m;&c_malmol;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">Rum&auml;nien</trd>
  <trd lng="en">Romania</trd>
  <trd lng="fa">&a_r;&a_w;&a_m;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">Roumanie</trd>
  <trd lng="hu">Rom&aacute;nia</trd>
  <trd lng="id">Rumania</trd>
  <trdgrp lng="pl">
    <trd>Rumunia</trd>
  </trdgrp>
  <trd lng="pt">Rom&ecirc;nia</trd>
</drv>

</art>

