<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ruman.xml,v 1.26 2024/04/14 06:28:51 revo Exp $">
<kap>
  <ofc>8</ofc><rad>ruman</rad>/o
        <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ruman.0o">
  <kap><tld/>o</kap>
  <snc mrk="ruman.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="ruman.R0ujo"><tld lit="R"/>ujo</ref>:
      <ekz>
        akra malla&ubreve;do kontra&ubreve; Poloj, Rusoj, a&ubreve; <tld lit="R"/>oj
        malpla&ccirc;is al li
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj volis, ke la ponto staru tiel, ke trafiko inter
        Grekio kaj okcidenta E&ubreve;ropo pasu tra Rumanio
        <fnt>
       <aut>Evgeni Georgiev/pg</aut>
          <vrk><url ref="&Monato;/2001/007150.html">Nova ponto sur Danubo</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ruman.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_m;&c_y;&c_n;</trd>
  <trd lng="cs">Rumun</trd>
  <trd lng="de">Rum&auml;ne</trd>
  <trd lng="en">Romanian <klr> (subst.)</klr></trd>
  <trdgrp lng="fr">
    <trd>Roumain</trd>
    <trd>Roumaine<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30EB;&#x30FC;&#x30DE;&#x30CB;&#x30A2;&#x4EBA;  <pr>&#x30EB;&#x30FC;&#x30DE;&#x30CB;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Roemeen</trd>
  <trd lng="pt">romeno<klr> (subst.)</klr></trd>
  <trd lng="sk">Rumun</trd>
  <trd lng="tok">jan pi ma Lomani</trd>
  <trd lng="uk">&c_r;&c_u;&#x43C;&c_u;&#x43D;</trd>
  <trd lng="vo">Rum&auml;nan</trd>
  <trdgrp lng="zh">
    <trd>&#x7F57;&#x9A6C;&#x5C3C;&#x4E9A;&#x4EBA; <pr>lu&#xF3;m&#x1CE;n&#xED;y&#xE0;r&#xE9;n</pr></trd>,
    <trd>&#x7F85;&#x99AC;&#x5C3C;&#x4E9E;&#x4EBA; <pr>lu&#xF3;m&#x1CE;n&#xED;y&#xE0;r&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ruman.0a">
  <kap><tld/>a
     <fnt>
      <bib>Ifigenio</bib>
      <lok>pa&gcirc;o 109, Verkaro de Dro L. L. Zamenhof</lok>
    </fnt>
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
         la rumana &scirc;tato
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tata <tld/>a riverkompanio deklaris, ke se la &scirc;ipveturado
        la&ubreve; Danubo longtempe ne funkcios
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006857.php">Danuba &scirc;iptrafiko paralizita</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli ol unu miliono da senlaboruloj, kio egalas al 9,3 centonoj
        de la <tld/>a lo&gcirc;antaro
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
   <trd lng="en">Romanian<klr> (adj.)</klr></trd>
   <trdgrp lng="fr">
    <trd>roumain</trd>
    <trd>roumaine<klr> (adj.)</klr></trd>
   </trdgrp>
   <trd lng="tok">pi ma Lomani</trd>
   <trd lng="vo">Rum&auml;nik</trd>
  </snc>
  <snc mrk="ruman.0a.LING">
    <dif>
      La <tld/>a <ref tip="super" cel="lingv.0o">lingvo</ref>:
      <ekz>
        inter la e&ubreve;ropaj lingvoj, mi trovas pli klare a&ubreve;deblaj kaj
        rekoneblaj la normigitan germanan kaj la <tld/>an
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007159.php">Flugilhavaj vortoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 22</lok>
          </fnt>.
      </ekz>
   </dif>
   <trd lng="en">Romanian<klr> (language)</klr></trd>
   <trd lng="fr">roumain <klr>(langue)</klr></trd>
   <trd lng="tok">toki Lomani</trd>
   <trd lng="vo">Rum&auml;nap&uuml;k</trd>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_m;&c_y;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">rum&auml;nisch</trd>
  <trd lng="ja">&#x30EB;&#x30FC;&#x30DE;&#x30CB;&#x30A2;&#x306E;  <pr>&#x308B;&#x30FC;&#x307E;&#x306B;&#x3042;&#x306E;</pr></trd>
  <trd lng="pt">romeno<klr> (adj.)</klr></trd>
  <trd lng="nl">Roemeens</trd>
  <trd lng="uk">&c_r;&c_u;&#x43C;&c_u;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="ruman.R0ujo">
  <kap>
    <tld lit="R"/>ujo
     <fnt>
      <bib>Ifigenio</bib>
      <lok>pa&gcirc;o 110, La Revuo</lok>
    </fnt>
  </kap>
  <mlg kod="ISO-3166">RO</mlg>
  <snc mrk="ruman.R0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la centra <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>;
      limanta kun
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>,
      <ref tip="vid" cel="moldav.M0ujo.GEOG">Moldavujo</ref>,
      <ref tip="vid" cel="ukrajn.U0ujo">Ukrainujo</ref>
      kaj <ref cel="mar.NigraM0o">Nigra Maro</ref>,
      kaj kun la &ccirc;efurbo Bukare&scirc;to:
      <ekz>
        &ccirc;ar la plimulto jam de longe uzas &leftquot;<tld lit="R"/>ujo&rightquot; kaj
        &leftquot;Bukare&scirc;to&rightquot;, kaj &ccirc;ar ekzistas nenia grava ka&ubreve;zo por tion &scirc;an&gcirc;i,
        tial mi konsilas lasi la formojn &gcirc;isnunajn
        <fnt>
          <bib>LR</bib>
          <lok>2. Transskribo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hunor Kelemen</nom> <klr>[estis]</klr> deputito de RMDSZ en
        parlamento de <tld lit="R"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=249409">Hunor
          Kelemen</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rumani.R0o.GEOG">Rumanio</ref>
  </snc>
  <trd lng="be">&c_R;&c_u;&c_m;&c_y;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_R;&c_u;&c_m;&c_malmol;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">Rum&auml;nien</trd>
  <trd lng="en">Romania</trd>
  <trd lng="fa">&a_r;&a_w;&a_m;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">Roumanie</trd>
  <trd lng="fy">Roemeenje</trd>
  <trd lng="hu">Rom&aacute;nia</trd>
  <trd lng="id">Rumania</trd>
  <trd lng="nl">Roemeni&euml;</trd>
  <trd lng="pl">Rumunia</trd>
  <trd lng="pt">Rom&ecirc;nia</trd>
  <trd lng="tok">ma Lomani</trd>
  <trd lng="vo">Rum&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x7F57;&#x9A6C;&#x5C3C;&#x4E9A; <pr>Lu&#xF3;m&#x1CE;n&#xED;y&#xE0;</pr></trd>,
    <trd>&#x7F85;&#x99AC;&#x5C3C;&#x4E9E; <pr>Lu&#xF3;m&#x1CE;n&#xED;y&#xE0;</pr></trd>
  </trdgrp>
</drv>

</art>
