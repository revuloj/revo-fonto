<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ruman</rad>/o
  <ofc>*</ofc>
  <ofc>8</ofc>
</kap>

<drv mrk="ruman.0ujo">
  <kap>
    <tld lit="R"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="ruman.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Bukare&scirc;to.
      ISO-3166 kodo: RO.
      &Gcirc;i limas kun
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="serb.0ujo.GEOG">Serbujo</ref>,
      <ref tip="vid" cel="moldav.0ujo.GEOG">Moldavujo</ref>,
      <ref tip="vid" cel="ukrain.U0ujo">Ukrainujo</ref>
      kaj <ref cel="mar.NigraM0o">Nigra Maro</ref>.
    </dif>
    <ref tip="sin" cel="rumani.0o.GEOG">Rumanio</ref>
    <trd lng="bg">&c_R;&c_u;&c_m;&c_malmol;&c_n;&c_i;&c_ja;</trd>
    <trd lng="de">Rum&auml;nien</trd>
    <trd lng="en">Romania</trd>
    <trd lng="fa">&a_r;&a_w;&a_m;&a_A;&a_n;&f_y;</trd>
    <trd lng="hu">Rom&aacute;nia</trd>
    <trdgrp lng="pl">
      <trd>Rumunia</trd>
    </trdgrp>
  </snc>
</drv>

</art>

