<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fant.xml,v 1.24 2023/10/14 18:06:40 revo Exp $">
  <kap>
    <rad>fant</rad>/o
  </kap>
<drv mrk="fant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="fant.0o.karta">
    <dif>
      <ref tip="super" cel="kart.lud0o">Ludkarto</ref> malsupera al 
      damo:
      <ekz>
        trefa <tld/>o, karoa damo, kera re&gcirc;o, pika aso, &jcirc;okero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10932">Kartludo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="sin" cel="bub.0o.karta">bubo</ref>
        estas <klr>[&#x2026;]</klr> alia vorto por la ludkarto <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=228078">Bubo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bub.0o.karta">bubo<sncref/></ref>,
      <ref cel="pagxi.0o.karto">pa&gcirc;io<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="as.0o.SPO">aso</ref>,
      <ref cel="regx.0o.karta">re&gcirc;o</ref>,
      <ref cel="regx.0ino.karta">re&gcirc;ino</ref>,
      <ref cel="dam.0o.karta">damo</ref>,
      <ref cel="jxoker.0o.karto">&jcirc;okero</ref>
    </refgrp>
    <trd lng="be">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
    <trd lng="br">lakez<klr tip="ind"> (kartenn c'hoari)</klr></trd>
    <trd lng="ca">jota<klr> (cartes)</klr></trd>
    <trdgrp lng="de">
      <trd>Bube <klr tip="ind">(Karte)</klr> <klr>(francaj bildoj)</klr></trd>,
      <trd>Unter <klr tip="ind">(Karte)</klr> <klr>(germanaj bildoj)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>jack</trd>,
      <trd>knave</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>sota</trd>,
      <trd>joto</trd>,
      <trd>jack</trd>
    </trdgrp>
    <trd lng="fa">&a_s;&a_r;&a_b;&a_A;&a_z;<klr tip="ind"> (&a_w;&a_r;&a_q; &a_b;&a_A;&a_z;&f_y;)</klr></trd>
    <trd lng="fr">valet<klr tip="ind"> (aux cartes)</klr></trd>
    <trdgrp lng="hu">
      <trd>filk&oacute;</trd>,
      <trd>bubi</trd>,
      <trd>als&oacute;<klr tip="ind">
      (k&aacute;rty&aacute;ban)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>fante</trd>,
      <trd>jack</trd>
     </trdgrp>
    <trd lng="nl">boer<klr tip="ind"> (in kaartspel)</klr></trd>
    <trdgrp lng="pl">
      <trd>walet</trd>,
      <trd>ch&lstroke;opak<klr> (pot.)</klr></trd>,
      <trd>ch&lstroke;opek<klr> (pot.)</klr></trd>,
      <trd>dupek<klr> (pot.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>valete <klr tip="amb">(baralho)</klr></trd>,
      <trd>conde <klr tip="amb">(baralho)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>fant</trd>,
    <trd>z&aacute;stava</trd>
  </trdgrp>
  <trd lng="ja">&#x30B8;&#x30E3;&#x30C3;&#x30AF;  <pr>&#x3058;&#x3083;&#x3063;&#x304F;</pr></trd>
  <trdgrp lng="sk">
    <trd>doln&iacute;k <klr>(karta)</klr></trd>,
    <trd>spodok</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x430;&#x43B;&#x435;&#x442; (&#x443; &#x43A;&#x430;&#x440;&#x442;&#x430;&#x445;)</trd>
</drv>

</art>

