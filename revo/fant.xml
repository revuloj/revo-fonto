<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>fant</rad>/o
    </kap>
    <drv mrk="fant.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">SPO</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="kart.lud0o">Ludkarto</ref> malsupera al damo.
        </dif>
      <ref tip="sin" cel="pagxi.0o.karto">pa&gcirc;io</ref>
      </snc>
      <trdgrp lng="de">
       <trd>Bube [francaj bildoj]</trd>,
       <trd>Unter [germanaj bildoj]</trd></trdgrp>
      <trdgrp lng="en">
        <trd>jack</trd>,
        <trd>knave</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>jack</trd>,
        <trd>joto</trd>
      </trdgrp>
      <trd lng="fr">valet<klr tip="ind"> (aux cartes)</klr></trd>
      <trdgrp lng="hu">
        <trd>bubi</trd>,
        <trd>filk&oacute;</trd>,
        <trd>als&oacute;<klr tip="ind"> (k&aacute;rty&aacute;ban)</klr></trd>
      </trdgrp>
      <trd lng="nl">boer<klr tip="ind"> (v.kaartspel)</klr></trd>
      <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
    </drv>
  </art>

