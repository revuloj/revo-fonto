  </trdgrp>
</drv>

<drv mrk="pel.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <snc mrk="pel.dis0igxi.KOMUNE">
    <dif>
      Rapide disiri:
      <ekz>
        dis<tld/>i&gcirc;as fumo
        <fnt><bib>MT</bib>, <lok>&Psa; 68:3</lok></fnt>.
      </ekz>      
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd><ind>dipersar</ind>se</trd>,
    <trd><ind>disipar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>disperser</ind></trd>,
    <trd>se <ind>dissiper</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pel.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pel.el0i.KOMUNE">
    <dif>
      Elirigi <tld/>ante:
      <ekz>
        estis tia vetero, ke oni e&ccirc; hundon ne el<tld/>us
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar ili ne povis pagi, li el<tld/>is ilin per bastonaj batoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rado de la grandega veturilo frakasis la bruston de Marta kaj el<tld/>is el
        &gcirc;i la vivon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mastro diris, ke mi foriru, &ccirc;ar se ne &mdash; li min el<tld/>os per la hundoj
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni el<tld/>is ilin for de Faraono
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 10:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ka&scirc;u la el<tld/>itojn
        <klr>(vd <ref cel="ekzil.0ito">ekzilito</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 16:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malsa&gcirc;eco forte sidas en la koro de knabo,
        sed vergo punanta &gcirc;in el<tld/>as el &gcirc;i
        <fnt><bib>MT</bib>, <lok>&Sen; 22:15</lok></fnt>;
      </ekz>
      <ekz>
        el<tld/>i iun el la mondo de la vivuloj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzil.0i">ekzili</ref>,
      <ref cel="ostrac.0i">ostracismi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vyhnat</trd>,
    <trd>vyhostit</trd>,
    <trd>vyk&aacute;zat</trd>,
    <trd>vypov&ecaron;d&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vertreiben</trd>,
    <trd>verjagen</trd>,
    <trd>hinausjagen</trd>,
    <trd>aussto&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>zarcear</trd>,
    <trd>desalojar</trd>,
    <trd>desanidar</trd>,
    <trd>expeler</trd>,
    <trd>expulsar</trd>,
    <trd>desterrar</trd>
  </trdgrp>
  <trd lng="fr">chasser<klr tip="amb"> (hors d'un endroit)</klr></trd>
  <trdgrp lng="hu">
    <trd>kiterel</trd>,
    <trd>ki&udblac;z</trd>,
    <trd>kihajt</trd>,
    <trd>kikerget</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbannen</trd>,
    <trd>naar buiten <ind>jagen</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyhna&tcaron;</trd>,
    <trd>vyhosti&tcaron;</trd>,
    <trd>vyk&aacute;za&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_g;&c_a;&c_n;&c_ja;&c_t;&c_i;</trd>
</drv>

<drv mrk="pel.el0isto">
  <kap>el<tld/>isto</kap>
  <snc mrk="pel.el0isto.TUR">
    <dif>
      Gardisto de drinkejo a&ubreve; amuzejo, kiu okaze de bezono el<tld/>as
      malbonkondutajn klientojn:
      <ekz>
        <klr>[li]</klr> la karieron komencis kiel el<tld/>isto en
        mondfamaj amuzejoj
        <fnt>
        <aut>V. Rybakov, trad. J. Finkel</aut>,
        <vrk><url ref="http://www.esperanto.mv.ru/Gravitavio/index.html">Gravitavio
        &leftquot;Carido&rightquot;</url></vrk>,
        <lok>1999-2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>portero</trd>,
    <trd>vigilante</trd>
  </trdgrp>
  <trd lng="fr">videur</trd>
  <trdgrp lng="nl">
    <trd>uitsmijter<klr tip="ind"> (buitenwipper)</klr></trd>,
    <trd>buitenwipper</trd>
  </trdgrp>
</drv>

<drv mrk="pel.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pel.for0i.KOMUNE">
    <dif>
      Forigi <tld/>ante; igi malaperi:
      <ekz>
        for<tld/>i la mu&scirc;ojn;
      </ekz>
      <ekz>
        for<tld/>u blasfemulon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la suno for<tld/>as la ombrojn;
      </ekz>
      <ekz>
        prunto amikon for<tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la faktoj for<tld/>is la belan iluzion.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odehnat</trd>,
    <trd>odpudit</trd>,
    <trd>zahnat</trd>,
    <trd>zapudit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vertreiben</trd>,
    <trd>wegjagen</trd>,
    <trd>wegscheuchen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>espantar</trd>,
    <trd>expeler</trd>,
    <trd>repeler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chasser<klr tip="amb"> (au loin)</klr></trd>,
    <trd>effaroucher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&udblac;z</trd>,
    <trd>elhajt</trd>,
    <trd>elkerget</trd>
  </trdgrp>
  <trd lng="nl">wegjagen</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>odduri&tcaron;</trd>,
    <trd>odohna&tcaron;</trd>,
    <trd>zahna&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_g;&c_a;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_n;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pel.ieletra0e">
  <kap>iele-tra<tld/>e</kap>
  <snc mrk="pel.ieletra0e.KOMUNE">
    <dif>
      Malfacile, pene, 
      <ref tip="vid" cel="glat.mal0a">malglate</ref>,
      ne <ref tip="ant" cel="ord.bon0a.KOMUNE">bonorde</ref>:
      <ekz>
        vivi iele-tra<tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;io okazis kaj plue okazadas iele-tra<tld/>e, hazarde kaj 
        senplane, la&ubreve; momentaj emoj kaj kapricoj
        <fnt>
          <aut>G. Neves</aut>,
          <vrk>Paroli kaj paroligi</vrk>,
          <lok><url ref="http://www.helsinki.fi/~jslindst/dn-ideoj.html">
          Ideoj por la gepatroj de denaskuloj</url>, 1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">jak&zcaron; tak&zcaron;</trd>
  <trdgrp lng="es">
    <trd>sin orden ni concierto</trd>,
    <trd>sin cuidado</trd>,
    <trd>a la buena de Dios</trd>,
    <trd>alocadamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; la va-comme-je-te-pousse</trd>,
    <trd>tant bien que mal</trd>
  </trdgrp>
  <trd lng="sk">ako tak</trd>
</drv>

<drv mrk="pel.musx0ilo">
  <kap>mu&scirc;<tld/>ilo</kap>
  <snc mrk="pel.musx0ilo.HEJM">
    <dif>
      Iaspeca vipo a&ubreve; ventumilo, per kiu oni for<tld/>as mu&scirc;ojn:
      <ekz>
        la du aliaj, per bovvostaj mu&scirc;<tld/>iloj,
        &ccirc;asadis la mu&scirc;ojn
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">matamoscas</trd>
  <trd lng="fr">chasse-mouche</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_p;&c_a;&c_h;&c_a;&c_l;&c_o; &c_v;&c_ib;&c_d; &c_m;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_k;&c_a; &c_v;&c_ib;&c_d; &c_m;&c_u;&c_h; (&c_d;&c_l;&c_ja; &c_k;&c_o;&c_n;&c_je;&c_j;)</trd>
  </trdgrp>
</drv>

<drv mrk="pel.re0i">
  <kap>re<tld/>i</kap>
  <snc mrk="pel.re0i.KOMUNE">
    <dif>
      <tld lit="P"/>i malanta&ubreve;en, retroirigi,
      malakcepti:
      <ekz>
        ili estis el <klr>[&#8230;]</klr> nedetruebla plasta materialo kiu 
        re<tld/>as preska&ubreve; &ccirc;iun alian substancon
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">repeler</trd>
  <trd lng="fr">repousser</trd>
</drv>

<drv mrk="pel.sin0i">
  <kap>sin <tld/>i</kap>
  <snc mrk="pel.sin0i.KOMUNE">
    <dif>
      Rapide iri, sub premo, en
      <ref tip="vid" cel="hast.0i">hasto</ref>.
    </dif>
  </snc>
  <trd lng="es">darse prisa</trd>
  <trd lng="fr">se <ind>d&eacute;p&ecirc;cher</ind></trd>
</drv>
