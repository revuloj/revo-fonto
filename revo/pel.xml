    <trd>pron&aacute;sledovat</trd>,
    <trd>pudit</trd>,
    <trd>&scaron;tv&aacute;t</trd>
  </trdgrp>
  <trd lng="da">k&#xF8;retur</trd>
  <trdgrp lng="de">
