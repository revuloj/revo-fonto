<drv mrk="pel.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pel.for0i.KOMUNE">
    <dif>
      Forigi <tld/>ante; igi malaperi:
      <ekz>
        la vulpo <klr>[&#8230;]</klr> for<tld/>is per la vosto la mu&scirc;ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        for<tld/>u blasfemulon, kaj foriros malpaco
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 22:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la matena lumo jam komencis for<tld/>i la ombrojn de la krepusko
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        prunto amikon for<tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la faktoj for<tld/>is la belan iluzion.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odehnat</trd>,
    <trd>odpudit</trd>,
    <trd>zahnat</trd>,
    <trd>zapudit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vertreiben</trd>,
    <trd>wegjagen</trd>,
    <trd>wegscheuchen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>espantar</trd>,
    <trd>expeler</trd>,
    <trd>repeler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chasser<klr tip="amb"> (au loin)</klr></trd>,
    <trd>effaroucher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&udblac;z</trd>,
    <trd>elhajt</trd>,
    <trd>elkerget</trd>
  </trdgrp>
  <trd lng="nl">wegjagen</trd>
  <trdgrp lng="pl">
    <trd>odgania&cacute;</trd>,
    <trd>odp&eogonek;dza&cacute;</trd>,
    <trd>wygania&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>alunga</trd>,
    <trd>izgoni</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>odduri&tcaron;</trd>,
    <trd>odohna&tcaron;</trd>,
    <trd>zahna&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_g;&c_a;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_n;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pel.ieletra0e">
  <kap>iele-tra<tld/>e</kap>
  <snc mrk="pel.ieletra0e.KOMUNE">
    <dif>
      Malfacile, pene, 
      <ref tip="vid" cel="glat.mal0a">malglate</ref>,
      ne <ref tip="ant" cel="ord.bon0a.io">bonorde</ref>:
      <ekz>
        vivi iele-tra<tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;io okazis kaj plue okazadas iele-tra<tld/>e, hazarde kaj 
        senplane, la&ubreve; momentaj emoj kaj kapricoj
        <fnt>
          <aut>G. Neves</aut>,
          <vrk>Paroli kaj paroligi</vrk>,
          <lok><url ref="http://www.helsinki.fi/~jslindst/dn-ideoj.html">
          Ideoj por la gepatroj de denaskuloj</url>, 1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">jak&zcaron; tak&zcaron;</trd>
  <trdgrp lng="de">
    <trd>von der <ind>Hand</ind> in den Mund</trd>,
    <trd>sich <ind>durchwursteln</ind>d</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sin orden ni concierto</trd>,
    <trd>sin cuidado</trd>,
    <trd>a la buena de Dios</trd>,
    <trd>alocadamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; la va-comme-je-te-pousse</trd>,
    <trd>tant bien que mal</trd>
  </trdgrp>
  <trd lng="pl">na &lstroke;ut szcz&eogonek;&sacute;cia</trd>
  <trd lng="ro">pentru o lovitur&abreve; de noroc</trd>
  <trd lng="sk">ako tak</trd>
</drv>

<drv mrk="pel.musx0ilo">
  <kap>mu&scirc;<tld/>ilo</kap>
  <snc mrk="pel.musx0ilo.HEJM">
    <dif>
      Iaspeca vipo a&ubreve; ventumilo, per kiu oni for<tld/>as mu&scirc;ojn:
      <ekz>
        la du aliaj, per bovvostaj mu&scirc;<tld/>iloj,
        &ccirc;asadis la mu&scirc;ojn
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Fliegenwedel</trd>
  <trd lng="es">matamoscas</trd>
  <trd lng="fr">chasse-mouche</trd>
  <trd lng="pl">odganiacz much</trd>
  <trd lng="ro">deflector de mu&#537;te</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_p;&c_a;&c_h;&c_a;&c_l;&c_o; &c_v;&c_ib;&c_d; &c_m;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_k;&c_a; &c_v;&c_ib;&c_d; &c_m;&c_u;&c_h; (&c_d;&c_l;&c_ja; &c_k;&c_o;&c_n;&c_je;&c_j;)</trd>
  </trdgrp>
</drv>

<drv mrk="pel.re0i">
  <kap>re<tld/>i</kap>
  <snc mrk="pel.re0i.KOMUNE">
    <dif>
      <tld lit="P"/>i malanta&ubreve;en, retroirigi,
      malakcepti:
      <ekz>
        ili komencis forkuri, sed la <klr>[arbitraciuloj]</klr> re<tld/>is ilin en la
        batalon per siaj plumbofinitaj vipoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devis foriri <klr>[&#8230;]</klr> al la arbaro, sed min <klr>[&#8230;]</klr> re<tld/>is
        de tie la pluvo
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estis el <klr>[&#8230;]</klr> nedetruebla plasta materialo kiu 
        re<tld/>as preska&ubreve; &ccirc;iun alian substancon
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zur&uuml;ck <ind>treiben</ind></trd>
  <trd lng="es">repeler</trd>
  <trd lng="fr">repousser</trd>
  <trdgrp lng="pl">
    <trd>odgania&cacute;</trd>,
    <trd>odgoni&cacute; od siebie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nt&icirc;rzia</trd>,
    <trd>alunga&#539;i unul de cel&abreve;lalt</trd>
  </trdgrp>
</drv>

<drv mrk="pel.sin0i">
  <kap>sin <tld/>i</kap>
  <snc mrk="pel.sin0i.KOMUNE">
    <dif>
      Rapide iri, sub premo, en
      <ref tip="vid" cel="hast.0i">hasto</ref>:
      <ekz>
        &scirc;i kuris preter la mastrino kaj <tld/>is sin en la &scirc;tuparon
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 1 (51)</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu familio <klr>[&#8230;]</klr> kuris anhelante, Dio scias kial, transpa&scirc;is la
        relojn, <tld/>is sin al la <klr>[&#8230;]</klr> trajno
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 6 (104)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>anspornen</ind></trd>,
    <trd>sich <ind>hetzen</ind></trd>
  </trdgrp>
  <trd lng="es">darse prisa</trd>
  <trd lng="fr">se <ind>d&eacute;p&ecirc;cher</ind></trd>
  <trd lng="pl">ugania&cacute; si&eogonek;</trd>
  <trdgrp lng="ro">
    <trd>s&abreve; urmezi</trd>,
    <trd>se &icirc;mpinge</trd>
  </trdgrp>
</drv>
