    <trd>pousser<klr tip="amb"> (faire aller)</klr></trd>
  </trdgrp>
  <trd lng="fy">ride</trd>
  <trd lng="gl">unidade</trd>
  <trd lng="gu">&#xAA1;&#xACD;&#xAB0;&#xABE;&#xA87;&#xAB5;</trd>
  <trd lng="ha">tu&#x199;a</trd>
