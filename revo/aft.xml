<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aft</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aft.0o">
  <kap><tld/>o</kap>
  <snc mrk="aft.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sendan&gcirc;era ulcereto en la bu&scirc;o.
    </dif>
    <trd lng="ru">&c_a;&c_f;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="aft.0o.epizootia">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Febrodona &scirc;vela&jcirc;o &ccirc;e la brusto, mamo, hufoj de brutaro:
      <ekz>
        <tld/>a epidemio <fnt>PIV</fnt>, <tld/>a malsano.
      </ekz>
    </dif>
    <ref tip="vid" cel="aftoz.0o">aftozo</ref>
    <trd lng="ru">&c_ja;&c_shch;&c_u;&c_r;</trd>
  </snc>
  <trdgrp lng="en">
    <trd>aphtha</trd>,
    <trd>aphthous ulcer</trd>
  </trdgrp>
  <trd lng="de">Aphte</trd>
  <trd lng="es">afta</trd>
  <trd lng="fr">aphte</trd>
  <trdgrp lng="nl">
    <trd>tongblaar</trd>,
    <trd>koortsblaasje</trd>,
    <trd>aft</trd>
  </trdgrp>
  <trd lng="pl">pryszcz</trd>
  <trd lng="pt">afta</trd>
</drv>
</art>
