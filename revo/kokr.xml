<art mrk="$Id: kokr.xml,v 1.21 2023/09/01 15:46:40 revo Exp $">
<kap>
  <rad>kokr</rad>/i <fnt>Y</fnt>
</kap>
<drv mrk="kokr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti <ref cel="fidel.mal0a">malfidela</ref> al 
      sia <ref cel="kun.viv0ulo">vivkunulo</ref> a&ubreve; vivkunulino:
      <ekz>
        &ccirc;e la ceteraj tri paroj preska&ubreve; certe almena&ubreve; unu el la geedzoj
        <tld/>is, se ne amba&ubreve;
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010215.php">Ami, estimi kaj
          kokri</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Meropso <klr>[&#x2026;]</klr> fami&gcirc;is nur per tio, ke lia edzino
        <tld/>is
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=567542">Meropso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dum &scirc;i vivis kun beletra eldonisto <nom>Jean-Paul
        Enthoven</nom>, &scirc;i <tld/>is lin kun lia filo <nom>Rapha&euml;l
        Enthoven</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=225533">Carla
          Bruni</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kok.0o.AGR">koko</ref>,
      <ref cel="adult.0i">adulti</ref>,
      <ref cel="tromp.0i">trompi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_d;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>&c_m;&c_u;&c_zh;&c_u;</klr></trd>
  <trdgrp lng="ca">
    <trd>&eacute;sser infidel</trd>,
    <trd>posar banyes a</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t nev&ecaron;rn&yacute;</trd>,
    <trd>podv&aacute;d&ecaron;t <klr>(v man&zcaron;elstv&iacute;)</klr></trd>
  </trdgrp>
  <trd lng="de">den Ehepartner <ind>betr&uuml;gen</ind></trd>
  <trd lng="en">cuckold <klr>(uzata nur p.p. viroj)</klr></trd>
  <trdgrp lng="es">
    <trd>ser infiel</trd>, 
    <trd>poner los cuernos<klr> (coloquial)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cocufier</trd>,
    <trd>faire porter les <ind>corne</ind>s (&agrave;)</trd>,
    <trd>tromper<klr tip="ind"> (son conjoint)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BC6;&#x901A;&#x3059;&#x308B;  [<ind>&#x307F;&#x3063;&#x3064;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5BDD;&#x53D6;&#x308B;  [<ind>&#x306D;&#x3068;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">bedriegen<klr tip="ind"> (partner)</klr></trd>
  <trd lng="pl">zdradza&cacute; <klr>(w ma&lstroke;&zdot;e&nacute;stwie)</klr></trd>
  <trdgrp lng="pt">
    <trd>cornear<klr> (trair)</klr></trd>
    <trd>chifrar<klr> (trair)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_r;&c_o;&c_g;&c_a; <klr>(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
  <trd lng="sk">by&tcaron; nevern&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x440;&#x430;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438; (&#x443; &#x43F;&#x43E;&#x434;&#x440;&#x443;&#x436;&#x43D;&#x44C;&#x43E;&#x43C;&#x443; &#x436;&#x438;&#x442;&#x442;&#x456;)</trd>,
    <trd>&#x43F;&#x43E;&#x440;&#x443;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43F;&#x43E;&#x434;&#x440;&#x443;&#x436;&#x43D;&#x44E; &#x432;&#x456;&#x440;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43D;&#x430;&#x441;&#x442;&#x430;&#x432;&#x43B;&#x44F;&#x442;&#x438; &#x440;&#x43E;&#x433;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="kokr.0ito">
  <kap><tld/>ito</kap>
  <snc>
    <dif>
      <tld lit="K"/>ita edzo a&ubreve; edzino:
      <ekz>
        <nom>Gengoux</nom> grava burgonja landsinjoro estis <tld/>ito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=360353">Saint-Gengoux-le-National</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">banyut</trd>
  <trdgrp lng="cs">
    <trd>paroh&aacute;&ccaron;</trd>,
    <trd>podv&aacute;d&ecaron;n&yacute; man&zcaron;el</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hahnrei</trd>,
    <trd>Geh&ouml;rnter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cuckold</trd>,
    <trd>deceived husband</trd>
  </trdgrp>
  <trd lng="es">cornudo<klr> (objeto de infidelidad)</klr></trd>
  <trdgrp lng="fr">
    <trd>cocu</trd>,
    <trd>mari tromp&eacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x5BDD;&#x53D6;&#x3089;&#x308C;&#x7537;  [<ind>&#x306D;&#x3068;&#x3089;&#x308C;&#x304A;&#x3068;&#x3053;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>hoorndrager</trd>,
    <trd>bedrogen <ind>echtgenoot</ind></trd>
  </trdgrp>
  <trd lng="pl">rogacz <klr> (zdradzany m&aogonek;&zdot;)</klr></trd>
  <trd lng="pt">corno<klr> (pop.)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_o;&c_n;&c_o;&c_s;&c_je;&c_c;
  <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;. &c_i;&c_r;&c_o;&c_n;.)</klr></trd>
  <trd lng="sk">paroh&aacute;&ccaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x433;&#x43E;&#x43D;&#x43E;&#x441;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x437;&#x440;&#x430;&#x434;&#x436;&#x435;&#x43D;&#x438;&#x439; &#x447;&#x43E;&#x43B;&#x43E;&#x432;&#x456;&#x43A;</trd>
  </trdgrp>
</drv>
