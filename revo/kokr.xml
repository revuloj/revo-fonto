<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kokr</rad>/i <fnt>Y</fnt>
</kap>
<drv mrk="kokr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc> 
    <dif>
      Sekse trompi sian edzon.
    </dif>
    <refgrp tip="sin">
      <ref cel="kok.0o">koko</ref>,
      <ref cel="adult">adulto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">den Ehemann <ind>betr&uuml;gen</ind></trd>
  <trd lng="en">cuckold <klr>(uzata nur p.p. viroj)</klr></trd>
  <trd lng="fr">cocufier</trd>
  <trd lng="nl">bedriegen<klr tip="ind"> (partner)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
&c_r;&c_o;&c_g;&c_a; <klr>(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
</drv>
<drv mrk="kokr.0ito">
  <kap><tld/>ito</kap>
  <snc>
    <dif>
      Sekse trompita edzo.
    </dif>
  </snc>
  <trd lng="de">Hahnrei</trd>
  <trdgrp lng="en">
     <trd>cuckold</trd>,
     <trd>deceived husband</trd>
  </trdgrp>
  <trd lng="fr">cocu</trd>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_o;&c_n;&c_o;&c_s;&c_je;&c_c;
<klr>(&c_p;&c_je;&c_r;&c_je;&c_n;. &c_i;&c_r;&c_o;&c_n;.)</klr></trd>
</drv>
</art>
