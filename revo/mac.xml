<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mac</rad>/o
</kap>
<drv mrk="mac.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Plata kuko el nefermentinta pasto, kiun la Hebreoj man&gcirc;as dum
paska
      tempo.
    </dif>
    <trd lng="be">&c_m;&c_a;&c_c;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Matze</trd>,
      <trd>Matzen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>matzo</trd>,
      <trd>matzoh</trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>macesz</trd>,
      <trd>p&aacute;szka</trd>
    </trdgrp>
    <trd lng="nl">matze</trd>
    <trd lng="ru">&c_m;&c_a;&c_c;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Nefermentinta pasto
    </dif>
    <refgrp tip="sin">
      <ref cel="oblat.0o">oblato</ref>.
    </refgrp>
    <trd lng="hu">kov&aacute;sztalan <ind>keny&eacute;r</ind></trd>
    <trd lng="nl">ongezuurd <ind>deeg</ind></trd>
    <trd lng="ru">&c_o;&c_p;&c_r;&c_je;&c_s;&c_n;&c_o;&c_k;&c_i;</trd>
  </snc>
</drv>
</art>
