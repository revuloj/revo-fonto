<art mrk="$Id: hxolesterin.xml,v 1.5 2025/11/06 22:32:52 revo Exp $">
<kap>
  <rad>&hcirc;olesterin</rad>/o
</kap>
<drv mrk="hxolesterin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="hxolesterin.0o.KEM">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="hxolesterol.0o">&Hcirc;olesterolo</ref>:
      <ekz>
        facile digestebla graso de la ovoflavo: <tld/>o
        <fnt>
          <aut>A. Wander</aut>,
          <vrk><url ref="http://arkivo.esperanto-france.org/divers/ovomalt/ovo-p4.htm">
          La Plej Esenca pri Ovomaltino</url></vrk>,
          <lok>1910</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_l;&c_je;&c_s;&c_t;&c_e;&c_r;&c_y;&c_n;</trd>
  <trd lng="fr">cholest&eacute;rine</trd>
  <trdgrp lng="pl">
    <trd>cholesterol</trd>,
    <trd>cholesteryna</trd>
  </trdgrp>
  <trd lng="ro">colesterol</trd>
  <trd lng="zh">&#x80C6;&#x56FA;&#37255; <pr>d&#x1CE;ng&#xF9;ch&#xFA;n</pr></trd>
</drv>
