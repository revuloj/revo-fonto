<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: oriel.xml,v 1.9 2024/01/30 16:09:26 revo Exp $">
    <kap>
      <rad>oriel</rad>/o <fnt><bib>PIV1</bib></fnt>
   </kap>
    <drv mrk="oriel.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ARKI</uzo>
      <snc mrk="oriel.0o.unuopa">
        <dif>
          Iom lar&gcirc;a parto de &ccirc;ambro, salono a&ubreve; alia ejo de domo,
          elstaranta eksteren el la fasado, provizita je fenestroj,
          apogita sur <ref cel="korbel.0o">korbelo</ref> (penda,
          korbela <tld/>o) a&ubreve; sur la grundo (surtera <tld/>o):
          <ekz>
            &scirc;i veki&gcirc;is preska&ubreve; je la tagmezo en sia dormo&ccirc;ambro
            havanta <tld/>on en la turo de la palaceto 
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 29a</lok></fnt>.
          </ekz>
       </dif>
     </snc>
      <snc mrk="oriel.0o.turo">
        <dif>
          En plureta&gcirc;a domo, prisma a&ubreve; cilindrosegmenta elstara&jcirc;o sur
          la fasado, konsistanta el
          <tld/>oj<sncref ref="oriel.0o.unuopa"/>:
          <ekz>
            la angula dueta&gcirc;a <tld/>o provizita per reliefa ornama&jcirc;o
            en amba&ubreve; eta&gcirc;oj
            <fnt>
              <vrk><url ref="http://olomouc-moravio.wz.cz/historie.html">Historio de
              Olomouc</url></vrk>
            </fnt>.
          </ekz>
       </dif>
      </snc>
      <trd lng="be">&c_e;&c_r;&c_k;&c_je;&c_r;</trd>
  <trd lng="cs">ark&yacute;&rcaron;</trd>
  <trd lng="de">Erker</trd>
  <trdgrp lng="en">
        <trd>oriel</trd>,
        <trd>bay window</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>oriel</trd>,
        <trd>fen&ecirc;tre arqu&eacute;e</trd>
      </trdgrp>
  <trd lng="nl">erker</trd>
  <trd lng="no">karnapp</trd>
  <trd lng="pl">wykusz</trd>
  <trd lng="ro">bovindou</trd>
  <trdgrp lng="ru">
        <trd>&c_e;&c_r;&c_k;&c_je;&c_r;</trd>,
        <trd>&c_f;&c_o;&c_n;&c_a;&c_r;&c_mol;<klr> (&c_a;&c_r;&c_h;&c_i;&c_t;.)</klr></trd>
      </trdgrp>
  <trd lng="sk">arkier</trd>
  <trd lng="uk">&c_je;&c_r;&#x43A;&c_je;&c_r;</trd>
  <trd lng="zh">&#x51F8;&#x7A97; <pr>t&#x16B;chu&#x101;ng</pr></trd>
</drv>
   </art>
   
