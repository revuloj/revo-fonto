<vortaro>
<art mrk="$Id: mate1.xml,v 1.22 2024/01/25 20:10:35 revo Exp $">
<kap>
  <rad>Mate</rad>/o
  <fnt>
    <bib>PIV1</bib>
  </fnt>
</kap>

<drv mrk="mate1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        kaj Jesuo <klr>[&#x2026;]</klr> vidis viron nomatan <tld/>o, sidantan
        &ccirc;e la impostejo
        <fnt>
          <bib>NT</bib>,
          <lok>&Mat; 9:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        maldungite, <tld/>o suferis pro nerva stre&ccirc;iteco
        <fnt>
          <bib>Monato</bib>
          <aut>Russ Williams</aut>
          <vrk><url ref="&Monato;/2010/010030.php">&Ccirc;i tiun lingvon vi ne uzu!</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc> 
  <rim>
    <ref tip="hom" cel="mate.0o">Mateo</ref> estas anka&ubreve; specio de ilekso
     kaj la trinka&jcirc;o preperita el &gcirc;iaj folioj.
  </rim>                   
  <trd lng="be">&c_M;&c_a;&c_c;&c_je;&c_j;</trd>
  <trd lng="bg">&c_M;&c_a;&c_t;&c_je;&c_j;</trd>
  <trd lng="br">Mazhev</trd>
  <trdgrp lng="ca">
    <trd>Mateu</trd>,
    <trd>Maties</trd>
  </trdgrp>
  <trd lng="de">Matth&auml;us</trd>
  <trd lng="en">Matthew</trd>
  <trd lng="fr">Matthieu</trd>
  <trd lng="hu">M&aacute;t&eacute;</trd>
  <trdgrp lng="it">
    <trd>Matteo</trd>,
    <trd>Mattia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30C6;&#x30AA;</trd>,
    <trd>&#x30DE;&#x30BF;&#x30A4;</trd>,
    <trd>&#x30DE;&#x30B7;&#x30E5;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">Mathias</trd>
  <trd lng="pl">Mateusz</trd>
  <trd lng="pt">Mateus</trd>
  <trdgrp lng="ru">
    <trd>&c_M;&c_a;&c_t;&c_v;&c_je;&c_j;</trd>,
    <trd>&c_M;&c_a;&c_t;&c_f;&c_je;&c_j;<klr>
    (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x41C;&#x430;&#x442;&#x432;&#x456;&#x439;</trd>,
    <trd>&#x41C;&#x430;&#x442;&#x435;&#x439;</trd>,
    <trd>&#x41C;&#x430;&#x442;&#x444;&#x435;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x739B;&#x7AA6; <pr>M&#x1CE;d&#xF2;u</pr></trd>,
    <trd>&#x746A;&#x7AC7; <pr>M&#x1CE;d&#xF2;u</pr></trd>,
    <trd>&#x9A6C;&#x4FEE; <pr>m&#x1CE;xi&#x16B;</pr></trd>,
    <trd>&#x99AC;&#x592A; <pr>M&#x1CE;t&#xE0;i</pr></trd>,
    <trd>&#x9A6C;&#x592A; <pr>M&#x1CE;t&#xE0;i</pr></trd>,
    <trd>&#x99AC;&#x592A;&#x798F;&#x97F3; <pr>m&#x1CE;t&#xE0;if&#xFA;y&#x12B;n</pr></trd>,
    <trd>&#x9A6C;&#x592A;&#x798F;&#x97F3; <pr>m&#x1CE;t&#xE0;if&#xFA;y&#x12B;n</pr></trd>
  </trdgrp>
</drv>
