<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stenog1.xml,v 1.27 2025/05/22 10:00:21 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>stenografi</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="stenog1.0o">
  <kap><tld/>o</kap>
  <snc mrk="stenog1.0o.TIP_ago">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenog.0ado.TIP">Stenografado</ref>:
      <ekz>
        lia idealo &scirc;ajnas iri pli malproksimen, ol nia nuna <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        germanaj tradukoj en <tld/>a skribo
        <fnt>
          <bib>Monato</bib>
          <aut>Bernhard Pabst</aut>
          <vrk><url ref="&Monato;/2008/009376.php">Inter libroj posttagmeze</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        lunde estos ekzameno pri <tld/>o
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>stenografado</ke>, <ke>stenografsignaro</ke>,
    <ke>stenografarto</ke>.
  </rim>

  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>taquigrafia</trd>,
    <trd>estenografia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>stenografie</trd>,
    <trd>t&ecaron;snopis</trd>
  </trdgrp>
  <trd lng="de">Stenografie</trd>
  <trdgrp lng="en">
    <trd>stenography</trd>,
    <trd>shorthand</trd>
  </trdgrp>
  <trd lng="es">taquigraf&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>st&eacute;nographie</trd>,
    <trd>tachygraphie</trd>
  </trdgrp>
  <trd lng="hu">gyors&iacute;r&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#36895;&#35352;&#34899;  <pr>&#12381;&#12387;&#12365;&#12376;&#12421;&#12388;</pr></trd>,
    <trd>&#36895;&#35352;&#27861;  <pr>&#12381;&#12387;&#12365;&#12411;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">stenografie</trd>
  <trdgrp lng="pl">
    <trd>stenograifa</trd>,
    <trd>stenografowanie</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">r&yacute;chlopis</trd>
  <trd lng="sv">stenografi</trd>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#36895;&#35352; <pr>s&ugrave;j&igrave;</pr></trd>,
    <trd>&#36895;&#35760; <pr>s&ugrave;j&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stenog1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="stenog1.0isto.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenog.0o.TIP">Stenografo</ref>:
      <ekz>
        neniu vidis ian kontra&ubreve;fundamentecon en la apartaj
        skriboj de <tld/>isto a&ubreve; de blindulo
        <fnt>
          <aut>G. Mattos</aut>,
          <vrk><url ref="&AdE;/oficialaj_informoj/oficialaj_informoj_1_2006.html">
          &Gcirc;enerala enkonduko</url></vrk>,
          <lok>Akademio de Esperanto, Oficialaj Informoj (1),
          2006-09-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="ca">
    <trd>taqu&iacute;graf</trd>,
    <trd>esten&ograve;graf</trd>
  </trdgrp>
  <trd lng="de">Stenograf</trd>
  <trd lng="en">stenographer</trd>
  <trd lng="es">taqu&iacute;grafo</trd>
  <trd lng="fr">st&eacute;nographe</trd>
  <trd lng="hu">gyors&iacute;r&oacute;</trd>  
  <trd lng="ja">&#36895;&#35352;&#32773;  <pr>&#12381;&#12387;&#12365;&#12375;&#12419;</pr></trd>
  <trd lng="nl">stenograaf</trd>
  <trd lng="pl">stenograf</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="zh">
    <trd>&#36895;&#24405;&#24072; <pr>s&ugrave;l&ugrave;sh&#299;</pr></trd>,
    <trd>&#36895;&#37636;&#24107; <pr>s&ugrave;l&ugrave;sh&#299;</pr></trd>,
    <trd>&#36895;&#35352;&#21729; <pr>s&ugrave;j&igrave;yu&aacute;n</pr></trd>,
    <trd>&#36895;&#35760;&#21592; <pr>s&ugrave;j&igrave;yu&aacute;n</pr></trd>
  </trdgrp>
</drv>
</art>
