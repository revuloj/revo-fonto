<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stenog1.xml,v 1.19 2022/05/05 14:43:48 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>stenografi</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="stenog1.0o">
  <kap><tld/>o</kap>
  <snc mrk="stenog1.0o.TIP_ago">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenog.0ado.TIP">Stenografado</ref>:
      <ekz>
        lia idealo &scirc;ajnas iri pli malproksimen, ol nia nuna <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        germanaj tradukoj en <tld/>a skribo
        <fnt>
          <bib>Monato</bib>
          <aut>Bernhard Pabst</aut>
          <vrk><url ref="&Monato;/2008/009376.php">Inter libroj posttagmeze</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        lunde estos ekzameno pri <tld/>o
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>stenografie</trd>,
    <trd>t&ecaron;snopis</trd>
  </trdgrp>
  <trd lng="de">Stenografie</trd>
  <trdgrp lng="en">
    <trd>stenography</trd>,
    <trd>shorthand</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>st&eacute;nographie</trd>,
    <trd>tachygraphie</trd>
  </trdgrp>
  <trd lng="hu">gyors&iacute;r&aacute;s</trd>
  <trd lng="nl">stenografie</trd>
  <trdgrp lng="pl">
    <trd>stenograifa</trd>,
    <trd>stenografowanie</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">r&yacute;chlopis</trd>
  <trd lng="sv">stenografi</trd>
  <trd lng="uk">&#x441;&#x442;&#x435;&#x43D;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F;</trd>
</drv>

<drv mrk="stenog1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="stenog1.0isto.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenog.0o.TIP">Stenografo</ref>:
      <ekz>
        neniu vidis ian kontra&ubreve;fundamentecon en la apartaj
        skriboj de <tld/>isto a&ubreve; de blindulo
        <fnt>
          <aut>G. Mattos</aut>,
          <vrk><url ref="http://akademio-de-esperanto.org/oficialaj_informoj/oficialaj_informoj_1_2006.html">
          &Gcirc;enerala enkonduko</url></vrk>,
          <lok>Akademio de Esperanto, Oficialaj Informoj (1),
          2006-09-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="de">Stenograf</trd>
  <trd lng="en">stenographer</trd>
  <trd lng="fr">st&eacute;nographe</trd>
  <trd lng="hu">gyors&iacute;r&oacute;</trd>  
  <trd lng="nl">stenograaf</trd>
  <trd lng="pl">stenograf</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="fr">st&eacute;nographe</trd>
</drv>
</art>
