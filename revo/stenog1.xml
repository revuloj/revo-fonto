<drv mrk="stenog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stenografado.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="de">Stenografie</trd>
