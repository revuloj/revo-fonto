<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: panari.xml,v 1.19 2024/03/23 08:23:42 revo Exp $">
<kap><rad>panarici</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="s">panaris</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="panari.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <snc mrk="panari.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" lst="voko:malsanoj" cel="inflam.0o">Inflamo</ref>
      de la &ccirc;ela histo de fingro:
      <ekz>
        mi havas <tld var="s"/>on, kaj la kuracisto preskribis al mi varmon
        <fnt>
          <bib>KrB</bib>
          <lok>Konsiloj pri higieno</lok>
        </fnt>;
      </ekz>
      <ekz>
        kandozo <klr>[&#8230;]</klr> plimulti&gcirc;as per sporoj, &ccirc;e la ungoj
        la koloro estas verda, ofte kombinita kun <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=251601">Mikozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_r;&c_y;&c_c;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_s;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_c;&c_ja;&c_je;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">panaritium</trd>
  <trdgrp lng="de">
    <trd>Panaritium</trd>,
    <trd>Nagelgeschw&uuml;r</trd>
  </trdgrp>
  <trd lng="fr">panaris</trd>
  <trdgrp lng="hu">
    <trd>panaritium</trd>,
    <trd>ujjphlegmone</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>cantengan</trd>,
    <trd>paronikia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fijt</trd>,
    <trd>vingerontsteking</trd>,
    <trd>omloop</trd>
  </trdgrp>
  <trd lng="pl">zastrza&lstroke;  <klr>(medycyna)</klr></trd>
  <trdgrp lng="pt">
    <trd>paron&iacute;quia</trd>,
    <trd>panar&iacute;cio</trd>
  </trdgrp>
  <trd lng="ro">panari&tcomma;iu</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_r;&c_i;&c_c;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_o;&c_g;&c_t;&c_o;&c_je;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_t;&c_i;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">hnisav&yacute; z&aacute;pal prstov</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_r;&c_i;&c_c;&c_ib;&c_j;</trd>,
    <trd>&c_n;&c_ib;&c_g;&c_t;&c_mol;&c_o;&c_ji;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#30002;&#24202;&#28814; <pr>ji&#462;chu&aacute;ngy&aacute;n</pr></trd>
</drv>
</art>
