<art mrk="$Id: panari.xml,v 1.16 2022/03/25 10:18:30 revo Exp $">
<kap><rad>panarici</rad>/o</kap>
<drv mrk="panari.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" lst="voko:malsanoj" cel="inflam.0o">Inflamo</ref>
      de la &ccirc;ela histo de fingro:
      <ekz>
        kandozo <klr>[&#x2026;]</klr> plimulti&gcirc;as per sporoj, &ccirc;e la ungoj 
        la koloro estas verda, ofte kombinita kun <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=251601">Mikozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">panaritium</trd>
  <trdgrp lng="de">
    <trd>Panaritium</trd>,
    <trd>Nagelgeschw&uuml;r</trd>
  </trdgrp>
  <trd lng="fr">panaris</trd>
  <trdgrp lng="hu">
    <trd>panaritium</trd>,
    <trd>ujjphlegmone</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>cantengan</trd>,
    <trd>paronikia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fijt</trd>,
    <trd>vingerontsteking</trd>,
    <trd>omloop</trd>
  </trdgrp>
  <trd lng="pl">zastrza&lstroke;  <klr>(medycyna)</klr></trd>
  <trdgrp lng="pt">
    <trd>paron&iacute;quia</trd>,
    <trd>panar&iacute;cio</trd>
  </trdgrp>
  <trd lng="ro">panari&#x21B;iu</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_r;&c_i;&c_c;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_o;&c_g;&c_t;&c_o;&c_je;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_t;&c_i;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">hnisav&yacute; z&aacute;pal prstov</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x43D;&#x430;&#x440;&#x438;&#x446;&#x456;&#x439;</trd>,
    <trd>&#x43D;&#x456;&#x433;&#x442;&#x44C;&#x43E;&#x457;&#x434;&#x430;</trd>
  </trdgrp>
</drv>
</art>
