<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>lat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longa, mallar&gcirc;a kaj plate maldika peco da ligno, uzata en konstruo:
      <ekz>
        kofroj faritaj el palmaj fibroj kaj &scirc;irmitaj per <tld/>oj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 67a, p. 430a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lamen.0o">lameno</ref>,
      <ref cel="tabul.0o">tabulo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_e;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Latte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lath</trd>,
    <trd>batten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lame<klr> (de bois)</klr></trd>
    <trd>latte</trd>
  </trdgrp>
  <trd lng="hu">l&eacute;c</trd>  
  <trd lng="nl">lat</trd>
  <trd lng="pt">ripa</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="lat.0kurteno">
  <kap><tld/>kurteno</kap>
  <snc>
    <dif>
      Fenestra kovrilo farita el klineblaj <tld/>oj por regi lum-enlason.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_ib;</trd>
  <trd lng="en">Venetian <ind>blind</ind></trd>
  <trdgrp lng="de">
    <trd>Rollo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>store v&eacute;nitien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>red&odblac;ny</trd>,
    <trd>rol&oacute;</trd>,
    <trd>reluxa</trd>
  </trdgrp>  
  <trd lng="nl">zonneblind</trd>
  <trd lng="pt">veneziana</trd>
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_i;</trd>
</drv>

<drv mrk="lat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>latter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&eacute;cez</trd>,
    <trd>bel&eacute;cez</trd>
  </trdgrp>  
  <trd lng="ru">&c_o;&c_b;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol; <ind>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;&c_m;&c_i;</ind></trd>
</drv>
</art>
