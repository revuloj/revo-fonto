<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>lat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lat.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Longa, mallar&gcirc;a kaj plate maldika peco da ligno, uzata en konstruo.
    </dif>
    <refgrp tip="sin">
      <ref cel="tabul.0o">tabulo</ref>,
      <ref cel="lamen.0o">lameno</ref>,
      <ref cel="lad.0o">lado</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>lath</trd>,
    <trd>batten</trd>
  </trdgrp>
  <trd lng="nl">lat</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="lat.0kurteno">
  <kap>
    <tld/>kurteno
  </kap>
  <snc>
    <dif>
      Fenestra kovrilo farita el moveblaj <tld/>oj.
    </dif>
  </snc>
  <trd lng="en">Venetian <ind>blind</ind></trd>
  <trd lng="nl">zonneblind</trd>
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_i;</trd>
</drv>

<drv mrk="lat.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj.
    </dif>
  </snc>
</drv>
</art>
