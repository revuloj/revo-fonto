<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ehxidn.xml,v 1.8 2023/11/07 19:22:22 revo Exp $">
<kap>
  <rad>e&hcirc;idn</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="k">ekidn</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="ehxidn.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="ehxidn.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
       <ref tip="dif" cel="erinac.posx0o.ZOO">Po&scirc;erinaco</ref>:
      <ekz>
        interesaj bestoj estas la monotremoj, kiel la ornitorinko
        kaj la <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/A%C5%ADstralio">
          A&ubreve;stralio</url>, 2012-12-12</lok>
        </fnt>.;
      </ekz>
      <ekz>
        la formikoman&gcirc;a <tld var="k"/>o <klr>[&#x2026;]</klr> estas a&ubreve;stralia
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Portalo:A%C5%ADstralio">
          Portalo:A&ubreve;stralio</url>, 2012-10-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld var="k"/>o ne havas dentojn, kiel &ccirc;iuj monotremoj, kaj man&gcirc;as same kiel
        formikursoj kaj terporkoj
        <fnt>
          <vrk><url ref="https://scivolemo.wordpress.com/2018/05/02/mamuloj-kiuj-venas-el-ovoj-la-monotremoj/">Mamuloj
          kiuj venas el ovoj: la monotremoj</url></vrk>
          <lok>Scivolemo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_h;&c_ib;&c_d;&c_n;&c_a;</trd>,
    <trd>&c_ja;&c_h;&c_ib;&c_d;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">je&zcaron;ura</trd>
  <trdgrp lng="de">
    <trd>Ameisenigel</trd>,
    <trd>Schnabeligel</trd>
  </trdgrp>
  <trd lng="en">echidna</trd>
  <trd lng="fr">&eacute;chidn&eacute;</trd>
  <trd lng="pl">kolczatka</trd>
  <trd lng="sk">je&zcaron;ura</trd>
  <trd lng="uk">&c_jeu;&c_h;&c_i;&c_d;&#x43D;&c_a;</trd>
  <trd lng="zh">&#x91DD;&#x9F34;&#x79D1; <pr>zh&#x113;ny&#x1CE;nk&#x113;</pr></trd>
</drv>
</art>

