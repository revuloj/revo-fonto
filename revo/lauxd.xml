   <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_r;&c_i;&c_c;&c_a;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_o;&c_s;&c_malmol;&c_zh;&c_d;&c_a;&c_m;</trd>,
