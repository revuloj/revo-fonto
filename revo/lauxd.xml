<drv mrk="lauxd.0i">
  <kap><ofc>*</ofc><tld/>i
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -i</lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi aproban, favoran opinion pri meritoj, kvalitoj de iu
      a&ubreve;
      io:
      <ekz>
        <tld/>i iun, ies agon;
      </ekz>
      <ekz>
        kiu sin mem <tld/>as, tiun neniu apla&ubreve;das<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne <tld/>u la tagon anta&ubreve; vespero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni <tld/>as homon la&ubreve; lia sa&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o de mortinto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apolog1.0o">apologio</ref>,
      <ref cel="panegi.0o">panegiro</ref>,
      <ref cel="apoteo.0o">apoteozo</ref>,
      <ref cel="flat.0i">flati</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="cxiel1.0o">&ccirc;ielo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_v;&c_a;&c_l;&c_ja;</trd>
  <trd lng="br">meuli&ntilde;</trd>
  <trd lng="cs">chv&aacute;lit</trd>
  <trd lng="de">loben</trd>
  <trdgrp lng="en">
    <trd>laud</trd>,
    <trd>praise</trd>,
    <trd>acclaim</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>alabar</trd>,
    <trd>laurear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>louer</trd>,
    <trd>vanter<klr> (les m&eacute;rites)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dics&eacute;r</trd>,
    <trd>megdics&eacute;r</trd>,
    <trd>m&eacute;ltat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loven</trd>,
    <trd>prijzen</trd>,
    <trd>roemen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chwali&cacute;</trd>,
    <trd>pochwala&cacute;</trd>,
    <trd>zaleca&cacute;</trd>,
    <trd>s&lstroke;awi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>louvar</trd>,
    <trd>gabar</trd>,
    <trd>elogiar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>accepta</trd>,
    <trd>laud&abreve;</trd>,
    <trd>recomanda</trd>,
    <trd>celebra</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">ber&ouml;mma</trd>
  <trd lng="uk">&c_h;&c_v;&c_a;&#x43B;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x5609;&#x8A31; <pr>ji&#x101;x&#x1D4;</pr></trd>,
    <trd>&#x5609;&#x8BB8; <pr>ji&#x101;x&#x1D4;</pr></trd>,
    <trd>&#x8868;&#x626C; <pr>bi&#x1CE;oy&#xE1;ng</pr></trd>,
    <trd>&#x8868;&#x63DA; <pr>bi&#x1CE;oy&#xE1;ng</pr></trd>,
    <trd>&#x8D0A;&#x8B7D; <pr>z&#xE0;ny&#xF9;</pr></trd>,
    <trd>&#x8D5E;&#x8A89; <pr>z&#xE0;ny&#xF9;</pr></trd>,
    <trd>&#x8B9A;&#x7F8E; <pr>z&#xE0;nm&#x11B;i</pr></trd>,
    <trd>&#x8D5E;&#x7F8E; <pr>z&#xE0;nm&#x11B;i</pr></trd>,
    <trd>&#x5938; <pr>ku&#x101;</pr></trd>,
    <trd>&#x8A87; <pr>ku&#x101;</pr></trd>,
    <trd>&#x5439;&#x62C2; <pr>chu&#x12B;f&#xFA;</pr></trd>,
    <trd>&#x5938;&#x5956; <pr>ku&#x101;ji&#x1CE;ng</pr></trd>,
    <trd>&#x8A87;&#x734E; <pr>ku&#x101;ji&#x1CE;ng</pr></trd>,
    <trd>&#x95C1; <pr>b&#x101;o</pr></trd>
  </trdgrp>
</drv>

