<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>la&ubreve;d</rad>/i
</kap>
<drv mrk="lauxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi aproban, favoran opinion pri meritoj, kvalitoj de iu a&ubreve; io:
      <ekz>
        <tld/>i iun, ies agon;
      </ekz>
      <ekz>
        kiu sin mem <tld/>as, tiun neniu apla&ubreve;das<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne <tld/>u la tagon anta&ubreve; vespero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni <tld/>as homon la&ubreve; lia sa&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o de mortinto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apolog1.0o">apologio</ref>,
      <ref cel="panegi.0o">panegiro</ref>,
      <ref cel="apoteo.0o">apoteozo</ref>,
      <ref cel="flat.0i">flati</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="cxiel1.0o">&ccirc;ielo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>                    
  <trd lng="br">meuli&ntilde;</trd>
  <trd lng="cs">chv&aacute;lit</trd>
  <trd lng="de">loben</trd>
  <trd lng="en">praise</trd>
  <trd lng="es">alabar</trd>
  <trdgrp lng="hu">
    <trd>dics&eacute;r</trd>,
    <trd>megdics&eacute;r</trd>,
    <trd>m&eacute;ltat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loven</trd>,
    <trd>prijzen</trd>,
    <trd>roemen</trd>
  </trdgrp>
  <trd lng="pt">elogiar</trd>
  <trd lng="ru">&c_h;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">ber&ouml;mma</trd>
</drv>

<drv mrk="lauxd.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Inda je <tld/>oj:
      <ekz>
        <tld/>inda konduto.
      </ekz>
    </dif>
  </snc>       
  <trd lng="br">meulus</trd>
  <trdgrp lng="hu">
    <trd>dics&eacute;rend&odblac;</trd>,
    <trd>dics&eacute;retes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lovenswaardig</trd>,
    <trd>prijzenswaardig</trd>,
    <trd>bewonderenswaardig</trd>,
    <trd>loffelijk</trd>,
    <trd>verdienstelijk</trd>,
    <trd>bewonderenswaard</trd>,
    <trd>prijzenswaard</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="lauxd.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi malaproban opinion pri io.
    </dif>
    <refgrp tip="vid">
      <ref cel="kondam.0i.deklari_malprava_riprocxinda">kondamni</ref>,
      <ref cel="kritik.0i">kritiki</ref>,
      <ref cel="vip.0i">vipi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_mol;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>     
   <trdgrp lng="br">
    <trd>karezi&ntilde;</trd>,
    <trd>rebech</trd>
  </trdgrp>
  <trd lng="de">tadeln</trd>
  <trdgrp lng="hu">
    <trd>szid</trd>,
    <trd>korhol</trd>,
    <trd>dorg&aacute;l</trd>,
    <trd>hib&aacute;ztat</trd>
  </trdgrp>
   <trdgrp lng="nl">
    <trd>laken</trd>,
    <trd>misprijzen</trd>,
    <trd>afkeuren</trd>
  </trdgrp>
  <trd lng="pt">criticar</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_u;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_c;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="vo">blam&ouml;n</trd>
</drv>

<drv mrk="lauxd.mal0inda">
  <kap>mal<tld/>inda</kap>
  <snc>
    <dif>
      Inda je mal<tld/>oj.
    </dif>
  </snc>     
  <trd lng="br">diveulus</trd>
  <trdgrp lng="de">
    <trd>tadelnswert</trd>,
    <trd>tadelnsw&uuml;rdig</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kifog&aacute;solhat&oacute;</trd>,
    <trd>k&aacute;rhoztatand&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afkeurenswaard</trd>,
    <trd>verwerpelijk</trd>,
    <trd>afkeurenswaardig</trd>,
    <trd>laakbaar</trd>,
    <trd>uit den <ind>boze</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j; <ind>&c_p;&c_o;&c_r;&c_i;&c_c;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
</drv>
</art>
