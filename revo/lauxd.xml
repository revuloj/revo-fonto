<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>la&ubreve;d</rad>/i
</kap>
<drv mrk="lauxd.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi aproban, favoran opinion pri meritoj, kvalitoj de iu a&ubreve; io:
      <ekz>
        <tld/>i iun, ies agon;
      </ekz>
      <ekz>
        kiu sin mem <tld/>as, tiun neniu apla&ubreve;das<fnt>Z</fnt>;
      </ekz>
      <ekz>ne <tld/>u la tagon anta&ubreve; vespero
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        oni <tld/>as homon la&ubreve; lia sa&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o de mortinto;
      </ekz>
      <ekz>
        <tld/>inda konduto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="apolog1.0o">apologio</ref>,
      <ref cel="panegi.0o">panegiro</ref>,
      <ref cel="apoteo.0o">apoteozo</ref>,
      <ref cel="flat.0i">flati</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="cxiel1.0o">&ccirc;ielo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">chv&aacute;lit</trd>
  <trd lng="en">praise</trd>
  <trd lng="es">alabar</trd>
  <trd lng="nl">prijzen</trd>
  <trd lng="ru">&c_h;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">ber&ouml;mma</trd>
</drv>

<drv mrk="lauxd.mal0i">
  <kap>
    mal<tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi malaproban opinion pri
    </dif>
    <refgrp tip="sin">
      <ref cel="kritik.0i">kritiki</ref>,
      <ref cel="vip.0i">vipi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>laken</trd>,
    <trd>afkeuren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_u;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_c;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
