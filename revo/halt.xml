<drv mrk="halt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="halt.0i.KOMUNE">
    <dif>
      &Ccirc;esi agi, iri, movi&gcirc;i ktp:
      <ekz>
        la soldatoj <tld/>is post longa mar&scirc;ado;
      </ekz>
      <ekz>
        la vagonaro <tld/>as dek minutojn &ccirc;e tiu stacio;
      </ekz>
      <ekz>
        hoko eksaltis, afero <tld/>is
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        laboro ne estas leporo &#x2010; &gcirc;i <tld/>os, ne forsaltos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne rigardu malanta&ubreve;en kaj <tld/>u nenie
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>u!</ind>
        <klr>(ne iru plue)</klr>;
        <trdgrp lng="be">
          <trd>&c_s;&c_t;&c_o;&c_p;!</trd>,
          <trd>&c_s;&c_t;&c_o;&c_j;!</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Halt!</trd>,
          <trd>Stopp!</trd>,
          <trd>Stop!</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>&#xA1;alto!</trd>,
          <trd>&#xA1;para!</trd>,
          <trd>&#xA1;pare!</trd>
        </trdgrp>
        <trd lng="fr">halte!</trd>
        <trdgrp lng="hu">
          <trd>&aacute;llj!</trd>,
          <trd>stop!</trd>
        </trdgrp>
        <trdgrp lng="id">
          <trd>ber<ind>henti</ind>!</trd>,
          <trd>stop!</trd>
        </trdgrp>
        <trd lng="nl">halt!</trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_t;&c_o;&c_p;!</trd>,
          <trd>&c_s;&c_t;&c_o;&c_j;!</trd>
        </trdgrp>
      </ekz>
      <ekz>
        jam de hiera&ubreve; mia horlo&gcirc;o <tld/>is;
      </ekz>
      <ekz>
        kiu tro rapide saltas, tiu balda&ubreve;
        <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia rigardo <tld/>is sur unu el ili.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frost.0igxi">frosti&gcirc;i</ref>,
      <ref cel="interm.0i">intermiti</ref>,
      <ref cel="pauxz.0i">pa&ubreve;zi</ref>,
      <ref cel="ripoz.0i">ripozi</ref>,
      <ref cel="strik.0i">striki</ref>
    </refgrp>
  </snc>
  <trd lng="af">stop</trd>
