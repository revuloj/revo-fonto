  <trd lng="pt">parada de metr&ocirc;</trd>
  <trd lng="ro">sta&tcomma;ie de metrou</trd>
  <trdgrp lng="zh">
    <trd>&#22320;&#37941;&#36554;&#31449; <pr>d&igrave;ti&ecaron;ch&emacron;zh&agrave;n</pr></trd>,
    <trd>&#22320;&#37941;&#31449; <pr>d&igrave;ti&ecaron;zh&agrave;n</pr></trd>
  </trdgrp>
</drv>

