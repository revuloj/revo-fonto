<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>halt</rad>/i
</kap>
<drv mrk="halt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;esi agi, iri, movi&gcirc;i ktp:
      <ekz>
        la soldatoj <tld/>is post longa mar&scirc;ado;
      </ekz>
      <ekz>
        la vagonaro <tld/>as dek minutojn &ccirc;e tiu stacio;
      </ekz>
      <ekz>
        <ind><tld/>u!</ind>
        <klr>(ne iru plue)</klr>;
        <trdgrp lng="be">
          <trd>&c_s;&c_t;&c_o;&c_p;!</trd>,
          <trd>&c_s;&c_t;&c_o;&c_j;!</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Halt!</trd>,
          <trd>Stopp!</trd>,
          <trd>Stop!</trd>
        </trdgrp>
        <trd lng="fr">halte!</trd>
        <trdgrp lng="hu">
          <trd>&aacute;llj!</trd>,
          <trd>stop!</trd>
        </trdgrp>
        <trd lng="nl">halt!</trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_t;&c_o;&c_p;!</trd>,
          <trd>&c_s;&c_t;&c_o;&c_j;!</trd>
        </trdgrp>
      </ekz>
      <ekz>
        jam de hiera&ubreve; mia horlo&gcirc;o <tld/>is;
      </ekz>
      <ekz>
        kiu tro rapide saltas, tiu balda&ubreve;
        <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia rigardo <tld/>is sur unu el ili.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pauxz.0i">pa&ubreve;zi</ref>,
      <ref cel="ripoz.0i">ripozi</ref>,
      <ref cel="interm.0i">intermiti</ref>,
      <ref cel="strik.0i">striki</ref>,
      <ref cel="frost.0igxi">frosti&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">zastavit se</trd>
  <trdgrp lng="de">
    <trd><klr tip="ind">(an)</klr>halten</trd>,
    <trd>anhalten</trd>
  </trdgrp>
  <trd lng="en">halt</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>arr&ecirc;ter</ind></trd>,
    <trd>cesser</trd>,
    <trd>faire une <ind>halte</ind></trd>,
    <trd>stopper<klr tip="ind"> (faire un arr&ecirc;t)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>meg&aacute;ll&iacute;t</trd>,
    <trd>le&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stoppen</trd>,
    <trd>halt houden</trd>,
    <trd>stilstaan</trd>
  </trdgrp>
  <trd lng="pt">parar</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="vo">stop&ouml;n</trd>
</drv>

<drv mrk="halt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i, stato de <tld/>into; momento, kiam oni ne plu agas,
      iras, movi&gcirc;as ktp:
      <ekz>
        doni fura&gcirc;on al sia azeno dum la nokta <tld/>o
        <fnt><bib>MT</bib>, <lok>&Gen; 42:27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_y;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
     <trd>Halten</trd>,
     <trd>Stoppen</trd>
  </trdgrp>
  <trd lng="fr">halte<klr tip="ind"> (action, &eacute;tat,
moment)</klr></trd>
  <trd lng="hu">meg&aacute;ll&aacute;s</trd>
  <trd lng="pt">parada</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;<klr tip="amb"> 
  (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;, &c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;, &c_m;&c_o;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>

<drv mrk="halt.0adi">
  <kap>
    <tld/>adi
  </kap>
  <snc>
    <dif>
      Restadi <tld/>ante:
      <ekz>
        la policanoj kriis al la homamaso: ne <tld/>adu!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_ja;&c_c;&c_mol; <klr>(&c_s;&c_p;&c_y;&c_n;&c_ib;&c_w;&c_sh;&c_y;&c_s;&c_ja;)</klr></trd>
  <trdgrp lng="de">
     <trd>Halten</trd>,
     <trd>Stehen</trd>
  </trdgrp>
  <trd lng="fr">stationner<klr tip="ind"> (intr., rester un certain
temps)</klr></trd>
  <trdgrp lng="hu">
     <trd>&aacute;ll<klr tip="ind"> (egy ideig)</klr></trd>,
     <trd>tart&oacute;zkodik</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; <klr>(&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_v;&c_sh;&c_i;&c_s;&c_mol;)</klr></trd>
</drv>

<drv mrk="halt.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie oni <tld/>as.
    </dif>
    <refgrp tip="vid">
      <ref cel="staci.0o">stacio</ref>,
      <ref cel="etap.0o">etapo</ref>,
      <ref cel="stadi.0o.haltejo">stadio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_p;&c_y;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Haltestelle</trd>,
     <trd>Haltepunkt</trd>,
     <trd>Halteplatz</trd>
  </trdgrp>
  <trd lng="fr">halte<klr tip="ind"> (lieu de halte)</klr></trd>
  <trdgrp lng="hu">
     <trd>meg&aacute;ll&oacute;</trd>,
     <trd>meg&aacute;ll&oacute;hely</trd>
  </trdgrp>
  <trd lng="nl">halte</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;<klr tip="amb"> (&c_m;&c_je;&c_s;&c_t;&c_o;)</klr></trd>,
    <trd>&c_s;&c_t;&c_o;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="halt.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      &Ccirc;esigi agi, iri, movi&gcirc;i ktp:
      <ekz>
        <tld/>igi veturilon, vagonaron, trupon, &ccirc;evalon,
        ma&scirc;inon, larmojn, propagandon;
      </ekz>
      <ekz>
        li <tld/>igis iun pasanton, por demandi lin pri la vojo;
      </ekz>
      <ekz>
        la ekkrioj <tld/>igis la oratoron;
      </ekz>
      <ekz>
        li kuris al la abismo kaj oni ne <tld/>igis lin.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Senmovigi, fiksi:
      <ekz>
        <tld/>igi sian rigardon sur iu, io;
      </ekz>
      <ekz>
        <tld/>igi la spiron.
      </ekz>
    </dif>
    <ref tip="vid" cel="parali.0i">paralizi</ref>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_y;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd><klr tip="ind">(jmd.)</klr> anhalten</trd>,
    <trd><klr tip="ind">(jmd.)</klr> stoppen</trd>,
    <trd><klr tip="ind">(jmd.)</klr> unterbrechen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arr&ecirc;ter</trd>,
    <trd>fixer</trd>,
    <trd>immobiliser</trd>,
    <trd>stopper<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meg&aacute;ll&iacute;t</trd>,
    <trd>le&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stopzetten</trd>,
    <trd>doen <ind>stoppen</ind></trd>
  </trdgrp>
  <trd lng="pt">parar</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="halt.0igilo">
  <kap>
    <tld/>igilo
  </kap>
  <snc>
    <dif>
      Ilo por <tld/>igi:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio estis <tld/>igilo por lia fervoro.
      </ekz>
    </dif>
    <ref tip="vid" cel="brid.0o">brido.</ref>
  </snc>
  <snc>
    <uzo tip="klr">(de veturilo)</uzo>
    <ref tip="dif" cel="brems.0o">bremso.</ref>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_m;&c_a;&c_z;</trd>
  <trd lng="de">Bremse</trd>
  <trdgrp lng="fr">
    <trd>frein</trd>,
    <trd>entrave</trd></trdgrp>
  <trd lng="hu">f&eacute;k</trd>
  <trd lng="nl">rem</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;</trd>
</drv>

<drv mrk="halt.0igsxuo">
  <kap>
    <tld/>ig&scirc;uo
  </kap>
  <snc>
    <dif>
      Ligna a&ubreve; fera peco, servanta por <tld/>igi per frotpremado sur
      la rado.
    </dif>
  </snc>
  <trd lng="de">Bremsschuh</trd>
  <trd lng="fr">patin<klr tip="ind"> (de frein)</klr></trd>
  <trd lng="hu">f&eacute;kpofa</trd>
  <trd lng="nl">remblok</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_n;&c_o;&c_j; &c_b;&c_a;&c_sh;&c_m;&c_a;&c_k;</trd>
</drv>

<drv mrk="halt.antaux0igi">
  <kap>
    anta&ubreve;<tld/>igi
  </kap>
  <snc>
    <dif>
      Malhelpi, ke iu evitinda&jcirc;o okazu:
      <ekz>
        anta&ubreve;<tld/>igi la revenon de malsano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Preska&ubreve;aj sinonimoj:
      <ref cel="help.antauxmal0i">anta&ubreve;malhelpi</ref>,
      <ref cel="for.antaux0igi">anta&ubreve;forigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_h;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_e;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>verh&uuml;ten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>emp&ecirc;cher<klr tip="ind"> (pr&eacute;venir)</klr></trd>,
    <trd>pr&eacute;venir<klr tip="ind"> (&eacute;viter)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>megel&odblac;z<klr tip="ind"> (rosszat)</klr></trd>,
    <trd>id&odblac;ben <ind>le&aacute;ll&iacute;t</ind></trd>
  </trdgrp>
  <trd lng="nl">voork&oacute;men</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="halt.sen0a">
  <kap>
    sen<tld/>a
  </kap>
  <snc>
    <dif>
      Sen&ccirc;esa, seninterrompa:
      <ekz>
        sen<tld/>a laborado;
      </ekz>
      <ekz>
        sen<tld/>a vagonaro
        <klr>(kiu iras rekte al la destinita loko, ne <tld/>ante
        dumvoje)</klr>;
      </ekz>
      <ekz>
        sen<tld/>e propagandi;
      </ekz>
      <ekz>
        nia afero kreskas regule kaj sen<tld/>e
        <fnt><bib>Paroloj</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsta.0a">konstanta</ref>,
      <ref cel="kontin3.0a">kontinua</ref>,
      <ref cel="dauxr.0a">da&ubreve;ra</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_s;&c_mol;&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_u;&c_p;&c_y;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_ja;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>ununterbrochen</trd>,
     <trd>ohne <ind>Unterbrechung</ind></trd>,
     <trd>durchgehend</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sans <ind>arr&ecirc;t</ind></trd>,
    <trd>continuel</trd>,
    <trd>incessant</trd>,
    <trd>sans <ind>interruption</ind></trd></trdgrp>
  <trdgrp lng="hu">
     <trd>meg&aacute;ll&aacute;s n&eacute;lk&uuml;li</trd>,
     <trd>szakadatlan</trd>
  </trdgrp>
  <trd lng="nl">ononderbroken</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
