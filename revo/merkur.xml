<art mrk="$Id: merkur.xml,v 1.21 2023/09/02 06:04:11 revo Exp $">
<kap>
  <rad>merkurial</rad>/o
</kap>
<drv mrk="merkur.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>
      (<trd lng="la">Mercurialis</trd>);
      <ref tip="lst" cel="veget.0ajxo" lst="voko:malpigioparencoj">vegeta&jcirc;o</ref>
      kun kontra&ubreve;sidaj segilrandaj ovo- a&ubreve; lancetformaj folioj,
      verdaj a&ubreve; flave verdaj tri- a&ubreve; kvarnombraj floretoj
      kaj haraj fruktoj similantaj testikujon:
      <ekz>
        dioikaj plantoj havas masklaj<klr>[n]</klr> kaj inaj<klr>[n]</klr>
        individuoj<klr>[n]</klr>, al tiu grupo apartenas mardorno, kaj kapra s<klr>[a]</klr>liko a&ubreve;
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=597488">Fremdpolenigado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/7/78/Mercurialis_annua_%288236700055%29.jpg/397px-Mercurialis_annua_%288236700055%29.jpg" prm="CC BY 2.0">jara <tld/>o (Mercurialis annua)

       <fnt>
         <aut>Radio Tonreg de Vieno, A&ubreve;strujo, provizita de Jacopo Werther</aut>
         <lok><url ref="&WCW;/File:Mercurialis_annua_(8236700055).jpg">&WCW;/File:Mercurialis_annua_(8236700055).jpg</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_je;&c_v;&c_a;&c_r;&c_o;&c_c;&c_mol;</trd>
  <trd lng="ca">mercurial</trd>
  <trd lng="de">Bingelkraut</trd>
  <trd lng="fr">mercuriale</trd>
  <trd lng="hu">sz&eacute;lf&udblac;</trd>
  <trd lng="ja">&#x30E4;&#x30DE;&#x30A2;&#x30A4;</trd>
  <trd lng="nl">bingelkruid</trd>
  <trd lng="pl">szczyr</trd>
  <trdgrp lng="pt">
    <trd>mercurial</trd>,
    <trd>azougue <klr>(bot.)</klr></trd>,
    <trd>urtiga-morta</trd>,
    <trd>mercurial-anual</trd>
  </trdgrp>
  <trd lng="ro">trep&abreve;d&abreve;toare</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_l;&c_je;&c_s;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_je;&#x43B;&c_ib;&c_s;&#x43A;&c_a;</trd>
</drv>
</art>
