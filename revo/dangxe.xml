<drv mrk="dangxe.sen0eco">
  <kap>sen<tld/>eco,
  <var><kap>sen<tld/>o</kap></var></kap>
  <snc mrk="dangxe.sen0eco.KOMUNE">
    <dif>
      Foresto de &ccirc;ia <tld/>o,
      <ref tip="vid" cel="sekur.0eco.KOMUNE">sekureco</ref>:
      <ekz>
        le&gcirc;o garantios al Esperanto <klr>[&#8230;]</klr>
        plenan sen<tld/>econ kontra&ubreve; &ccirc;iuj personaj kapricoj
        <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        esplori la sen<tld/>econ de kuracilo;
      </ekz>
      <ekz>
        &scirc;i rifuzis permesi lian eventualan malproksimon difekti la
        &gcirc;ojon, kiun alportis scio pri lia sen<tld/>o
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y; &c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Gefahrlosigkeit</trd>,
    <trd>Ungef&auml;hrlichkeit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>innocuit&eacute;</trd>,
    <trd>s&ucirc;ret&eacute;<klr tip="ind">
    (innocuit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="id"><baz>aman</baz>keamanan</trd>
  <trd lng="ja">&#x5B89;&#20840;&#24615;  <pr>&#12354;&#12435;&#x305C;&#12435;&#x305B;&#12356;</pr></trd>
  <trdgrp lng="nl">
    <trd>onschadelijkheid</trd>,
    <trd>ongevaarlijkheid</trd>,
    <trd>onschuldigheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezpiecze&nacute;stwo</trd>,
    <trd>nieszkodliwo&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_p;&c_je;&c_k;&c_a;</trd>
</drv>

<drv mrk="dangxe.okazo_de_0o">
  <kap>
    okazo de <tld/>o,
    <var><kap>okaze de <tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      Okazo en kiu oni frontas <tld/>on:
      <ekz>
        <klr>[li]</klr> neniam forlasu Ba&hcirc;ara&hcirc;on, esceptante nur okazon de <tld/>o por
        la vivo
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistis nur unu en- kaj el-irejo, kaj mankis indikiloj kien fu&gcirc;i okaze de
        <tld/>o
        <fnt>

          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2013/010971.php">Kiso mortiga</url></vrk>
          <bib>Monato</bib>
          <lok>2013/04, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="okaz.0o_de_bezono">okazo de ur&gcirc;eco</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a; <klr>(&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k;, &c_a;&c_b;&c_s;&c_t;&c_a;&c_v;&c_ib;&c_n;&c_y;)</klr></trd>,
    <trd>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_a;&c_k;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k; &c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">im <ind>Notfall</ind></trd>
  <trd lng="en">in case of <ind>emergency</ind></trd>
  <trd lng="fr">en cas d'<ind>urgence</ind></trd>
  <trd lng="id"><ind>dalam</ind> keadaan bahaya</trd>
  <trdgrp lng="nl">
    <trd>in een <ind>noodgeval</ind></trd>,
    <trd>in geval van <ind>nood</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w przypadku <ind>niebezpiecze&nacute;stwa</ind></trd>,
    <trd>w razie <ind>niebezpiecze&nacute;stwa</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D27;&#24613;&#x65F6; <pr>j&#x1D0;nj&#xED;sh&#xED;</pr></trd>,
    <trd>&#22823;&#x4E0D;&#x4E86; <pr>d&#xE0;ib&#xF9;le</pr></trd>,
    <trd>&#x4E07;&#x4E0D;&#x5F97;&#x5DF2; <pr>w&#xE0;nb&#xF9;d&#xE9;y&#x1D0;</pr></trd>,
    <trd>&#22914;&#x679C;&#x5FC5;&#35201;&#30340;&#x8A71; <pr>r&#xFA;gu&#x1D2;b&#xEC;y&#xE0;odehu&#xE0;</pr></trd>
  </trdgrp>
</drv>
