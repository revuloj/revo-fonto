<drv mrk="dangxe.0a">
  <kap><tld/>a</kap>
  <snc mrk="dangxe.0a.KOMUNE">
    <dif>
      Enhavanta, prezentanta <tld/>on:
      <ekz>
        la situacio fari&gcirc;is sufi&ccirc;e <tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur <tld/>a vojo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ofte ili haltigas a&ubreve; turnas la &scirc;ipon, por eviti <tld/>ajn akvoturnojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas <tld/>a homo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        fini la <tld/>an interparoladon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kuracistoj <klr>[&#8230;]</klr> ekster granda malgrasi&gcirc;o trovis neniun <tld/>an
        simptomon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia kunestado povas doni <tld/>an ekzemplon por niaj laboristinoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ci ludas tre <tld/>an ludon, &scirc;i diris
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvo trankvila estas akvo <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        estinta amiko estas plej <tld/>a malamiko
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sekur.mal0a">malsekura</ref>,
<ref cel="risk.0a">riska</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_o;&c_p;&c_a;&c_s;&c_je;&c_n;</trd>
