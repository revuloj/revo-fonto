      <trd>nonhazardous</trd>
    </trdgrp>
    <trd lng="fa">&a_b;&f_y;&#x200C;&a_kh;&a_T;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>b&eacute;nin</trd>,
