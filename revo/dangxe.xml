      Meti en <tld/>on:
      <ekz>
        kial ni anka&ubreve; en<tld/>i&gcirc;as &ccirc;iun horon
        <fnt><bib>NT</bib>, <lok>&Ko1; 15:30</lok></fnt>?
      </ekz>
      <ekz>
        mi timas, ke mi grave en<tld/>igas min
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Chilo</nom> ne ektir<klr>[i]</klr>s lin je la rando de la mantelo, timante, ke
        <nom>Vinicius</nom> faros ion, kio povus ilin en<tld/>igi
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompro1.0i">kompromiti</ref>,
      <ref cel="sxance.0i">&scirc;anceli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;
  <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_y;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_a;&c_sh;&c_a;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_ja;&c_m; &c_v; &c_o;&c_p;&c_a;&c_s;&c_n;&c_o; &c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>arvari&ntilde;<klr tip="ind"> (v.k.)</klr></trd>,
    <trd>lakaat en <ind>da&ntilde;jer</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>posar en <ind>perill</ind></trd>,
    <trd>arriscar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gef&auml;hrden</trd>
    <trd>in <ind>Gefahr bringen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>endanger</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_h; <ind>&a_kh;&a_T;&a_r;</ind>
  &a_A;&a_n;&a_d;&a_A;&a_kh;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>mettre en <ind>danger</ind></trd>,
    <trd>exposer<klr tip="ind"> (&agrave; un danger)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vesz&eacute;lybe sodor</trd>,
    <trd>vesz&eacute;lyeztet</trd>
  </trdgrp>
  <trd lng="nl">in <ind>gevaar</ind> brengen</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;
  <ind>&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  <trd lng="sk">ohrozi&tcaron;</trd>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_v; &c_n;&c_je;&c_b;&c_je;&c_z;&c_p;&c_je;&c_ch;&c_n;&c_je; &c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
</drv>

<drv mrk="dangxe.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="dangxe.sen0a.KOMUNE">
    <dif>
      Ne prezentanta <tld/>on; sen <tld/>o:
      <ekz>
        sen<tld/>a malsano <klr>(sen risko por la vivo)</klr>;
      </ekz>
      <ekz>
        pri tio pli sen<tld/>e estis silenti
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        vivo modera estas vivo sen<tld/>a
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_s;&c_sh;&c_k;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>inofensiu</trd>,
      <trd>innocu</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ungef&auml;hrlich</trd>,
      <trd>gefahrlos</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>nondangerous</trd>,
      <trd>nonhazardous</trd>
    </trdgrp>
    <trd lng="fa">&a_b;&f_y;&zwnj;&a_kh;&a_T;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>b&eacute;nin</trd>,
      <trd>anodin</trd>,
      <trd>sans <ind>danger</ind></trd>,
      <trd>inoffensif</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vesz&eacute;lytelen</trd>,
      <trd>&aacute;rtalmatlan</trd>,
    </trdgrp>
    <trdgrp lng="nl">
      <trd>onschuldig</trd>,
      <trd>ongevaarlijk</trd>,
      <trd>onschadelijk</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bezpieczny</trd>,
      <trd>nieszkodliwy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_v;&c_r;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dangxe.sen0a.eksterdangxera">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="dangxe.ekster0a">Ekster<tld/>a</ref>;
      <ref tip="dif" cel="sekur.0a">sekura</ref>:
      <ekz>
        vi sentis vin sen<tld/>a en via malboneco
        <fnt><bib>MT</bib>, <lok>&Jes; 47:10</lok></fnt>;
      </ekz>
      <ekz>
        Miajn decidojn observu <klr>[&#8230;]</klr>, kaj tiam vi lo&gcirc;os en la
        lando sen<tld/>e
        <fnt><bib>MT</bib>, <lok>&Lev; 25:18</lok></fnt>;
      </ekz>
      <ekz>
        ne &gcirc;enas min al vi skribi ripete, kaj por vi tio estas
        sen<tld/>iga
        <fnt><bib>NT</bib>, <lok>&Fil; 3:1</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ja;&c_k;&c_ib; &c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a; &c_w; <ind>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_y;</ind></trd>,
      <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>segur</trd>,
      <trd>a estalvi</trd>
    </trdgrp>
    <trd lng="fa">&a_d;&a_w;&a_r; &a_A;&a_z;
    <ind>&a_kh;&a_T;&a_r;</ind></trd>
    <trd lng="fr">s&ucirc;r<klr tip="ind"> (hors de danger)</klr></trd>
    <trd lng="nl">buiten <ind>gevaar</ind><klr tip="ind">
    (arch.)</klr></trd>
    <trd lng="ru">&c_n;&c_a;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja;
    &c_v;
    <ind>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_je;&c_n;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&c_r;&c_je;&c_d;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="br">diza&ntilde;jer</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_p;&c_je;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_sh;&c_k;&c_ib;&c_d;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="dangxe.sen0eco">
  <kap>sen<tld/>eco,
  <var><kap>sen<tld/>o</kap></var></kap>
  <snc mrk="dangxe.sen0eco.KOMUNE">
    <dif>                  
      Foresto de &ccirc;ia <tld/>o,
      <ref tip="vid" cel="sekur.0eco.KOMUNE">sekureco</ref>:
      <ekz>
        le&gcirc;o garantios al Esperanto <klr>[&#8230;]</klr>
        plenan sen<tld/>econ kontra&ubreve; &ccirc;iuj personaj kapricoj
        <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        esplori la sen<tld/>econ de kuracilo;
      </ekz>
      <ekz>
        &scirc;i rifuzis permesi lian eventualan malproksimon difekti la 
        &gcirc;ojon, kiun alportis scio pri lia sen<tld/>o
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y; &c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Gefahrlosigkeit</trd>,
    <trd>Ungef&auml;hrlichkeit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>innocuit&eacute;</trd>,
    <trd>s&ucirc;ret&eacute;<klr tip="ind">
    (innocuit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onschadelijkheid</trd>,
    <trd>ongevaarlijkheid</trd>,
    <trd>onschuldigheid</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_p;&c_je;&c_k;&c_a;</trd>
</drv>
