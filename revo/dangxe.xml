<drv mrk="dangxe.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dangxe.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        mi jam estas transe de ilia potenco, jam neniel <tld/>as, ke mi estos suspektata
        <fnt>
          <bib>VaK</bib>
          <lok>Dekna&ubreve;a &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio <tld/>as, estas la fakto, ke ili mortpafas &ccirc;iun
        <fnt>
          <bib>Monato</bib>
          <aut>Ionel Onet</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007036.html"
              >Ekstremismo: Pri kaldronoj kaj potoj: &ccirc;u amba&ubreve; kotaj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as por disputema virino esti la edzino de kuracisto
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;oseo <tld/>as pro subitaj <klr>[&#8230;]</klr> abismorandoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2005/008395.php"
              >Vizito al la nacia parko Vanoise</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        por virta orelo ne <tld/>as vorto malbela
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_p;&c_a;&c_s;&c_je;&c_n; &c_s;&c_malmol;&c_m;</trd>
  <trd lng="br">beza&ntilde; <ind>da&ntilde;jerus</ind></trd>
  <trd lng="ca">ser <ind>perill</ind>&oacute;s</trd>
  <trd lng="de">gef&auml;hrlich sein</trd>
  <trd lng="fa">&a_kh;&a_T;&a_r;&a_n;&a_A;&f_k;
  &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>dangereux</ind></trd>,
    <trd>pr&eacute;senter un <ind>danger</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&samekh;&kaf;&fnun;</trd>
  <trd lng="hu">vesz&eacute;lyes</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>niebezpiecznym</ind></trd>,
    <trd>zagra&zdot;a&cacute;</trd>,
    <trd>by&cacute; <ind>zagro&zdot;eniem</ind></trd>
  </trdgrp>
  <trd lng="nl">gevaarlijk zijn</trd>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol;
  <ind>&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="sk">hrozi&tcaron;</trd>
</drv>

<drv mrk="dangxe.ekster0a">
  <kap>ekster<tld/>a</kap>
  <snc mrk="dangxe.ekster0a.KOMUNE">
    <dif>
      Ne estanta en <tld/>o;
      <ref tip="sin" cel="sekur.0a">sekura</ref>:
      <ekz>
        la tendaro opiniis sin tute ekster<tld/>a
        <fnt><bib>MT</bib>, <lok>&Jug; 8:11</lok></fnt>;
      </ekz>
      <ekz>
        Mi donos al ili la eblon dormi ekster<tld/>e
        <fnt><bib>MT</bib>, <lok>&Hos; 2:18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;-&c_z;&c_a;
  <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="bg">&c_k;&c_o;&c_j;&c_t;&c_o; &c_je; &c_i;&c_z;&c_v;&c_malmol;&c_n; &c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="br">
    <trd>barrek<klr tip="ind"> (er-maez a za&ntilde;jer)</klr></trd>,
    <trd>er-maez a za&ntilde;jer</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>segur</trd>,
    <trd>a estalvi</trd>,
    <trd>fora de perill</trd>
  </trdgrp>
  <trd lng="de">au&szlig;er <ind>Gefahr</ind></trd>
  <trdgrp lng="en">
    <trd>out of <ind>danger</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_r; &a_A;&a_z; <ind>&a_kh;&a_T;&a_r;</ind></trd>,
    <trd>&a_A;&a_m;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&ucirc;r<klr tip="ind"> (hors de danger)</klr></trd>,
    <trd>hors de <ind>danger</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>biztons&aacute;gos</trd>,
    <trd>vesz&eacute;lyen k&iacute;v&uuml;li</trd>
  </trdgrp>
  <trd lng="nl">buiten <ind>gevaar</ind></trd>
  <trdgrp lng="pl">
    <trd>z dala od <ind>niebezpiecze&nacute;stwa</ind></trd>,
    <trd>z dala od <ind>zagro&zdot;enia</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;
    <ind>&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>,
    <trd>&c_n;&c_a;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja;
    &c_v;
    <ind>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </trdgrp>
