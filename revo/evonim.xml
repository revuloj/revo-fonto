  <trdgrp lng="pt">
    <trd>ev&ocirc;nimo</trd>,
    <trd>eu&ocirc;nimo</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_je;&c_s;&c_k;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">br≈°len</trd>
