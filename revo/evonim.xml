<art mrk="$Id: evonim.xml,v 1.21 2023/08/31 11:49:31 revo Exp $">
<kap>
<rad>evonim</rad>/o
</kap>
<drv mrk="evonim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="celastr.0oj">celastracoj</ref>
      (<trd lng="la">Euonymus</trd>), arbedo, kelkaj specioj estas
      kulturataj:
      <ekz>
        <tld/>o estas genro de arbustoj kun kontra&ubreve;e duopaj 
        folioj pinte elipsaj a&ubreve; lancetformaj, glataj, ofte ledecaj,
        vintroda&ubreve;raj &ccirc;e tiuj specioj, kun okulfrape koloraj, kapsulecaj 
        fruktoj, venenaj, ege laksigaj, kaj kun semoj unuope kovritaj de oran&gcirc;e ru&gcirc;a arilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19997">Evonimo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/06/Euonymus_europaeus_9961.jpg/640px-Euonymus_europaeus_9961.jpg" prm="CC BY-SA 4.0">E&ubreve;ropa <tld/>o (Euonymus europaeus) Urbo de Skaneateles, Distrikto Onondaga, Nova Jorko
        <fnt>
          <aut>R. A. Nonenmacher, propra verko</aut>
          <lok><url ref="&WCW;/File:Euonymus_europaeus_9961.jpg">&WCW;/File:Euonymus_europaeus_9961.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="en"><trd>Euonymus</trd>, <trd>spindle</trd></trdgrp>
  <trd lng="be">&c_b;&c_r;&c_y;&c_z;&c_g;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">brslen</trd>
  <trdgrp lng="de">
    <trd>Spindelstrauch</trd>,
    <trd>Pfaffenh&uuml;tchen</trd>
  </trdgrp>
  <trd lng="fr">fusain<klr tip="ind"> (bot.)</klr></trd>
  <trd lng="hu">kecsker&aacute;g&oacute;</trd>
  <trd lng="ja">&#x30CB;&#x30B7;&#x30AD;&#x30AE;</trd>
  <trd lng="pl">trzmielina</trd>
  <trdgrp lng="pt">
    <trd>ev&ocirc;nimo</trd>,
    <trd>eu&ocirc;nimo</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_je;&c_s;&c_k;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">br&scaron;len</trd>
  <trd lng="uk">&#x431;&#x440;&#x443;&#x441;&#x43B;&#x438;&#x43D;&#x430;</trd>
</drv>
</art>
