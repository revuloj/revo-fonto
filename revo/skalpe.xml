<art mrk="$Id: skalpe.xml,v 1.23 2024/03/25 06:40:50 revo Exp $">
<kap>
  <rad>skalpel</rad>/o
</kap>
<drv mrk="skalpe.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Malgranda <ref tip="super" cel="trancx.0ilo">tran&ccirc;ilo</ref>
      por <ref cel="sekc.0i.ANA">sekci</ref>:
      <ekz>
        post kiam la jur-medicinisto <nom>Jakob Kolletschka</nom>
        <klr>[&#x2026;]</klr> dum
        nekropsio vundi&gcirc;is per la <tld/>o kaj mortis malmultajn tagojn 
        poste de sangotoksi&gcirc;o, <nom>Semmelweis</nom> pensis nun koni la
        ka&ubreve;zon 
        de la puerpera febro: la studentoj pri medicino laboris 
        en la anatomia sekcio kaj sekcis tie mortintojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=14930">Ignaz
          Semmelweis</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bistur.0o">bisturio</ref>
    <bld lok="&WCU;/thumb/1/1f/Skalpell.JPG/640px-Skalpell.JPG" prm="CC-BY-SA-3.0"><tld/>o
        <fnt>
          <aut>Markus W&uuml;rfel</aut>
          <lok><url ref="&WCW;/File:Skalpell.JPG">&WCW;/File:Skalpell.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x89E3;&#x5256;&#x5200;  <pr>&#x304B;&#x3044;&#x307C;&#x3046;&#x304B;&#x305F;&#x306A;</pr></trd>,
    <trd>&#x30E1;&#x30B9;  <pr>&#x3081;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="lat">scalprum</trd>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_k;&c_a;&c_l;&c_p;&c_je;&c_l;</trd>
  <trd lng="ca">escalpel</trd>
  <trd lng="cs">skalpel</trd>
  <trd lng="de">Skalpell</trd>
  <trd lng="en">scalpel</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&f_k;&a_A;&a_l;&f_p;&a_l;</trd>,
    <trd>&a_n;&f_y;&a_sh;&a_t;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">scalpel</trd>
  <trdgrp lng="hu">
    <trd>szike</trd>,
    <trd>bonck&eacute;s</trd>
  </trdgrp>
  <trd lng="it">scalpello<klr> (coltello chirurgico)</klr></trd>
  <trd lng="la">scalprum</trd>
  <trdgrp lng="nl">
    <trd>scalpel</trd>,
    <trd>ontleedmes</trd>
  </trdgrp>
  <trd lng="pl">skalpel</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>opera&ccaron;n&yacute; n&ocirc;&zcaron;</trd>,
    <trd>skalpel</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x43A;&#x430;&#x43B;&#x44C;&#x43F;&#x435;&#x43B;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x89E3;&#x5256;&#x5200; <pr>ji&#x11B;p&#x14D;ud&#x101;o</pr></trd>,
    <trd>&#x624B;&#x672F;&#x5200; <pr>sh&#x1D2;ush&#xF9;d&#x101;o</pr></trd>,
    <trd>&#x624B;&#x8853;&#x5200; <pr>sh&#x1D2;ush&#xF9;d&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
