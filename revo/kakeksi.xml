<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kakeksi.xml,v 1.4 2022/04/23 04:41:16 revo Exp $">
<kap>
  <rad>kakeksi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kakeksi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kakeksi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kahxeksi.0o">Ka&hcirc;eksio</ref>:
      <ekz>
        ili resanigos la junulinon en kelkaj horoj &mdash; gangrena inflamo,
        plena <tld/>o &mdash; tio estas malagrabla, sed tute ne dan&gcirc;era
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cachexia</trd>
  <trdgrp lng="pl">
    <trd>kacheksja</trd>,
    <trd>wyniszczenie</trd>,
    <trd>char&lstroke;actwo</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>ca&#537;exie</trd>,
    <trd>epuiz&abreve;rii</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_a;&c_h;&c_je;&c_k;&c_s;&c_ib;&c_ja;</trd>
</drv>
</art>

