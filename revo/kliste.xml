<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klister</rad>/o
</kap>
<drv mrk="kliste.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Laksiga a&ubreve; nutra flua&jcirc;o, en&scirc;prucigita en la rektan inteston
      tra la anuso. 
    </dif>
  </snc>
  <trdgrp lng="en">
