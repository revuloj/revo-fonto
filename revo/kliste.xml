<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kliste.xml,v 1.16 2022/11/02 20:32:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klister</rad>/o
</kap>
<drv mrk="kliste.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Laksiga a&ubreve; nutra flua&jcirc;o, en&scirc;prucigita en la rektan inteston
      tra la anuso.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>klyst&yacute;r</trd>,
    <trd>o&ccaron;istn&eacute; klyzma</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Einlauf</trd>,
    <trd>Klistier</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>enema</trd>,
    <trd>douche<klr> (en Usono, uzata nur p.p. purigo tra la
    vagino)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lavement<klr tip="ind"> (clyst&egrave;re)</klr></trd>,
    <trd>clyst&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>klisteer</trd>,
    <trd>klysma</trd>,
    <trd>darmspoeling</trd>,
    <trd>enema<klr tip="ind"> (darmspoeling)</klr></trd>,
    <trd>lavement</trd>
  </trdgrp>
  <trd lng="pl">lewatywa</trd>     
  <trd lng="ro">clism&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_m;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_s;&c_t;&c_i;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">klyst&iacute;r</trd>
  <trd lng="uk">&#x43A;&#x43B;&c_ib;&c_z;&#x43C;&c_a; (&#x43F;&c_r;&#x43E;&c_c;&c_je;&c_d;&c_u;&c_r;&c_a;)</trd>
</drv>

<drv mrk="kliste.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Doni <tld/>on:
      <ekz>
        <ind><tld/>ilo</ind><fnt>Z</fnt>.
        <trd lng="fr">poire &agrave; lavement</trd>
        <trdgrp lng="nl">
          <trd>klisteerspuit</trd>,
          <trd>enema<klr tip="ind"> (spuit)</klr></trd>,
          <trd>lavementsspuit</trd>
        </trdgrp>
        <trd lng="pl">gruszka <klr>(do lewatywy)</klr></trd>
        <trd lng="ru">&c_k;&c_l;&c_i;&c_z;&c_m;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <adm>
    Koncerne la ekz-on "~ilo": PIV2 distingas inter "~ilo" (tuta
    aparato (ujo, tubo, kanulo) kaj "~pilko". Franca traduko
    provizora.
    <aut>MB</aut>
  </adm>
  <trd lng="nl">een <ind>lavement</ind> zetten</trd>
  <trdgrp lng="fr">
    <trd>administrer un <ind>lavement</ind></trd>,
    <trd>administrer un <ind>clyst&egrave;re</ind></trd>
  </trdgrp>
  <trd lng="pl">lewatyw&eogonek; wykonywa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
    &c_k;&c_l;&c_i;&c_z;&c_m;&c_u;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_p;&c_r;&c_o;&c_m;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">da&tcaron; klyst&iacute;r</trd>
  <trd lng="cs">d&aacute;t klyst&yacute;r</trd>
</drv>
</art>
