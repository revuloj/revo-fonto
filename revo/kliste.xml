  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_k;&c_l;&c_i;&c_z;&c_m;&c_u;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_p;&c_r;&c_o;&c_m;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
