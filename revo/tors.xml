<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tors.xml,v 1.24 2023/10/21 14:37:42 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>tors</rad>/o
</kap>
<drv mrk="tors.0o">
  <kap><tld/>o</kap>
  <snc mrk="tors.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Supera parto de la homa <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>
      sen la kapo kaj sen la brakoj,
      konsiderata aparte de la cetera korpo;
      <ref tip="sin" cel="trunk.0o.ANA">trunko
      <sncref ref="trunk.0o.ANA"/></ref>:
      <ekz>
        la kapo de Dagon kaj liaj amba&ubreve; manoj dehakitaj ku&scirc;as sur la sojlo, nur la <tld/>o restis
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 5:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru pa&scirc;ojn iom pli longajn ol kutime kaj la irado iom
        pli rapida, &ccirc;iu muskolo movi&gcirc;as, la <tld/>o ritme turni&gcirc;as
        dekstren kaj maldekstren
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008790.php">Praktiki bastonmar&scirc;adon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Rumpf</trd>
    <trdgrp lng="en">
       <trd>torso</trd>,
       <trd>trunk</trd>
    </trdgrp>
    <trdgrp lng="hu">
       <trd>t&ouml;rzs</trd>,
       <trd>fels&odblac; <ind>test</ind></trd>
    </trdgrp>
    <trd lng="pl">tors</trd>
    <trdgrp lng="pt">
       <trd>torso</trd>,
       <trd>tronco</trd>
    </trdgrp>
  </snc>
  <snc mrk="tors.0o.BELA">
     <uzo tip="fak">BELA</uzo>
     <dif>
       Skulpta&jcirc;o prezentanta nur tiun parton:
      <ekz>
        Apolonio de Ateno <klr>[&#x2026;]</klr>
        estas fama &ccirc;efe pro lia mirinda <tld/>o de Heraklo, kiu nuntempe
        trovi&gcirc;as en la <nom>Belvedere</nom>, unu de la muzeoj de Vatikano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=141913">Apolonio de Ateno</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="bust.0o">busto</ref>.
     </refgrp>
     <trd lng="en">torso</trd>
     <trd lng="hu">torz&oacute;</trd>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_s;</trd>
  <trd lng="ca">tors</trd>
  <trdgrp lng="cs">
    <trd>torzo</trd>,
    <trd>trup sochy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Torso</trd>,
  </trdgrp>
  <trd lng="fr">torse</trd>
  <trdgrp lng="it">
     <trd>torso</trd>,
     <trd>busto</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0A;&#x534A;&#x8EAB;  <pr>&#x3058;&#x3087;&#x3046;&#x306F;&#x3093;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x80F4;  <pr>&#x3069;&#x3046;</pr></trd>,
    <trd>&#x30C8;&#x30EB;&#x30BD;</trd>
  </trdgrp>
  <trd lng="nl">romp</trd>
  <trd lng="pl">popiersie</trd>
  <trdgrp lng="pt">
    <trd>torso</trd>,
    <trd>tronco</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_s;</trd>
  <trd lng="sk">poprsie s ramenami</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43E;&#x440;&#x441;</trd>,
    <trd>&#x442;&#x443;&#x43B;&#x443;&#x431;</trd>
  </trdgrp>
</drv>
</art>
