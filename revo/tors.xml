<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tors</rad>/o
</kap>
<drv mrk="tors.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>  
    <dif>
      Supera parto de la homa korpo sen la kapo kaj sen la brakoj,
      konsiderata aparte de la cetera korpo;
      <ref tip="sin" cel="trunk.0o.ANA">trunko
      <sncref ref="trunk.0o.ANA"/></ref>.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BELA</uzo>  
    <dif>
      Skulpta&jcirc;o prezentanta nur tiun parton.
    </dif>
    <refgrp tip="sin">
      <ref cel="bust.0o">busto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Torso</trd>
  <trd lng="en">trunk</trd>
  <trd lng="nl">romp</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_s;</trd>
</drv>
</art>
