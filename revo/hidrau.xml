<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidra&ubreve;lik</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="hidrau.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Scienco pri la fortoj kaj efikoj produktataj de akvo. 
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_v;&c_l;&c_i;&c_k;&c_a;</trd>
</drv>
<drv mrk="hidrau.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Rilatanta al akvo. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Funkcianta per akvo:
      <ekz><tld/>a bremso, ma&scirc;ino, premilo.</ekz>
    </dif>
  </snc>
  <snc num="3">
    <dif>
      Malmoli&gcirc;anta en akvo:
      <ekz><tld/>a kalko.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_v;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
