<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidra&ubreve;lik</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="hidrau.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako"
      lst="voko:sciencoj">Scienco</ref> kaj
      <ref tip="super" cel="teknik.0o">te&hcirc;niko</ref>
      pri la fortoj kaj efikoj produktataj de flua&jcirc;oj kaj precipe
      de akvo.
    </dif>
  </snc>
  <adm>
    La formo "tehxniko" uzata de mi en la difino ankoraux
    ne ekzistas. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_w;&c_l;&c_ib;&c_k;&c_a;</trd>
  <trd lng="de">Hydraulik</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&alfa;&ypsilon;&lambda;&jota;&kappa;&eta_ton;</trd>
  <trd lng="en">hydraulics</trd>
  <trd lng="es">hidr&aacute;ulica</trd>
  <trd lng="fr">hydraulique<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="it">idraulica<klr tip="amb"> (scienza)</klr></trd>
  <trd lng="nl">hydraulica</trd>
  <trd lng="pl">hydraulika</trd>
  <trd lng="pt">hidr&aacute;ulica</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_v;&c_l;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="hidrau.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o a&ubreve; al akvo, konsiderata de la vidpunkto de
      tiu scienco:
      <ekz>
        <tld/>a forto.
      </ekz>
    </dif>
    <adm>
      Kontroli la trd de/nl. La difino unue tekstis nur
      "rilata al akvo". <aut>MB</aut>
    </adm>
    <trd lng="de">Wasser-</trd>
    <trd lng="nl">water-</trd>
  </snc>
  <snc>
    <dif>
      Funkcianta per akvo:
      <ekz>
        <tld/>a bremso, ma&scirc;ino, premilo.
      </ekz>
    </dif>
    <trd lng="de">hydraulisch</trd>
    <trd lng="nl">hydraulisch</trd>
  </snc>
  <snc>
    <dif>
      Malmoli&gcirc;anta en akvo:
      <ekz>
        <tld/>a kalko.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_w;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&alfa;&ypsilon;&lambda;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">hydraulic</trd>
  <trd lng="es">hidr&aacute;ulico</trd>
  <trd lng="fr">hydraulique<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="it">idraulico<klr> (agg.)</klr></trd>
  <trd lng="pt">hidr&aacute;ulico</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_v;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
