<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>refrakt</rad>/i
</kap>
<drv mrk="refrak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      &Scirc;an&gcirc;i la direkton de l' radioj, de la lumo, kiam ili trairas
      de unu medio en alian.
    </dif>
  </snc>
  <trd lng="de">brechen</trd>
  <trd lng="nl">breken<klr tip="ind"> (v.lichtstralen)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_l;&c_u;&c_ch;&c_i;)</klr></trd>,
    <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_l;&c_ja;&c_t;&c_mol; <klr
    tip="amb">(&c_l;&c_u;&c_ch;&c_i;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="refrak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      &Scirc;an&gcirc;o de la direkto de elektromagnetaj radioj, kiam ili iras
      de unu medio al alia. Kp. <ref tip="vid"
      cel="difrak.0o">difrakto</ref>.
    </dif>
  </snc>
  <trd lng="de">Brechung</trd>
  <trd lng="en">refraction</trd>
</drv>
</art>
