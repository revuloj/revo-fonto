<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: refrak.xml,v 1.21 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>refrakt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="refrak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="refrak.0i.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &Scirc;an&gcirc;i la direkton de l' radioj, de la lumo, kiam ili trairas
      de unu medio en alian.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a;&c_m;&c_ja;&c_n;&c_ib;)</klr></trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_ch;&c_u;&c_p;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">refractar</trd>
  <trd lng="cs">l&aacute;mat <klr>(sv&ecaron;tlo)</klr></trd>
  <trd lng="de">brechen</trd>
  <trd lng="en">to refract</trd>
  <trd lng="es">refractar</trd>
  <trd lng="fr">r&eacute;fracter</trd>
  <trd lng="ja">&#x5C48;&#x6298;&#x3055;&#x305B;&#x308B;  <pr>&#x304F;&#x3063;&#x305B;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">breken<klr tip="ind"> (v.lichtstralen)</klr></trd>
  <trd lng="pl">za&lstroke;amywa&cacute;  <klr>(promienie)</klr></trd>
  <trd lng="ro">refract&abreve;  <klr>(razele)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_l;&c_u;&c_ch;&c_i;)</klr></trd>,
    <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_l;&c_ja;&c_t;&c_mol; <klr tip="amb">(&c_l;&c_u;&c_ch;&c_i;)</klr></trd>
  </trdgrp>
  <trd lng="sk">l&aacute;ma&tcaron; l&uacute;&ccaron;e</trd>
  <trd lng="uk">&c_z;&c_a;&c_l;&c_o;&c_m;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_ib;</trd>
  <trd lng="zh">&#x6298;&#x5C04; <pr>zh&#xE9;sh&#xE8;</pr></trd>
</drv>

<drv mrk="refrak.0o">
  <kap><tld/>o</kap>
  <snc mrk="refrak.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &Scirc;an&gcirc;o de la direkto de elektromagnetaj radioj, kiam ili iras
      de unu medio al alia:
      <ekz>
        ormozaiko <klr>[&#x2026;]</klr> en mil <tld/>oj re&jcirc;etis la lumon
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Newcomb</nom> direktis la desegnon kaj instaladon
        de nova <tld/>a teleskopo
        <fnt>
          <aut>S. Norvell</aut>,
          <vrk>La Astronomo kiu ne multe Rigardis la Astrojn</vrk>,
          <lok><url ref="http://www.esperanto.ca/files/halifax/inter_ni_2005_06.pdf">
          Inter Ni, 2005-06 (230)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difrak.0o">difrakto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_p;&c_r;&c_a;&c_m;&c_ja;&c_n;&c_jo;&c_w;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_f;&c_r;&c_a;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_ch;&c_u;&c_p;&c_v;&c_a;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">refracci&oacute;</trd>
  <trd lng="cs">refrakce</trd>
  <trd lng="de">Brechung</trd>
  <trd lng="en">refraction</trd>
  <trd lng="es">refracci&oacute;n</trd>
  <trd lng="fr">r&eacute;fraction</trd>
  <trd lng="ja">&#x5C48;&#x6298;  <pr>&#x304F;&#x3063;&#x305B;&#x3064;</pr></trd>
  <trdgrp lng="pl">
    <trd>refrakcja</trd>,
    <trd>za&lstroke;amanie</trd>
  </trdgrp>
  <trd lng="ro">refrac&tcomma;ie</trd>
  <trdgrp lng="sk">
    <trd>lom l&uacute;&ccaron;ov</trd>,
    <trd>refrakcia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_f;&c_r;&c_a;&c_k;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_o;&c_m;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x6298;&#x5C04; <pr>zh&#xE9;sh&#xE8;</pr></trd>
</drv>
</art>
