<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trakto1.xml,v 1.12 2020/02/22 20:10:36 revo Exp $">
  <kap>
    <rad>traktori</rad>/o
  </kap>
<drv mrk="trakto1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc mrk="trakto1.0o.ebena">
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, 
      <ref cel="volv.el0anto">elvolvanto</ref> de
      <ref cel="katen.0o.linio">kateno</ref>:
      <ekz>
        la distanco inter ajna punkto de <tld/>o kaj la punkto
        de &gcirc;ia asimptoto, kie pasas la tan&gcirc;anto &ccirc;e tiu 
        punkto, estas konstanta.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/k_traktorio.svg" tip="svg" alt="400" lrg="400">
    <tld/>o
  </bld>
  <trd lng="be">&c_t;&c_r;&c_a;&c_k;&c_t;&c_r;&c_y;&c_s;&c_a;</trd>
  <trd lng="ca">tractriu</trd>
  <trd lng="de">Traktrix</trd>
  <trd lng="en">tractrix</trd>
  <trd lng="fr">tractrice<klr tip="ind"> (math.)</klr></trd>
  <trdgrp lng="hu">
    <trd>h&uacute;z&oacute;g&ouml;rbe</trd>,
    <trd>traktrix</trd>
  </trdgrp>
  <trd lng="pl">traktrysa</trd>
  <trdgrp lng="pt">
    <trd>tratriz</trd>,
    <trd>trat&oacute;ria</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_k;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>traktrix</trd>,
    <trd>vle&ccaron;n&aacute; krivka</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>traktrix</trd>,
    <trd>vle&ccaron;n&aacute; k&rcaron;ivka</trd>
  </trdgrp>
</drv>
</art>

