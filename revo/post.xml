<drv mrk="post.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="post.0ajxo.KOMUNE">
    <dif>
      <ref tip="dif" cel="post.0o.loke"><tld/>o <sncref/></ref>:
      <ekz>
        <tld/>a&jcirc;o de veturilo, de domo
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o de <nom>Kremser</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=530924">Tendo&ccirc;aro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="antaux.mal0o">malanta&ubreve;o, malanta&ubreve;a&jcirc;o</ref>
    <trdgrp lng="de">
      <trd>Hinterteil</trd>,
      <trd>R&uuml;ckseite</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>trasero</trd>,
      <trd>parte de atr&aacute;s</trd>
    </trdgrp>
    <trd lng="fr">arri&egrave;re<klr> (subst.)</klr></trd>
    <trdgrp lng="hu">
      <trd>h&aacute;tulja</trd>,
      <trd>far</trd>
    </trdgrp>
    <trd lng="nl">achterste deel</trd>
    <trd lng="pl">tylna <klr>(cz&eogonek;&sacute;&cacute;)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_d;</trd>,
      <trd>&c_z;&c_a;&c_d;&c_n;&c_ja;&c_ja;
      &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="post.0ajxo.pugo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref cel="pug.0o">Pugo</ref> de homo, <ref cel="grop.0o">gropo</ref>
      de iuj kvarpieduloj a&ubreve; analoga korpoparto de aliaj bestoj:
      <ekz>
        hunda&ccirc;' <klr>[&#x2026;]</klr> kaptis vin je l' pantalon' <klr>[&#x2026;]</klr>
        
        sur la posta&jcirc;o lar&gcirc;an truon vi honte maskis per &ccirc;apel'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        libero de parolo: <klr>[&#x2026;]</klr> obscene la&ubreve;di <tld/>a&jcirc;ojn de
        piedirantinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Juan Jacobo Schmitter-soto</aut>
          <vrk><url ref="https://www.monato.be/2004/008257.html">Lando
          de la (mal)liberuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili plu ne povis elteni la suferon de konstipi&gcirc;o, kaj levante la
        <tld/>a&jcirc;on petis sian patrinon bastonete elfosi la ferbulan
        feka&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="&Monato;/2012/010573.php">La biciklo de mia
          patro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li donis al li fratecan baton sur la <tld/>a&jcirc;on, tiam li turnis
        sin kaj iris
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Akvo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sago estis bone celita kaj enfiksi&gcirc;is en la
        <tld/>a&jcirc;o de la &ccirc;evalo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 21a</lok></fnt>;
      </ekz>
      <ekz>
        la plej abomenaj <klr>[simioj]</klr> havas malkonvene ru&gcirc;ajn
        <tld/>a&jcirc;ojn, kiuj ne estas en la simia socio maskendaj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_d;&c_u;&c_p;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Hintern</trd>,
      <trd>Po</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>bottom</trd>
      <trd>backside</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>culo</trd>
      <trd>grupa</trd>
    </trdgrp>
    <trd lng="fr">derri&egrave;re<klr> (cul)</klr></trd>
    <trd lng="hu">far</trd>
    <trd lng="nl">achterwerk</trd>
    <trdgrp lng="pl">
      <trd>ty&lstroke;ek</trd>,
      <trd>zad</trd>,
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_d;&c_n;&c_i;&c_c;&c_a;</trd>
  </snc>
</drv>

<drv mrk="post.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="post.0anto.KOMUNE">
    <dif>
      Iu, kiu venas <tld/>e.
    </dif>
    <trdgrp lng="es">
      <trd>siguiente</trd>,
      <trd>el que viene despu&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ut&aacute;na k&ouml;vetkez&odblac;</trd>,
      <trd>r&aacute;k&ouml;vetkez&odblac;</trd>
    </trdgrp>
    <trd lng="pl">p&oacute;&zacute;niejszy
    <klr>(go&sacute;&cacute;)</klr></trd>
  </snc>
  <snc mrk="post.0anto.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib>, <lok>&para;288</lok></fnt>
    <dif>
      <klr>(de elemento en <ref cel="ar.orda0o">orda aro</ref>)</klr>
      Tiu elemento, se &gcirc;i ekzistas,
      <ref cel="najbar.0a.en_orda_aro">najbara</ref> kun &gcirc;i kaj
      pli granda ol &gcirc;i:
      <ekz>
        <frm>5</frm> estas la <tld/>anto de <frm>4</frm>;
      </ekz>
      <ekz>
        &ccirc;iu natura nombro <frm><k>a</k></frm> havas <tld/>anton,
        <frm><k>a</k> + 1</frm>,
        neniu natura nombro havas <frm>0</frm> kiel <tld/>anton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3082">Natura
          nombro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">sucesor</trd>
    <trd lng="pl">nast&eogonek;pnik</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_s;&c_l;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;
      <ind>&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>n&aacute;sledn&yacute; &ccaron;len</trd>,
    <trd>n&aacute;sledovn&iacute;k</trd>,
    <trd>n&aacute;sleduj&iacute;c&iacute; &ccaron;len</trd>
  </trdgrp>
  <trd lng="de">Nachfolger</trd>
  <trd lng="en">successor</trd>
  <trd lng="fr">successeur</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nasledovn&iacute;k</trd>,
    <trd>nasleduj&uacute;ci</trd>
  </trdgrp>
</drv>
<drv mrk="post.0ea">
  <kap><tld/>ea</kap>
  <snc>
    <dif>
     <ref tip="dif" cel="post.0a.tempe"><tld/>a <sncref/></ref>:
      <ekz>
        la&ubreve; <tld/>eaj komentistoj Pitagoro komprenadis filozofion kiel
        amon de sa&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1166">Filozofio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la milito kaj <tld/>ea komunisma nelibereco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3419">Pola
          literaturo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pozd&ecaron;j&scaron;&iacute;</trd>
  <trd lng="de">sp&auml;terer</trd>
  <trd lng="es">posterior</trd>
  <trd lng="hu">ut&aacute;na k&ouml;vetkez&odblac;</trd>
  <trd lng="pl">p&oacute;&zacute;niejszy</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">neskor&scaron;&iacute;</trd>
</drv>

<drv mrk="post.0enigi">
