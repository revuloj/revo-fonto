<drv mrk="post.0a">
  <kap><tld/>a</kap>
  <snc mrk="post.0a.tempe">
    <dif>
      Pli malfrua:
      <ekz>
        la <tld/>an paroladon mi ne a&ubreve;dis;
      </ekz>
      <ekz>
        la historio de la re&gcirc;o <nom>David</nom>, la anta&ubreve;a kaj la
        <tld/>a, estas 
        priskribita en la kroniko de la anta&ubreve;vidisto <nom>Samuel</nom>
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 29:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a jarcento
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj generacioj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la pomoj maturi&gcirc;is, oni kalkulis ilin, sed tuj en la
        <tld/>a tago unu mankis
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sp&auml;terer</trd>
    <trd lng="es">posterior<klr> (en el tiempo)</klr></trd>
    <trd lng="fr">ult&eacute;rieur</trd>
    <trd lng="hu">ut&aacute;na k&ouml;vetkez&odblac;</trd>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="post.0a.loke">
    <dif>
      Loke pli malproksima,
      <ref tip="sin" cel="antaux.mal0a">malanta&ubreve;a</ref>:
      <ekz>
        la <tld/>a parto de &scirc;ipo;
      </ekz>
      <ekz>
        la glavon <klr>[&#x2026;]</klr> trapenetris la <tld/>an parton de la
        korpo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 3:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la leporo ekstaris sur la <tld/>aj piedoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        terurita li kuris al la <tld/>a pordo, sed la hundo, kiu tie
        ku&scirc;is, sin &jcirc;etis al li
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">hinterer</trd>
    <trdgrp lng="es">
      <trd>posterior<klr> (en el espacio)</klr></trd>,
      <trd>parte trasera</trd>,
    </trdgrp>
    <trd lng="fr">arri&egrave;re<klr> (adj.)</klr></trd>
    <trd lng="hu">h&aacute;ts&oacute;</trd>
    <trdgrp lng="pl">
      <trd>p&oacute;&zacute;niejszy</trd>,
      <trd>tylny</trd>,
    </trdgrp>
    <trd lng="ro">din spate</trd>
    <trd lng="ru">&c_z;&c_a;&c_d;&c_n;&c_i;&c_j;</trd>
  </snc>
  <trd lng="cs">pozd&ecaron;j&scaron;&iacute;</trd>
  <trd lng="sk">neskor&scaron;&iacute;</trd>
</drv>

<drv mrk="post.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc mrk="post.0e.tempe">
    <dif>
      Pli malfrue:
      <ekz>
        li pali&gcirc;is de timo kaj <tld/>e li
        ru&gcirc;i&gcirc;is de honto
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        anta&ubreve;e intencu kaj <tld/>e komencu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve;e kion vi devas, <tld/>e kion vi volas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        le&gcirc;o valoras por <tld/>e sed ne por anta&ubreve;e
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pas">pasinta&jcirc;o</ref>.
    </refgrp>
    <trd lng="ar">&a_th;&a_damma;&a_m;&a_fatha;&a_shadda;</trd>
    <trd lng="de">nach</trd>
    <trdgrp lng="es">
      <trd>despu&eacute;s</trd>,
      <trd>m&aacute;s tarde</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ensuite</trd>,
      <trd>puis</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ut&aacute;na</trd>,
      <trd>k&eacute;s&odblac;bb</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dalej</trd>,
      <trd>potem</trd>,
    </trdgrp>
    <trd lng="ro">mai t&acirc;rziu</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_t;&c_o;&c_m;</trd>,
      <trd>&c_p;&c_o;&c_z;&c_zh;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="post.0e.loke">
    <dif>
      Pli malproksime,
      <ref tip="sin" cel="antaux.mal0e">malanta&ubreve;e</ref>:
      <ekz>
        anta&ubreve;e iris kantistoj, <tld/>e kordinstrumentistoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 68:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> la trumpetistoj rajdis dekkelko da asiriaj kavaliroj
        <klr>[&#x2026;]</klr>
        <tld/>e iris la piedistaro en kaskoj kaj en manteloj longaj &gcirc;is
        la tero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">hinter</trd>
    <trdgrp lng="es">
      <trd>luego</trd>,
      <trd>a continuaci&oacute;n</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_a;&c_d;&c_i;</trd>
  </snc>
  <trd lng="bg">&c_s;&c_l;&c_je;&c_d;</trd>
  <trd lng="cs">potom</trd>
  <trd lng="en">after</trd>
  <trd lng="hu">m&ouml;g&ouml;tte</trd>
  <trdgrp lng="nl">
    <trd>na</trd>,
    <trd>achter</trd>
  </trdgrp>
  <trd lng="ny">patapita</trd>
  <trdgrp lng="pl">
    <trd>dalej</trd>,
    <trd>potem</trd>,
  </trdgrp>
  <trd lng="ro">mai departe</trd>
  <trdgrp lng="sk">
    <trd>nato</trd>,
    <trd>potom</trd>,
    <trd>za t&yacute;m</trd>
  </trdgrp>
</drv>

<drv mrk="post.0en">
  <kap><tld/>en</kap>
  <snc>
    <dif>
      Celante plue al pli <tld/>a loko,
      <ref tip="sin" cel="antaux.mal0en">malanta&ubreve;en</ref>:
      <ekz>
        la princo rimarkis, ke la polva nebulo fleksi&gcirc;is
        <tld/>en
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la barbaroj e&ccirc; ne rigardis <tld/>en
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la soldato rigardis <tld/>en kaj ekvidis urson
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidi <tld/>en kaj anta&ubreve;en
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &jcirc;etis siajn brakojn <tld/>en kaj anta&ubreve;en
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> sin klinis <tld/>en
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj densaj nigraj ondaj haroj kun intenca nezorgeco estis
        for&jcirc;etitaj <tld/>en 
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dozadu</trd>,
    <trd>d&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>nach hinten</trd>,
    <trd>zur&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">behind</trd>
  <trdgrp lng="es">
    <trd>por detr&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&ouml;g&eacute;je</trd>,
    <trd>h&aacute;tulra</trd>
  </trdgrp>
  <trd lng="nl">achterwaarts</trd>
  <trd lng="pl">dalej</trd>
  <trd lng="ro">mai departe</trd>
  <trd lng="ru">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>dozadu</trd>,
    <trd>&dcaron;alej</trd>
  </trdgrp>
</drv>

<drv mrk="post.0o">
