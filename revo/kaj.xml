  </trdgrp>
  <trdgrp lng="fi">
      <trd>ja-operaattori</trd>,
      <trd>(looginen) <ind>konjunktio</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
      <trd>&Eacute;S m&udblac;veletjel</trd>,
      <trd>&Eacute;S oper&aacute;tor</trd>,
      <trd>&Eacute;S m&udblac;velet</trd>
      <trd>konjunkci&oacute;</trd>
  </trdgrp>
  <trd lng="id">operator AND</trd>
  <trdgrp lng="ja">
    <trd>&#23736;&#22721;  <pr>&#12364;&#12435;&#12410;&#12365;</pr></trd>,
    <trd>&#27874;&#27490;&#22580;  <pr>&#12399;&#12392;&#12400;</pr></trd>,
    <trd>&#27827;&#23736;  <pr>&#12363;&#12375;</pr></trd>,
    <trd>&#33655;&#25562;&#12370;&#22580;  <pr>&#12395;&#12354;&#12370;&#12400;</pr></trd>,
