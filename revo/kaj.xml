  </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_o;&c_p;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja; <ind>&c_I;</ind></trd>,
      <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_malmol;&c_ju;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
