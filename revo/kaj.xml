<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kaj</rad>
</kap>
<drv mrk="kaj.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      <ref tip="super" cel="konjun.0o.GRA">Konjunkcio</ref>,
      liganta du frazojn, propoziciojn a&ubreve; vortojn havantajn saman
      gramatikan rolon, kaj signifanta &leftquot;plie, aldone&rightquot;.
    </dif>
  </snc>                  
  <trd lng="be">&c_ib;</trd>
  <trd lng="br">ha<klr tip="ind"> (stl. kenur.)</klr></trd>
  <trd lng="cs">a</trd>
  <trd lng="de">und</trd>
  <trd lng="en">and</trd>
  <trd lng="es">y</trd>
  <trd lng="fr">et</trd>
  <trdgrp lng="he">
    <trd>&vav;-</trd>,
    <trd>&vav;&gimel;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;s</trd>,
    <trd>s</trd>,
    <trd>meg <klr tip="ind"> (k&ouml;t&odblac;sz&oacute;)</klr></trd>
  </trdgrp>
  <trd lng="nl">en</trd>
  <trd lng="pl">i</trd>
  <trd lng="ru">&c_i;</trd>
  <trdgrp lng="sk">
    <trd>a</trd>,
    <trd>aj</trd>
  </trdgrp>
  <trd lng="sv">och</trd>
  <trd lng="vo">e</trd>
</drv>
</art>
