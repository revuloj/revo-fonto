<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uragan.xml,v 1.34 2023/10/21 16:50:28 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>uragan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="uragan.0o">
  <kap><tld/>o</kap>
  <snc mrk="uragan.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Fortega detruema <ref tip="super" cel="vent.0ego.MET">ventego</ref>:
      <ekz>
        unu vesperon fari&gcirc;is granda <tld/>o: fulmis kaj tondris, forte pluvegis, estis
        terure
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sciis, ke balda&ubreve; levi&gcirc;os terura <tld/>o kun alfluego, kiu trafos neatendite
        &ccirc;iujn tiujn kompatindajn homojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
	 la timo atakos vin kiel <tld/>o
	 <fnt><bib>MT</bib>, <lok>&Sen; 1:27</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o dis&scirc;iradis la &gcirc;ardenon
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ciklon.0o">ciklono</ref>,
       <ref cel="sxtorm.0o">&scirc;tormo</ref>,
       <ref cel="tempes.0o">tempesto</ref>,
       <ref cel="tornad.0o">tornado</ref>
   </refgrp>
  </snc>
  <snc mrk="uragan.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io fortega, konfuza a&ubreve; bruega kiel <tld/>o:
      <ekz>
        ekbruis sonoj de trombonoj <klr>[&#x2026;]</klr> <tld/>o de plenaj, majestaj tonoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de terura premego bolis en mi
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[malgra&ubreve;]</klr> la <klr>[&#x2026;]</klr> <tld/>o de via pasio vi devas observi
        certan moderecon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;</trd>
  <trd lng="ca">hurac&agrave;</trd>
  <trdgrp lng="cs">
    <trd>ork&aacute;n</trd>,
    <trd>urag&aacute;n</trd>,
    <trd>vich&rcaron;ice</trd>
  </trdgrp>
  <trd lng="de">Orkan</trd>
  <trd lng="en">hurricane</trd>
  <trd lng="es">hurac&aacute;n</trd>
  <trd lng="fi">hirmumyrsky</trd>
  <trd lng="fr">ouragan</trd>
  <trd lng="hu">ork&aacute;n</trd>
  <trd lng="it">uragano</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x66B4;&#x98A8;  <pr>&#x3060;&#x3044;&#x307C;&#x3046;&#x3075;&#x3046;</pr></trd>,
    <trd>&#x98B6;&#x98A8;  <pr>&#x3050;&#x3075;&#x3046;</pr></trd>,
    <trd/>,
    <trd>&#x3042;&#x3089;&#x3057;</trd>
  </trdgrp>
  <trd lng="nl">orkaan</trd>
  <trdgrp lng="pl">
     <trd>huragan</trd>,
     <trd>orkan</trd>
  </trdgrp>
  <trd lng="pt">furac&atilde;o</trd>
  <trdgrp lng="ru">
     <trd>&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;</trd>,
     <trd>&c_b;&c_u;&c_r;&c_ja;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>urag&aacute;n</trd>,
    <trd>v&iacute;chor</trd>
  </trdgrp>
  <trd lng="sv">orkan</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&c_a;&c_g;&c_a;&#x43D;</trd>,
    <trd>&c_b;&c_u;&c_r;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="uragan.0a">
   <kap><tld/>a</kap>
  <snc mrk="uragan.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        atakis nin <klr>[&#x2026;]</klr> <tld/>a vento, kiu estas nomata E&ubreve;rakilo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fulmotondro i&gcirc;is <tld/>a &scirc;tormo
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="uragan.0a.KOMUNE">
    <dif>
      Simila al <tld/>o:
      <ekz>
	 li havas tiel <tld/>an &hcirc;arakteron
	 <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        kun la morto <klr>[lia]</klr> mallonga kaj <tld/>a regado fini&gcirc;is tiel malfeli&ccirc;e
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>a juneco, li rifu&gcirc;is en mona&hcirc;ejon
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        epoko tre stre&ccirc;ita, preska&ubreve; <tld/>a
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="impet.0ega">impetega</ref>,
       <ref cel="sxtorm.0a">&scirc;torma</ref>,
       <ref cel="torent.0a">torenta</ref>
    </refgrp>
    <ref tip="vid" cel="verv.0a">verva</ref>
    <trdgrp lng="de">
      <trd>st&uuml;rmisch</trd>,
      <trd>furios</trd>
    </trdgrp>
  </snc>
   <trd lng="be">&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;&c_n;&c_y;</trd>
   <trd lng="ca">huracanat</trd>
   <trd lng="de">orkanartig</trd>
   <trd lng="es">huracanado</trd>
   <trd lng="hu">ork&aacute;nszer&udblac;</trd>
   <trdgrp lng="ja">
    <trd>&#x66B4;&#x98A8;&#x306E;  <pr>&#x307C;&#x3046;&#x3075;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x5D50;&#x306E;&#x3088;&#x3046;&#x306A;  <pr>&#x3042;&#x3089;&#x3057;&#x306E;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x6FC0;&#x3057;&#x3044;  <pr>&#x306F;&#x3052;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">orkaanachtig</trd>
   <trd lng="pl">huraganowy</trd>
   <trd lng="ru">&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&c_u;&c_r;&c_a;&c_g;&c_a;&#x43D;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="uragan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="uragan.0i.KOMUNE">
    <dif>
      <ref cel="furioz.0i">Furiozi</ref> kiel <tld/>o:
      <ekz>
        &ccirc;u hajlos, &ccirc;u ne&gcirc;os, &ccirc;u <tld/>os, mi nepre estos tie
        <fnt>
          <bib>KKl</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_ja;&c_n;&c_ib;&c_c;&c_mol; <klr>(&c_n;&c_ib;&c_b;&c_y; &c_w;&c_r;&c_a;&c_g;&c_a;&c_n;)</klr></trd>,
    <trd>&c_sh;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
     <trd>a&iuml;rar-se</trd>,
     <trd>enfellonir-se</trd>
   </trdgrp>
  <trd lng="cs">zu&rcaron;it jako urag&aacute;n</trd>
  <trdgrp lng="de">
    <trd>toben</trd>,
    <trd>st&uuml;rmen</trd>
  </trdgrp>
  <trdgrp lng="fi">
     <trd>riehua</trd>,
     <trd>myrskyt&auml;</trd>,
     <trd>raivota</trd>
   </trdgrp>
  <trd lng="hu">tombol</trd>
  <trd lng="ja">&#x8352;&#x308C;&#x72C2;&#x3046;  <pr>&#x3042;&#x308C;&#x304F;&#x308B;&#x3046;</pr></trd>
  <trd lng="nl">als een <ind>orkaan</ind> te keer gaan</trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">z&uacute;ri&tcaron; ako urag&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&#x43B;&c_je;&#x43D;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
