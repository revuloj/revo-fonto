<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cizel</rad>/i
</kap>
<drv mrk="cizel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cizel.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Delikate prilabori, skulpti metalon per marteleto, <tld/>ilo
      kaj <ref cel="cxiz.0ileto">&ccirc;izileto</ref>:
      <ekz>
        &ccirc;izante oni forigas pecetojn de metalo, <tld/>ante
        oni premfrapas la metalan supra&jcirc;on.
      </ekz>
    </dif>
    <refgrp tip="vid">
     <ref cel="cxiz.0i">&ccirc;izi</ref>,
     <ref cel="gravur.0i">gravuri</ref>.
    </refgrp>
    <trd lng="be">&c_ch;&c_a;&c_k;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>repousser<klr tip="ind"> (cuir, m&eacute;tal)</klr></trd>,
      <trd>ciseler</trd>
    </trdgrp>
    <trd lng="hu">cizell&aacute;l</trd>
    <trd lng="de">ziselieren</trd>
    <trd lng="pt">cinzelar</trd>
    <trd lng="ru">&c_ch;&c_je;&c_k;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="cizel.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Zorgege prilabori, ekz-e literaturan verkon:
      <ekz>
        <tld/>i versojn;
      </ekz>
      <ekz>
        artisme <tld/>itaj esprimoj.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_sh;&c_t;&c_a;&c_l;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="fr">ciseler<klr tip="ind"> (fig.)</klr></trd>
    <trd lng="hu">csiszol<klr tip="amb"> (pl. st&iacute;lust)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_t;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="cizel.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala stangeto, kies finon oni uzas por enpremi
      la surfacon de la prilaborata objekto.
    </dif>
  </snc>
  <trd lng="fr">repoussoir<klr tip="ind"> (cuir, m&eacute;tal)</klr></trd>
  <trd lng="pt">cinzel</trd>
  <trd lng="ru">&c_ch;&c_je;&c_k;&c_a;&c_n;<klr tip="amb"> (&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>
</art>
