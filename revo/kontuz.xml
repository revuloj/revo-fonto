<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontuz.xml,v 1.19 2022/06/03 16:43:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kontuz</rad>/i
</kap>
<drv mrk="kontuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontuz.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="difekt.0i">Difekti</ref> pli a&ubreve; malpli profunde organikan teksa&jcirc;on,
      ne &scirc;irvundante la ha&ubreve;ton:
      <ekz>
        tiel mi boksas,<klr>[&#8230;]</klr> sed mi <tld/>as mian korpon
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 9:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nutristino lin <tld/>is
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i portis &ccirc;iam mallongajn manikojn kaj havis &ccirc;iam <tld/>itajn kubutojn
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Beatrico <klr>[povis]</klr> ver&scirc;i iom da konsoliga balzamo en lian <tld/>itan
        koron
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por li la vivo estas &scirc;ar&gcirc;o, <klr>[&#8230;]</klr> kiu lin <tld/>as
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lez.0i.MED">lezi<sncref/></ref>
  </snc>
  <snc mrk="kontuz.0i.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="vund.0i.MED">Vundi</ref>:
      <ekz>
        <tld/>ita genuo sangis kaj ru&gcirc;i&gcirc;is
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_sh;&c_y;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>contusionar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>od&rcaron;&iacute;t</trd>,
    <trd>pohmo&zcaron;dit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>quetschen</trd>,
     <trd>prellen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bruise</trd>,
    <trd>contuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contusionar</trd>
  </trdgrp>
  <trd lng="fr">contusionner</trd>
  <trdgrp lng="hu">
     <trd>&ouml;sszez&uacute;z</trd>,
     <trd>z&uacute;z&oacute;d&aacute;st okoz</trd>
  </trdgrp>
  <trd lng="nl">kneuzen</trd>
  <trd lng="pl">kontuzjowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>contudir</trd>,
    <trd>machucar <klr tip="amb">(contundir)</klr></trd>,    
    <trd>pisar <klr tip="amb">(contundir)</klr></trd>,    
    <trd>magoar <klr tip="amb">(contundir)</klr></trd>,
    <trd>fazer <ind>contus&atilde;o</ind> em</trd>
  </trdgrp>
  <trd lng="ro">contuziona</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_sh;&c_i;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odrie&tcaron;</trd>,
    <trd>pomlia&zcaron;di&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kontuz.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Korpa <ref tip="super" cel="difekt.0o">difekto</ref> sekvanta el kontuzado:
      <ekz>
        tio e&ccirc; ne estas <ref cel="vund.0o.MED">vundo</ref>, nur <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vundetoj de la viza&gcirc;o kaj la <tld/>oj de la dorso &scirc;ajnas negravaj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maristoj <klr>[&#8230;]</klr> pligrandigis la doloron de <tld/>oj per la premado de
        siaj tuberoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ehximoz.0o">e&hcirc;imozo</ref>,
      <ref cel="sang.0umo">sangumo</ref>,
      <ref cel="sxvel.0ajxo">&scirc;vela&jcirc;o</ref>      
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_sh;&c_y;&c_b;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>contusi&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kontuze</trd>,
    <trd>mod&rcaron;ina</trd>,
    <trd>podlitina</trd>,
    <trd>pohmo&zcaron;d&ecaron;nina</trd>,
    <trd>zhmo&zcaron;d&ecaron;nina</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Quetschung</trd>,
     <trd>Prellung</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contusi&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">contusion</trd>
  <trd lng="hu">z&uacute;z&oacute;d&aacute;s</trd>
  <trd lng="nl">kneuzing</trd>
  <trd lng="pl">kontuzja</trd>
  <trd lng="pt">contus&atilde;o</trd>
  <trdgrp lng="ro">
    <trd>contuzie</trd>,
    <trd>jignire</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_sh;&c_i;&c_b;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_u;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>modrina</trd>,
    <trd>podliatina</trd>,
    <trd>pomlia&zcaron;denie</trd>
  </trdgrp>
</drv>
</art>
