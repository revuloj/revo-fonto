<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontuz.xml,v 1.17 2018/08/20 23:10:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kontuz</rad>/i
</kap>
<drv mrk="kontuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Difekti pli a&ubreve; malpli profunde organikan teksa&jcirc;on,
      ne &scirc;irvundante la ha&ubreve;ton:
      <ekz>
        <tld/>itaj kubutoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la bastono <tld/>is al li la dorson;
      </ekz>
      <ekz>
        <tld/>a (netran&ccirc;a) instrumento;
      </ekz>
      <ekz>
        fari al iu <tld/>ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia sento pri justo estis profunde <tld/>ita.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>contusionar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>od&rcaron;&iacute;t</trd>,
    <trd>pohmo&zcaron;dit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>quetschen</trd>,
     <trd>prellen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bruise</trd>,
    <trd>contuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contusionar</trd>
  </trdgrp>
  <trd lng="fr">contusionner</trd>
  <trdgrp lng="hu">
     <trd>&ouml;sszez&uacute;z</trd>,
     <trd>z&uacute;z&oacute;d&aacute;st okoz</trd>
  </trdgrp>
  <trd lng="nl">kneuzen</trd>
  <trd lng="pl">kontuzjowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>contudir</trd>,
    <trd>machucar <klr tip="amb">(contundir)</klr></trd>,    
    <trd>pisar <klr tip="amb">(contundir)</klr></trd>,    
    <trd>magoar <klr tip="amb">(contundir)</klr></trd>,
    <trd>fazer <ind>contus&atilde;o</ind> em</trd>
  </trdgrp>
  <trd lng="ro">contuziona</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_sh;&c_i;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odrie&tcaron;</trd>,
    <trd>pomlia&zcaron;di&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kontuz.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Korpa difekto sekvanta el kontuzado.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>contusi&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kontuze</trd>,
    <trd>mod&rcaron;ina</trd>,
    <trd>podlitina</trd>,
    <trd>pohmo&zcaron;d&ecaron;nina</trd>,
    <trd>zhmo&zcaron;d&ecaron;nina</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Quetschung</trd>,
     <trd>Prellung</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contusi&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">contusion</trd>
  <trd lng="hu">z&uacute;z&oacute;d&aacute;s</trd>
  <trd lng="nl">kneuzing</trd>
  <trd lng="pl">kontuzja</trd>
  <trd lng="pt">contus&atilde;o</trd>
  <trdgrp lng="ro">
    <trd>contuzie</trd>,
    <trd>jignire</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_sh;&c_i;&c_b;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_u;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>modrina</trd>,
    <trd>podliatina</trd>,
    <trd>pomlia&zcaron;denie</trd>
  </trdgrp>
</drv>
</art>
