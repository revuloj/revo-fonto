<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hipokr.xml,v 1.30 2023/11/15 17:08:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hipokrit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hipokr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="hipokr.0i.absolute">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      &Scirc;ajnigi virton a&ubreve; piecon, esti nesincera:
      <ekz>
        per sia lango ili <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        profeto kaj pastro <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 23:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is dum sia tuto vivo.
      </ekz>
    </dif>
    <trd lng="de">scheinheilig sein</trd>
    <trd lng="hu">&aacute;lszenteskedik</trd>
    <trd lng="nl">schijnheilig zijn</trd>
  </snc>
  <snc mrk="hipokr.0i.ion">
    <dif>
      <ref tip="super" cel="sxajn.0igi">&Scirc;ajnigi</ref>
      ne posedatan virton a&ubreve; meriton:
      <ekz>
        <tld/>i amikecon, honestecon;
      </ekz>
      <ekz>
        promesojn piajn ili <tld/>as
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>u, ke deliro mia / parolas nun kaj ne la krimo via
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt.0i">afekti</ref>,
      <ref cel="komedi.0i">komedii</ref>,
      <ref cel="kis.0o">kiso</ref>.
    </refgrp>
    <trd lng="de">heucheln</trd>
    <trd lng="hu">sz&iacute;nlel</trd>
    <trd lng="nl">huichelen</trd>
    <trdgrp lng="zh">
    <trd>&#x4F6F; <pr>y&#xE1;ng</pr></trd>,
    <trd>&#x4E0D;&#x8001;&#x5B9E; <pr>b&#xF9;l&#x1CE;osh&#xED;</pr></trd>,
    <trd>&#x4E24;&#x9762; <pr>li&#x1CE;ngmi&#xE0;n</pr></trd>,
    <trd>&#x5FC3;&#x53E3;&#x4E0D;&#x4E00; <pr>x&#x12B;nk&#x1D2;ub&#xF9;y&#x12B;</pr></trd>,
    <trd>&#x88C5;&#x5047; <pr>zhu&#x101;ngji&#x1CE;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">beza&ntilde; <ind>pilpous</ind></trd>
  <trdgrp lng="cs">
    <trd>pokrytecky jednat</trd>,
    <trd>p&rcaron;edst&iacute;rat</trd>,
    <trd>p&rcaron;etva&rcaron;ovat se</trd>,
    <trd>simulovat</trd>
  </trdgrp>
  <trd lng="en">dissemble</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>hypocrite</ind></trd>,
    <trd>feindre</trd>
  </trdgrp>
  <trd lng="it">fingere</trd>
  <trdgrp lng="ja">
    <trd>&#x898B;&#x305B;&#x304B;&#x3051;&#x308B;  <pr>&#x307F;&#x305B;&#x304B;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x507D;&#x5584;&#x3092;&#x3059;&#x308B;  <pr>&#x304E;&#x305C;&#x3093;&#x3092;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x732B;&#x3092;&#x304B;&#x3076;&#x308B;  <pr>&#x306D;&#x3053;&#x3092;&#x304B;&#x3076;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>agir com <ind>hipocrisia</ind></trd>, 
   <trd>fingir</trd> 
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">pretvarova&tcaron; sa</trd>
  <trd lng="uk">&c_l;&c_i;&c_c;&c_je;&c_m;&c_ib;&c_r;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="hipokr.0a">
  <kap><tld/>a</kap>
  <snc mrk="hipokr.0a.nesincera">
    <dif>
      <tld lit="H"/>anta:
      <ekz>
        <tld/>a konduto, rigardo, respondo;
      </ekz>
      <ekz>
        malveron ili parolas unuj al aliaj, vortojn flatajn el koro <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 12:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne plu alportu <tld/>ajn donacojn, la incensado estas abomeninda&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistis plu neniu, pri kiu li povus svati&gcirc;i, kaj tial li kun <tld/>a malestimo
        &jcirc;uris, ke li ne volas edzi&gcirc;i
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF42;&#xF61;&#xF7C;&#xF0B;&#xF66;&#xF92;&#xFB1;&#xF74;&#xF0B;&#xF5A;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="br">pilpous</trd>
  <trdgrp lng="cs">
    <trd>obojetn&yacute;</trd>,
    <trd>pokryteck&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>heuchlerisch</trd>,
    <trd>scheinheilig</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&omikron;&kappa;&rho;&jota;&tau;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">hypocritical</trd>
  <trd lng="fr">hypocrite<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;nlel&odblac;</trd>,
    <trd>&aacute;lszent</trd>,
    <trd>k&eacute;pmutat&oacute;</trd>
  </trdgrp>
  <trd lng="it">ipocrita<klr> (agg.)</klr></trd>
  <trd lng="ja">&#x507D;&#x5584;&#x306E;  <pr>&#x304E;&#x305C;&#x3093;&#x306E;</pr></trd>
  <trd lng="nl">schijnheilig</trd>
  <trd lng="pt">hip&oacute;crita<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">pokryteck&yacute;</trd>
  <trd lng="uk">&c_l;&c_i;&c_c;&c_je;&c_m;&c_ib;&c_r;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x865A;&#x4F2A; <pr>x&#x16B;w&#x11B;i</pr></trd>,
    <trd>&#x5047;&#x60FA;&#x60FA; <pr>ji&#x1CE;x&#x12B;ngx&#x12B;ng</pr></trd>,
    <trd>&#x4F2A;&#x5584; <pr>w&#x11B;ish&#xE0;n</pr></trd>,
    <trd>&#x5047;&#x4EC1;&#x5047;&#x4E49; <pr>ji&#x1CE;r&#xE9;nji&#x1CE;y&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hipokr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="hipokr.0ajxo.ago">
    <dif>
      Ago nesincera a&ubreve; &scirc;ajniganta neposedatan virton:
      <ekz>
        ili volus iri en pre&gcirc;ejon, sed ili ne povas tion &ccirc;i fari, &ccirc;ar &gcirc;i estus <tld/>a&jcirc;o
        de ilia flanko
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Heuchelei</trd>
  </snc>
  <trd lng="en">hypocrisy</trd>
  <trd lng="it">ipocrisia<klr> (atto ipocrita)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x5047;&#x610F; <pr>ji&#x1CE;y&#xEC;</pr></trd>,
    <trd>&#x53E3;&#x662F;&#x5FC3;&#x975E; <pr>k&#x1D2;ush&#xEC;x&#x12B;nf&#x113;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hipokr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hipokr.0o.kapablo">
    <dif>
      Kapablo &scirc;ajnigi ne posedatan virton a&ubreve; meriton:
      <ekz>
        amo sen<tld/>a
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 6:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o! <klr>[&#x2026;]</klr> &#x2010; vi pre&gcirc;as al la dioj, je kiuj vi ne kredas
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="de">Scheinheiligkeit</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hypokrize</trd>,
    <trd>pokrytectv&iacute;</trd>,
    <trd>p&rcaron;etva&rcaron;ov&aacute;n&iacute;</trd>,
    <trd>p&rcaron;etv&aacute;&rcaron;ka</trd>
  </trdgrp>
  <trd lng="en">hypocrisy</trd>
  <trd lng="it">ipocrisia</trd>
  <trd lng="ja">&#x507D;&#x5584;&#x7684;&#x884C;&#x70BA;  <pr>&#x304E;&#x305C;&#x3093;&#x3066;&#x304D;&#x3053;&#x3046;&#x3044;</pr></trd>
  <trd lng="sk">pretvarovanie</trd>
  <trd lng="uk">&c_l;&c_i;&c_c;&c_je;&c_m;&c_ib;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="hipokr.0e">
  <kap><tld/>e</kap>
  <snc mrk="hipokr.0e.nesincere">
    <dif>
      Nesincere, &scirc;ajnigante virtecon:
      <ekz>
        ne lo&gcirc;os interne de mia domo iu, kiu agas <tld/>e, kiu diras malveron
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 101:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ridetantaj lipoj kaj <tld/>e mallevitaj okuloj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">hypocritically</trd>
  <trd lng="it">ipocritamente</trd>
</drv>

<drv mrk="hipokr.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc mrk="hipokr.0eco.eco">
    <dif>
      Eco esti <tld/>a:
      <ekz>
        vi ekstere &scirc;ajnas justaj anta&ubreve; homoj, sed interne vi estas plenaj de <tld/>eco
        kaj maljusteco
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 23:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub influo de la konstantaj intrigoj a&ubreve; suspektoj pri intrigoj, komencis naski&gcirc;i
        instinkto de la <tld/>eco
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        via amo estu sen <tld/>eco
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        cinikismo estas ofte <tld/>eco.   
      </ekz>
    </dif>
    <trd lng="de">Scheinheiligkeit</trd>
  </snc>
  <trd lng="br">pilpouzerezh</trd>
  <trd lng="en">hypocrisy</trd>
  <trd lng="fr">hypocrisie</trd>
  <trd lng="it">ipocrisia</trd>
  <trd lng="ja">&#x507D;&#x5584;  <pr>&#x304E;&#x305C;&#x3093;</pr></trd>
  <trd lng="uk">&c_l;&c_i;&c_c;&c_je;&c_m;&c_ib;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="zh">&#x4F2A;&#x5584; <pr>w&#x11B;ish&#xE0;n</pr></trd>
</drv>

<drv mrk="hipokr.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="hipokr.0ismo.agmaniero">
    <dif>
      Konvinko kaj agmaniero kiu celas atingi rezultojn pere de
      &scirc;ajnigo. 
    </dif>
  </snc>
  <trd lng="en">hypocrisy</trd>
  <trd lng="it">gattopardismo</trd>
</drv>
  
<drv mrk="hipokr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="hipokr.0ulo.iu">
    <dif>
      Iu <tld/>a:
      <ekz>
        la senkulpulo eksciti&gcirc;os kontra&ubreve; la <tld/>ulo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 17:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi donos almozon, ne sonigu trumpeton anta&ubreve; vi, kiel faras la <tld/>uloj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 6:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ulo, eligu unue la trabon el via okulo, kaj tiam vi klare vidos, por
        eltiri la lignereton el la okulo de via frato
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komedi">komedianto</ref>,
      <ref cel="farise.0o">fariseo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">pilpouz<klr tip="ind"> (den)</klr></trd>
  <trdgrp lng="cs">
    <trd>hypokrit</trd>,
    <trd>hypokrita</trd>,
    <trd>pokrytec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heuchler</trd>,
    <trd>Scheinheiliger</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&omikron;&kappa;&rho;&jota;&tau;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">hypocrite</trd>
  <trd lng="fr">hypocrite<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>hipokrita</trd>,
    <trd>&aacute;lszent</trd>,
    <trd>k&eacute;pmutat&oacute;</trd>
  </trdgrp>
  <trd lng="it">ipocrita<klr> (sost.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x507D;&#x5584;&#x8005;  <pr>&#x304E;&#x305C;&#x3093;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x732B;&#x304B;&#x3076;&#x308A;  <pr>&#x306D;&#x3053;&#x304B;&#x3076;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huichelaar</trd>,
    <trd>schijnheilige</trd>
  </trdgrp>
  <trd lng="pt">hip&oacute;crita<klr tip="amb"> (pessoa)</klr></trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;</trd>
  <trd lng="sk">pokrytec</trd>
  <trd lng="uk">&c_l;&c_i;&c_c;&c_je;&c_m;&c_ib;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F2A;&#x541B;&#x5B50; <pr>w&#x11B;ij&#x16B;nz&#x1D0;</pr></trd>,
    <trd>&#x5047;&#x6B63;&#x7D93; <pr>ji&#x1CE;zh&#xE8;ngj&#x12B;ng</pr></trd>,
    <trd>&#x4F2A;&#x5584;&#x8005; <pr>w&#xE8;ish&#xE0;nzh&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hipokr.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc mrk="hipokr.mal0eco.PSI">
    <dif>
      <ref tip="dif" cel="sincer.0o.PSI">Sincero</ref> ne
      ka&scirc;anta sian malvirtecon:
      <ekz>
        vagabondo <klr>[&#x2026;]</klr> simple preferas 
        la &ccirc;ifonojn de spiritlibereco kaj mal<tld/>eco
        <fnt>
          <aut>C. Oxenford</aut>,
          <vrk><url ref="http://arkivo.esperanto-france.org/divers/oxenford/vagabond.htm">
          La Historio de la Esperantista Vagabonda Klubo</url></vrk>,
          <lok>1912</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Aufrichtigkeit</trd>
    <trdgrp lng="zh">
    <trd>&#x771F;&#x631A; <pr>zh&#x113;nzh&#xEC;</pr></trd>,
    <trd>&#x771F;&#x8AA0; <pr>zh&#x113;nch&#xE9;ng</pr></trd>,
    <trd>&#x8AF4; <pr>xi&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="fr">rectitude<klr> (morale)</klr></trd>
</drv>
</art>
