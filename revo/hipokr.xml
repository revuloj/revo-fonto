<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hipokrit</rad>/i
</kap>
<drv mrk="hipokr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      &Scirc;ajnigi ne posedatan virton a&ubreve; meriton:
      <ekz><tld/>i amikecon, honestecon;</ekz>
      <ekz>promesojn belajn ili <tld/>as<fnt>Z</fnt>.</ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="afekt.0i">afekti</ref>,
      <ref cel="komedi.0i">komedii</ref>,
      <ref cel="kis.0o">kiso</ref>.
    </refgrp>
    <trd lng="de">heucheln</trd>
    <trd lng="nl">huichelen</trd>
</snc>
  <snc num="2">
    <uzo tip="klr">(abs)</uzo>
    <dif>
      &Scirc;ajnigi virton a&ubreve; piecon, esti nesincera:
      <ekz>li <tld/>is dum sia tuto vivo.</ekz>
    </dif>
    <trd lng="de">scheinheilig sein</trd>
    <trd lng="nl">schijnheilig zijn</trd>
  </snc>
  <trd lng="en">dissemble</trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="hipokr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="H"/>anta:
      <ekz><tld/>a konduto, rigardo, respondo;</ekz>
      <ekz>cinikismo estas ofte <tld/>eco.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>heuchlerisch</trd>,
     <trd>scheinheilig</trd>
  </trdgrp>
  <trd lng="nl">schijnheilig</trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;&c_i;&c_je;</trd>
</drv>
<drv mrk="hipokr.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      <tld lit="H"/>anto 
    </dif>
    <refgrp tip="sin">
      <ref cel="komedi">komedianto</ref>,
      <ref cel="farise.0o">fariseo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Heuchler</trd>,
     <trd>Scheinheiliger</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huichelaar</trd>,
    <trd>schijnheilige</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;</trd>
</drv>
</art>
