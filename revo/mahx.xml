<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mahx.xml,v 1.12 2022/03/23 14:01:05 revo Exp $">
    <kap><rad>Ma&hcirc;</rad>/o <fnt><bib>PIV2</bib></fnt></kap>

    <drv mrk="mahx.M0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">PERS</uzo>
	<dif>
          Germanlingve <trd lng="de">Ernst <ind>Mach</ind></trd>,
          a&ubreve;stra
          fizikisto kaj filozofo (1838&ndash;1916).
	</dif>
      </snc>
      <trd lng="bg">&c_M;&c_a;&c_h;</trd>
      <trd lng="ca">Mach</trd>
      <trd lng="fa">&a_A;&a_r;&a_n;&a_s;&a_t; <ind>&a_m;&a_A;&a_kh;</ind></trd>
      <trd lng="fr">Ernest <ind>Mach</ind></trd>
      <trd lng="it">Mach</trd>
      <trd lng="nl">Ernst <ind>Mach</ind></trd>
      <trd lng="pl">Mach</trd>
      <trd lng="pt">Mach</trd>
      <trd lng="ru">&c_M;&c_a;&c_h;</trd>
    </drv>

    <drv mrk="mahx.m0o">
      <kap><tld lit="m"/>o</kap>
      <snc>
      <uzo tip="fak">FIZ</uzo>
	<dif>
          Grava karakterizo de gasfluo a&ubreve; de movi&gcirc;o en gasa medio, la
          nombro egala al kvociento de ties rapido <frm am="v">v</frm>
          per la
          sonrapido <frm am="a">a</frm> en la sama medio: <frm am="M=va">M = v/a</frm>:
          <ekz>
            flugi je 2 <tld lit="m"/>oj.
          </ekz>
	</dif>
      </snc>
      <trdgrp lng="bg">
        <trd>&c_ch;&c_i;&c_s;&c_l;&c_o; &c_n;&c_a; &c_M;&c_a;&c_h;</trd>,
        <trd>&c_m;&c_a;&c_h;</trd>
      </trdgrp>
      <trd lng="ca">nombre de Mach</trd>
      <trdgrp lng="de">
        <trd>Mach</trd>,
        <trd>Mach-Zahl</trd>
      </trdgrp>
      <trd lng="en">Mach number</trd>
      <trdgrp lng="fa">
        <trd>&a_ayn;&a_d;&a_d; <ind>&a_m;&a_A;&a_kh;</ind></trd>,
        <trd>&a_ayn;&a_d;&a_d; &a_n;&a_s;&a_b;&f_y; <ind>&a_m;&a_A;&a_kh;</ind></trd>
      </trdgrp>
      <trd lng="fr">Mach<klr> (nombre)</klr></trd>
      <trdgrp lng="it">
        <trd>mach</trd>,
        <trd>numero di <ind>Mach</ind></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>mach</trd>,
        <trd>machgetal</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>mach</trd>,
        <trd>liczba <ind>Macha</ind></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>mach</trd>,
        <trd>n&uacute;mero de <ind>Mach</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_ch;&c_i;&c_s;&c_l;&c_o; <ind>&c_M;&c_a;&c_h;&c_a;</ind></trd>
      <trd lng="uk">&#x41C;&#x430;&#x445; (&#x444;&#x456;&#x437;&#x438;&#x43A;)</trd>
</drv>

  </art>

