<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: oktet.xml,v 1.3 2024/09/04 09:29:54 afido Exp $">
    <kap>
      <rad>oktet</rad>/o
      <fnt><bib>PIV3</bib></fnt>
    </kap>

<drv mrk="oktet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref>
      por ok vo&ccirc;oj a&ubreve; ses instrumentoj:
      <ekz>
        printempon de 1824 li verkis sian <tld/>o F-ma&jcirc;ora
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20108"
              >Franz Schubert</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="oktet.0o.opo">
    <dif>
      Oko da muzikantoj:
      <ekz>
        la Studenta <tld lit="O"/>o el <nom>Ljubljana</nom> prezentis programon
        &leftquot;Kantoj de Popoloj el amba&ubreve; Duonsferoj&rightquot;
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Oktett</trd>
<trd lng="be">&c_a;&c_k;&c_t;&c_e;&c_t;</trd>
<trd lng="ru">&c_o;&c_k;&c_t;&c_je;&c_t;</trd>
</drv>

  </art>

