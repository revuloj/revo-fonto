<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fulg</rad>/o
</kap>
<drv mrk="fulg.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nigra densa materio lasita de fumo en kamentuboj:
      <ekz><tld/>okolora.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bistr.0o">bistro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Ru&szlig;</trd>
  <trd lng="fr">suie</trd>
  <trd lng="ru">&c_s;&c_a;&c_zh;&c_a;</trd>
</drv>
<drv mrk="fulg.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Nigrigi, makuli per <tld/>o:
      <ekz><tld/>igita kameno.</ekz>
    </dif>
  </snc>
  <trd lng="de">ru&szlig;en</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
