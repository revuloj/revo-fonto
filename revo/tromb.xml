<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tromb</rad>/o
</kap>
<drv mrk="tromb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Kolonforma turni&gcirc;anta akvomaso levita el la maro fare de turnvento
      kaj rapide anta&ubreve;eniranta kun terura detrua forto.
    </dif>
    <refgrp tip="sin">
      <ref cel="ciklon.0o">ciklono</ref>,
      <ref cel="uragan.0o">uragano</ref>,
      <ref cel="vent">kirloventego</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_r;&c_ch;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_r;&c_a;&c_s;&c_l;&c_u;&c_p;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Trombe</trd>
  <trd lng="de">v&iacute;zt&ouml;lcs&eacute;r</trd>
  <trd lng="nl">waterhoos</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_je;&c_r;&c_ch;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_r;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
