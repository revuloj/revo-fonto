<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tromb</rad>/o
</kap>
<drv mrk="tromb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tromb.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Turniĝa akvokolono levita el la maro fare de turnovento
      kaj rapide antaŭeniranta kun terura detrua forto; akva <ref
      tip="vid" cel="ciklon.0o.MET">ciklono</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="vent">kirloventego</ref>,
      <ref cel="uragan.0o.MET">uragano</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>сьмерч</trd>,
    <trd>віхраслуп</trd>,
    <trd>віхура</trd>
  </trdgrp>
  <trd lng="de">Trombe</trd>
  <trdgrp lng="en">
    <trd>waterspout</trd>,
    <trd>tornado over water</trd>,
    <trd>column of water</trd>
  </trdgrp>
  <trd lng="fr">trombe</trd>
  <trd lng="hu">víztölcsér</trd>
  <trd lng="nl">waterhoos</trd>
  <trd lng="pl">trąba wodna</trd>
  <trdgrp lng="pt">
    <trd>tromba d'água</trd>,
    <trd>tromba</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>смерч</trd>,
    <trd>вихрь</trd>
  </trdgrp>
</drv>
</art>
