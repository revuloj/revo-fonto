<vortaro>
<art mrk="$Id$">
<kap>
  <rad>faktorial</rad>/o
</kap>
<drv mrk="faktor5.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 10</lok></fnt>
  <snc>
    <dif>
      <klr>(de pozitiva <ref cel="entjer.0o">entjero</ref>
      <frm><k>n</k></frm>)</klr>
      La produto <frm>1&#215;2&#215;3... &#215;<k>n</k></frm>; 
      simb. <frm><k>n</k>!</frm> <klr>(legu: no faktoriale)</klr>:
      <ekz>
        la nombro de bijekcioj en aro kun <frm><k>n</k></frm> elementoj 
        estas <tld/>o de <frm><k>n</k></frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_k;&c_t;&c_a;&c_r;&c_y;&c_ja;&c_l;</trd>
  <trd lng="de">Fakult&auml;t</trd>
  <trd lng="en">factorial</trd>
  <trd lng="es">factorial</trd>
  <trd lng="fr">factorielle</trd>
  <trd lng="hu">faktori&aacute;lis</trd>
  <trd lng="pl">silnia<klr tip="ind"> (liczby naturalnej)</klr></trd>
  <trd lng="nl">faculteit<klr tip="ind"> (wiskunde)</klr></trd>
  <trd lng="pt">fatorial</trd>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_t;&c_o;&c_r;&c_i;&c_a;&c_l;</trd>
</drv>
</art>
