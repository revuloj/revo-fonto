<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ankiloz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ankilo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Senmoveco de artiko:
      <ekz>
        suferi je <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_k;&c_ib;&c_l;&c_o;&c_z;</trd>
  <trd lng="de">Versteifung</trd>
  <trd lng="fr">ankylose</trd>
  <trdgrp lng="hu">
    <trd>iz&uuml;leti merevs&eacute;g</trd>
    <trd>ankylosis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ankylosis</trd>,
    <trd>gewrichtsverstijving</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_k;&c_i;&c_l;&c_o;&c_z;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_d;&c_v;&c_i;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_k;&c_i;&c_l;&c_o;&c_z;</trd>
  <trd lng="sk">ankyl&oacute;za</trd>
</drv>

<drv mrk="ankilo.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti trafita de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">versteifen</trd>
  <trd lng="fr">s'<ind>ankyloser</ind></trd>
  <trd lng="hu">megmerevedik<klr tip="ind"> (iz&uuml;let)</klr>></trd>
  <trd lng="nl">verstijven<klr tip="ind"> (v.gewrichten)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_je;&c_n;&c_je;&c_t;&c_mol;<klr tip="ind"> (&c_o;
    &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_h;)</klr></trd>,
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol; <ind>&c_p;&c_o;&c_d;&c_v;&c_i;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</ind><klr
    tip="amb"> (&c_o; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_h;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="ankilo.0igo">
  <kap><tld/>igo</kap>
  <snc>
    <dif>
      Operacio a&ubreve; procezo ka&ubreve;zanta <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Ruhigstellung</trd>
  <trd lng="hu">m&udblac;vi <ind>ankylosis</ind></trd>
  <trd lng="nl">veroorzaken van <ind>ankylosis</ind></trd>
  <trd lng="ru">&c_i;&c_m;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
