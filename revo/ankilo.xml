<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ankiloz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ankilo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Senmoveco de artiko:
      <ekz>
        suferi je <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ankylose</trd>
  <trd lng="ru">&c_a;&c_n;&c_k;&c_i;&c_l;&c_o;&c_z;</trd>
</drv>

<drv mrk="ankilo.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti trafita de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">s'<ind>ankyloser</ind></trd>
</drv>

<drv mrk="ankilo.0igo">
  <kap><tld/>igo</kap>
  <snc>
    <dif>
      Operacio a&ubreve; procezo ka&ubreve;zanta <tld/>on.
    </dif>
  </snc>
</drv>
</art>
