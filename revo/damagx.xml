<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dama&gcirc;</rad>/i
</kap>
<drv mrk="damagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malutili al iu, ka&ubreve;zante materian a&ubreve; moralan perdon:
      <ekz>
        la striko <tld/>is la najbarajn industriojn;
      </ekz>
      <ekz>
        kompensi la ka&ubreve;zitajn <ind><tld/>o</ind>jn;
        <trdgrp lng="fr">
          <trd>pr&eacute;judice</trd>,
          <trd>dommage<klr tip="ind"> (pr&eacute;judice)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_u;&c_shch;&c_je;&c_r;&c_b;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i ies reputacion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="domagx.0i">doma&gcirc;i 3</ref>,
      <ref cel="difekt.0i">difekti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>damage</trd>,
    <trd>harm</trd>,
    <trd>hurt</trd>,
    <trd>injure</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nuire (&agrave;)</trd>,
    <trd>porter <ind>pr&eacute;judice</ind> (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_je;&c_s;&c_t;&c_i; &c_u;&c_shch;&c_je;&c_r;&c_b;</trd>
  </trdgrp>
</drv>
</art>
