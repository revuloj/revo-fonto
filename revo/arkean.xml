<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>arkean</rad>/o
</kap>
<drv mrk="arkean.0o">
  <kap><tld/>o</kap>
  <snc mrk="arkean.0o.GEOL">
    <dif>
      La 1a (plej praa) <ref tip="super" cel="era.0o.GEOL">erao</ref> 
      de <ref tip="super"
cel="kambri.antaux0o.GEOL">anta&ubreve;kambrio</ref>,
      iam taksita kiel periodo.
      <tld lit="A"/>o da&ubreve;ris proksimume anta&ubreve; inter 3000
kaj 1500 milionoj da jaroj.
    </dif>
    <ref tip="vid" cel="proterozoik.0o.GEOL">proterozoiko</ref>
    <ref tip="vid" cel="paleozoik.0o.GEOL">paleozoiko</ref>
    <ref tip="vid" cel="mezozoik.0o.GEOL">mezozoiko</ref>
    <ref tip="vid" cel="cenozoik.0o.GEOL">cenozoiko</ref>

    <trdgrp lng="pl">
      <trd>era <ind>archaiczna</ind></trd>,
      <trd>archaik</trd>,
      <trd>era <ind>archeozoiczna</ind></trd>
    </trdgrp>

  </snc>
</drv>
</art>

