<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: robini.xml,v 1.13 2024/08/03 15:28:27 revo Exp $">
<kap>
    <rad>robini</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="robini.0o">
  <kap><tld/>o</kap>
  <snc mrk="robini.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Somerverda arbo a&ubreve; arbusto kun iom e&gcirc;aj tigoj, kontra&ubreve;sidaj
      elipsaj folioj kun apudfolioj transformitaj al dornoj, kaj kun pendantaj
      florgrapoloj; <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Robinia</trd>) kun specioj kreskantaj en
      Nordameriko sed anka&ubreve; E&ubreve;ropo:
      <ekz>
        <nom>Jean Robin</nom>, 1550&#x202F;-&#x202F;1629, estis botanikisto de la re&gcirc;o de Francujo
        Henriko la
        4-a, kaj la plej unua ekzemplero de <tld/>o plantita de li en Parizo en la
        jaro 1601 plu vivas kaj videblas en la &leftquot;Planto-&Gcirc;ardeno&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21305">Robinio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="lst" cel="plant1.floro0o" lst="voko:faboparencoj">faboparencaj floroplantoj</ref>

  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="cs">trnovn&iacute;k</trd>
  <trd lng="de">Robinie</trd>
  <trd lng="fr">robinier</trd>
  <trdgrp lng="pl">
    <trd>grochodrzew</trd>,
    <trd>robinia</trd>
  </trdgrp>
  <trd lng="ro">acacia</trd>
  <trd lng="sk">ag&aacute;t</trd>
  <trd lng="uk">&c_r;&#x43E;&c_b;&c_ib;&#x43D;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D0B;&#x69D0; <pr>y&#xE1;nghu&#xE1;i</pr></trd>,
    <trd>&#x6D0B;&#x69D0;&#26641; <pr>y&#xE1;nghu&#xE1;ish&#xF9;</pr></trd>,
    <trd>&#x6D0B;&#x69D0;&#x6A39; <pr>y&#xE1;nghu&#xE1;ish&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="robini.akacia0o">
  <kap>&scirc;ajnakacia <tld/>o</kap>
  <snc mrk="robini.akacia0o.specio">
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> de <tld/>o
      (<trd lng="la">Robinia pseudoacacia</trd>)
      kun blankaj floraroj, el kiu produkti&gcirc;as bonkvalita mielo;
      pro la foliaroj kaj dornoj la arbo estas konfuzata kun
      <ref cel="akaci.0o">akacio</ref>.
    </dif>
    <bld
    lok="&WCU;/thumb/2/2d/Robinia_(Robinia_pseudoacacia).jpg/640px-Robinia_(Robinia_pseudoacacia).jpg"
    prm="CC-BY-SA-3.0">floraroj de &scirc;ajnakacia <tld/>o (Robinia pseudoacacia)
        <fnt>
          <aut>Rasbak, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Robinia_(Robinia_pseudoacacia).jpg">&WCW;/File:Robinia_(Robinia_pseudoacacia).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
 <trdgrp lng="be">
   <trd>&c_b;&c_je;&c_l;&c_a;&c_ja; <ind>&c_a;&c_k;&c_a;&c_c;&c_y;&c_ja;</ind></trd>,
   <trd>&c_ib;&c_l;&c_zh;&c_e;&c_a;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>,
   <trd>&c_r;&c_a;&c_b;&c_ib;&c_n;&c_ib;&c_ja; &c_p;&c_s;&c_e;&c_w;&c_d;&c_a;&c_a;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
 </trdgrp>
 <trdgrp lng="de">
    <trd>Gew&ouml;hnliche Robinie</trd>,
    <trd>Falsche Akazie</trd>,
    <trd>Silberregen</trd>
  </trdgrp>
  <trd lng="fr">faux-acacia</trd>
  <trd lng="pl">robinia akacjowa</trd>
  <trd lng="ro">salc&acirc;m</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_ib;&#x43D;&c_ib;&#x44F; &c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&#x43D;&c_a;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&c_a; &c_a;&#x43A;&c_a;&c_c;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>

</art>

