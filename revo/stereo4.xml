<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stereo4.xml,v 1.5 2024/03/25 07:28:36 revo Exp $">
<kap>
    <rad>stereo</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stereo4.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="stereo4.0.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="vid" cel="relief.0a.FORM">Reliefa</ref>,
      ka&ubreve;zanta impreson de volumeneco, spaco, profundeco, distanco:
      <ekz>
        estingi&gcirc;is la <tld/>bildo kaj la komunikado kun la stel&scirc;ipo
        &ccirc;esis
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/horo.html">
          Horo de Bovo</url></vrk>,
          <lok>2005</lok>;
        </fnt>
      </ekz>
      <ekz>
        &ccirc;i tiuj son-dosieroj estas de malalta kvalito kaj ne estas
        <tld/>-sonaj
        <fnt>
          <aut>Enrique</aut>,
          <vrk><url ref="http://esperantofre.com/eroj/ilo01e.htm">
          Por lerni Esperanton</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plasti.0a.BELA">plastika</ref>,
      <ref cel="dimens.tri0a.FORM">tridimensia</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;-</trd>
  <trd lng="fr">st&eacute;r&eacute;o-</trd>
  <trd lng="pl">stereo-</trd>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_r;&c_je;&#x43E;&#x2026;</trd>
  <trdgrp lng="zh">
    <trd>&#x7ACB;&#x4F53; <pr>l&#xEC;t&#x1D0;</pr></trd>,
    <trd>&#x7ACB;&#x9AD4; <pr>l&#xEC;t&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>

