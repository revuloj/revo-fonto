<art mrk="$Id: kep.xml,v 1.21 2024/06/12 06:32:01 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kep</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kep.0o">
  <kap><tld/>o</kap>
  <snc mrk="kep.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Cilindra <ref tip="super" cel="cxap.0o.TEKS">&ccirc;apo</ref> kun
      lar&gcirc;a viziero, ofte peco de uniformo milista, lernejana...:
      <ekz>
        amba&ubreve; &gcirc;endarmoj levi&gcirc;is, rektige re&scirc;ovis la <tld/>ojn
        alfrunte
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>;
      </ekz>
      <ekz>
        flegme ili foliumas monbiletojn, ka&scirc;e, kun nigraj <tld/>oj, nigraj ledjakoj,
        hararo mallonge tondita
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kasked.0o">kaskedo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_je;&c_p;&c_ib;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>brigad&yacute;rka</trd>,
    <trd>&ccaron;epice se &scaron;t&iacute;tkem</trd>
  </trdgrp>
  <trd lng="de">K&auml;ppi</trd>
  <trd lng="en">kepi</trd>
  <trd lng="fr">k&eacute;pi</trd>
  <trd lng="hu">t&aacute;ny&eacute;rsapka</trd>
  <trdgrp lng="ja">
    <trd>&#12465;&#12500;&#24125;  <pr>&#12465;&#12500;&#12412;&#12358;</pr></trd>,
    <trd>&#23398;&#29983;&#24125;  <pr>&#12364;&#12367;&#12379;&#12356;&#12412;&#12358;</pr></trd>,
    <trd>&#21046;&#24125;  <pr>&#12379;&#12356;&#12412;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kepie</trd>,
    <trd>sjako</trd>
  </trdgrp>
  <trd lng="pl">kepi</trd>
  <trd lng="ro">chipiu</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">brigad&iacute;rka</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
