<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kep</rad>/o
</kap>
<drv mrk="kep.0o">
  <kap><tld/>o</kap>
  <snc mrk="kep.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Cilindra <ref tip="super" cel="cxap.0o.TEKS">&ccirc;apo</ref> kun lar&gcirc;a 
      viziero, ofte peco de uniformo milista, lernejana...: 
      <ekz>
        amba&ubreve; &gcirc;endarmoj levi&gcirc;is, rektige re&scirc;ovis la <tld/>ojn alfrunte
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_je;&c_p;&c_ib;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">K&auml;ppi</trd>
  <trd lng="en">kepi</trd>
  <trd lng="fr">k&eacute;pi</trd>
  <trd lng="hu">t&aacute;ny&eacute;rsapka</trd>
  <trd lng="nl">kepi</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_i;</trd>
  </trdgrp>
</drv>
</art>
