<kap>
  <ofc>2</ofc>
  <rad>kep</rad>/o
</kap>
<drv mrk="kep.0o">
  <kap><tld/>o</kap>
  <snc mrk="kep.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Cilindra <ref tip="super" cel="cxap.0o.TEKS">&ccirc;apo</ref> kun
      lar&gcirc;a 
      viziero, ofte peco de uniformo milista, lernejana...: 
      <ekz>
        amba&ubreve; &gcirc;endarmoj levi&gcirc;is, rektige re&scirc;ovis la <tld/>ojn
        alfrunte
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_je;&c_p;&c_ib;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>brigad&yacute;rka</trd>,
    <trd>&ccaron;epice se &scaron;t&iacute;tkem</trd>
  </trdgrp>
  <trd lng="de">K&auml;ppi</trd>
  <trd lng="en">kepi</trd>
  <trd lng="fr">k&eacute;pi</trd>
  <trd lng="hu">t&aacute;ny&eacute;rsapka</trd>
  <trdgrp lng="ja">
    <trd>&#x30B1;&#x30D4;&#x5E3D;  <pr>&#x30B1;&#x30D4;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x5B66;&#x751F;&#x5E3D;  <pr>&#x304C;&#x304F;&#x305B;&#x3044;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x5236;&#x5E3D;  <pr>&#x305B;&#x3044;&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kepie</trd>,
    <trd>sjako</trd>
  </trdgrp>
  <trd lng="pl">kepi</trd>     
  <trd lng="ro">chipiu</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_je;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_i;</trd>
