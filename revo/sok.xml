<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sok.xml,v 1.20 2023/11/27 21:22:48 revo Exp $">
<kap>
  <rad>sok</rad>/o <fnt><bib>MEE</bib></fnt>
</kap>
<drv mrk="sok.0o">
  <kap><tld/>o</kap>
  <snc mrk="sok.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="plug.0ilo.AGR">plugilo</ref>,
      konsistanta el triangula oblikva klingo,
      kiu sublevas la teron kaj renversas &gcirc;in flanken:
      <ekz>
        en blazono de <nom>Rozst&aacute;n&iacute;</nom> estas rektflanka plugila
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=347304">Rozst&aacute;n&iacute;
          (distrikto Svitavy)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_m;&c_je;&c_h;</trd>,
    <trd>&c_l;&c_ja;&c_m;&c_je;&c_sh;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_o;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>radlice</trd>,
    <trd>r&aacute;dlo</trd>
  </trdgrp>
  <trd lng="de">Pflugschar</trd>
  <trdgrp lng="en">
    <trd>ploughshare<klr> (brite)</klr></trd>,
    <trd>plowshare<klr> (usone)</klr></trd>
  </trdgrp>
  <trd lng="fr">soc</trd>
  <trd lng="hu">ekevas</trd>
  <trd lng="ja">&#12377;&#12365;&#12409;&#12425;</trd>
  <trd lng="nl">ploegschaar</trd>
  <trd lng="pl">lemiesz</trd>
  <trd lng="pt">l&acirc;mina de arado</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_je;&c_m;&c_je;&c_h;</trd>
  </trdgrp>
  <trd lng="sk">radlica <klr>(po&lcaron;nohosp.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_je;&c_m;&c_ib;&c_sh;</trd>
  </trdgrp>
</drv>
</art>
