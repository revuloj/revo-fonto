<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: azurit.xml,v 1.3 2022/03/12 15:52:30 revo Exp $">
<kap>
  <rad>azurit</rad>/o <fnt><vrk><url ref="http://www.majstro.com">Majstro</url></vrk></fnt>
</kap>
<drv mrk="azurit.0o">
  <kap><tld/>o</kap>
  <snc mrk="azurit.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Blua <ref tip="super" cel="sxton.juvel0o.MIN">juvel&scirc;tono</ref>,
      ofte kristala, el kupra karbonato:
      <ekz>
        dum jarcentoj la nura alternativo por krei
        bluan koloron estis la mineralo <tld/>o
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2018/02/14/la-scienco-de-arto/">
          La scienco de arto</url></vrk>
          <lok>Scivolemo, 2018-02-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_u;&c_r;&c_y;&c_t;</trd>
  <trd lng="en">azurite</trd>
  <trd lng="fr">azurite</trd>
  <trd lng="it">azzurrite</trd>
  <trd lng="uk">&#x430;&#x437;&#x443;&#x440;&#x438;&#x442;</trd>
</drv>
</art>

