<drv mrk="fek.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
Eligi <tld/>a&jcirc;on.
</dif>
</snc>
<trd lng="en">defecate</trd>
<trd lng="fr">d&eacute;f&eacute;quer</trd>
<trd lng="ru">&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
