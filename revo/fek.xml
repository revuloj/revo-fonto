      <trd>stolec</trd>,
      <trd>kupa</trd>,
      <trd>odchody</trd>,
      <trd>ekskrementy</trd>,
      <trd>g&oacute;wno<klr> (wulg.)</klr></trd>
    </trdgrp>
    <trdgrp lng="sv">
