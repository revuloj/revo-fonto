<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fek</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fek.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Eligi <tld/>a&jcirc;on.
    </dif>
  </snc>
  <trd lng="es">defecar</trd>
  <trdgrp lng="en">
    <trd>defecate</trd>,
    <trd>shit</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;f&eacute;quer</trd>
  <trd lng="hu">sz&eacute;kel</trd>
  <trd lng="nl">stoelgang hebben</trd>
  <trd lng="pl">wypr&oacute;&zdot;nia&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
    <trd>defecar</trd>,
    <trd>evacuar</trd>,
    <trd>cagar</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>bajsa</trd>,
    <trd>skita</trd>
  </trdgrp>
</drv>

<drv mrk="fek.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="fek.0.ekkrio">
    <dif>
      Ekkrio esprimanta malkontenton, koleron:
      <ekz>
        Jo&ccirc;jo provas fari siajn lernotaskojn, sed da&ubreve;re grumblas. 
        &leftquot;<tld lit="F"/>!&rightquot; li fine diras. &leftquot;Panjo, mi bezonas novan 
        krajonon!&rightquot;
        <fnt>
          <aut>A. Mendelawitz</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/esperantoaustralia/message/663">
          Bona krajono</url></vrk>,
          <lok>[forumo] Esperanto Australia, 2005-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schei&szlig;e !</trd>
  <trdgrp lng="en">
    <trd>crap !</trd>,
    <trd>shit !</trd>
  </trdgrp>
  <trd lng="fr">merde !</trd>
  <trd lng="nl">shit<klr tip="ind"> (uitroep)</klr></trd>
  <trd lng="it">merda !</trd>
  <trd lng="pt">merda !</trd>
  <trd lng="sv">skit !</trd>
</drv>

<drv mrk="fek.0o">
  <kap><tld/>o</kap>
  <snc mrk="fek.0o.ado">
    <ref tip="dif" cel="fek.0ado.FIZL"><tld/>ado</ref>
      <ekz>
        en antikva &Ccirc;inujo oni vi&scirc;is sian anuson post <tld/>o ne
        per papero sed per vergo ligna
        <fnt>
        <aut>B. Y. Takata</aut>,
        <vrk><url
        ref="http://groups.yahoo.co.jp/group/gajagaja/messages/334?expand=1">
        Spatelo por forvi&scirc;i feka&jcirc;ojn!</url></vrk>,
        <lok>2000-11</lok>
        </fnt>.
      </ekz>
    <trd lng="hu">sz&eacute;kel&eacute;s</trd>
    <trdgrp lng="en">
      <trd>defecation</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>stoelgang<klr tip="ind"> (handeling)</klr></trd>,
      <trd>ontlasting<klr tip="ind"> (handeling)</klr></trd>
    </trdgrp>
    <trd lng="sv">bajsande<klr> (handling)</klr></trd>
  </snc>
  <snc mrk="fek.0o.ajxo">
    <ref tip="dif" cel="fek.0ajxo.FIZL"><tld/>a&jcirc;o</ref>
    <trd lng="hu">sz&eacute;klet</trd>
       <trdgrp lng="nl">
      <trd>stoelgang<klr tip="ind"> (voorwerp)</klr></trd>,
      <trd>ontlasting<klr tip="ind"> (voorwerp)</klr></trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>bajs</trd>,
      <trd>avf&ouml;ring</trd>
    </trdgrp>
  </snc>
  <rim>
    Jen ekzemplo kiu kombinas amba&ubreve; sencojn:
    <ekz>
      diareo <uzo tip="fak">MED</uzo> oftaj kaj likvaj <tld/>oj
      <fnt><bib>PIV2</bib></fnt>.
    </ekz>
  </rim>
  <adm>
    PIV1 donas al ~o la sencon de ~a&jcirc;o, kaj al
    ~a&jcirc;o &gcirc;i donas kolektivan sencon. &Ccirc;u sa&gcirc;e?
    <aut>MB</aut>
  </adm>
  <trdgrp lng="en">
    <trd>defecation</trd>,
    <trd>shit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_l;<klr tip="amb"> (&c_m;&c_je;&c_d;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fek.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fek.0ado.FIZL">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="en">defecation</trd>
  <trd lng="fr">d&eacute;f&eacute;cation</trd>
  <trd lng="hu">sz&eacute;kel&eacute;s</trd>
  <trdgrp lng="nl">
      <trd>stoelgang<klr tip="ind"> (handeling)</klr></trd>,
      <trd>ontlasting<klr tip="ind"> (handeling)</klr></trd>
  </trdgrp>
  <trd lng="pl">wypr&oacute;&zdot;nienie</trd>
  <trd lng="ru">&c_d;&c_je;&c_f;&c_je;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>bajsande<klr> (handling)</klr></trd>,
    <trd>skitande<klr> (handling)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fek.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fek.0ajxo.FIZL">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="ekskre1.0o">Ekskremento</ref> eligita tra
      la anuso:
      <ekz>
        vind/i<klr>: ...</klr>; garni etan infanon per tuko
        a&ubreve; simila peco, kiu sorbas a&ubreve; retenas &gcirc;iajn urinon
        kaj <tld/>a&jcirc;ojn
        <fnt><bib>HejmVort</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>defecation</trd>
    </trdgrp>
  </snc>
  <snc mrk="fek.0ajxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      A&ccirc;a afero a&ubreve; a&jcirc;o.
    </dif>
  </snc>
  <trd lng="en">shit</trd>
  <trdgrp lng="fr">
    <trd>crotte</trd>,
    <trd>excr&eacute;ment<klr tip="ind"> (mati&egrave;res f&eacute;cales)</klr></trd>,
    <trd>fiente</trd>,
    <trd>mati&egrave;res f&eacute;cales</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;klet</trd>
  <trdgrp lng="nl">
      <trd>stoelgang<klr tip="ind"> (voorwerp)</klr></trd>,
      <trd>ontlasting<klr tip="ind"> (voorwerp)</klr></trd>
  </trdgrp>
  <trd lng="pl">odchody</trd>
  <trdgrp lng="pt">
    <trd>bosta</trd>,
    <trd>merda</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>bajs</trd>,
    <trd>skit<klr> (subst.)</klr></trd>,
    <trd>avf&ouml;ring</trd>
  </trdgrp>
</drv>
</art>
