</dif>
</snc>
<trdgrp lng="fr">
  <trd>crotte</trd>,
  <trd>excr&eacute;ment<klr tip="ind"> (mati&egrave;res f&eacute;cales)</klr></trd>,
  <trd>fiente</trd>,
  <trd>mati&egrave;res f&eacute;cales</trd></trdgrp> 
