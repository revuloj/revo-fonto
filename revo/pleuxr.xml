<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pleuxr.xml,v 1.18 2024/02/17 07:52:16 revo Exp $">
<kap>
  <rad>ple&ubreve;r</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pleuxr.0o">
  <kap><tld/>o</kap>
  <snc mrk="pleuxr.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Sereca <ref tip="super" cel="membra.0o.ANA">membrano</ref>,
      kiu surkovras la internon de la <ref tip="malprt" cel="brust.0o">brusto</ref>
      kaj tegas la pulmojn:
      <ekz>
        diagnostiko kaj terapio de malsanoj de tra&hcirc;eo, bron&hcirc;oj kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9956">Ladislav Riedl</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        birdoj havas nek diafragmon nek <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3921">Spira sistemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_w;&c_r;&c_a;</trd>
  <trd lng="ca">pleura</trd>
  <trdgrp lng="de">
    <trd>Pleura</trd>,
    <trd>Lungenfell</trd>,
    <trd>Rippenfell</trd>
  </trdgrp>
  <trd lng="en">pleura</trd>
  <trd lng="es">pleura</trd>
  <trd lng="fa">&f_p;&a_r;&a_d;&a_h;&a_hamza_sure; &a_j;&a_n;&a_b;</trd>
  <trdgrp lng="hu">
    <trd>mellh&aacute;rtya</trd>,
    <trd>pleura</trd>
  </trdgrp>
  <trd lng="fr">pl&egrave;vre</trd>
  <trd lng="it">pleura</trd>
  <trd lng="nl">borstvlies</trd>
  <trd lng="pl">op&lstroke;ucna</trd>   
  <trd lng="pt">pleura</trd>                          
  <trd lng="ru">&c_p;&c_l;&c_je;&c_v;&c_r;&c_a;</trd>
  <trd lng="uk">&#x43F;&#x43B;&#x435;&#x432;&#x440;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x80F8;&#x819C; <pr>xi&#x14D;ngm&#xF2;</pr></trd>,
    <trd>&#x810F;&#x80F8;&#x819C; <pr>z&#x101;ngxi&#x14D;ngm&#xF3;</pr></trd>,
    <trd>&#x9AD2;&#x80F8;&#x819C; <pr>z&#x101;ngxi&#x14D;ngm&#xF3;</pr></trd>
  </trdgrp>
</drv>
</art>
