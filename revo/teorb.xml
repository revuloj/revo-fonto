<kap>
  <rad>teorb</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="teorb.0o">
  <kap><tld/>o</kap>
  <snc mrk="teorb.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">Kordinstrumento</ref>,
      granda, basa <ref tip="vid" cel="liut.0o.MUZ">liuto</ref>
      populara precipe en renesanca E&ubreve;ropo:
      <ekz>
        <tld/>isto <nom>Tommie Andersson</nom> ludas &ccirc;akonon
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://usa.errachidia.org/1-teorbo.html">
          Teorbo</url></vrk>,
          <lok>[vidita en 2009]</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/87/Joueuse_de_th&eacute;orbe_-_Jacob_Duck.jpg/478px-Joueuse_de_th&eacute;orbe_-_Jacob_Duck.jpg"
      prm="PD"
      >ludanto de <tld/>o de Jacob Duck
        <fnt>
          <aut>Jacob Duck, http://collection.imamuseum.org/artwork/56811/</aut>
          <lok><url ref="&WCW;/File:Joueuse_de_th%C3%A9orbe_-_Jacob_Duck.jpg"
              >https://commons.wikimedia.org/wiki/File:Joueuse_de_th&eacute;orbe_-_Jacob_Duck.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_o;&c_r;&c_b;&c_a;</trd>
  <trd lng="bg">&c_t;&c_je;&c_o;&c_r;&c_b;&c_a;</trd>
  <trd lng="ca">tiorba</trd>
  <trdgrp lng="cs">
    <trd>basloutna</trd>,
    <trd>teorba</trd>
  </trdgrp>
  <trd lng="de">Theorbe</trd>
  <trd lng="en">theorbo</trd>
  <trd lng="es">tiorba</trd>
  <trd lng="fr">th&eacute;orbe</trd>
  <trd lng="io">teorbo</trd>
  <trd lng="it">tiorba</trd>
  <trd lng="ja">&#12486;&#12458;&#12523;&#12508;</trd>
  <trd lng="mk">&c_t;&c_je;&c_o;&c_r;&c_b;&c_a;</trd>
  <trd lng="nl">theorbe</trd>
  <trd lng="no">teorbe</trd>
  <trd lng="oc">teorba</trd>
  <trd lng="pl">teorban</trd>
  <trd lng="pt">teorba</trd>
  <trd lng="sk">teorba</trd>
  <trd lng="sv">teorb</trd>
  <trd lng="uk">&c_t;&c_je;&#x43E;&c_r;&c_b;&c_a; (&c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;)</trd>
