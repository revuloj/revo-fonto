<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: teorb.xml,v 1.6 2022/11/28 14:28:01 revo Exp $">
<kap>
  <rad>teorb</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="teorb.0o">
  <kap><tld/>o</kap>
  <snc mrk="teorb.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">Kordinstrumento</ref>,
      granda, basa <ref tip="vid" cel="liut.0o.MUZ">liuto</ref>
      populara precipe en renesanca E&ubreve;ropo:
      <ekz>
        <tld/>isto Tommie Andersson ludas &ccirc;akonon 
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://usa.errachidia.org/1-teorbo.html">
          Teorbo</url></vrk>,
          <lok>[vidita en 2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_o;&c_r;&c_b;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>basloutna</trd>,
    <trd>teorba</trd>
  </trdgrp>
  <trd lng="fr">th&eacute;orbe</trd>
  <trd lng="sk">teorba</trd>
  <trd lng="uk">&c_t;&c_je;&#x43E;&c_r;&c_b;&c_a; (&c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;)</trd>
</drv>
</art>

