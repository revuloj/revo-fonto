<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: versx.xml,v 1.41 2024/01/22 01:39:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ver&scirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="versx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fluigi flua&jcirc;on el ujo:
      <ekz>
        <tld/>i vinon el botelo en glason,
        inkon en inkujon;
      </ekz>
      <ekz>
        <tld/>i ies sangon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i larmojn;
      </ekz>
      <ekz>
        <tld/>i akvon al maro<fnt>Z</fnt>
        <klr>(vane peni)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>gie&szlig;en</trd>,
      <trd>vergie&szlig;en</trd>,
      <trd>sch&uuml;tten</trd>,
      <trd>versch&uuml;tten</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>verter</trd>,
      <trd>derramar<klr> (un l&iacute;quido)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>verser</trd>
    </trdgrp>
    <trd lng="he">&lamed;&mem;&zayin;&vav;&gimel;</trd>
    <trd lng="hu">&ouml;nt</trd>
    <trdgrp lng="nl">
      <trd>schenken</trd>,
      <trd>gieten</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>la&cacute;</trd>,
      <trd>wylewa&cacute;</trd>
    </trdgrp>
    <trd lng="sk">lia&tcaron;</trd>
  </snc>
  <snc>
    <dif>
      Abunde meti a&ubreve; &jcirc;eti sur:
      <ekz>
        la suno <tld/>is oran lumon<fnt>B</fnt>;
      </ekz>
      <ekz>
        la luno <tld/>is bluan lumon sur la arbaron<fnt>B</fnt>;
      </ekz>
      <ekz>
        de la tuta &ccirc;ielo ala&ubreve;doj <tld/>is siajn
        kantetojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i sian koleron egale sur
        malamikojn kaj sur amikojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i insultojn sur iun<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i &ccirc;iujn favorojn sur
        iu(n);
      </ekz>
      <ekz>
        la 30-jara milito <tld/>is multajn
        malfeli&ccirc;ojn kaj suferojn sur la lando(n).
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pluv">pluvigi</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>derramar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>r&eacute;pandre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ont</trd>,
      <trd>el&aacute;raszt</trd>
    </trdgrp>
    <trd lng="pl">zalewa&cacute;</trd>
  </snc>
  <snc>
    <dif>
      Abunde meti a&ubreve; &jcirc;eti en:
      <ekz>
        &scirc;ia boneco rapide
        <tld/>is en la koron de l' infano kura&gcirc;on kaj
        konfidon<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu sukceso <tld/>is fre&scirc;ajn fortojn en la animojn;
      </ekz>
      <ekz>
        la Eternulo <tld/>is en ilin spiriton de
        konfuzo<fnt><bib>MT</bib><lok>&Jes; 19:14</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>llenar</trd>,
      <trd>rellenar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>emplir</trd>,
      <trd>remplir</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;lt</trd>
    <trdgrp lng="pl">
      <trd>wlewa&cacute;</trd>,
      <trd>zalewa&cacute;</trd>
    </trdgrp>
    <trd lng="sk">lia&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_lata;&t_shabchu;&t_ga;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="en"><trd>to pour</trd>, <trd>spill</trd></trdgrp>
  <trd lng="he">&lamed;&shin;&pe;&vav;&fkaf;</trd>
  <trdgrp lng="it">
    <trd>versare</trd>,
    <trd>riversare</trd>,
    <trd>mescere</trd>,
    <trd>spargere<klr tip="ind"> (versare)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6CE8;&#12368;  <pr>&#12388;&#12368;</pr></trd>,
    <trd>&#x6D41;&#12377;  <pr>&#x306A;&#x304C;&#12377;</pr></trd>,
    <trd>&#12371;&#x307C;&#12377;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>derramar</trd>,
    <trd>verter</trd>,
    <trd>despejar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_s;&c_ib;&#x44E;&c_v;&c_a;&c_t;&c_i; (&c_s;&c_v;&c_ib;&c_t;&#x43B;&#x43E; &c_ib; &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>

<drv mrk="versx.V0isto">
  <kap><tld lit="V"/>isto</kap>
  <mlg>Aqr</mlg>
  <snc mrk="versx.V0isto.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Aquarius</trd>).
    </dif>
    <ref tip="sin" cel="akv.A0isto">Akvisto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_a;&c_d;&c_a;&c_l;&c_je;&c_j;</trd>,
    <trd>&c_V;&c_a;&c_d;&c_a;&c_l;&c_ib;&c_w;</trd>
  </trdgrp>
  <trd lng="de">Wassermann<klr tip="amb"> (Sternbild)</klr></trd>
  <trd lng="en">Aquarius</trd>
  <trd lng="fa">&a_d;&a_l;&a_w;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trd lng="es">Acuario<klr> (constelaci&oacute;n)</klr></trd>
  <trd lng="fr">Verseau<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &dalet;&lamed;&yod;</trd>
  <trd lng="hu">V&iacute;z&ouml;nt&odblac;</trd>
  <trd lng="it">Acquario<klr tip="ind"> (costellazione)</klr></trd>
  <trd lng="ja">&#x5B9D;&#x74F6;&#x5BAE; <pr>&#x307B;&#12358;&#12408;&#12356;&#x304D;&#12421;&#12358;</pr></trd>
  <trd lng="nl">Waterman<klr tip="ind"> (sterrebeeld)</klr></trd>
  <trd lng="pl">Wodnik <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="pt">Aqu&aacute;rio</trd>
  <trd lng="ro">V&abreve;rs&abreve;torul <klr>(constela&#539;ie)</klr></trd>
  <trd lng="ru">&c_V;&c_o;&c_d;&c_o;&c_l;&c_je;&c_j;</trd>
  <trd lng="sk">Vodn&aacute;r</trd>
</drv>

<drv mrk="versx.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="versx.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;igi <tld/>ante:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la hela suno <klr>[...]</klr> &ccirc;irka&ubreve;<tld/>is
        la homojn per varmoondoj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 8</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">to engulf</trd>
  <trd lng="hu">el&aacute;raszt <klr tip="ind">(&aacute;tv.)</klr></trd>
  <trd lng="pl">oblewa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="versx.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="versx.dis0i.KOMUNE">
    <dif>
      Disigi per <tld/>ado:
      <ekz>
        dis<tld/>i inkon sur paperojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        la lar&gcirc;e dis<tld/>ita Volgo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i koloniojn en tutan mondon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sem.0i">semi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>aussch&uuml;tten</trd>,
    <trd>versch&uuml;tten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to pour out</trd>,
    <trd>spill</trd>, <trd>diffuse</trd>
  </trdgrp>
  <trd lng="es">derramar</trd>
  <trd lng="fr">r&eacute;pandre</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;t&ouml;nt</trd>,
    <trd>sz&eacute;tlocsol</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>spargere<klr tip="ind"> (un liquido)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25746;&#x304D;&#25955;&#12425;&#12377;  <pr>&#x307E;&#x304D;&#12385;&#12425;&#12377;</pr></trd>,
    <trd>&#12406;&#12385;&#x307E;&#12369;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">naar alle kanten <ind>uitgieten</ind></trd>
  <trd lng="pl">rozlewa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozlia&tcaron;</trd>
  <trd lng="uk">&c_r;&#x43E;&c_z;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="versx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="versx.el0i.KOMUNE">
    <dif>
      Malplenigi per <tld/>ado:
      <ekz>
        edzino pli el<tld/>as per funelo ol edzo
        en<tld/>as per sitelo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        valora rekompenco por
        via el<tld/>ita sango<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de la manoj &gcirc;is
        lipoj la sup' el<tld/>i&gcirc;is
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el<tld/>i sur iun sian koleron
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausschenken</trd>,
    <trd>ausgie&szlig;en</trd>,
    <trd>aussch&uuml;tten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to pour out</trd>,
    <trd>spill out <klr>(emptying)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>verter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;verser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&ouml;nt</trd>,
    <trd>kiont</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sversare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6CE8;&#x304E;&#x51FA;&#12377;  <pr>&#x305D;&#x305D;&#x304E;&#12384;&#12377;</pr></trd>,
    <trd>&#x6D41;&#12375;&#x51FA;&#12377;  <pr>&#x306A;&#x304C;&#12375;&#12384;&#12377;</pr></trd>,
    <trd>&#21520;&#38706;&#12377;&#x308B;  <pr>&#12392;&#x308D;&#12377;&#x308B;</pr></trd>,
    <trd>&#12406;&#12385;&#x307E;&#12369;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">uitgieten</trd>
  <trd lng="pl">wylewa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="versx.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="versx.en0i.KOMUNE">
    <dif>
      <tld lit="V"/>i en:
      <ekz>
        trinku el la vino, kiun mi
        en<tld/>is<fnt><bib>MT</bib><lok>&Sen; 9:5</lok></fnt>;
      </ekz>
      <ekz>
        rivero en<tld/>i&gcirc;is en la sova&gcirc;an
        maron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        de la momento, kiam li ali&gcirc;is
        al nia anaro, en nian aferon en<tld/>i&gcirc;is ia nova
        energio<fnt><bib>Paroloj</bib><lok>Parolado super la Tombo de
        Karlo Bourlet, 1913</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eingie&szlig;en</trd>,
    <trd>einschenken</trd>
  </trdgrp>
  <trd lng="en">to pour in</trd>
  <trdgrp lng="es">
    <trd>rellenar</trd>,
    <trd>verter en</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>remplir</trd>,
    <trd>verser dans</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bet&ouml;lt</trd>,
    <trd>be&ouml;nt</trd>
  </trdgrp>
  <trd lng="ja">&#x6CE8;&#x304E;&#x8FBC;&#12416;  <pr>&#12388;&#x304E;&#12371;&#12416;</pr></trd>
  <trdgrp lng="nl">
    <trd>ingieten</trd>,
    <trd>inschenken</trd>
  </trdgrp>
  <trd lng="pl">wlewa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vlia&tcaron;</trd>,
    <trd>nalia&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="versx.pri0i">
  <kap>pri<tld/>i, <var><kap>sur<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="versx.pri0i.KOMUNE">
    <dif>
      Malsekigi <tld/>ante:
      <ekz>
        pri<tld/>i florojn<fnt>Z</fnt>,
        vojon<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;iun nokton mi pri<tld/>as mian
        liton<fnt><bib>MT</bib><lok>&Psa; 6:6</lok></fnt>;
      </ekz>
      <ekz>
        tiuj, kiuj &ccirc;iutage sur<tld/>as sian panon per
        larmoj<fnt><bib>Far1</bib>, <lok>&ccirc;apitro 14a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la okcidentan horizonton sur<tld/>is ora koloro
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">begie&szlig;en</trd>
  <trdgrp lng="en">
    <trd>to water</trd>,
    <trd>to wet</trd>
  </trdgrp>
  <trd lng="es">regar</trd>
  <trd lng="fr">arroser</trd>
  <trdgrp lng="he">
    <trd>&lamed;&shin;&tet;&vav;&fpe;</trd>,
    <trd>&lamed;&he;&resh;&tet;&yod;&bet;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meg&ouml;nt&ouml;z</trd>,
    <trd>el&aacute;ztat</trd>
  </trdgrp>
  <trdgrp lng="ja">
