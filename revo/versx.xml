<drv mrk="versx.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="versx.super0i.KOMUNE">
    <dif>
      Tute kovri per <tld/>ado:
      <ekz>
        la plej malaltaj kampoj estis
        super<tld/>itaj<fnt><bib>Far1</bib>, <lok>&ccirc;apitro
        11a</lok></fnt>;
      </ekz>
      <ekz>
        Nilo super<tld/>as tutan Egipton<fnt><bib>Far1</bib>,
        <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la landon <nom>Gosen</nom> super<tld/>is
        ru&gcirc;o<fnt><bib>Far1</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        pargeto<klr>[&#x2026;]</klr> super<tld/>ita per la reklama lumo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        senhomaj, super<tld/>itaj per sunradioj, stratoj de la provinca urbeto
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ban.0i">bani</ref>,
      <ref cel="dron.0igi">dronigi</ref>,
      <ref cel="inund.0i">inundi</ref>,
      <ref cel="satur.0i">saturi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">inundar</trd>
