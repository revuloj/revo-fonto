<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>juvel</rad>/o
</kap>
<drv mrk="juvel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="juvel.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Ornama&jcirc;o, &gcirc;enerale malgranda, tre multekosta pro la materio (oro,
      ar&gcirc;ento, gemoj) a&ubreve; la prilaboriteco:
      <ekz>
        li respondis, ke li estas komercisto, malvolvis la tukon kaj
        montris la <tld/>ojn
        <fnt><bib>ElektFab</bib>, <lok>fidela Johano</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="br">bravig<klr tip="ind"> (brageriz)</klr></trd>
    <trdgrp lng="de">
      <trd>Juwel</trd>,
      <trd>Kleinod</trd>,
      <trd>Schmuckst&uuml;ck</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="juvel.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Multvalora&jcirc;o:
      <ekz>
        pluraj el tiuj rakontoj estas etaj <tld/>oj
        <fnt>
        <aut>Sten Johansson</aut>,
        <vrk>Satira, humure serioza, amara, ridiga (T&oacute;th-M&aacute;th&eacute; Miklos,
        Trans la se&gcirc;o)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 1a, p. 19a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="perl.0o">perlo</ref>,
      <ref cel="trezor.0o.aprezindajxo">trezoro <sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_a;&c_r;&c_b;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Juwel</trd>,
      <trd>Wertgegenstand</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
    </trdgrp>
    <trd lng="sv">juvel</trd>
  </snc>
  <trd lng="es">joya</trd>
  <trdgrp lng="fr">
    <trd>bijou</trd>,
    <trd>joyau</trd>
  </trdgrp>
  <trd lng="hu">&eacute;kszer</trd>
  <trd lng="nl">juweel</trd>
  <trd lng="pt">j&oacute;ia</trd>
</drv>

<drv mrk="juvel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="juvel.0isto.KOMUNE">
    <dif>
      Homo, kiu prilaboras a&ubreve; vendas <tld/>ojn:
      <ekz>
        diamanto en magazeno de <tld/>isto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_v;&c_e;&c_l;&c_ib;&c_r;</trd>
  <trd lng="br">bravigour</trd>
  <trdgrp lng="de">
    <trd>Juwelier</trd>,
    <trd>Schmuckh&auml;ndler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bijoutier</trd>,
    <trd>joaillier</trd>
  </trdgrp>
  <trd lng="es">joyero</trd>
  <trd lng="hu">&eacute;kszer&eacute;sz</trd>
  <trd lng="nl">juwelier</trd>
  <trd lng="pt">joalheiro</trd>
  <trd lng="ru">&c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;</trd>
  <trd lng="sv">juvelerare</trd>
</drv>

<drv mrk="juvel.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="juvel.0ejo.vendejo">
    <dif>
      Vendejo de <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_v;&c_e;&c_l;&c_ib;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="br">bravigerezh</trd>
  <trdgrp lng="de">
    <trd>Juwelierladen</trd>,
    <trd>Juweliergesch&auml;ft</trd>,
    <trd>Schmuckgesch&auml;ft</trd>
  </trdgrp>
  <trd lng="es">joyer&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>bijouterie</trd>,
    <trd>joaillerie<klr tip="ind"> (magasin)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;kszer&uuml;zlet</trd>,
    <trd>&eacute;kszerbolt</trd>
  </trdgrp>
  <trd lng="nl">juwelierszaak</trd>
  <trd lng="pt">joalheiria</trd>
  <trdgrp lng="ru">
    <trd>&c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;&c_n;&c_a;&c_ja; &c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;&c_n;&c_y;&c_j; &c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="juvel.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Skatolo por <tld/>oj:
      <ekz>
        la dukino malfermis sian <tld/>ujon kaj donis al la policisto la
        duan braceleton
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la karaj
        braceletoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_f;&c_e;&c_r;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;)</klr></trd>
  <trd lng="br">skrin</trd>
  <trdgrp lng="de">
    <trd>Juwelenk&auml;stchen</trd>,
    <trd>Schmuckk&auml;stchen</trd>,
    <trd>Schmuckschatulle</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>boite &agrave; bijoux</trd>,
    <trd>&eacute;crin<klr tip="amb"> (&agrave; bijoux)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;kszerdoboz</trd>
  <trd lng="nl">juwelenkistje</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_r;&c_je;&c_c;<klr tip="ind"> (&c_d;&c_l;&c_ja; &c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_je;&c_j;)</klr></trd>,
    <trd>&c_f;&c_u;&c_t;&c_l;&c_ja;&c_r;<klr tip="ind"> (&c_d;&c_l;&c_ja; &c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_je;&c_j;)</klr></trd>,
    <trd>&c_sh;&c_k;&c_a;&c_t;&c_u;&c_l;&c_k;&c_a;<klr tip="amb"> (&c_d;&c_l;&c_ja; &c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_je;&c_j;)</klr></trd>
  </trdgrp>
</drv>
</art>
