<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>juvel</rad>/o
</kap>
<drv mrk="juvel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="juvel.0o.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      Ornama&jcirc;o, &gcirc;enerale malgranda, tre multekosta pro la materio (oro,
      ar&gcirc;ento, gemoj) a&ubreve; la prilaboriteco:
      <ekz>
        tibetaj manfaritaj arta&jcirc;oj (<tld/>oj, malnovaj kaj modernaj
        tapi&scirc;oj, incensoj<fnt><aut>Robert Roze</aut>: <vrk>Verkistino -
        orientalistino - esploristino</vrk>, <bib>Monato</bib><lok>jaro
        1999a, numero 1a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>Juwel</trd>,
      <trd>Kleinod</trd>,
      <trd>Schmuckst&uuml;ck</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="juvel.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Multvalora&jcirc;o:
      <ekz>
        pluraj el tiuj rakontoj estas etaj <tld/>oj<fnt><aut>Sten
        Johansson</aut>: <vrk>Satira, humure serioza, amara, ridiga
        (T&oacute;th-M&aacute;th&eacute; Miklos, Trans la se&gcirc;o)</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 1a, p. 19a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="trezor.0o">trezoro</ref>,
      <ref cel="perl.0o">perlo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_a;&c_r;&c_b;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Juwel</trd>,
      <trd>Wertgegenstand</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
    </trdgrp>
    <trd lng="sv">juvel</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>joyau</trd>
  </trdgrp>
  <trd lng="nl">juweel</trd>
  <trd lng="pt">j&oacute;ia</trd>
</drv>

<drv mrk="juvel.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu prilaboras kaj vendas <tld/>ojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Juwelier</trd>,
    <trd>Schmuckh&auml;ndler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>joailler</trd>
  </trdgrp>
  <trd lng="be">&c_ju;&c_v;&c_e;&c_l;&c_ib;&c_r;</trd>
  <trd lng="nl">juwelier</trd>
  <trd lng="pt">joalheiro</trd>
  <trd lng="ru">&c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;</trd>
  <trd lng="sv">juvelerare</trd>
</drv>

<drv mrk="juvel.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Vendejo de <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_v;&c_e;&c_l;&c_ib;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Juwelierladen</trd>,
    <trd>Juweliergesch&auml;ft</trd>,
    <trd>Schmuckgesch&auml;ft</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>joaillerie<klr> (magasin)</klr></trd>
  </trdgrp>
  <trd lng="nl">juwelierszaak</trd>
  <trd lng="pt">joalheiria</trd>
</drv>

<drv mrk="juvel.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Skatolo por <tld/>oj
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_f;&c_e;&c_r;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;)</klr></trd>
  <trdgrp lng="de">
    <trd>Juwelenk&auml;stchen</trd>,
    <trd>Schmuckk&auml;stchen</trd>,
    <trd>Schmuckschatulle</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;crin</trd>
  </trdgrp>
  <trd lng="nl">juwelenkistje</trd>
</drv>
</art>
