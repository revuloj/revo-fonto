<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stativ</rad>/o
</kap>
<drv mrk="stativ.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ordinara tripieda ilo, uzata por subteni aparaton a&ubreve; instrumenton:
      <ekz>
        fotografaparatoj bezonas <tld/>ojn;
      </ekz>
      <ekz>
        kunmetebla <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="postam.0o">postamento</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_a;&c_t;&c_y;&c_w;</trd>
  <trdgrp lng="de">
    <trd>Stativ</trd>,
    <trd>St&auml;nder</trd>,    
    <trd>Dreibein</trd>,
    <trd>Tripode</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;szer&aacute;llv&aacute;ny</trd>,
    <trd>h&aacute;roml&aacute;b</trd>
  </trdgrp>
  <trd lng="nl">statief</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
</drv>
</art>
