<art mrk="$Id: stativ.xml,v 1.23 2025/04/06 06:47:45 revo Exp $">
<kap>
  <rad>stativ</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="stativ.0o">
  <kap><tld/>o</kap>
  <snc mrk="stativ.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ordinara tripieda
      <ref tip="super" cel="baz.0o.postamento">bazo</ref>,
      uzata por subteni aparaton a&ubreve; instrumenton:
      <ekz>
        fotografaparatoj bezonas <tld/>ojn;
      </ekz>
      <ekz>
        kunmetebla <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="postam.0o">postamento</ref>.
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>starigilo</ke>.
  </rim>

  <trd lng="be">&c_sh;&c_t;&c_a;&c_t;&c_y;&c_w;</trd>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="cs">
    <trd>podstavec</trd>,
    <trd>stativ</trd>,
    <trd>stojan</trd>,
    <trd>v&#283;&#353;&#225;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stativ</trd>,
    <trd>St&auml;nder</trd>,    
    <trd>Dreibein</trd>,
    <trd>Tripode</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_h;&zwnj;&f_p;&a_A;&f_y;&a_h;</trd>
  <trd lng="fr">tr&eacute;pied</trd>
  <trdgrp lng="hu">
    <trd>m&udblac;szer&aacute;llv&aacute;ny</trd>,
    <trd>h&aacute;roml&aacute;b</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33050;  <pr>&#12365;&#12419;&#12367;</pr></trd>,
    <trd>&#21488;&#24231;  <pr>&#12384;&#12356;&#12374;</pr></trd>
  </trdgrp>
  <trd lng="nl">statief</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>podstavec</trd>,
    <trd>stat&#237;v</trd>,
    <trd>stojan</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1096;&#1090;&#1072;&#1090;&#1080;&#1074;</trd>,
    <trd>&#1087;&#1110;&#1076;&#1089;&#1090;&#1072;&#1074;&#1082;&#1072;</trd>,
    <trd>&#1089;&#1090;&#1086;&#1081;&#1082;&#1072;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19977;&#35282;&#26550; <pr>s&#257;nji&#462;oji&#224;</pr></trd>,
    <trd>&#19977;&#33050;&#26550; <pr>s&#257;nji&#462;oji&#224;</pr></trd>,
    <trd>&#19977;&#33139;&#26550; <pr>s&#257;nji&#462;oji&#224;</pr></trd>,
    <trd>&#40722; <pr>z&#299;</pr></trd>
  </trdgrp>
</drv>
</art>
