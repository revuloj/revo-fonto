<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stativ</rad>/o
</kap>
<drv mrk="stativ.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ordinara tripieda ilo, uzata por subteni aparaton a&ubreve; instrumenton:
      <ekz>
        fotografaparatoj bezonas <tld/>ojn;
      </ekz>
      <ekz>
        kunmetebla <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="postam.0o">postamento</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Stativ</trd>
  <trd lng="nl">statief</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
</drv>
</art>
