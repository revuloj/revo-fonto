<art mrk="$Id: elit.xml,v 1.27 2024/03/04 11:15:00 revo Exp $">
<kap>
  <rad>elit</rad>/o <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>
</kap>
<drv mrk="elit.0o">
  <kap><tld/>o</kap>
  <snc mrk="elit.0o.POL">
    <dif>
      Plej bona parto el aro, precipe el homgrupo:
      <ekz>
        intelekta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo Franceschi</aut>
          <vrk><url ref="&Monato;/2006/009045.php">Mussolini, Hitler, Stalin kaj Bush</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la reganta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2008/009380.php">Kiu estas la perfidulo?</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        samtempe lerni 5&#x202F;-&#x202F;6 lingvojn kapablas nur malgranda <tld/>o de lingvotalentaj
        sciencistoj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la valutunio estas riska projekto, kiun la politika kaj ekonomia <tld/>oj
        <klr>[&#8230;]</klr> volas trudi al la popolo
        <fnt>
          <bib>Monato</bib>
          <aut>Bo Sandelin</aut>
          <vrk><url ref="&Monato;/2004/008235.html">Svedoj balotis kontra&ubreve; e&ubreve;ro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro sekurecaj konsideroj, tiu koridoro estis elektita por la konferenca <tld/>o
        <fnt>
          <bib>CKv</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Sanktavalo, kontra&ubreve;e, oni sentas sin kvaza&ubreve; &ccirc;e <tld/>o, tiel dignaj,
        bonedukitaj, dol&ccirc;avortaj aperas la lo&gcirc;antoj
        <fnt>
          <bib>ChC</bib>
          <lok>3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krem.0o.plejbonajxo">kremo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_ib;&c_t;&c_a;</trd>
  <trd lng="ca">elit</trd>
  <trdgrp lng="cs">
    <trd>elita</trd>,
    <trd>noblesa</trd>,
    <trd>smet&aacute;nka <klr>(spole&ccaron;nosti)</klr></trd>,
    <trd>v&yacute;kv&ecaron;t</trd>,
    <trd>&scaron;pice</trd>,
    <trd>&scaron;pi&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Elite</trd>,
    <trd>Auserw&auml;hlte</trd>,
    <trd>Bestenauslese</trd>
  </trdgrp>
  <trd lng="en">elite</trd>
  <trd lng="es">&eacute;lite</trd>
  <trd lng="fr">&eacute;lite</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30EA;&#x30FC;&#x30C8;  <pr>&#12360;&#x308A;&#x30FC;&#12392;</pr></trd>,
    <trd>&#36984;&#x826F;  <pr>&#x305B;&#12435;&#x308A;&#12423;&#12358;</pr></trd>,
    <trd>&#x7CBE;&#x92ED;  <pr>&#x305B;&#12356;&#12360;&#12356;</pr></trd>,
    <trd>&#36984;&#x308A;&#x629C;&#x304D;&#x306E;&#x4EBA;&#12293; 
    <pr>&#12360;&#x308A;&#x306C;&#x304D;&#x306E;&#12402;&#12392;&#12403;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="nl">elite</trd>
  <trd lng="pl">elita</trd>
  <trdgrp lng="pt">
    <trd>elite</trd>,
    <trd>fina-flor</trd>,
    <trd>nata</trd>,
    <trd>escol</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_i;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>elita</trd>,
    <trd>v&yacute;kvet</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_l;&c_ib;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_shch;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_ib;&c_r;&c_n;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_ib;&c_r;&c_n;&c_i;&c_j; &c_je;&c_k;&c_z;&c_je;&c_m;&c_p;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7CBE;&#x82F1;&#x4E3B;&#x4E49; <pr>j&#x12B;ngy&#x12B;ngzh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x4F18;&#x79C0;&#20998;&#x5B50; <pr>y&#x14D;uxi&#xF9;f&emacron;nz&#x1D0;</pr></trd>,
    <trd>&#x4FCA;&#20625; <pr>j&#xF9;nji&#xE9;</pr></trd>,
    <trd>&#x7CBE;&#x82F1; <pr>j&#x12B;ngy&#x12B;ng</pr></trd>,
    <trd>&#x7CBE;&#x534E; <pr>j&#x12B;nghu&#xE1;</pr></trd>,
    <trd>&#x4E0A;&#x6D41;&#x793E;&#x4F1A; <pr>sh&#xE0;ngli&#xFA;sh&#xE8;hu&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="elit.0a">
  <kap><tld/>a</kap>
  <snc mrk="elit.0a.POL">
    <dif>
      <ref cel="eminen1.0a">Eminenta</ref>, de la plej bona:
      <ekz>
        <tld/>a posteno, kun gravaj respondecoj
        <fnt>
          <bib>CKv</bib>
          <lok>18</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia filo studas &ccirc;e <tld/>a instituto
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2004/008386.html">Sociolingvistoj &ccirc;u glacikoraj?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        Aso apartenas al <tld/>a kaj ri&ccirc;ega familio
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2008/009639.php">Elitulo elektita</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distin.0inda">distinginda</ref>,
      <ref cel="elekt.0ita">elektita</ref>,
      <ref cel="star.el0a.eminenta">elstara<sncref/></ref>
    </refgrp>
  <trd lng="de">Elite-</trd>
  </snc>
  <snc mrk="elit.0a.grupo">
    <dif>
      <klr>(pri grupo)</klr> Malfacile enirebla:
      <ekz>
        <tld/>a grupo de aktivuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2013/011111.php">Saluton, ser&gcirc;ento!</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        malnova societo, <tld/>a, tre ri&ccirc;a
        <fnt>
          <bib>Gerda</bib>
          <lok>&Ccirc;apitro 24 (dudek Kvar)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esoter.0a">esotera</ref>,
      <ref cel="ferm.0ita.PSI">fermita<sncref/></ref>,
      <ref cel="societ.sekreta0o">sekreta societo</ref>
    </refgrp>
    <trd lng="de">elit&auml;r</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_ib;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_l;&c_ib;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">elitista</trd>
  <trd lng="en">elite</trd>
  <trd lng="fr">d'<ind>&eacute;lite</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30EA;&#x30FC;&#x30C8;&#x306E;  <pr>&#12360;&#x308A;&#x30FC;&#12392;&#x306E;</pr></trd>,
    <trd>&#36984;&#x308A;&#x629C;&#x304D;&#x306E;  <pr>&#12360;&#x308A;&#x306C;&#x304D;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_l;&c_ib;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_ib;&c_r;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="elit.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="elit.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Sinteno, kiu atentas kaj favoras <tld/>on, eventuale neglektante
      ceterajn homojn:
      <ekz>
        tio, kion usonanoj (almena&ubreve; la plejparto) ne &scirc;atas,
        estas <tld/>ismo
        <fnt>
          <aut>T. Sankaro</aut>,
          <vrk><url ref="http://gxangalo.com/modules/weblog/details.php?blog_id=752">Pli
          da &Gcirc;angala
          &leftquot;ne&ubreve;traleco&rightquot;</url></vrk>,
          <lok>[blogo], 2004-12-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Gramsci</nom> <klr>[&#8230;]</klr> inspirita de intelektula <tld/>ismo,
        e&ccirc; opinias ke la subaj popoltavoloj ne tiom bezonas internaciajn
        kontaktojn
        <fnt>
          <aut>A. Montagner</aut>,
          <vrk>Sinistra e esperanto</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          2005-05-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="egal.0ismo.POL">egalismo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="snob.0ismo.KOMUNE">snobismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_ib;&c_t;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">elitisme</trd>
  <trd lng="cs">elit&aacute;&rcaron;stv&iacute;</trd>
  <trd lng="de">Elitismus</trd>
  <trd lng="en">elitism</trd>
  <trd lng="fr">&eacute;litisme</trd>
  <trd lng="pt">elitismo</trd>
  <trd lng="sk">elit&aacute;rstvo</trd>
</drv>

<drv mrk="elit.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="elit.0ulo.KOMUNE">
    <dif>
      Ano de <tld/>o:
      <ekz>
        distingita <tld/>ulo
        <fnt>
          <bib>ChV</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> estos sola, izolita
        en korpuso de la <tld/>uloj, kiuj superece rigardos liajn
        baraktojn kun milda surprizo, scivolante de kie li levi&gcirc;is en
        iliajn vicojn
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>uloj en la luksaj man&gcirc;ohaloj supre povas sin regali
        <fnt>
          <bib>KrM</bib>
          <lok>2. La &Jcirc;eto Estas Farita.</lok>
        </fnt>;
      </ekz>
      <ekz>
        Amelia aparte &scirc;atas eventojn en grandaj kasteloj, kie &scirc;i povas konversacii kun
        la <tld/>uloj de la irlanda socio
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2008/009326.php">Analoj de Bailenambeann</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k; <ind>&c_e;&c_l;&c_ib;&c_t;&c_y;</ind></trd>
  <trd lng="en">elite</trd>
  <trdgrp lng="fr">
    <trd>cador</trd>,
    <trd>ponte</trd>
  </trdgrp>
</drv>
