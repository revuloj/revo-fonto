<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aristolokiac</rad>/oj
</kap>
<drv mrk="aristo3.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="famili.0o.BIO">Familio</ref> de sta&ubreve;doj a&ubreve; lignoplantoj sinuantaj
      el la ordo de piperoparencaj
      <ref tip="lst" lst="voko:piperoparencoj" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Aristolochiaceae</trd>) kun:
      folioj alternaj, glatrandaj;
      floroj duseksaj, stelaj a&ubreve; dulipaj;
      tepaloj plataj, kunkreskintaj, tubecaj a&ubreve; klo&scirc;aj;
      6 &gcirc;is 12 stamenoj kolone kunkreskintaj kun la pistilo;
      ovolujo malsupra, kun 4 &gcirc;is 6 fakoj;
      stigmo sesradia.
    </dif>
  </snc>
  <trd lng="de">Osterluzeigew&auml;chse</trd>
  <trd lng="fr">aristolochiac&eacute;es</trd>
  <trd lng="hu">farkasalmaf&eacute;l&eacute;k</trd>
  <trd
lng="ru">&c_k;&c_i;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">piprankev&auml;xter</trd>
</drv>
</art>
