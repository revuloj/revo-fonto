<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aristo3.xml,v 1.11 2022/03/30 09:33:00 revo Exp $">
<kap>
  <rad>aristolokiac</rad>/oj
</kap>
<drv mrk="aristo3.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj"
      cel="famili.0o.BIO">Familio</ref> de sta&ubreve;doj a&ubreve; lignoplantoj
      sinuantaj
      el la ordo de piperoparencaj
      <ref tip="lst" lst="voko:piproparencoj"
      cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Aristolochiaceae</trd>) kun:
      folioj alternaj, glatrandaj;
      floroj duseksaj, stelaj a&ubreve; dulipaj;
      tepaloj plataj, kunkreskintaj, tubecaj a&ubreve; klo&scirc;aj;
      6 &gcirc;is 12 stamenoj kolone kunkreskintaj kun la pistilo;
      ovolujo malsupra, kun 4 &gcirc;is 6 fakoj;
      stigmo sesradia:
      <ekz>
        la <tld/>oj estas arbedoj, moltigaj plantoj a&ubreve; lianoj parencaj al unukotiledonaj
        plantoj (triparta floro)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19753"
              >Aristolokiacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="aristo2.0o">aristolokio</ref>,
       <ref cel="azar.0o">azaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="de">Osterluzeigew&auml;chse</trd>
  <trd lng="fr">aristolochiac&eacute;es</trd>
  <trd lng="hu">farkasalmaf&eacute;l&eacute;k</trd>
  <trd lng="ru">&c_k;&c_i;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">piprankev&auml;xter</trd>
</drv>
</art>
