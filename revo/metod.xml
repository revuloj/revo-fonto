<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>metod</rad>/o
</kap>
<drv mrk="metod.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Sisteme pripensita maniero procedi por atingi difinitan
      celon:
      <ekz>
        la rekta <tld/>&c_o; <klr>(instrui
        lingvojn)</klr>,
      </ekz>
      <ekz>
        la Cseh-<tld/>&c_o; <klr>(legu
        &leftquot;&Ccirc;e-metodo&rightquot;, la&ubreve; la nomo de la inventinto)</klr>;
      </ekz>
      <ekz>
        indukta, dedukta <tld/>o;
      </ekz>
      <ekz>
        praktika, logika, scienca <tld/>o.
      </ekz>
    </dif>
    <ref tip="super" cel="manier.0o">maniero</ref>
    <ref tip="malprt" cel="sistem.0o">sistemo</ref>
    <ref tip="sub" cel="algor.0o">algoritmo</ref>
    <ref tip="vid" cel="rimed.0o">rimedo</ref>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Methode</trd>,
    <trd>Verfahren</trd>,
    <trd>Verfahrensweise</trd>
  </trdgrp>
  <trd lng="en">method</trd>
  <trd lng="fr">m&eacute;thode</trd>
  <trdgrp lng="hu">
    <trd>m&oacute;dszer</trd>,
    <trd>elj&aacute;r&aacute;sm&oacute;d</trd>
  </trdgrp>
  <trd lng="nl">methode</trd>
  <trd lng="pt">m&eacute;todo</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_o;&c_d;</trd>
</drv>

<drv mrk="metod.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Aganta la&ubreve; <tld/>o:
      <ekz>
        <tld/>a spirito.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_je;&c_t;&c_o;&c_d;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Konforma al difinita <tld/>o:
      <ekz>
        <tld/>a gramatiko, parolado.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_je;&c_t;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>methodisch</trd>,
    <trd>planm&auml;&szlig;ig</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;thodique</trd>
  <trdgrp lng="hu">
    <trd>m&oacute;dszeres</trd>,
    <trd>tervszer&udblac;</trd>,
    <trd>metodikus</trd>
  </trdgrp>
  <trd lng="nl">methodisch</trd>
</drv>

<drv mrk="metod.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc>
    <dif>
      Kolekto da metodoj, reguloj kaj kutimoj praktikataj
      en iu fako:
      <ekz>
        <tld/>aro de scienca esploro;
      </ekz>
      <ekz>
        <tld/>aro de instruado.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="skol.0o">skolo</ref>
  <trd lng="be">&c_m;&c_e;&c_t;&c_o;&c_d;&c_y;&c_k;&c_a;</trd>
  <trd lng="en">methodology</trd>
  <trd lng="fr">m&eacute;thodologie</trd>
  <trdgrp lng="hu">
    <trd>m&oacute;dszertan</trd>,
    <trd>metodika</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_o;&c_d;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
