<drv mrk="metod.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="metod.0ema.KLA">
    <dif>
      Aganta la&ubreve; <tld/>o;
      <ref tip="sin" cel="metod.0a.KLA">metoda<sncref/></ref>:
      <ekz>
        <nom>Henri Dunant</nom> <klr>[&#x2026;]</klr>
        estas <tld/>ema kaj akordi&gcirc;ema
        <fnt>
          <aut>Claude Gacond</aut>,
          <vrk><url ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego_0106.pdf">
          La heroa vivo de Henri Dunant</url></vrk>,
          <lok>Radioprelego, 1964-12-09 kaj 12 (106)</lok>
        </fnt>;
      </ekz>
      <ekz>
        juna&gcirc;e <klr>[li]</klr> ekrenkontis Esperanton de <nom>Freinet</nom>-<tld/>ema instruisto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=534105">Jorgos</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="fr">m&eacute;thodique</trd>
