<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>venk</rad>/i
    </kap>

    <drv mrk="venk.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>x</vspec></gra>
      <snc mrk="venk.0i.batale">
        <dif>
          Superi malamikon en batalo, milito:
          <ekz>
            mi <tld/>is &ccirc;iujn kvin atakantojn
            <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
          </ekz><ekz>
            nia lando <tld/>os
            <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
          </ekz><ekz>
            <tld/>obati armeon<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>opreni urbon<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>siegen</trd>,
          <trd>besiegen</trd>
        </trdgrp>
        <trd lng="fr">d&eacute;faire<klr> (vaincre)</klr></trd>
      </snc>
      <snc mrk="venk.0i.konkurse">
        <dif>
          Superi konkuranton:
          <ekz>
            la la&ubreve;reato facile <tld/>is &ccirc;iujn poetojn en la
            konkurso<fnt>B</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>siegen</trd>,
          <trd>besiegen</trd>
        </trdgrp>
      </snc>
      <snc mrk="venk.0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Transpasi malhelpa&jcirc;on, baron:
          <ekz>
            sukcesa <tld/>anto de pordoj malfermitaj
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            li neniam <tld/>is la alfabeton
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <tld/>i &ccirc;iujn barojn<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i malfacila&jcirc;on<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i sian timon, konsternon, multajn anta&ubreve;ju&gcirc;ojn;
          </ekz><ekz>
            la scivolemo <tld/>is la singardon;
          </ekz><ekz>
            la partiaj pasioj <tld/>is super la prudento;
          </ekz><ekz>
            lia insisto <tld/>is mian &scirc;anceli&gcirc;on;
          </ekz><ekz>
            la vero &ccirc;iam <tld/>as
            <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
          </ekz><ekz>
            la <tld/>itoj de la vivo;
          </ekz><ekz>
            se min ne malhelpos ne<tld/>eblaj cirkonstancoj, mi penos
            veni<fnt>Z</fnt>;
          </ekz><ekz>
            ni povos super<tld/>i la krizon.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="pont.trans0igi.FIG">transpontigi</ref>
        </refgrp>
        <refgrp tip="vid">
          <ref cel="triumf.0i">triumfi</ref>,
        </refgrp>
        <trd lng="de">&uuml;berwinden</trd>
      </snc>
      <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_m;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
      <trd lng="el">&ny;&jota;&kappa;&omega_ton;</trd>
      <trd lng="en">conquer</trd>
      <trd lng="fr">vaincre</trd>
      <trdgrp lng="hu">
        <trd>gy&odblac;z</trd>,
        <trd>legy&odblac;z</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>overwinnen</trd>,
        <trd>winnen</trd>
      </trdgrp>
      <trd lng="pt">vencer</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_b;&c_je;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_o;&c_b;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="venk.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Sukceso en batalo a&ubreve; konkurso:
          <ekz>
            atingi, gajni la <tld/>on.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_m;&c_o;&c_g;&c_a;</trd>
      <trd lng="de">Sieg</trd>
      <trd lng="el">&ny;&jota_ton;&kappa;&eta;</trd>
      <trdgrp lng="fr">
        <trd>victoire</trd>
      </trdgrp>
      <trd lng="hu">gy&odblac;zelem</trd>
      <trd lng="nl">overwinning</trd>
      <trd lng="pt">vit&oacute;ria</trd>
      <trd lng="ru">&c_p;&c_o;&c_b;&c_je;&c_d;&c_a;</trd>
    </drv>

    <drv mrk="venk.mal0i">
      <kap>mal<tld/>i</kap>
      <snc>
        <dif>
          Malprosperi en batalo a&ubreve; konkurso, atingi malpli ol aliaj:
          <ekz>
            Venecio mal<tld/>is en milito kontra&ubreve; &Gcirc;enovo
            <fnt><aut>Wolfram Rohloff</aut>:
              <vrk>Voja&gcirc;ante kun Marko Polo
                (Marko Polo, La libro de la mirinda&jcirc;oj a&ubreve; priskribo de la
                mondo)</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 2002a, numero 12a, p. 20a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">subir une <ind>d&eacute;faite</ind></trd>
      <trdgrp lng="hu">
        <trd>vesz&iacute;t</trd>,
        <trd>veres&eacute;get szenved</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol;
<ind>&c_p;&c_o;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
    </drv>

    <drv mrk="venk.mal0o">
      <kap>mal<tld/>o</kap>
      <snc>
        <dif>
          Milita, batala a&ubreve; konkursa malsukceso:
          <ekz>
            suferi mal<tld/>on;
          </ekz><ekz>
            katastrofaj mal<tld/>oj en du mondmilitoj
            <fnt><aut>Stefan Maul</aut>:
              <vrk>Germanio-Auschwitz/Malbonaj konsciencoj</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 1995a, numero 3a, p. 5a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">d&eacute;faite</trd>
      <trd lng="hu">veres&eacute;g</trd>
      <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </drv>

    <drv mrk="venk.egal0o">
      <kap>egal<tld/>o <fnt><bib>CEE</bib></fnt></kap>
      <uzo tip="fak">SPO</uzo>
      <snc>
        <dif>
          Sportludo kiu fini&gcirc;is egale (mal)sukcese por amba&ubreve; partioj:
          <ekz>
            proponi, akcepti, malakcepti egal<tld/>on;
          </ekz><ekz>
            male al la okcidenta &scirc;ako, se vi patigas vian
            kontra&ubreve;ulon, vi <tld/>as kaj ne estas egal<tld/>o
            <fnt>
              <vrk><url ref="http://www.kisa.ca/shako/china.html"
                  >&Ccirc;ina &scirc;ako</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">draw<klr> (game)</klr></trd>
      <trd lng="ru">&c_n;&c_i;&c_ch;&c_mol;&c_ja;</trd>
    </drv>

  </art>
