<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>venk</rad>/i
</kap>
<drv mrk="venk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Superi malamikon en batalo, milito:
      <ekz>
        mi <tld/>is &ccirc;iujn kvin atakantojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nia lando <tld/>os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>obati armeon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>opreni urbon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>siegen</trd>,
      <trd>besiegen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Superi konkuranton:
      <ekz>
        la la&ubreve;reato facile <tld/>is &ccirc;iujn poetojn en la
        konkurso<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>siegen</trd>,
      <trd>besiegen</trd>
    </trdgrp>
  </snc>
  <snc mrk="venk.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Transpasi malhelpa&jcirc;on, baron:
      <ekz>
        sukcesa <tld/>anto de pordoj malfermitaj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li neniam <tld/>is la alfabeton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i &ccirc;iujn barojn<fnt>Z</fnt>,
      </ekz>
      <ekz>
        malfacila&jcirc;on<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sian timon, konsternon, multajn anta&ubreve;ju&gcirc;ojn;
      </ekz>
      <ekz>
        la scivolemo <tld/>is la singardon;
      </ekz>
      <ekz>
        la partiaj pasioj <tld/>is super la prudento;
      </ekz>
      <ekz>
        lia insisto <tld/>is mian &scirc;anceli&gcirc;on;
      </ekz>
      <ekz>
        la vero &ccirc;iam <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>itoj de la vivo;
      </ekz>
      <ekz>
        se min ne malhelpos ne<tld/>eblaj cirkonstancoj, mi penos
        veni<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni povos super<tld/>i la krizon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pont.trans0igi.FIG">transpontigi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="triumf.0i">triumfi</ref>,
    </refgrp>
    <trd lng="de">&uuml;berwinden</trd>
  </snc>
  <trd lng="en">conquer</trd>
  <trdgrp lng="fr">
    <trd>vaincre</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>overwinnen</trd>,
    <trd>winnen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_b;&c_je;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="venk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sukceso en batalo a&ubreve; konkurso:
      <ekz>
        atingi, gajni la <tld/>on;
      </ekz>
      <ekz>
        suferi mal<tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sieg</trd>
  <trdgrp lng="fr">
    <trd>victoire</trd>
  </trdgrp>
  <trd lng="nl">overwinning</trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_je;&c_d;&c_a;</trd>
</drv>
</art>
