<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: venk.xml,v 1.53 2023/10/21 16:50:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>venk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="venk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="venk.0i.batale">
    <dif>
      <ref tip="super" cel="super.0i">Superi</ref> malamikon en batalo, milito:
      <ekz>
        mi <tld/>is &ccirc;iujn kvin atakantojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        nia lando <tld/>os
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        mi luktis kun mia fratino, kaj mi <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ni batalos kontra&ubreve; ili sur ebena&jcirc;o, ni certe <tld/>os ilin
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 20:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o de <nom>Moab</nom> vidis, ke la batalo lin <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 3:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malamikoj provis <tld/>i nin per malsato
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la drakon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun popolon <klr>[&#x2026;]</klr> mi povos <tld/>obati <klr>[&#x2026;]</klr> kaj elpeli
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 22:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>opreni la urbon
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 32:18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">d&eacute;faire<klr> (vaincre)</klr></trd>
    <trdgrp lng="pl">
      <trd>wygra&cacute;</trd>,
      <trd>pokona&cacute;</trd>,
      <trd>zwyci&eogonek;&zdot;y&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="venk.0i.konkurse">
    <dif>
      Superi konkuranton:
      <ekz>
        kiu <tld/>os lian filinon en vetkurado
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>?
      </ekz>
      <ekz>
        <klr>[la]</klr> soldato <tld/>is en la veto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konservativa partio <tld/>is &ccirc;e la balotoj
        <fnt>
          <bib>FSp</bib>
          <lok>Tria Parto: Ligo De Nacioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        certe en la venonta vo&ccirc;donado mi <tld/>us lin finfine
        <fnt>
          <bib>ChB</bib>
          <lok>13</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gajn.0i.FIG">gajni</ref>
    </refgrp>
    <trd lng="de">gewinnen</trd>
    <trdgrp lng="pl">
      <trd>wygra&cacute;</trd>,
      <trd>zwyci&eogonek;&zdot;y&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="venk.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="pas.trans0i">Transpasi</ref> malhelpa&jcirc;on, baron:
      <ekz>
        oni diras, ke la vero &ccirc;iam <tld/>as
        <fnt>
          <bib>F</bib>
          <lok>&FE; 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u malbonon per bono
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 12:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[se tiu]</klr> la instruo perdis sian forton, la justeco neniam <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;abakuk 1:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is sian maldiligentecon: estis laborema kaj obeema
        <fnt>
          <bib>ElektFab</bib>
          <lok>La maljunulino Holle</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> la &scirc;oveliloj <tld/>as la kontra&ubreve;staron
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la seruro estis rusti&gcirc;inta <klr>[&#x2026;]</klr>, sed mi <tld/>is la malfacilon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is la revemon, saltis el la lito
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        prosperos al mi sisteme <tld/>i &ccirc;iujn barojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ne povas <tld/>i en la koro la amon al <nom>Vinicius</nom>
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        scivolo <tld/>is la singardemon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> penante <tld/>i la kreskantan en la brusto koleron, afable
        salutis
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        sukcesa <tld/>anto de pordoj malfermitaj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li neniam <tld/>is la alfabeton
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sukces.0i">sukcesi</ref>,
      <ref cel="pont.super0i.FIG">superponti</ref>,
      <ref cel="triumf.0i">triumfi</ref>
    </refgrp>
    <trd lng="de">&uuml;berwinden</trd>
    <trdgrp lng="pl">
      <trd>przezwyci&eogonek;&zdot;y&cacute;</trd>,
      <trd>pokona&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_m;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_pha;&t_ma;&t_tsheg;&t_nya;&t_drengbu;&t_sa;&t_tsheg;</trd>
  <trd lng="ca">v&egrave;ncer</trd>
  <trdgrp lng="cs">
    <trd>porazit</trd>,
    <trd>pot&rcaron;&iacute;t</trd>,
    <trd>p&rcaron;eb&iacute;t</trd>,
    <trd>p&rcaron;ekonat</trd>,
    <trd>p&rcaron;emoci</trd>,
    <trd>v&iacute;t&ecaron;zit</trd>,
    <trd>zv&iacute;t&ecaron;zit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>besiegen</trd>,
    <trd>siegen</trd>
  </trdgrp>
  <trd lng="el">&ny;&jota;&kappa;&omega_ton;</trd>
  <trdgrp lng="en">
    <trd>conquer</trd>,
    <trd>win</trd>
  </trdgrp>
  <trd lng="es">vencer</trd>
  <trd lng="fr">vaincre</trd>
  <trdgrp lng="hu">
    <trd>gy&odblac;z</trd>,
    <trd>legy&odblac;z</trd>
  </trdgrp>
  <trd lng="id">me<ind>menang</ind>kan</trd>
  <trd lng="it">vincere<klr> (battere, sconfiggere)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x52DD;&#x3064;  <pr>&#x304B;&#x3064;</pr></trd>,
    <trd>&#x6253;&#x3061;&#x7834;&#x308B;  <pr>&#x3046;&#x3061;&#x3084;&#x3076;&#x308B;</pr></trd>,
    <trd>&#x514B;&#x670D;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3075;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6253;&#x3061;&#x52DD;&#x3064;  <pr>&#x3046;&#x3061;&#x304B;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>overwinnen</trd>,
    <trd>winnen</trd>
  </trdgrp>
  <trd lng="pt">vencer</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_b;&c_je;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>porazi&tcaron;</trd>,
    <trd>prem&ocirc;c&tcaron;</trd>,
    <trd>zv&iacute;&tcaron;azi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_a;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_d;&c_o;&c_l;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="venk.0o">
  <kap><tld/>o</kap>
  <snc mrk="venk.0o.KOMUNE">
    <dif>
      <ref tip="vid" cel="sukces.0o">Sukceso</ref> en batalo a&ubreve;
      konkurso:
      <ekz>
        atingi, gajni la <tld/>on;
      </ekz>
      <ekz>
        kriado de <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 32:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o venas per multe da konsilantoj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 24:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        post &ccirc;iu <tld/>o kontra&ubreve; vi mem vi ricevos pli da forto
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistis por &scirc;i fine espero de <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        se ilia estos la <tld/>o, pereos ne nur vi, sed anka&ubreve; via dinastio
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_la;&t_tsheg;&t_kha;&t_tsheg;&t_tha;&t_naro;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="en">
    <trd>victory</trd>,
    <trd>conquest</trd>
  </trdgrp>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_m;&c_o;&c_g;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>vict&ograve;ria</trd>,
    <trd>triomf</trd>
  </trdgrp>
  <trd lng="cs">v&iacute;t&ecaron;zstv&iacute; <klr>(nad)</klr></trd>
  <trd lng="de">Sieg</trd>
  <trd lng="el">&ny;&jota_ton;&kappa;&eta;</trd>
  <trd lng="es">victoria</trd>
  <trd lng="fr">victoire</trd>
  <trd lng="hu">gy&odblac;zelem</trd>
  <trd lng="id">ke<ind>menang</ind>an</trd>
  <trd lng="it">vittoria</trd>
  <trd lng="ja">&#x52DD;&#x5229;  <pr>&#x3057;&#x3087;&#x3046;&#x308A;</pr></trd>
  <trd lng="nl">overwinning</trd>
  <trdgrp lng="pl">
    <trd>wygrana</trd>,
    <trd>zwyci&eogonek;stwo</trd>
  </trdgrp>
  <trd lng="pt">vit&oacute;ria</trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_je;&c_d;&c_a;</trd>
  <trd lng="sk">v&iacute;&tcaron;azstvo</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_m;&c_o;&c_g;&c_a;</trd>
</drv>

<drv mrk="venk.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="venk.mal0i.KOMUNE">
    <dif>
      Malprosperi en batalo a&ubreve; konkurso, atingi malpli ol aliaj:
      <ekz>
        kiam oni imagas sin la <tld/>into, &gcirc;uste tiam oni mal<tld/>as
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        Venecio mal<tld/>is en milito kontra&ubreve; &Gcirc;enovo
        <fnt>
          <aut>W. Rohloff</aut>,
          <vrk>Voja&gcirc;ante kun Marko Polo</vrk>,
          <bib>Monato</bib>,
          <lok>2002:12, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia teamo mal<tld/>is per 8-3
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en militoj &ccirc;iuj mal<tld/>as
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia populareco draste falis kaj li e&ccirc; mal<tld/>is en la partiaj anta&ubreve;balotoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="gajn.mal0i">malgajni</ref>,
      <ref cel="perd.0i">perdi</ref>
    </refgrp>
  </snc>
  <trd lng="bo">&t_sha;&t_naro;&t_ra;&t_tsheg;</trd>
  <trd lng="en">to be defeated</trd>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol; <ind>&c_p;&c_a;&c_r;&c_a;&c_z;&c_u;</ind></trd>,
    <trd>&c_p;&c_r;&c_a;&c_j;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>perdre</trd>,
    <trd>ser derrotat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t pora&zcaron;en</trd>,
    <trd>podlehnout</trd>,
    <trd>podl&eacute;hat</trd>,
    <trd>prohr&aacute;t</trd>,
    <trd>prohr&aacute;vat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verlieren</trd>,
    <trd>unterliegen</trd>
  </trdgrp>
  <trd lng="es">perder <klr>(sufrir una derrota)</klr></trd>
  <trd lng="fr">subir une <ind>d&eacute;faite</ind></trd>
  <trdgrp lng="hu">
    <trd>vesz&iacute;t</trd>,
    <trd>veres&eacute;get szenved</trd>
  </trdgrp>
  <trd lng="id">kalah</trd>
  <trd lng="it">perdere<klr tip="amb"> (essere sconfitto)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8CA0;&#x3051;&#x308B;  <pr>&#x307E;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x6557;&#x308C;&#x308B;  <pr>&#x3084;&#x3076;&#x308C;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przegrywa&cacute;</trd>,
    <trd>odnosi&cacute; <ind>pora&zdot;k&eogonek;</ind></trd>,
    <trd>ponosi&cacute; <ind>kl&eogonek;sk&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; <ind>&c_p;&c_o;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; porazen&yacute;</trd>,
    <trd>pod&lcaron;ahn&uacute;&tcaron; v&nbsp;boji</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_ib;&c_t;&c_i; &c_p;&c_o;&c_r;&c_a;&c_z;&c_k;&c_u;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_m;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="venk.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="venk.mal0o.KOMUNE">
    <dif>
      Milita, batala a&ubreve; konkursa malsukceso:
      <ekz>
        suferi mal<tld/>on;
      </ekz>
      <ekz>
        malgranda mal<tld/>o <klr>[&#x2026;]</klr> estus leciono de prudento kaj humileco
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni devas taksi &ccirc;iun homon ne la&ubreve; lia <tld/>o a&ubreve; mal<tld/>o, sed la&ubreve; liaj laboroj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o a&ubreve; mal<tld/>o dependas de multaj cirkonstancoj, sed anta&ubreve; &ccirc;io de la
        lerteco de la oratoro
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#x2026;]</klr> ne konfesas mian mal<tld/>on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        katastrofaj mal<tld/>oj en du mondmilitoj
        <fnt>
          <aut>S. Maul</aut>,
          <vrk>Germanio-Auschwitz...</vrk>,
          <bib>Monato</bib>,
          <lok>1995:3, p. 5a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">defeat</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_j;&c_g;&c_r;&c_y;&c_sh;</trd>
  </trdgrp>
  <trd lng="ca">derrota</trd>
  <trdgrp lng="cs">
    <trd>por&aacute;&zcaron;ka</trd>,
    <trd>prohra</trd>
  </trdgrp>
  <trd lng="de">Niederlage</trd>
  <trd lng="es">derrota</trd>
  <trd lng="fr">d&eacute;faite</trd>
  <trd lng="hu">veres&eacute;g</trd>
  <trd lng="id">ke<ind>kalah</ind>an</trd>
  <trd lng="it">sconfitta</trd>
  <trdgrp lng="ja">
    <trd>&#x6557;&#x5317;  <pr>&#x306F;&#x3044;&#x307C;&#x304F;</pr></trd>,
    <trd>&#x6557;&#x6226;  <pr>&#x306F;&#x3044;&#x305B;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przegrana</trd>,
    <trd>pora&zdot;ka</trd>,
