<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kulas.xml,v 1.23 2024/04/17 07:23:31 revo Exp $">
<kap>
  <rad>kulas</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kulas.0o">
  <kap><tld/>o</kap>
  <snc mrk="kulas.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ekstrema parto de <ref tip="malprt" cel="tub.pafil0o">paftubo</ref>, kiu ricevas la pafotan
      karto&ccirc;on
      a&ubreve; obuson, kaj kie okazas la pulveksplodo:
      <ekz>
        movebla <tld/>o;
      </ekz>
      <ekz>
        <tld/>&scirc;arga pafilo;
      </ekz>
      <ekz>
        li faris noton de la numero stampita sur la <tld/>o de la fusilo
        <fnt>
          <aut>Ronald Cecil Gates</aut>
          <vrk><url ref="https://books.google.de/books?id=IP0AtNT3uiUC&amp;pg=PA92">Mortiga
          Ekskurso</url></vrk>
          <lok>15. Tria Aresto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>bloko</ind> <klr>(kiu fermas la <tld/>aperturon)</klr>;
        <trd lng="ru">&c_z;&c_a;&c_t;&c_v;&c_o;&c_r;</trd>
      </ekz>
      <ekz>
        <tld/>riglilo <klr>(kiu tenas &gcirc;in fermita dum la pafo)</klr>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="paf.0ilo">pafilo</ref>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_z;&c_jo;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_k;&c_a;&c_z;&c_jo;&c_n;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; &c_s;&c_t;&c_v;&c_a;&c_l;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">recambra</trd>
    <trdgrp lng="de">
      <trd>Gesch&uuml;tzverschluss</trd>,
      <trd>Bodenst&uuml;ck</trd>,
      <trd>Gewehrverschluss</trd>,
      <trd>Gewehrschloss</trd>,
      <trd>Kammer <klr>(Gesch&uuml;tz)</klr></trd>,
      <trd>Patronenlager</trd>
    </trdgrp>
    <trd lng="en">breech</trd>
    <trdgrp lng="hu">
      <trd>z&aacute;r<klr tip="amb"> (fegyveren)</klr></trd>,
      <trd>z&aacute;v&aacute;rzat</trd>,
      <trd>z&aacute;rszerkezet</trd>
    </trdgrp>
    <trd lng="it">culatta</trd>
    <trd lng="nl">kulas</trd>
    <trd lng="pl">komora</trd>
    <trd lng="pt">culatra</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_z;&c_jo;&c_n;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;<klr>
      (&c_o;&c_g;&c_n;&c_je;&c_s;&c_t;&c_r;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o; &c_o;&c_r;&c_u;&c_zh;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_z;&c_a;&c_t;&c_v;&c_o;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x67AA;&#x819B; <pr>qi&amacron;ngt&#xE1;ng</pr></trd>,
    <trd>&#x69CD;&#x819B; <pr>qi&amacron;ngt&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="kulas.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">AUT</uzo>
    <dif>
      Supera parto de eksplodmotoro, kie okazas la eksplodo
      de la karbura&jcirc;o; kapo de <ref tip="malprt" cel="cilind.0o.TEK">cilindro</ref>:
      <ekz>
        la sparkilo estas en&scirc;ra&ubreve;bita en la <tld/>o de la motoro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=490950">Sparkilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a; <klr tip="amb">(&c_c;&c_y;&c_l;&c_ib;&c_n;&c_d;&c_r;&c_a; &c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;&c_a;)</klr></trd>
    <trd lng="ca">culata</trd>
    <trd lng="de">Zylinderkopf</trd>
    <trd lng="hu">hengerfej</trd>
    <trd lng="it">testata</trd>
    <trd lng="nl">cilinderkop</trd>
    <trd lng="pl">g&lstroke;owica</trd>
    <trdgrp lng="pt">
      <trd>culatra</trd>,
      <trd>cabe&ccedil;a de <ind>cilindro</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a; &c_c;&c_i;&c_l;&c_i;&c_n;&c_d;&c_r;&c_a; <klr>(&c_m;&c_o;&c_t;&c_o;&c_r;&c_a;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x6C7D;&#x7F38;&#22836; <pr>q&#xEC;g&amacron;ngt&#xF3;u</pr></trd>,
    <trd>&#x6C7D;&#x7F38;&#x982D; <pr>q&#xEC;g&amacron;ngt&#xF3;u</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">n&aacute;bojov&aacute; komora</trd>
  <trd lng="fr">culasse<klr> (arme, automobile)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#37507;&#x5C3E;  <pr>&#12376;&#12421;&#12358;&#x304A;</pr></trd>,
    <trd>&#30770;&#x5C3E;  <pr>&#x307B;&#12358;&#12403;</pr></trd>,
    <trd>&#x904A;&#x5E95;  <pr>&#12422;&#12358;&#12390;&#12356;</pr></trd>,
    <trd>&#x30B7;&#x30EA;&#x30F3;&#x30C0;&#x30FC;&#x30D8;&#x30C3;&#x30C9;</trd>
  </trdgrp>
  <trd lng="ro">baz&abreve;</trd>
  <trdgrp lng="sk">
    <trd>1.z&aacute;ver strelnej zbrane</trd>,
    <trd>2.hlava valca</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_z;&c_je;&c_n;&c_n;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a; (&c_v;&c_o;&c_g;&c_n;&c_je;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_ji; &c_z;&c_b;&c_r;&c_o;&c_ji;)</trd>,
    <trd>&c_z;&c_a;&c_t;&c_v;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a; &c_c;&c_i;&c_l;&c_ib;&c_n;&c_d;&c_r;&c_a; (&c_m;&c_o;&c_t;&c_o;&c_r;&c_a;)</trd>
  </trdgrp>
</drv>
</art>
