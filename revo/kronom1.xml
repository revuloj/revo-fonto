<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kronometri</rad>/o
</kap>
<drv mrk="kronom1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Parto de la fiziko, pritraktanta la mezuron de la tempo.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Chronometrie</trd>,
     <trd>Zeitmessung</trd>
  </trdgrp>
  <trd lng="en">chronometry</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="kronom1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>chronometrisch</trd>,
     <trd>zeitlich (eingeordnet)</trd>
  </trdgrp>
  <trd lng="en">chronometric</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
