<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Marten</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="marten.0o">
  <kap><tld/>o</kap>
  <snc mrk="marten.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        al li venas la kura&gcirc;a maristo <tld/>o Alonzo Pinzon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Martin</trd>
  <trd lng="nl">Maarten</trd>
  <trdgrp lng="pt">
    <trd>Martinho</trd>,
    <trd>Martim</trd>,
    <trd>Martins</trd>,
    <trd>Martiniano</trd>,
    <trd>Martino</trd>    
  </trdgrp>
</drv>
</art>

