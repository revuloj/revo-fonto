  <trd lng="hu">teremtm&eacute;ny</trd>
  <trd lng="nl">schepsel</trd>
  <trd lng="pl">istota</trd>     
  <trd lng="ro">fiin&#x21B;&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_zh;&c_i;&c_v;&c_o;&c_je;)</klr></trd>,
