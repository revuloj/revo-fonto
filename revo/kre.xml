<drv mrk="kre.0o">
  <kap><tld/>o</kap>
  <snc mrk="kre.0o.KOMUNE">
    <dif>
      Estigo, elfarado:
      <ekz>
        venki &ccirc;iujn barojn por la <tld/>o kaj enkonduko en uzadon de
        racia
        lingvo internacia
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, el la unua
        libro de la lingvo esperanto</lok></fnt>;
      </ekz>
      <ekz>
        tiam sonis &ccirc;iuj valsoj, polkoj kaj mazurkoj, kiujn oni iam
        konis depost la <tld/>o de la mondo
        <fnt><bib>Fab2</bib>, <lok>pa&scirc;tisto de porkoj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Erschaffung</trd>,
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>tvorba</trd>,
    <trd>tvo&rcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="en">creation<klr tip="ind"> (action)</klr></trd>
  <trd lng="fr">cr&eacute;ation<klr tip="amb"> (acte)</klr></trd>
  <trdgrp lng="nl">
    <trd>creatie<klr tip="ind"> (daad)</klr></trd>,
    <trd>schepping<klr tip="ind"> (daad)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kreacja</trd>,
    <trd>stworzenie</trd>,
    <trd>tworzenie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>creare</trd>,
    <trd>creativitate</trd>,
    <trd>f&abreve;ptur&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>,
    <trd>&c_t;&c_v;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tvorba</trd>,
    <trd>tvorenie</trd>
  </trdgrp>
</drv>

<drv mrk="kre.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kre.0ajxo.KOMUNE">
    <dif>
      Io, kion iu <tld/>is:
      <ekz>
        la <tld/>a&jcirc;o de <nom>Zamenhof</nom> estas nun mirinda lingvo
        <fnt><bib>Lanti</bib>, <lok>nia pozicio</lok></fnt>;
      </ekz>
      <ekz>
        la dioj ne rigardas tian <tld/>a&jcirc;on, kiu havas manojn nur por
        la
        laboro, bu&scirc;on nur por la ploro, kaj dorson por la bastonoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        neniu <tld/>a&jcirc;o estas por mi tiel abomena, kiel
        araneo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kreatur</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_je;&c_sh;&c_t;&c_a;)</klr></trd>,
    <trd>&c_t;&c_v;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>creation<klr tip="ind"> (result)</klr></trd>,
    <trd>creature</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cr&eacute;ation<klr tip="amb"> (chose cr&eacute;&eacute;e)</klr></trd>,
    <trd>cr&eacute;ature</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>alkot&aacute;s</trd>,
    <trd>teremt&eacute;s<klr tip="ind">(teremtett dolog)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>creatie<klr tip="ind"> (resultaat)</klr></trd>,
    <trd>schepping<klr tip="ind"> (resultaat)</klr></trd>,
    <trd>schepsel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tw&oacute;r</trd>,
    <trd>wytw&oacute;r</trd>,
    <trd>wyr&oacute;b</trd>,
    <trd>stworzenie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>produs</trd>,
    <trd>crea&#x21B;ie</trd>,
    <trd>munc&abreve;</trd>,
    <trd>lucru</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_n;&c_je;&c_ch;&c_t;&c_o;)</klr></trd>,
    <trd>&c_t;&c_v;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_n;&c_je;&c_ch;&c_t;&c_o;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kre.0emo">
  <kap><tld/>emo</kap>
  <snc mrk="kre.0emo.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Impulso kaj kapablo por <tld/>i:
      <ekz>
        la malnova lernejo havigas nur unuflankan kleron;
        &gcirc;i precipe formas, disvolvas memoron kaj intelekton;
        &gcirc;i malzorgas pri la produktemo, <tld/>emo de l' infano
        <fnt>
          <aut>[P.?] Trarbach</aut>,
          <vrk>La laborolernejo</vrk>,
          <lok>Esperanto Triumfonta, 1922-11-05 (109)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kreativit&auml;t</trd>
  </snc>
  <trd lng="fr">cr&eacute;ativit&eacute;</trd>
  <trd lng="pl">ch&eogonek;&cacute; tw&oacute;rcza</trd>     
  <trd lng="ro">nevoia de crea&#x21B;ie</trd>
</drv>

<drv mrk="kre.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kre.0igxi.KOMUNE">
    <dif>
      Esti&gcirc;i, komenci&gcirc;i, aperi:
      <ekz>
        brilantaj perspektivoj <tld/>i&gcirc;is anta&ubreve; &scirc;iaj okuloj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is kondi&ccirc;oj por plilar&gcirc;i&gcirc;o de la homaj
        grupi&gcirc;oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><ind>erschaffen</ind> werden</trd>,
      <trd>entstehen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>be created</trd>,
    <trd>come into being</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>apparaitre</trd>,
    <trd>appara&icirc;tre</trd>,
    <trd>se <ind>cr&eacute;er</ind></trd>,
    <trd>se <ind>former</ind></trd>
  </trdgrp>
  <trd lng="nl">ontstaan</trd>
  <trdgrp lng="pl">
    <trd>sta&cacute; si&eogonek;</trd>,
    <trd>stworzy&cacute; si&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;nt&acirc;mpla</trd>,
    <trd>petrece</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_ja;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kre.0into">
  <kap>
    <tld/>into
  </kap>
  <snc mrk="kre.0into.iu">
    <dif>
      Tiu, kiu <tld/>is ion:
      <ekz>
        Dio Plejsupra, la <tld lit="K"/>into de la &ccirc;ielo kaj la tero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 14:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="K"/>into de la naturo, kiu multe superas la plej glorajn
        <tld/>ita&jcirc;ojn kaj konstruojn de la homo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ne volas esti <tld/>into de l' lingvo, mi volas nur esti iniciatoro
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Linus Torvalds</nom>, la <tld/>into de Linukso
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007114.html">Intel alfrontas konkurencon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="di.0o.monoteisma">Dio</ref>,
       <ref cel="verk.0isto">verkisto</ref>,
       <ref cel="auxtor.0o">a&ubreve;toro</ref>,
       <ref cel="invent.0isto">inventisto</ref>
    </refgrp>
  </snc>
