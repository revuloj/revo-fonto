<drv mrk="kre.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="kre.0o.KOMUNE">
    <dif>
      Estigo, elfarado:
      <ekz>
        venki &ccirc;iujn barojn por la <tld/>o kaj enkonduko en uzadon de
        racia
        lingvo internacia
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, el la unua
        libro de la lingvo esperanto</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>ado de tute plena vortaro estas laboro ne ebla por unu homo, &ccirc;ar la
        nombro de l' vortoj <klr>[&#8230;]</klr> estas senfina
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi sentis, ke en mia cerbo estas &ccirc;io: la vivo kaj la morto, la pereo kaj la
        <tld/>o &dash; la tuta universo
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiam sonis &ccirc;iuj valsoj, polkoj kaj mazurkoj, kiujn oni iam
        konis depost la <tld/>o de la mondo
        <fnt><bib>Fab2</bib>, <lok>pa&scirc;tisto de porkoj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Erschaffung</trd>,
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_u;&c_t;&c_v;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tvorba</trd>,
    <trd>tvo&rcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="en">creation<klr tip="ind"> (action)</klr></trd>
  <trd lng="fr">cr&eacute;ation<klr tip="amb"> (acte)</klr></trd>
  <trdgrp lng="id">
    <trd><baz>cipta</baz>penciptaan</trd>,
    <trd><baz>diri</baz>pendirian</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21109;&#36896;  <pr>&#x305D;&#12358;&#x305E;&#12358;</pr></trd>,
    <trd>&#21109;&#x4F5C;  <pr>&#x305D;&#12358;&#12373;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>creatie<klr tip="ind"> (daad)</klr></trd>,
    <trd>schepping<klr tip="ind"> (daad)</klr></trd>
