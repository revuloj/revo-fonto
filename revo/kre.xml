<drv mrk="kre.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kre.0igxi.KOMUNE">
    <dif>
      <ref tip="super" cel="est.0igxi">Esti&gcirc;i</ref> per iu
      (a&ubreve; eventuale per evoluo),
      <ref cel="komenc.0igxi">komenci&gcirc;i</ref>, <ref tip="super" cel="aper.0i">aperi</ref>:
      <ekz>
        Li ordonis, kaj ili <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 148:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estis preta en la tago de via <tld/>i&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 28:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie la grandaj bran&ccirc;oj dividi&gcirc;as, <tld/>i&gcirc;is plenforme malgranda pendanta
        &gcirc;ardeno kun framboj kaj birdoherbo
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is tuta fabelo, <tld/>i&gcirc;is multaj, ilin rakontu aliaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        brilantaj perspektivoj <tld/>i&gcirc;is anta&ubreve; &scirc;iaj okuloj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la lingvo de &ccirc;iu nacio <tld/>i&gcirc;is tiam, kiam la nacio ne sciis ankora&ubreve; skribi
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is internacia movado
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
