<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sponta1.xml,v 1.27 2025/12/30 08:42:55 revo Exp $">
<kap>
  <ofc>3</ofc><rad>spontane</rad>/a <fnt><bib>PVEF</bib></fnt>,
  <var><kap><rad var="n">spontan</rad>/a <fnt><bib>V</bib></fnt></kap></var>,
  <var><kap><rad var="t">spont</rad>/a <fnt><bib>PGl</bib></fnt></kap></var>
</kap>
<drv mrk="sponta1.0a">
  <kap><tld/>a,
  <var><kap><tld var="n"/>a</kap></var>,
  <var><kap><tld var="t"/>a</kap></var></kap>
  <snc mrk="sponta1.0a.per_si_mem">
    <dif>
      Fari&gcirc;anta per si mem, sen ia ekstera influo; <ref cel="refleks.0o">refleksa</ref>,
      <ref tip="sin" cel="pens.senpri0a">senpripensa</ref>,
      <ref tip="sin" cel="plan.sen0a">senplana</ref>:
      <ekz>
        <tld/>a generado
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili kutimi&gcirc;is al zorge preparitaj prelegoj
        anstata&ubreve; <tld/>a diskuto kia okazas nun
        <fnt>
          <aut>Platono, tr. D. Broadribb</aut>:
          <vrk>La Respubliko</vrk>. <lok>Parto 6</lok>.
        </fnt>;
      </ekz><ekz>
        la mutacioj povas esti <tld/>aj, sen videblaj a&ubreve;
        rekoneblaj eksteraj instigoj
        <fnt>
          <vrk><url ref="http://www.fw.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm">Enkonduko en la
          biologion</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj vivantaj lingvoj pli kaj pli simpli&gcirc;is, kaj <tld/>e, kaj la&ubreve;plane
       <fnt>
          <aut>Ott&oacute; Haszpra</aut>
          <vrk><url ref="&Monato;/2000/006803.php">REAGO:
            Supersignoj, pro kiu diabla ka&ubreve;zo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/03, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="n"/>a alparolo
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mia respondo estis <tld var="n"/>a: &leftquot;mi ne scias&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2006/009046.php">Mia religio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        neniam mi planas ion anta&ubreve;e, la ideoj &ccirc;iam venas <tld var="n"/>e en mian kapon
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008445.html">Kolekti antikva&jcirc;ojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        infana <tld var="n"/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009307.php">La neforgesebla Ada</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
          son&gcirc;as pure kaj senhonte &parsl;
          pri Nialo bronzabrova &parsl;
          kiu &scirc;in prila&ubreve;dos <tld var="t"/>e, &parsl;
          kiam kuros &scirc;i renkonte &parsl;
          en la blua robo nova
          <fnt>
            <aut>John Francis</aut>:
            <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr44/hebridea.html">Hebridea
            Romanco</url></vrk>,
            <lok>La nica literatura revuo, n-ro 22 (4:4, 1959)</lok>
          </fnt>.
        </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_a;&c_d;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_h;&c_ib;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>samovoln&yacute;</trd>,
    <trd>spont&aacute;nn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spontan</trd>,
    <trd>unvermittelt</trd>
  </trdgrp>
  <trd lng="en">spontaneous</trd>
  <trd lng="fr">spontan&eacute;</trd>
  <trdgrp lng="hu">
    <trd>spont&aacute;n</trd>,
    <trd>&ouml;nk&eacute;nytelen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#x767A;&#x7684;&#x306A;  <pr>&#x3058;&#x306F;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x81EA;&#x7136;&#x306B;&#x8D77;&#x3053;&#x308B;  <pr>&#x3057;&#x305C;&#x3093;&#x306B;&#x304A;&#x3053;&#x308B;</pr></trd>,
    <trd>&#x81EA;&#x7136;&#x767A;&#x751F;&#x7684;&#x306A;  <pr>&#x3057;&#x305C;&#x3093;&#x306F;&#x3063;&#x305B;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x81EA;&#x751F;&#x306E;  <pr>&#x3058;&#x305B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">spontaan</trd>
  <trd lng="pl">spontaniczny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_h;&c_i;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>samovo&lcaron;n&yacute;</trd>,
    <trd>spont&aacute;nny</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#xE0B;&#xE36;&#xE48;&#xE07;&#xE40;&#xE1B;&#xE47;&#xE19;&#xE44;&#xE1B;&#xE15;&#xE32;&#xE21;&#xE18;&#xE23;&#xE23;&#xE21;&#xE0A;&#xE32;&#xE15;&#xE34;</trd>,
    <trd>&#xE40;&#xE01;&#xE34;&#xE14;&#xE02;&#xE36;&#xE49;&#xE19;&#xE40;&#xE2D;&#xE07;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_m;&c_o;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_h;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E3B;&#x52A8; <pr>zh&#x1D4;d&ograve;ng</pr></trd>,
    <trd>&#x4E3B;&#x52D5; <pr>zh&#x1D4;d&ograve;ng</pr></trd>,
    <trd>&#x81EA;&#x53D1; <pr>z&igrave;f&amacron;</pr></trd>,
    <trd>&#x81EA;&#x767C; <pr>z&igrave;f&amacron;</pr></trd>,
    <trd>&#x4E00;&#x65F6;&#x51B2;&#x52A8; <pr>y&igrave;sh&iacute;ch&omacron;ngd&ograve;ng</pr></trd>,
    <trd>&#x4E00;&#x6642;&#x885D;&#x52D5; <pr>y&igrave;sh&iacute;ch&omacron;ngd&ograve;ng</pr></trd>,
    <trd>&#x4E0D;&#x7531;&#x81EA;&#x4E3B;&#x7684; <pr>b&ugrave;y&oacute;uz&igrave;zh&#x1D4;de</pr></trd>,
    <trd>&#x81EA;&#x751F; <pr>z&igrave;sh&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sponta1.0eco">
  <kap>
    <tld/>eco,
    <var><kap><tld var="n"/>eco</kap></var>,
    <var><kap><tld var="t"/>eco</kap></var>
  </kap>
  <snc>
    <dif>
      Stato, eco de iu a&ubreve; io <tld/>a:
      <ekz>
        kiam &scirc;i parolis, komplete mankis <tld var="n"/>eco
        <fnt>
          <bib>ChV</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eco, &gcirc;ojo kaj pureco &ccirc;eestas en la muziko de tiu sveda grupo
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spontanit&auml;t</trd>
</drv>

</art>
