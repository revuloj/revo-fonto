<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: lauxzan.xml,v 1.2 2024/08/11 18:35:13 afido Exp $">
    <kap>
      <rad>La&ubreve;zan</rad>/o <fnt><bib>PIV1</bib></fnt>,
      <var><kap><rad var="o">Lozan</rad>/o</kap></var>
    </kap>

<drv mrk="lauxzan.0o">
  <kap><tld/>o, <var><kap><tld var="o"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Urbo &ccirc;e la norde bordo de <ref cel="leman.0o">Lemano</ref>,
      &ccirc;efa urbo de <ref tip="malprt" cel="vauxd.0o">Va&ubreve;do</ref>:
      <ekz>
        indignantoj preferis organizi la manifestaciojn sur la alia, svisa flanko de la
        lago, en &Gcirc;enevo kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Piron</aut>
          <vrk><url ref="&Monato;/2003/008141.html"
              >Francio invitas, Svislando suferas</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        ricevinte la katedron de latina literaturo en <tld var="o"/>o (Svisujo), M. prelegas tie
        <fnt>
          <bib>SrT</bib>
          <lok>Anta&ubreve;parolo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a Esperanto-Societo
        <fnt>
          <bib>Kon11</bib>
          <lok>Reagoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lausanne</trd>
    <trd lng="en">Lausanne</trd>
    <trd lng="fr">Lausanne</trd>
    <trd lng="ru">&c_L;&c_o;&c_z;&c_a;&c_n;&c_n;&c_a;</trd>
  </snc>
<trd lng="be">&c_L;&c_ja;&c_z;&c_a;&c_n;&c_a;</trd>
</drv>

  </art>

