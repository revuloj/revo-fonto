<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Harlem</rad>/o
</kap>
<drv mrk="harlem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="harlem.0o.nl">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la nederlanda provinco
      <ref tip="malprt" cel="holand.nord0o">Nord-Holando</ref>
      (4&ring;18'E, 52&ring;05'N):
      <ekz>
        kaj se vi jam havos monon, iru al <tld/>o, tie anka&ubreve; mi estis,
        same &ccirc;i tiel!
        <fnt><aut>S&aacute;ndor BR&Oacute;DY, elhungarigis d-ro B. SZEMENYEI
        </aut>:
        <vrk>Rembrandt</vrk>,
        <lok>Norda Prismo, 56/6, pa&gcirc;oj 229-238</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_H;&c_a;&c_r;&c_l;&c_je;&c_m;</trd>
  </snc>
  <snc mrk="harlem.0o.us">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en Usono.
    </dif>
    <trd lng="ru">&c_G;&c_a;&c_r;&c_l;&c_je;&c_m;</trd>
  </snc>
  <trd lng="en">Harlem</trd>
  <trd lng="fr">Harlem</trd>
  <trd lng="hu">Harlem</trd>
  <trd lng="nl">Haarlem</trd>
</drv>
</art>
