<art mrk="$Id: harlem.xml,v 1.16 2023/12/06 10:33:08 revo Exp $">
<kap>
  <rad>Harlem</rad>/o
</kap>
<drv mrk="harlem.H0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="harlem.H0o.nl">
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref> en la nederlanda provinco
      <ref tip="malprt" cel="holand.NordH0o">Nord-Holando</ref>
      (4&deg;18'E, 52&deg;05'N):
      <ekz>
        kaj se vi jam havos monon, iru al <tld/>o, tie anka&ubreve; mi
        estis, same &ccirc;i tiel!
        <fnt><aut>
        S&aacute;ndor BR&Oacute;DY, elhungarigis d-ro B. SZEMENYEI
        </aut>:
        <vrk>Rembrandt</vrk>,
        <lok>Norda Prismo, 56/6, pa&gcirc;oj 229-238</lok>
        </fnt>;
      </ekz>
      <ekz>
la festparolado de Ivo Lapenna por la 39a UK en <tld/>o
 <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2007/009131.php">Tri diskoj kun ses paroladoj de Ivo
          Lapenna
</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_a;&c_a;&c_r;&c_l;&c_je;&c_m; <klr>(&c_u; &c_G;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_ib;)</klr></trd>
    <trd lng="ru">&c_H;&c_a;&c_r;&c_l;&c_je;&c_m;</trd>
  </snc>
  <snc mrk="harlem.H0o.us">
    <dif>
      <ref tip="super" cel="kvarta.0o">Kvartalo</ref> de
      <ref tip="malprt" cel="jork.nov0o">Novjorko</ref>.
    </dif>
    <adm>Cxu ne temas pri nura urbero de Novjorko? (Ulriko)</adm>
    <trd lng="be">&c_G;&c_a;&c_r;&c_l;&c_je;&c_m; <klr>(&c_u; &c_N;&c_ju;-&c_Jo;&c_r;&c_k;&c_u;)</klr></trd>
    <trd lng="ru">&c_G;&c_a;&c_r;&c_l;&c_je;&c_m;</trd>
  </snc>
  <trd lng="de">Harlem</trd>
  <trd lng="en">Harlem</trd>
  <trd lng="fr">Harlem</trd>
  <trd lng="hu">Harlem</trd>
  <trd lng="it">Harlem</trd>
  <trd lng="nl">Haarlem</trd>
  <trd lng="pl">Harlem</trd>
  <trd lng="pt">Harlem</trd>
  <trdgrp lng="zh">
    <trd>&#x54C8;&#29747; <pr>h&amacron;l&#xED;n</pr></trd>,
    <trd>&#x54C8;&#x83B1;&#x59C6;&#x533A; <pr>h&amacron;l&#xE1;im&#x1D4;q&#x16B;</pr></trd>
  </trdgrp>
</drv>
</art>
