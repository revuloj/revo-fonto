<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>citr</rad>/o
</kap>
<drv mrk="citr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="citr.0o.antikva">
    <dif>
      Diversforma precipe antikva senkola
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>,
      kun sonkesto.
    </dif>
    <trdgrp lng="en">
      <trd>zither</trd>
    </trdgrp>
    <trd lng="fr">cithare</trd>
    <trd lng="nl">citer</trd>
    <trd lng="pt">c&iacute;tara</trd>
    <trd lng="ru">&c_c;&c_i;&c_t;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="citr.0o.zitero">
  <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="ziter.0o">zitero</ref>
  </snc>
</drv>
</art>
