<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fermat</rad>/o
</kap>
<drv mrk="fermat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Signo, kiu haltigas momente la movon k montras,
      ke la da&ubreve;ro de la noto k de la silento, super kiu &gcirc;i
      estas lokita, povas esti plilongigata.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Fermate</trd>,
     <trd>Haltezeichen</trd></trdgrp>
  <trdgrp lng="en">
    <trd>fermata</trd>,
    <trd>hold</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;</trd>
</drv>
</art>
