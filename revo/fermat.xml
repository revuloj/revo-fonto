<vortaro>
<art mrk="$Id$">
<kap>
<rad>fermat</rad>/o
</kap>
<drv mrk="fermat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Signo, kiu haltigas momente la movon k montras,
      ke la da&ubreve;ro de la noto k de la silento, super kiu &gcirc;i
      estas lokita, povas esti plilongigata.  
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_r;&c_m;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Fermate</trd>,
     <trd>Haltezeichen</trd></trdgrp>
  <trdgrp lng="en">
    <trd>fermata</trd>,
    <trd>hold</trd>
  </trdgrp>
  <trd lng="fr">point d'orgue</trd>
  <trdgrp lng="hu">
    <trd>korona<klr tip="ind"> (zene)</klr></trd>,
    <trd>ferm&aacute;ta</trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>fermate</trd>,
     <trd>pauzeteken</trd></trdgrp>
  <trdgrp lng="pt">
    <trd>fermata</trd>,
    <trd>caldeir&#227;o</trd>,
    <trd>coroa</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;</trd>
</drv>
</art>
