<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fermat.xml,v 1.18 2023/10/14 18:06:40 revo Exp $">
<kap>
<rad>fermat</rad>/o
</kap>
<drv mrk="fermat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sign.0o.KOMUNE">Signo</ref>,
      kiu haltigas momente la movon k montras,
      ke la da&ubreve;ro de la noto k de la silento, super kiu &gcirc;i
      estas lokita, povas esti plilongigata:
      <ekz>
        famaj <tld/>oj estas la komenco de la 5-a simfonio de <nom>Beethoven</nom> a&ubreve; la uverturo
        de <nom>Zauberfl&ouml;te</nom> de <nom>Mozart</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=338054">Fermato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_r;&c_m;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>fermata</trd>,
    <trd>korunka</trd>,
    <trd>prodlu&zcaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Fermate</trd>,
     <trd>Haltezeichen</trd></trdgrp>
  <trdgrp lng="en">
    <trd>fermata</trd>,
    <trd>hold</trd>
  </trdgrp>
  <trd lng="fr">point d'orgue</trd>
  <trdgrp lng="hu">
    <trd>korona<klr tip="ind"> (zene)</klr></trd>,
    <trd>ferm&aacute;ta</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30A7;&#x30EB;&#x30DE;&#x30FC;&#x30BF;</trd>,
    <trd>&#x5EF6;&#x97F3;&#x8A18;&#x53F7;  <pr>&#12360;&#12435;&#12362;&#12435;&#12365;&#12372;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>fermate</trd>,
     <trd>pauzeteken</trd></trdgrp>
  <trdgrp lng="pt">
    <trd>fermata</trd>,
    <trd>caldeir&atilde;o</trd>,
    <trd>coroa</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>korunka</trd>,
    <trd>predl&zcaron;ka</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_je;&c_r;&#x43C;&c_a;&c_t;&c_a;</trd>
</drv>
</art>
