<kap>
  <rad>stanc</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="stanc.0o">
  <kap><tld/>o</kap>
  <snc mrk="stanc.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Okversa <ref tip="super" cel="strof.0o">strofo</ref>:
      <ekz>
        <nom>Don Ki&hcirc;oto</nom> Â­ konas iom da toskana lingvo, kaj mi e&ccirc; povas deklami kelkajn
        <tld/>ojn de Ariosto
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> povas originlingve reciti <tld/>ojn el la poezio de <nom>Cam&otilde;es</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 5 (103)</lok>
        </fnt>;
      </ekz>
      <ekz>
        46 kantoj, kiuj havas po 80 al 100 <tld/>ojn, preska&ubreve; 5000 sume, &ccirc;iu <tld/>o
        havas 8 versojn kun la rimaran&gcirc;o <esc>abababcc</esc>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30960">Orlando Furioso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">octava</trd>
    <trd lng="de">Stanze</trd>
    <trdgrp lng="en">
      <trd>octave<klr tip="amb"> (poetry)</klr></trd>,
      <trd>octet<klr tip="amb"> (poetry)</klr></trd>
    </trdgrp>
    <trd lng="hu">stanza</trd>
    <trd lng="nl">stanza</trd>
  </snc>
  <snc mrk="stanc.0o.strofo">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="strof.0o.POE">strofo</ref>
    <!-- EVI ne traduku
    <trd lng="ca">estrofa</trd>
    <trdgrp lng="hu">
      <trd>versszak</trd>,
      <trd>str&oacute;fa</trd>
    </trdgrp>
    <trd lng="nl">strofe</trd>
    -->
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_n;&c_s;</trd>
  <trd lng="cs">stance</trd>
  <trd lng="en">stanza</trd>
  <trdgrp lng="fr">
    <trd>stance</trd>,
    <trd>stanza</trd>
  </trdgrp>
  <trd lng="ja">&#12473;&#12479;&#12531;&#12470;</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>osemver&scaron;ov&aacute; strofa</trd>,
    <trd>stanca</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_n;&c_s;</trd>
</drv>
