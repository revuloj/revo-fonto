<kap>
<rad>fat</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="fat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fatal.0o">Fatalo</ref>: 
      <ekz>
        ankora&ubreve; kvardek ok horoj restas &gcirc;is la fino de tiu &ccirc;i batalo kun la lin
        persekutanta <tld/>o &mdash; kaj li batalos &gcirc;is la fino mem
        <fnt>
          <bib>FK</bib>
        </fnt>!
      </ekz>
      <ekz>
        perversa <tld/>o sur mi pezas: / la tradici' min ne aprezas, / agnoskas min nur
        kun fobio / obtuz-a&ubreve;dace la prolet'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_jo;&c_s;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_f;&c_a;&c_t;&c_u;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>f&aacute;tum</trd>,
    <trd>p&rcaron;edur&ccaron;en&yacute; osud</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Fatum</trd>,
     <trd>Geschick</trd>,
     <trd>Schicksal</trd>,
     <trd>Verh&auml;ngnis</trd></trdgrp>
  <trd lng="es">destino</trd>
  <trdgrp lng="fr">
    <trd>destin</trd>,
    <trd>destin&eacute;e</trd>,
    <trd>fatalit&eacute;</trd>,
    <trd>fatum</trd>,
    <trd>sort</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>v&eacute;gzet</trd>,
   <trd>f&aacute;tum</trd>,
   <trd>sorsszer&udblac;s&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>lot</trd>,
   <trd>nootlot</trd>
  </trdgrp>
  <trd lng="pt">fatalidade</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_t;&c_u;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">predur&ccaron;en&yacute; osud</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x43B;&#x44F;</trd>,
    <trd>&#x444;&#x430;&#x442;&#x443;&#x43C;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x437;&#x43D;&#x430;&#x447;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F79;&#x96BE; <pr>l&#xED;n&#xE0;n</pr></trd>,
    <trd>&#x6BC0;&#x6EC5; <pr>hu&#x1D0;mi&#xE8;</pr></trd>,
    <trd>&#x547D;&#x8FD0;&#x7684;&#x6253;&#x51FB; <pr>m&#xEC;ngy&#xF9;nded&#x1CE;j&#x12B;</pr></trd>,
    <trd>&#x5384;&#x8FD0; <pr>&#xE8;y&#xF9;n</pr></trd>,
