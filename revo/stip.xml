<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>stip</rad>/o
</kap>
<drv mrk="stip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="stip.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Stipa</trd>),
      unu specio estas <ref tip="sub" cel="alf.0o">alfo</ref>.
    </dif>
    <trd lng="de">Federgras</trd>
    <trd lng="nl">vedergras</trd>
    <trd lng="sv">fj&auml;dergr&auml;s</trd>
  </snc>
  <snc mrk="stip.0o.tigo">
    <dif>
      Trunketo, tigo.
    </dif>
    <trdgrp lng="de">
       <trd>Stiel</trd>,
       <trd>Stengel</trd>
    </trdgrp>
  </snc>
</drv>
</art>
