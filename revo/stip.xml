<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>stip</rad>/o
</kap>
<drv mrk="stip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="stip.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="po1.0acoj">poacoj</ref>
      (<trd lng="la">Stipa</trd>),
      unu specio estas <ref tip="sub" cel="alf.0o">alfo</ref>.
    </dif>
    <trd lng="de">Federgras</trd>
  </snc>
  <snc mrk="stip.0o.tigo">
    <dif>
      Trunketo, tigo.
    </dif>
  </snc>
</drv>
</art>
