<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cir</rad>/o
</kap>
<drv mrk="cir.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
&Scirc;mira&jcirc;o por purigi kaj briligi &scirc;uojn kaj aliajn
ledajn objektojn.
</dif>
</snc>
<trd lng="fr">cirage</trd>
<trdgrp lng="nl">
    <trd>schoensmeer</trd>,
    <trd>schoenpoets</trd>
  </trdgrp>
<trd lng="ru">&c_v;&c_a;&c_k;&c_s;&c_a;</trd>
</drv>
<drv mrk="cir.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
&Scirc;miri per <tld/>o.
</dif>
</snc>
<trd lng="fr">cirer</trd>
<trd lng="nl">schoenen <ind>poetsen</ind></trd>
<trdgrp lng="ru">
  <trd>&c_v;&c_a;&c_k;&c_s;&c_i;&c_t;&c_mol;</trd>,
  <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_o;&c_b;&c_u;&c_v;&c_mol; &c_v;&c_a;&c_k;&c_s;&c_o;&c_j;</trd>
</trdgrp>
</drv>
</art>
