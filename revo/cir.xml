<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cir.xml,v 1.26 2023/10/14 11:10:02 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>cir</rad>/o
</kap>

<drv mrk="cir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;mira&jcirc;o por purigi kaj briligi &scirc;uojn kaj aliajn
      ledajn objektojn:
      <ekz>
        kiel kun <tld/>o, tiel anka&ubreve; kun gudro
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        li postulis <klr>[&#x2026;]</klr> <tld/>on, kaj li regule ricevadis <klr>[&#x2026;]</klr>
        &scirc;mira&jcirc;on
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ampanbotelo povas esti plenigita per <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_k;&c_s;&c_a;</trd>
  <trd lng="br">koaraj</trd>
  <trd lng="ca">betum</trd>
  <trdgrp lng="cs">
    <trd>c&iacute;didlo</trd>,
    <trd>kr&eacute;m na boty</trd>,
    <trd>le&scaron;tidlo na boty</trd>
  </trdgrp>
  <trd lng="de">Schuhcreme</trd>
  <trd lng="el">&beta;&epsilon;&rho;&ny;&jota_ton;&kappa;&jota;</trd>
  <trdgrp lng="en">
    <trd>polish</trd>,
    <trd>wax</trd>
  </trdgrp>
  <trd lng="es">bet&uacute;n</trd>
  <trd lng="fr">cirage</trd>
  <trd lng="he">&mem;&shin;&het;&tav; &nun;&ayin;&lamed;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>boksz</trd>,
    <trd>cip&odblac;ken&odblac;cs</trd>,
    <trd>cip&odblac;kr&eacute;m</trd>
  </trdgrp>
  <trd lng="it">lucido<klr> (per scarpe e pelli)</klr></trd>
  <trd lng="ja">&#x9774;&#x58A8;  <pr>&#x304F;&#x3064;&#x305A;&#x307F;</pr></trd>
  <trdgrp lng="nl">
    <trd>schoensmeer</trd>,
    <trd>schoenpoets</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pasta do but&oacute;w</trd>,
    <trd>szuwaks<klr> (przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">graxa</trd>
  <trd lng="ru">&c_v;&c_a;&c_k;&c_s;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kr&eacute;m na top&aacute;nky</trd>,
    <trd>pasta</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x430;&#x43A;&#x441;&#x430;</trd>,
    <trd>&#x43F;&#x430;&#x441;&#x442;&#x430; &#x434;&#x43E; &#x432;&#x437;&#x443;&#x442;&#x442;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="cir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        <nom>Ejnar</nom> <klr>[&#x2026;]</klr> prenis broson kaj <tld/>on por <tld/>i siajn &scirc;uojn
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mastrino bonfavore permesis al mi uzi la kvaza&ubreve; <tld/>itan fortepianon
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
<ref tip="sin" cel="vaks.0i">vaksi</ref>
  </snc>
  <rim>
     Ciro estas anka&ubreve; persa re&gcirc;o en la Malnova Testamento:
     <ekz>
      Ciro, re&gcirc;o de Persujo
      <fnt>
        <bib>MT</bib>
        <lok>II. Kroniko 36:22</lok>
      </fnt>.
    </ekz>
  </rim>
  <trd lng="be">&c_v;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">koara&ntilde;<klr tip="ind"> (l&ecirc;r)</klr></trd>
  <trd lng="ca">embetumar</trd>
  <trdgrp lng="cs">
    <trd>c&iacute;dit</trd>,
    <trd>le&scaron;tit</trd>,
    <trd>vyle&scaron;tit</trd>,
    <trd>&ccaron;istit <klr>(obuv)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schuhe <ind>putzen</ind></trd>,
    <trd>wienern</trd>,
    <trd>wichsen <klr>(Schuhe)</klr></trd>
  </trdgrp><trd lng="el">&lambda;&omikron;&ypsilon;&sigma;&tau;&rho;&alfa_ton;&rho;&omega;</trd>
  <trd lng="en">polish</trd>
  <trd lng="es">embetunar</trd>
  <trd lng="fr">cirer</trd>
  <trdgrp lng="hu">
    <trd>bokszol<klr tip="ind"> (cip&odblac;t)</klr></trd>,
    <trd>beken<klr tip="ind"> (cip&odblac;t)</klr></trd>,
    <trd>f&eacute;nyes&iacute;t<klr tip="ind">
    (cip&odblac;t)</klr></trd>
  </trdgrp>
  <trd lng="it">lucidare<klr> (scarpe o pelli)</klr></trd>
  <trd lng="ja">&#x9774;&#x58A8;&#x3067;&#x78E8;&#x304F;  <pr>&#x304F;&#x3064;&#x3059;&#x307F;&#x3067;&#x307F;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">schoenen <ind>poetsen</ind></trd>
  <trd lng="pl">pastowa&cacute;<klr> buty</klr></trd>
  <trd lng="pt">engraxar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_k;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;
    &c_o;&c_b;&c_u;&c_v;&c_mol; &c_v;&c_a;&c_k;&c_s;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">le&scaron;ti&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x438;&#x441;&#x442;&#x438;&#x442;&#x438; &#x432;&#x437;&#x443;&#x442;&#x442;&#x44F; &#x432;&#x430;&#x43A;&#x441;&#x43E;&#x44E;</trd>,
    <trd>&#x43F;&#x430;&#x441;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438; &#x432;&#x437;&#x443;&#x442;&#x442;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
