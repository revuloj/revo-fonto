<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cir</rad>/o
</kap>

<drv mrk="cir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;mira&jcirc;o por purigi kaj briligi &scirc;uojn kaj aliajn
      ledajn objektojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_k;&c_s;&c_a;</trd>
  <trd lng="br">koaraj</trd>
  <trd lng="de">Schuhcreme</trd>
  <trdgrp lng="en">
    <trd>polish</trd>,
    <trd>wax</trd>
  </trdgrp>
  <trd lng="el">&beta;&epsilon;&rho;&ny;&jota_ton;&kappa;&jota;</trd>
  <trd lng="fr">cirage</trd>
  <trdgrp lng="hu">
    <trd>boksz</trd>,
    <trd>cip&odblac;ken&odblac;cs</trd>,
    <trd>cip&odblac;kr&eacute;m</trd>
  </trdgrp>
  <trd lng="it">lucido<klr> (per scarpe e pelli)</klr></trd>
  <trdgrp lng="nl">
    <trd>schoensmeer</trd>,
    <trd>schoenpoets</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pasta do but&oacute;w</trd>,
    <trd>szuwaks<klr> (przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">graxa</trd>
  <trd lng="ru">&c_v;&c_a;&c_k;&c_s;&c_a;</trd>
</drv>

<drv mrk="cir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">koara&ntilde;<klr tip="ind"> (l&ecirc;r)</klr></trd>
  <trd
  lng="el">&lambda;&omikron;&ypsilon;&sigma;&tau;&rho;&alfa_ton;&rho;&omega;</trd>
  <trd lng="en">polish</trd>
  <trd lng="fr">cirer</trd>
  <trdgrp lng="hu">
    <trd>bokszol<klr tip="ind"> (cip&odblac;t)</klr></trd>,
    <trd>beken<klr tip="ind"> (cip&odblac;t)</klr></trd>,
    <trd>f&eacute;nyes&iacute;t<klr tip="ind">
    (cip&odblac;t)</klr></trd>
  </trdgrp>
  <trd lng="it">lucidare<klr> (scarpe o pelli)</klr></trd>
  <trd lng="nl">schoenen <ind>poetsen</ind></trd>
  <trd lng="pl">pastowa&cacute;<klr> buty</klr></trd>
  <trd lng="pt">engraxar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_k;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;
    &c_o;&c_b;&c_u;&c_v;&c_mol; &c_v;&c_a;&c_k;&c_s;&c_o;&c_j;</trd>
  </trdgrp>
  </drv>
</art>
