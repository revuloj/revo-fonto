<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxpal.xml,v 1.10 2023/10/21 07:46:48 revo Exp $">
<kap>
  <rad>&scirc;pal</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sxpal.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxpal.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="trab.0o.KON">Trabo</ref> ku&scirc;anta sur
      vando a&ubreve; grundo kaj portanta suprajn peza&jcirc;ojn.
    </dif>
    <subsnc mrk="sxpal.0o.FER">
      <dif>
        Trabo subportanta kaj fiksanta la relojn de <ref tip="malprt" cel="trak.0o.FER">trako</ref>:
        <ekz>
          du &scirc;talaj reloj, ku&scirc;antaj sur <tld/>oj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=69724">Fervoja trako</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          el tramtrako kiun mi preterpasis &ccirc;iuj lignaj <tld/>oj
          estis malaperintaj
          <fnt>
            <aut>T. Menger</aut>,
            <vrk>
              <url ref="http://homepage.mac.com/mkardol/Ne%20Tiam%2C%20Ne%20Nun%2C%20Neniam.pdf">
              Ne tiam &dash; ne nun &dash; neniam ...</url></vrk>,
            <lok>1982</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">Schwelle <klr>(Bahngleis)</klr></trd>
      <trd lng="hu">talpfa</trd>
      <trd lng="it">traversina</trd>
      <trd lng="nl">biel</trd>
    </subsnc>
    <subsnc>
      <uzo tip="klr">(&ccirc;e faktraba domo)</uzo>
      <dif>
         Horizontala longa trabo portanta la vertikalajn kaj oblikvajn trabojn:
      <ekz>
        sur soklaj <tld/>oj konstruitaj faktrabaj konstruktoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=534155">Faktraba domo</url></lok>
        </fnt>.
      </ekz>

      </dif>
      <trd lng="de">Schwelle <klr>(Fachwerk)</klr></trd>
    </subsnc>
    <trdgrp lng="nl">
      <trd>dwarsligger</trd>,
      <trd>dwarsbalk</trd>,
      <trd>kruisbalk</trd>,
      <trd>travers</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="steg.0o.TEK">stego</ref>
  </refgrp>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_l;&c_a;</trd>
  <trd lng="fr">traverse</trd>
  <trd lng="it">traversa</trd>
  <trdgrp lng="ja">
    <trd>&#x6A2A;&#x6728;  <pr>&#x3088;&#x3053;&#x304D;</pr></trd>,
    <trd>&#x6795;&#x6728;  <pr>&#x307E;&#x304F;&#x3089;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="pl">podk&lstroke;ad<klr> kolejowy</klr></trd>
  <trd lng="uk">&#x448;&#x43F;&#x430;&#x43B;&#x430;</trd>
</drv>
</art>

