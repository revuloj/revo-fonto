<art mrk="$Id: antem.xml,v 1.15 2022/04/25 15:10:59 revo Exp $">
<kap>
  <rad>antem</rad>/o
</kap>
<drv mrk="antem.0o">
  <kap><tld/>o</kap>
  <snc mrk="antem.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Anthemis</trd>), kun &ccirc;. 150 specioj, pluraj 
      similaspektaj al <ref cel="kamomi.0o">kamomilo</ref>:
      <ekz>
        de &ccirc;io bonodoras la vila&gcirc;o: de ligno kaj bovinoj, de pajlo kaj fojno, kaj
        <klr>[&#x2026;]</klr> de <tld/>o kaj urtikoj
        <fnt>
          <aut>Karel &Ccaron;apek , trad. Josef Vondrou&scaron;ek</aut>
          <vrk><url ref="https://azrefs.org/esperantigis-josef-vondrouek.html?page=2">Hordubal</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/a3/Anthemis_arvensis_-_K&ouml;hler&ndash;s_Medizinal-Pflanzen-159.jpg/457px-Anthemis_arvensis_-_K&ouml;hler&ndash;s_Medizinal-Pflanzen-159.jpg" prm="PD">botanika tabulo de Kampa <tld/>o (Anthemis arvensis)
        <fnt>
          <aut>Franz Eugen K&ouml;hler, K&ouml;hler's Medizinal-Pflanzen, List of Koehler Images</aut>
          <lok><url ref="&WCW;/File:Anthemis_arvensis_-_K%C3%B6hler%E2%80%93s_Medizinal-Pflanzen-159.jpg">&WCW;/File:Anthemis_arvensis_-_K&ouml;hler&ndash;s_Medizinal-Pflanzen-159.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="cs">anthem</trd>
  <trd lng="de">Hundskamille</trd>
  <trd lng="fr">anth&eacute;mis</trd>
  <trd lng="hu">pipit&eacute;r</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_je;&c_m;&c_i;&c_s;</trd>,
    <trd>&c_p;&c_u;&c_p;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">anthem</trd>
  <trd lng="sv">kulla</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43D;&#x437;&#x435;&#x43C;</trd>,
    <trd>&#x430;&#x43D;&#x442;&#x435;&#x43C;</trd>
  </trdgrp>
</drv>
</art>
