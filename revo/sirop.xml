<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sirop</rad>/o
</kap>
<drv mrk="sirop.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Densa kuirita solva&jcirc;o de sukero ofte aromigita per fruktosuko.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_r;&c_o;&c_p;</trd>
  <trd lng="ca">xarop</trd>
  <trd lng="de">Sirup</trd>
  <trd lng="en">syrup</trd>
  <trd lng="es">sirope</trd>
  <trd lng="hu">szirup</trd>  
  <trdgrp lng="nl">
    <trd>stroop</trd>,
    <trd>siroop</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_o;&c_p;</trd>
</drv>
</art>
