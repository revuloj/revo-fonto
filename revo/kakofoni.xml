<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kakofoni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kakofoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kakofoni.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Mal<ref tip="ant" cel="harmon1.0o.MUZ">harmonio</ref>, missonado de
      kunaj tonoj:
      <ekz>
        orkestro &leftquot;&jcirc;azzband&rightquot; el kvar personoj surdigas la vizitantojn per
        sova&gcirc;a <tld/>o
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_f;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">cacofonia</trd>
  <trd lng="es">cacofon&iacute;a</trd>
  <trd lng="fr">cacophonie</trd>
</drv>
</art>

