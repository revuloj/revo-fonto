<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pe&ccirc;</rad>/o
</kap>
<drv mrk="pecx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nigra glueca substanco ekstraktata el rezino.
    </dif>
  </snc>
  <trd lng="de">Pech</trd>
  <trd lng="en">pitch</trd>
  <trd lng="nl">pek</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_l;&c_a;</trd>
</drv>

<drv mrk="pecx.ter0o">
  <kap>ter<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="asfalt.0o">Asfalto</ref>.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>asfalt</trd>,
    <trd>aardpek</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
</drv>
</art>
