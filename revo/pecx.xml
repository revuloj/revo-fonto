<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pe&ccirc;</rad>/o
</kap>
<drv mrk="pecx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nigra glueca substanco ekstraktata el rezino.
    </dif>
  </snc>                    
  <trd lng="be">&c_s;&c_m;&c_a;&c_l;&c_a;</trd>
  <trd lng="br">peg</trd>
  <trd lng="de">Pech</trd>
  <trd lng="en">pitch</trd> 
  <trd lng="fr">poix</trd>
  <trdgrp lng="hu">
    <trd>szurok</trd>,
    <trd>k&aacute;tr&aacute;ny</trd>
  </trdgrp>
  <trd lng="nl">pek</trd>
  <trdgrp lng="pt">
    <trd>pez</trd>,
    <trd>breu</trd>,
    <trd>piche</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_l;&c_a;</trd>
</drv>

<drv mrk="pecx.ter0o">
  <kap>ter<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="asfalt.0o">Asfalto</ref>.
    </dif>
  </snc>                         
  <trd lng="be">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="br">asfalt</trd>
  <trd lng="en">asphalt</trd> 
  <trd lng="fr">asphalte</trd>
  <trd lng="hu">aszfalt</trd>  
  <trdgrp lng="nl">
    <trd>asfalt</trd>,
    <trd>aardpek</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
</drv>
</art>
