<kap>
  <rad>stiv</rad>/i
</kap>
<drv mrk="stiv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Fiksaran&gcirc;i la &scirc;ar&gcirc;ojn de la &scirc;ipo en la holdo, por ke ili ne movi&gcirc;u
      dum la osciloj de la &scirc;ipo:
      <ekz>
        karba &scirc;ar&gcirc;o ne estas <tld/>ata.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
    &c_g;&c_r;&c_u;&c_z; <klr>(&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr>
  </trd>
  <trdgrp lng="cs">
    <trd>rovnat lodn&#xED; n&#xE1;klad</trd>,
    <trd>upev&ncaron;ovat lodn&#xED; n&#xE1;klad</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verstauen</trd>,
    <trd>stauen</trd>
  </trdgrp>
  <trd lng="fr">arrimer</trd>
  <trdgrp lng="hu">
    <trd>rakom&aacute;nyt r&ouml;gz&iacute;t <klr tip="amb">(haj&oacute;n </klr>)</trd>,
    <trd>rakom&aacute;nyt elrendez <klr tip="amb">(haj&oacute;n </klr>)</trd>
  </trdgrp>
  <trd lng="ja">&#x304D;&#12385;&#12435;&#12392;&#x7A4D;&#x307F;&#x8FBC;&#12416;  <pr>&#x304D;&#12385;&#12435;&#12392;&#12388;&#x307F;&#12371;&#12416;</pr></trd>
  <trd lng="nl">stuwen<klr tip="ind"> (v.scheepslading)</klr></trd>
  <trd lng="pl">sztauowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol; &c_g;&c_r;&c_u;&c_z; <klr tip="amb">(&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">upevni&tcaron; lodn&#xFD; n&#xE1;klad</trd>
  <trd lng="uk">&c_r;&#x43E;&c_z;&#x43C;&c_ib;&c_shch;&c_a;&c_t;&c_i; &c_ib; &#x43A;&c_r;&c_ib;&#x43F;&c_i;&c_t;&c_i; &c_v;&c_a;&#x43D;&c_t;&c_a;&c_zh; (&#x43D;&c_a;
  &c_s;&c_u;&c_d;&#x43D;&c_ib;)</trd>
</drv>
</art>
