<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: latun.xml,v 1.28 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>latun</rad>/o
</kap>
<drv mrk="latun.0o">
  <kap><tld/>o</kap>
  <snc mrk="latun.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      Flava <ref tip="super" cel="aloj.0o">alojo</ref> el kupro kaj
      zinko:
      <ekz>
        en la &scirc;ranko &scirc;i ordometis <klr>...</klr> <tld/>an
        kandelingon kaj
        malgrandan samovaron
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</trd>
    <trd lng="ca">llaut&oacute;</trd>
    <trd lng="de">Messing</trd>
    <trd lng="en">brass</trd>
    <trd lng="es">lat&oacute;n</trd>
    <trd lng="fa">&a_b;&a_r;&a_n;&a_j;<klr tip="ind"> (&a_A_madda;&a_l;&f_y;&a_A;&f_zh; &a_m;&a_s;
    &a_w;
    &a_r;&a_w;&f_y;)</klr></trd>
    <trd lng="fr">laiton</trd>
    <trd lng="hu">s&aacute;rgar&eacute;z</trd>
    <trdgrp lng="nl">
      <trd>messing</trd>,
      <trd>geel <ind>koper</ind></trd>
    </trdgrp>
    <trd lng="pl">mosi&aogonek;dz</trd>
    <trd lng="pt">lat&atilde;o</trd>
    <trd lng="ru">&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</trd>
    <trd lng="it">ottone</trd>
    <trdgrp lng="zh">
    <trd>&#x9EC3;&#x9285; <pr>hu&#xE1;ngt&#xF3;ng</pr></trd>,
    <trd>&#x9EC4;&#x94DC; <pr>hu&#xE1;ngt&#xF3;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&t_ra;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>mosaz</trd>,
    <trd>&zcaron;lut&aacute; m&ecaron;&dcaron;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9EC4;&#x9285;  <pr>&#x304A;&#x3046;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x771F;&#x3061;&#x3085;&#x3046;  <pr>&#x3057;&#x3093;&#x3061;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="sk">mosadz</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</trd>,
    <trd>&c_zh;&c_o;&c_v;&c_t;&c_a; &c_m;&c_ib;&c_d;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="latun.0a">
  <kap><tld/>a</kap>
  <snc mrk="latun.0a.rilata">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Rilata al <tld/>o.
      <ekz>
        Samtempe la <tld/>a kapo de Hipokrato kuntiris la brovojn kaj
        diris: "Sindetenu!"
        <fnt>
          <aut>Nathaniel Hawthorne, trad. Edwin P. Grobe</aut>,
          <vrk>Tri Noveloj
          </vrk>,
          <lok>&ccirc;. 1 "La eksperimento de d-ro Hejdegero"</lok>
       </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">mosi&aogonek;dzowy</trd>
  </snc>
  <snc mrk="latun.0a.el">
    <dif>
      El <tld/>o.
      <ekz>
        La pezglobo estas fera a&ubreve; <tld/>a globo kun difinitaj
        pezo kaj diametro
        <fnt>
          <aut>Tibor Ujlaky-Nagy</aut>,
          <vrk>La sporta ligvo en Esperanto</vrk>,
          <lok>Pezglob-&jcirc;eto</lok>
       </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>mosi&eogonek;&zdot;ny</trd>,
      <trd>mosi&aogonek;dzowy</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_t;&c_u;&c_n;&c_je;&c_v;&c_y;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_u;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">de <ind>llaut&oacute;</ind></trd>
  <trd lng="cs">mosazn&yacute;</trd>
  <trd lng="en">brass</trd>
  <trd lng="it"><klr>di </klr>ottone</trd>
  <trd lng="ja">&#x771F;&#x3061;&#x3085;&#x3046;&#x306E;  <pr>&#x3057;&#x3093;&#x3061;&#x3085;&#x3046;&#x306E;</pr></trd>
  <trdgrp lng="nl">
      <trd>uit <ind>messing</ind></trd>,
      <trd>uit geel <ind>koper</ind></trd>
    </trdgrp>
  <trd lng="sk">mosadzn&yacute;</trd>
  <trd lng="uk">&c_l;&c_a;&c_t;&c_u;&c_n;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
