<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: smirg.xml,v 1.21 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>smirg</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="smirg.0o">
  <kap><tld/>o</kap>
  <snc mrk="smirg.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Nepura <ref tip="super" cel="korund.0o">korundo</ref>
      miksita kun kvarco kaj aliaj mineraloj kaj kies
      pulvoro estas uzata por glatigi a&ubreve; poluri metalojn, a&ubreve; malpoluri
      la vitron:
      <ekz>
        <tld/>o estas nigra a&ubreve; malhel-griza &scirc;tono <klr>[&#8230;]</klr> el fajnaj kristaloj de
        korundo kaj spinelo miksitaj kun magnetito a&ubreve; hematito, oni elfosas &gcirc;in
        precipe en Turkio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=298370">Smirgo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a papero estas peco da papero kun surgluita <tld/>opulvoro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=591791">Smirga papero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fb/Schmirgel.009607.png/637px-Schmirgel.009607.png" prm="CC BY-SA
    3.0">peco de neprilaborita <tld/>o el Naxos
        <fnt>
          <aut>HaTe, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Schmirgel.009607.png">&WCW;/File:Schmirgel.009607.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_zh;&c_d;&c_a;&c_k;</trd>
  <trd lng="cs">smirek</trd>
  <trd lng="de">Schmirgel</trd>
  <trd lng="en">emery</trd>
  <trd lng="fa">&a_s;&a_n;&a_b;&a_A;&a_d;&a_h;</trd>
  <trd lng="fr">&eacute;meri</trd>
  <trd lng="hu">smirgel</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30E1;&#x30EA;&#x30FC;</trd>,
    <trd>&#x947D;&#37444; <pr>&#12373;&#12435;&#12390;&#12388;</pr></trd>,
    <trd>&#x91D1;&#x525B;&#30722;  <pr>&#12371;&#12435;&#12372;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">amaril</trd>
  <trdgrp lng="pl">
    <trd>szmergiel</trd>,
    <trd>&sacute;cierniwo</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_zh;&c_d;&c_a;&c_k;</trd>
  <trd lng="sk">&scaron;mirge&lcaron;</trd>
  <trd lng="uk">&#x43D;&c_a;&c_zh;&c_d;&c_a;&#x43A;</trd>
</drv>

<drv mrk="smirg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="smirg.0i.TEK">
    <dif>
      <ref tip="super" cel="polur.0i.KOMUNE">Poluri</ref>,
      glatigi a&ubreve; malpoluri per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>brousit smirkem</trd>,
    <trd>le&scaron;tit smirkem</trd>,
    <trd>smirkovat</trd>
  </trdgrp>
  <trd lng="de">schmirgeln</trd>
  <trdgrp lng="en">
    <trd>grind with emery</trd>,
    <trd>polish with emery</trd>,
    <trd>rub with emery</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_n;&a_b;&a_A;&a_d;&a_h; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">abraser</trd>
  <trdgrp lng="hu">
    <trd>csiszol <klr tip="ind">(smirglivel)</klr></trd>,
    <trd>smirgliz</trd>
  </trdgrp>
  <trd lng="ja">&#x91D1;&#x525B;&#30722;&#12391;&#x78E8;&#x304F;
  <pr>&#12371;&#12435;&#12372;&#12358;&#12377;&#x306A;&#12391;&#x307F;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">gladschuren</trd>
  <trd lng="pl">polerowa&cacute;  <ind>szmerglem</ind></trd>
  <trd lng="ru">&c_z;&c_a;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <ind>&c_n;&c_a;&c_zh;&c_d;&c_a;&c_k;&c_o;&c_m;</ind></trd>
  <trd lng="sk">&scaron;mirg&lcaron;ova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a;&c_zh;&c_d;&c_a;&#x43A;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>
