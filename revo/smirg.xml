<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>smirg</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="smirg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Nepura korundo miksita kun kvarco kaj aliaj mineraloj kaj kies
      pulvoro estas uzata por glatigi a&ubreve; poluri metalojn, a&ubreve; malpoluri la
      vitron.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_zh;&c_d;&c_a;&c_k;</trd>
  <trd lng="de">Schmirgel</trd>
  <trd lng="hu">smirgel</trd>  
  <trd lng="nl">amaril</trd>
  <trd lng="ru">&c_n;&c_a;&c_zh;&c_d;&c_a;&c_k;</trd>
</drv>

<drv mrk="smirg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Poluri, glatigi a&ubreve; malpoluri per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">schmirgeln</trd>
  <trdgrp lng="hu">
    <trd>csiszol <klr tip="ind">(smirglivel)</klr></trd>,
    <trd>smirgliz</trd>
  </trdgrp>
  <trd lng="nl">gladschuren</trd>
  <trd lng="ru">&c_z;&c_a;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <ind>&c_n;&c_a;&c_zh;&c_d;&c_a;&c_k;&c_o;&c_m;</ind></trd>
</drv>

<drv mrk="smirg.0apapero">
  <kap><tld/>a papero</kap>
  <snc>
    <dif>
      Papero sur kiu estas algluita tavolo de <tld/>opulvoro kaj uzata por
      glatigi a&ubreve; poluri.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_zh;&c_d;&c_a;&c_ch;&c_n;&c_a;&c_ja; 
    &c_p;&c_a;&c_p;&c_je;&c_r;&c_a;</trd>
  <trd lng="de">Schmirgelpapier</trd>
  <trdgrp lng="hu">
    <trd>d&ouml;rzspap&iacute;r</trd>,
    <trd>csiszol&oacute;pap&iacute;r</trd>
    <trd>smirgli</trd>
  </trdgrp>
  <trd lng="nl">schuurpapier</trd>
  <trd lng="ru">&c_n;&c_a;&c_zh;&c_d;&c_a;&c_ch;&c_n;&c_a;&c_ja; &c_b;&c_u;&c_m;&c_a;&c_g;&c_a;</trd>
</drv>
</art>
