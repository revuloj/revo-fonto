<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kontingent</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kontin2.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontin2.0o.KOMUNE">
    <dif>
      Nombro, parto, kiun oni devas liveri &ccirc;iufoje:
      <ekz>
        &ccirc;iu provinco liveris sian <tld/>on da greno;
      </ekz>
      <ekz>
        pagi sian <tld/>on al la bankedo;
      </ekz>
      <ekz>
        <tld/>o de la armeo
        <klr>(nombro da soldatoj postulata &ccirc;iujare)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tribut.0o.POL">tributo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_y;&c_n;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">contigent</trd>
  <trd lng="de">Kontingent</trd>
  <trd lng="en">contingent</trd>
  <trd lng="es">contingente</trd>
  <trdgrp lng="fr">
    <trd>quota</trd>,
    <trd>quote-part</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kontingens</trd>,
    <trd>keret</trd>
  </trdgrp>
  <trd lng="nl">contingent</trd>
  <trd lng="pt">contingente</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_i;&c_n;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="kontin2.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontin2.0i.KOMUNE">
    <dif>
      Liveri <tld/>on:
      <ekz>
        <tld/>i eldonon de akcioj, festenon, komunan donacon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontri.0i">kontribui</ref>,
      <ref cel="kotiz.0i">kotizi</ref>,
      <ref cel="tribut.0i.POL">tributi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>kontingentieren</trd>,
    <trd>begrenzen</trd>,
    <trd>limitieren</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kontingent&aacute;l</trd>,
    <trd>kontingenst meg&aacute;llap&iacute;t</trd>,
    <trd>keretet meg&aacute;llap&iacute;t</trd>
  </trdgrp>
  <trd lng="fr">payer son <ind>&eacute;cot</ind></trd>
  <trd lng="nl">contingenteren</trd>
</drv>
</art>
