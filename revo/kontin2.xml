<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontin2.xml,v 1.23 2024/11/18 12:55:31 revo Exp $">
<kap>
  <rad>kontingent</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kontin2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo><uzo tip="fak">EKON</uzo>
  <snc mrk="kontin2.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="nombr.0o.kvanto">Nombro</ref>,
      <ref tip="super" cel="part.0o.porcio">parto</ref>, kiun oni devas liveri &ccirc;iufoje:
      <ekz>
        <tld/>o de soldatoj
        <klr>(kontribuenda al la landa armeo)</klr>;
      </ekz>
      <ekz>
        pro la el&ccirc;erpo de stoko ni devis resti <klr>[&#8230;]</klr> dum tri semajnoj por
        atendi novan <tld/>on
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        neniu do konis e&ccirc; la patron kaj <klr>[do]</klr> proponis <nom>Avanov</nom>, la princo, tre
        bonvole ke li mem, kiel sian <tld/>on en la armado de l&#8217; malgrandulo
        kontra&ubreve; la malbona&jcirc;oj de l&#8217; mondo, donu sian nomon
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>Du blinduloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        el tiu <tld/>o varbi&gcirc;as la legantoj
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Redakcie</url></vrk>
          <lok>2003. &#8470; 11 (109)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontri1.0o">kontribucio</ref>,
      <ref cel="kvot.0o">kvoto</ref>,
      <ref cel="porci.0o.KOMUNE">porcio</ref>,
      <ref cel="proviz.0o.ajxo">provizo</ref>,
      <ref cel="stok.0o">stoko</ref>,
      <ref cel="tribut.0o.POL">tributo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_y;&c_n;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">contigent</trd>
  <trdgrp lng="cs">
    <trd>kontingent</trd>,
    <trd>kv&oacute;ta</trd>,
    <trd>pevn&yacute; po&ccaron;et</trd>,
    <trd>pod&iacute;l</trd>,
    <trd>povinn&aacute; d&aacute;vka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kontingent</trd>,
    <trd>Zuteilung</trd>
  </trdgrp>
  <trd lng="en">contingent</trd>
  <trd lng="es">contingente</trd>
  <trdgrp lng="fr">
    <trd>quota</trd>,
    <trd>quote-part</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kontingens</trd>,
    <trd>keret</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21106;&#x5F53;&#12390;  <pr>&#x308F;&#x308A;&#12354;&#12390;</pr></trd>,
    <trd>&#20998;&#x62C5;  <pr>&#12406;&#12435;&#x305F;&#12435;</pr></trd>,
    <trd>&#21106;&#x5F53;&#12390;&#x91CF;  <pr>&#x308F;&#x308A;&#12354;&#12390;&#x308A;&#12423;&#12358;</pr></trd>,
    <trd>&#20998;&#x62C5;&#x984D;  <pr>&#12406;&#12435;&#x305F;&#12435;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">contingent</trd>
  <trd lng="pl">kontyngent</trd>
  <trd lng="pt">contingente</trd>
  <trdgrp lng="ro">
    <trd>cot&abreve;</trd>,
    <trd>contingent</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_i;&c_n;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kontingent</trd>,
    <trd>kv&oacute;ta</trd>,
    <trd>pevn&yacute; po&ccaron;et</trd>,
    <trd>podiel</trd>,
    <trd>povinn&aacute; d&aacute;vka</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EFD;&#x984D; <pr>f&#xE8;n&#xE9;</pr></trd>,
    <trd>&#x914D;&#x984D; <pr>p&#xE8;i&#xE9;</pr></trd>,
    <trd>&#x984D;&#x5EA6; <pr>&#xE9;d&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kontin2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontin2.0i.KOMUNE">
    <dif>
      Liveri <tld/>on:
      <ekz>
        <tld/>i eldonon de akcioj, festenon, komunan donacon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontri.0i">kontribui</ref>,
      <ref cel="kotiz.0i">kotizi</ref>,
      <ref cel="tribut.0i.POL">tributi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_y;&c_n;&c_g;&c_je;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>dodat d&aacute;vku</trd>,
    <trd>odv&eacute;st kontingent</trd>,
    <trd>splnit kontingent</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kontingentieren</trd>,
    <trd>zuteilen</trd>,
  </trdgrp>
  <trd lng="fr">payer son <ind>&eacute;cot</ind></trd>
  <trdgrp lng="hu">
    <trd>kontingent&aacute;l</trd>,
    <trd>kontingenst meg&aacute;llap&iacute;t</trd>,
    <trd>keretet meg&aacute;llap&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#21106;&#x5F53;&#12390;&#20998;&#12434;&#x51FA;&#12377;  <pr>&#x308F;&#x308A;&#12354;&#12390;&#12406;&#12435;&#12434;&#12384;&#12377;</pr></trd>
  <trd lng="nl">contingenteren</trd>
  <trd lng="pl">podzieli&cacute;</trd>
  <trd lng="ro">a&#537;terne</trd>
  <trdgrp lng="sk">
    <trd>odvies&tcaron; kontingent</trd>,
    <trd>splni&tcaron; d&aacute;vku</trd>
  </trdgrp>
  <trd lng="zh">&#x914D;&#x7D66; <pr>p&#xE8;ij&#x1D0;</pr></trd>
</drv>
</art>
