<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kontingent</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kontin2.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Nombro, parto, kiun oni devas liveri &ccirc;iufoje:
      <ekz>
        &ccirc;iu provinco liveris sian <tld/>on da greno;
      </ekz>
      <ekz>
        pagi sian <tld/>on al la bankedo;
      </ekz>
      <ekz>
        <tld/>o de la armeo
        <klr>(nombro da soldatoj postulata &ccirc;iujare)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_y;&c_n;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Kontingent</trd>
  <trd lng="en">contingent</trd>
  <trd lng="nl">contingent</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_i;&c_n;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="kontin2.0i">
  <kap>
    <tld/>i<fnt>Z</fnt>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Liveri <tld/>on:
      <ekz>
        <tld/>i eldonon de akcioj, festenon, komunan donacon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kontri.0i">kontribui</ref>,
      <ref cel="kotiz.0i">kotizi</ref>.
    </refgrp>
  </snc>
<trd lng="nl">contingenteren</trd>
</drv>
</art>
