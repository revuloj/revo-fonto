<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bard</rad>/o
</kap>
<drv mrk="bard.0o">
  <kap><tld/>o</kap>
  <snc mrk="bard.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;e la antikvaj keltoj poeto, komisiita prikanti la
      heroa&jcirc;ojn.
    </dif>
    <trd lng="pl">bard</trd>
  </snc>
  <snc mrk="bard.0o.FIG">
    <uzo tip="fak">POE</uzo>
    <uzo tip="klr">(lar&gcirc;asence)</uzo>
    <dif>
      Praa nacia <ref tip="vid" cel="poet.0o">poeto</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>bard</trd>,
      <trd>piewca</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_d;</trd>
  <trd lng="br">barzh<klr tip="ind"> (da vare ar gelted
  kozh)</klr></trd>
  <trd lng="ca">bard</trd>
  <trd lng="de">Barde</trd>
  <trd lng="en">bard</trd>
  <trd lng="es">bardo</trd>
  <trd lng="fr">barde</trd>
  <trd lng="hu">b&aacute;rd<klr tip="ind">
  (k&ouml;lt&odblac;)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_d;</trd>
  <trd lng="sk">bard</trd>
  <trd lng="sv">bard</trd>
</drv>
</art>
