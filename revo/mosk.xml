<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mosk.xml,v 1.25 2025/04/12 20:27:20 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mosk</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="mosk.0o">
  <kap><tld/>o</kap>
  <snc mrk="mosk.0o.PAR">
    <dif>
      Tre odora substanco uzata por fabriki <ref cel="parfum.0o">parfumo</ref>jn,
      sekreciita de iuj bestoj, precipe de <ref cel="mosk.0ulo">moskulo</ref>,
      iam anka&ubreve; de <ref cel="zibet.0o">zibeto</ref>:
      <ekz>
        la materialo de la kvintesenco estas orienta safrano, ru&gcirc;a rozfolio,
        <klr>[&#8230;]</klr> kaj <tld/>o
        <fnt>
          <bib>SatirRak</bib>
          <lok>Supersti&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_k;&c_u;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>mo&scaron;us</trd>,
    <trd>pi&zcaron;mo</trd>
  </trdgrp>
  <trd lng="de">Moschus</trd>
  <trd lng="en">musk</trd>
  <trd lng="fr">musc</trd>
  <trd lng="ja">&#12376;&#12419;&#39321;  <pr>&#12376;&#12419;&#12363;&#12362;&#12426;</pr></trd>
  <trd lng="nl">muscus</trd>
  <trd lng="pl">pi&zdot;mo</trd>
  <trd lng="pt">alm&iacute;scar</trd>
  <trd lng="ro">mosc</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_u;&c_s;</trd>
  <trd lng="sk">pi&zcaron;mo</trd>
  <trd lng="uk">&c_m;&c_u;&c_s;&c_k;&c_u;&c_s;</trd>
  <trd lng="zh">&#40605;&#39321; <pr>sh&egrave;xi&amacron;ng</pr></trd>
</drv>

<drv mrk="mosk.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="mosk.0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref> de
      <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref> (<trd lng="la">Moschus</trd>),
      vivantaj precipe en la montaroj de suda Azio:
      <ekz>
        dek <tld/>ulaj feloj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
        367a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2f/Siberian_Musk_Deer.jpg/446px-Siberian_Musk_Deer.jpg" prm="CC0">siberia <tld/>ulo en Tajgo
        <fnt>
          <aut>Erik Adamsson, propra verko</aut>
          <lok><url ref="&WCW;/File:Siberian_Musk_Deer.jpg">&WCW;/File:Siberian_Musk_Deer.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_a;&c_r;&c_g;&c_a; &c_s;&c_ib;&c_b;&c_ib;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  <trd lng="de">Moschustier</trd>
  <trd lng="en">musk <ind>deer</ind></trd>
  <trd lng="fr">moschus</trd>
  <trd lng="ja">&#12472;&#12515;&#12467;&#12454;&#12472;&#12459;</trd>
  <trd lng="pl">pi&zdot;mowiec syberyjski</trd>
  <trd lng="ro">moschus</trd>
  <trd lng="ru">&c_K;&c_a;&c_b;&c_a;&c_r;&c_g;&c_i;</trd>
  <trd lng="uk">&c_k;&c_a;&c_b;&c_a;&c_r;&c_g;&c_a;</trd>
  <trd lng="zh">&#40605; <pr>sh&egrave;</pr></trd>
</drv>
</art>
