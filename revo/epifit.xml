<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: epifit.xml,v 1.4 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>epifit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="epifit.0o">
  <kap><tld/>o</kap>
  <snc mrk="epifit.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Vegeta&jcirc;o kreskanta sur alia vegeta&jcirc;o
      sen paraziteco:
      <ekz>
        <tld/>o kelkfoje estas nomata aera planto
        &ccirc;ar &gcirc;i ne radiki&gcirc;as en la grundo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Epifito">
          Epifito</url>, 2013-09-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">epifyt</trd>
  <trd lng="en">epiphyte</trd>
  <trd lng="fr">&eacute;piphyte</trd>
  <trd lng="pl">epifit</trd>
  <trd lng="sk">epifyt</trd>
</drv>
</art>

