<drv mrk="lokal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parto de domo kun apartaj funkcio kaj uzantoj,
      eventuale nur dumtempaj, ekzemple asocia kunvenejo, oficejeto:
      <ekz>
        la <tld/>o, en kiu estas legota mia raporto, estas fama bierejo
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_ja;&c_sh;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="es">local</trd>
  <trd lng="fr">local<klr> (subst.)</klr></trd>
</drv>
</art>

