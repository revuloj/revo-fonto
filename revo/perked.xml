<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perked.xml,v 1.14 2022/04/15 08:36:12 revo Exp $">
<kap>
  <rad>perked</rad>/oj
</kap>
<drv mrk="perked.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="perk.0oformaj">perkoformaj</ref>
      fi&scirc;oj (<trd lng="la">Percidae</trd>):
      <ekz>
        la <ref tip="prt" cel="sandr.0o">sandro</ref> estas fi&scirc;o el la familio de la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=506217">Sandro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="percx.0o">per&ccirc;o</ref>,
       <ref cel="perk.0o.genro">perko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_n;&c_jo;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">p&egrave;rcids</trd>
  <trd lng="cs">okounovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Barsche</trd>
  <trd lng="fr">Percid&eacute;s</trd>
  <trd lng="hu">s&uuml;g&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">echte baarzen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">okoniowate</trd>
  <trd lng="pt">perc&iacute;deos</trd>
  <trd lng="ro">Percide</trd>
  <trd lng="ru">&c_o;&c_k;&c_u;&c_n;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">ostrie&zcaron;ovit&eacute;</trd>
  <trd lng="uk">&#x43E;&#x43A;&c_u;&#x43D;&c_je;&c_v;&c_ib;</trd>
</drv>
</art>

