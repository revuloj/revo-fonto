<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dioskore.xml,v 1.2 2022/03/15 15:50:55 revo Exp $">
  <kap>
    <rad>dioskore</rad>/o
  </kap>

<drv mrk="dioskore.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio dioskoreacoj (<trd lng="la">Dioscorea</trd>), el kies plej 
      ofte tropikaj specioj, kelkaj estas gravaj kurac- kaj nutra&jcirc;plantoj:
      <ekz>
        la ordinara <tld/>o kaj la lupulo estas <klr>[&#x2026;]</klr> dekstrenvolvantaj plantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=568852">Volvanta planto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        japana <tld/>o a&ubreve; jamaino <klr>[&#x2026;estas]</klr> hejmi&gcirc;inta en Japanujo, Koreujo,
        &Ccirc;inujo, Tajvano kaj Asamo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=619026">Japana dioskoreo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="ignam.0o">ignamo</ref>
    <bld lok="&WCU;/thumb/9/95/Starr_030807-0029_Dioscorea_pentaphylla.jpg/360px-Starr_030807-0029_Dioscorea_pentaphylla.jpg" prm="CC BY 3.0">Dioscorea pentaphylla, Maui
        <fnt>
          <aut>Forest &amp; Kim Starr, Plants of Hawaii</aut>
          <lok><url ref="&WCW;/File:Starr_030807-0029_Dioscorea_pentaphylla.jpg">https://commons.wikimedia.org/wiki/File:Starr_030807-0029_Dioscorea_pentaphylla.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Yams</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x434;i&#x43E;&#x441;&#x43A;&#x43E;&#x440;&#x435;&#x44F;</trd>,
    <trd>&#x44F;&#x43C;&#x441;</trd>
  </trdgrp>
</drv>

</art>

