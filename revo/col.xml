<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>col</rad>/o
</kap>
<drv mrk="col.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malnova malgranda komunuza
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref> 
      de longo,
      valoranta proksimume 25 milimetrojn, nuntempe ankora&ubreve; uzata
      ekzemple en Usono <klr>(25,4 mm)</klr>:
      <ekz>
        <tld/>o estas dekduono de <ref tip="malprt"
        cel="fut.0o">futo</ref>;
      </ekz>
      <ekz>
        <ind>li ne lasis defali e&ccirc; unu <tld/>on de siaj
        pretendoj</ind><fnt>Z</fnt>.
      <trd lng="de">er wich nicht einen Zoll von seinen Forderungen
      ab</trd>
      <trdgrp lng="fr">
        <trd>il n'a pas c&eacute;d&eacute; un <ind>pouce</ind></trd>,
        <trd>il n'a pas c&eacute;d&eacute; un
        <ind>centim&egrave;tre</ind></trd>,
        <trd>il a camp&eacute; sur ses <ind>positions</ind></trd>
      </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_a;&c_l;&c_ja;</trd>
    <trd lng="bg">&c_i;&c_n;&c_ch;</trd>
    <trd lng="br">meutad</trd>
    <trd lng="de">Zoll</trd>
    <trd lng="en">inch</trd>
    <trd lng="es">pulgada</trd>
    <trd lng="fa">&a_A;&f_y;&a_n;&f_ch;</trd>
    <trd lng="fr">pouce <klr tip="ind">(mesure)</klr></trd>
    <trd lng="nl">duim</trd>
    <trdgrp lng="hu">
      <trd>h&uuml;velyk</trd>,
      <trd>col</trd>
    </trdgrp>
    <trd lng="it">pollice<klr> (mis.)</klr></trd>
    <trd lng="pl">cal</trd>
    <trd lng="pt">polegada</trd>
    <trd lng="ru">&c_d;&c_ju;&c_j;&c_m;</trd>
    <trd lng="sk">palec <klr>(d&lacute;&zcaron;kov&aacute;
    miera)</klr></trd>
    <trd lng="sv">tum</trd>
  </snc>
</drv>
</art>
