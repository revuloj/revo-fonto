    <trd lng="fa">&a_A;&f_y;&a_n;&f_ch;</trd>
    <trd lng="fr">pouce <klr tip="ind">(mesure)</klr></trd>
    <trd lng="id">inci</trd>
    <trd lng="nl">duim</trd>
    <trdgrp lng="hu">
      <trd>h&uuml;velyk</trd>,
      <trd>col</trd>
    </trdgrp>
    <trd lng="it">pollice<klr> (mis.)</klr></trd>
