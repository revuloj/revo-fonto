    <trd lng="pt">polegada</trd>
    <trd lng="ru">&c_d;&c_ju;&c_j;&c_m;</trd>
    <trd lng="sk">palec <klr>(d&lacute;&zcaron;kov&aacute;
    miera)</klr></trd>
    <trd lng="sv">tum</trd>
  </snc>
