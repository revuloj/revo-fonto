<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>da&ubreve;b</rad>/o
  </kap>
  <drv mrk="dauxb.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">TEK</uzo>
    <snc>
      <dif>
        &Ccirc;iu el la kurbigitaj tabuletoj, kiujn oni uzas por fari la
        korpon de barelo.  
      </dif>
    </snc>
    <trd lng="be">&c_k;&c_l;&c_jo;&c_p;&c_k;&c_a;
    <klr>(&c_b;&c_o;&c_ch;&c_k;&c_ib;)</klr></trd>
    <trd lng="ca">doga</trd>
    <trdgrp lng="de">
       <trd>Daube</trd>,
       <trd>Fassdaube</trd>
    </trdgrp>
    <trd lng="en">stave</trd>
    <trd lng="es">duela<klr> (de barril)</klr></trd>
    <trd lng="fr">douve<klr tip="ind"> (de tonneau)</klr></trd>
    <trd lng="hu">donga</trd>
    <trd lng="pl">klepka<klr> beczki</klr></trd>
    <trd lng="pt">aduela</trd>
    <trd lng="ru">&c_k;&c_l;&c_jo;&c_p;&c_k;&c_a;
    <klr>(&c_b;&c_o;&c_ch;&c_a;&c_r;&c_n;&c_a;&c_ja;)</klr></trd>
    </drv>
</art>
