<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ink.xml,v 1.37 2024/04/17 10:27:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ink</rad>/o
</kap>

<drv mrk="ink.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ink.0o.skriba">
    <dif>
      Kolora <ref tip="super" cel="flu.0ajxo.io_flua">flua&jcirc;o</ref> uzata
      por <ref cel="skrib.0i.signojn">skribi</ref> a&ubreve; <ref cel="desegn.0i.KOMUNE">desegni</ref>:
      <ekz>
        nigra, ru&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        skribi <klr>[&#8230;]</klr> per <tld/>o kaj kano
        <fnt>
          <bib>NT</bib>
          <lok>III. Johano 1:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi skribis en la libron per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 36:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi jam elskribis la tutan <tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la akvo <klr>[&#8230;]</klr> &scirc;ajnis nigra kiel <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i unu fojon gustumis <tld/>on, &scirc;i fari&gcirc;is soifa je &gcirc;i, kiel la
        tigro, kiu
        eklekis sangon, kaj &scirc;i volis subskribi &ccirc;ion eblan
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pt">tinta<klr tip="amb"> (para escrever)</klr></trd>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="ink.0o.presa">
    <ref tip="dif" cel="ink.pres0o">pres<tld/>o</ref>
    <trd lng="pt">tinta<klr tip="amb"> (de impress&atilde;o)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xFA3;&#xF42;&#xF0B;&#xF5A;&#xF0B;</trd>
  <trd lng="cs">inkoust</trd>
  <trd lng="de">Tinte</trd>
  <trd lng="en">ink</trd>
  <trd lng="es">tinta</trd>
  <trd lng="fr">encre</trd>
  <trd lng="hu">tinta</trd>
  <trd lng="id">tinta</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30F3;&#x30AF;</trd>,
    <trd>&#x30A4;&#x30F3;&#x30AD;</trd>
  </trdgrp>
  <trd lng="nl">inkt</trd>
  <trd lng="pl">atrament</trd>
  <trd lng="sk">atrament</trd>
  <trd lng="uk">&c_ch;&#x43E;&c_r;&#x43D;&c_i;&#x43B;&#x44C;&#x43D;&c_a; &#x43F;&#x43B;&#x44F;&#x43C;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x58A8;&#x6C34; <pr>m&#xF2;shu&#x1D0;</pr></trd>,
    <trd>&#x58A8; <pr>m&#xF2;</pr></trd>,
    <trd>&#x58A8;&#x9B5A; <pr>m&#xF2;y&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ink.0ujo">
  <kap><ofc>*</ofc><tld/>ujo
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -uj'</lok></fnt></kap>
  <snc>
    <dif>
      Malgranda vazo por <tld/>o:
      <ekz>
        mia skribilaro konsistas el <tld/>ujo, sablujo, kelke da
        plumoj, krajono kaj <ind><tld/>sorbilo</ind>
        <fnt><bib>F</bib><lok>&FE; 34</lok></fnt>;
        <trdgrp lng="de">
          <trd>Tintenl&ouml;scher</trd>,
          <trd>L&ouml;schwiege</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la <tld/>ujo elfalis el lia mano kaj rompi&gcirc;is kaj la tuta <tld/>o
        elver&scirc;i&gcirc;is
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la skribisto prenis <tld/>ujon kaj penikon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis tro profunde trempita en la <tld/>ujo
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kalam&aacute;&rcaron;</trd>,
    <trd>n&aacute;dr&zcaron; na polygr. barvu</trd>
  </trdgrp>
  <trd lng="de">Tintenfass</trd>
  <trdgrp lng="en">
    <trd>inkpot</trd>,
    <trd>inkwell</trd>
  </trdgrp>
  <trd lng="es">tintero</trd>
  <trd lng="fr">encrier</trd>
  <trd lng="hu">tintatart&oacute;</trd>
  <trd lng="id"><ind>tempat</ind> tinta</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30F3;&#x30AF;&#12388;&#x307C;</trd>,
    <trd>&#x30A4;&#x30F3;&#x30AF;&#12403;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">inktpot</trd>
  <trd lng="pl">ka&lstroke;amarz</trd>
  <trd lng="pt">tinteiro</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">kalam&aacute;r</trd>
  <trd lng="uk">&c_ch;&#x43E;&c_r;&#x43D;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&c_c;&#x44F;</trd>
</drv>

<drv mrk="ink.pres0o">
  <kap>pres<tld/>o</kap>
  <snc>
    <dif>
      Farbo a&ubreve; <tld/>o uzata en presado:
      <ekz>
        en la dinastio <nom>Sui</nom> (581&ndash;618 de nia erao) <klr>[&#8230;]</klr>
        oni inventis la kli&scirc;an presadon, t.e. unue gravuri
        ideogramojn a&ubreve; bildojn sur lignan tabulon la&ubreve;
        difinita normo, poste tegi sur &gcirc;in la pres<tld/>on, kaj fine presis
        pa&gcirc;ojn unu post alia
        <fnt>
          <vrk>Kvar famaj inventa&jcirc;oj</vrk>,
          <lok><url ref="http://es.chinabroadcast.cn/1/2004/06/07/1@10212.htm">&Ccirc;ina Radio
          Internacia</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_u;&c_k;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; <ind>&c_f;&c_a;&c_r;&c_b;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Druckerschw&auml;rze</trd>,
    <trd>Druckfarbe</trd>
  </trdgrp>
  <trd lng="en">printing ink</trd>
  <trd lng="cs">tiska&rcaron;sk&aacute; barva</trd>
  <trd lng="hu">nyomdafest&eacute;k</trd>
  <trd lng="id"><ind>tinta</ind> cetak</trd>
  <trd lng="ja">&#21360;&#21047;&#x30A4;&#x30F3;&#x30AD;  <pr>&#12356;&#12435;&#12373;&#12388;&#x30A4;&#x30F3;&#x30AD;</pr></trd>
  <trdgrp lng="pl">
    <trd>tusz</trd>,
    <trd>farba drukarska</trd>
  </trdgrp>
  <trd lng="pt">tinta<klr tip="amb"> (de impress&atilde;o)</klr></trd>
  <trd lng="ru">&c_t;&c_i;&c_p;&c_o;&c_g;&c_r;&c_a;&c_f;&c_s;&c_k;&c_a;&c_ja;
  &c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">tla&ccaron;iarska farba</trd>
  <trdgrp lng="zh">
    <trd>&#x6CB9;&#x58A8; <pr>y&#xF3;um&#xF2;</pr></trd>,
    <trd>&#x9ED1;&#x58A8; <pr>h&emacron;im&#xF2;</pr></trd>,
    <trd>&#21360;&#21047;&#x5F69;&#33394;&#x6CB9;&#x58A8;
    <pr>y&#xEC;nshu&#xE0;c&#x1CE;is&#xE8;y&#xF3;um&#xF2;</pr></trd>,
    <trd>&#21360;&#21047;&#x6CB9;&#x58A8; <pr>y&#xEC;nshu&#xE0;y&#xF3;um&#xF2;</pr></trd>,
    <trd>&#x58A8;&#33394; <pr>m&#xF2;s&#xE8;</pr></trd>,
    <trd>&#25171;&#21360;&#x58A8;&#x6C34; <pr>d&#x1CE;y&#xEC;nm&#xF2;shu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ink.cxina0o">
  <kap>&ccirc;ina <tld/>o,
    <var><kap>&hcirc;ina <tld/>o<fnt>Z</fnt></kap><uzo tip="stl">ARK</uzo></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="tucx.0o">Tu&ccirc;o</ref>:
      <ekz>
        desegna&jcirc;oj, faritaj sur papero per kolorigiloj kaj &hcirc;ina <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_ib; &c_a;&c_t;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_t;&c_u;&c_sh;</trd>
  </trdgrp>
  <trd lng="de">Tusche</trd>
  <trdgrp lng="en">
    <trd>carbon ink</trd>,
    <trd>Chinese ink</trd>
  </trdgrp>
  <trd lng="es">Tinta china</trd>
  <trd lng="fr">encre de Chine</trd>
  <trd lng="hu">tus</trd>
  <trd lng="id"><ind>tinta</ind> cina</trd>
  <trdgrp lng="ja">
    <trd>&#x58A8;  <pr>&#x307C;&#x304F;</pr></trd>,
    <trd>&#x58A8;&#x6C41;  <pr>&#x307C;&#x304F;&#12376;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">Oost-Indische <ind>inkt</ind></trd>
  <trd lng="pl">tusz</trd>
  <trd lng="pt">tinta nanquim</trd>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_mol;</trd>
  <trd lng="uk">&c_t;&c_u;&c_sh;</trd>
  <trd lng="zh">&#x58A8;&#x6C41; <pr>m&#xF2;zh&#x12B;</pr></trd>
</drv>

