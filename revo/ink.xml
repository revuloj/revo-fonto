<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ink</rad>/o
</kap>
<drv mrk="ink.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kolora flua&jcirc;o uzata por skribi, presi a&ubreve; desegni:
      <ekz>nigra, ru&gcirc;a <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="de">Tinte</trd>
  <trd lng="en">ink</trd>
  <trd lng="nl">inkt</trd>
  <trd lng="fr">encre</trd>
  <trd lng="pl">atrament</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_a; <klr>(por skribi a&ubreve; desegni)</klr></trd>,
    <trd>&c_t;&c_i;&c_p;&c_o;&c_g;&c_r;&c_a;&c_f;&c_s;&c_k;&c_a;&c_ja; &c_k;&c_r;&c_a;&c_s;&c_k;&c_a; <klr>(por presi)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="ink.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Malgranda vazo por <tld/>o. 
    </dif>
  </snc>
  <trd lng="de">Tintenfass</trd>
  <trd lng="nl">inktpot</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
<drv mrk="ink.hxina0o">
  <kap>&hcirc;ina <tld/>o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Speciala nigra kolorilo por desegni. 
    </dif>
    <ref tip="sin" cel="tucx.0o">tu&ccirc;o</ref>,
    <ref tip="vid" cel="lav.0umi">lavumi</ref>.
  </snc>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_mol;</trd>
</drv>
</art>
