<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>marsupiul</rad>/oj
</kap>
<drv mrk="marsup.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subklaso el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Marsupialia</trd>), kies inoj surhavas ventran
      po&scirc;on, subtenatan de ostoj, por gardi tie novnaski&gcirc;intajn idojn.
    </dif>
  </snc>
  <trd lng="de">Beuteltiere</trd>
  <trd lng="en">marsupials</trd>
  <trd lng="nl">buideldieren</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_ch;&c_a;&c_t;&c_y;&c_je; <klr tip="amb">(&c_p;&c_o;&c_d;&c_k;&c_l;&c_a;&c_s;&c_s; &c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_ju;&c_shch;&c_i;&c_h;)</klr></trd>
</drv>
</art>
