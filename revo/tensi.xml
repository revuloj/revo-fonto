<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tensi</rad>/o
</kap>
<drv mrk="tensi.0o">
  <kap><tld/>o</kap>
  <snc mrk="tensi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Stre&ccirc;orezisto de organa parieto al &gcirc;ia enhavo:
      <ekz>
        <ind>arteria <tld/>o</ind>.
        <trd lng="be">&c_a;&c_r;&c_t;&c_e;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y; &c_c;&c_ib;&c_s;&c_k;</trd>
        <trd lng="ru">&c_a;&c_r;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; <ind>&c_d;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_ib;&c_s;&c_k; <klr tip="amb">(&c_m;&c_e;&c_d;.)</klr></trd>
    <trdgrp lng="hu">
      <trd>nyom&aacute;s</trd>,
      <trd>tenzi&oacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_m;&c_je;&c_d;.)</klr></trd>
  </snc>
  <snc mrk="tensi.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      <klr>(inter du punktoj A kaj B)</klr>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      egala al la diferenco inter la <ref
      cel="potenc1.0o.ELE">potencialo</ref>
      en B kaj la potencialo en A:
      <ekz>
        la <tld/>o inter la du ekstrema&jcirc;oj de rezistilo estas
        proporcia al la <klr>(intenso de la)</klr> &gcirc;in trapasanta kurento.
      </ekz>
    </dif>
    <adm>
      Enkonduki "rezistilo". <aut>MB</aut>
    </adm>
    <refgrp tip="sin">
      <ref cel="potenc1.0adiferenco">potenciala diferenco</ref>,
      <ref cel="elektr.0astrecxo">elektra stre&ccirc;o</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fort.elektromova0o">elektromova forto</ref>,
      <ref cel="volt.v0o">volto</ref>.
    </refgrp>
    <trd lng="be">&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;.)</klr></trd>
  </snc>
  <trd lng="de">Spannung</trd>
  <trd lng="en">voltage</trd>
  <trdgrp lng="fr">
    <trd>tension<klr tip="ind"> (&eacute;lec.)</klr></trd>,
    <trd>diff&eacute;rence de potentiel</trd>,
    <trd>voltage</trd>
  </trdgrp>
  <trd lng="hu">fesz&uuml;lts&eacute;g</trd>
  <trd lng="nl">spanning</trd>
</drv>

<drv mrk="tensi.kontinua0o">
  <kap>kontinua <tld/>o, konstanta <tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o, konstanta la&ubreve; grando kaj direkto.
    </dif>
  </snc>
  <trd lng="fr">tension continue</trd>
  <trd lng="hu">egyenfesz&uuml;lts&eacute;g</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_n;&c_o;&c_je; <ind>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind>
  <klr tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;.)</klr></trd>
</drv>
</art>
