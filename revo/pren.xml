  <trdgrp lng="en">
    <trd>imprescriptible</trd>,
    <trd>inalienable<klr> (right)</klr></trd>,
    <trd>integral<klr> (part)</klr></trd>
  </trdgrp>
  <trd lng="fr">inali&eacute;nable</trd>
  <trdgrp lng="pl">
