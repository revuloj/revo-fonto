<drv mrk="pren.0o">
  <kap><ofc>*</ofc><tld/>o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5169">pren'o</url></lok></fnt></kap>
  <snc mrk="pren.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        de la famo pri la <tld/>o de <nom>Babel</nom> ektremos la tero
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 50:46</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grizan katinon <klr>[&#8230;]</klr> li kelkafoje trenis <klr>[&#8230;]</klr> je la vosto,
        portis &gcirc;in per <tld/>o &ccirc;irka&ubreve; la ventro kvaza&ubreve; <klr>[&#8230;]</klr> paka&jcirc;o
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>o de la refre&scirc;iga&jcirc;oj la vizitantoj <klr>[&#8230;]</klr> eniris
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i malmole ek<tld/>is lian nukon kaj arde premis lin al si, kiam &scirc;ia <tld/>o
        inerti&gcirc;is, li levi&gcirc;is
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekvilibro el dono kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2014/011306.php"
              >De bretoj al &scirc;rankoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">megv&eacute;tel<klr tip="ind"> (elfoglal&aacute;s)</klr></trd>
    <trd lng="pl">wzi&eogonek;cie</trd>
    <trd lng="ru">&c_v;&c_z;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="pren.0o.prenita">
    <dif>
      Io <tld/>ita:
      <ekz>
        <tld/>eto <klr>(<ref tip="sub" cel="pincx.0ajxo">pin&ccirc;a&jcirc;o</ref>)</klr> 
        da flartabako
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>eto</ind> da tabako;
        <trd lng="de">Prise</trd>
      </ekz>
      <ekz>
        ili enfermis grandan amason da fi&scirc;oj, kaj iliaj retoj ekrompi&gcirc;is <klr>[&#8230;]</klr>
        pro la <tld/>o de fi&scirc;oj, kiun ili akiris, miro kaptis lin
        <fnt>
          <bib>NT</bib>
          <lok>Luko 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lan&ccirc;into, surprize por &ccirc;iuj, maltrafis <klr>[&#8230;]</klr> &ndash; perdita <tld/>o! &ndash;
        blekegis la kato
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">SPO</uzo>
        pro <ind>karta <tld/>o</ind> perdi&gcirc;is bieno
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">Stich <klr>(Kartenspiel)</klr></trd>
        <trd lng="en">trick <klr>(cards)</klr></trd>
      </ekz>
    </dif>
    <trd lng="hu">csipet</trd>
    <trd lng="pl">bitka <klr> (wzi&aogonek;tka)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bran&iacute;</trd>,
    <trd>ko&rcaron;ist</trd>,
    <trd>odn&ecaron;t&iacute;</trd>,
    <trd>zmocn&ecaron;n&iacute;</trd>,
    <trd>z&aacute;bor</trd>,
    <trd>&uacute;lovek</trd>
  </trdgrp>
  <trd lng="es">cogedura<klr tip="amb"> (acci&oacute;n de coger)</klr></trd>
  <trdgrp lng="fr">
    <trd>prise</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>greep</trd>,
    <trd>inname</trd>
  </trdgrp>
  <trd lng="ro">luare</trd>
  <trdgrp lng="sk">
    <trd>obsadenie</trd>,
    <trd>vzatie</trd>,
    <trd>zmocnenie sa</trd>
  </trdgrp>
</drv>

