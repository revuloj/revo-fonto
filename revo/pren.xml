<drv mrk="pren.cxirkaux0i">
  <kap><ofc>*</ofc> &ccirc;irka&ubreve;<tld/>i <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-4997">&ccirc;irka&ubreve;'pren'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i, &ccirc;irka&ubreve;igante per la
      manoj a&ubreve; la brakoj:
      <ekz>
        ame &ccirc;irka&ubreve;<tld/>i iun;
      </ekz><ekz>
        <nom>Esav</nom> kuris al <klr>[<nom>Jakob</nom>]</klr> renkonte kaj
        &ccirc;irka&ubreve;<tld/>is lin, kaj &jcirc;etis sin sur
        lian kolon kaj kisis lin
        <fnt><bib>MT</bib><lok>&Gen; 33:4</lok></fnt>;
      </ekz><ekz>
        pro nehavado de rifu&gcirc;ejo ili &ccirc;irka&ubreve;<tld/>as rokon
        <fnt><bib>MT</bib><lok>&Ijo; 24:8</lok></fnt>;
      </ekz><ekz>
        via <ind>&ccirc;irka&ubreve;<tld/>o</ind> estas dol&ccirc;a kiel
        la dormo de laca homo
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 14</lok></fnt>;
        <trd lng="es">abrazo</trd>
        <trd lng="fr">&eacute;treinte</trd>
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        ili venis al loko, de kie per unu ekrigardo oni povis
        &ccirc;irka&ubreve;<tld/>i la tutan landon
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 4</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        nia movado &ccirc;irka&ubreve;<tld/>as pli kaj pli vastajn sferojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brak.cxirkaux0i">&ccirc;irka&ubreve;braki</ref>,
      <ref cel="brak.en0igi">enbrakigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_ma;&t_tsheg;&t_-a;&t_kha;&t_yata;&t_shabchu;&t_da;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trd lng="de">umarmen</trd>
  <trdgrp lng="es">
    <trd>abrazar<klr tip="ind"> (rodear con las manos o los
    brazos)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>embrasser<klr tip="ind"> (entourer)</klr></trd>,
    <trd>entourer</trd>,
    <trd>&eacute;treindre</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&het;&bet;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>meg&ouml;lel</trd>,
    <trd>k&ouml;rbefog</trd>,
    <trd>&aacute;tfog</trd>
  </trdgrp>
  <trd lng="nl">omvatten</trd>
  <trdgrp lng="pl">
    <trd>garn&aogonek;&cacute; <klr> (obejmowa&cacute;)</klr></trd>,
    <trd>ogarn&aogonek;&cacute;</trd>
  </trdgrp> 
  <trd lng="pt">abra&ccedil;ar</trd>
  <trd lng="ro">&icirc;mbr&abreve;&#539;i&#537;a</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

