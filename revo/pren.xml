    <trd>vzia&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pren.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pren.for0i.KOMUNE">
    <dif>
      <tld lit="P"/>i kaj forigi ion, senigante iun je &gcirc;i:
      <ekz>
        ne povi for<tld/>i la okulojn for de iu<fnt>Z</fnt>
        (<ref tip="vid" cel="katen.0i">kateni</ref>);
      </ekz><ekz>
        for<tld/>i de iu la panon per grandaj impostoj<fnt>Z</fnt>;
      </ekz><ekz>
        vi for<tld/>as de maljunuloj la prudenton<fnt>Z</fnt>;
      </ekz><ekz>
        mi ne volas for<tld/>i vian tempon<fnt>Z</fnt>
        (<ref tip="sin" cel="konsum.0i">konsumi</ref>);
      </ekz><ekz>
        for<tld/>i la vivon de iu<fnt>Z</fnt>;
      </ekz><ekz>
        for<tld/>i ion al iu<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>odebrat</trd>,
    <trd>odn&eacute;st</trd>,
    <trd>pobrat</trd>,
    <trd>rozebrat</trd>,
    <trd>sebrat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wegnehmen</trd>,
    <trd>entziehen</trd>
  </trdgrp>
  <trd lng="es">quitar y eliminar</trd>
  <trd lng="fr">emporter</trd>
  <trd lng="nl">wegnemen</trd>
  <trdgrp lng="pl">
    <trd>bi&cacute; figur&eogonek;</trd>,
    <trd>garn&aogonek;&cacute;<klr> (zabra&cacute;)</klr></trd>,
    <trd>kasowa&cacute;</trd>,
    <trd>odbiera&cacute;</trd>,
    <trd>usuwa&cacute;</trd>,
    <trd>zagarn&aogonek;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ridica</trd>,
    <trd>r&abreve;pi</trd>,
    <trd>&icirc;ndep&abreve;rta</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>odobra&tcaron;</trd>,
    <trd>zbavi&tcaron;</trd>
  </trdgrp>
</drv>

