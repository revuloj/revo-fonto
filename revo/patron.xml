<drv mrk="patron.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="sin" cel="protek.0i.patroni">Protekti</ref> kiel <tld/>o:
      <ekz>
        la organiza&jcirc;oj, kiuj <tld/>as la konferencon <klr>[&#x2026;]</klr> insistis por ricevi
        la statuson de eksterteritorieco
        <fnt>
          <bib>CKv</bib>
          <lok>14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unuan &leftquot;Ligon de Kolombiaj Esperantistoj&rightquot; <tld/>is en 1911 la
        Prezidento de la
        Respubliko
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la iniciaton <tld/>as la edzino de la prezidanto
        <fnt>
          <aut>Zofia Banet-Fornalowa</aut>
            <vrk><url ref="&Monato;/2000/007138.php">Malklariga mallongigo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 4</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mecena.0i">mecenati</ref>,
      <ref cel="ten.sub0i.FIG">subteni<sncref/></ref>,
      <ref cel="tegmen.0i">tegmenti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_je;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>patrocinar</trd>,
    <trd>emparar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chr&aacute;nit</trd>,
    <trd>ochra&ncaron;ovat</trd>,
    <trd>patronovat <klr>(hovor.)</klr></trd>
  </trdgrp>
  <trd lng="de">protektionieren</trd>
  <trdgrp lng="en">
    <trd>be patron to</trd>,
    <trd>sponsor</trd>,
    <trd>patronize (be a patron)</trd>,
    <trd>patronise (be a patron)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>patronner</trd>,
    <trd>soutenir<klr> (patronner)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>patron&aacute;l</trd>,
    <trd>p&aacute;rtfogol</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x63F4;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x3048;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4FDD;&#x8B77;&#x3059;&#x308B;  <pr>&#x307B;&#x3054;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5B88;&#x8B77;&#x3059;&#x308B;  <pr>&#x3057;&#x3085;&#x3054;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">beschermen</trd>
  <trd lng="pl">przewodzi&cacute;</trd>
  <trd lng="ro">conduce</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_je;&c_f;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chr&aacute;ni&tcaron;</trd>,
    <trd>ochra&ncaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="patron.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      <ref tip="super" cel="protek.0ado">Protektado</ref>, 
      publika subteno de eminentulo al institucio a&ubreve;
      homgrupo:
      <ekz>
        tiuj tagoj de universala <tld/>ado de la Kinematografo
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>De Beaufront</nom> <klr>[&#x2026;]</klr> &ndash; kreante la sistemon de <tld/>ado &ndash; ornamis sian
        Societon per la nomoj de eminentuloj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro ne limigas sin je <nom>SEJM</nom>, sed rakontas anka&ubreve; pri la altruda
        &leftquot;<tld/>ado&rightquot;
        (fakte kontrolado kaj premado) de la movado fare de burokratecaj kaj
        sekurecaj organoj en Sovet-Unio
        <fnt>
          <aut>Boris Kolker</aut>
            <vrk><url ref="&Monato;/2000/007107.php">Libro leg-enda pri tempo legend-a</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        artistoj transprenis <tld/>adojn por tiuj bestoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2006/009020.php">Bavario funebras pro Beppo Bruno</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        helpi la tibetajn infanojn kaj rifu&gcirc;intojn en Hindio pere de
        <tld/>ado kaj donacado
        <fnt>
          <aut>Robert Roze</aut>: <vrk>Verkistino
          &#x2010; orientalistino &#x2010; esploristino</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1999a, numero 1a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="egid.0o.protekto">egido<sncref/></ref>
    </refgrp>
  </snc>
  
  <trd lng="ca">protectorat</trd>
  <trdgrp lng="cs">
    <trd>chr&aacute;n&ecaron;n&iacute;</trd>,
    <trd>ochrana</trd>,
    <trd>patronace</trd>,
    <trd>patronance</trd>,
    <trd>patron&aacute;t</trd>,
    <trd>z&aacute;&scaron;tita</trd>
  </trdgrp>
  <trd lng="de">Schirmherrschaft</trd>
  <trdgrp lng="en">
    <trd>sponsorship</trd>,
    <trd>patronage</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>patronnage</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&eacute;dn&ouml;ks&eacute;g</trd>,
    <trd>p&aacute;rtfog&aacute;s</trd>,
    <trd>patron&aacute;l&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x63F4;  <pr>&#x3053;&#x3046;&#x3048;&#x3093;</pr></trd>,
    <trd>&#x4FDD;&#x8B77;  <pr>&#x307B;&#x3054;</pr></trd>,
    <trd>&#x5B88;&#x8B77;  <pr>&#x3057;&#x3085;&#x3054;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bescherming</trd>,
    <trd>auspici&euml;n</trd>
  </trdgrp>
  <trd lng="pl">auspicja</trd>
  <trd lng="ro">auspic&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_sh;&c_je;&c_f;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>patron&aacute;t</trd>,
    <trd>z&aacute;&scaron;tita</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_o;&c_n;&c_a;&c_t;</trd>,
    <trd>&c_o;&c_p;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_p;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="patron.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="patron.0eco.eco">
    <dif>
      <tld lit="P"/>a rolo, publika protekta funkcio rilate al
      institucio a&ubreve; homgrupo:
      <ekz>
        &ccirc;iuj ministroj prenis sur sin la <tld/>econ de nia kongreso
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en Barcelona en la 6a de septembro
          1909</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="mecena.0eco">mecenateco</ref>
  </snc>
  <snc mrk="patron.0eco.ado">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="patron.0ado"><tld lit="P"/>ado</ref>:
      <ekz>
        tiu &ccirc;i festivalo <klr>[&#x2026;]</klr> okazis sub la <tld/>eco de Unui&gcirc;o Internacia de
        Marionetistoj
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la <tld/>eco de la Pakto pri Stabileco en Sudorienta
        E&ubreve;ropo, ok Balkan-landoj subskribis interkonsenton pri regiona komerca
        liberigo
        <fnt>
          <aut>Evgeni Georgiev</aut>: 
          <vrk>Interkonsento pri komerca liberigo</vrk>,
          <bib>Monato</bib><lok>jaro 2002a, numero
          2a, p. 13a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>patronat</trd>,
    <trd>patrocini</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chr&aacute;n&ecaron;n&iacute;</trd>,
    <trd>ochrana</trd>,
    <trd>patronace</trd>,
    <trd>patronance</trd>,
    <trd>patron&aacute;t</trd>,
    <trd>z&aacute;&scaron;tita</trd>
  </trdgrp>
  <trd lng="de">Schirmherrschaft</trd>
  <trdgrp lng="en">
    <trd>sponsorship</trd>,
    <trd>patronage</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>auspices<klr> (patronnage)</klr></trd>,
    <trd>protection<klr> (patronnage)</klr></trd>
