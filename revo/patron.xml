<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>patron</rad>/o
</kap>
<drv mrk="patron.0o">
  <kap><tld/>o</kap>
  <snc mrk="patron.0o.protektanto">
    <dif>
      Eminenta persono protektanta alian personon a&ubreve; institucion:
      <ekz>
        por la projekto oni invitas <tld/>ojn kaj
        subtenantojn<fnt><aut>John G. Champlin</aut>: <vrk>Kiel pentri
        demokration?</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 12a,
        p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>protecteur</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_t;&c_r;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="patron.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Sanktulo protektanta urbon, pre&gcirc;ejon a&ubreve; tiujn, kiuj
      ricevis &ccirc;e bapto ties nomon:
      <ekz>
        la 14an de februaro soleni&gcirc;as la festo de sankta Valenteno,
        <tld/>o de amantoj, kiuj tiutage inter&scirc;an&gcirc;as
        festkartojn<fnt><aut>M&aacute;ire McKay</aut>: <vrk>Pri libroj... kaj pri
        braceletoj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 5a, p.
        10a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>saint patron</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="patron.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;e antikvaj Romanoj, ri&ccirc;a potenculo, kies protekton petis plebanoj
    </dif>
    <refgrp tip="vid">
      <ref cel="klient.0o">kliento</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Patron</trd>,
    <trd>Schutzherr</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>patron<klr> (protecteur)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>patroon</trd>,
    <trd>beschermheer</trd>,
    <trd>beschermheilige</trd>
  </trdgrp>
</drv>

<drv mrk="patron.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Protekti kiel <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>patronner</trd>,
    <trd>soutenir<klr> (patronner)</klr></trd>
  </trdgrp>
  <trd lng="nl">beschermen</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="patron.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Protektado, publika subteno de eminentulo al institucio a&ubreve; homgrupo:
      <ekz>
        helpi la tibetajn infanojn kaj rifu&gcirc;intojn en Hindio pere de
        <tld/>ado kaj donacado<fnt><aut>Robert Roze</aut>: <vrk>Verkistino
        - orientalistino - esploristino</vrk>, <bib>Monato</bib><lok>jaro
        1999a, numero 1a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>patronnage</trd>
  </trdgrp>
</drv>

<drv mrk="patron.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      <tld lit="P"/>a rolo, publika protekta funkcio rilate al institucio
      a&ubreve; homgrupo:
      <ekz>
        sub la <tld/>eco de la Pakto pri Stabileco en Sudorienta E&ubreve;ropo,
        ok Balkan-landoj subskribis interkonsenton pri regiona komerca
        liberigo<fnt><aut>Evgeni Georgiev</aut>: <vrk>Interkonsento pri
        komerca liberigo</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero
        2a, p. 13a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="egid.0o.protekto">egido</ref>
    </refgrp>
  </snc>
    <trdgrp lng="fr">
      <trd>auspices<klr> (patronnage)</klr></trd>,
      <trd>protection<klr> (patronnage)</klr></trd>
    </trdgrp>
</drv>
</art>
