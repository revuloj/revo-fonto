<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: barbaros.xml,v 1.5 2022/06/01 07:33:32 revo Exp $">
    <kap>
      <rad>Barbaros</rad>/o <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
   </kap>
    <drv mrk="barbaros.0o">
      <kap><tld/>o</kap>
      <snc mrk="barbaros.0o.HIS">
        <uzo tip="fak">HIS</uzo>
        <uzo tip="fak">PERS</uzo>
        <dif>
          Kromnomo de la Frederiko la 1-a Hohen&scirc;ta&ubreve;feno (1122&ndash;1190),
          imperiestro de la Sankta Roma Imperio:
          <ekz>
            li ricevis la nobelecon de <tld/>o, al kiu li servis
            kontra&ubreve; marrabistoj
            <fnt>
              <aut>&Scirc;ilero, tr. &Z;</aut>: <vrk>La rabistoj</vrk>.
              <lok>4:2.</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_B;&c_a;&c_r;&c_b;&c_a;&c_r;&c_o;&c_s;&c_a;</trd>
      <trd lng="en">Barbarossa</trd>
      <trd lng="fr">Barberousse</trd>
      <trd lng="pl">Barbarossa</trd>
      <trd lng="ru">&c_B;&c_a;&c_r;&c_b;&c_a;&c_r;&c_o;&c_s;&c_s;&c_a;</trd>
      <trd lng="uk">&c_B;&c_a;&c_r;&c_b;&c_a;&c_r;&#x43E;&c_s;&c_s;&c_a;</trd>
</drv>
  </art>

