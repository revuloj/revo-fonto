          <ekz>
            li ricevis la nobelecon de <tld/>o, al kiu li servis
            kontra&ubreve; marrabistoj
            <fnt>
              <aut>&Scirc;ilero, tr. &Z;</aut>: <vrk>La rabistoj</vrk>.
              <lok>4:2.</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">Barbarossa</trd>
      <trd lng="ru">&c_B;&c_a;&c_r;&c_b;&c_a;&c_r;&c_o;&c_s;&c_s;&c_a;</trd>
    </drv>
  </art>

