<drv mrk="et.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Infaneto, ido:
      <ekz>
        la an&gcirc;elo <klr>[&#x2026;]</klr> kisis <klr>[&#x2026;]</klr> la infanon, kaj la <tld/>ulo duone
        malfermis siajn okulojn
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ludas <klr>[la dramojn]</klr> anta&ubreve; la <tld/>uloj, kaj la <tld/>uloj ploras,
        kiel iam ploris patro kaj patrino
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        En supra &ccirc;ambro lulo ... lulo ... /
        An&gcirc;ele dormas la <tld/>ulo
        <fnt><aut>R. Schwartz</aut>:
        <vrk>La diversaj a&gcirc;oj de l' homo</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="beb.0o">bebo</ref>,
      <ref cel="id.0o.GEN">ido<sncref/></ref>,
      <ref cel="grand.mal0egulo">malgrandegulo</ref>,
      <ref cel="nan.0o">nano</ref>
    </refgrp>
    <trd lng="de">Kleiner</trd>
    <trd lng="en">little one</trd>
    <trdgrp lng="hu">
      <trd>p&ouml;tt&ouml;m<klr tip="ind"> (ember)</klr></trd>,
      <trd>kicsike</trd>
    </trdgrp>
    <trd lng="io">mikro</trd>
    <trd lng="fr">petit<klr> (subst.)</klr></trd>
    <trd lng="nl">kleintje</trd>
    <trdgrp lng="pl">
      <trd>maluch</trd>,
      <trd>maluszek</trd>,
      <trd>berbe&cacute;</trd>,
      <trd>karze&lstroke;</trd>,
      <trd>kurdupel<klr> (obra&zacute;l.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_l;&c_y;&c_sh;</trd>
    <trd lng="tp">jan lili</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>d&ecaron;&tcaron;&aacute;tko</trd>,
    <trd>mrn&ecaron;</trd>,
    <trd>prcek</trd>,
    <trd>&scaron;kvrn&ecaron;</trd>
  </trdgrp>
