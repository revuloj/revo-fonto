<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>in&gcirc;eni</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ingxeni.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sagac.0a">Sagaca</ref>, ema inventi
      neatenditajn solvojn kaj te&hcirc;nika&jcirc;ojn:
      <ekz>
        <ctl>La in&gcirc;enia hidalgo Don Quijote de la
        Mancha</ctl><fnt><bib>DonKihxoto</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ing&eacute;nieux</trd>
</drv>

<drv mrk="ingxeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Talento, lerto por eltrovi ta&ubreve;gajn solvojn fronte al la
      cirkonstancoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ing&eacute;niosit&eacute;</trd>,
    <trd>inventivit&eacute;</trd>
  </trdgrp>
</drv>
</art>

