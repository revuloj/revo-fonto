<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pelamid.xml,v 1.7 2024/02/03 15:20:03 revo Exp $">
<kap>
  <rad>pelamid</rad>/o
</kap>
<drv mrk="pelamid.0o">
  <kap><tld/>o</kap>
  <snc mrk="pelamid.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Marfi&scirc;o de tropikaj kaj subtropikaj maroj, parenca al <ref cel="skombr.0o">skombro</ref>j
      kaj <ref cel="tinus.0o">tinuso</ref>j
      (<trd lng="la">Katsuwonus pelamis</trd>):
      <ekz>
        la gastoj estis metantaj sur la telerojn sprotojn, sardinojn,
        <tld/>ojn
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/8a/Katsuwonus_pelamis_Java.jpg/640px-Katsuwonus_pelamis_Java.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o, Palabuhanratu, Okcidenta Javo
        <fnt>
          <aut>Wibowo Djatmiko (Wie146), propra verko</aut>
          <lok><url ref="&WCW;/File:Katsuwonus_pelamis_Java.jpg"
              >https://commons.wikimedia.org/wiki/File:Katsuwonus_pelamis_Java.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_l;&c_ja;&c_m;&c_ib;&c_d;&c_a;</trd>
  <trd lng="de">Echter <ind>Bonito</ind></trd>
  <trdgrp lng="fr">
    <trd>bonite</trd>,
    <trd>leiche</trd>,
    <trd>p&eacute;lamide</trd>,
    <trd>p&eacute;lamyde</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boniet</trd>,
    <trd>bonetvis</trd>,
    <trd>albikoor</trd>
  </trdgrp>
  <trd lng="pl">bonito</trd>
  <trd lng="pt">bonito <klr tip="amb"> (peixe)</klr></trd>
  <trd lng="ro">bonit&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_l;&c_a;&c_m;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_m;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j; &c_t;&c_u;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>

