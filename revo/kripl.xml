    <trd>&#x80A2;&#x4F53;&#x4E0D;&#x81EA;&#x7531;&#x306E;  <pr>&#x3057;&#x305F;&#x3044;&#x3075;&#x3058;&#x3086;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x6B20;&#x9665;&#x306E;&#x3042;&#x308B;  <pr>&#x3051;&#x3063;&#x304B;&#x3093;&#x306E;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>misvormd</trd>,
    <trd>vergroeid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kaleki</trd>,
    <trd>u&lstroke;omny</trd>,
    <trd>niepe&lstroke;nosprawny</trd>,
    <trd>dotkni&eogonek;ty kalectwem</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_v;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>chrom&yacute;</trd>,
    <trd>skazen&yacute;</trd>,
    <trd>zmrza&ccaron;en&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_a;&c_l;&c_ib;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_l;&c_ib;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
