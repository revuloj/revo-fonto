<drv mrk="public.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Skribi en gazeto a&ubreve; revuo:
      <ekz>
        politika <ind><tld/>isto</ind><fnt>Z</fnt>.
        <trd lng="be">&c_p;&c_u;&c_b;&c_l;&c_ib;&c_c;&c_y;&c_s;&c_t;</trd>
        <trdgrp lng="hu">
          <trd>k&ouml;z&iacute;r&oacute;</trd>,
          <trd>&uacute;js&aacute;g&iacute;r&oacute;</trd>,
          <trd>publicista</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_u;&c_b;&c_l;&c_i;&c_c;&c_i;&c_s;&c_t;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_u; &c_p;&c_r;&c_e;&c_s;&c_je;)</klr></trd>
    <trdgrp lng="hu">
      <trd>(&uacute;js&aacute;got) <ind>&iacute;r</ind></trd>,
      <trd>publik&aacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_v; &c_g;&c_a;&c_z;&c_je;&c_t;&c_je;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Profesie reklami.
    </dif>
    <trd lng="be">&c_r;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ru">&c_r;&c_je;&c_k;&c_l;&c_a;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>ver&ouml;ffentlichen</trd>,
    <trd>publizieren</trd>
  </trdgrp>
  <trd lng="en">publicize</trd>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;noss&aacute;got biztos&iacute;t</trd>,
    <trd>rekl&aacute;moz</trd>
  </trdgrp>
  <trd lng="nl">publiceren</trd>
</drv>
