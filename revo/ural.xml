<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Ural</rad>/o
</kap>

<drv mrk="ural.U0o">
  <kap><tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="ural.U0o.montaro">
    <dif>
      Montara &ccirc;eno en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>, inter 
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>Urals</trd>, <trd>Ural Mountains</trd></trdgrp>
  </snc>
  <snc mrk="ural.U0o.rivero">
    <dif>
      Rivero en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>, kaj
      Okcidenta Kaza&hcirc;ujo, fluanta 2533km de la suda parto de la
      <tld/>a montaro suden, okcidenten kaj suden en
      <ref cel="kaspi.K0o">Kaspion</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>Ural</trd>, <trd>Ural River</trd></trdgrp>
  </snc>
  <trd lng="ru">&c_U;&c_r;&c_a;&c_l;</trd>
</drv>

<drv mrk="ural.0a">
  <kap><tld lit="u"/>a</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="ural.0a.GEOG">
    <dif>
      Rilata al <tld/>o <sncref ref="ural.U0o.rivero"/> a&ubreve;
      <sncref ref="ural.U0o.montaro"/>.
    </dif>
  </snc>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="ural.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de la <tld lit="u"/>a regiono (en la montaro kaj
      &ccirc;irka&ubreve;e).
    </dif>
  </snc>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_je;&c_c;</trd>
</drv>

<drv mrk="ural.0alingvo">
  <kap><tld/>a lingvo</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Etna lingvo el granda lingvofamilio de popoloj lo&gcirc;antaj en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref> kaj Orienta
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      (interalie, la lingvoj hungara kaj suoma).
    </dif>
  </snc>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
</drv>

<drv mrk="ural.0aaltajalingvofamilio">
  <kap><tld/>a-altaja lingvfamilio</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Hipoteze rekonstruata superfamilio konsistanta el la
      <ref tip="prt" cel="ural.0alingvo"><tld/>aj lingvoj</ref>
      kaj la 
      <ref tip="prt" cel="altaj.0a.lingvofamilio">altaja
      lingvofamilio</ref>.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Ural-Altaic group</trd>,
    <trd>Turanian language group</trd></trdgrp>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_o;-&c_a;&c_l;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_a;&c_k;&c_r;&c_o;&c_s;&c_je;&c_m;&c_mol;&c_ja; &c_ja;&c_z;&c_y;&c_k;&c_o;&c_v;</trd>
</drv>

</art>

<!--
$Log$
-->
</vortaro>