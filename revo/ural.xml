<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ural.xml,v 1.23 2021/05/12 17:33:28 revo Exp $">
<kap>
<rad>Ural</rad>/o
</kap>
<drv mrk="ural.U0o">
 <kap><tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="ural.U0o.montaro">
    <dif>
      <ref tip="lst" cel="mont.0aro" lst="voko:montaroj">Montara</ref>
      &ccirc;eno en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>, inter
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>:
      <ekz>
        la centro de fizika E&ubreve;ropo, de Atlantiko &gcirc;is <tld/>o, situas proksime al Vilniuso en Litovio
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.be/2004/008334.html">Centra demando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        movi&gcirc;ante de <tld/>o en la profundon de Siberio, oni malkovras 
        malanta&ubreve; la altaja mont&ccirc;eno misteran landon, kiu per siaj ne&gcirc;kovritaj pintoj kunfandi&gcirc;as kun
        la nuboj
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="https://www.monato.be/2004/008350.html">Tiu tago</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">els <ind>Ural</ind>s</trd>
    <trdgrp lng="en">
      <trd>Urals</trd>, <trd>Ural Mountains</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Urales</trd>, <trd>Montes Urales</trd>
    </trdgrp>
    <trd lng="fr">Oural</trd>
    <trd lng="it">Urali</trd>
    <trdgrp lng="pt">
      <trd>montes Urais</trd>,
      <trd>montes do Ural</trd>
    </trdgrp>
  </snc>
  <snc mrk="ural.U0o.rivero">
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref> en <ref tip="malprt"
      cel="rus.R0ujo">Rusujo</ref> kaj
      Okcidenta Kaza&hcirc;ujo, fluanta 2533&nbsp;km de la suda parto de la
      <tld/>a montaro suden, okcidenten kaj suden en
      <ref cel="kaspi.K0o">Kaspion</ref>.
    </dif>
    <trd lng="ca">Ural</trd>
    <trdgrp lng="en">
      <trd>Ural</trd>, <trd>Ural River</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Ural</trd>, <trd>R&iacute;o Ural</trd>
    </trdgrp>
    <trd lng="fr">Oural</trd>
    <trd lng="it">Ural</trd>
  </snc>
  <trd lng="be">&c_U;&c_r;&c_a;&c_l;</trd>
  <trd lng="bg">&c_U;&c_r;&c_a;&c_l;</trd>
  <trd lng="de">Ural</trd>
  <trd lng="fi">Ural</trd>
  <trd lng="hu">Ur&aacute;l</trd>
  <trd lng="nl">Oeral</trd>
  <trd lng="ru">&c_U;&c_r;&c_a;&c_l;</trd>
  <trd lng="sk">Ural</trd>
</drv>

<drv mrk="ural.0a">
  <kap><tld lit="u"/>a</kap>
  <snc mrk="ural.0a.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="ural.U0o.montaro"/>
      (a&ubreve;, eventuale, <tld/>o <sncref ref="ural.U0o.rivero"/>).
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">urali&agrave;<klr> (adj.)</klr></trd>
  <trdgrp lng="fi">
     <trd>Uralin</trd>,
     <trd>uralilainen <klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="fr">ouralien</trd>
  <trd lng="hu">ur&aacute;li</trd>
  <trd lng="nl">Oeralisch</trd>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="ural.0ano">
  <kap><tld lit="u"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de la <tld lit="u"/>a regiono (en la montaro kaj
      &ccirc;irka&ubreve;e).
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="ca">urali&agrave;<klr> (subst.)</klr></trd>
  <trd lng="fi">uralilainen <klr tip="amb"> (subst.)</klr></trd>
  <trd lng="fr">ouralien</trd>
  <trd lng="hu">ur&aacute;li (lakos)</trd>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_je;&c_c;</trd>
</drv>

<drv mrk="ural.0alingvo">
  <kap><tld lit="u"/>a lingvo</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Etna lingvo el granda lingvofamilio de popoloj lo&gcirc;antaj en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref> kaj Orienta
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      (interalie, la lingvoj hungara kaj suoma):
      <ekz>
        <tld lit="u"/>aj estas la finna kaj la hungara
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007025.html">La Man&ccirc;ura:
          Formortanta lingvo de malplimulto en &Ccirc;inio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
&c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="ca">llengua <ind>ural</ind>iana</trd>
  <trd lng="fi">uralilainen kieli</trd>
  <trd lng="fr">langue ouralienne</trd>
  <trd lng="hu">ur&aacute;li nyelv</trd>
  <trd lng="nl">Oeralisch</trd>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;
  &c_ja;&c_z;&c_y;&c_k;</trd>
</drv>

<drv mrk="ural.0aaltajalingvofamilio">
  <kap><tld lit="u"/>a-altaja lingvofamilio</kap>
  <tezrad fak="LIN"/>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Hipoteze rekonstruata superfamilio konsistanta el la
      <ref tip="prt" cel="ural.0alingvo"><tld lit="u"/>aj
      lingvoj</ref>
      kaj la
      <ref tip="prt" cel="altaj.0a_lingvofamilio">altaja
      lingvofamilio</ref>.
    </dif>
  </snc>
  <trd lng="ca">lleng&uuml;es uraloaltaiques</trd>
  <trdgrp lng="en">
    <trd>Ural-Altaic group</trd>,
    <trd>Turanian language group</trd>
  </trdgrp>
  <trd lng="es">grupo uralo-altaico</trd>
  <trd lng="fi">uralilais-altailainen kielikunta</trd>
  <trd lng="fr">langues ouralo-alta&iuml;ques</trd>
  <trd lng="hu">ur&aacute;l-altaji nyelvcsal&aacute;d</trd>
  <trd lng="nl">Oeral-Alta&iuml;sche taalgroep</trd>
  <trd

lng="ru">&c_u;&c_r;&c_a;&c_l;&c_o;-&c_a;&c_l;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_ja;
  &c_m;&c_a;&c_k;&c_r;&c_o;&c_s;&c_je;&c_m;&c_mol;&c_ja;
  &c_ja;&c_z;&c_y;&c_k;&c_o;&c_v;</trd>
</drv>
</art>
