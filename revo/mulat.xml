<art mrk="$Id: mulat.xml,v 1.20 2023/09/02 06:04:14 revo Exp $">
<kap>
  <rad>mulat</rad>/o <fnt><aut>A. Grabowski</aut>, <vrk>Slownik jezyka
  Esperanto</vrk>, <lok>[1910-1916?]</lok></fnt>
</kap>
<drv mrk="mulat.0o">
  <kap><tld/>o</kap>
  <snc mrk="mulat.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="ras.miks0ulo.BIO">Miksrasulo</ref>, ido de
      nigrarasa kaj blankrasa gepatroj; duonnegro:
      <ekz>
        pacemaj <tld/>oj duonkantante prilaboras
        &gcirc;ardenojn, kie senorde kreskas la odora mento
        kaj la ebriiga kanabo
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;&c_a;&c_t;</trd>
  <trd lng="bg">&c_m;&c_u;&c_l;&c_a;&c_t;</trd>
  <trd lng="ca">mulat</trd>
  <trdgrp lng="cs">
    <trd>mulat</trd>,
    <trd>m&iacute;&scaron;enec</trd>
  </trdgrp>
  <trd lng="de">Mulatte</trd>
  <trd lng="en">mulatto</trd>
  <trd lng="es">mulato</trd>
  <trd lng="fr">mul&acirc;tre</trd>
  <trd lng="ja">&#x30E0;&#x30E9;&#x30FC;&#x30C8;</trd>
  <trd lng="nl">mulat</trd>
  <trd lng="pl">mulat</trd>
  <trd lng="pt">mulato <klr tip="amb">(filho de branco(a) e
  negro(a))</klr></trd>
  <trd lng="ro">mulatru</trd>
  <trd lng="ru">&c_m;&c_u;&c_l;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>mie&scaron;anec</trd>,
    <trd>mulat</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x443;&#x43B;&#x430;&#x442;</trd>
</drv>
</art>
