<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ha&ubreve;s</rad>/o
</kap>
<drv mrk="hauxs.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <fnt><bib>KrED</bib></fnt>
    <dif>
      &Gcirc;enerala, persista alti&gcirc;o de borsaj kurzoj.
    </dif>
    <trd lng="de">Hausse</trd>
    <trd lng="ru">&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_o;&c_v; &c_c;&c_je;&c_n;&c_n;&c_y;&c_h; &c_b;&c_u;&c_m;&c_a;&c_g;</trd>
  </snc>
  <snc>
    <fnt><bib>IKEV</bib></fnt>
    <dif>
      Alta konjunkturo. Tempintervalo de ekonomia florado. Kp. depresio.
    </dif>
    <trdgrp lng="de">
      <trd>Hochkonjunktur</trd>,
      <trd>Aufschwung</trd>
    </trdgrp>
    <trd lng="en">boom</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;</trd>,
      <trd>&c_b;&c_u;&c_m;</trd> 
    </trdgrp>
  </snc>
  <trd lng="hu">hossz</trd>
</drv>

<drv mrk="hauxs.mal0o">
  <kap>mal<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      &Gcirc;enerala, persista malalti&gcirc;o de borsaj kurzoj.
    </dif>
    <trd lng="de">Baisse</trd>
    <trd lng="ru">&c_p;&c_o;&c_n;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_y;&c_h; &c_k;&c_u;&c_r;&c_s;&c_o;&c_v;</trd>
  </snc>
  <snc>
    <dif>
      Malalta konjunkturo. Tempintervalo de ekonomia depresio.
    </dif>
    <trd lng="de">Abschwung</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_s;&c_p;&c_a;&c_d;</trd>,
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
