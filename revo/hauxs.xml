<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hauxs.xml,v 1.12 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>ha&ubreve;s</rad>/o
</kap>
<drv mrk="hauxs.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="hauxs.0o.borsa">
    <fnt><bib>KrED</bib></fnt>
    <dif>
      &Gcirc;enerala, persista alti&gcirc;o de borsaj kurzoj.
    </dif>
    <trd lng="ca">al&ccedil;a</trd>
    <trd lng="de">Hausse</trd>
    <trd lng="pl">hossa</trd>
    <trd lng="pt">alta <klr tip="amb">(econ.)</klr></trd>
    <trd lng="ru">&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je;
    &c_k;&c_u;&c_r;&c_s;&c_o;&c_v; &c_c;&c_je;&c_n;&c_n;&c_y;&c_h;
    &c_b;&c_u;&c_m;&c_a;&c_g;</trd>
    <trd lng="zh">&#x725B;&#x5E02; <pr>ni&#xFA;sh&#xEC;</pr></trd>
</snc>
  <snc mrk="hauxs.0o.ekonomia">
    <fnt><bib>IKEV</bib></fnt>
    <dif>
      Alta <ref tip="super" cel="konjun3.0o.EKON">konjunkturo</ref>, tempintervalo
      de ekonomia florado:
      <ekz>
        la interna haveno en <nom>Duisburg</nom> <klr>[&#x2026;]</klr> estis dum la <tld/>o de la
        Industria Revolucio en Germanujo pli ol jarcenton la centra havena kaj
        komerca placo de la urbo 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=15959">Duisburg</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="depresi.0o.EKON">depresio</ref>
    <trd lng="ca">auge</trd>
    <trdgrp lng="de">
      <trd>Hochkonjunktur</trd>,
      <trd>Aufschwung</trd>
    </trdgrp>
    <trd lng="en">boom</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
      &c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;</trd>,
      <trd>&c_b;&c_u;&c_m;</trd> 
    </trdgrp>
  </snc>
  <trd lng="hu">hossz</trd>
  <trdgrp lng="it">
      <trd>boom<klr> economico</klr></trd>,
      <trd><ind>congiuntura</ind> favorevole</trd>
    </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;&#x456;&#x447;&#x43D;&#x435; &#x43F;&#x456;&#x434;&#x43D;&#x435;&#x441;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x435;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;&#x456;&#x447;&#x43D;&#x438;&#x439; &#x440;&#x43E;&#x437;&#x43A;&#x432;&#x456;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="hauxs.mal0o">
  <kap>mal<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="hauxs.mal0o.borsa">
    <dif>
      &Gcirc;enerala, persista malalti&gcirc;o de borsaj kurzoj:
      <ekz>
        monpolitikoj dizajnitaj por kontra&ubreve;agi ekonomiajn mal<tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=573301">Miksa ekonomio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">baixa</trd>
    <trd lng="de">Baisse</trd>
    <trd lng="pl">bessa</trd>
    <trd lng="ru">&c_p;&c_o;&c_n;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;
    &c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_y;&c_h;
    &c_k;&c_u;&c_r;&c_s;&c_o;&c_v;</trd>
    <trd lng="zh">&#x9577;&#x6642;&#x9593;&#x7684;&#x5F37;&#x70C8;&#x4E0B;&#x8DCC; <pr>ch&#xE1;ngsh&#xED;ji&#x101;ndeqi&#xE1;ngli&#xE8;xi&#xE0;di&#x113;</pr></trd>
</snc>
  <snc mrk="hauxs.mal0o.ekonomia">
    <dif>
      Malalta <ref tip="super" cel="konjun3.0o.EKON">konjunkturo</ref>, tempintervalo
      de ekonomia <ref tip="sin" cel="depresi.0o.EKON">depresio</ref>:
      <ekz>
        la 14-a jarcento devis marki mal<tld/>on en la ri&ccirc;a&jcirc;oj de la urbo: unue venis
        ekaperoj de pesto, poste la Centjara Milito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4197">Tuluzo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">depressi&oacute;</trd>
    <trd lng="de">Abschwung</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
      &c_s;&c_p;&c_a;&c_d;</trd>,
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
      &c_d;&c_je;&c_p;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="it">
    <trd>crisi<klr> economica</klr></trd>,
    <trd><ind>congiuntura</ind> sfavorevole</trd>
  </trdgrp>
</drv>
</art>
