<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>harem</rad>/o
</kap>
<drv mrk="harem.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Apartamento rezervita &ccirc;e la mahometanoj al la edzinoj; virinejo. 
    </dif>
    <refgrp tip="sin">
      <ref cel="ginece.0o">gineceo</ref>,
      <ref cel="serajl.0o">serajlo</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      La aro de tiuj edzinoj:
      <ekz>Salomono havis <tld/>on el tricent virinoj.</ekz>
    </dif>
  </snc>
  <trd lng="de">Harem</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_je;&c_m;</trd>
</drv>
</art>
