<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kanti</rad>/a
</kap>

<drv mrk="kanti.0a">
  <kap><tld/>a</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
    <dif>
    Rilata al <tld lit="K"/>o kaj al liaj filozofiaj teorioj: 
    <ekz><tld/>a kategorio</ekz>
    </dif>
  </snc>
  <trd lng="fa">&f_k;&a_A;&a_n;&a_t;&f_y;</trd>
  <trd lng="it">kantiano</trd>
  <trd lng="en">Kantian</trd>
</drv>

<drv mrk="kanti.0o">
  <kap><tld lit="K"/>o</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
  <uzo tip="fak">PERS</uzo>
    <dif>
    Immanuel Kant (1724 &ndash; 1804). Germana filozofo, suma elstarulo
    de la germana <ref tip="vid" cel="kler.0ismo.HIS">klerismo</ref>,
    kaj a&ubreve;toro de filozofia revolucio pri kapabloj kaj limoj de
    la homa kono.
    </dif>
  </snc>
  <trd lng="en">Kant</trd>
  <trd lng="fa">&a_A;&f_y;&a_m;&a_A;&a_n;&a_w;&a_y_hamza;&a_l; <ind>&f_k;&a_A;&a_n;&a_t;</ind></trd>
  <trd lng="it">Kant</trd>
  <trd lng="pl">Kant</trd>
</drv>

<drv mrk="kanti.0isto">
  <kap><tld/>isto</kap>
  <snc>
<uzo tip="fak">FIL</uzo>
    <dif>
    Sekvanto a&ubreve; estimanto de filozofiaj metodoj kaj teorioj de 
    <tld lit="K"/>o.
    </dif>
  </snc>
  <trd lng="fa">&f_k;&a_A;&a_n;&a_t;&zwnj;&f_g;&a_r;&a_A;</trd>
  <trd lng="it">kantiano</trd>
  <trd lng="pl">kantysta</trd>
</drv>

<drv mrk="kanti.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
    <dif>
    Filozofa doktrino kaj metodo de <tld lit="K"/>o.
    </dif>
  </snc>
  <trd lng="fa">&f_k;&a_A;&a_n;&a_t;&zwnj;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  <trd lng="it">scuola kantiana</trd>
  <trd lng="pl">kantyzm</trd>
</drv>

</art>

