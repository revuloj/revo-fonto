<vortaro>
<art mrk="$Id$">
<kap><rad>glot</rad>/o</kap>
<drv mrk="glot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Kavoforma parto de laringo inter la vo&ccirc;aj kordoj.
    </dif>
    <ref tip="vid" cel="plosiv.glota0o"><tld/>a plosivo.</ref>
  </snc>
  <trd lng="ca">glotis</trd>
  <trdgrp lng="de">
    <trd>Glottis</trd>,
    <trd>Stimmritze</trd>
  </trdgrp>
  <trd lng="es">glotis</trd>
  <trd lng="fr">glotte</trd>
  <trdgrp lng="hu">
    <trd>hangr&eacute;s</trd>,
    <trd>glottis</trd>
  </trdgrp>
  <trd lng="it">glottide</trd>
  <trdgrp lng="nl">
    <trd>glottis</trd>,
    <trd>stemspleet</trd>
  </trdgrp>
  <trd lng="pl">g&lstroke;o&sacute;nia</trd>
  <trd lng="pt">glote</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_s;&c_o;&c_v;&c_a;&c_ja; &c_shch;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
