<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>trink</rad>/i
</kap>
<drv mrk="trink.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Gluti flua&jcirc;on:
      <ekz>
        <tld/>i kiel spongo
        <klr>(avide kaj multe)</klr>;
      </ekz>
      <ekz>
        <tld/>i akvon el la puto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        el glaso;
      </ekz>
      <ekz>
        doni <tld/>i al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u el la vino, kiun mi enver&scirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i pro ies sano<fnt>Z</fnt>;
      </ekz>
      <ekz>
        aroma, ebriiga <tld/>a&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi min man&gcirc;igos, mi vin <tld/>igos<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el<tld/>i per unu
        <ref tip="vid" cel="tir.0o">tiro</ref><fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="drink.0i">drinki</ref>
    <refgrp tip="vid">
      <ref cel="sorb.0i">sorbi</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">p&iacute;t</trd>
  <trd lng="de">trinken</trd>
  <trd lng="en">drink</trd>
  <trd lng="es">beber</trd>
  <trd lng="fr">boire</trd>
  <trdgrp lng="lat">
    <trd>bibo, ere</trd>; <trd>poto, are</trd>
  </trdgrp>
  <trd lng="nl">drinken</trd>
  <trd lng="ru">&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="trink.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Ujo destinita por trinkado:
    </dif>
    <subsnc>
      <dif>
        de homoj:
        <ekz>
          sur tuko staris granda stanaloja <tld/>ujo plena de sambuka vin';
          <fnt><bib>Malben</bib>, <lok>Unua libro &dash; Tor&ccirc;oj ka
          rosmarenoj</lok></fnt>
        </ekz>
        <ekz>
          krome vi ricevas kompleta&jcirc;on de iloj konsistanta el telero,
          trinkujo, tran&ccirc;ilo, forko, kulero kaj bu&scirc;tuko;
          <fnt><bib>Ekolud</bib></fnt>
        </ekz>
        <ekz>
          glaso: vitra <tld/>ujo.
          <fnt><bib>BazRad</bib></fnt>
        </ekz>
      </dif>
      <trd lng="de">Trinkgef&auml;&szlig;</trd>
    </subsnc>
    <subsnc>
      <dif>
        de bestoj:
        <ekz>
          &scirc;i diris: anka&ubreve; por viaj kameloj mi &ccirc;erpos,
          &gcirc;is ili finos trinki; kaj rapide &scirc;i elver&scirc;is
          el sia kru&ccirc;o en la trinkujon kaj kuris denove al la puto
          por &ccirc;erpi por &ccirc;iuj liaj kameloj;
          <fnt><bib>MT</bib>, <lok>Genezo 24:20</lok></fnt>
        </ekz>
        <ekz>
          trinkujo = granda longforma ujo (<ref tip="sin"
          cel="trog.0o">trogo</ref>),
          el kiu bestoj trinkas.
          <fnt><bib>PMEG</bib>, <lok><url
          ref="http://purl.oclc.org/net/pmego?d=pmeg/pmeg10/uj.htm">uj</url></lok></fnt>
        </ekz>
      </dif>
      <trd lng="de">Tr&auml;nke</trd>
    </subsnc>
  </snc>
</drv>
<drv mrk="trink.0mono">
<kap><tld/>mono</kap>
<snc>
  <dif>
    Donaco de kelkaj moneroj al kelnero a&ubreve; servisto
  </dif>
  <refgrp tip="sin">
    <ref cel="gratif">gratifiko</ref>.
  </refgrp>
</snc>
<trd lng="de">Trinkgeld</trd>
<trd lng="en">tip</trd>
<trd lng="fr">porboire</trd>
<trd lng="nl">fooi</trd>
<trd lng="ru">&c_ch;&c_a;&c_je;&c_v;&c_y;&c_je;</trd>
</drv>
</art>
