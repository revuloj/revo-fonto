<drv mrk="trink.0vazo">
  <kap><tld/>vazo</kap>
  <snc>
     <dif>
      <ref tip="dif" cel="trink.0ujo"><tld lit="T"/>ujo</ref>:
      <ekz>
        &ccirc;iuj <tld/>vazoj de la re&gcirc;o Salomono
        estis el oro
        <fnt><bib>MT</bib>, <lok>&Kr2; 9:20</lok></fnt>;
      </ekz>
      <ekz>
        kolo de botelo <klr>[&#8230;]</klr> servos kiel <tld/>vazeto por birdo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja;
  &c_p;&c_ib;&c_c;&c_mol;&c_c;&c_ja;)</klr></trd>
  <trdgrp lng="cs">
  <trd>n&aacute;doba na pit&iacute;</trd>,
  <trd>poh&aacute;r</trd>,
  <trd>&ccaron;&iacute;&scaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Trinkbecher</trd>,
    <trd>Kelch</trd>
  </trdgrp>
  <trd lng="hu">iv&oacute;ed&eacute;ny</trd>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_u;&c_d;<klr tip="amb"> (&c_d;&c_l;&c_ja;
  &c_p;&c_i;&c_t;&c_mol;&c_ja;)</klr></trd>
  <trdgrp lng="sk">
  <trd>n&aacute;doba na pitie</trd>,
  <trd>poh&aacute;r</trd>,
  <trd>&ccaron;a&scaron;a</trd>
  </trdgrp>
</drv>
</art>
