<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: intros.xml,v 1.20 2022/03/21 15:27:49 revo Exp $">
<kap>
  <rad>introspekt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="intros.0o">
  <kap><tld/>o</kap>
  <snc mrk="intros.0o.PSI">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      Metoda memobservo de la konscio:
      <ekz>
        ne neglektu la aliajn, pli privatajn momentojn, kiuj permesas <tld/>on,
        memanalizon
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011415.php">Grincantaj genuoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tillich</nom> diras ke dum niaj plej <tld/>aj momentoj ni alfrontas la teruron de nia
        propra nenieco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=59056">Paul Tillich</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; <tld/>oj kaj gepatraj referoj iuj a&ubreve;tistoj akiras senton de humoro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=202924">Sindromo de Asperger</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        la lingvo <klr>(<nom>Python</nom>)</klr> subtenas vastan <tld/>on de datumtipoj kaj klasoj, datumtipoj povas
        esti legataj kaj komparataj &dash; ja, &ccirc;ar kiel <nom>Smalltalk</nom>, datumtipoj estas mem
        datumtipo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3401">Python (programlingvo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_r;&c_a;&c_s;&c_p;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>introspekce</trd>,
    <trd>sebepozorov&aacute;n&iacute;</trd>,
    <trd>zkoum&aacute;n&iacute; vlastn&iacute;ho nitra</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Selbstbeobachtung</trd>,
    <trd>Introspektion</trd>,
    <trd>Reflexion</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>introspection</trd>,
    <trd>reflection</trd>
  </trdgrp>
  <trd lng="es">introspecci&oacute;n</trd>
  <trd lng="fa">&a_d;&a_r;&a_w;&a_n;&zwnj;&a_n;&f_g;&a_r;&f_y;</trd>
  <trd lng="fr">introspection</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nmegfigyel&eacute;s</trd>,
    <trd>&ouml;nvizsg&aacute;lat</trd>,
    <trd>introspekci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">introspectie</trd>
  <trd lng="pl">introspekcja</trd>
  <trd lng="pt">introspec&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_n;&c_a;&c_b;&c_l;&c_ju;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poh&lcaron;ad do vn&uacute;tra</trd>,
    <trd>sebapozorovanie</trd>,
    <trd>sk&uacute;manie vlastn&eacute;ho vn&uacute;tra</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x442;&#x440;&#x43E;&#x441;&#x43F;&#x435;&#x43A;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x441;&#x430;&#x43C;&#x43E;&#x441;&#x43F;&#x43E;&#x441;&#x442;&#x435;&#x440;&#x435;&#x436;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x441;&#x430;&#x43C;&#x43E;&#x430;&#x43D;&#x430;&#x43B;&#x456;&#x437;</trd>
  </trdgrp>
</drv>
<drv mrk="intros.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Observanta sian propran menson:
      <ekz>
        knabo de <tld/>a karaktero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35751">Samuel Hahnemann</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>nach <ind>Innen</ind> gekehrt</trd>,
    <trd>reflektiert</trd>
  </trdgrp>
<trdgrp lng="en">
    <trd>introspecting</trd>,
    <trd>reflecting</trd>
  </trdgrp>
  <trd lng="uk">&#x456;&#x43D;&#x442;&#x440;&#x43E;&#x441;&#x43F;&#x435;&#x43A;&#x442;&#x438;&#x432;&#x43D;&#x438;&#x439;</trd>
</drv>



</art>
