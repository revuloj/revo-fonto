<kap>
  <rad>kripton</rad>/o
</kap>
<drv mrk="kripto1.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Kr</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="36">elemento</ref>
      kun atomnumero 36, atompezo 83,80;
      malofta <ref tip="super" cel="gas.nobla0o">nobla gaso</ref>:
      <ekz>
        <nom>Ferenc K&odblac;r&ouml;sy</nom> <klr>[trovis]</klr> manieron akiri <tld/>on per distilado el la
        aero, tial oni povis profiti la <tld/>on en elektraj lampoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22394">Kriptono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_p;&c_t;&c_o;&c_n;</trd>    
  <trd lng="bg">&c_k;&c_r;&c_i;&c_p;&c_t;&c_o;&c_n;</trd>
  <trd lng="br">kripton</trd>
  <trd lng="ca">cript&oacute;</trd>
  <trd lng="de">Krypton</trd>
  <trd lng="en">krypton</trd>
  <trd lng="es">cript&oacute;n</trd>
  <trd lng="fr">krypton</trd>
  <trd lng="hu">kripton</trd>
  <trd lng="id">kripton</trd>
  <trd lng="it">krypton</trd>
  <trd lng="ja">&#x30AF;&#x30EA;&#x30D7;&#x30C8;&#x30F3;</trd>
  <trd lng="la">Krypton</trd>
  <trd lng="nl">krypton</trd>
  <trd lng="pl">krypton</trd>
  <trdgrp lng="pt">
    <trd>cript&ocirc;nio</trd>,
    <trd>cr&iacute;ton</trd>,
    <trd>cripto</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_p;&c_t;&c_o;&c_n;</trd>
  <trd lng="sv">krypton</trd>
  <trd lng="uk">&#x43A;&#x440;&#x438;&#x43F;&#x442;&#x43E;&#x43D;</trd>
  <trd lng="zh">&#x6C2A; <pr>k&#xE8;</pr></trd>
