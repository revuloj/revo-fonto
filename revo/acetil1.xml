<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: acetil1.xml,v 1.24 2023/08/27 16:31:05 revo Exp $">
<kap>
  <rad>acetilen</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="acetil1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>2</sub>H<sub>2</sub></frm>, bruligebla gasa hidrokarbonido, uzata
      por lumigado kaj aliaj industriaj bezonoj:
      <ekz>
        etino, a&ubreve; pli malmoderne <tld/>o, estas kemia kombina&jcirc;o <frm>H-C&#8801;C-H</frm>, kiu estas en
        normalaj kondi&ccirc;oj gaso, &gcirc;i havas pli altan temperaturon de brulado (&gcirc;is
        3.000&#x202F;&#xBA;C) ol plejmulto da aliaj substancoj kaj pro tio estas uzata en te&hcirc;niko
        por veldi
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=45189">Etino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_c;&c_e;&c_t;&c_y;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_e;&c_t;&c_y;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_a;&c_c;&c_je;&c_t;&c_i;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_je;&c_t;&c_i;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">acetil&eacute;</trd>
  <trdgrp lng="cs">
    <trd>acetyl&eacute;n</trd>,
    <trd>etin</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&Auml;thin</trd>,
    <trd>Azetylen</trd>
  </trdgrp>
  <trd lng="en">acetylene</trd>
  <trd lng="es">acetileno</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_t;&f_y;&a_l;&a_n;</trd>,
    <trd>&a_A;&a_t;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">ac&eacute;tyl&egrave;ne</trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5E6;&#x5B5;&#x5D9;&#x5D8;&#x5B4;&#x5D9;&#x5DC;&#x5B5;&#x5DF;</trd>
  <trd lng="hu">acetil&eacute;n</trd>
  <trdgrp lng="it">
    <trd>etino</trd>,
    <trd>acetilene</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30BB;&#x30C1;&#x30EC;&#x30F3;</trd>
  <trd lng="nl">acetyleen</trd>
  <trd lng="pl">acetylen</trd>
  <trd lng="pt">acetileno</trd>
  <trd lng="ro">acetilen&abreve; </trd>
  <trd lng="ru">&c_a;&c_c;&c_je;&c_t;&c_i;&c_l;&c_je;&c_n;</trd>
  <trd lng="sk">acetyl&eacute;n</trd>
  <trdgrp lng="sv">
    <trd>acetylen</trd>,
    <trd>etyn</trd>
  </trdgrp>
</drv>
</art>
