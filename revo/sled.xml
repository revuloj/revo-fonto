<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>sled</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sled.0o">
  <kap><tld/>o</kap>
  <snc mrk="sled.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Malalta <ref tip="super"
      cel="vetur.glit0ilo.TRA">glitveturilo</ref> kun du
      glitiloj, sur kiu oni sidas:
      <ekz>
        mi tuj sidi&gcirc;is rajde sur la <tld/>eto kaj lasis &gcirc;in gliti
        malsupren
        <fnt><bib>Kiso</bib>, <lok>la punkto de Arkimedo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_n;&c_ib;</trd>,
    <trd>&c_s;&c_a;&c_n;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_sh;&c_je;&c_j;&c_n;&c_a;</trd>
  <trd lng="ca">trineu</trd>
  <trd lng="de">Schlitten</trd>
  <trdgrp lng="en">
    <trd>sled</trd>,
    <trd>bobsled</trd>,
    <trd>bobsleigh</trd>,
    <trd>toboggan</trd>
  </trdgrp>
  <trd lng="fr">luge</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;n</trd>,
    <trd>sz&aacute;nk&oacute;</trd>
  </trdgrp>
  <trd lng="it">slitta</trd>
  <trdgrp lng="nl">
    <trd>slee</trd>,
    <trd>slede</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_n;&c_i;</trd>,
    <trd>&c_s;&c_a;&c_n;&c_k;&c_i;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_a;&c_z;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
</art>
