<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>angil</rad>/o
</kap>
<drv mrk="angil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familo <ref tip="super" cel="angil.0edoj"><tld/>edoj</ref>     
      (<trd lng="la">Anguilla</trd>), serpentosimila man&gcirc;ebla fi&scirc;o.  
    </dif>
  </snc>
  <trd lng="de">Aal</trd>
</drv>
<drv mrk="angil.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="angil.0oformaj"><tld/>oformaj</ref>
      fi&scirc;oj (<trd lng="la">Anguillidae</trd>), kun muka, skvama
      ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="de">Aale</trd>
</drv>
<drv mrk="angil.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso 
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Apodes</trd>), sen brustna&gcirc;iloj.
    </dif>
  </snc>
  <trd lng="de">Aalartige Fische</trd>
</drv>
</art>
