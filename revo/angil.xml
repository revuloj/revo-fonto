<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: angil.xml,v 1.39 2025/12/27 16:39:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>angil</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="angil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Serpentosimila man&gcirc;ebla <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>, vivanta
      plenkreske en nesala akvo,
      sed frajanta en maroj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familo <ref
      tip="malprt" cel="angile.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anguilla</trd>).
      <ekz>
        por la dua man&gcirc;o ni prenos <tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>!
      </ekz>
      <ekz>
        <klr>[ili]</klr> batalis inter si pro kapo de <tld/>o, kaj, malgra&ubreve; tio &gcirc;in ricevis la kato
        <fnt>
          <bib>Fab2</bib>
          <lok><url ref="&Steloj;fabeloj2/">Malbela anasido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la patro de Kristino a&ccirc;etis al si grandan faskon da <tld/>oj
        kaj bu&ccirc;itan porkideton, kaj &ccirc;ion kune en korbo li starigis malanta&ubreve;e en la &scirc;ipo
        <fnt>
          <bib>Fab3</bib>
          <lok><url ref="&Steloj;fabeloj3/">Ib kaj malgranda
          kristino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu ricevadis <klr>[&#8230;]</klr> glaseton da brando <klr>[&dash;]</klr> tio estas
       farata, por bone digesti la grasan <tld/>on, diradis la <tld/>vendisto
        <fnt>
          <bib>Fab4</bib>
          <lok><url ref="&Steloj;fabeloj4/">Historio el la dunoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fb/Anguilla-anguilla_1.jpg/640px-Anguilla-anguilla_1.jpg" prm="CC BY-SA 
       3.0"><tld/>oj (Anguilla anguilla)
        <fnt>
          <aut>Dmitriy Konstantinov, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Anguilla-anguilla_1.jpg">&WCW;/File:Anguilla-anguilla_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_a;&c_r;</trd>
  <trd lng="bg">&c_z;&c_m;&c_i;&c_o;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">silienn</trd>
  <trd lng="ca">anguila</trd>
  <trd lng="cs">&uacute;ho&rcaron;</trd>
  <trd lng="de">Aal</trd>
  <trd lng="en">eel</trd>
  <trd lng="es">anguila</trd>
  <trd lng="fr">anguille</trd>
  <trd lng="he">&tsadi;&lamed;&vav;&pe;&het;</trd>
  <trd lng="hu">angolna</trd>
  <trdgrp lng="id">
    <trd>belut</trd>,
    <trd>sidat</trd>,
    <trd>lindung (2)</trd>
  </trdgrp>
  <trd lng="ja">&#12454;&#12490;&#12462;</trd>
  <trd lng="nl">aal</trd>
  <trd lng="pl">w&eogonek;gorz</trd>
  <trd lng="pt">enguia</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>&uacute;hor <klr>(ryba)</klr></trd>,
    <trd>&uacute;hor</trd>
  </trdgrp>
  <trd lng="sv">&aring;l</trd>
  <trd lng="uk">&c_v;&c_u;&c_g;&c_o;&c_r;</trd>
</drv>

<drv mrk="angil.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de
      <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Anguilliformes</trd>), sen brustna&gcirc;iloj.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">anguil&middot;liformes</trd>
  <trd lng="cs">&uacute;ho&rcaron;i <klr>(rod)</klr></trd>
  <trd lng="de">Aalartige Fische</trd>
  <trd lng="en">Anguilla</trd>
  <trd lng="fr">anguilliformes</trd>
  <trd lng="he">&bet;&tsadi;&vav;&resh;&tav; &tsadi;&lamed;&vav;&pe;&het;</trd>
  <trd lng="hu">angolnaalkat&uacute;ak</trd>
  <trd lng="id">Anguilliformes (ordo)</trd>
  <trd lng="pl">w&eogonek;gorzowate</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_i;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_u;&c_g;&c_r;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="sk">&uacute;hory</trd>
  <trd lng="uk">&c_v;&c_u;&c_g;&c_r;&c_je;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
</drv>
</art>
