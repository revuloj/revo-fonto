<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>angil</rad>/o
</kap>
<drv mrk="angil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familo <ref tip="super" cel="angile.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anguilla</trd>), serpentosimila man&gcirc;ebla
      fi&scirc;o.
    </dif>
  </snc>
  <trd lng="br">silienn</trd>
  <trd lng="bg">&c_z;&c_m;&c_i;&c_o;&c_r;&c_k;&c_a;</trd>
  <trd lng="de">Aal</trd>
  <trd lng="en">eel</trd>
  <trd lng="es">anguila</trd>
  <trd lng="fr">anguille</trd>
  <trd lng="he">&tsadi;&lamed;&vav;&pe;&het;</trd>
  <trd lng="hu">angolna</trd>
  <trd lng="nl">aal</trd>
  <trd lng="pl">w&eogonek;gorz</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_r;&c_mol;</trd>
  <trd lng="sk">&uacute;hor <klr>(ryba)</klr></trd>
  <trd lng="sv">&aring;l</trd>
</drv>

<drv mrk="angil.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Apodes</trd>), sen brustna&gcirc;iloj.
    </dif>
  </snc>
  <trd lng="de">Aalartige Fische</trd>
  <trd lng="fr">anguilliformes</trd>
  <trd lng="hu">angolnaalkat&uacute;ak</trd>
  <trd lng="pl">w&eogonek;gorzowate</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_i;<klr tip="ind">
    (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_u;&c_g;&c_r;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
    <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
