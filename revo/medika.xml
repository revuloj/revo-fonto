<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>medikag</rad>/o
</kap>
<drv mrk="medika.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Medicago</trd>), fura&gcirc;a herbo.
    </dif>
    <ref tip="sub" cel="luzern.0o">luzerno</ref>
  </snc>
  <bld lok="../bld/medikag.jpg">
    <ind>lupola <tld/>o</ind>
    <trd lng="la">Medicago lupulina</trd>
    <trd lng="hu">koml&oacute;s <ind>lucerna</ind></trd>
  </bld>
  <trd lng="be">&c_l;&c_ju;&c_c;&c_e;&c_r;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schneckenklee</trd>,
    <trd>Luzerne</trd>
  </trdgrp>
  <trd lng="hu">lucerna</trd>
  <trd lng="nl">rupsklaver</trd>
  <trd lng="pt">medicago</trd>
  <trd lng="ru">&c_l;&c_ju;&c_c;&c_je;&c_r;&c_n;&c_a; <klr
tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
