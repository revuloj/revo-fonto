<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>medikag</rad>/o
</kap>
<drv mrk="medika.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fab.0acoj">fabacoj</ref>
      (<trd lng="la">Medicago</trd>), fura&gcirc;a herbo.
    </dif>
    <ref tip="sub" cel="luzern.0o">luzerno</ref>
  </snc>
  <bld lok="../bld/medikag.jpg">
    <ind>lupola <tld/>o</ind>
    <trd lng="la">Medicago lupulina</trd>
  </bld>
  <trdgrp lng="de">
    <trd>Schneckenklee</trd>,
    <trd>Luzerne</trd>
  </trdgrp>
</drv>
</art>
