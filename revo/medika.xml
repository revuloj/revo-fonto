<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: medika.xml,v 1.20 2020/07/22 22:10:33 revo Exp $">
<kap>
  <rad>medikag</rad>/o
</kap>
<drv mrk="medika.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Medicago</trd>), fura&gcirc;a herbo.
      <ekz>
        <tld/>o<klr>[j]</klr> estas <klr>[&#8230;]</klr> unu-, du- a&ubreve; plurjaraj
        herboj kaj arbustetoj kun trifolieraj, grandstipulaj folioj, 
        kun unuopaj a&ubreve; kape ari&gcirc;intaj floroj kaj kun kurbaj a&ubreve; helice tordaj gu&scirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=491837">Medikago</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="luzern.0o">luzerno</ref>
  </snc>
  <bld
     lok="&WCU;/thumb/6/6f/Medicago_lupulina_inflorescence%2C_hopklaver_bloeiwijze.jpg/320px-Medicago_lupulina_inflorescence%2C_hopklaver_bloeiwijze.jpg"
     prm="CC BY-SA 3.0"
     ><ind>lupola <tld/>o</ind> (Medicago lupulina)
       <fnt>
         <aut>Rasbak, propra verko</aut>
         <lok><url ref="&WCW;/File:Medicago_lupulina_inflorescence,_hopklaver_bloeiwijze.jpg"
             >&WCW;/File:Medicago_lupulina_inflorescence,_hopklaver_bloeiwijze.jpg</url></lok>
       </fnt>
    <trd lng="ca"><ind>melg&oacute;</ind> menut</trd>
    <trd lng="la">Medicago lupulina</trd>
    <trdgrp lng="fa">
      <trd>&f_y;&a_w;&a_n;&a_j;&a_h;&a_hamza_sure; &a_r;&a_A;&a_z;&f_k;&f_y;</trd>,
      <trd>&f_y;&a_w;&a_n;&a_j;&a_h;&a_hamza_sure; &a_s;&f_y;&a_A;&a_h;</trd>
    </trdgrp>
    <trd lng="hu">koml&oacute;s <ind>lucerna</ind></trd>
  </bld>
  <trd lng="be">&c_l;&c_ju;&c_c;&c_e;&c_r;&c_n;&c_a;</trd>
  <trd lng="bg">&c_l;&c_ju;&c_c;&c_je;&c_r;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>alfals</trd>,
    <trd>melg&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schneckenklee</trd>,
    <trd>Luzerne</trd>
  </trdgrp>
  <trd lng="en">medick</trd>
  <trd lng="fa">&f_y;&a_w;&a_n;&a_j;&a_h;</trd>
  <trd lng="hu">lucerna</trd>
  <trd lng="nl">rupsklaver</trd>
  <trd lng="pl">lucerna</trd>
  <trd lng="pt">medicago</trd>
  <trd lng="ro">trifoi m&abreve;runt</trd>
  <trd lng="ru">&c_l;&c_ju;&c_c;&c_je;&c_r;&c_n;&c_a; <klr
tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
