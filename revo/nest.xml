<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nest</rad>/o
</kap>
<drv mrk="nest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="nest.0o.besto">
    <dif>
      Korbeto a&ubreve; konstrua&jcirc;eto farata de la birdoj kaj de kelkaj insektoj
      kaj fi&scirc;oj por demeti siajn ovojn kaj kovi la idojn:
      <ekz>
        al &ccirc;iu besto pla&ccirc;as &gcirc;ia <tld/>o
        (<ref tip="vid" cel="si.0a">sia</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al pec' pecon algluas <klr>(poiome progresas)</klr>,
        kiu <tld/>on konstruas
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="nest.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Restadejo, rifu&gcirc;ejo:
      <ekz>
        dol&ccirc;a familia <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fari sian <tld/>on
        <klr>(sin da&ubreve;re instali)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;u mian domon vi rigardas kiel <tld/>on de rabistoj?<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Nest</trd>
  <trd lng="nl">nest</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</trd>
</drv>

<drv mrk="nest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Konstrui <tld/>on:
      <ekz>
        la birdoj <tld/>as en printempo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nisten</trd>
  <trd lng="nl">nesten</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_n;&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_mol; <ind>&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="nest.en0igxi">
  <kap>(en)<tld/>i&gcirc;i</kap>
  <snc mrk="nest.en0igxi.lauxvorte">
    <dif>
      Sin instali en <tld/>o.
    </dif>
  </snc>
  <snc mrk="nest.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin da&ubreve;re instali:
      <ekz>
        floroj <tld/>i&gcirc;as en la fendoj de la roko;
      </ekz>
      <ekz>
        la militfebro <tld/>i&gcirc;is en ilia kapo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="logx.0i">lo&gcirc;i</ref>,
      <ref cel="rest.0i">resti</ref>,
      <ref cel="okup.0i">okupi</ref>,
      <ref cel="radik.en0igxi">enradiki&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">sich <ind>einnisten</ind></trd>
  <trd lng="nl">zich <ind>nestelen</ind></trd>
  <trd lng="ru">&c_u;&c_g;&c_n;&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
