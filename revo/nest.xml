</drv>

<drv mrk="nest.en0igi">
  <kap>
    en<tld/>igi
  </kap>
  <snc mrk="nest.en0igi.ZOO">
    <dif>
      Meti ion en <tld/>o:
      <ekz>
        la gardistoj en<tld/>igis ordinarajn anserojn kun malgrandaj anseridoj
        <fnt>
          <bib>Revizoro</bib>
          <aut>Z</aut>
          <lok>Akto 1 Sceno 1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">put into a <ind>nest</ind></trd>
</drv>

<drv mrk="nest.en0igxi">
  <kap>
    en<tld/>i&gcirc;i
  </kap>
  <snc mrk="nest.en0igxi.ZOO">
    <dif>
      Meti&gcirc;i en <tld/>on:
    </dif>
    <ekz>
        la ordinaraj anseroj en<tld/>i&gcirc;is kun malgrandaj anseridoj.
      </ekz>
    <trd lng="en">nestle <klr>(in a nest)</klr></trd>
  </snc>
  <snc mrk="nest.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Meti&gcirc;i en lokon rigardatan kiel restadejo, rifu&gcirc;ejo:
      <ekz>
        poste vi en<tld/>i&gcirc;as en kafejoj
        <fnt>
          <bib>Rabistoj</bib>
          <aut>Z</aut>
          <lok>Akto 2 Sceno 1</lok>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto en<tld/>i&gcirc;is en tiu universitato
        <fnt>
          <aut>Mirejo Grosjean, Stefan MacGill</aut>
          <vrk>Revuo Esperanto 2017 1</vrk>
          <lok>Muzeoj: Zaozhuang</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">nestle <klr>(comfortably or safely in a place)</klr></trd>
  </snc>
</drv>

</art>
