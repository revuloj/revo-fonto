<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>argument</rad>/o
</kap>
<drv mrk="argume.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rezona pruvo en diskuto:
      <ekz>
        tiel la granda <tld/>o de niaj kontra&ubreve;uloj disfalas;
      </ekz>
      <ekz>
        la <tld/>oj per kiuj oni kontra&ubreve; tio &ccirc;i
        batalas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se niaj <tld/>oj estas mal&gcirc;ustaj, for&jcirc;etu
        ilin<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="br">arguzenn</trd>
    <trdgrp lng="de">
      <trd>Argument</trd>
    </trdgrp>     
    <trd lng="fr">argument</trd>
    <trdgrp lng="hu">
      <trd>&eacute;rv</trd>,
      <trd>argumentum</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="argume.0o.donitajxo">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(de <ref cel="funkci.0o.MAT">funkcio</ref>,
        <ref cel="operac.0o.MAT">operacio</ref> a&ubreve;
        <ref cel="rilat.0o.MAT">rilato</ref>)</klr>:
        <ekz>
          en la esprimo <frm><k>f</k>(<k>x</k>,<k>y</k>+<k>z</k>)</frm> la
          funkcio <frm><k>f</k></frm> havas du <tld/>ojn, nome
          <frm><k>x</k></frm> kaj <frm><k>y</k>+<k>z</k></frm>;
        </ekz>
        <ekz>
          <frm><k>f</k></frm> estas du<tld/>a funkcio;
        </ekz>
        <ekz>
          unu<tld/>a funkcio estas du<tld/>a rilato;
        </ekz>
        <ekz>
          la <tld/>oj de adicio (resp. multipliko) oni nomas anka&ubreve;
          <ref cel="term.0o.sumo">termoj</ref>
          (resp.. <ref cel="faktor.0o.multip">faktoroj</ref>).
        </ekz>
      </dif>
      <ref tip="vid" cel="parame.0o.MAT">parametro.</ref>
      <rim>
        La nocio estas sufi&ccirc;e elasta. Kiel <tld/>on
        de bildigo oni povas konsideri &ccirc;iun elementon de &gcirc;ia
        fonto-aro, foje nomata <tld/>aro, sed se tiaj elementoj estas opoj,
        anka&ubreve; iliaj termoj estas konsidereblaj kiel <tld/>oj. La
        kunteksto devas klarigi, pri kio oni parolas.
      </rim>
      <trdgrp lng="de">
        <trd>Argument<klr tip="ind"> (unabh&auml;ngige Variable)</klr></trd>,
        <trd>Operand</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>argument<klr tip="ind"> (independant variable)</klr></trd>,
        <trd>operand</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>argument<klr tip="ind"> (variable ind&eacute;pendante) </klr></trd>,
        <trd>op&eacute;rande</trd>
      </trdgrp>
      <trd lng="hu">f&uuml;ggetlen <ind>v&aacute;ltoz&oacute;</ind><klr tip="ind"> (f&uuml;ggv&eacute;nyben)</klr></trd>
      <trdgrp lng="pl">
        <trd>argument<klr tip="ind"> (zmienna niezale&zdot;na)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_a;&c_r;&c_g;&c_u;&c_m;&c_je;&c_n;&c_t;<klr tip="ind"> (&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;)</klr></trd>,
        <trd>&c_o;&c_p;&c_je;&c_r;&c_a;&c_n;&c_d;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="argume.0o.komplekso">
      <fnt><bib>MatTerm</bib><lok>p. 15</lok></fnt>
      <dif>
        <klr>(de <ref cel="komple.0o.MAT">komplekso</ref>
        <frm><k>z</k></frm>)</klr>
        Tia reelo <frm>&theta;</frm>, ke
        <frm><k>z</k> = |<k>z</k>|.(cos&theta; + <k>i</k>.sin&theta;)</frm>,
        kie <frm>|<k>z</k>|</frm> prezentas la
        <ref cel="modul.0o.komplekso">modulon</ref> de <frm><k>z</k></frm>:
        <ekz>
          la <tld/>on oni kutime prenas en la
          intervalo <frm>]-&pi;,+&pi;]</frm> kaj foje signas per
          <frm>Arg <k>z</k></frm>;
        </ekz>
        <ekz>
          la <tld/>o de nenula reelo egalas al <frm>0</frm>
          <klr>(se pozitiva)</klr> a&ubreve; <frm>&pi;</frm>
          <klr>(se negativa)</klr>.
        </ekz>
      </dif>
      <trd lng="de">Argument</trd>
      <trdgrp lng="en">
        <trd>argument<klr tip="ind"> (of a complex number)</klr></trd>,
        <trd>amplitude<klr tip="ind"> (of a complex number)</klr></trd>
      </trdgrp>
      <trd lng="fr">argument<klr tip="ind"> (d'un nombre complexe)</klr></trd>
      <trdgrp lng="hu">
        <trd>argumentum<klr tip="ind"> (komplex sz&aacute;m&eacute;)</klr></trd>,
        <trd>amplit&uacute;d&oacute;<klr tip="ind"> (komplex sz&aacute;m&eacute;)</klr></trd>
      </trdgrp>
      <trd lng="pl">argument<klr tip="ind"> (liczby zespolonej)</klr></trd>
      <trd lng="ru">&c_a;&c_r;&c_g;&c_u;&c_m;&c_je;&c_n;&c_t;<klr tip="ind"> (&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_o;&c_g;&c_o; &c_ch;&c_i;&c_s;&c_l;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="argume.0o.polinomo">
      <ref tip="vid" cel="polino.0o.formala">polinomo.</ref>
      <rim>
        &Ccirc;i-kuntekste Bricard
        <fnt><bib>MatTerm</bib><lok>p. 12</lok></fnt> uzas <ctl>varianto</ctl>,
        kiel &ccirc;e funkcio, kaj Werner <fnt><bib>MatVort</bib></fnt> konas
        <ctl>nedeterminita grando</ctl>, kiu ver&scirc;ajne respondas al la
        sama intenco, sed &gcirc;enas pro la konkreta senco de
        <ctl>grando</ctl>, dum temas pri nocio tre formala.
      </rim>
      <trd lng="de">Unbestimmte</trd>
      <trd lng="en">indeterminate</trd>
      <trd lng="fr">ind&eacute;termin&eacute;e</trd>
      <trd lng="hu">hat&aacute;rozatlan v&aacute;ltoz&oacute;</trd>      
      <trd lng="pl">niezmienna<klr tip="ind"> (wielomianu)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_a;&c_ja;</trd>,
        <trd>&c_n;&c_je;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_a;&c_ja;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_u;&c_m;&c_e;&c_n;&c_t;</trd>  
  <trd lng="br">argumant</trd>
  <trd lng="es">argumento</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_g;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_d;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">argument</trd>
</drv>

<drv mrk="argume.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Prezenti <tld/>ojn en diskuto.
    </dif>
    <ref tip="super" cel="pruv.0i">pruvi</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>    
  <trd lng="br">arguzenni&ntilde;</trd>
  <trd lng="en">argue</trd>
  <trd lng="es">argumentar</trd>
  <trd lng="fr">argumenter</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rvel</trd>,
    <trd>argumentumt&aacute;l</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_g;&c_u;&c_m;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">argumentova&tcaron;</trd>
</drv>
</art>
