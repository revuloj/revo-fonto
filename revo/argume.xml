<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>argument</rad>/o
    </kap>
    <drv mrk="argume.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Rezona pruvo en diskuto:
          <ekz>
            tiel la granda <tld/>o de  niaj kontra&ubreve;uloj disfalas;
          </ekz><ekz>
            la <tld/>oj per kiuj oni kontra&ubreve; tio &ccirc;i batalas<fnt>Z</fnt>;
          </ekz><ekz>
            se niaj <tld/>oj estas mal&gcirc;ustaj, for&jcirc;etu ilin<fnt>Z</fnt>.
          </ekz> 
        </dif>
      </snc>
      <snc mrk="argume.0o.donitajxo">
        <uzo tip="fak">MAT</uzo>
        <dif>
          <ref tip="super" cel="don.0itajxo">Donita&jcirc;o</ref> por
          <ref cel="funkci.0o.mat">funkcio</ref> a&ubreve; operacio:
          <ekz>
            la esprimo <frm>f(x,y+z)</frm> havas du <tld/>ojn,
            <frm>x</frm> kaj <frm>y+z</frm>;
          </ekz><ekz>
            <frm>f</frm> estas du<tld/>a funkcio.
          </ekz> 
        </dif>
        <ref tip="vid" cel="parame.0o">parametro</ref>
      </snc>
      <trdgrp lng="ru">
        <trd>&#x430;&#x440;&#x433;&#x443;&#x43c;&#x435;&#x43d;&#x442;</trd>,
        <trd>&#x434;&#x43e;&#x432;&#x43e;&#x434;</trd>
      </trdgrp>
    </drv>
    <drv mrk="argume.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Prezenti <tld/>ojn en diskuto.  
        </dif>
        <ref tip="super" cel="pruv.0i">pruvi</ref>
      </snc>
      <trd lng="en">maintain</trd>
      <trd lng="ru">&#x430;&#x440;&#x433;&#x443;&#x43c;&#x435;&#x43d;&#x442;&#x438;&#x440;&#x43e;&#x432;&#x430;&#x442;&#x44c;</trd>
    </drv>
  </art>
  <!--
