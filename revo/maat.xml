<art mrk="$Id: maat.xml,v 1.13 2023/10/19 15:13:16 revo Exp $">
<kap>
  <rad>maat</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="maat.0o">
  <kap><tld/>o</kap>
  <snc mrk="maat.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Subestro en &scirc;ipo, ranga inter &scirc;ipestro kaj nura &scirc;ipano:
      <ekz>
        la <tld/>o aperis sur la ferdeko, blovis signalon per la fajfilo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 63a, p.
        757a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
     <trd>primer oficial (a bord)</trd>
  </trdgrp>
  <trd lng="cs">maat</trd>
  <trd lng="de">Maat<klr tip="amb"> (Schiff)</klr></trd>
  <trd lng="en">mate<klr tip="amb"> (ship)</klr></trd>
  <trd lng="fr">sous-officier de marine</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;sodkapit&aacute;ny</trd>
    <trd>els&odblac; tiszt<klr tip="amb"> (haj&oacute;n)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0B;&#x58EB;&#x5B98;  <pr>&#x304B;&#x3057;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x822A;&#x6D77;&#x58EB;  <pr>&#x3053;&#x3046;&#x304B;&#x3044;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bootsman</trd>,
    <trd>maat</trd>
  </trdgrp>
  <trd lng="pl">mat <klr>(marynarz)</klr></trd>
  <trd lng="pt">marinheiro-cabo<klr> (Port.)</klr></trd>
  <trd lng="ro">marinar</trd>
  <trd lng="sk">maat</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x443;&#x43D;&#x442;&#x435;&#x440;-&#x43E;&#x444;&#x456;&#x446;&#x435;&#x440;</trd>,
    <trd>&#x431;&#x43E;&#x446;&#x43C;&#x430;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

