<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: maat.xml,v 1.16 2025/07/12 08:37:06 revo Exp $">
<kap>
  <rad>maat</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="maat.0o">
  <kap><tld/>o</kap>
  <snc mrk="maat.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Subestro en &scirc;ipo, ranga inter &scirc;ipestro kaj nura &scirc;ipano:
      <ekz>
        la <tld/>o aperis sur la ferdeko, blovis signalon per la fajfilo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 63a, p.
        757a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&scirc;ipa suboficiro</ke>.
  </rim>

  <trd lng="be">&c_m;&c_a;&c_t; <klr tip="amb">(&c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_n;&c_a; &c_f;&c_l;&c_jo;&c_c;&c_je;)</klr></trd>
  <trdgrp lng="ca">
     <trd>primer oficial (a bord)</trd>
  </trdgrp>
  <trd lng="cs">maat</trd>
  <trd lng="de">Maat<klr tip="amb"> (Schiff)</klr></trd>
  <trd lng="en">mate<klr tip="amb"> (ship)</klr></trd>
  <trd lng="fr">sous-officier de marine</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;sodkapit&aacute;ny</trd>
    <trd>els&odblac; tiszt<klr tip="amb"> (haj&oacute;n)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19979;&#22763;&#23448;  <pr>&#12363;&#12375;&#12363;&#12435;</pr></trd>,
    <trd>&#33322;&#28023;&#22763;  <pr>&#12371;&#12358;&#12363;&#12356;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bootsman</trd>,
    <trd>maat</trd>
  </trdgrp>
  <trd lng="pl">mat <klr>(marynarz)</klr></trd>
  <trd lng="pt">marinheiro-cabo<klr> (Port.)</klr></trd>
  <trd lng="ro">marinar</trd>
  <trd lng="sk">maat</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_u;&c_n;&c_t;&c_je;&c_r;-&c_o;&c_f;&c_ib;&c_c;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_o;&c_c;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>

