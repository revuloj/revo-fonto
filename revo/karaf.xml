<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karaf</rad>/o
</kap>
<drv mrk="karaf.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Vitra a&ubreve; kristala lar&gcirc;baza 
      <ref tip="super" cel="botel.0o">botelo</ref>, destinita por servado de
      akvo a&ubreve; vino dum la man&gcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="flakon.0o">flakono</ref>,
      <ref cel="bokal.0o">bokalo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Karaffe</trd>
  <trd lng="en">carafe</trd>
  <trd lng="fr">carafe</trd>
  <trd lng="nl">karaf</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_n;</trd>
  <trd lng="sv">karaff</trd>
</drv>
</art>
