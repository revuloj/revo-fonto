<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karaf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="karaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="karaf.0o.VAZ">
    <dif>
      Vitra a&ubreve; kristala lar&gcirc;baza
      <ref tip="super" cel="botel.0o.VAZ">botelo</ref> por servado de
      trinka&jcirc;o dum man&gcirc;o:
      <ekz>
        la popo ver&scirc;is en la glasojn alkoholon, movis al Maksim
        unu glason kaj <tld/>on kun akvo
        <fnt>
        <aut>V. &Scirc;uk&scirc;in, trad. N. Lozga&ccirc;ev</aut>,
        <vrk>Simbolo de kredo</vrk>,
        <bib>LOdE</bib>,
        <lok>1994:3-4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bokal.0o">bokalo</ref>,
      <ref cel="flakon.0o">flakono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_n;</trd>
  <trd lng="ca">garrafa</trd>
  <trd lng="de">Karaffe</trd>
  <trd lng="en">carafe</trd>
  <trd lng="es">garrafa</trd>
  <trd lng="fr">carafe</trd>
  <trdgrp lng="hu">
    <trd>csiszolt <ind>&uuml;vegpalck</ind></trd>,
    <trd>vizes&uuml;veg</trd>,
    <trd>boros&uuml;veg</trd>,
    <trd>karaffa</trd>
  </trdgrp>
  <trd lng="nl">karaf</trd>
  <trd lng="pl">karafka</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_n;</trd>
  <trd lng="sk">karafa</trd>
  <trd lng="sv">karaff</trd>
</drv>
</art>
