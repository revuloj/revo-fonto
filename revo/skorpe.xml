<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skorpe.xml,v 1.8 2018/12/12 18:10:25 revo Exp $">
<kap>
  <rad>skorpened</rad>/oj
</kap>
<drv mrk="skorpe.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="skorpen.0oformaj">skorpenoformaj
      fi&scirc;oj</ref> (<trd lng="la">Scorpaenidae</trd>):
      <ekz>
        <nom>Sebastes</nom> estas genro de fi&scirc;oj de la familio Sebastedoj 
        (kvankam kelkaj inkludas
        tiujn en <tld lit="S"/>oj), plej <klr>[multaj]</klr> el kiuj ricevas la
        komunan nomon de rokfi&scirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=418661"
              >Sebastes</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Skorpionfische</trd>,
    <trd>Drachenk&ouml;pfe<klr tip="ind"> (Familie)</klr></trd>
  </trdgrp>
  <trd lng="fr">Scorpaenid&eacute;s</trd>
  <trd lng="hu">s&aacute;rk&aacute;nyfej&udblac; half&eacute;l&eacute;k</trd>
  <trd lng="nl">schorpioenvissen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_o;&c_r;&c_p;&c_je;&c_n;&c_y;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_p;&c_je;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
