<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>avel</rad>/o
</kap>
<drv mrk="avel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      La man&gcirc;ebla nukso de 
      <ref tip="malprt" cel="avel.0ujo"><tld/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Haselnu&szlig;</trd>
  <trd lng="fr">noisette</trd>
  <trd lng="tr">f&inodot;nd&inodot;kaj</trd>
</drv>
<drv mrk="avel.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="betul.0acoj">betulacoj</ref>
      kaj precipe ties sova&gcirc;a specio (<trd lng="la">Corylus avellana</trd>).
    </dif>
  </snc>
  <trd lng="de">Haselstrauch</trd>
  <trd lng="fr">noisetier</trd>
  <trd lng="tr">f&inodot;nd&inodot;kaj</trd>
</drv>
</art>
