<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>juniper</rad>/o
</kap>
<drv mrk="junipe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cipres1.0oj">cipresacoj</ref>
      (<trd lng="la">Juniperus</trd>), &ccirc;iam verda arbeto
      kies beroj estas uzataj por aromigi brandon.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_d;&c_l;&c_o;&c_v;&c_je;&c_c;</trd>
  <trd lng="de">Wacholder</trd>
  <trd lng="en">juniper</trd>
  <trd lng="fr">gen&eacute;vrier</trd>
  <trd lng="hu">bor&oacute;ka</trd>
  <trd lng="nl">jeneverbesstruik</trd>
  <trdgrp lng="pt">
    <trd>zimbro</trd>,
    <trd>jun&iacute;pero</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_zh;&c_zh;&c_je;&c_v;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
