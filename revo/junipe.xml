<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>juniper</rad>/o
</kap>
<drv mrk="junipe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cipres.0acoj">cipresacoj</ref>
      (<trd lng="la">Juniperus</trd>), &ccirc;iam verda arbeto
      kies beroj estas uzataj por aromigi brandon.
    </dif>
  </snc>
  <trd lng="de">Wachholder</trd>
   <trd lng="en">juniper</trd> 
<trd lng="ru">&c_m;&c_o;&c_zh;&c_zh;&c_je;&c_v;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
