<art mrk="$Id: junipe.xml,v 1.30 2023/12/03 07:09:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>juniper</rad>/o
</kap>
<drv mrk="junipe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="junipe.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda arbeto el <ref tip="lst" cel="konifer.0o" lst="voko:koniferoj">koniferoj</ref> kreskanta en malri&ccirc;aj
      grundoj, anka&ubreve; 
en altaj
      montaroj, kies beroj estas uzataj por aromigi brandon;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="cipres1.0oj">cipresacoj</ref>
      (<trd lng="la">Juniperus</trd>):
      <ekz>
        la vento <klr>...</klr> skuis la malaltajn kaj maldensajn
        <tld/>ojn
        <fnt>
        <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
        <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
        Surprizitaj nuptofestantoj</url></vrk>,
        <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_d;&c_l;&c_o;&c_v;&c_je;&c_c;</trd>
  <trd lng="br">jenevreg</trd>
  <trd lng="ca">ginebre</trd>
  <trd lng="cs">jalovec</trd>
  <trd lng="de">Wacholder</trd>
  <trd lng="en">juniper</trd>
  <trd lng="es">enebro</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_damma;&a_r;&a_s;</trd>,
    <trd>&a_s;&a_r;&a_w; &f_k;&a_w;&a_h;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">gen&eacute;vrier</trd>
  <trd lng="hu">bor&oacute;ka</trd>
  <trd lng="id"><ind>jintan</ind> saru</trd>
  <trd lng="it">ginepro</trd>
  <trdgrp lng="ja">
    <trd>&#x30D3;&#x30E3;&#x30AF;&#x30B7;&#x30F3;</trd>,
    <trd>&#x30BB;&#x30A4;&#x30E8;&#x30A6;&#x30CD;&#x30BA;</trd>
  </trdgrp>
  <trd lng="nl">jeneverbesstruik</trd>
  <trd lng="pl">ja&lstroke;owiec</trd>
  <trdgrp lng="pt">
    <trd>zimbro</trd>,
    <trd>jun&iacute;pero</trd>
  </trdgrp>
  <trd lng="ru">
&c_m;&c_o;&c_zh;&c_zh;&c_je;&c_v;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;
</trd>
  <trd lng="sk">borievka <klr>(ker)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x43B;&#x456;&#x432;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x44F;&#x43B;&#x43E;&#x432;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x675C;&#x677E; <pr>d&#xF9;s&#x14D;ng</pr></trd>,
    <trd>&#x523A;&#x67CF; <pr>c&#xEC;b&#xF3;</pr></trd>
  </trdgrp>
</drv>
</art>
