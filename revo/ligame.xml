<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ligament</rad>/o
</kap>
<drv mrk="ligame.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Fibreca liga&jcirc;o, kiu kunligas ostojn a&ubreve; altenas internajn
      korpopartojn.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ligament</trd>,
     <trd>Ligamentum</trd>,
     <trd>Band</trd>
  </trdgrp>
  <trd lng="el">&tau;&epsilon_ton;&ny;&omikron;&ny;&tau;&alfa;&sigma_fina;</trd>
  <trd lng="en">ligament</trd>
  <trd lng="hu">&iacute;nszalag</trd>
  <trdgrp lng="nl">
    <trd>band</trd>,
    <trd>ligament</trd>
  </trdgrp>
  <trd lng="pt">ligamento</trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
</drv>
</art>
