<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ligament</rad>/o
</kap>
<drv mrk="ligame.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Fibreca liga&jcirc;o, kiu kunligas ostojn a&ubreve; altenas internajn
      korpopartojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_z; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
  <trdgrp lng="de">
     <trd>Ligament</trd>,
     <trd>Ligamentum</trd>,
     <trd>Band</trd>
  </trdgrp>
  <trd lng="el">&tau;&epsilon_ton;&ny;&omikron;&ny;&tau;&alfa;&sigma_fina;</trd>
  <trd lng="en">ligament</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_r;&a_d;&f_p;&f_y;</trd>,
    <trd>&a_r;&a_b;&a_A;&a_T;</trd>
  </trdgrp>
  <trd lng="hu">&iacute;nszalag</trd>
  <trdgrp lng="nl">
    <trd>band</trd>,
    <trd>ligament</trd>
  </trdgrp>
  <trd lng="pl">wi&aogonek;zad&lstroke;o <klr tip="amb">
(anatomia)</klr></trd>
  <trd lng="pt">ligamento <klr tip="amb">(anat.)</klr></trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
</drv>
</art>
