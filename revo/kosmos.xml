<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kosmos.xml,v 1.11 2023/10/18 17:23:25 revo Exp $">
<kap>
  <rad>kosmos</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="kosmos.0o">
  <kap><tld/>o</kap>
  <snc mrk="kosmos.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro de jaraj a&ubreve; plurjaraj herboj kun lar&gcirc;aj, koloraj floroj
      el sudo de Nordameriko kaj Centrameriko,
      el familio <ref tip="super" cel="astera.0oj.BOT">asteracoj</ref>
      (<trd lng="la">Cosmos</trd>):
      <ekz>
        la <tld/>o, kiun mia edzino plantis, hodia&ubreve; havas kvar
        rozkolorajn
        florojn
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
        <lok>La kosmoso floras, 2005-09-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_s;&c_m;&c_a;&c_s; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">cosmos</trd>
  <trdgrp lng="de">
    <trd>Cosmos<klr tip="amb"> (Pflanzengattung)</klr></trd>,
    <trd>Kosmee<klr tip="amb"> (Pflanzengattung)</klr></trd>,
    <trd>Schmuckk&ouml;rbchen<klr tip="amb"> (Pflanzengattung)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&a_kasra;&a_w;&f_y;&a_d;&f_y;</trd>
  <trd lng="fr">Cosmos<klr tip="amb"> (fleurs)</klr></trd>
  <trd lng="ja">&#x30B3;&#x30B9;&#x30E2;&#x30B9;  <pr>&#12371;&#12377;&#12418;&#12377;</pr></trd>
  <trdgrp lng="nl">
    <trd>cosmos</trd>,
    <trd>cosmosbloem</trd>
  </trdgrp>
  <trd lng="pl">kosmos podw&oacute;jnie pierzasty <klr>(ro&sacute;l.)</klr></trd>
  <trd lng="pt">cosmos <klr tip="amb">(bot.)</klr></trd>
  <trd lng="ro">cosmos bipinnatus</trd>
</drv>
</art>

