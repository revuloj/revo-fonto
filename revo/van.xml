<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: van.xml,v 1.32 2022/10/14 07:01:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>van</rad>/a
</kap>
<drv mrk="van.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Ne produktanta efikan rezulton; <ref cel="util.ne0asen0a">senutila</ref>:
      <ekz>
        nia voko ne restos <tld/>a
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        al vi klopodojn kaj al mi suferojn vi ka&ubreve;zas, tamen amba&ubreve;
        estas <tld/>aj
        <klr>(<ref cel="frukt.sen0a.malfekunda">senfruktaj</ref>)</klr>
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili komprenis, ke &ccirc;ia ser&ccirc;ado estus <tld/>a
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi parolos anta&ubreve;e mem kun la posedantino <klr>[&#8230;]</klr>, vi ne estu elmetata al
        <tld/>a malagrabla&jcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la juna barono kunportis sian fajfilon, sed &ccirc;iuj liaj penoj <klr>[&#8230;]</klr> eligi
        ian tonon, estis <tld/>aj
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iuj tiuj libroj trovi&gcirc;as nur <tld/>aj sistemoj kaj ne e&ccirc; unu sola utila afero
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        lecionoj al profesoro estas <tld/>a laboro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kontra&ubreve; forta mano la le&gcirc;o estas <tld/>a</ind>
        <klr>(senpova)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a t&ouml;rv&eacute;ny csak a gy&aacute;moltalant k&ouml;ti</trd>
      </ekz>
      <ekz>
        <ind>servo al Dio <tld/>a <klr>(senrekompenca)</klr> ne 
        restas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">az Isten soha nem marad ad&oacute;sunk</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cel.sen0a">sencela</ref>,
      <ref cel="pov.sen0a">senpova</ref>,
      <ref cel="senc.sen0a">sensenca</ref>,
      <ref cel="rezult1.sen0a">senrezultata</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="vant.0a.io">vanta<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zwecklos</trd>,
      <trd>fruchtlos</trd>,
      <trd>ergebnislos</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>in&uacute;til</trd>,
      <trd>ineficaz</trd>,
      <trd>infructuoso</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Havanta nur &scirc;ajnon sen realeco; ne<ref tip="ant" cel="efekti.0a">efektiva</ref>:
      <ekz>
        eh, tio estas nur <tld/>aj minacoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi vidis kun &gcirc;ojo, ke miaj duboj estis <tld/>aj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la ferma ceremonio de la Sesa Kongreso Esperantista 
          en Washington en la 20a de a&ubreve;gusto 1910 (a)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj miaj esperoj estis <tld/>aj, mia edzo ne reaperis
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili parolas <tld/>ajn vortojn, &jcirc;uras mensoge
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 10:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        Lia graco donacita al mi montri&gcirc;is ne <tld/>a
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 15:10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxajn.0a">&scirc;ajna</ref>,
      <ref cel="svag.0a">svaga</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>vergeblich</trd>,
      <trd>umsonst</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_e;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="br">aner<klr tip="ind"> (ag.)</klr></trd>
  <trd lng="ca">va</trd>
  <trdgrp lng="cs">
    <trd>marn&yacute;</trd>,
    <trd>pom&iacute;jej&iacute;c&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vergeblich</trd>,
    <trd>umsonst</trd>,
    <trd>unn&uuml;tz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>futile</trd>,
    <trd>vain</trd>
  </trdgrp>
  <trd lng="es">vano</trd>
  <trd lng="fr">vain</trd>
  <trdgrp lng="hu">
    <trd>hi&aacute;baval&oacute;</trd>,
    <trd>medd&odblac;</trd>,
    <trd>hi&uacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vergeefs</trd>,
    <trd>ijdel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>daremny</trd>,
    <trd>pr&oacute;&zdot;ny</trd>,
    <trd>p&lstroke;onny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>v&atilde;o<klr tip="ind"> (adj.)</klr></trd>,
    <trd>in&uacute;til<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_shch;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>daromn&yacute;</trd>,
    <trd>m&aacute;rny</trd>,
    <trd>zbyto&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_r;&c_je;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="van.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Sen rezulto; senutile:
      <ekz>
        &ccirc;iuj niaj vortoj pereus <tld/>e
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj niaj laboroj pereos <tld/>e
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi fari&gcirc;is ofero de liaj intrigoj, <klr>[&#8230;]</klr> de kiu<klr>[j]</klr> mi vin
        <tld/>e avertadis
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>e vi tentas, mi ne konsentos</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">m&eacute;g a te k&eacute;t szemed&eacute;rt se</trd>
      </ekz>
      <ekz>
        <ind>el malplena telero <tld/>e &ccirc;erpas kulero</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">ahol nincs, ott ne keress</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_e;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="br">en <ind>aner</ind></trd>
  <trd lng="ca">en <ind>va</ind></trd>
  <trdgrp lng="cs">
    <trd>darmo</trd>,
    <trd>marn&ecaron;</trd>,
    <trd>marn&ecaron;</trd>,
    <trd>naplano</trd>,
    <trd>zbyte&ccaron;n&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zwecklos</trd>,
    <trd>fruchtlos</trd>,
    <trd>ergebnislos</trd>,
    <trd>nutzlos</trd>,
    <trd>vergeblich</trd>,
    <trd>umsonst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>in vain</trd>,
    <trd>vainly</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>en vano</trd>,
    <trd>vanamente</trd>,
    <trd>ineficazmente</trd>,
    <trd>infructuosamente</trd>
  </trdgrp>
  <trd lng="fr">en <ind>vain</ind></trd>
  <trd lng="hu">hi&aacute;ba</trd>
  <trd lng="nl">tevergeefs</trd>
  <trdgrp lng="pl">
    <trd>daremnie</trd>,
    <trd>na <ind>pr&oacute;&zdot;no</ind></trd>
  </trdgrp>
  <trd lng="pt">em v&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_r;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_s;&c_n;&c_o;</trd>,
    <trd>&c_t;&c_shch;&c_je;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>daromne</trd>,
    <trd>m&aacute;rne</trd>,
    <trd>nadarmo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_r;&c_je;&c_m;&c_n;&c_o;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_o;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="van.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="van.0eco.KOMUNE">
    <dif>
      Senrezulteco, senfrukteco:
      <ekz>
        la pensoj de <nom>Adaha</nom> sin okupis ne nur <klr>[&#8230;]</klr> pri la
        <tld/>eco de knabinaj revoj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 23a, paca
        Betra&hcirc;</lok></fnt>;
      </ekz>
      <ekz>
        spertoj en la anta&ubreve;a kongreslando konvinkis min pri la <tld/>eco kaj la malhomaj
        efikoj de la embargo
        <fnt>
          <bib>Monato</bib>
          <aut>Rob Moerbeek</aut>
          <vrk><url ref="https://www.monato.be/2011/010306.php"
              >Embargu la embargon!</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kreitaro subi&gcirc;is sub la <tld/>econ, ne volonte
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 8:20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_e;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vanitat</trd>,
    <trd>inefic&agrave;cia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>marnost</trd>,
    <trd>marn&aacute; snaha</trd>,
    <trd>zbyte&ccaron;nost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zwecklosigkeit</trd>,
    <trd>Vergeblichkeit</trd>
  </trdgrp>
  <trd lng="en">futility</trd>
  <trdgrp lng="es">
   <trd>vanidad<klr tip="amb"> (cualidad de vano, no de
   vanidoso)</klr></trd>,
   <trd>inutilidad</trd>,
   <trd>ineficacia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>st&eacute;rilit&eacute;<klr tip="amb"> (fig.)</klr></trd>,
    <trd>vanit&eacute;<klr tip="amb"> (inutilit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">hi&aacute;baval&oacute;s&aacute;g</trd>
  <trd lng="pl">daremno&sacute;&cacute;</trd>
  <trdgrp lng="pt">
   <trd>inutilidade</trd>,
   <trd>futilidade</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>m&aacute;rnos&tcaron;</trd>,
    <trd>zbyto&ccaron;nos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_r;&c_je;&c_m;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="van.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="van.0igi.KOMUNE">
    <dif>
      Senigi je esperita rezulto, senfruktigi, fiaskigi:
      <ekz>
        ilian laboron <tld/>igus la foresto de asocio
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj kirasitaj grandeguloj <tld/>igis &ccirc;iun kontra&ubreve;batalon
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vidanta Mano</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>igas la gracon de Dio
        <fnt><bib>NT</bib>, <lok>&Gal; 2:21</lok></fnt>;
      </ekz>
      <ekz>
        klarvideble estas en &ccirc;io &ccirc;i tio la mano de la Eternulo, kiu volas uzi vian
        kortu&scirc;an fidelecon kiel ilon por <tld/>igi la malvirtajn planojn de
        fanatikuloj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esting.0i">estingi</ref>,
      <ref cel="aper.mal0igi">malaperigi</ref>,
      <ref cel="valid.mal0igi">malvalidigi</ref>,
      <ref cel="nul0.0igi.KOMUNE">nuligi</ref>,
      <ref cel="valor.sen0igi">senvalorigi</ref>
    </refgrp>
  </snc>
  <trd lng="ca">tornar <ind>va</ind></trd>
  <trdgrp lng="cs">
    <trd>ma&rcaron;it</trd>,
    <trd>p&rcaron;ekazit</trd>,
    <trd>zma&rcaron;it</trd>
  </trdgrp>
  <trd lng="de"><ind>zwecklos</ind> machen</trd>
  <trdgrp lng="en">
    <trd>frustrate</trd>,
    <trd>thwart</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inutilizar</trd>,
    <trd>quitar importancia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rendre <ind>vain</ind></trd>,
    <trd>ruiner les espoirs de</trd>
  </trdgrp>
  <trd lng="hu">meghi&uacute;s&iacute;t</trd>
  <trd lng="pl">udaremnia&cacute;</trd>
  <trdgrp lng="pt">
   <trd>tornar <ind>v&atilde;o</ind></trd>,
   <trd>tornar <ind>in&uacute;til</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prekazi&tcaron;</trd>,
    <trd>zmari&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_d;&c_a;&c_r;&c_je;&c_m;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_i;&c_m;</trd>
  </trdgrp>
</drv>
</art>
