<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>van</rad>/a
</kap>
<drv mrk="van.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Ne produktanta efikan rezulton; senutila:
      <ekz>
        <ind>kontra&ubreve; forta mano la le&gcirc;o estas <tld/>a</ind>
        <klr>(senpova)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a t&ouml;rv&eacute;ny csak a gy&aacute;moltalant k&ouml;ti</trd>
      </ekz>
      <ekz>
        <tld/>a teoria laboro kaj senutila batado de vento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nia voko ne restos <tld/>a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al vi klopodojn kaj al mi suferojn vi ka&ubreve;zas, tamen amba&ubreve;
        estas <tld/>aj<fnt>Z</fnt>
        <klr>(senfruktaj)</klr>;
      </ekz>
      <ekz>
        <ind>servo al Dio <tld/>a <klr>(senrekompenca)</klr> ne 
        restas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">az Isten soha nem marad ad&oacute;sunk</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zwecklos</trd>,
      <trd>fruchtlos</trd>,
      <trd>ergebnislos</trd>,
      <trd>nutzlos</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>in&uacute;til</trd>,
      <trd>ineficaz</trd>,
      <trd>infructuoso</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Havanta nur &scirc;ajnon sen realeco; neefektiva:
      <ekz>
        <tld/>a espero
        (<ref tip="vid" cel="kan.0o">kano</ref>);
      </ekz>
      <ekz>
        tiuj profeta&jcirc;oj estis <tld/>aj<fnt>Z</fnt>
        <klr>(ne plenumi&gcirc;is)</klr>;
      </ekz>
      <ekz>
        <tld/>a ombro, fantomo, son&gcirc;o;
      </ekz>
      <ekz>
        mia timo estis <tld/>a.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>vergeblich</trd>,
      <trd>umsonst</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_e;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="br">aner<klr tip="ind"> (ag.)</klr></trd>
  <trd lng="ca">va</trd>
  <trdgrp lng="cs">
    <trd>marn&yacute;</trd>,
    <trd>pom&iacute;jej&iacute;c&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zwecklos</trd>,
    <trd>fruchtlos</trd>,
    <trd>ergebnislos</trd>,
    <trd>nutzlos</trd>,
    <trd>vergeblich</trd>,
    <trd>umsonst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>futile</trd>,
    <trd>vain</trd>
  </trdgrp>
  <trd lng="es">vano</trd>
  <trd lng="fr">vain</trd>
  <trdgrp lng="hu">
    <trd>hi&aacute;baval&oacute;</trd>,
    <trd>medd&odblac;</trd>,
    <trd>hi&uacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vergeefs</trd>,
    <trd>ijdel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>v&atilde;o<klr tip="ind"> (adj.)</klr></trd>,
    <trd>in&uacute;til<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_shch;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">m&aacute;rny</trd>
</drv>

<drv mrk="van.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Sen rezulto; senutile:
      <ekz>
        &ccirc;u &ccirc;iuj niaj laboroj pereos <tld/>e?<fnt>Z</fnt>
      </ekz>
      <ekz>
        pri tia intrigo mi jam dufoje vin <tld/>e avertis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>e vi tentas, mi ne konsentos</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">m&eacute;g a te k&eacute;t szemed&eacute;rt se</trd>
      </ekz>
      <ekz>
        <ind>el malplena telero <tld/>e &ccirc;erpas kulero</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ahol nincs, ott ne keress</trd>
      </ekz>
      <ekz>
        <tld/>e labori.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drasx.0i">dra&scirc;i</ref>,
      <ref cel="erp.0i">erpi</ref>,
      <ref cel="hirud.0o">hirudo</ref>,
      <ref cel="kribr.0ilo">kribrilo</ref>,
      <ref cel="melk.0i">melki</ref>,
      <ref cel="musx.0o">mu&scirc;o</ref>,
      <ref cel="plen.0igi">plenigi</ref>,
      <ref cel="sciur.0o">sciuro</ref>,
      <ref cel="sxnur.0o">&scirc;nuro</ref>,
      <ref cel="tord.0i">tordi</ref>,
      <ref cel="vent.0o">vento</ref>,
      <ref cel="versx.0i">ver&scirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_e;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="br">en <ind>aner</ind></trd>
  <trd lng="ca">en <ind>va</ind></trd>
  <trd lng="cs">marn&ecaron;</trd>
  <trdgrp lng="de">
    <trd>zwecklos</trd>,
    <trd>fruchtlos</trd>,
    <trd>ergebnislos</trd>,
    <trd>nutzlos</trd>,
    <trd>vergeblich</trd>,
    <trd>umsonst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>in vain</trd>,
    <trd>vainly</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>en vano</trd>,
    <trd>vanamente</trd>,
    <trd>ineficazmente</trd>,
    <trd>infructuosamente</trd>
  </trdgrp>
  <trd lng="fr">en <ind>vain</ind></trd>
  <trd lng="hu">hi&aacute;ba</trd>
  <trd lng="nl">tevergeefs</trd>
  <trd lng="pt">em v&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_r;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_s;&c_n;&c_o;</trd>,
    <trd>&c_t;&c_shch;&c_je;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">m&aacute;rne</trd>
</drv>

<drv mrk="van.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="van.0eco.KOMUNE">
    <dif>
      Senrezulteco, senfrukteco:
      <ekz>
        &ccirc;u vi fine konfesos la <tld/>econ de viaj penoj?
      </ekz>
      <ekz>
        la pensoj de Adaha sin okupis ne nur <klr>...</klr> pri la
        <tld/>eco de knabinaj revoj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 23a, paca
Betra&hcirc;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>vanitat</trd>,
    <trd>inefic&agrave;cia</trd>
  </trdgrp>
  <trd lng="en">futility</trd>
  <trdgrp lng="es">
   <trd>vanidad<klr tip="amb"> (cualidad de vano, no de
   vanidoso)</klr></trd>,
   <trd>inutilidad</trd>,
   <trd>ineficacia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>st&eacute;rilit&eacute;<klr tip="amb"> (fig.)</klr></trd>,
    <trd>vanit&eacute;<klr tip="amb"> (inutilit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">hi&aacute;baval&oacute;s&aacute;g</trd>
  <trdgrp lng="pt">
   <trd>inutilidade</trd>,
   <trd>futilidade</trd>
  </trdgrp>
</drv>

<drv mrk="van.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="van.0igi.KOMUNE">
    <dif>
      Senigi je esperita rezulto, senfruktigi, fiaskigi:
      <ekz>
        mi ne <tld/>igas la gracon de Dio
        <fnt><bib>NT</bib>, <lok>&Gal; 2:21</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nul0.0igi.KOMUNE">nuligi</ref>
    </refgrp>
  </snc>
  <trd lng="ca">tornar <ind>va</ind></trd>
  <trdgrp lng="en">
    <trd>frustrate</trd>,
    <trd>thwart</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inutilizar</trd>,
    <trd>quitar importancia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rendre <ind>vain</ind></trd>,
    <trd>ruiner les espoirs de</trd>
  </trdgrp>
  <trd lng="hu">meghi&uacute;s&iacute;t</trd>
  <trdgrp lng="pt">
   <trd>tornar <ind>v&atilde;o</ind></trd>,
   <trd>tornar <ind>in&uacute;til</ind></trd>
  </trdgrp>
</drv>
</art>
