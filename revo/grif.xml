<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grif</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="grif.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mitologia flugilhava besto kun kapo de aglo k korpo de leono. 
    </dif>
    <trd lng="de">Greif</trd>
    <trd lng="hu">griffmad&aacute;r</trd>
    <trd lng="ru">&c_g;&c_r;&c_i;&c_f;&c_o;&c_n;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Nomo de diversaj rabobirdoj nutrantaj sin precipe de putra&jcirc;o.  
    </dif>
    <refgrp tip="sub">
      <ref cel="vultur.0o">vulturo</ref>,
      <ref cel="kondor.0o">kondoro</ref>,
      <ref cel="gipaet.0o">gipaeto</ref>, k.a.
    </refgrp>
    <trd lng="cs">sup</trd>
    <trd lng="de">Geier</trd>
    <trd lng="en">vulture</trd>
    <trd lng="hu">kesely&udblac;</trd>
    <trd lng="pl">s&eogonek;p</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>griffon<klr tip="ind"> (mith., oiseau de proie)</klr></trd>,
    <trd>vautour</trd></trdgrp> 
  <trd lng="ru">&c_g;&c_r;&c_i;&c_f;</trd>
</drv>
</art>
