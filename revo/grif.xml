<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grif.xml,v 1.20 2018/12/25 13:10:15 revo Exp $">
<kap>
  <rad>grif</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="grif.0o">
  <kap><tld/>o</kap>
  <snc mrk="grif.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Nomo de diversaj <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref> nutrantaj sin precipe de putra&jcirc;o.
    </dif>
    <refgrp tip="sub">
      <ref cel="vultur.0o">vulturo</ref>,
      <ref cel="kondor.0o">kondoro</ref>,
      <ref cel="gipaet.0o">gipaeto</ref>, k.a.
    </refgrp>
    <trd lng="bg">&c_l;&c_je;&c_sh;&c_o;&c_ja;&c_d;</trd>
    <trd lng="ca">carronyaire<klr> (au)</klr></trd>
    <trd lng="cs">sup</trd>
    <trd lng="de">Geier</trd>
    <trd lng="en">vulture</trd>
    <trd lng="fr">charognard</trd>
    <trd lng="hu">kesely&udblac;</trd>
    <trd lng="it">avvoltoio</trd>
    <trd lng="nl">gier</trd>
    <trd lng="pl">s&eogonek;p</trd>
    <trd lng="pt">abutre</trd>
  </snc>
  <snc mrk="grif.0o.MIT">
    <uzo tip="fak">HER</uzo>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mita <ref tip="super" cel="monstr.0o.BIO">monstro</ref> 
      kun aglaj kapo, flugiloj kaj ungoj, leona korpo, ofte figurata sur
      blazonoj, ornama&jcirc;oj:
      <ekz>
        tio estas grifo!... mi klare vidis la flugilojn... <klr>[&#8230;]</klr> &dash; en la
        dezerto svarmas monstroj!... &dash; <klr>[&#8230;]</klr> anka&ubreve; al li &scirc;ajnis, ke la
        preterkuranta ombro havis kapon de serpento kaj kvaza&ubreve; mallongajn flugilojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu stalo cetere estis ankora&ubreve; multaj aliaj rajdbestoj: rozkolora kaj blua
        elefanto, grandega birdo <tld lit="G"/>o, kies anta&ubreve;a korpoduono aspektis
        kiel tiu de aglo kaj la malanta&ubreve;a kiel tiu de leono <klr>[&#8230;]</klr>
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">griu</trd>
    <trd lng="de">Greif</trd>
    <trd lng="en">griffin</trd>
    <trd lng="fr">griffon<klr> (myth.)</klr></trd>
    <trd lng="hu">griffmad&aacute;r</trd>
    <trd lng="it">grifone</trd>
    <trd lng="nl">griffoen</trd>
    <trdgrp lng="pl">
      <trd>gryf</trd>,
      <trd>gryfon</trd>
    </trdgrp>
    <trd lng="pt">grifo<klr tip="amb"> (mitologia)</klr></trd>
    <trd lng="ru">&c_g;&c_r;&c_i;&c_f;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>Noh</trd>,
    <trd>gryf</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_f;</trd>
  <trdgrp lng="sk">
    <trd>Noh</trd>,
    <trd>gryf</trd>
  </trdgrp>
</drv>
</art>
