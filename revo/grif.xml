<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>grif</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="grif.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mitologia flugilhava besto kun kapo de aglo kaj korpo de leono.
    </dif>
    <trd lng="de">Greif</trd>
    <trd lng="hu">griffmad&aacute;r</trd>
    <trd lng="nl">griffoen</trd>
    <trd lng="ru">&c_g;&c_r;&c_i;&c_f;&c_o;&c_n;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Nomo de diversaj <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref>
      nutrantaj sin precipe de putra&jcirc;o.
    </dif>
    <refgrp tip="sub">
      <ref cel="vultur.0o">vulturo</ref>,
      <ref cel="kondor.0o">kondoro</ref>,
      <ref cel="gipaet.0o">gipaeto</ref>, k.a.
    </refgrp>
    <trd lng="cs">sup</trd>
    <trd lng="de">Geier</trd>
    <trd lng="en">vulture</trd>
    <trd lng="hu">kesely&udblac;</trd>
    <trd lng="nl">gier</trd>
    <trd lng="pl">s&eogonek;p</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>griffon<klr tip="ind"> (mith., oiseau de proie)</klr></trd>,
    <trd>vautour</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_f;</trd>
</drv>
</art>
