  <rad>Sigmund</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sigmund.0o">
  <kap><tld/>o</kap>
  <snc mrk="sigmund.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> varia&jcirc;o de
      <ref tip="vid" cel="sigismund.0o">Sigismundo</ref>:
