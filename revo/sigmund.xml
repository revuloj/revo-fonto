      <trd>&c_Z;&c_y;&c_g;&c_m;&c_u;&c_n;&c_t;</trd>,
      <trd>&c_Z;&c_y;&c_g;&c_m;&c_u;&c_s;&c_mol;</trd>,
      <trd>&c_Zh;&c_y;&c_g;&c_ib;&c_m;&c_o;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="fr">Sigmund</trd>
    <trd lng="pl">Zygmunt</trd>
  </snc>
  <trd lng="en">Sigmund</trd>
  <trd lng="uk">&c_Z;&c_i;&c_gu;&c_m;&c_u;&c_n;&c_d;</trd>
  <trd lng="zh">&#35199;&#26684;&#33945;&#24503; <pr>X&imacron;g&eacute;m&emacron;ngd&eacute;</pr></trd>
</drv>
</art>

