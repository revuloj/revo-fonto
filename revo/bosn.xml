<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bosn</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="bosn.0o">
  <kap><tld/>o</kap>
  <snc mrk="bosn.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="bosn.B0ujo"><tld lit="B"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="bosn.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="en">Bosnian</trd>
  <trd lng="fr">Bosniaque<klr> (subst.)</klr></trd>
</drv>

<drv mrk="bosn.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="en">Bosnian</trd>
  <trd lng="fr">bosniaque<klr> (adj.)</klr></trd>
</drv>

<drv mrk="bosn.B0ujo">
  <kap>
    <tld lit="B"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bosn.B0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      nuntempe parto de la teritorio de <ref tip="malprt"
      cel="bosn.B0ujoHercegovino.sxtato">Bosnujo kaj Hercegovino</ref>.
    </dif>
    </snc>
    <ref tip="sin" cel="bosni.B0o.GEOG">Bosnio</ref>
    <ref tip="vid" cel="hercegov.H0o.GEOG">Hercegovino</ref>
    <trd lng="bg">&c_B;&c_o;&c_s;&c_n;&c_a;</trd>
    <trd lng="de">Bosnien</trd>
    <trd lng="en">Bosnia</trd>
    <trd lng="fr">Bosnie</trd>
    <trdgrp lng="pl">
      <trd>Bo&sacute;nia</trd>
    </trdgrp>
    <trd lng="pt">B&oacute;snia</trd>
</drv>

<drv mrk="bosn.B0ujoHercegovino">
  <kap>
    <tld lit="B"/>ujo kaj Hercegovino
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bosn.B0ujoHercegovino.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Sarajevo.
      Teritorio de Federacio Bosnujo kaj Hercegovino
      kaj Respubliko Serbujo.
      Iam parto de <ref tip="malprt"
      cel="jugoslav.J0ujo.GEOG">Jugoslavujo</ref>.
      ISO-3166 kodo: BA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kroat.K0ujo.GEOG">Kroatujo</ref>,
      <ref tip="vid" cel="monteneg.M0o.GEOG">Montenegro</ref>,
      <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>
      kaj Adriatiko.
    </dif>
    </snc>
    <ref tip="sin" cel="bosni.B0oHercegovino.sxtato">Bosnio kaj
    Hercegovino</ref>
    <trd lng="bg">&c_B;&c_o;&c_s;&c_n;&c_a; &c_i; &c_H;&c_je;&c_r;&c_c;&c_je;&c_g;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
    <trd lng="de">Bosnien und Herzegowina</trd>
    <trd lng="en">Bosnia and Herzegovina</trd>
    <trd lng="fr">Bosnie-Herz&eacute;govine</trd>
    <trdgrp lng="pl">
      <trd>Bo&sacute;nia i Hercogowina</trd>
    </trdgrp>
</drv>

</art>

