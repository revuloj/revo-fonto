<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bosn</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="bosn.0ujo">
  <kap>
    <tld lit="B"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bosn.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      nuntempe parto de la teritorio de <ref tip="malprt"
      cel="bosn.0ujohercegovino.sxtato">Bosnujo kaj Hercegovino</ref>.
    </dif>
    <ref tip="sin" cel="bosni.0o.GEOG">Bosnio</ref>
    <ref tip="vid" cel="hercegov.0o.GEOG">Hercegovino</ref>
    <trd lng="bg">&c_B;&c_o;&c_s;&c_n;&c_a;</trd>
    <trd lng="de">Bosnien</trd>
    <trd lng="en">Bosnia</trd>
    <trdgrp lng="pl">
      <trd>Bo&sacute;nia</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="bosn.0ujohercegovino">
  <kap>
    <tld lit="B"/>ujo kaj Hercegovino
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bosn.0ujohercegovino.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Sarajevo.
      Teritorio de Federacio Bosnujo kaj Hercegovino
      kaj Respubliko Serbujo.
      Iam parto de <ref tip="malprt"
      cel="jugoslav.0ujo.GEOG">Jugoslavujo</ref>.
      ISO-3166 kodo: BA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kroat.0ujo.GEOG">Kroatujo</ref>,
      <ref tip="vid" cel="monteneg.0o.GEOG">Montenegro</ref>,
      <ref tip="vid" cel="serb.0ujo.GEOG">Serbujo</ref>
      kaj Adriatiko.
    </dif>
    <ref tip="sin" cel="bosni.0ohercegovino.sxtato">Bosnio kaj
    Hercegovino</ref>
    <trd lng="bg">&c_B;&c_o;&c_s;&c_n;&c_a; &c_i; &c_H;&c_je;&c_r;&c_c;&c_je;&c_g;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
    <trd lng="de">Bosnien und Herzegowina</trd>
    <trd lng="en">Bosnia and Herzegovina</trd>
    <trdgrp lng="pl">
      <trd>Bo&sacute;nia i Hercogowina</trd>
    </trdgrp>
  </snc>
</drv>

</art>

