<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bosn.xml,v 1.12 2021/03/05 15:48:38 revo Exp $">
<kap>
  <rad>bosn</rad>/o
</kap>

<drv mrk="bosn.0o">
  <kap><tld/>o</kap>
  <snc mrk="bosn.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="bosn.B0ujo"><tld lit="B"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="bosn.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        kantono <nom>Zenica-Doboj</nom> <klr>[&#x2026;]</klr> lo&gcirc;antaro:
        <tld lit="B"/>oj 334 031 (83,6%)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4606">Kantono
          Zenica-Doboj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne sukcesos nei la nemezureble pli ampleksajn perfortadojn
        kaj
        bu&ccirc;adojn faritajn de serboj kontra&ubreve; kroatoj, <tld/>oj kaj
        albanoj ol inverse
        <fnt>
          <bib>Monato</bib>
          <aut>Guido Van Damme</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007043.html">Na&ubreve;
          kontra&ubreve;batoj</url></vrk>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Bosnian</trd>
  <trd lng="fr">Bosniaque<klr> (subst.)</klr></trd>
  <trd lng="he">&#x5D1;&#x5D5;&#x5E1;&#x5E0;&#x5D9;&#x5D4;</trd>
  <trdgrp lng="io">
    <trd>Bosniano</trd>,
    <trd>Bosno</trd>
  </trdgrp>
  <trd lng="tp">jan pi ma Posan</trd>
  <trd lng="vo">Bosn&auml;nan</trd>
</drv>

<drv mrk="bosn.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        en tiu regiono lo&gcirc;as pli<klr>[ ]</klr>multe <tld/>aj serboj, 
        sed dum jarcentoj ili vivis kiel najbaroj de islamanoj kaj
        kroatoj
        kaj ili alkutimi&gcirc;is al diverseco kaj kuna vivo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006811.html">Reago:
          Nekredeblaj malveroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Bosnian</trd>
  <trd lng="fr">bosniaque<klr> (adj.)</klr></trd>
  <trd lng="he">&#x5D1;&#x5D5;&#x5E1;&#x5E0;&#x5D9;</trd>
  <trdgrp lng="io">
    <trd>Bosniana</trd>,
    <trd>Bosna</trd>
  </trdgrp>
  <trd lng="nl">Bosnisch</trd>
  <trd lng="tp">pi ma Posan</trd>
  <trd lng="vo">Bosn&auml;nik</trd>
</drv>

<drv mrk="bosn.0alingvo">
  <kap><tld/>a lingvo</kap>
  <snc>
    <dif>Oficiala lingvo de <ref tip="malprt" cel="bosn.B0ujoHercegovino.sxtato">Bosnujo kaj Hercegovino</ref>:
      <ekz>
        la slava lingvaro <klr>[&#x2026;]</klr> dividi&gcirc;as je la orienta (rusa,
        ukraina,
        belorusa lingvoj), la okcidenta (pola, &ccirc;e&hcirc;a, slovaka, ka&scirc;uba,
        soraba
        lingvoj) kaj la suda (slovena, kroata, <tld/>a, montenegra,
        serba, makedona, bulgara lingvoj) 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3842">Slava
          lingvaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_n;&c_ib;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="de">Bosnisch</trd>
  <trd lng="en">Bosnian language</trd>
  <trd lng="hu">bosny&aacute;k nyelv</trd>  
  <trdgrp lng="io">
    <trd>Bosniana linguo</trd>,
    <trd>Bosna linguo</trd>
  </trdgrp>
  <trd lng="nl">Bosnisch</trd>
  <trd lng="pl"><klr tip="amb">j&eogonek;zyk </klr>bo&sacute;niacki</trd>
  <trd lng="ru">&c_b;&c_o;&c_s;&c_n;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
  <trd lng="tp">toki Posan</trd>
  <trd lng="vo">Bosn&auml;nap&uuml;k</trd>
</drv>

<drv mrk="bosn.B0ujo">
  <kap>
    <tld lit="B"/>ujo
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc mrk="bosn.B0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      nuntempe parto de la teritorio de <ref tip="malprt" cel="bosn.B0ujoHercegovino.sxtato">Bosnujo kaj Hercegovino</ref>.
    </dif>
    <ref tip="sin" cel="bosni.B0o.GEOG">Bosnio</ref>
    <ref tip="vid" cel="hercegov.H0o.GEOG">Hercegovino</ref>
  </snc>
  <trd lng="bg">&c_B;&c_o;&c_s;&c_n;&c_a;</trd>
  <trd lng="de">Bosnien</trd>
  <trd lng="en">Bosnia</trd>
  <trd lng="fr">Bosnie</trd>
  <trd lng="fy">Bosnje</trd>
  <trd lng="id">Bosnia</trd>
  <trd lng="io">Bosnia</trd>
  <trd lng="nl">Bosni&euml;</trd>
  <trdgrp lng="pl">
    <trd>Bo&sacute;nia</trd>
  </trdgrp>
  <trd lng="pt">B&oacute;snia</trd>
  <trd lng="tp">ma Posan</trd>
  <trd lng="vo">Bosn&auml;n</trd>
</drv>

<drv mrk="bosn.B0ujoHercegovino">
  <kap>
    <tld lit="B"/>ujo kaj Hercegovino
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">BA</mlg>
  <snc mrk="bosn.B0ujoHercegovino.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la suda <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Sarajevo; teritorio de Federacio Bosnujo kaj
      Hercegovino
      kaj Respubliko Serbujo;
      iam parto de <ref tip="malprt" cel="jugoslav.J0ujo.GEOG">Jugoslavujo</ref>.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kroat.K0ujo.GEOG">Kroatujo</ref>,
      <ref tip="vid" cel="monteneg.M0o.GEOG">Montenegro</ref>,
      <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>
      kaj Adriatiko:
      <ekz>
        la plej granda mezurita ordinara piceo, kiu altas 63 m, trovi&gcirc;as
        en Praarbaro <nom>Perucika</nom>, Nacia Parko
        <nom>Sutjeska</nom>, <tld lit="B"/>ujo kaj Hercegovino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=337773">Ordinara
          piceo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="bosni.B0oHercegovino.sxtato">Bosnio kaj
      Hercegovino</ref>
  </snc>
  <trd lng="bg">&c_B;&c_o;&c_s;&c_n;&c_a; &c_i; &c_H;&c_je;&c_r;&c_c;&c_je;&c_g;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
  <trd lng="de">Bosnien und Herzegowina</trd>
  <trd lng="en">Bosnia and Herzegovina</trd>
  <trd lng="fr">Bosnie-Herz&eacute;govine</trd>
  <trd lng="fy">Bosnje-Hertsegovina</trd>
  <trd lng="id">Bosnia Herzegovina</trd>
  <trd lng="io">Bosnia e Herzegovina</trd>
  <trd lng="nl">Bosni&euml; en Herzegovina</trd>
  <trd lng="pl">Bo&sacute;nia i Hercogowina</trd>
  <trd lng="vo">Bosn&auml;n e H&auml;rzegov&auml;n</trd>
</drv>

</art>

