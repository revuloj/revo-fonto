<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gargojl</rad>/o</kap>
<drv mrk="gargoj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Elstaranta kanaleto, ofte skulptita kun formo de vomanta besto
      a&ubreve; homo, el kiu la akvo de tegmentoj falas eksteren, distance de
      la muroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.de0ilo">defluilo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Wasserspeier</trd>
  <trd lng="en">gargoyle</trd>
  <trd lng="es">g&aacute;rgola</trd>
  <trd lng="fr">gargouille<klr tip="ind"> (pour &eacute;vacuer l'eau)</klr></trd>
  <trd lng="hu">v&iacute;zk&ouml;p&odblac;</trd>
  <trd lng="nl">waterspuwer</trd>
  <trd lng="pl">gargulec</trd>
  <trd lng="pt">g&aacute;rgula</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_g;&c_u;&c_l;&c_mol;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
