<kap>
  <rad>gargojl</rad>/o
</kap>
<drv mrk="gargoj.0o">
  <kap><tld/>o</kap>
  <snc mrk="gargoj.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tegmentelstara kanaleto, ofte figuranta vomantan beston
      a&ubreve; homon, el kiu pluvakvo falas eksteren, distance de
      la muroj:
      <ekz>
        Ruben <klr>[&#x2026;]</klr> dum tuta vivo grimpadis supren-suben, / &ccirc;izante sur
        pre&gcirc;ejoj <tld/>ojn kaj an&gcirc;elojn
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.de0ilo">defluilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_g;&c_u;&c_l;&c_mol;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>chrli&ccaron;</trd>,
    <trd>okapov&aacute; roura</trd>
  </trdgrp>
  <trd lng="de">Wasserspeier</trd>
  <trd lng="en">gargoyle</trd>
  <trd lng="es">g&aacute;rgola</trd>
  <trd lng="fr">gargouille<klr tip="ind"> (archi.)</klr></trd>
  <trd lng="hu">v&iacute;zk&ouml;p&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30FC;&#x30B4;&#x30A4;&#x30EB;</trd>,
    <trd>&#x3072;&#x306F;&#x3057;</trd>
  </trdgrp>
  <trd lng="nl">waterspuwer</trd>
  <trd lng="pl">gargulec</trd>
  <trd lng="pt">g&aacute;rgula</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_g;&c_u;&c_l;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">chrli&ccaron; <klr>(pre odtok vody zo strechy)</klr></trd>
  <trd lng="uk">(&c_d;&c_je;&#x43A;&#x43E;&c_r;&c_a;&c_t;&c_i;&c_v;&#x43D;&c_a;)&#xA0;&c_r;&c_i;&#x43D;&c_v;&c_a;</trd>
  <trd lng="zh">&#x6EF4;&#x6C34;&#x5634;&#x517D; <pr>d&#x12B;shu&#x1D0;zu&#x1D0;sh&#xF2;u</pr></trd>
</drv>
