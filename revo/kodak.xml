<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kodak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kodak.0o">
  <kap><tld/>o</kap>
  <snc mrk="kodak.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Portebla <ref tip="super" cel="fotogr.0ilo">fotografilo</ref> de
      simpla uzo, kun karto&ccirc;filmo:
      <ekz>
        tiam la pasa&gcirc;eroj malrapide elpo&scirc;igas la <tld/>ojn kaj, celante,
        klakas
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">appareil <ind>photo</ind></trd>
  <rim mrk="kodak.0o.RIM_ETIMO">
    Markonomo.
  </rim>
</drv>
</art>

