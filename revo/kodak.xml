<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kodak.xml,v 1.15 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>kodak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kodak.0o">
  <kap><tld/>o</kap>
  <snc mrk="kodak.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Portebla <ref tip="super" cel="fotogr.0ilo">fotografilo</ref> de
      simpla uzo, kun karto&ccirc;filmo:
      <ekz>
        tiam la pasa&gcirc;eroj malrapide elpo&scirc;igas la <tld/>ojn
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kodak.0o.RIM_ETIMO">
     Markonomo.
  </rim>
  <trd lng="be">&c_k;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="cs">kodak</trd>
  <trd lng="de">Fotoapparat</trd>
  <trdgrp lng="en">
    <trd>instant camera</trd>,
    <trd>Polaroid</trd>, <trd>Kodak</trd>
  </trdgrp>
  <trd lng="fr">appareil <ind>photo</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30C0;&#x30C3;&#x30AF;</trd>,
    <trd>&#x30AB;&#x30E1;&#x30E9;  <pr>&#x304B;&#x3081;&#x3089;</pr></trd>
  </trdgrp>
  <trd lng="nl">fototoestel<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pl"><klr>ma&lstroke;oobrazkowy</klr> aparat
  <klr>fotograficzny</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_d;&c_a;&c_k;<klr tip="amb"> (&c_m;&c_a;&c_r;&c_k;&c_a;
  &c_f;&c_o;&c_t;&c_o;&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;&c_a;)</klr></trd>
  <trd lng="sk">kodak</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_d;&c_a;&#x43A; (&c_f;&#x43E;&c_t;&#x43E;&c_a;&#x43F;&c_a;&c_r;&c_a;&c_t;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5B9D;&#x4E3D;&#x6765; <pr>B&#x1CE;ol&#xEC;l&#xE1;i</pr></trd>,
    <trd>&#x62CD;&#x7ACB;&#x5F97; <pr>P&#x101;il&#xEC;d&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

