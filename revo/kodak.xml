<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kodak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kodak.0o">
  <kap><tld/>o</kap>
  <snc mrk="kodak.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Portebla <ref tip="super" cel="fotogr.0ilo">fotografilo</ref> de
      simpla uzo, kun karto&ccirc;filmo:
      <ekz>
        tiam la pasa&gcirc;eroj malrapide elpo&scirc;igas la <tld/>ojn
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kodak</trd>
  <trd lng="de">Fotoapparat</trd>
  <trdgrp lng="en">
    <trd>instant camera</trd>,
    <trd>Polaroid</trd>, <trd>Kodak</trd>
  </trdgrp>
  <trd lng="fr">appareil <ind>photo</ind></trd>
  <trd lng="nl">fototoestel<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_d;&c_a;&c_k;<klr tip="amb"> (&c_m;&c_a;&c_r;&c_k;&c_a;
  &c_f;&c_o;&c_t;&c_o;&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;&c_a;)</klr></trd>
  <trd lng="sk">kodak</trd>
</drv>
</art>

