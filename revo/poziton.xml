<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>poziton</rad>/o
</kap>
<drv mrk="poziton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Pozitiva <ref tip="super" cel="elektr14.0o">elektrono</ref>,
      <ref tip="vid" cel="partik2.kontraux0o">kontra&ubreve;partiklo</ref>
de
      <ref tip="vid" cel="negaton.0o">negatrono</ref>.
    </dif>
  </snc>
  <ref tip="sin" cel="pozitr.0o">pozitrono</ref>
  <trd lng="be">&c_p;&c_a;&c_z;&c_ib;&c_t;&c_r;&c_o;&c_n;</trd>
<trdgrp lng="de">
   <trd>Positon</trd>,
   <trd>Positron</trd>
</trdgrp>
  <trd lng="en">positron</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_z;&f_y;&a_t;&a_r;&a_w;&a_n;</trd>,
    <trd>&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_n; &a_m;&a_th;&a_b;&a_t;</trd>
  </trdgrp>
  <trd lng="hu">pozitron</trd>
  <trd lng="nl">positron</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_t;&c_r;&c_o;&c_n;</trd>
</drv>
</art>

