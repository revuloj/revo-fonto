<kap>
  <rad>alberg</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="alberg.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="alberg.0o.TUR">
    <dif>
      <ref tip="super" cel="gast.0ejo.TUR">Gastejo</ref> senkomforta,
      ofte montara a&ubreve; kampara:
      <ekz>
        du el la amikoj adia&ubreve;is la trian, &ccirc;ar ili ne &ccirc;iuj lo&gcirc;is en la
        sama <tld/>o
        <fnt>
          <aut>J. A. Schneiderfranken, trad. A. Rehfeldt</aut>,
          <vrk><url ref="www.a-rehfeldt.de/LaSekretoEspTNR.pdf">la
          sekreto</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        tagman&gcirc;o en la <tld/>o <nom>Brandon</nom>
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=IRa7NALVqSw">Irlande 7: Killarney</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kaban.0o">kabano</ref>
    <trdgrp lng="zh">
    <trd>&#x5B89;&#x6392;&#x81B3;&#x5BBF; <pr>&#x101;np&#xE1;ish&#xE0;ns&#xF9;</pr></trd>,
    <trd>&#x5E90; <pr>l&#xFA;</pr></trd>,
    <trd>&#x5EEC; <pr>l&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&#xF58;&#xF42;&#xFB2;&#xF7C;&#xF53;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Herberge</trd>,
    <trd>H&uuml;tte</trd>,
    <trd>Landgasthof</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>auberge</trd>,
    <trd>inn</trd>, <trd>tavern</trd>, <trd>hostel</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_s;&a_A;&a_f;&a_r;&a_kh;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_m;&a_h;&a_m;&a_A;&a_n;&a_kh;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_m;&a_h;&a_m;&a_A;&a_n;&a_s;&a_r;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>auberge</trd>,
    <trd>taverne</trd>
  </trdgrp>
  <trd lng="nl">taverne</trd>
  <trd lng="pt">albergue</trd>
  <trd lng="hu">vend&eacute;gfogad&oacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x441;&#x442;&#x43E;&#x44F;&#x43B;&#x438;&#x439;/&#x437;&#x430;&#x457;&#x436;&#x434;&#x436;&#x438;&#x439; &#x434;&#x432;&#x456;&#x440;</trd>,
