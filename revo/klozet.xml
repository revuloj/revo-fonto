<art mrk="$Id: klozet.xml,v 1.8 2022/05/01 08:09:24 revo Exp $">
<kap>
  <rad>klozet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klozet.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="klozet.0o.HEJM">
    <dif>
      <ref tip="dif" cel="neces.0ejo.HEJM">Necesejo</ref>:
      <ekz>
        ili <klr>[&#x2026;]</klr> veki&gcirc;is samtempe, sentis bezonon iri al la 
        <tld/>o en la sama horo, suferis la samajn san-perturbojn
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        la&ubreve; sia vespera kutimo, <nom>Raneto Krozeti</nom> estis
        ka&scirc;fumanta en la tereta&gcirc;a <tld/>o
        <fnt>
          <aut>I. Ertl</aut>,
          <vrk>La Postdomo</vrk>,
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        Putino <klr>[&#x2026;]</klr> promesis &leftquot;trafi &ccirc;iujn ribelajn &ccirc;e&ccirc;enojn &gcirc;is la plej
        ka&scirc;itaj <tld/>oj kaj <ref cel="latrin.0o">latrino</ref>j&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="&Steloj;monato2/006861.html">&Steloj;monato2/006861.html</url></lok>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trdgrp lng="cs">
    <trd>WC</trd>,
    <trd>klozet</trd>,
    <trd>splachovac&iacute; z&aacute;chod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Klosett</trd>,
    <trd>Latrine</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>toilet</trd>,
    <trd>water closet</trd>, <trd>W.C.</trd>
  </trdgrp>
  <trd lng="fr">lieux d'aisances</trd>
  <trdgrp lng="pl">
    <trd>klozet</trd>,
    <trd>sedes</trd>,
    <trd>ubikacja</trd>,
    <trd>ust&eogonek;p</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;chod splachovac&iacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x430;&#x43B;&#x435;&#x442;</trd>,
    <trd>&#x443;&#x431;&#x438;&#x440;&#x430;&#x43B;&#x44C;&#x43D;&#x44F;</trd>,
    <trd>&#x43A;&#x43B;&#x43E;&#x437;&#x435;&#x442;</trd>
  </trdgrp>
</drv>
</art>

