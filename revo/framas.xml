<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>framason</rad>/o
</kap>
<drv mrk="framas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ano de iu kulturasocieto kun sekretaj ritoj, ekzistanta en
      multaj landoj kaj celanta la kunhelpon inter siaj membroj k
      &gcirc;enerale la plibonigon de la moraleco, frateco kaj justeco en la 
      socio:
      <ekz>
        <ind><tld/>a</ind> <ref cel="logxi.0o.framasona">lo&gcirc;io</ref>;
        <trd lng="be">&c_m;&c_a;&c_s;&c_o;&c_n;&c_s;&c_k;&c_ib;</trd>
        <trdgrp lng="fr">
          <trd>franc-ma&ccedil;onnique</trd>,
          <trd>ma&ccedil;onnique</trd></trdgrp>
          <trd lng="hu">szabadk&odblac;m&udblac;ves</trd> 
        <trd lng="nl">vrijmetselaars-</trd>
      </ekz>
      <ekz>
        <ind><tld/>aro</ind>;
        <trd lng="be">&c_m;&c_a;&c_s;&c_o;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
        <trdgrp lng="fr">
          <trd>franc-ma&ccedil;onnerie<klr tip="ind"> (ensemble des adeptes)</klr></trd>,
          <trd>ma&ccedil;onnerie<klr tip="ind"> (ensemble des adeptes)</klr></trd></trdgrp>
          <trd lng="hu">szabadk&odblac;m&udblac;vess&eacute;g<klr tip="ind"> (tagok)</klr></trd> 
      </ekz>
      <ekz>
        <ind><tld/>ismo</ind>.
        <trd lng="be">&c_m;&c_a;&c_s;&c_o;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
        <trdgrp lng="fr">
          <trd>franc-ma&ccedil;onnerie<klr tip="ind"> (syst&egrave;me)</klr></trd>,
          <trd>ma&ccedil;onnerie<klr tip="ind"> (syst&egrave;me)</klr></trd></trdgrp>
          <trd lng="hu">szabadk&odblac;m&udblac;vess&eacute;g<klr tip="ind"> (mozgalom)</klr></trd> 
      <trd lng="nl">vrijmetselarij</trd>
     </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_o;&c_n;</trd>
  <trd lng="de">Freimaurer</trd>
  <trdgrp lng="fr">
    <trd>franc-ma&ccedil;on</trd>,
    <trd>ma&ccedil;on<klr tip="ind"> (franc-ma&ccedil;on)</klr></trd></trdgrp>
  <trd lng="hu">szabadk&odblac;m&udblac;ves</trd> 
  <trd lng="nl">vrijmetselaar</trd>
  <trdgrp lng="pt">
    <trd>franco-ma&#231;om</trd>,
    <trd>franco-ma&#231;&#227;o</trd>,
    <trd>ma&#231;om</trd>,
    <trd>ma&#231;&#227;o</trd>,
    <trd>pedreiro-livre</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_o;&c_n;</trd>
</drv>
</art>
