<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: birm.xml,v 1.20 2023/10/29 19:11:02 revo Exp $">
<kap>
  <ofc>8</ofc> <rad>Birm</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="birm.B0o">
  <kap>
    <ofc>8</ofc><tld lit="B"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">MM</mlg>
  <snc mrk="birm.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la sudorienta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kun &ccirc;efurbo Nejpido (&gcirc;is
      2005 jaro &dash; Ranguno);
      teritorio de Unio Mjanmao (oficiale),
      limanta kun
      <ref tip="vid" cel="banglade.B0o.GEOG">Banglade&scirc;o</ref>,
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>,
      <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>,
      <ref tip="vid" cel="laos.L0o.GEOG">Laoso</ref>,
      <ref tip="vid" cel="taj.T0lando.GEOG">Tajlando</ref>
      kaj <ref tip="vid" cel="ocean.Hinda0o">Hinda Oceano</ref> 
      (la Bengala Golfo):
      <ekz>
        la gvidantoj de la diversaj &scirc;tatoj postulis de <tld/>o, ke &gcirc;i pli forte 
        strebu al demokrataj reformoj kaj forigu barojn al politikaj partioj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008465.html">&Ccirc;u 
          e&ubreve;rop-azia bloko kontra&ubreve; Usono?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o mortas homoj pro malsato, &ccirc;ar regantaj generaloj malpermesas &ccirc;ian eksterlandan
        helpon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009572.php">Perdata influo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;umaoj estas anoj de popolo, kiu vivas en montetoj en orienta Banglade&scirc;o &ccirc;e la landolimo kun
        Barato kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2013/010908.php">Bruligita tero, bruligitaj
          hejmoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiam mondvaste oni raportis pri ribelado de mona&hcirc;oj, vi legis 
        kaj a&ubreve;dis la landnomon Mjanmao a&ubreve; <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009340.php">Bizara lando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      Mi ne trovis uzon de nomo Mjanmao (kiu estas nuna [2009]
      oficiala &scirc;tatonomo nek Nejpido<aut>SC</aut>
    </adm>
    <trd lng="zh">&#x7F05;&#x7538; <pr>mi&#x1CE;ndi&#xE0;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_ib;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_M;&#x2019;&c_ja;&c_n;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_M;&c_i;&c_a;&c_n;&c_m;&c_a;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>Myanmar</trd>,
    <trd>Rep&uacute;blica de la Uni&oacute; de Myanmar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Myanmar</trd>,
    <trd>Birma</trd>,
    <trd>Burma</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Burma</trd>,
    <trd>Union of <ind>Myanmar</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Birmanie</trd>,
    <trd>Myanmar</trd>
  </trdgrp>
  <trd lng="hu">Burma</trd>
  <trd lng="id">Myanmar</trd>
  <trd lng="ja">&#x30D3;&#x30EB;&#x30DE;</trd>
  <trdgrp lng="nl">
    <trd>Birma</trd>,
    <trd>Myanmar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Birma</trd>,
    <trd>Myanmar</trd>,
    <trd>Zi&aogonek;zek <ind>Birma&nacute;ski</ind></trd>,
    <trd>Zwi&aogonek;zek <ind>Myanmar</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Birm&acirc;nia</trd>,
    <trd>Myanmar</trd>
  </trdgrp>
  <trd lng="uk">&#x411;&#x456;&#x440;&#x43C;&#x430;</trd>
</drv>

<drv mrk="birm.0a">
  <kap>
    <tld lit="b"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <nom>Jirake</nom> anka&ubreve; scipovis bone la <tld lit="b"/>an kaj anka&ubreve; sadari, 
        kiu estas parolata anka&ubreve; en la okcident-barata sub&scirc;tato de Biharo
        <fnt>
          <bib>Monato</bib>
          <aut>A Giridhar Rao</aut>
          <vrk><url ref="&Monato;/2005/008689.php">Formortis pluraj lingvoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="b"/>a opozicio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009340.php">Bizara lando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nia deziro, ke iam la <tld lit="b"/>a popolo venku kontra&ubreve; la subpremo fare de la fortaj kaj
        rapidaj generaloj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009340.php">Bizara lando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_r;&c_m;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
</drv>



</art>

