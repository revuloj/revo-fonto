<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaper</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kaper.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte devigi kondukiston haltigi sian veturilon a&ubreve; &scirc;an&gcirc;i la
      direkton, plej ofte pro krimaj a&ubreve; politikaj celoj:
      <ekz>
        <tld/>i aviadilon; ili <tld/>is la trajnon por atentigi la
        mondon pri la situacio en sia lando.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entf&uuml;hren</trd>,
      <trd>kapern</trd>
    </trdgrp>
    <trd lng="hu">elt&eacute;r&iacute;t</trd>
    <trd lng="ru">&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;, &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_mol;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Perforte alproprigi al si:
      <ekz>
        unu el la konsekvencoj estis, ke la jugoslava armeo ne plu
        havis legitiman estron kaj estis <tld/>ita de la beograda
        serba regantaro
        <fnt><aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/006947.html">Eks-Jugoslavio: kiu kiun
        mistifikas?</url></vrk>
        <bib>Monato</bib>
        <lok>n-ro 06/2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>an sich bringen</trd>,
      <trd>&uuml;bernehmen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>co-opt</trd>,
      <trd>take over</trd>,
      <trd>assimilate</trd>,
      <trd>absorb</trd>
    </trdgrp>
    <trd lng="hu">kisaj&aacute;t&iacute;t mag&aacute;nak</trd>
  </snc>
  <snc>
    <dif>
      Korsare kapti malamikan &scirc;ipon.
    </dif>
    <trdgrp lng="en">
      <trd>hijack</trd>,
      <trd>pirate</trd>
    </trdgrp>
    <trd lng="hu">elrabol <klr tip="ind">(haj&oacute;t)</klr></trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_g;&c_v;&c_a;&c_l;&c_t;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="fr">
    <trd>prendre d'<ind>assaut</ind></trd>,
    <trd>aborder<klr tip="ind"> (prendre d'assaut)</klr></trd>,
    <trd>accaparer</trd>,
    <trd>d&eacute;tourner<klr tip="ind"> (un avion)</klr></trd>,
    <trd>s'<ind>emparer</ind> (de)</trd>,
    <trd>mettre la main (sur)</trd>
  </trdgrp>
  <trd lng="nl">kapen</trd>
</drv>

<drv mrk="kaper.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Persono a&ubreve; instanco, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_o;&c_p;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>kapern</trd>,
    <trd>aufbringen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hijacker</trd>,
    <trd>pirate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pirate de l'air</trd>,
    <trd>pirate</trd>,
    <trd>accapareur<klr tip="ind"> (subst.)</klr></trd>,
    <trd>pr&eacute;dateur<klr tip="ind"> (qui s'accapare qc)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elt&eacute;r&iacute;t&odblac;</trd>,
    <trd>kisaj&aacute;t&iacute;t&oacute;</trd>
  </trdgrp>
  <trd lng="nl">kaper</trd>
</drv>

<drv mrk="kaper.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="korsar.0o">korsaro 2</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_s;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pirat</trd>,
    <trd>Korsar</trd>,
    <trd>Seer&auml;uber</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pirate</trd>,
    <trd>corsair</trd>,
    <trd>freebooter</trd>,
    <trd>buccaneer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>corsaire</trd>,
    <trd>flibustier</trd>,
    <trd>pirate</trd>
  </trdgrp>
  <trd lng="hu">kal&oacute;z</trd>
  <trd lng="nl">zeerover</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
