<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaper.xml,v 1.28 2024/07/18 05:52:30 revo Exp $">
<kap>
  <rad>kaper</rad>/i <fnt> <bib>PV</bib></fnt>
</kap>
<drv mrk="kaper.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kaper.0i.KOMUNE">
    <dif>
      Perforte devigi kondukiston haltigi sian veturilon a&ubreve; &scirc;an&gcirc;i la
      direkton, plej ofte pro krimaj a&ubreve; politikaj celoj:
      <ekz>
        <tld/>i aviadilon;
      </ekz>
      <ekz>
        ili <tld/>is la trajnon por atentigi la
        mondon pri la situacio en sia lando;
      </ekz>
      <ekz>
        tri krimuloj eskapis el prizono en Bru&gcirc;o per <tld/>ita helikoptero
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2009/009893.php">Pri lokoj kaj mokoj</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entf&uuml;hren</trd>
    </trdgrp>
    <trd lng="en">hijack</trd>
    <trd lng="hu">elt&eacute;r&iacute;t</trd>
  </snc>
  <snc mrk="kaper.0i.FIG">
    <dif>
      Perforte <ref tip="super" cel="propr.al0igi">alproprigi</ref> al si:
      <ekz>
        unu el la konsekvencoj estis, ke la jugoslava armeo ne plu
        havis legitiman estron kaj estis <tld/>ita de la beograda
        serba regantaro
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        oni ekzemple sendas viruson al la kompatindulo a&ubreve; provas <tld/>i ties komputilon
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2009/009687.php">Retturmentado subtaksita</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>an sich rei&szlig;en</trd>,
      <trd>&uuml;bernehmen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>co-opt</trd>,
      <trd>take over</trd>,
      <trd>assimilate</trd>,
      <trd>absorb</trd>
    </trdgrp>
    <trd lng="hu">kisaj&aacute;t&iacute;t mag&aacute;nak</trd>
  </snc>
  <snc mrk="kaper.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Korsare kapti malamikan &scirc;ipon:
      <ekz>
        <tld/>&scirc;ipo
        <fnt>
          <bib>PVEF</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vorto &leftquot;<tld/>i&rightquot; <klr>[...]</klr> signifas proksimume &leftquot;rabi
        korsare&rightquot; a&ubreve; &leftquot;rabi sur la maro&rightquot;
        <fnt><bib>LR</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>aufbringen</trd>,
      <trd>kapern</trd>,
      <trd>entern</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hijack</trd>,
      <trd>pirate</trd>
    </trdgrp>
    <trd lng="hu">elrabol <klr tip="ind">(haj&oacute;t)</klr></trd>
    <trd lng="ru">&c_v;&c_z;&c_ja;&c_t;&c_mol; &c_n;&c_a;
    <ind>&c_a;&c_b;&c_o;&c_r;&c_d;&c_a;&c_zh;</ind></trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_g;&c_v;&c_a;&c_l;&c_t;&c_a;&c_m;)</klr></trd>
  <trd lng="cs">kaperovat</trd>
  <trdgrp lng="fr">
    <trd>prendre d'<ind>assaut</ind></trd>,
    <trd>aborder<klr tip="ind"> (prendre d'assaut)</klr></trd>,
    <trd>accaparer</trd>,
    <trd>d&eacute;tourner<klr tip="ind"> (un avion)</klr></trd>,
    <trd>s'<ind>emparer</ind> (de)</trd>,
    <trd>mettre la main (sur)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79C1;&#x63A0;&#12377;&#x308B;  <pr>&#x308F;&#x305F;&#12375;&#x304B;&#12377;&#12377;&#x308B;</pr></trd>,
    <trd>&#25429;&#29554;&#12377;&#x308B;  <pr>&#x307B;&#x304B;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">kapen</trd>
  <trd lng="ru">&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">pir&aacute;tsky sa zmocni&tcaron; lode</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_p;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_k;&c_a;&c_p;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_m;
    &c_r;&c_o;&c_z;&c_b;&c_o;&c_jeu;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="kaper.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Persono a&ubreve; instanco, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_o;&c_p;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="en">
    <trd>hijacker</trd>,
    <trd>pirate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pirate de l'air</trd>,
    <trd>pirate</trd>,
    <trd>accapareur<klr tip="ind"> (subst.)</klr></trd>,
    <trd>pr&eacute;dateur<klr tip="ind"> (qui s'accapare qc)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elt&eacute;r&iacute;t&odblac;</trd>,
    <trd>kisaj&aacute;t&iacute;t&oacute;</trd>
  </trdgrp>
  <trd lng="nl">kaper</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_n;&c_shch;&c_i;&c_k;<klr tip="amb">
    (&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_a;, &c_s;&c_u;&c_d;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x6D77;&#x76D7; <pr>h&#x1CE;id&#xE0;o</pr></trd>
</drv>

<drv mrk="kaper.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kaper.0isto.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="dif" cel="korsar.0o.MAR_sxipisto">Korsaro<sncref/></ref>:
      <ekz>
        la <tld/>istoj deflanki&gcirc;is kaj veliris for
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_ib;&c_r;&c_a;&c_t;</trd>,
      <trd>&c_k;&c_a;&c_r;&c_s;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_e;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Pirat</trd>,
      <trd>Korsar</trd>,
      <trd>Seer&auml;uber</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>pirate</trd>,
      <trd>corsair</trd>,
      <trd>freebooter</trd>,
      <trd>buccaneer</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>corsaire</trd>,
      <trd>flibustier</trd>
    </trdgrp>
    <trd lng="hu">kal&oacute;z</trd>
    <trd lng="nl">zeerover</trd>
    <trd lng="pl">kaper</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_r;</trd>,
      <trd>&c_p;&c_i;&c_r;&c_a;&c_t;</trd>,
      <trd>&c_f;&c_l;&c_i;&c_b;&c_u;&c_s;&c_t;&c_mol;&c_je;&c_r;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_je;&c_r;</trd>
    </trdgrp>
    <trd lng="zh">&#x6D77;&#x76D7; <pr>h&#x1CE;id&#xE0;o</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>kaper</trd>,
    <trd>n&aacute;mo&rcaron;n&iacute; lupi&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79C1;&#x63A0;&#32773;  <pr>&#x308F;&#x305F;&#12375;&#x304B;&#12377;&#12375;&#12419;</pr></trd>,
    <trd>&#x79C1;&#x63A0;&#33337;&#x306E;&#33337;&#38263;&#x30FB;&#x4E57;&#x7D44;&#x54E1; 
    <pr>&#12375;&#x308A;&#12419;&#x304F;&#x305B;&#12435;&#x306E;&#x305B;&#12435;&#12385;&#12423;&#12358;&#x30FB;&#x306E;&#x308A;&#x304F;&#x307F;&#12356;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="sk">pir&aacute;t</trd>
  <trd lng="uk">&c_k;&c_a;&c_p;&c_je;&c_r; (&c_o;&c_s;&c_o;&c_b;&c_a;)</trd>
</drv>
</art>
