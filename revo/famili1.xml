<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>familiar</rad>/a
</kap>
<drv mrk="famili1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Intime senceremonia:
      <ekz><tld/>a esprimo, stilo, gesto, tono;</ekz>
      <ekz><tld/>aj manieroj<fnt>B</fnt>;</ekz>
      <ekz>li estas tro <tld/>a kun siaj superuloj.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_f;&c_a;&c_m;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="famili1.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Persono, kiu konstante k senceremonie interrilatas kun alia. 
    </dif>
    <refgrp tip="sin">
      <ref cel="intim.0a">intima</ref>,
      <ref cel="gxen.sen0a">sen&gcirc;ena</ref>.
    </refgrp>
  </snc>
</drv>
</art>

