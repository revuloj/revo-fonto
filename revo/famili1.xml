<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: famili1.xml,v 1.30 2025/03/29 16:45:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>familiar</rad>/a <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="famili1.0a">
  <kap><tld/>a</kap>
  <snc mrk="famili1.0a.MOR">
    <dif>
      Intime senceremonia:
      <ekz>
        ni trinku nun porfrati&gcirc;on, kaj ni estu pli <tld/>aj unu kun la alia
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne povas permesi, ke vi parolu al mi <tld/>e
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;al la diablo la aferojn!&rightquot; &ndash; li diris &scirc;an&gcirc;ante la &gcirc;entilan tonon al pli <tld/>a,
        pli kamarada
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        li parolis konfidence kaj patre per skota dialekto, kaj ofte metis la brakon
        &ccirc;irka&ubreve; la &scirc;ultron de virino, tiu ago ne estis tro <tld/>a a&ubreve; ofenda, kiel &gcirc;i
        estus, se aliulo farus &gcirc;in
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
    </dif>
    <ref tip="ant" cel="fremd.0a.aliula">fremda<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="ci.0">ci</ref>,
      <ref cel="intim.0a">intima</ref>,
      <ref cel="proksi.0a.FIG">proksima<sncref/></ref>,
      <ref cel="gxen.sen0a">sen&gcirc;ena</ref>
    </refgrp>
    <rim>
      La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>neformala</ke>, <ke>malformala</ke>, <ke>senceremonia</ke> , <ke>sen&#285;ena</ke>.
    </rim>    
    <trd lng="be">&c_f;&c_a;&c_m;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_n;&c_y;</trd>
    <trdgrp lng="ca">
      <trd>familiar</trd>,
      <trd>informal</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>d&uring;v&ecaron;rn&yacute;</trd>,
      <trd>intimn&iacute;</trd>,
      <trd>nenucen&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vertraulich <klr>(famili&auml;r)</klr></trd>,
      <trd>famili&auml;r</trd>,
      <trd>heimisch</trd>,
      <trd>br&uuml;derlich</trd>,
      <trd>per <ind>du</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>familiar</trd>,
      <trd>friendly</trd>,
      <trd>informal</trd>,
      <trd>intimate</trd>
    </trdgrp>
    <trd lng="es">familiar</trd>
    <trd lng="fr">familier<klr tip="ind"> (sans
    c&eacute;r&eacute;monie)</klr></trd>
    <trdgrp lng="hu">
      <trd>csal&aacute;dias</trd>,
      <trd>famili&aacute;ris</trd>
    </trdgrp>
    <trd lng="nl">familiair</trd>
    <trdgrp lng="pl">
      <trd>famialiarny</trd>,
      <trd>poufa&lstroke;y</trd>,
      <trd>za&zdot;y&lstroke;y</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>familiar</trd>,
      <trd>&iacute;ntimo</trd>
    </trdgrp>
    <trd lng="ru">&c_f;&c_a;&c_m;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
    <trdgrp lng="sk">
      <trd>d&ocirc;vern&yacute;</trd>,
      <trd>int&iacute;mny</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#20146;&#23494;&#22320; <pr>q&#299;nm&#236;de</pr></trd>,
      <trd>&#24357;&#21512; <pr>m&#237;h&#233;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="famili1.0a.kutima">
    <dif>
      <ref tip="sin" cel="kutim.0a">Kutima</ref>, konata
      al tiuj, kiuj apartenas al certa medio:
      <ekz>
        &leftquot;tie&rightquot; estis <tld/>a e&ubreve;femismo por Hadeso
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> specialigita vortaro <klr>[&#8230;]</klr> estas <tld/>a nur al tiuj,
        kiuj estas okupataj en la kampo
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_n;&c_a;&c_jo;&c_m;&c_y;</trd>,
      <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_v;&c_y;&c_k;&c_l;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>gel&auml;ufig</trd>,
      <trd>eing&auml;ngig</trd>
    </trdgrp>
  </snc>

  <trdgrp lng="ja">
    <trd>&#35242;&#12375;&#12356;  <pr>&#12385;&#12363;&#12375;&#12356;</pr></trd>,
    <trd>&#24931;&#12428;&#35242;&#12375;&#12435;&#12384;  <pr>&#12394;&#12428;&#12375;&#12383;&#12375;&#12435;&#12384;</pr></trd>,
    <trd>&#24515;&#23433;&#12356;  <pr>&#12371;&#12371;&#12429;&#12420;&#12377;&#12356;</pr></trd>,
    <trd>&#12367;&#12384;&#12369;&#12383;</trd>,
    <trd>&#24179;&#20439;&#12394;  <pr>&#12402;&#12425;&#12382;&#12367;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&#1084;&c_ib;&#1083;&#1100;&#1103;&c_r;&#1085;&c_i;&c_j;</trd>,
    <trd>&#1087;&c_a;&#1085;&c_ib;&c_b;&c_r;&c_a;&c_t;&c_s;&#1100;&#1082;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="famili1.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="famili1.0eco.MOR">
    <dif>
      <tld lit="F"/>a sinteno, atmosfero, rilato al iu a&ubreve; io:
      <ekz>
        preska&ubreve; infana mikso de deca respekto kaj facila <tld/>eco
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fremdulo dum &ccirc;i tiuj festoj forgesas la &gcirc;enon
        kaj la senkunulecon de sia fremdeco, &ccirc;ar li trovas
        &ccirc;e la homoj <tld/>econ kaj amemon
        <fnt>
          <aut>K. Gibran, trad. G. Abraham</aut>
          <vrk>Rakontoj kaj Poemoj</vrk>
          <lok><url ref="http://www.esperokatolika.org/ek19711975/ek1973_01.htm">
          Espero Katolika, 1973:1</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la lingvo donas al la homoj sekurecon kaj <tld/>ecan senton
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2013/010677.php">Lingva konfuzo kaj
          Konfuceo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_m;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Vertrautheit</trd>
  <trd lng="fr">familiarit&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#35242;&#12375;&#12415;  <pr>&#12375;&#12383;&#12375;&#12415;</pr></trd>,
    <trd>&#24515;&#23433;&#12373;  <pr>&#12371;&#12371;&#12429;&#12420;&#12377;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&#1084;&c_ib;&#1083;&#1100;&#1103;&c_r;&#1085;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&#1087;&c_a;&#1085;&c_ib;&c_b;&c_r;&c_a;&c_t;&c_s;&c_t;&c_v;&#1086;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_c;&c_je;&c_r;&c_je;&#1084;&#1086;&#1085;&#1085;&c_ib;&c_s;&c_t;&#1100; (&c_r;&c_i;&c_s;&c_a;
    &#1087;&#1086;&c_v;&c_je;&c_d;&c_ib;&#1085;&#1082;&c_i;)</trd>
  </trdgrp>
  <trd lng="zh">&#20146;&#26165; <pr>q&#299;nn&#236;</pr></trd>
</drv>

<drv mrk="famili1.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="famili1.0ulo.MOR">
    <dif>
      Persono, kiu konstante kaj senceremonie interrilatas kun alia:
      <ekz>
        estis agrabla restoracio, malhavanta &ccirc;ian <klr>[&#8230;]</klr> pretendemon, sed tute
        komforta, <klr>[&#8230;]</klr> ver&scirc;ajne <tld/>uloj tie &ccirc;i regule frandis hejmecajn
        kuira&jcirc;ojn
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;mi ne estas kuracisto kaj do parolas klare,&rightquot;
        &scirc;ercas <nom>Owsiak</nom>. &leftquot;mi parolas negramatike kaj
        do estas <tld/>ulo por avera&gcirc;a polo&rightquot;
        <fnt>
          <aut>Z. Banet-Fornalowa</aut>
          <vrk>Kiam orkestro fari&gcirc;as orkesto</vrk>
          <bib>Monato</bib>
          <lok>2000:05, p.12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="amik.0o.KOMUNE">amiko<sncref/></ref>,
       <ref cel="proksi.0ulo">proksimulo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_n;&c_a;&c_jo;&c_m;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_z;&c_n;&c_a;&c_jo;&c_m;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>company</trd>,
    <trd>col&middot;lega</trd>
  </trdgrp>
  <trd lng="cs">rodinn&yacute; p&rcaron;&iacute;slu&scaron;n&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Vertrauter</trd>,
    <trd>Angestammter</trd>,
    <trd>Dutzfreund</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>familier<klr> (subst.)</klr></trd>,
    <trd>proche<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&ouml;zvetlen ember</trd>
  <trdgrp lng="ja">
    <trd>&#12394;&#12376;&#12415;&#12398;&#20154;  <pr>&#12394;&#12376;&#12415;&#12398;&#12376;&#12435;</pr></trd>,
    <trd>&#24515;&#23433;&#12356;&#20154;  <pr>&#12371;&#12371;&#12429;&#12420;&#12377;&#12356;&#12376;&#12435;</pr></trd>,
    <trd>&#35242;&#21451;  <pr>&#12375;&#12435;&#12422;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="sk">rodinn&yacute; pr&iacute;slu&scaron;n&iacute;k</trd>
  <trd lng="zh">&#21451;&#22909; <pr>y&#466;uh&#462;o</pr></trd>
</drv>

<drv mrk="famili1.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="famili1.mal0a.io">
    <dif>
      Tute ne <tld/>a sed <ref tip="sin" cel="fremd.0a.aliula">fremda</ref>,
      <ref tip="sin" cel="strang.0a">stranga</ref>, nekutima:
      <ekz>
        imponas min la mistero de la mal<tld/>a medio
        <fnt>
          <aut>R. Dumain</aut>,
          <vrk><url ref="http://www.autodidactproject.org/my/durankulak.html">
          Durankulak [recenzo]</url></vrk>,
          <lok>2004-01-25</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="proksi.mal0a">malproksima</ref>
    <trd lng="de">fremdartig</trd>
    <trdgrp lng="fr">
      <trd>&eacute;tranger<klr> (adj., inhabituel)</klr></trd>,
      <trd>inhabituel</trd>
    </trdgrp>
    <trd lng="zh">&#24322;&#26679; <pr>y&#236;y&#224;ng</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_v;&c_y;&c_k;&c_l;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_n;&c_a;&c_jo;&c_m;&c_y;</trd>
  </trdgrp>
</drv>
</art>

