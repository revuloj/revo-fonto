<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Meksik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="meksik.M0o">
  <kap><tld/>o</kap>
  <snc mrk="meksik.M0o.urbo">
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <ref tip="dif" cel="meksik.M0urbo"><tld/>urbo</ref>
    <ekz>
      <klr>...</klr>por fari diferencon inter la urbo kaj la
      samnoma lando, mi konsilas fari tion, kion ni faris kun aliaj
      similcirkonstancaj landoj, t.e. nomi la urbon
      <ctl><tld/>o</ctl> kaj la landon <ctl><tld/>io</ctl>
      <fnt><bib>LR</bib>,
        <lok>Respondo 47a, Oficiala Gazeto, III, 1911, p. 291</lok>
      </fnt>;
    </ekz><ekz>
      ne forgesu ke <tld/>o estas urbo konstruita sur lago
      <fnt>
        <aut>Enrique</aut>: <vrk>Meksiko-Urbo</vrk>.
        <lok><url ref="http://take2001.8k.com/Take31.htm#mexu"
            >5a Tut-Amerika Kongreso de Esperanto, 2001</url></lok>
      </fnt>.
    </ekz>
    <trd lng="en">Mexico City</trd>
    <trd lng="fr">Mexico</trd>
    <trd lng="ru">&c_M;&c_je;&c_h;&c_i;&c_k;&c_o;</trd>
  </snc>
  <snc mrk="meksik.M0o.lando">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>, <lok>numero 12</lok>
    </fnt>
    <ref tip="dif" cel="meksik.M0lando"><tld/>lando</ref>
    <trd lng="bg">&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;</trd>
    <trd lng="de">Mexiko</trd>
    <trd lng="en">Mexico</trd>
    <trd lng="fr">Mexique</trd>
    <trdgrp lng="es">
      <trd>M&eacute;xico</trd>,
      <trd>M&eacute;jico</trd>,
      <trd>Estados Unidos Mexicanos</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Meksyk</trd>,
      <trd>Meksyka&nacute;skie Stany Zjednoczone</trd>
    </trdgrp>
    <trd lng="pt">M&eacute;xico</trd>
    <trd lng="ru">&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="meksik.M0o.provinco">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco (sub&scirc;tato) de <tld/>lando.
    </dif>
    <trd lng="en">Mexico State</trd>
    <trd lng="ru"><klr>&c_sh;&c_t;&c_a;&c_t; </klr><ind>&c_M;&c_je;&c_h;&c_i;&c_k;&c_o;</ind></trd>
  </snc>
  <trd lng="ca">M&egrave;xic</trd>
  <trd lng="sk">Mexiko <klr>(mesto)</klr></trd>
</drv>

<drv mrk="meksik.M0urbo">
  <kap><tld/>urbo</kap>
  <snc mrk="meksik.M0urbo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      La <ref tip="lst" cel="urb.cxef0o"
      lst="voko:&ccirc;efurboj">&ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="meksik.M0lando"><tld/>lando</ref> 
      (99&#730;07'U, 19&#730;26'N).
    </dif>
  </snc>
  <trd lng="ca">Ciutat de M&egrave;xic</trd>
  <trd lng="en">Mexico City</trd>
  <trd lng="fr">Mexico</trd>
  <trd lng="id">Kota Meksiko</trd>
  <trd lng="ru">&c_M;&c_je;&c_h;&c_i;&c_k;&c_o;</trd>
  <trd lng="sk">Mexiko <klr>(mesto)</klr></trd>
</drv>

<drv mrk="meksik.M0lando">
  <kap><tld/>lando <fnt><bib>EdE</bib></fnt></kap>
  <mlg kod="IS-3166">MX</mlg>
  <snc mrk="meksik.M0lando.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o" lst="voko:&scirc;tatoj">Lando</ref> 
      en la centra kaj norda <ref tip="malprt" 
        cel="amerik.A0o">Amerikoj</ref>,
      kies &ccirc;efurbo estas <tld/>urbo;
      la teritorio de la Meksikaj Unui&gcirc;intaj &Scirc;tatoj,
      limanta kun
      <ref tip="vid" cel="beliz.B0o.GEOG">Belizo</ref>,
      <ref tip="vid" cel="gvatemal.G0o.GEOG">Gvatemalo</ref>,
      <ref tip="vid" cel="uson.U0o">Usono</ref>,
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref> (Kariba Maro
      kaj Meksika Golfo)
      kaj <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>:
      <ekz>
        la vivo pulsas ankora&ubreve; en tiu parto, en kiu la a&ubreve;toro nin
        kondukas en <tld/>landon <klr>...</klr>
        <fnt>
          <lok>Literatura mondo,
            <url
              ref="http://www.tekstoj.nl/lm/lm22-3/bibliografio.html#luyken"
              >Dec. 1922, p. 67</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;</trd>
  <trd lng="ca">M&egrave;xic</trd>
  <trd lng="de">Mexiko</trd>
  <trd lng="en">Mexico</trd>
  <trd lng="fr">Mexique</trd>
  <trdgrp lng="es">
    <trd>M&eacute;xico</trd>,
    <trd>M&eacute;jico</trd>,
    <trd>Estados Unidos Mexicanos</trd>
  </trdgrp>
  <trd lng="id">Meksiko</trd>
  <trdgrp lng="pl">
    <trd>Meksyk</trd>,
    <trd>Meksyka&nacute;skie Stany Zjednoczone</trd>
  </trdgrp>
  <trd lng="ru">&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="meksik.nov0o">
  <kap>Nov<tld lit="m"/>o</kap>
  <snc mrk="meksik.nov0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la suda parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>;
      <ref tip="sin" cel="meksiki.NovM0o.GEOG">Novmeksikio</ref>:
      <ekz>
        la&ubreve; raporto de CNN <klr>...</klr> la plej dan&gcirc;eraj 
        usonaj &scirc;tatoj estas Nevado, Nov-<tld/>o kaj Arizono
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Aprilo_2007.html">
          taglibro</url></vrk>,
          <lok>2007-04-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Nou <ind>M&egrave;xic</ind></trd>
  <trd lng="en">New Mexico</trd>
  <trd lng="fr">Nouveau Mexique</trd>
  <trd lng="nl">Nieuw-Mexico</trd>
  <trd lng="pl">Nowy Meksyk</trd>
  <trd lng="ru">&c_N;&c_mol;&c_ju;-&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;</trd>
</drv>
</art>

