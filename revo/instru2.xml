<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>instrument</rad>/o
</kap>
<drv mrk="instru2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fabrikita objekto, speciale destinita por fari ian agon a&ubreve; laboron:
      <ekz>&hcirc;irurgiaj, terkulturaj, muzikaj <tld/>oj.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aparat.0o">aparato</ref>,
      <ref cel="masxin.0o">ma&scirc;ino</ref>,
      <ref cel="il">ilo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">n&aacute;stroj</trd>
  <trd lng="de">Instrument</trd>
  <trd lng="nl">instrument</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_o;&c_r;&c_u;&c_d;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="instru2.muzik0o">
  <kap>muzik<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Fabrikita objekto, speciale destinita por fari muzikon. Muzikilo.
    </dif>
  </snc>
  <trd lng="nl">muziekinstrument</trd>
</drv>
<drv mrk="instru2.blov0o">
  <kap>blov<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en kiu sonoro estas produktata per blovado.
    </dif>
  </snc>
  <trd lng="nl">blaasinstrument</trd>
</drv>
<drv mrk="instru2.kord0o">
  <kap>kord<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en kiu sonoro estas produktata per vibrado de kordoj.
    </dif>
  </snc>
  <trd lng="nl">snaarinstrument</trd>
</drv>
<drv mrk="instru2.frap0o">
  <kap>frap<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en kiu sonoro estas produktata per frapado.
    </dif>
  </snc>
  <trd lng="nl">slaginstrument</trd>
</drv>
</art>
