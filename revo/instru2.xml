<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>instrument</rad>/o
</kap>
<drv mrk="instru2.0o">
  <kap><tld/>o</kap>
  <snc mrk="instru2.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fabrikita objekto, speciale destinita por fari ian agon a&ubreve; laboron:
      <ekz>
        &hcirc;irurgiaj, terkulturaj, muzikaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aparat.0o">aparato</ref>,
      <ref cel="masxin.0o">ma&scirc;ino</ref>
    </refgrp>
  </snc>
  <trd lng="cs">n&aacute;stroj</trd>
  <trdgrp lng="de">
    <trd>Instrument</trd>,
    <trd>Werkzeug</trd>,
    <trd>Ger&auml;t</trd>
  </trdgrp>
  <trd lng="es">instrumento</trd>
  <trdgrp lng="fr">
    <trd>instrument</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szersz&aacute;m</trd>,
    <trd>eszk&ouml;z</trd>,
    <trd>m&udblac;szer</trd>
  </trdgrp>
  <trd lng="nl">instrument</trd>
  <trd lng="pt">instrumento</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_o;&c_r;&c_u;&c_d;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="instru2.0isto">
  <kap><tld/>isto</kap>
  <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="instru2.0isto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>
      uzanta son-instrumenton.
    </dif>
  </snc>
  <trd lng="de">Musikinstrumentspieler</trd>
  <trdgrp lng="fr">
    <trd>instrumentiste<klr tip="ind"> (musique)</klr></trd>
  </trdgrp>
  <trd lng="hu">hangszeres zen&eacute;sz</trd>
  <trd lng="pt">instrumentista<klr> (m&uacute;sica)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="instru2.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="instru2.muzik0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      Fabrikita objekto, speciale destinita por fari muzikon.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="muzik.0ilo">muzikilo</ref>
  </refgrp>
  <trd lng="de">Musikinstrument</trd>
  <trd lng="es"><ind>instrumento</ind> musical</trd>
  <trdgrp lng="fr">
    <trd>instrument de musique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hangszer</trd>,
    <trd>zeneszersz&aacute;m</trd>
  </trdgrp>
  <trd lng="nl">muziekinstrument</trd>
  <trd lng="pt"><ind>instrumento</ind> musical</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

<drv mrk="instru2.blov0o">
  <kap>blov<tld/>o</kap>
  <snc mrk="instru2.blov0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en kiu
      sono estas produktata per blovado.
    </dif>
  </snc>
  <trd lng="de">Blasinstrument</trd>
  <trd lng="es"><ind>instrumento</ind> de viento</trd>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;) </klr>vent</trd>
  <trd lng="hu">f&uacute;v&oacute;s <ind>hangszer</ind></trd>
  <trd lng="nl">blaasinstrument</trd>
  <trd lng="pt"><ind>instrumento</ind> de sopro</trd>
  <trd lng="ru">&c_d;&c_u;&c_h;&c_o;&c_v;&c_o;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

<drv mrk="instru2.kord0o">
  <kap>kord<tld/>o</kap>
  <snc mrk="instru2.kord0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en kiu
      sono estas produktata per vibrado de kordoj.
    </dif>
  </snc>
  <trd lng="de">Saiteninstrument</trd>
  <trd lng="es"><ind>instrumento</ind> de cuerdas</trd>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;) </klr>cordes</trd>
  <trd lng="hu">h&uacute;ros <ind>hangszer</ind></trd>
  <trd lng="nl">snaarinstrument</trd>
  <trd lng="pt"><ind>instrumento</ind> de cordas</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_n;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

<drv mrk="instru2.frap0o">
  <kap>frap<tld/>o</kap>
  <snc mrk="instru2.frap0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en kiu
      sono estas produktata per frapado.
    </dif>
  </snc>
  <trd lng="de">Schlaginstrument</trd>
  <trd lng="es"><ind>instrumento</ind> de percusi&oacute;n</trd>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;) </klr>percussion</trd>
  <trd lng="hu">&uuml;t&odblac;hangszer</trd>
  <trd lng="nl">slaginstrument</trd>
  <trd lng="pt"><ind>instrumento</ind> de percuss&atilde;o</trd>
  <trd lng="ru">&c_u;&c_d;&c_a;&c_r;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
