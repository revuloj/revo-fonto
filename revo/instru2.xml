<drv mrk="instru2.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="instru2.0isto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <fnt><bib>MuzTerm</bib></fnt>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>
      uzanta son-<tld/>on.
    </dif>
    <trd lng="fr">instrumentiste<klr tip="ind"> (musique)</klr></trd>
  </snc>
  <trd lng="ca">instrumentista</trd>
  <trd lng="cs">instrument&aacute;&rcaron;</trd>
  <trd lng="de">Musikinstrumentspieler</trd>
  <trd lng="en">instrumentalist</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_z;&a_z;&a_n;</trd>,
    <trd>&a_n;&a_w;&a_A;&a_z;&a_n;&a_d;&a_h;</trd>
  </trdgrp>
  <trd lng="hu">hangszeres zen&eacute;sz</trd>
  <trd lng="id">instrumentalis</trd>
  <trd lng="it">strumentista<klr> (musicista)</klr></trd>
  <trd lng="pl">instrumentalista</trd>
  <trd lng="pt">instrumentista<klr> (m&uacute;sica)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">in&scaron;trument&aacute;r</trd>
</drv>

<drv mrk="instru2.arcx0o">
  <kap>ar&ccirc;<tld/>o</kap>
  <snc mrk="instru2.arcx0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.kord0o.MUZ">Kord<tld/>o</ref>
      kiun oni ordinare sonigas per
      <ref tip="vid" cel="arcx.0o.MUZ">ar&ccirc;o</ref>:
      <ekz>
        ili pagis ciganajn muzikistojn, kiuj ludis la <klr>...</klr> 
        ar&ccirc;<tld/>ojn, foje kompletigite kun klarneto
        <fnt>
          <aut>P. Kov&aacute;cs</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/kovacspeter/45146">
          Pri hungaraj popolaj muzikinstrumentoj</url></vrk>,
          <lok>2008-03-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>instrument &agrave; archet</trd>,
    <trd>instrument &agrave; <ind>cordes frott&eacute;es</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>alat</ind> musik gesek</trd>
</drv>

<drv mrk="instru2.blov0o">
  <kap>blov<tld/>o</kap>
  <snc mrk="instru2.blov0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en
      kiu  sono estas produktata per blovado.
    </dif>
    <lstref lst="voko:blovinstrumentoj">blov<tld/>oj</lstref>
  </snc>
  <trd lng="ca">instrument de vent</trd>
  <trd lng="cs">dechov&yacute; n&aacute;stroj</trd>
  <trd lng="de">Blasinstrument</trd>
  <trd lng="en">wind instrument</trd>
  <trd lng="es"><ind>instrumento</ind> de viento</trd>
  <trd lng="fa">&a_s;&a_A;&a_z; &a_b;&a_A;&a_d;&f_y;</trd>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;)
  </klr>vent</trd>
  <trd lng="hu">f&uacute;v&oacute;s <ind>hangszer</ind></trd>
  <trd lng="id"><ind>alat</ind> musik tiup</trd>
  <trd lng="it"><ind>strumento</ind> a fiato</trd>
  <trd lng="nl">blaasinstrument</trd>
  <trd lng="pl">instrument d&eogonek;ty</trd>
  <trd lng="pt"><ind>instrumento</ind> de sopro</trd>
  <trd lng="ru">&c_d;&c_u;&c_h;&c_o;&c_v;&c_o;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">dychov&yacute; hud. n&aacute;stroj</trd>
</drv>

<drv mrk="instru2.frap0o">
  <kap>frap<tld/>o</kap>
  <snc mrk="instru2.frap0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en
      kiu  sono estas produktata per frapado.
    </dif>
    <lstref lst="voko:frapinstrumentoj">frap<tld/>oj</lstref>
  </snc>
  <trd lng="ca">instrument de percussi&oacute;</trd>
  <trd lng="cs">bic&iacute; n&aacute;stroj</trd>
  <trd lng="de">Schlaginstrument</trd>
  <trd lng="en">percussion instrument</trd>
  <trd lng="es"><ind>instrumento</ind> de percusi&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_z; &a_D;&a_r;&a_b;&f_y;</trd>,
    <trd>&a_s;&a_A;&a_z; &f_k;&a_w;&a_b;&a_h;&zwnj;&a_A;&f_y;</trd>
  </trdgrp>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;)
  </klr>percussion</trd>
  <trd lng="hu">&uuml;t&odblac;hangszer</trd>
  <trd lng="id"><ind>alat</ind> musik pukul</trd>
  <trd lng="it"><ind>strumento</ind> a percussione</trd>
  <trd lng="nl">slaginstrument</trd>
  <trd lng="pl">instrument perkusyjny</trd>
  <trd lng="pt"><ind>instrumento</ind> de percuss&atilde;o</trd>
  <trd lng="ru">&c_u;&c_d;&c_a;&c_r;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">bic&iacute; n&aacute;stroj</trd>
</drv>

<drv mrk="instru2.kord0o">
  <kap>kord<tld/>o</kap>
  <snc mrk="instru2.kord0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en
      kiu sono estas produktata per vibrado de kordoj.
    </dif>
    <lstref lst="voko:kordinstrumentoj">kord<tld/>oj</lstref>
  </snc>
  <trd lng="ca">instrument de corda</trd>
  <trd lng="cs">strunn&yacute; n&aacute;stroj</trd>
  <trd lng="de">Saiteninstrument</trd>
  <trd lng="en">string instrument</trd>
  <trd lng="es"><ind>instrumento</ind> de cuerdas</trd>
  <trd lng="fa">&a_s;&a_A;&a_z; &a_z;&a_h;&f_y;</trd>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;)
  </klr>cordes</trd>
  <trd lng="hu">h&uacute;ros <ind>hangszer</ind></trd>
  <trdgrp lng="id">
    <trd><ind>alat</ind> musik gesek</trd>,
    <trd><ind>alat</ind> musik petik</trd>
  </trdgrp>
  <trd lng="it"><ind>strumento</ind> a corda</trd>
  <trd lng="nl">snaarinstrument</trd>
  <trd lng="pl">instrument strunowy</trd>
  <trd lng="pt"><ind>instrumento</ind> de cordas</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_n;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">strunov&yacute; n&aacute;stroj</trd>
</drv>

<drv mrk="instru2.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="instru2.muzik0o.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Fabrikita objekto, speciale destinita por fari muzikon;
      <ref tip="sin" cel="muzik.0ilo.muzikinstrumento">muzikilo</ref>.
    </dif>
  </snc>
  <trd lng="ca">instrument musical</trd>
  <trd lng="cs">hudebn&iacute; n&aacute;stroj</trd>
  <trd lng="de">Musikinstrument</trd>
  <trd lng="en">musical instrument</trd>
  <trd lng="es"><ind>instrumento</ind> musical</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_z;</trd>,
    <trd>&a_A;&a_b;&a_z;&a_A;&a_r; &a_m;&a_w;&a_s;&f_y;&a_q;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">instrument de musique</trd>
  <trdgrp lng="hu">
    <trd>hangszer</trd>,
    <trd>zeneszersz&aacute;m</trd>
  </trdgrp>
  <trd lng="id"><ind>alat</ind> musik</trd>
  <trd lng="it"><ind>strumento</ind> musicale</trd>
  <trd lng="nl">muziekinstrument</trd>
  <trd lng="pl">instrument muzyczny</trd>
  <trd lng="pt"><ind>instrumento</ind> musical</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">hudobn&yacute; n&aacute;stroj</trd>
</drv>
</art>
