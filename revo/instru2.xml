  <trd lng="fa">&a_s;&a_A;&a_z; &a_b;&a_A;&a_d;&f_y;</trd>
  <trd lng="fr"><klr tip="amb">(instrument de musique &agrave;)
  </klr>vent</trd>
  <trd lng="hu">f&uacute;v&oacute;s <ind>hangszer</ind></trd>
  <trd lng="id"><ind>alat</ind> musik tiup</trd>
  <trd lng="it"><ind>strumento</ind> a fiato</trd>
