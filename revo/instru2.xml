<drv mrk="instru2.arcx0o">
  <kap>ar&ccirc;<tld/>o</kap>
  <snc mrk="instru2.arcx0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.kord0o.MUZ">Kord<tld/>o</ref>
      kiun oni ordinare sonigas per
      <ref tip="vid" cel="arcx.0o.MUZ">ar&ccirc;o</ref>:
      <ekz>
        ili pagis ciganajn muzikistojn, kiuj ludis la <klr>[&#8230;]</klr>
        ar&ccirc;<tld/>ojn, foje kompletigite kun klarneto
        <fnt>
          <aut>P. Kov&aacute;cs</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/kovacspeter/45146">
          Pri hungaraj popolaj muzikinstrumentoj</url></vrk>,
          <lok>2008-03-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_y;&c_ch;&c_k;&c_o;&c_v;&c_y; <ind>&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;</ind></trd>
  <trd lng="de">Streichinstrument</trd>
  <trdgrp lng="fr">
    <trd>instrument &agrave; archet</trd>,
    <trd>instrument &agrave; <ind>cordes frott&eacute;es</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>alat</ind> musik gesek</trd>
  <trd lng="uk">&c_s;&c_m;&c_i;&c_ch;&c_k;&c_o;&c_v;&c_i;&c_j; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="zh">&#24358;&#20048;&#22120; <pr>xi&aacute;nyu&egrave;q&igrave;</pr></trd>
</drv>

<drv mrk="instru2.blov0o">
  <kap>blov<tld/>o</kap>
  <snc mrk="instru2.blov0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.muzik0o">Muzikinstrumento</ref> en
      kiu  sono estas produktata per blovado:
      <ekz>
        oni tie havis<klr>[&#8230;]</klr> e&ccirc; ne ian malgrandan balon de ursoj, &ccirc;e kiu la
        blov<tld/>ojn povus ludi la ventego
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        poluritaj blov<tld/>oj brilas kiel oro kaj eligas valson, kiu povus veki
        mortintojn
        <fnt>
          <bib>Tor</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        simplaj kanzonoj, ludataj per akordiono kaj blov<tld/>oj akompanataj de
        <klr>[&#8230;]</klr> vo&ccirc;o, akustika gitaro, ban&gcirc;o kaj foja drumo
        <fnt>
          <bib>Kon11</bib>
          <lok>Recenzo</lok>
        </fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:blovinstrumentoj">blov<tld/>oj</lstref>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_h;&c_o;&c_v;&c_y; <ind>&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;</ind></trd>
  <trd lng="ca">instrument de vent</trd>
  <trd lng="cs">dechov&yacute; n&aacute;stroj</trd>
  <trd lng="de">Blasinstrument</trd>
  <trd lng="en">wind instrument</trd>
