<drv mrk="instru2.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="instru2.muzik0o.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="I"/>o destinita por fari muzikon;
      <ref tip="sin" cel="muzik.0ilo.muzikinstrumento">muzikilo</ref>:
      <ekz>
        interesis lin nove elpensitaj muzik<tld/>oj
        <fnt>
          <bib>QuV</bib>
          <lok>Epilogo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Post la konferenco <klr>[&#8230;]</klr> estis prezentitaj ukraina folkloro
        <klr>[&#8230;]</klr>, ukrainiaj tradiciaj muzik<tld/>oj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;u iom fu&scirc;a? Tamen bonetosa!</lok>
        </fnt>;
      </ekz>
      <ekz>
        amantoj de folkloro fondis <klr>[&#8230;]</klr> muzeon pri malnovaj popolaj
        muzik<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008666.php"
              >Folklora festivalo en Kaustinen</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; <ind>&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;</ind></trd>
