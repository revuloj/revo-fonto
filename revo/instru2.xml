<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>instrument</rad>/o
</kap>
<drv mrk="instru2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fabrikata objekto, speciale destinita por fari ian agon a&ubreve; laboron:
      <ekz>&hcirc;irurgiaj, terkulturaj, muzikaj <tld/>oj;</ekz>
      <ekz>
        blov<tld/>o
        <klr>(muzikilo, en kiu sonoro estas produktata per blovado)</klr>;
      </ekz>
      <ekz>
        kord<tld/>o
        <klr>(muzikilo, en kiu sonoro estas produktata per vibrado
        de kordoj)</klr>;
      </ekz>
      <ekz>
        frap<tld/>o
        <klr>(muzikilo, en kiu sonoro estas produktata per frapado)</klr>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="aparat.0o">aparato</ref>,
      <ref cel="masxin.0o">ma&scirc;ino</ref>,
      <ref cel="il">ilo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">n&aacute;stroj</trd>
  <trd lng="de">Instrument</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_o;&c_r;&c_u;&c_d;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
