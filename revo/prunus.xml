<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prunus</rad>/o
</kap>
<drv mrk="prunus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Prunus</trd>), kun multaj kulturataj specioj kaj
      varia&jcirc;oj, ekz.:
      <refgrp tip="sub">
        <ref cel="cxeriz.0ujo">&ccirc;erizujo</ref>,
        <ref cel="migdal.0arbo">migdalarbo</ref>,
        <ref cel="persik.0ujo0arbo">persikujo</ref>,
        <ref cel="prun.0ujo0arbo">prunujo</ref>,
        <ref cel="prunel.0arbo">prunelarbo</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="de">Prunus</trd>
  <trd lng="hu">szilva<klr tip="amb"> (nemzets&eacute;g)</klr></trd>
  <trd lng="nl">prunus</trd>
  <trdgrp lng="pt">
    <trd>ameixeira</trd>,  
    <trd>ameixa</trd>,
    <trd>ameixieira</trd>,    
    <trd>am&ecirc;ixoa</trd>,
    <trd>ameixoeira</trd>    
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_v;&c_a; <klr
tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
