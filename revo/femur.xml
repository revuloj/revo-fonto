    <trd lng="kn">&#3236;&#3274;&#3233;&#3270;</trd>
    <trd lng="ko">&#50633;&#45929;&#51060;</trd>
    <trd lng="la">femur</trd>
    <trd lng="mk">&c_b;&c_u;&c_t;</trd>
    <trd lng="ml">&#3364;&#3393;&#3359;</trd>
    <trd lng="mr">&#2350;&#2366;&#2306;&#2337;&#2368;</trd>
    <trdgrp lng="nl">
      <trd>dij</trd>,
      <trd>dijbeen</trd>
    </trdgrp>
    <trd lng="no">l&aring;r</trd>
    <trd lng="pl">udo</trd>
    <trd lng="pt">coxa</trd>
    <trd lng="qu">mama chanka</trd>
    <trd lng="ro">coaps&abreve;</trd>
    <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
    <trd lng="scn">coscia</trd>
    <trd lng="sk">stehno</trd>
    <trd lng="sl">stegno</trd>
    <trd lng="sv">l&aring;r</trd>
    <trd lng="te">&#3108;&#3146;&#3105;</trd>
    <trd lng="tl">hita</trd>
    <trd lng="tr">uyluk</trd>
    <trd lng="ug">&a_s;&a_A;&a_n;</trd>
    <trd lng="yi">&pe;&vav;&lamed;&qof;&ayin;</trd>
    <trd lng="zh">&#22823;&#33151;</trd>
  </snc>
  <snc mrk="femur.0o.artikula">
    <dif>
      <klr>(&ccirc;e artikuloj: insektoj, araneuloj k.a.)</klr>:
      Ordinare plej longa parto de la membro inter la kokso
      (a&ubreve; anta&ubreve;<tld/>o) kaj tibio.
    </dif>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_o;</trd>
  </snc>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_g;&c_n;&c_o;</trd>
</drv>
