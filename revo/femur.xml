<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: femur.xml,v 1.25 2021/11/25 07:27:22 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>femur</rad>/o
</kap>
<drv mrk="femur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <klr>(&ccirc;e surteraj verteruloj)</klr> Supra parto de la malanta&ubreve;a respektive malsupraj membroj, 
      de <ref cel="genu.0o.ANA">genuo</ref> &gcirc;is <ref cel="koks.0o">kokso</ref>:
      <ekz>
        metu vian manon sub mian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de <nom>Jakob</nom> elartiki&gcirc;is <klr>[&#8230;]</klr> kaj li lamis per sia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:25,31</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu sidi&gcirc;is sur lia nuko, alia sur la <tld/>oj kaj du aliaj kalkulis sur lia
        nuda korpo kvindek flekseblajn kanojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="ingven.0o">ingveno</ref> estas la enprofundi&gcirc;o inter la <tld/>o kaj ventro
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>osto</ind>.
        <trd lng="es">f&eacute;mur</trd>
        <trd lng="fr">f&eacute;mur</trd>
        <trd lng="hu">combcsont</trd>
        <trd lng="it">femore</trd>
        <trd lng="pt">f&ecirc;mur</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o">kruro</ref>,
      <ref cel="gamb.0o">gambo</ref>,
      <ref cel="tibi.0o">tibio</ref>,
      <ref cel="sur1.0o">suro</ref>.
    </refgrp>  
    <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_o;</trd>
    <trd lng="bg">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
    <trd lng="bo">&t_ba;&t_ra;&t_lata;&t_tsheg;&t_sha;&t_tsheg;</trd>
    <trdgrp lng="cs">
      <trd>femur</trd>,
      <trd>kost stehenn&iacute;</trd>,
      <trd>stehno</trd>
    </trdgrp>
    <trd lng="de">Oberschenkel</trd>
    <trdgrp lng="en">
      <trd>upper leg</trd>,
      <trd>thigh</trd>,
      <trd>femur</trd>
    </trdgrp>
    <trd lng="es">muslo</trd>
    <trd lng="fr">cuisse</trd>
    <trd lng="hu">comb</trd>
    <trd lng="id">paha</trd>
    <trd lng="it">coscia</trd>
    <trdgrp lng="nl">
      <trd>dij</trd>,
      <trd>dijbeen</trd>
    </trdgrp>
    <trd lng="pl">udo</trd>
    <trd lng="pt">coxa</trd>
    <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
    <trd lng="sk">stehno</trd>
    <trd lng="tr">uyluk</trd>
  </snc>
  <snc mrk="femur.0o.artikula">
    <dif>
      <klr>(&ccirc;e artikuloj: insektoj, araneuloj k.a.)</klr>:
      Ordinare plej longa parto de la membro inter la kokso
      (a&ubreve; anta&ubreve;<tld/>o) kaj tibio.
    </dif>
    <trd lng="de">Femur</trd>
  </snc>
</drv>
