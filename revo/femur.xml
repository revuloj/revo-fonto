    <trd lng="eu">izter</trd>
    <trd lng="fa">&a_r;&a_A;&a_n;</trd>
    <trd lng="fi">reisi</trd>
    <trd lng="fj">saga</trd>
    <trd lng="fo">l&aelig;r</trd>
    <trd lng="fr">cuisse</trd>
    <trd lng="gn">birdi</trd>
    <trd lng="ha">thai-th&uacute;i</trd>
    <trd lng="he">&yod;&resh;&fkaf;</trd>
    <trd lng="hr">butina</trd>
    <trd lng="ht">kwis</trd>
    <trd lng="hu">comb</trd>
    <trd lng="hy">&#1378;&#1400;&#1410;&#1380;</trd>
    <trd lng="id">paha</trd>
    <trd lng="is">l&aelig;ri</trd>
    <trd lng="it">coscia</trd>
    <trd lng="ja">&#22826;&#32929; <pr>&#12405;&#12392;&#12418;&#12418;</pr></trd>
