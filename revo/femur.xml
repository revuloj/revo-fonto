<kap>
<ofc>*</ofc>
<rad>femur</rad>/o
</kap>
<drv mrk="femur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="femur.0o.vertebrula">
    <dif>
      <klr>(&ccirc;e surteraj vertebruloj)</klr> Supra parto de la malanta&ubreve;a respektive malsupraj membroj,

      de <ref cel="genu.0o.ANA">genuo</ref> &gcirc;is <ref cel="koks.0o">kokso</ref>:
      <ekz>
        metu vian manon sub mian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de <nom>Jakob</nom> elartiki&gcirc;is <klr>[&#8230;]</klr> kaj li lamis per sia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:25,31</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu sidi&gcirc;is sur lia nuko, alia sur la <tld/>oj kaj du aliaj kalkulis sur lia
        nuda korpo kvindek flekseblajn kanojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="ingven.0o">ingveno</ref> estas la enprofundi&gcirc;o inter la <tld/>o kaj ventro
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>osto</ind>.
        <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_c;&c_a;</trd>
        <trd lng="ca">f&egrave;mur</trd>
        <trd lng="cs">stehenn&iacute; kost</trd>
        <trd lng="da">l&aring;rbensknogle</trd>
        <trdgrp lng="de">
           <trd>Femur</trd>,
           <trd>Oberschenkelknochen</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>thighbone</trd>,
          <trd>femur</trd>
        </trdgrp>
        <trd lng="es">f&eacute;mur</trd>
        <trd lng="eu">izterrezur</trd>
        <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n; &a_r;&a_A;&a_n;</trd>
        <trd lng="fi">reisiluu</trd>
        <trd lng="fr">f&eacute;mur</trd>
        <trd lng="gl">f&eacute;mur</trd>
        <trd lng="he">&ayin;&tsadi;&fmem; &he;&yod;&resh;&fkaf;</trd>
        <trd lng="hr">bedrena kost</trd>
        <trd lng="hu">combcsont</trd>
        <trd lng="hy">&#1377;&#1382;&#1380;&#1408;&#1400;&#1405;&#1391;&#1408;</trd>
        <trd lng="id">tulang paha</trd>
        <trd lng="is">l&aelig;rleggur</trd>
        <trd lng="it">femore</trd>
        <trd lng="ja">&#22823;&#33151;&#39592; <pr>&#12384;&#12356;&#12383;&#12356;&#12371;&#12388;</pr></trd>
        <trd lng="ka">&#4305;&#4304;&#4320;&#4331;&#4304;&#4327;&#4312;&#4321; &#4331;&#4309;&#4304;&#4314;&#4312;</trd>
        <trd lng="ko">&#45824;&#53748;&#44264;</trd>
        <trd lng="la">femur</trd>
        <trd lng="lt">&scaron;launikaulis</trd>
        <trd lng="lv">aug&scaron;stilba kauls</trd>
        <trd lng="mi">p&uuml;kaka</trd>
        <trd lng="nl">dijbeen</trd>
        <trd lng="no">femur</trd>
        <trd lng="oc">femur</trd>
        <trd lng="pl">ko&sacute;&cacute; udowa</trd>
        <trd lng="pt">f&ecirc;mur</trd>
        <trd lng="ro">femur</trd>
        <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
        <trd lng="sh">bedrena kost</trd>
        <trd lng="sl">stegnenica</trd>
        <trd lng="sq">femuri i njeriut</trd>
        <trd lng="sv">l&aring;rben</trd>
        <trd lng="syc">&#1835;&#1833;&#1808;</trd>
        <trd lng="ta">&#2980;&#3018;&#2975;&#3016;&#2991;&#3014;&#2994;&#3009;&#2990;&#3021;&#2986;&#3009;</trd>
        <trd lng="te">&#3108;&#3137;&#3074;&#3103;&#3135; &#3086;&#3118;&#3137;&#3093;</trd>
        <trd lng="th">&#3585;&#3619;&#3632;&#3604;&#3641;&#3585;&#3605;&#3657;&#3609;&#3586;&#3634;</trd>
        <trdgrp lng="tr">
          <trd>femur</trd>,
          <trd>kal&ccedil;a kemi&#240;i</trd>
        </trdgrp>
        <trd lng="zh">&#32929;&#39592;</trd>

      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o">kruro</ref>,
      <ref cel="gamb.0o">gambo</ref>,
      <ref cel="tibi.0o">tibio</ref>,
      <ref cel="sur1.0o">suro</ref>.
    </refgrp>
    <trd lng="af">dy</trd>
    <trd lng="an">cuixa</trd>
    <trd lng="ar">&a_f;&a_kh;&a_dh;</trd>
    <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_o;</trd>
    <trd lng="bg">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
    <trd lng="bo">&t_ba;&t_ra;&t_lata;&t_tsheg;&t_sha;&t_tsheg;</trd>
    <trd lng="ca">cuixa</trd>
    <trdgrp lng="cs">
      <trd>femur</trd>,
