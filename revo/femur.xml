<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>femur</rad>/o
</kap>
<drv mrk="femur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la homa malsupra a&ubreve; besta malanta&ubreve;a membro, 
      de genuo &gcirc;is kokso:
      <ekz>
        <ind><tld/>osto</ind>.
        <trd lng="es">f&eacute;mur</trd>
        <trd lng="fr">f&eacute;mur</trd>
        <trd lng="hu">combcsont</trd>
        <trd lng="it">femore</trd>
        <trd lng="pt">f&ecirc;mur</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o">kruro</ref>,
      <ref cel="gamb.0o">gambo</ref>,
      <ref cel="tibi.0o">tibio</ref>,
      <ref cel="sur1.0o">suro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_o;</trd>
  <trd lng="bg">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
  <trd lng="de">Oberschenkel</trd>
  <trdgrp lng="en">
    <trd>upper leg</trd>,
    <trd>thigh</trd>,
    <trd>femur</trd>
  </trdgrp>
  <trd lng="es">muslo</trd>
  <trd lng="fr">cuisse</trd>
  <trd lng="hu">comb</trd>
  <trd lng="id">paha</trd>
  <trd lng="it">coscia</trd>
  <trdgrp lng="nl">
    <trd>dij</trd>,
    <trd>dijbeen</trd>
  </trdgrp>
  <trd lng="pl">udo</trd>
  <trd lng="pt">coxa</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
  <trd lng="sk">stehno</trd>
  <trd lng="tr">uyluk</trd>
</drv>
</art>
