<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: femur.xml,v 1.35 2025/09/04 08:37:17 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>femur</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="femur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="femur.0o.vertebrula">
    <dif>
      <klr>(&ccirc;e surteraj vertebruloj)</klr> Supra parto de la malanta&ubreve;a respektive malsupraj membroj,

      de <ref cel="genu.0o.ANA">genuo</ref> &gcirc;is <ref cel="koks.0o">kokso</ref>:
      <ekz>
        metu vian manon sub mian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de <nom>Jakob</nom> elartiki&gcirc;is <klr>[&#8230;]</klr> kaj li lamis per sia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:25,31</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu sidi&gcirc;is sur lia nuko, alia sur la <tld/>oj kaj du aliaj kalkulis sur lia
        nuda korpo kvindek flekseblajn kanojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="ingven.0o">ingveno</ref> estas la enprofundi&gcirc;o inter la <tld/>o kaj ventro
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>osto</ind>.
        <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_c;&c_a;</trd>
        <trd lng="ca">f&egrave;mur</trd>
        <trd lng="cs">stehenn&iacute; kost</trd>
        <trd lng="da">l&aring;rbensknogle</trd>
        <trdgrp lng="de">
           <trd>Femur</trd>,
           <trd>Oberschenkelknochen</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>thighbone</trd>,
          <trd>femur</trd>
        </trdgrp>
        <trd lng="es">f&eacute;mur</trd>
        <trd lng="eu">izterrezur</trd>
        <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n; &a_r;&a_A;&a_n;</trd>
        <trd lng="fi">reisiluu</trd>
        <trd lng="fr">f&eacute;mur</trd>
        <trd lng="gl">f&eacute;mur</trd>
        <trd lng="he">&ayin;&tsadi;&fmem; &he;&yod;&resh;&fkaf;</trd>
        <trd lng="hr">bedrena kost</trd>
        <trd lng="hu">combcsont</trd>
        <trd lng="hy">&#1377;&#1382;&#1380;&#1408;&#1400;&#x57D;&#x56F;&#1408;</trd>
        <trd lng="id"><baz>tulang</baz>tulang paha</trd>
        <trd lng="is">l&aelig;rleggur</trd>
        <trd lng="it">femore</trd>
        <trd lng="ja">&#22823;&#x817F;&#x9AA8; <pr>&#12384;&#12356;&#x305F;&#12356;&#12371;&#12388;</pr></trd>
        <trd lng="ka">&#x10D1;&#x10D0;&#x10E0;&#x10EB;&#x10D0;&#x10E7;&#x10D8;&#x10E1;
        &#x10EB;&#x10D5;&#x10D0;&#x10DA;&#x10D8;</trd>
        <trd lng="ko">&#xB300;&#xD1F4;&#xACE8;</trd>
        <trd lng="la">femur</trd>
        <trd lng="lt">&scaron;launikaulis</trd>
        <trd lng="lv">aug&scaron;stilba kauls</trd>
        <trd lng="mi">p&uuml;kaka</trd>
        <trd lng="nl">dijbeen</trd>
        <trd lng="no">femur</trd>
        <trd lng="oc">femur</trd>
        <trd lng="pl">ko&sacute;&cacute; udowa</trd>
        <trd lng="pt">f&ecirc;mur</trd>
        <trd lng="ro">femur</trd>
        <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
        <trd lng="sh">bedrena kost</trd>
        <trd lng="sl">stegnenica</trd>
        <trd lng="sq">femuri i njeriut</trd>
        <trd lng="sv">l&aring;rben</trd>
        <trd lng="syc">&#x72B;&#1833;&#1808;</trd>
        <trd
        lng="ta">&#xBA4;&#xBCA;&#xB9F;&#xBC8;&#xBAF;&#xBC6;&#xBB2;&#xBC1;&#xBAE;&#xBCD;&#xBAA;&#xBC1;</trd>
        <trd lng="te">&#xC24;&#xC41;&#xC02;&#xC1F;&#xC3F; &#xC0E;&#xC2E;&#xC41;&#xC15;</trd>
        <trd
        lng="th">&#xE01;&#xE23;&#xE30;&#xE14;&#xE39;&#xE01;&#xE15;&#xE49;&#xE19;&#xE02;&#xE32;</trd>
        <trdgrp lng="tr">
          <trd>femur</trd>,
          <trd>kal&ccedil;a kemi&#xF0;i</trd>
        </trdgrp>
        <trd lng="zh">&#x80A1;&#x9AA8;</trd>

      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o">kruro</ref>,
      <ref cel="gamb.0o">gambo</ref>,
      <ref cel="tibi.0o">tibio</ref>,
      <ref cel="sur1.0o">suro</ref>.
    </refgrp>
    <trd lng="af">dy</trd>
    <trd lng="an">cuixa</trd>
    <trd lng="ar">&a_f;&a_kh;&a_dh;</trd>
    <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_o;</trd>
    <trd lng="bg">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
    <trd lng="bo">&t_ba;&t_ra;&t_lata;&t_tsheg;&t_sha;&t_tsheg;</trd>
    <trd lng="ca">cuixa</trd>
    <trdgrp lng="cs">
      <trd>femur</trd>,
      <trd>kost stehenn&iacute;</trd>,
      <trd>stehno</trd>
    </trdgrp>
    <trd lng="de">Oberschenkel</trd>
    <trd lng="el">&jota;&sigma;&chi;&jota_ton;&omikron;</trd>
    <trdgrp lng="en">
      <trd>upper leg</trd>,
      <trd>thigh</trd>,
      <trd>femur</trd>
    </trdgrp>
    <trd lng="es">muslo</trd>
    <trd lng="eu">izter</trd>
    <trd lng="fa">&a_r;&a_A;&a_n;</trd>
    <trd lng="fi">reisi</trd>
    <trd lng="fj">saga</trd>
    <trd lng="fo">l&aelig;r</trd>
    <trd lng="fr">cuisse</trd>
    <trd lng="gn">birdi</trd>
    <trd lng="ha">thai-th&uacute;i</trd>
    <trd lng="he">&yod;&resh;&fkaf;</trd>
    <trd lng="hr">butina</trd>
    <trd lng="ht">kwis</trd>
    <trd lng="hu">comb</trd>
    <trd lng="hy">&#1378;&#1400;&#1410;&#1380;</trd>
    <trd lng="id">paha</trd>
    <trd lng="is">l&aelig;ri</trd>
    <trd lng="it">coscia</trd>
    <trd lng="ja">&#x592A;&#x80A1; <pr>&#12405;&#12392;&#12418;&#12418;</pr></trd>
    <trd lng="jv">pupu</trd>
    <trd lng="ka">&#x10D1;&#x10D0;&#x10E0;&#x10EB;&#x10D0;&#x10E7;&#x10D8;</trd>
    <trd lng="kl">tharra</trd>
    <trd lng="kn">&#xCA4;&#xCCA;&#xCA1;&#xCC6;</trd>
    <trd lng="ko">&#xC5C9;&#xB369;&#xC774;</trd>
    <trd lng="la">femur</trd>
    <trd lng="mk">&c_b;&c_u;&c_t;</trd>
    <trd lng="ml">&#xD24;&#xD41;&#xD1F;</trd>
    <trd lng="mr">&#x92E;&#x93E;&#2306;&#2337;&#2368;</trd>
    <trdgrp lng="nl">
      <trd>dij</trd>,
      <trd>dijbeen</trd>
    </trdgrp>
    <trd lng="no">l&aring;r</trd>
    <trd lng="pl">udo</trd>
    <trd lng="pt">coxa</trd>
    <trd lng="qu">mama chanka</trd>
    <trd lng="ro">coaps&abreve;</trd>
    <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_o;</trd>
    <trd lng="scn">coscia</trd>
    <trd lng="sk">stehno</trd>
    <trd lng="sl">stegno</trd>
    <trd lng="sv">l&aring;r</trd>
    <trd lng="te">&#xC24;&#xC4A;&#xC21;</trd>
    <trd lng="tl">hita</trd>
    <trd lng="tr">uyluk</trd>
    <trd lng="ug">&a_s;&a_A;&a_n;</trd>
    <trd lng="yi">&pe;&vav;&lamed;&qof;&ayin;</trd>
    <trd lng="zh">&#22823;&#x817F;</trd>
  </snc>
  <snc mrk="femur.0o.artikula">
    <dif>
      <klr>(&ccirc;e artikuloj: insektoj, araneuloj k.a.)</klr>:
      Ordinare plej longa parto de la membro inter la kokso
      (a&ubreve; anta&ubreve;<tld/>o) kaj tibio.
    </dif>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_o;</trd>
  <trd lng="pl">gole&nacute;</trd>
  </snc>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_g;&c_n;&c_o;</trd>
