<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>abism</rad>/o
</kap>
<drv mrk="abism.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Profundega&jcirc;o:
      <ekz>
        la &scirc;ipo, renkontinte glacitavolegon,
        en<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <trd lng="cs">propast</trd>
    <trd lng="de">Tiefe</trd>
    <trd lng="en">abyss</trd>
    <trd lng="hu">m&eacute;lys&eacute;g</trd>
    <trd lng="nl">afgrond</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena, &gcirc;isfunda ruini&gcirc;o:
      <ekz>
        miaj esperoj forfalis en la
        <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="en">oblivion</trd>
    <trd lng="nl">afgrond</trd>
    <trd lng="hu">t&ouml;nk</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena, esenca neakordigebla malsameco:
      <ekz>
        estas <tld/>o inter amba&ubreve; opinioj;
      </ekz>
      <ekz>
        fremda animo estas <tld/>o sen limo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="fund.0o">fundo</ref>,
      <ref cel="mar.0o">maro</ref>,
      <ref cel="fund.sen0ajxo">senfunda&jcirc;o</ref>,
      <ref cel="glut.0ejo">glutejo</ref>.
    </refgrp>
    <trd lng="hu">szakad&eacute;k</trd>
    <trd lng="nl">kloof</trd>
  </snc>
  <adm>
    Reverkenda.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>isfont</trd>,
    <trd>islonk</trd>
  </trdgrp>
  <trd lng="de">Abgrund</trd>
  <trd lng="es">abismo</trd>
  <trdgrp lng="fr">
    <trd>ab&icirc;me</trd>,
    <trd>abysse</trd>,
    <trd>gouffre</trd>,
    <trd>pr&eacute;cipice</trd>,
    <trd>tr&eacute;fonds</trd>
  </trdgrp>
  <trd lng="it">abisso</trd>
  <trd lng="pt">abismo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;<klr tip="ind"> (&c_s;&c_u;&c_shch;.)</klr></trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">priepas&tcaron;</trd>
  <trd lng="sv">avgrund</trd>
</drv>

<drv mrk="abism.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="br">islonkus</trd>
  <trdgrp lng="fr">
    <trd>abyssal</trd>,
    <trd>illimit&eacute;<klr tip="ind"> (insondable)</klr></trd>,
    <trd>incommensurable<klr tip="ind"> (insondable)</klr></trd>,
    <trd>insondable<klr tip="ind"> (abyssal)</klr></trd>,
    <trd>infini<klr tip="ind"> (tr&egrave;s profond)</klr></trd>,
    <trd>profond<klr tip="ind"> (insondable)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
