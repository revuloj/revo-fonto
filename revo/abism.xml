<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>abism</rad>/o
</kap>
<drv mrk="abism.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Profundega&jcirc;o:
      <ekz>
        la &scirc;ipo, renkontinte glacitavolegon,
        en<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <trd lng="cs">propast</trd>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Plena, &gcirc;isfunda ruini&gcirc;o:
      <ekz>
        miaj esperoj  forfalis en la
        <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Plena, esenca neakordigebla  malsameco:
      <ekz>
        estas <tld/>o inter
        amba&ubreve; opinioj;
      </ekz>
      <ekz>
       fremda animo estas <tld/>o sen limo
       <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="fund.0o">fundo</ref>,
      <ref cel="mar.0o">maro</ref>,
      <ref cel="fund.sen0ajxo">senfunda&jcirc;o</ref>,
      <ref cel="glut.0ejo">glutejo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Abgrund</trd>
  <trd lng="en">abyss</trd>
  <trd lng="fr">ab&icirc;me</trd>
  <trd lng="it">abisso</trd>
  <trd
lng="ru">&#1087;&#1088;&#1086;&#1087;&#1072;&#1089;&#1090;&#1100;</trd>
</drv>
</art>
