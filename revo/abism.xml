<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abism.xml,v 1.58 2023/10/27 12:51:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>abism</rad>/o
</kap>
<drv mrk="abism.0o">
  <kap><tld/>o</kap>
  <snc mrk="abism.0o.profundo">
    <dif>
      <ref tip="super" cel="profun.0egajxo">Profundega&jcirc;o</ref>:
      <ekz>
        mallumo estis super la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        neforgesebla alterno de belegaj pejza&gcirc;oj alpaj: krutaj rokmuroj, ombraj
        <tld/>oj, idiliaj pa&scirc;tejoj
        <fnt>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007151.php">900 km tra montbicikla paradizo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;iaj pupiloj, nigraj kaj profundaj kiel <tld/>o, brulis kiel vulkanoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i falis en senfundan <tld/>on de malespero
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krut.inter0ejo">interkrutejo</ref>,
      <ref cel="fend.0ego">fendego</ref>,
      <ref cel="kavern.0o">kaverno</ref>,
      <ref cel="krater.0o.GEOL">kratero<sncref/></ref>,
      <ref cel="ravin.0o">ravino</ref>,
      <ref cel="val.0o">valo</ref>
    </refgrp>
    <trd lng="ca">abisme</trd>
    <trd lng="cs">propast</trd>
    <trd lng="de">Tiefe</trd>
    <trd lng="en">abyss</trd>
    <trd lng="hu">m&eacute;lys&eacute;g</trd>
    <trd lng="nl">afgrond</trd>
    <trd lng="oc">abisme</trd>
    <trdgrp lng="pl">
      <trd>czelu&sacute;&cacute;</trd>,
      <trd>przepa&sacute;&cacute;</trd>,
      <trd>otch&lstroke;a&nacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x58D1; <pr>h&#xE8;</pr></trd>,
    <trd>&#x5CED;&#x58C1; <pr>qi&#xE0;ob&#xEC;</pr></trd>,
    <trd>&#x5D16; <pr>y&#xE1;</pr></trd>,
    <trd>&#x60AC;&#x5D16; <pr>xu&#xE1;ny&#xE1;</pr></trd>,
    <trd>&#x61F8;&#x5D16; <pr>xu&#xE1;ny&#xE1;</pr></trd>,
    <trd>&#x6DF1;&#x5751; <pr>sh&#x113;nk&#x113;ng</pr></trd>,
    <trd>&#x6DF1;&#x5EA6; <pr>sh&#x113;nd&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="abism.0o.ruinigxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena, &gcirc;isfunda <ref tip="super" cel="ruin.0igxo">ruini&gcirc;o</ref>:
      <ekz>        
        memoru <klr>[&#x2026;]</klr> ekkonu anta&ubreve;e la profundecon 
        de la <tld/>o, anta&ubreve; ol vi ensaltas en &gcirc;in!
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la esperojn de mia vivo mi devis vidi forfali en la <tld/>on, kaj restis al mi
        nenio
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la morga&ubreve;a tago ofte ruinigadis kaj &jcirc;etadis en <tld/>on de forgeso tion, kion &scirc;i
        kun granda malfacileco sukcesis atingi hiera&ubreve;
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tradiciaj partioj falis en <tld/>on
        <fnt>
          <aut>Stefan Maul</aut>
          
          <vrk><url ref="&Monato;/1993/A930802.php">Alarmsignalo el Italio</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bankro.0o">bankroto</ref>,
       <ref cel="depresi.0o.EKON">depresio<sncref/></ref>,
       <ref cel="hauxs.mal0o">malha&ubreve;so</ref>
    </refgrp>
    <trd lng="en">oblivion</trd>
    <trd lng="nl">afgrond</trd>
    <trd lng="hu">t&ouml;nk</trd>
    <trdgrp lng="pl">
      <trd>przepa&sacute;&cacute;</trd>,
      <trd>niebyt</trd>,
      <trd>nico&sacute;&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="abism.0o.diferenco">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena, esenca neakordigebla diferenco:
      <ekz>
        fremda animo estas <tld/>o sen limo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        estas giganta <tld/>o inter malri&ccirc;aj kaj industriaj landoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008117.html">Grizi&gcirc;anta mondo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        evidenti&gcirc;as, ke da&ubreve;re ekzistas <tld/>o inter diversaj klasoj koncerne
        universitatan edukadon
        <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2001/007287.html">Studentoj universitaton soifantaj</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 12</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="fund.0o">fundo</ref>,
      <ref cel="mar.0o">maro</ref>,
      <ref cel="fund.sen0ajxo">senfunda&jcirc;o</ref>,
      <ref cel="glut.0ejo">glutejo</ref>.
    </refgrp>
    <trd lng="hu">szakad&eacute;k</trd>
    <trd lng="nl">kloof</trd>
    <trd lng="pl">przepa&sacute;&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>isfont</trd>,
    <trd>islonk</trd>
  </trdgrp>
  <trd lng="ca">abisme</trd>
  <trd lng="de">Abgrund</trd>
  <trd lng="es">abismo</trd>
  <trdgrp lng="fa">
    <trd>&a_q;&a_ayn;&a_r;</trd>,
    <trd>&f_zh;&a_r;&a_f;&a_n;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ab&icirc;me</trd>,
    <trd>abysse</trd>,
    <trd>gouffre</trd>,
    <trd>pr&eacute;cipice</trd>,
    <trd>tr&eacute;fonds</trd>
  </trdgrp>
  <trd lng="he">&tav;&he;&vav;&fmem;</trd>
  <trd lng="it">abisso</trd>
  <trdgrp lng="ja">
    <trd>&#x6DF1;&#x6DF5;  <pr>&#x3057;&#x3093;&#x3048;&#x3093;</pr></trd>,
    <trd>&#x6DF1;&#x6D77;  <pr>&#x3057;&#x3093;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x5E95;&#x306A;&#x3057;&#x306E;&#x6DF5;  <pr>&#x305D;&#x3053;&#x306A;&#x3057;&#x306E;&#x3075;&#x3061;</pr></trd>,
    <trd>&#x5929;&#x5730;&#x5275;&#x9020;&#x4EE5;&#x524D;&#x306E;&#x6DF7;&#x6C8C;  <pr>&#x3066;&#x3093;&#x3061;&#x305D;&#x3046;&#x305E;&#x3046;&#x3044;&#x305C;&#x3093;&#x306E;&#x3053;&#x3093;&#x3068;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pt">abismo</trd>
  <trd lng="ro">abis</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;<klr tip="ind">
    (&c_s;&c_u;&c_shch;.)</klr></trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">priepas&tcaron;</trd>
  <trd lng="sv">avgrund</trd>
  <trd lng="tr">u&ccedil;urum</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&#x43E;&c_d;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_ib;&c_r;&c_v;&c_a;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_v;&c_a;&#x43B;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="abism.0a">
  <kap><tld/>a</kap>
  <snc mrk="abism.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        en la <tld/>a mallumo, vido estus mal&ccirc;efa kaj tu&scirc;senso <klr>[&#x2026;]</klr> estus &ccirc;efa
        <fnt>
          <aut>Jean-Marc L&eacute;vy-Leblond </aut>
          <vrk><url ref="&Mondediplo;article1115.html">&Ccirc;u scienco estas
          universala?</url></vrk>
          <lok>Le Monde diplomatique en Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj fundoj kun silente kaj ka&scirc;ite embuskantaj rampa&jcirc;oj de minacaj tentoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_d;&c_o;&c_n;&c_n;&c_y;</trd>
  <trd lng="br">islonkus</trd>
  <trd lng="ca">abismal</trd>
  <trd lng="cs">propastn&yacute;</trd>
  <trd lng="da">afgrundsdyb</trd>
  <trdgrp lng="fr">
    <trd>abyssal</trd>,
    <trd>illimit&eacute;<klr tip="ind"> (insondable)</klr></trd>,
    <trd>incommensurable<klr tip="ind"> (insondable)</klr></trd>,
    <trd>insondable<klr tip="ind"> (abyssal)</klr></trd>,
    <trd>infini<klr tip="ind"> (tr&egrave;s profond)</klr></trd>,
    <trd>profond<klr tip="ind"> (insondable)</klr></trd>
  </trdgrp>
  <trd lng="he">&tav;&he;&vav;&mem;&yod;</trd>
  <trd lng="it">abissale</trd>
  <trdgrp lng="ja">
    <trd>&#x6DF1;&#x6DF5;&#x306E;  <pr>&#x3057;&#x3093;&#x3048;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x5E95;&#x77E5;&#x308C;&#x306A;&#x3044;&#x307B;&#x3069;&#x6DF1;&#x3044;  <pr>&#x305D;&#x3053;&#x3057;&#x308C;&#x306A;&#x3044;&#x307B;&#x3069;&#x3075;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x6DF1;&#x6D77;&#x306E;  <pr>&#x3057;&#x3093;&#x304B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czelustny</trd>,
    <trd>przepastny</trd>,
    <trd>bezdenny</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">priepastn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_d;&#x43E;&#x43D;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_g;&#x43B;&c_i;&c_b;&#x43E;&#x43A;&#x43E;&c_v;&#x43E;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_b;&c_ib;&c_s;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="abism.en0igxi">
  <kap>
    en<tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      <ref cel="aper.mal0i.nevidebligxi">Malaperi</ref> en <tld/>on:
      <ekz>
        &scirc;i deziris, ke la pargeto en<tld/>i&gcirc;u sub &scirc;iaj piedoj, por ke &scirc;i estu ka&scirc;ita for
        de &ccirc;iuj rigardoj.
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i en<tld/>i&gcirc;is en &gcirc;emo kaj agonio
        <fnt>
          <vrk><url ref="http://esperas.info/index.php?05waka-teksto">Juna Violo (12), el Rakontaro
          Gen&gcirc;i</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Lizio eksilentis, kaj mi en<tld/>i&gcirc;is en profundan meditadon
        <fnt>
          <vrk><url ref="https://pt.scribd.com/document/47173615/Nia-Hejmo">Nia Hejmo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="dron.0i">droni</ref>,
       <ref cel="sink.0i">sinki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol; &c_u; &c_b;&c_je;&c_z;&c_d;&c_a;&c_n;&c_ib;</trd>
  <trd lng="de">in den <ind>Abgrund</ind> versinken</trd>
  <trdgrp lng="ja">
    <trd>&#x6DF1;&#x304F;&#x6C88;&#x3080;  <pr>&#x3075;&#x304B;&#x304F;&#x3057;&#x305A;&#x3080;</pr></trd>,
    <trd>&#x6C88;&#x307F;&#x3053;&#x3080;  <pr>&#x3057;&#x305A;&#x307F;&#x3053;&#x3080;</pr></trd>
  </trdgrp>
</drv>

