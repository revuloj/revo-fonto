<art mrk="$Id: skorbu.xml,v 1.24 2024/03/25 06:40:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skorbut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skorbu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skorbu.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> karakterizata per muskola
      malforti&gcirc;o, sangado de la
      dentokarno a&ubreve; hemoragioj de l' ha&ubreve;to, ka&ubreve;zata de
      manko je fre&scirc;a nutra&jcirc;o enhavanta vitaminojn:
      <ekz>
        <tld/>o <klr>...</klr> estis tre timata dum la
        epoko de la grandaj voja&gcirc;oj per vel&scirc;ipo, kiam
        multaj maristoj mortis pro &gcirc;i
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_y;&c_n;&c_g;&c_a;</trd>, 
    <trd>&c_s;&c_k;&c_a;&c_r;&c_b;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_o;&c_r;&c_b;&c_u;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>kurd&ecaron;je</trd>,
    <trd>skorbut</trd>
  </trdgrp>
  <trd lng="de">Skorbut</trd>
  <trd lng="en">scurvy</trd>
  <trd lng="fr">scorbut</trd>
  <trd lng="hu">skorbut</trd>
  <trd lng="id">skorbut</trd>
  <trd lng="ja">&#x58CA;&#x8840;&#x75C5;  <pr>&#x304B;&#x3044;&#x3051;&#x3064;&#x3073;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">scheurbuik</trd>
  <trd lng="no">skj&oslash;rbuk</trd>
  <trd lng="pl">szkorbut</trd>
  <trd lng="ru">&c_c;&c_i;&c_n;&c_g;&c_a;</trd>
  <trd lng="sk">skorbut</trd>
  <trdgrp lng="uk">
    <trd>&#x446;&#x438;&#x43D;&#x433;&#x430;</trd>,
    <trd>&#x441;&#x43A;&#x43E;&#x440;&#x431;&#x443;&#x442;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x574F;&#x8840;&#x75C5; <pr>hu&#xE0;ixu&#xE8;b&#xEC;ng</pr></trd>,
    <trd>&#x58DE;&#x8840;&#x75C5; <pr>hu&#xE0;ixu&#xE8;b&#xEC;ng</pr></trd>,
    <trd>&#x574F;&#x8840;&#x75C5; <pr>hu&#xE0;ixi&#x11B;b&#xEC;ng</pr></trd>,
    <trd>&#x58DE;&#x8840;&#x75C5; <pr>hu&#xE0;ixi&#x11B;b&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
