<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>kodeks</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="kodeks.0o">
      <kap><tld/>o</kap>
      <snc mrk="kodeks.0o.TIP">
	<uzo tip="fak">TIP</uzo>
	<dif>
	  Manuskripto el kunkudritaj folioj.  
	</dif>
	<trdgrp lng="de">
	  <trd>Kodex</trd>,
	  <trd>alte <ind>Handschrift</ind></trd>
	</trdgrp>
	<trd lng="ru">&c_k;&c_o;&c_d;&c_je;&c_k;&c_s;</trd>
      </snc>
      <snc mrk="kodeks.0o.MED">
	<uzo tip="fak">MED</uzo>
	<dif>
	  Kolekto de la farmaciaj formuloj akceptitaj de
	  la medicina fakultato.
	</dif>
	<ref tip="sin" cel="farmak1.0o">farmakopeo</ref>
	<trdgrp lng="de">
	  <trd>Pharmakop&ouml;e</trd>,
	  <trd>Verzeichnis der offiziellen <ind>Arzneimittel</ind></trd>
	</trdgrp>
	<trdgrp lng="en">
	  <trd>pharmacoepia</trd>,
	  <trd>pharmacopia</trd>
	</trdgrp>
	<trd lng="nl">farmacopee</trd>
	<trdgrp lng="ru">
	  <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_k;&c_o;&c_p;&c_je;&c_ja;</trd>,
	  <trd>&c_r;&c_je;&c_c;&c_je;&c_p;&c_t;&c_u;&c_r;&c_n;&c_a;&c_ja;
&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
	</trdgrp>
      </snc>
      <snc mrk="kodeks.0o.JUR">
	<uzo tip="fak">JUR</uzo>
	<dif>
	  Kohera <ref tip="super" cel="kolekt.0o.libr">kolekto</ref>
	  da normoj a&ubreve; le&gcirc;oj, regulantaj iun bran&ccirc;on
	  de sociaj rilatoj:
          <ekz>
            civila, kriminala, komerca <tld/>o;
          </ekz><ekz>
	    la parte vivrandaj roluloj havas tre malsamajn opiniojn
	    pri metafizikaj aferoj kaj la ebla morala <tld/>o
	    <fnt>
	      <aut>PEJNO Simono</aut>:
	      <vrk> Atentokapta sed ru&gcirc;krajoninda</vrk>.
	      <lok><url
		  ref="http://www.esperanto.be/fel/mon/rec/napa.html"
		  >Monato</url></lok>
	    </fnt>;
	  </ekz><ekz>
	    la fama <tld/>o de Hammurabi
	    <fnt>
	      <aut>Andrzej Grz&eogonek;bowski</aut>:
	      <vrk>Irano: la 131a lando de MT</vrk>.
	      <lok>LOdE,
		<url ref="http://www.esperanto.org/Ondo/81-lode.htm"
		  >n-ro 81 (2001:7)</url></lok>
	    </fnt>;
	  </ekz><ekz>
	    tradicia islama <tld/>o ja postulas pruvon pri virg(in)eco
	    <fnt>
	      <aut>P. Thorsen</aut>:
	      <vrk>&Ccirc;u eblas reakiri virgecon?</vrk>
	      <lok>La Kancerkliniko, dec. 1994 (citite la&ubreve;
		<url ref="http://www.eventoj.hu/arkivo/eve-068.htm"
		  >Eventoj n-ro 068</url></lok>).
	    </fnt>
	  </ekz>
	</dif>
	<ref tip="sin" cel="kod.0o.JUR">kodo<sncref/></ref>
        <trdgrp lng="de">
	  <trd>Kodex</trd>,
	  <trd>Gesetzbuch</trd>
        </trdgrp>
	<trdgrp lng="ru">
	  <trd>&c_k;&c_o;&c_d;&c_je;&c_k;&c_s;</trd>,
	  <trd>&c_s;&c_v;&c_o;&c_d; &c_z;&c_a;&c_k;&c_o;&c_n;&c_o;&c_v;</trd>,
	  <trd>&c_u;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
	</trdgrp>
      </snc>
    <trd lng="fr">codex</trd>
    </drv>
  </art>
