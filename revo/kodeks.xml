<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kodeks</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kodeks.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Manuskripto el kunkudritaj folioj.  
    </dif>
    <trdgrp lng="de">
      <trd>Kodex</trd>,
      <trd>alte <ind>Handschrift</ind></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Kolekto de la farmaciaj formuloj akceptitaj de
      la medicina fakultato. Farmakopeo.
    </dif>
    <trdgrp lng="de">
      <trd>Pharmakop&ouml;e</trd>,
      <trd>Verzeichnis der offiziellen <ind>Arzneimittel</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>pharmacoepia</trd>,
      <trd>pharmacopia</trd>
    </trdgrp>
    <trd lng="nl">farmacopee</trd>
    <trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_k;&c_o;&c_p;&c_je;&c_ja;</trd>,
      <trd>&c_r;&c_je;&c_c;&c_je;&c_p;&c_t;&c_u;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_n;&c_i;&c_g;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
