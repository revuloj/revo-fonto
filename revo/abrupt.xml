<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abrupt.xml,v 1.40 2022/05/15 09:31:59 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>abrupt</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="abrupt.0a">
  <kap><tld/>a</kap>
  <snc mrk="abrupt.0a.KOMUNE">
    <dif>
      Malagrable subita, senprepara,
      <ref tip="vid" cel="mild.mal0a.KOMUNE">malmilda</ref>:
      <ekz>
        <tld/>e demandi, frapi, aperi;
      </ekz>
      <ekz>
        en la kanto <ind><tld/>e</ind>
        <trdgrp lng="be">
          <trd>&c_r;&c_a;&c_p;&c_t;&c_o;&c_w;&c_n;&c_a;</trd>,
          <trd>&c_z;&c_mol;&c_n;&c_ja;&c_n;&c_a;&c_c;&c_k;&c_u;</trd>,
          <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;</trd>,
          <trd>&c_r;&c_e;&c_z;&c_k;&c_a;</trd>
        </trdgrp>
        alternis tirata moleco kaj ra&ubreve;ka
        malmoleco.
      </ekz>
    </dif>
    <trd lng="th">&#xE09;&#xE31;&#xE1A;&#xE1E;&#xE25;&#xE31;&#xE19;
    (&#xE44;&#xE21;&#xE48;&#xE44;&#xE14;&#xE49;&#xE15;&#xE31;&#xE49;&#xE07;&#xE15;&#xE31;&#xE27;)</trd>
    <trd lng="ms">mendadak</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_p;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF64;&#xF0B;&#xF66;&#xF90;&#xF58;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>hrub&yacute;</trd>,
    <trd>kolm&yacute;</trd>,
    <trd>nezdvo&rcaron;il&yacute;</trd>,
    <trd>obo&rcaron;iv&yacute;</trd>,
    <trd>prudk&yacute;</trd>,
    <trd>p&rcaron;&iacute;kr&yacute;</trd>,
    <trd>strm&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abrupt</trd>,
    <trd>j&auml;h</trd>,
    <trd>pl&ouml;tzlich</trd>,
    <trd>zerkl&uuml;ftet</trd>
  </trdgrp>
  <trd lng="en">abrupt</trd>
  <trd lng="es">abrupto</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_A;&f_g;&a_h;&a_A;&a_n;&f_y;</trd>,
    <trd>&a_b;&f_y;&#8204;&a_kh;&a_b;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abrupt</trd>,
    <trd>brusque</trd>,
    <trd>brutal</trd>,
    <trd>inattendu</trd>,
    <trd>rude<klr tip="ind"> (brusque)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E4;&#x5BC;&#x5B4;&#x5EA;&#x5B0;&#x5D0;&#x5D5;&#x5B9;&#x5DE;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>hirtelen</trd>,
    <trd>v&aacute;ratlan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>improvviso</trd>,
    <trd>subitaneo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>abrupt</trd>,
    <trd>brusk</trd>,
    <trd>kortaf</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gwa&lstroke;towny</trd>,
    <trd>raptowny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abrupto</trd>,
    <trd>brusco</trd>,
    <trd>inesperado</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>p&abreve;trunz&abreve;tor</trd>,
    <trd>fulger&abreve;tor</trd>,
    <trd>violent</trd>,
    <trd>brusc</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_z;&c_a;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;hly</trd>,
    <trd>ne&ccaron;akan&yacute;</trd>,
    <trd>prudk&yacute;</trd>
  </trdgrp>
  <trd lng="sv">abrupt</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_z;&#x43A;&c_i;&c_j; (&#x43F;&c_r;&#x43E; &#x43C;&#x43E;&c_v;&c_u;</trd>,
    <trd>&c_t;&#x43E;&#x43D;)</trd>,
    <trd>&c_u;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_r;&c_ib;&c_v;&#x43D;&c_i;&c_j; (&#x43F;&c_r;&#x43E; &c_s;&c_t;&c_i;&#x43B;&#x44C;)</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="abrupt.0i.KOMUNE">
    <dif>
      Malmilde, subite aperi, trafi, esprimi:
      <ekz>
        <ctl>mi ne edzi&gcirc;os al &scirc;i!</ctl> <tld/>is
        malka&scirc;e 
        mia frato
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a; <klr>(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t &uacute;se&ccaron;n&yacute;</trd>,
    <trd>obo&rcaron;it se <klr>(na n&ecaron;koho)</klr></trd>
  </trdgrp>
  <trd lng="fr">brusquer</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; &uacute;se&ccaron;n&yacute;</trd>,
    <trd>obori&tcaron; sa na niekoho</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="abrupt.0eco.KOMUNE">
    <dif>
      Subiteco, malmildeco:
      <ekz>
        mi neniel povas kompreni la <tld/>econ de lia amo
        <fnt>
          <aut>manlajo</aut>,
          <vrk><url ref="http://en.lernu.net/komunikado/forumo/">
          Vorto de la tago 2012</url></vrk>,
          <lok>lernu.net, 2014-11-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">abruptness</trd>
  <trdgrp lng="cs">
    <trd>sr&aacute;znost</trd>,
    <trd>strmost</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brusquerie</trd>,
    <trd>rudesse</trd>
  </trdgrp>
  <trd
  lng="he">&#x5E4;&#x5BC;&#x5B4;&#x5EA;&#x5B0;&#x5D0;&#x5D5;&#x5B9;&#x5DE;&#x5B4;&#x5D9;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trdgrp lng="pl">
    <trd>gwa&lstroke;towno&sacute;&cacute;</trd>,
    <trd>raptowno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>strmos&tcaron;</trd>,
    <trd>zr&aacute;zovitos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="abrupt.mal0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="iom.0post0a.KOMUNE">Iompostioma</ref>,
      milda:
      <ekz>
        &scirc;oseo deklivanta mal<tld/>e &gcirc;is <klr>...</klr>
        rivero
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="grad.0a.iompostioma">grada</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_u;&c_p;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="en"><trd>gradual</trd>, <trd>circuitous</trd>,
  <trd>expansive</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>en rondeur</trd>,
    <trd>progressif</trd>
  </trdgrp>
</drv>
</art>
