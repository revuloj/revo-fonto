<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>abrupt</rad>/a
</kap>
<drv mrk="abrupt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malagrable subita, senprepara:
      <ekz>
        <tld/>e demandi, frapi,  aperi;
      </ekz>
      <ekz>
        en la kanto <tld/>e alternis tirata moleco kaj ra&ubreve;ka
        malmoleco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sku.0i">skui</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>abrupt</trd>,
    <trd>brusk</trd>,
    <trd>kortaf</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abrupt</trd>,
    <trd>j&auml;h</trd>,
    <trd>pl&ouml;tzlich</trd>,
    <trd>zerkl&uuml;ftet</trd>
  </trdgrp>
  <trd lng="en">abrupt</trd>
  <trdgrp lng="fr">
    <trd>abrupt</trd>,
    <trd>brusque</trd>,
    <trd>inattendu</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>improvviso</trd>,
    <trd>subitaneo</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abrupto</trd>,
    <trd>brusco</trd>,
    <trd>inesperado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&#1074;&#1085;&#1077;&#1079;&#1072;&#1087;&#1085;&#1099;&#1081;</trd>,
    <trd>&#1088;&#1077;&#1079;&#1082;&#1080;&#1081;</trd>,
    <trd>&#1086;&#1090;&#1088;&#1099;&#1074;&#1080;&#1089;&#1090;&#1099;&#1081;</trd>
  </trdgrp>
  <trd lng="sv">abrupt</trd>
</drv>
</art>
