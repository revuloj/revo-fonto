<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abrupt.xml,v 1.45 2023/12/03 15:23:09 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>abrupt</rad>/a <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="abrupt.0a">
  <kap><tld/>a</kap>
  <snc mrk="abrupt.0a.KOMUNE">
    <dif>
      <ref tip="sin" cel="subit.0a">Subita</ref>,
      <ref tip="sin" cel="prepar.sen0a">senprepara</ref>,
      akre <ref tip="sin" cel="surpri.0a">surpriza</ref>:
      <ekz>
        tuj post la trapaso de arbaro sekvas <tld/>a pejza&gcirc;-&scirc;an&gcirc;o, kvaza&ubreve; oni trafus en
        alian kontinenton
        <fnt>
          <bib>Monato</bib>
          <aut>Hose Marques</aut>
          <vrk>Argentino: Voja&gcirc;o al la provinco Tukumano</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ru&gcirc;a koloro &ccirc;e &scirc;iaj lipoj fini&gcirc;as per <tld/>a linio la&ubreve; la interna rando
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sensacia disvasti&gcirc;o kaj <tld/>a pereo de <nom>Volap&uuml;k</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a malalti&gcirc;o de la kvalita nivelo
        <fnt>
          <bib>KFl</bib>
          <lok>I. Enkonduko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj malsupreni&gcirc;oj al basa gamo
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="&Monato;/2007/009131.php"
              >Tri diskoj kun ses paroladoj de Ivo Lapenna</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenio akra en &scirc;i, nenio iam ajn <tld/>a, nur tiu agrabla, glata harmonieco de la
        vo&ccirc;o, de la movoj
        <fnt>
          <bib>ChV</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la turno estas tiel <tld/>a, ke Karlo ektremas, &ccirc;ar la radoj kontuzi&gcirc;as al la
        trotuarrando, &ccirc;e kiu li sidas
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontra8.0o">kontrasta</ref>,
      <ref cel="krut.0a">kruta</ref>,
      <ref cel="sponta1.0a">spontanea</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>abrupt</trd>,
      <trd>j&auml;h</trd>
    </trdgrp>
    <trd lng="th">&#3593;&#3633;&#3610;&#3614;&#3621;&#3633;&#3609;
    (&#3652;&#3617;&#3656;&#3652;&#3604;&#3657;&#3605;&#3633;&#3657;&#3591;&#3605;&#3633;&#3623;)</trd>
    <trd lng="ms">mendadak</trd>
    <trdgrp lng="zh">
      <trd>&#19968;&#19979;&#23376; <pr>y&#299;xi&agrave;zi</pr></trd>,
      <trd>&#20345; <pr>gu&#464;</pr></trd>,
      <trd>&#20882;&#28982; <pr>m&agrave;or&aacute;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="abrupt.0a.bruska">
    <dif>
      Kvaza&ubreve; fortran&ccirc;ita, &scirc;irita;
      malagrable <ref cel="mild.mal0a.KOMUNE">malmilda</ref>;
      <ref tip="sin" cel="brusk.0a">bruska</ref>:
      <ekz>
        <klr>[li ekparolis]</klr> per rapida <tld/>a
        <klr>(<ref cel="stakat.0e">stakata</ref>)</klr> vo&ccirc;o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun tre <tld/>a &gcirc;isrevido li forlasis <nom>Lizabeta</nom>n
        <fnt>
          <bib>ChV</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;as <tld/>a virina rido
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a morto
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009082.php"
              >Majstra verko de a&ubreve;toro preska&ubreve; nekonata</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesos <tld/>aj, drastaj kaj dolorigaj &scirc;an&gcirc;oj, kiuj parte estos arbitraj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008117.html"
              >Grizi&gcirc;anta mondo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        malaperi per ia <tld/>a, singulta fino
        <fnt>
          <bib>KKr</bib>
          <lok>II</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drast.0a">drasta</ref>,
      <ref cel="koher.mal0a">malkohera</ref>,
      <ref cel="kontin3.mal0a">malkontinua</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>abgehackt <klr>(Stimme)</klr></trd>,
      <trd>gebrochen <klr>(Stimme)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_p;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_sha;&t_tsheg;&t_sa;&t_kata;&t_ma;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hrub&yacute;</trd>,
    <trd>kolm&yacute;</trd>,
    <trd>nezdvo&rcaron;il&yacute;</trd>,
    <trd>obo&rcaron;iv&yacute;</trd>,
    <trd>prudk&yacute;</trd>,
    <trd>p&rcaron;&iacute;kr&yacute;</trd>,
    <trd>strm&yacute;</trd>
  </trdgrp>
  <trd lng="de">schroff</trd>
  <trd lng="en">abrupt</trd>
  <trd lng="es">abrupto</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_A;&f_g;&a_h;&a_A;&a_n;&f_y;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_kh;&a_b;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abrupt</trd>,
    <trd>brusque</trd>,
    <trd>brutal</trd>,
    <trd>inattendu</trd>,
    <trd>rude<klr tip="ind"> (brusque)</klr></trd>
  </trdgrp>
  <trd lng="he">&pe;&#1468;&#1460;&tav;&#1456;&alef;&vav;&#1465;&mem;&#1460;&yod;</trd>
  <trdgrp lng="hu">
    <trd>hirtelen</trd>,
    <trd>v&aacute;ratlan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>improvviso</trd>,
    <trd>subitaneo</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21776;&#31361;&#12394;  <pr>&#12392;&#12358;&#12392;&#12388;&#12394;</pr></trd>,
    <trd>&#31361;&#28982;&#12398;  <pr>&#12392;&#12388;&#12380;&#12435;&#12398;</pr></trd>,
    <trd>&#19981;&#24847;&#12398;  <pr>&#12405;&#12356;&#12398;</pr></trd>,
    <trd>&#12406;&#12387;&#12365;&#12425;&#12412;&#12358;&#12394;</trd>,
    <trd>&#38522;&#12375;&#12356;  <pr>&#12369;&#12431;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>abrupt</trd>,
    <trd>brusk</trd>,
    <trd>kortaf</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gwa&lstroke;towny</trd>,
    <trd>raptowny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abrupto</trd>,
    <trd>brusco</trd>,
    <trd>inesperado</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>p&abreve;trunz&abreve;tor</trd>,
    <trd>fulger&abreve;tor</trd>,
    <trd>violent</trd>,
    <trd>brusc</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_z;&c_a;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;hly</trd>,
    <trd>ne&ccaron;akan&yacute;</trd>,
    <trd>prudk&yacute;</trd>
  </trdgrp>
  <trd lng="sv">abrupt</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_z;&c_k;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_m;&c_o;&c_v;&c_u;</trd>,
    <trd>&c_t;&c_o;&c_n;)</trd>,
    <trd>&c_u;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_ib;&c_v;&c_n;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_s;&c_t;&c_i;&c_l;&c_mol;)</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      En <tld/>a maniero, <ref tip="sin" cel="subit.0e">subite</ref>:
      <ekz>
        ne pensu malbone pri ni pro tio, ke ni tiel <tld/>e forlasas vin
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        hontu! li diris <tld/>e
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>e silenti&gcirc;is, frapita de la aspekto de Johano
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_p;&c_t;&c_o;&c_w;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_n;&c_a;&c_c;&c_k;&c_u;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unvermittelt</trd>,
    <trd>abrupt</trd>,
    <trd>pl&ouml;tzlich</trd>,
    <trd>unversehens</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="abrupt.0i.KOMUNE">
    <dif>
      Malmilde, subite aperi, trafi, esprimi:
      <ekz>
        <ctl>mi ne edzi&gcirc;os al &scirc;i!</ctl> <tld/>is
        malka&scirc;e
        mia frato
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a; <klr>(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t &uacute;se&ccaron;n&yacute;</trd>,
    <trd>obo&rcaron;it se <klr>(na n&ecaron;koho)</klr></trd>
  </trdgrp>
  <trd lng="fr">brusquer</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; &uacute;se&ccaron;n&yacute;</trd>,
    <trd>obori&tcaron; sa na niekoho</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="abrupt.0eco.KOMUNE">
    <dif>
      Subiteco, malmildeco:
      <ekz>
        mi neniel povas kompreni la <tld/>econ de lia amo
        <fnt>
          <aut>manlajo</aut>,
          <vrk><url ref="http://en.lernu.net/komunikado/forumo/">
          Vorto de la tago 2012</url></vrk>,
          <lok>lernu.net, 2014-11-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">abruptness</trd>
  <trdgrp lng="cs">
    <trd>sr&aacute;znost</trd>,
    <trd>strmost</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brusquerie</trd>,
    <trd>rudesse</trd>
  </trdgrp>
  <trd lng="he">&pe;&#1468;&#1460;&tav;&#1456;&alef;&vav;&#1465;&mem;&#1460;&yod;&vav;&#1468;&tav;</trd>
  <trdgrp lng="pl">
    <trd>gwa&lstroke;towno&sacute;&cacute;</trd>,
    <trd>raptowno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>strmos&tcaron;</trd>,
    <trd>zr&aacute;zovitos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="abrupt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="abrupt.mal0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="iom.0post0a.KOMUNE">Iompostioma</ref>,
      milda:
      <ekz>
        &scirc;oseo deklivanta mal<tld/>e &gcirc;is <klr>[&#8230;]</klr>
        rivero
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="grad.0a.iompostioma">grada</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_u;&c_p;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="en"><trd>gradual</trd>, <trd>circuitous</trd>,
  <trd>expansive</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>en rondeur</trd>,
    <trd>progressif</trd>
  </trdgrp>
</drv>
</art>
