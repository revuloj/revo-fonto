    <kap>
      <rad>Rodan</rad>/o
    </kap>

<drv mrk="rodan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref>
      fluanta el gla&ccirc;ero en la svisaj Alpoj, trafluanta la &Gcirc;enevan lagon kaj en suda <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>
      formanta delton &ccirc;e Mediteraneo:
      <ekz>
        homamasoj blokis a&ubreve;to&scirc;oseojn kaj &ccirc;iujn pontojn super <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Piron</aut>
          <vrk><url ref="&Monato;/2003/008141.html">Francio invitas, Svislando
          suferas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pasintjare la fi&scirc;oj de la franca <tld/>o mortis pro kemia&jcirc;oj, &jcirc;etitaj de
        proksimaj kemiaj entreprenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2012/010696.php">&Ccirc;u vivofonto a&ubreve;
          profitfonto?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;longe de la rivero <tld/>o sude de Liono
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2014/011141.php">Kurioza vila&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_R;&c_o;&c_n;&c_a;</trd>
    <trd lng="de">Rhone</trd>
    <trd lng="en">Rh&ocirc;ne</trd>
    <trd lng="fr">Rh&ocirc;ne</trd>
    <trd lng="pl">Rodan</trd>
    <trd lng="ro">Ron</trd>
    <trd lng="uk">&c_R;&#x43E;&#x43D;&c_a;</trd>
  </snc>
  <trd lng="ja">&#x30ED;&#x30FC;&#x30CC;&#x5DDD;  <pr>&#x30ED;&#x30FC;&#x30CC;&#x304B;&#x308F;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x7F57;&#x8BB7;&#x6CB3; <pr>Lu&#xF3;n&#xE8;H&#xE9;</pr></trd>,
    <trd>&#x7F85;&#x8A25;&#x6CB3; <pr>Lu&#xF3;n&#xE8;H&#xE9;</pr></trd>,
    <trd>&#x9686;&#x6CB3; <pr>L&#xF3;ngH&#xE9;</pr></trd>
  </trdgrp>
</drv>

