<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: recipr.xml,v 1.21 2018/06/03 20:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>reciprok</rad>/a
</kap>
<drv mrk="recipr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Adj. kvalifikanta la agadon, senton a&ubreve; efikon de du
      personoj a&ubreve;
      objektoj, el kiuj unu agas, sentas a&ubreve; efikas sur la duan
      tute
      same,
      kiel la dua agas, sentas a&ubreve; efikas sur la unuan:
      <ekz>
        <tld/>a amo, konfido, abomeno, donaco.
      </ekz>
     </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_a;&c_je;&c_m;&c_n;&c_y;</trd>
  <trd lng="ca">rec&iacute;proc</trd>
  <trdgrp lng="cs">
    <trd>reciprok&yacute;</trd>,
    <trd>recipro&ccaron;n&iacute;</trd>,
    <trd>vz&aacute;jemn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>reziprok</trd>,
    <trd>gegenseitig</trd>
  </trdgrp>
  <trd lng="en">reciprocal</trd>
  <trd lng="es">rec&iacute;proco</trd>
  <trd lng="fr">r&eacute;ciproque<klr> (adj.)</klr></trd>
  <trd lng="hu">k&ouml;lcs&ouml;n&ouml;s</trd>
  <trdgrp lng="nl">
    <trd>wederkerig</trd>,
    <trd>onderling</trd>,
    <trd>wederzijds</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>recipro&ccaron;n&yacute;</trd>,
    <trd>vz&aacute;jomn&yacute;</trd>
  </trdgrp>
  <trd lng="zh">&#30456;&#20114;</trd>
</drv>

<drv mrk="recipr.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
    En <tld/>a maniero:
    </dif>
    <ekz>
        sin <tld/>e pardoni<klr> (=pardoni unu la alian)</klr>;
      </ekz>
      <ekz>
        &ccirc;iuj sin <tld/>e komprenas<fnt>Z</fnt><klr> (=&ccirc;iuj
        komprenas unu
        la alian)</klr>;
      </ekz>
      <ekz>
        premi al si <tld/>e la manojn<klr> (=premi inter si la
        manojn)</klr><fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>e</ind> batalantaj nacioj<klr> (=inter si batalantaj
        nacioj)</klr><fnt>Z</fnt>.
        <trd lng="be">&c_u;&c_z;&c_a;&c_je;&c_m;&c_n;&c_a;</trd>
        <trd lng="ca">rec&iacute;procament</trd>
        <trd lng="hu">k&ouml;lcs&ouml;n&ouml;sen</trd>
        <trd lng="ru">&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_o;</trd>
      </ekz>
  </snc>
  <ref tip="vid" cel="si.0">si</ref>
  <trd lng="fr">r&eacute;ciproquement</trd>
  <trdgrp lng="nl">
    <trd>onderling</trd>,
    <trd>over en weer</trd>,
    <trd>onder elkaar</trd>
  </trdgrp>
  <trd lng="sk">navz&aacute;jom</trd>
  <trd lng="cs">navz&aacute;jem</trd>
</drv>

<drv mrk="recipr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="R"/>e plenumi:
      <ekz>
        mi <tld/>as viajn bondezirojn;
      </ekz>
      <ekz>
        <tld/>i saluton.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="inter">inter</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol;
    <ind>&c_u;&c_z;&c_a;&c_je;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;</ind></trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_u;
    &c_a;&c_d;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
  <trd lng="ca">correspondre</trd>
  <trdgrp lng="cs">
    <trd>oplatit</trd>,
    <trd>op&ecaron;tovat</trd>
  </trdgrp>
  <trd lng="de">erwidern</trd>
  <trd lng="en">reciprocate</trd>
  <trdgrp lng="es">
    <trd>corresponder<klr> (actuar con reciprocidad)</klr></trd>
  </trdgrp>
  <trd lng="fr">r&eacute;ciproquer</trd>
  <trd lng="hu">viszonoz</trd>
  <trdgrp lng="nl">
    <trd><klr tip="amb">(iets) </klr>terugdoen</trd>,
    <trd><klr tip="amb">(iets) </klr>beantwoorden</trd>,
    <trd>vergelden</trd>
  </trdgrp>
  <trd lng="sk">op&auml;tova&tcaron;</trd>
</drv>
</art>
