<art mrk="$Id: leksikograf.xml,v 1.11 2022/05/01 12:04:34 revo Exp $">
<kap>
  <rad>leksikograf</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="leksikograf.0o">
  <kap><tld/>o</kap>
  <snc mrk="leksikograf.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="vort.0aristo.LIN">Vortaristo</ref>:
      <ekz>
        &leftquot;arobo&rightquot; <klr>[&#x2026;]</klr> estis preteratentita de <tld/>oj dum
        preska&ubreve;
        sep jardekoj
        <fnt>
          <aut>Bernard Golden</aut>,
          <vrk>Konsiloj por evoluigo de la Esperanta vortprovizo, tria
          parto</vrk>,
          <lok>Eventoj, numero 104a, 1996-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        la laboro de niaj terminografoj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2005/008650.php">Por pli kompleta lingvo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_k;&c_s;&c_y;&c_k;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="cs">
    <trd>lexikograf</trd>,
    <trd>slovn&iacute;k&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Lexikograph</trd>
  <trd lng="fr">lexicographe</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;t&aacute;r&iacute;r&oacute;</trd>,
    <trd>lexikogr&aacute;fus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lexicograaf</trd>,
    <trd>woordenaar</trd>
  </trdgrp>
  <trd lng="pl">leksykograf</trd>
  <trdgrp lng="pt">
    <trd>lexic&oacute;grafo</trd>,
    <trd>dicionarista</trd>
  </trdgrp>
  <trd lng="ro">lexicograf</trd>
  <trd lng="sk">slovnik&aacute;r</trd>
  <trd lng="uk">&#x43B;&#x435;&#x43A;&#x441;&#x438;&#x43A;&#x43E;&#x433;&#x440;&#x430;&#x444;</trd>
</drv>
