<art mrk="$Id: subjunkci.xml,v 1.9 2022/03/29 14:19:46 revo Exp $">
<kap>
  <rad>subjunkci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="subjunkci.0o">
  <kap><tld/>o</kap>
  <snc mrk="subjunkci.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="ord.sub0igilo">Subordigilo</ref>.
      <ekz>
        la parolanto elektus konjunkcion a&ubreve; <tld/>on
        <fnt>
        <aut>P. Blondiau</aut>,
        <vrk>Re: Esperanto estas malviva lingvo ?</vrk>,
        <lok>soc.culture.esperanto, 1999-07-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">spojka pod&rcaron;ad&iacute;c&iacute;</trd>
  <trdgrp lng="de">
    <trd>unterordnende Konjunktion</trd>,
    <trd>unterordnendes Bindewort</trd>
  </trdgrp>
  <trd lng="en">subordinating <ind>conjunction</ind></trd>
  <trdgrp lng="es">
    <trd>conjunci&oacute;n subordinante</trd>,
    <trd>subjunci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">subjonction</trd>
  <trd lng="hu">al&aacute;rendel&odblac; <ind>k&ouml;t&odblac;sz&oacute;</ind></trd>
  <trdgrp lng="nl">
    <trd>ondergeschikte zin</trd>,
    <trd>onderschikkend <ind>voegwoord</ind></trd>
  </trdgrp>
  <trd lng="pt">conjun&ccedil;&atilde;o subordinada</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_o;&c_ju;&c_z;</trd>
  <trd lng="sk">spojka podra&dcaron;ovacia</trd>
  <trd lng="uk">&#x43F;&#x456;&#x434;&#x440;&#x44F;&#x434;&#x43D;&#x438;&#x439; &#x441;&#x43F;&#x43E;&#x43B;&#x443;&#x447;&#x43D;&#x438;&#x43A;</trd>
</drv>
