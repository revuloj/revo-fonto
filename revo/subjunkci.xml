<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>subjunkci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="subjunkci.0o">
  <kap><tld/>o</kap>
  <snc mrk="subjunkci.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <ref tip="dif" cel="konjun.subordiga0o">subordiga konjunkcio</ref>
    <ekz>
      la parolanto elektus konjunkcion au <tld/>on
      <fnt>
      <aut>P. Blondiau</aut>,
      <vrk>Re: Esperanto estas malviva lingvo ?</vrk>,
      <lok>soc.culture.esperanto, 1999-07-16</lok>
      </fnt>.
    </ekz>
    <rim mrk="subjunkci.0o.RIM_DIF">
      Iuj lingvosciencaj skoloj radikale distingas inter <em>kunordiga
      konjunkcio</em> kaj <em>subordiga konjunkcio</em>. Ili nomas ilin
      respektive <em>konjunkcio</em> kaj <em>subjunkcio</em>.
    </rim>
  </snc>
  <trd lng="fr">subjonction</trd>
  <trd lng="hu">al&aacute;rendel&odblac; k&ouml;t&odblac;sz&oacute;</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_s;&c_o;&c_ju;&c_z;</ind></trd>
</drv>
</art>

