<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konjunkci</rad>/o
</kap>
<drv mrk="konjun.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Senfleksia vorto uzata por kunigi vortojn, propoziciojn a&ubreve; frazojn.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Konjunktion</trd>,
     <trd>Bindewort</trd>
  </trdgrp>
  <trd lng="en">conjunction</trd>
  <trd lng="es">conjunci&oacute;n</trd>
  <trd lng="nl">voegwoord</trd>
  <trd lng="ru">&c_s;&c_o;&c_ju;&c_z;</trd>
</drv>
<drv mrk="konjun.kunordiga0o">
  <kap>
    kunordiga <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o, kiu kunigas samfunkciajn elementojn, ekz.
      <ekz>
        kaj, sed, a&ubreve;
      </ekz>
      kc.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>beiordnende Konjunktion</trd>,
     <trd>beiordnendes Bindewort</trd>
  </trdgrp>
  <trd lng="en">coordinating <ind>conjunction</ind></trd>
  <trd lng="nl">nevenschikkend <ind>voegwoord</ind></trd>
</drv>
<drv mrk="konjun.subordiga0o">
  <kap>
    subordiga <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o, kiu dependigas unu propozicion de alia, ekz.
      <ekz>
        ke, kvankam, se
      </ekz>
      kc.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>unterordnende Konjunktion</trd>,
     <trd>unterordnendes Bindewort</trd>
  </trdgrp>
  <trd lng="en">subordinating <ind>conjunction</ind></trd>
  <trd lng="nl">onderschikkend <ind>voegwoord</ind></trd>
</drv>
</art>
