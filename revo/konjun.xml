<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>konjunkci</rad>/o
    </kap>

    <drv mrk="konjun.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="konjun.0o.GRA">
        <uzo tip="fak">GRA</uzo>
        <dif>
          Senfleksia vorto uzata por kunigi vortojn,
          propoziciojn a&ubreve; frazojn.
        </dif>
        <trd lng="be">&c_z;&c_l;&c_u;&c_ch;&c_n;&c_ib;&c_k;</trd>
        <trdgrp lng="de">
          <trd>Konjunktion</trd>,
          <trd>Bindewort</trd>
        </trdgrp>
        <trd lng="en">conjunction</trd>
        <trd lng="es">conjunci&oacute;n</trd>
        <trd lng="nl">voegwoord</trd>
        <trd lng="pl">sp&oacute;jnik</trd>
        <trd lng="ru">&c_s;&c_o;&c_ju;&c_z;</trd>
      </snc>

      <snc mrk="konjun.0o.logika">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>MatVort</bib></fnt>
        <dif>
          <ref tip="super" cel="operac.logika0o">Logika operacio</ref>,
          kiu al du <ref cel="propoz.0o.aserto">propozicioj</ref>
          asocias la propozicion, kiu estas vera, se kaj nur se
          amba&ubreve; estas veraj; rezulto de tiu operacio:
          <ekz>
            la <tld/>on de <frm>P</frm> kaj <frm>Q</frm> oni foje signas per 
            <frm>P&#8743;Q</frm> <klr>(legu: po kaj kuo)</klr>.
          </ekz>
        </dif>
        <bld tip="svg" lok="../bld/t_konjunkcio.svg" lrg="150" alt="150">
          vertabelo de <tld/>o
        </bld>
        <rim>
          Pli esperanteca, tradicia kaj nemalkohera termino estas
          <ref tip="sin" cel="kaj.0o">kajo</ref>.  La uzo de la vorto
          <tld/>o por nomi nur unu
          <ref tip="super" cel="lig.0o.MAT">ligilon<sncref/></ref> sekvas
          mistradicion latinidan, kaj estas logike difekta en lingvoj,
          kiuj per la sama nomo signas la pli &gcirc;eneralan koncepton
          gramatikan (kaj Esperanto estas tia lingvo, &ccirc;ar la gramatika
          senco de la vorto <tld/>o estas por &gcirc;i fundamenta &dash; dum la
          uzo por nomi logikan operacion, tute fre&scirc;data: PIV1 &gcirc;in ne
          konas).  Ekz-e <ctl>kaj</ctl>, <ctl>a&ubreve;</ctl>, <ctl>se</ctl>
          estas <tld/>oj<sncref ref="konjun.0o.GRA"/> &dash; sed ial
          (kial?!) nur <ctl>kaj</ctl> estas <tld/>o<sncref
            ref="konjun.0o.logika"/>.  Kiel pruvis diskuto en
          soc.culture.esperanto, iuj latinidlingvaj esperantistoj
          efektive konfuzas la sencon fundamentan kun la speciala
          senco matematika.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_ju;&c_n;&c_k;&c_c;&c_y;&c_ja;</trd>
        <trdgrp lng="de">
          <trd>Konjunktion</trd>,
          <trd>logisches <ind>Produkt</ind></trd></trdgrp> 
        <trd lng="en">conjunction</trd>
        <trdgrp lng="fr">
          <trd>conjonction<klr tip="ind"> (produit logique)</klr></trd>,
          <trd>produit logique</trd></trdgrp> 
        <trdgrp lng="pl">
          <trd>koniunkcja</trd>,
          <trd>iloczyn logiczny</trd></trdgrp> 
        <trdgrp lng="ru">
          <trd>&c_k;&c_o;&c_n;&c_malmol;&c_ju;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>,
          <trd>&c_o;&c_p;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja; <ind>&c_I;</ind></trd>,
          <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
            <ind>&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
        </trdgrp> 
      </snc>
    </drv>

    <drv mrk="konjun.kunordiga0o">
      <kap>kunordiga <tld/>o</kap>
      <snc>
        <dif>
          <tld lit="K"/>o, kiu kunigas samfunkciajn elementojn,
          ekzemple
          <ekz>
            kaj, sed, a&ubreve;
          </ekz>
          kc.
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>beiordnende Konjunktion</trd>,
        <trd>beiordnendes Bindewort</trd>
      </trdgrp>
      <trd lng="en">coordinating <ind>conjunction</ind></trd>
      <trd lng="nl">nevenschikkend <ind>voegwoord</ind></trd>
      <trd lng="ru">&c_s;&c_o;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
        &c_s;&c_o;&c_ju;&c_z;</trd>
    </drv>

    <drv mrk="konjun.subordiga0o">
      <kap>subordiga <tld/>o</kap>
      <snc>
        <dif>
          <tld lit="K"/>o, kiu dependigas unu propozicion de alia,
          ekzemple
          <ekz>
            ke, kvankam, se
          </ekz>
          kc.
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>unterordnende Konjunktion</trd>,
        <trd>unterordnendes Bindewort</trd>
      </trdgrp>
      <trd lng="en">subordinating <ind>conjunction</ind></trd>
      <trd lng="nl">onderschikkend <ind>voegwoord</ind></trd>
      <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
        &c_s;&c_o;&c_ju;&c_z;</trd>
    </drv>

    <drv mrk="konjun.disiga0o">
      <kap>disiga <tld/>o</kap>
      <snc>
        <dif>
          Kunordiga <tld/>o, esprimanta tian rilaton inter
          la kunigataj partoj, ke eblas ilia disigo a&ubreve; alternado;
          ekzemple
          <ekz>
            a&ubreve;...a&ubreve;, jen...jen.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">disjunctive conjunction</trd>
      <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_s;&c_o;&c_ju;&c_z;</trd>
    </drv>

    <drv mrk="konjun.kontrauxmeta0o">
      <kap>kontra&ubreve;meta <tld/>o</kap>
      <snc>
        <dif>
          Kunordiga <tld/>o, interliganta tiajn partojn, kiuj
          aperas kiel kontra&ubreve;oj; ekzemple
          <ekz>
            sed, tamen.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">adversative conjunction</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_s;&c_o;&c_ju;&c_z;</trd>
    </drv>

    <drv mrk="konjun.korelativaj0oj">
      <kap>korelativaj <tld/>oj</kap>
      <snc>
        <dif>
          Paraj <tld/>oj; ekzemple
          <ekz>
            ju ... des, nek ... nek.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">adversative conjunction</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_s;&c_o;&c_ju;&c_z;</trd>
    </drv>

  </art>
