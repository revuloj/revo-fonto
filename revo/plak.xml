<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plak.xml,v 1.22 2025/03/07 18:48:48 revo Exp $">
<kap>
  <rad>plak</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="plak.0o">
  <kap><tld/>o</kap>
  <snc mrk="plak.0o.FOT">
  <uzo tip="fak">FOT</uzo>
    <dif>
      Fotografa <ref tip="super" cel="plat.0o">plato</ref>, iam konsistanta el vitro, poste el
      metalo,
      sursternita per impresi&gcirc;ema substanco:
      <ekz>
        <ind>pres<tld/>o</ind>.
        <trd lng="hu">nyom&oacute;lemez</trd>
        <trd lng="nl">drukplaat</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_o;&c_t;&c_a;&c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_k;&c_a;</trd>
    <trdgrp lng="cs">
      <trd>d&yacute;hov&aacute;n&iacute;</trd>,
      <trd>forn&yacute;rov&aacute;n&iacute;</trd>,
      <trd>oblo&zcaron;en&iacute;</trd>
    </trdgrp>
    <trd lng="de">Fotoplatte</trd>
    <trd lng="fr">plaque<klr> (photographique)</klr></trd>
    <trd lng="hu">fotolemez</trd>
    <trdgrp lng="ja">
      <trd>&#x677F;  <pr>&#x3044;&#x305F;</pr></trd>,
      <trd>&#x611F;&#x5149;&#x677F;  <pr>&#x304B;&#x3093;&#x3053;&#x3046;&#x3070;&#x3093;</pr></trd>,
      <trd>&#x4E7E;&#x677F;  <pr>&#x304B;&#x3093;&#x3071;&#x3093;</pr></trd>
    </trdgrp>
    <trd lng="nl">plaat</trd>
    <trd lng="pl">p&lstroke;yta fotograficzna</trd>
    <trd lng="pt">chapa</trd>
    <trd lng="ro">plac&abreve; fotografic&abreve;</trd>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a; <klr>(&c_f;&c_o;&c_t;&c_o;)</klr></trd>
    <trdgrp lng="sk">
      <trd>krvn&aacute; do&scaron;ti&ccaron;ka</trd>,
      <trd>plak</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_h;&c_u;&c_d;&c_o;&c_zh;&c_n;&c_jeu; &c_f;&c_a;&c_n;&c_je;&c_r;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_k;&c_je;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a; (&c_u; &c_m;&c_o;&c_l;&c_ju;&c_s;&c_k;&c_ib;&c_v; &c_ib;
      &c_t;.&c_p;.)</trd>
    </trdgrp>
    <trd lng="zh">&#x5E73;&#x7248; <pr>p&iacute;ngb&#x1CE;n</pr></trd>
  </snc>
  <snc mrk="plak.0o.tabulo">
    <dif>
      <ref tip="super" cel="tabul.0o">Tabulo</ref> reliefa a&ubreve; gravurita
      kaj muntita sur muro, monumento...:
      <ekz>
        metalaj plakoj kun la surskribo &leftquot;Esperanto <klr>[&#x2026;]</klr>&rightquot; <klr>[&#x2026;]</klr>,
        instalitaj &ccirc;e la &ccirc;efaj enirvojoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;5</lok>
        </fnt>;
      </ekz>
      <ekz>
        bronzaj <tld/>a&jcirc;oj <klr>[&#x2026;]</klr> ornamis palacon en <nom>Oba</nom> (Benino)
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kahel.0o">kahelo</ref>,
      <ref cel="relief.0o.BELA">reliefo<sncref/></ref>,
      <ref cel="sxild.0o.KOMUNE">&scirc;ildo<sncref/></ref>
    </refgrp>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_l;&c_mol;&c_d;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_b;&c_l;&c_ib;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_n;&c_a; &c_s;&c_mol;&c_c;&c_ja;&c_n;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Plakette</trd>,
    <trd>Platte <klr>(Plakette)</klr></trd>
  </trdgrp>
  </snc>
</drv>
</art>
