<drv mrk="plak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Fotografa plato, iam konsistanta el vitro, nun el metalo,
      sursternita per impresema substanco:
      <ekz>
        <ind>pres<tld/>o</ind>.
        <trd lng="hu">nyom&oacute;lemez</trd>
        <trd lng="nl">drukplaat</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Platte</trd>
  <trd lng="hu">fotolemez</trd>
  <trd lng="nl">plaat</trd>
  <trd lng="pt">chapa</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a; <klr>(&c_f;&c_o;&c_t;&c_o;)</klr></trd>
</drv>
</art>
