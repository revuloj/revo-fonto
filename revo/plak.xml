<kap>
  <rad>plak</rad>/o
</kap>
<drv mrk="plak.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">FOT</uzo>
    <dif>
      Fotografa plato, iam konsistanta el vitro, nun el metalo,
      sursternita per impresema substanco:
      <ekz>
        <ind>pres<tld/>o</ind>.
        <trd lng="hu">nyom&oacute;lemez</trd>
        <trd lng="nl">drukplaat</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_t;&c_a;&c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>d&#xFD;hov&#xE1;n&#xED;</trd>,
    <trd>forn&#xFD;rov&#xE1;n&#xED;</trd>,
    <trd>oblo&#x17E;en&#xED;</trd>
  </trdgrp>
  <trd lng="de">Platte</trd>
  <trd lng="fr">plaque<klr> (photographique)</klr></trd>
  <trd lng="hu">fotolemez</trd>
  <trdgrp lng="ja">
    <trd>&#x677F;  [<ind>&#x3044;&#x305F;</ind>]</trd>,
    <trd>&#x611F;&#x5149;&#x677F;  [<ind>&#x304B;&#x3093;&#x3053;&#x3046;&#x3070;&#x3093;</ind>]</trd>,
    <trd>&#x4E7E;&#x677F;  [<ind>&#x304B;&#x3093;&#x3071;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">plaat</trd>
  <trd lng="pl">p&lstroke;yta fotograficzna</trd>
  <trd lng="pt">chapa</trd>
  <trd lng="ro">plac&abreve; fotografic&abreve;</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a; <klr>(&c_f;&c_o;&c_t;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>krvn&#xE1; do&scaron;ti&#x10D;ka</trd>,
    <trd>plak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_u;&c_d;&#x43E;&c_zh;&#x43D;&c_jeu; &c_f;&c_a;&#x43D;&c_je;&c_r;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43B;&c_a;&#x43A;&c_je;</trd>,
    <trd>&#x43F;&#x43B;&c_a;&c_s;&c_t;&c_i;&#x43D;&#x43A;&c_a; (&c_u; &#x43C;&#x43E;&#x43B;&#x44E;&c_s;&#x43A;&c_ib;&c_v; &c_ib;
    &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
