<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plak.xml,v 1.13 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>plak</rad>/o
</kap>
<drv mrk="plak.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">FOT</uzo>
    <dif>
      Fotografa plato, iam konsistanta el vitro, nun el metalo,
      sursternita per impresema substanco:
      <ekz>
        <ind>pres<tld/>o</ind>.
        <trd lng="hu">nyom&oacute;lemez</trd>
        <trd lng="nl">drukplaat</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dýhování</trd>,
    <trd>fornýrování</trd>,
    <trd>obložení</trd>
  </trdgrp>
  <trd lng="de">Platte</trd>
  <trd lng="fr">plaque<klr> (photographique)</klr></trd>
  <trd lng="hu">fotolemez</trd>
  <trd lng="nl">plaat</trd>
  <trd lng="pt">chapa</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a; <klr>(&c_f;&c_o;&c_t;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>krvná doštička</trd>,
    <trd>plak</trd>
  </trdgrp>
</drv>
</art>
