<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: maskulen.xml,v 1.4 2024/04/11 21:22:11 revo Exp $">
<kap>
  <rad>maskulen</rad>/o
  <fnt>
    <aut>Christian Declerck</aut>,
    <vrk><url
    ref="http://universala-esperanto.net/index_htm_files/Nova%20Nomenklaturo%20por%20Lingviko%20-%20Recenzo.pdf">&Ccirc;u
    perdite inter fundamenta kaj plena gramatikoj?</url></vrk>
  </fnt>
</kap>
<drv mrk="maskulen.0o">
  <kap><tld/>o, <var><kap><tld/>a genro</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="maskulen.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="genr.vir0o">Virgenro</ref>:
      <ekz>
        la nord&gcirc;ermanaj lingvoj montras sekundaran kuni&gcirc;on de la <tld/>a
        kaj feminena al komuna genro
        <fnt>
          <aut>Cyril Brosch</aut>:
          <vrk>La hitita lingvo: &Ccirc;u &rightquot;filino&#8221; a&ubreve; &rightquot;fratino&#8221; de la
          prahinde&ubreve;ropa?</vrk>.
          <lok><url
          ref="&STEB;miksitaj/iku2015.pdf">Internacia
          Kongresa Universitato</url>, 2015, ISBN
          978-92-9017-126-3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="feminen.0o">femineno</ref>
  <trd lng="be">&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_ib; <ind>&c_r;&c_o;&c_d;</ind></trd>
  <trd lng="de">m&auml;nnliches Geschlecht</trd>
  <trd lng="en">masculine gender</trd>
  <trd lng="es">g&eacute;nero masculino</trd>
  <trd lng="fr">genre masculin</trd>
  <trd lng="hu">h&iacute;mnem</trd>
  <trd lng="it">maschile<klr> (gram)</klr></trd>
  <trd lng="nl">mannelijk <ind>geslacht</ind></trd>
  <trd lng="pl">rodzaj <ind>m&eogonek;ski</ind></trd>
  <trd lng="ro">gen <ind>masculin</ind></trd>
  <trd lng="ru">&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j; &c_r;&c_o;&c_d;</trd>
</drv>
</art>

