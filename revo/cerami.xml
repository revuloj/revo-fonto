<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cerami.xml,v 1.41 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>ceramik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cerami.0o">
  <kap><tld/>o</kap>
  <snc mrk="cerami.0o.arto">
    <dif>
      <ref tip="lst" cel="art.0o" lst="voko:artoj">Arto</ref>
      baki teron por fari potojn a&ubreve; kahelojn:
      <ekz>
        la &ccirc;ina <tld/>o havas historion de 10 mil jaroj
        <fnt>
          <bib>EncCxi</bib>,
          <lok>&ccirc;ap. &leftquot;Ceramiko&rightquot;, p. 209a</lok>
         </fnt>;
      </ekz>
      <ekz>
        litova majstrino de la nigra <tld/>o
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
            <vrk><url ref="&Monato;/2000/007061.php">Ceramika&jcirc;oj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 24</lok>
          </fnt>.
      </ekz>
    </dif>  
    <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a;
      <klr>(&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;
        &c_v;&c_y;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    <trd lng="br">prierezh</trd>
    <trd lng="ca">cer&agrave;mica</trd>
    <trd lng="cs">keramika <klr>(tvorba)</klr></trd>
    <trd lng="de">T&ouml;pferkunst</trd>
    <trdgrp lng="en">
      <trd>ceramics</trd>,
      <trd>pottery</trd>
    </trdgrp>
    <trd lng="es">cer&aacute;mica</trd>
    <trd lng="fr">c&eacute;ramique<klr> (technique)</klr></trd>
    <trd lng="he">&qof;&#x5B5;&resh;&#x5B8;&mem;&#x5B4;&yod;&qof;&#x5B8;&he;</trd>
    <trdgrp lng="hu">
      <trd>agyagm&udblac;vess&eacute;g</trd>,
      <trd>keramika</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>keramik</trd>,
      <trd>tembikar</trd>
    </trdgrp>
    <trd lng="it">ceramica<klr> (tecnica)</klr></trd>
    <trd lng="pl">ceramika</trd>
    <trd lng="pt">cer&acirc;mica</trd>
    <trd lng="ru">&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>
    <trdgrp lng="sk">
      <trd>keramika</trd>,
      <trd>keramik&aacute;rske umeleck&eacute; remeslo</trd>
    </trdgrp>
  </snc>
  <snc mrk="cerami.0o.materialo">
    <dif>
      La <ref tip="super" cel="materi1.0o.substanco">materialo</ref>, kiu konsistigas la tiel
      produktatajn objektojn:
      <ekz>
        la materialo de la fluto estis iuspeca <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Hans B. Welling</aut>
          <vrk><url ref="&Monato;/2014/011331.php">Nesuspektindaj flutistoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        por en&ccirc;ambra bonsajo oni elektas malprofundan ujon el <tld/>o a&ubreve; plasto
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2007/009228.php">Kultivi bonsajon</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tupforma <tld/>a tombo&scirc;tono de <nom>Kusam ibn Abbas</nom> havas kvar tavolojn,
        kovritajn per reliefaj majolikaj kaheloj kun tre ri&ccirc;a plant-ornamo
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la teston oni ju&gcirc;is kaj &ccirc;iu gajnis valoran premion, ekzemple mi hejmen forportis
        <tld/>an anason, al <nom>Kim</nom> rezultis pli bone, li ricevis <tld/>an pokalon
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;u en kuirejo a&ubreve; sur podio? Kim mojosas &ccirc;ie!</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a; <klr>(&c_m;&c_a;&c_t;&c_a;&c_r;'&c_ja;&c_l;)</klr></trd>
    <trd lng="de">Keramik</trd>
  </snc>
  <trd lng="ja">&#x88FD;&#x9676;  <pr>&#x305B;&#x3044;&#x3068;&#x3046;</pr></trd>
  <trd lng="uk">&#x43A;&c_je;&c_r;&c_a;&#x43C;&c_ib;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x530B; <pr>t&#xE1;o</pr></trd>,
    <trd>&#x9676; <pr>t&#xE1;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cerami.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="cerami.0ajxo.VAZ">
    <dif>
      Objekto el bakita tero:
      <ekz>
        artistoj ekspoziciis <klr>[&#x2026;]</klr> <tld/>a&jcirc;ojn
        kaj ligna&jcirc;ojn
        <fnt>
          <aut>Last</aut>,
          <vrk>Bruas la mezepoko</vrk>,
          <bib>Monato</bib>,
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        homaj kaj bestaj figuroj sur koloraj <tld/>a&jcirc;oj
        <fnt>
          <bib>Kon11</bib>
          <lok>El &Ccirc;inio</lok>
        </fnt>;
      </ekz>
      <ekz>
        kolkotaro <klr>[&#x2026;]</klr> estis uzata kiel glazuro por <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2007/009114.php">Vizito al Takaha&scirc;i</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argil.0ajxo">argila&jcirc;o</ref>,
      <ref cel="fajenc.0ajxo">fajenca&jcirc;o</ref>,
      <ref cel="porcel.0ajxo">porcelana&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a;
    <klr>(&c_v;&c_y;&c_r;&c_a;&c_b;)</klr></trd>
  <trd lng="br">priaj<klr tip="ind"> (traezenn)</klr></trd>
  <trd lng="de">Keramik</trd>
  <trd lng="ca">cer&agrave;mica</trd>
  <trdgrp lng="en">
    <trd>ceramic object</trd>,
    <trd>ceramic art</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&eacute;ramique<klr> (objet)</klr></trd>
  </trdgrp>
  <trd lng="he">&qof;&#x5B5;&resh;&#x5B8;&mem;&#x5B4;&yod;&qof;&#x5B8;&he;</trd>
  <trdgrp lng="hu">
    <trd>ker&aacute;mia</trd>,
    <trd>agyaged&eacute;ny</trd>
  </trdgrp>
  <trd lng="id">keramik</trd>
  <trd lng="it">ceramica<klr> (oggetto)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x9676;&#x78C1;&#x5668;  <pr>&#x3068;&#x3046;&#x3058;&#x304D;</pr></trd>,
    <trd>&#x30BB;&#x30E9;&#x30DF;&#x30C3;&#x30AF;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ceramika</trd>,
    <trd>wyr&oacute;b z <ind>ceramiki</ind></trd>
  </trdgrp>
  <trd lng="pt">cer&acirc;mica</trd>
  <trd lng="es">cer&aacute;mica</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">keramika</trd>
  <trd lng="uk">&#x43A;&c_je;&c_r;&c_a;&#x43C;&c_ib;&c_ch;&#x43D;&c_i;&c_j; &c_v;&c_i;&c_r;&c_ib;&c_b;</trd>
  <trdgrp lng="zh">
    <trd>&#x9676; <pr>t&#xE1;o</pr></trd>,
    <trd>&#x9676;&#x74F7; <pr>t&#xE1;oc&#xED;</pr></trd>,
    <trd>&#x9676;&#x5668; <pr>t&#xE1;oq&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cerami.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="cerami.0isto.VAZ">
    <dif>
      Faristo de <tld/>a&jcirc;oj:
      <ekz>
        <tld/>istoj <klr>[&#x2026;]</klr> man&gcirc;ante anta&ubreve; la rado dum ili <tld/>as la&ubreve;dezire
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>;
      </ekz>
      <ekz>
        dek kvin bildpa&gcirc;oj <klr>[&#x2026;]</klr> montrantaj &ccirc;efverkojn de
        &ccirc;inaj <tld/>istoj
        <fnt>
          <aut>B. Golden</aut>,
          <vrk>&Ccirc;ina ceramika historio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1995a, numero 8a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pot.0faristo.VAZ">potfaristo</ref>
    <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_s;&c_t;</trd>
    <trd lng="br">poder</trd>
    <trd lng="ca">ceramista</trd>
    <trd lng="de">T&ouml;pfer</trd>
    <trdgrp lng="en">
      <trd>ceramist</trd>,
      <trd>ceramic artist</trd>,
      <trd>potter</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>c&eacute;ramiste</trd>,
      <trd>potier</trd>
    </trdgrp>
    <trd lng="id"><ind>tukang</ind> keramik</trd>
    <trd lng="it">ceramista</trd>
    <trd lng="pl">ceramik</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_s;&c_t;</trd>
    </trdgrp>
    <trd lng="zh">&#x9676;&#x5DE5; <pr>t&#xE1;og&#x14D;ng</pr></trd>
</snc>
  <trd lng="cs">keramik</trd>
  <trdgrp lng="ja">
    <trd>&#x88FD;&#x9676;&#x696D;&#x8005;  <pr>&#x305B;&#x3044;&#x9676;&#x696D;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x9676;&#x5DE5;  <pr>&#x3068;&#x3046;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="sk">keramik</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_je;&c_r;&c_a;&#x43C;&c_ib;&#x43A;</trd>,
    <trd>&#x43A;&c_je;&c_r;&c_a;&#x43C;&c_ib;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="cerami.sxton0o">
  <kap>&scirc;ton<tld/>o, <var><kap>&scirc;ton<tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="cerami.sxton0o.VAZ">
    <dif>
      Tre malmola
      <ref tip="super" cel="cerami.0ajxo.VAZ"><tld/>a&jcirc;o</ref>,
      bakita je pli ol 1200 &deg;C
      (sufi&ccirc;e por formi&gcirc;o de vitreca glazuro):
      <ekz>
        la plej bona &scirc;ton<tld/>o estis fabrikata en <nom>Annaberg</nom>,
        <nom>Siegburg</nom> a&ubreve; <nom>Nassau</nom> kaj <nom>Kreussen</nom>
        <fnt>
          <aut>V. T&uuml;rk</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/tuurq/189251">
          Kastelo Kratochv&iacute;le &#x2010; &ccirc;i&ccirc;erona teksto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a; &c_v;&c_y;&c_s;&c_o;&c_k;&c_a;&c_t;&c_e;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a; &c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">gres</trd>
  <trd lng="cs">kamenina</trd>
  <trd lng="de">Steinzeug</trd>
  <trd lng="en">stoneware</trd>
  <trd lng="fr">gr&egrave;s<klr> (c&eacute;ramique)</klr></trd>
  <trd lng="id"><ind>periuk</ind> belanga</trd>
  <trd lng="ja">&#x305B;&#x3063;&#x5668;  <pr>&#x305B;&#x3063;&#x304D;</pr></trd>
  <trd lng="pl">kamionka</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_k;&c_a;<klr> (&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="zh">&#x70BB;&#x5668; <pr>sh&#xED;q&#xEC;</pr></trd>
</drv>
</art>
