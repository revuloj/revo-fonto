<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>ceramik</rad>/o
</kap>

<drv mrk="cerami.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="art.0o">Arto</ref>
      baki teron por fari potojn a&ubreve; kahelojn:
      <ekz>
        la &ccirc;ina <tld/>o havas historion de 10 000
        jaroj<fnt><bib>EncCxi</bib> <lok>&ccirc;ap.
        &leftquot;Ceramiko&rightquot;, p.
        209a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a;
  <klr>(&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;
  &c_v;&c_y;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trd lng="br">prierezh</trd>
  <trd lng="ca">cer&agrave;mica</trd>
  <trd lng="de">T&ouml;pferkunst</trd>
  <trdgrp lng="en">
    <trd>ceramics</trd>,
    <trd>pottery</trd>
  </trdgrp>
  <trd lng="es">cer&aacute;mica</trd>
  <trd lng="fr">c&eacute;ramique<klr> (technique)</klr></trd>
  <trdgrp lng="hu">
    <trd>agyagm&udblac;vess&eacute;g</trd>,
    <trd>keramika</trd>
  </trdgrp>
  <trd lng="it">ceramica<klr> (tecnica)</klr></trd>
  <trd lng="pl">ceramika</trd>
  <trd lng="pt">cer&acirc;mica</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">keramika</trd>
</drv>

<drv mrk="cerami.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Objekto el bakita tero:
      <ekz>
        &gcirc;i alvoja&gcirc;is en ligna kesto kun <tld/>a&jcirc;oj el
        &Ccirc;inio<fnt><aut>Stefan Maul</aut>: <vrk>Surprizo el ligna
        kesto</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 1a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_k;&c_a;
  <klr>(&c_v;&c_y;&c_r;&c_a;&c_b;)</klr></trd>
  <trd lng="br">priaj<klr tip="ind"> (traezenn)</klr></trd>
  <trd lng="de">Keramik</trd>
  <trd lng="ca">cer&agrave;mica</trd>
  <trdgrp lng="en">
    <trd>ceramic object</trd>,
    <trd>ceramic art</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&eacute;ramique<klr> (objet)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ker&aacute;mia</trd>,
    <trd>agyaged&eacute;ny</trd>
  </trdgrp>
  <trd lng="it">ceramica<klr> (oggetto)</klr></trd>
  <trdgrp lng="pl">
    <trd>ceramika</trd>,
    <trd>wyr&oacute;b z <ind>ceramiki</ind></trd>
  </trdgrp>
  <trd lng="pt">cer&acirc;mica</trd>
  <trd lng="es">cer&aacute;mica</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">keramika</trd>
</drv>

<drv mrk="cerami.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Faristo de <tld/>a&jcirc;oj:
      <ekz>
        dek kvin bildpa&gcirc;oj [...] montrantaj &ccirc;efverkojn de
        &ccirc;inaj
        <tld/>istoj<fnt><aut>Bernard Golden</aut>: <vrk>&Ccirc;ina
        ceramika
        historio (Zhao Hongsheng, &Ccirc;ina ceramiko)</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 8a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pot.0faristo.argilo">potfaristo</ref>
  <trd lng="be">&c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_s;&c_t;</trd>
  <trd lng="br">poder</trd>
  <trd lng="ca">ceramista</trd>
  <trd lng="de">T&ouml;pfer</trd>
  <trdgrp lng="en">
    <trd>ceramist</trd>,
    <trd>ceramic artist</trd>,
    <trd>potter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&eacute;ramiste</trd>,
    <trd>potier</trd>
  </trdgrp>
  <trd lng="it">ceramista</trd>
  <trd lng="pl">ceramik</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_je;&c_r;&c_a;&c_m;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  </snc>
</drv>
</art>
