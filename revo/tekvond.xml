<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tekvond</rad>/o
</kap>
<drv mrk="tekvond.0o">
  <kap><tld/>o</kap>
  <snc mrk="tekvond.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Koredevena lukto<ref tip="super" cel="sport.0o.SPO">sporto</ref>,
      en kiu oni uzas precizajn, rapidajn man- kaj piedbatoj:
      <ekz>
        aliaj interesoj de &scirc;i estas <tld/>o, rolludoj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.tokipona.org/kioestas.html">toki pona li seme?</url></vrk>,
          <lok>[vidita je 2006-03-20]</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi planas establi pli multe da <tld/>ejoj en aliaj urboj de la
        provinco Shandong
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url
        ref="http://es.chinabroadcast.cn/1/2005/05/18/1@24661.htm">Ano de
        Korea Respubliko...</url></vrk>
        <lok>2005-05-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">taekwondo</trd>
</drv>

<drv mrk="tekvond.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="tekvond.0isto.SPO">
    <dif>
      Sportisto, kiu praktikas <tld/>on.
    </dif>
  </snc>
</drv>
</art>

