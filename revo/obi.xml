<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>obi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="obi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Japana <ref tip="super" cel="zon.0o.TEKS">zono</ref>
      por formalaj okazoj,
      el lar&gcirc;a tuko, kiun oni vindas dufoje &ccirc;irka&ubreve;
      sia talio kaj bante nodas &ccirc;e la dorso:
      <ekz>
        la <tld/>o estas tiel surmetita, ke la desegna&jcirc;o montras sin &gcirc;uste
        sidanta sur la nodego
        <fnt><bib>EncJap</bib>, <lok>artikolo &leftquot;obio&rightquot;, p. 126a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_b;&c_ib; <klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib; &c_p;&c_o;&c_ja;&c_s;)</klr></trd>
  <trd lng="de">Obi</trd>
  <trd lng="es">obi</trd>
  <trd lng="fr">obi</trd>
  <trd lng="hu">obi<klr tip="ind"> (jap&aacute;n &ouml;v></klr></trd>
  <trd lng="it">obi</trd>
  <trdgrp lng="nl">
    <trd>obi<klr tip="ind"> (Japanse gordel)</klr></trd>,
    <trd>Japanse <ind>gordel</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>obi</trd>
  </trdgrp>
</drv>
</art>
