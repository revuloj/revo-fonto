<art mrk="$Id: obi.xml,v 1.16 2023/10/20 05:22:17 revo Exp $">
<kap>
  <rad>obi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="obi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Japana <ref tip="super" cel="zon.0o.TEKS">zono</ref>
      por formalaj okazoj,
      el lar&gcirc;a tuko, kiun oni vindas dufoje &ccirc;irka&ubreve;
      sia talio kaj bante nodas &ccirc;e la dorso:
      <ekz>
        la <tld/>o estas tiel surmetita, ke la desegna&jcirc;o montras sin
        &gcirc;uste
        sidanta sur la nodego
        <fnt><bib>EncJap</bib>, <lok>artikolo &leftquot;obio&rightquot;, p.
        126a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_b;&c_ib; <klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib; &c_p;&c_o;&c_ja;&c_s;)</klr></trd>
  <trd lng="cs">obi</trd>
  <trd lng="de">Obi</trd>
  <trd lng="es">obi</trd>
  <trd lng="fr">obi</trd>
  <trd lng="hu">obi<klr tip="ind"> (jap&aacute;n &ouml;v&gt;</klr></trd>
  <trd lng="id">obi</trd>
  <trd lng="it">obi</trd>
  <trd lng="ja">&#x5E2F;  <pr>&#x305F;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>obi<klr tip="ind"> (Japanse gordel)</klr></trd>,
    <trd>Japanse <ind>gordel</ind></trd>
  </trdgrp>
  <trd lng="pl">obi  <klr>(japo&nacute;ski pas)</klr></trd>
  <trdgrp lng="pt">
   <trd>obi</trd>
  </trdgrp>
  <trd lng="ro">obi</trd>
  <trd lng="sk">obi</trd>
  <trd lng="uk">&#x43E;&#x431;&#x456; (&#x448;&#x438;&#x440;&#x43E;&#x43A;&#x438;&#x439; &#x43F;&#x43E;&#x44F;&#x441; &#x434;&#x43B;&#x44F; &#x43A;&#x456;&#x43C;&#x43E;&#x43D;&#x43E;)</trd>
</drv>
</art>
