<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>putor</rad>/o
</kap>
<drv mrk="putor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="mustel.0o">mustelo</ref>
      (<trd lng="la">Mustela putorius</trd>), kun helaj makuloj
      malanta&ubreve; la
      okuloj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_sh;&c_a;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">tcho&rcaron;</trd>
  <trd lng="de">Iltis</trd>
  <trd lng="en">polecat</trd>
  <trd lng="es">tur&oacute;n</trd>
  <trd lng="fr">putois</trd>
  <trd lng="hu">g&ouml;r&eacute;ny</trd>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_jo;&c_k;</trd>
  <trd lng="nl">bunzing</trd>
  <trdgrp lng="pt">
    <trd>put&oacute;rio</trd>,
    <trd>doninha</trd>
  </trdgrp></drv>
<drv mrk="putor.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="fur.0o">furo.</ref>
  </snc>
  <trd lng="cs">fretka</trd>
  <trd lng="de">Frettchen</trd>
  <trd lng="fr">furet<klr tip="ind"> (zoo.)</klr></trd>
  <trd lng="hu">vad&aacute;szg&ouml;r&eacute;ny</trd>
  <trd lng="nl">fret</trd>
</drv>
</art>
