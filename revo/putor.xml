<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>putor</rad>/o
</kap>
<drv mrk="putor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="mustel.0o">mustelo</ref>
      (<trd lng="la">Mustela putorius</trd>), kun helaj makuloj
      malanta&Atilde;&#133;&Acirc;&#173; la
      okuloj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&Atilde;&#145;&Acirc;&#130;&Atilde;&#145;&Acirc;&#133;&Atilde;&#144;&Acirc;&#190;&Atilde;&#145;&Acirc;&#128;</trd>,
    <trd>&Atilde;&#145;&Acirc;&#136;&Atilde;&#144;&Acirc;&deg;&Atilde;&#145;&Acirc;&#136;&Atilde;&#144;&Acirc;&#190;&Atilde;&#144;&Acirc;&#186;</trd>
  </trdgrp>
  <trd lng="cs">tcho&Atilde;&#133;&Acirc;&#153;</trd>
  <trd lng="de">Iltis</trd>
  <trd lng="en">polecat</trd>
  <trd lng="es">tur&Atilde;&#131;&Acirc;&cubic;n</trd>
  <trd lng="fr">putois</trd>
  <trd lng="hu">g&Atilde;&#131;&Acirc;&para;r&Atilde;&#131;&Acirc;&#169;ny</trd>
  <trd lng="ru">&Atilde;&#145;&Acirc;&#133;&Atilde;&#144;&Acirc;&#190;&Atilde;&#145;&Acirc;&#128;&Atilde;&#145;&Acirc;&#145;&Atilde;&#144;&Acirc;&#186;</trd>
  <trd lng="nl">bunzing</trd>
  <trdgrp lng="pt">
    <trd>put&Atilde;&#131;&Acirc;&cubic;rio</trd>,
    <trd>doninha</trd>
  </trdgrp></drv>
<drv mrk="putor.cxas0o">
  <kap>&Atilde;&#132;&Acirc;&#137;as<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="fur.0o">furo.</ref>
  </snc>
  <trd lng="cs">fretka</trd>
  <trd lng="de">Frettchen</trd>
  <trd lng="fr">furet<klr tip="ind"> (zoo.)</klr></trd>
  <trd lng="hu">vad&Atilde;&#131;&Acirc;&#161;szg&Atilde;&#131;&Acirc;&para;r&Atilde;&#131;&Acirc;&#169;ny</trd>
  <trd lng="nl">fret</trd>
</drv>
</art>
