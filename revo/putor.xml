<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>putor</rad>/o
</kap>
<drv mrk="putor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="mustel.0o">mustelo</ref>
      (<trd lng="la">Mustela putorius</trd>), kun helaj makuloj malanta&ubreve; la
      okuloj.
    </dif>
  </snc>
  <trd lng="de">Iltis</trd>
  <trd lng="fr">putois</trd>
  <trd lng="nl">bunzing</trd>
</drv>
<drv mrk="putor.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="fur.0o">furo.</ref>
  </snc>
  <trd lng="fr">furet<klr tip="ind"> (zoo.)</klr></trd>
</drv>
</art>
