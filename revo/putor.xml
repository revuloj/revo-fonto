<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: putor.xml,v 1.34 2024/02/17 12:12:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>putor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="putor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="putor.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref> de <ref tip="super" cel="mustel.0o">mustelo</ref>; malgranda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref>
      kun helaj makuloj malanta&ubreve; la okuloj (<trd lng="la">Mustela putorius</trd>):
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> havas brilan, nigrabrunan &gcirc;is nigran kovran
        hararon, kiu sur flankoj ne kovras flavan vilon, post la okuloj
        kaj sub la oreloj estas blanka makulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9127">Putoro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Lula</nom> anka&ubreve; estas speco de produkto
        el la brazila ekvatora &gcirc;angalo, sen felo sed barboplena kun oreloj de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2003/008015.html">Misa mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2e/Mustela_putorius_01-cropped.jpg/640px-Mustela_putorius_01-cropped.jpg" prm="CC BY 2.0"><tld/>ino, en la Brita Centro de Sova&gcirc;a Vivo, Newchapel, Surrey
        <fnt>
          <aut>Peter Trimming, Mrs Polecat, provizita de Mariomassone</aut>
          <lok><url ref="&WCW;/File:Mustela_putorius_01-cropped.jpg">&WCW;/File:Mustela_putorius_01-cropped.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_sh;&c_a;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tcho&rcaron;</trd>,
    <trd>tcho&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Iltis</trd>
  <trd lng="en">polecat</trd>
  <trd lng="es">tur&oacute;n</trd>
  <trd lng="fr">putois</trd>
  <trd lng="hu">g&ouml;r&eacute;ny</trd>
  <trd lng="ja">&#x30B1;&#x30CA;&#x30AC;&#x30A4;&#x30BF;&#x30C1;</trd>
  <trd lng="nl">bunzing</trd>
  <trd lng="pl">tch&oacute;rz</trd>
  <trdgrp lng="pt">
    <trd>put&oacute;rio</trd>,
    <trd>doninha</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_jo;&c_k;</trd>
  <trd lng="sk">tchor</trd>
  <trd lng="uk">&c_t;&c_h;&c_ib;&c_r;</trd>
  <trd lng="zh">&#x827E;&#x864E; <pr>&#xE0;ih&#x1D4;</pr></trd>
</drv>

<drv mrk="putor.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc mrk="putor.cxas0o.CXAS">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malsova&gcirc;a formo de <ref tip="super" cel="putor.0o">putoro</ref>
      (<trd lng="la">Mustela putorius forma furo</trd>):
      <ekz>
        furo <klr>[&#x2026;]</klr> estas uzata por &ccirc;asi kuniklojn kaj tial &gcirc;i estas nomata anka&ubreve; &ccirc;as<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9138">Furo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fur.0o">furo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_u;&c_r;&c_o;</trd>,
    <trd>&c_f;&c_r;&c_e;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_t;&c_n;&c_ib; <ind>&c_t;&c_h;&c_o;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="cs">fretka</trd>
  <trd lng="de">Frettchen</trd>
  <trd lng="en">ferret</trd>
  <trd lng="es">hur&oacute;n</trd>
  <trd lng="fr">furet<klr tip="ind"> (zoo.)</klr></trd>
  <trd lng="hu">vad&aacute;szg&ouml;r&eacute;ny</trd>
  <trd lng="nl">fret</trd>
  <trd lng="pl">&lstroke;asica</trd>
  <trd lng="ro">nev&abreve;stuic&abreve;</trd>
  <trd lng="pt">fur&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_u;&c_r;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;
    <ind>&c_h;&c_o;&c_r;&c_jo;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_h;&c_ib;&c_r;-&c_a;&#x43B;&#x44C;&c_b;&c_ib;&#x43D;&#x43E;&c_s;</trd>
  <trd lng="zh">&#x96EA;&#x8C82; <pr>xu&#x11B;di&#x101;o</pr></trd>
</drv>
</art>
