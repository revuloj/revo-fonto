<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bov</rad>/o
</kap>
<drv mrk="bov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>
      (<trd lng="la">Bos</trd>), el kiu kelkaj specioj estas bredataj
      pro viando kaj lakto.
    </dif>
  </snc>
  <bld lok="../bld/bov.jpg">e&ubreve;ropa hejmbovo</bld>
  <trdgrp lng="cs">
    <trd>skot</trd>,
    <trd>v&ugrave;l</trd>
  </trdgrp>
  <trd lng="de">Rind</trd>
  <trd lng="en">cow</trd>
  <trd lng="fr">b&oelig;uf <klr tip="ind">(esp&egrave;ce)</klr></trd>
  <trd lng="nl">rund</trd>
  <trd lng="pl">byk</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="tr">inek</trd>
</drv>
<drv mrk="bov.B0o">
  <kap><tld lit="B"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Taurus</trd>).
    </dif>
  </snc>
  <trd lng="de">Stier</trd>
  <trd lng="fr">Taureau</trd>
  <trd lng="nl">stier</trd>
  <trd lng="pl">byk</trd>
  <trd lng="ru">&c_T;&c_je;&c_l;&c_je;&c_c;</trd>
</drv>
<drv mrk="bov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Viando de <tld/>o.
    </dif>
  </snc>
  <trd lng="nl">rundsvlees</trd>
  <trd lng="fr">viande de b&oelig;uf</trd>
  <trd lng="ru">&c_g;&c_o;&c_v;&c_ja;&c_d;&c_i;&c_n;&c_a;</trd>
</drv>
<drv mrk="bov.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Bovidae</trd>) de rema&ccirc;antaj, plej
      ofte kornoportantaj (amba&ubreve; seksoj) bestoj.
    </dif>
  </snc>
  <trd lng="de">Horntiere</trd>
  <trd lng="fr">bovid&eacute;s</trd>
</drv>
<drv mrk="bov.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Ino de <tld/>o:
      <ekz>
        e&ccirc; plej nigra <tld/>ino donas lakton nur blankan
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kuh</trd>
  <trd lng="fr">vache</trd>
  <trd lng="nl">koe</trd>
  <trd lng="pl">krowa</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_v;&c_a;</trd>
</drv>
<drv mrk="bov.0ido">
  <kap><tld/>ido, <tld/>idino</kap>
  <snc>
    <dif>
      Ido de <tld/>o:
      <ekz>
        foriris <tld/>ido revenis <tld/>o <klr>(pli stulti&gcirc;is)</klr>
        <fnt><bib>PrV</bib></fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="de">Kalb</trd>
  <trdgrp lng="fr">
    <trd>veau</trd>,
    <trd>g&eacute;nisse</trd>
  </trdgrp>
</drv>
<drv mrk="bov.vir0o">
  <kap>vir<tld/>o</kap>
  <snc>
    <dif>
      Virseksa <tld/>o:
      <ekz>
        prenu vir<tld/>on kaj vir&scirc;afon por pacofero
        <fnt><bib>M</bib>, <lok>Levidoj 9:4</lok></fnt>
      </ekz>;
      <ekz>
        <ind>eksvir<tld/>o</ind> <klr>(kastrita <tld/>o)</klr>.
        <trd lng="fr">b&oelig;uf <klr tip="ind">(castr&eacute;)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stier</trd>
  <trd lng="fr">taureau</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
</drv>
</art>
