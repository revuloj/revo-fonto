<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bov</rad>/o
</kap>
<drv mrk="bov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>
      (<trd lng="la">Bos</trd>), el kiu kelkaj specioj estas bredataj
      pro viando kaj lakto.
    </dif>
  </snc>
  <bld lok="../bld/bov.jpg">e&ubreve;ropa hejmbovo</bld>
  <trdgrp lng="cs">
    <trd>skot</trd>,
    <trd>v&ugrave;l</trd>
  </trdgrp>
  <trd lng="de">Rind</trd>
  <trd lng="en">cow</trd>
  <trd lng="fr">b&oelig;uf</trd>
  <trd lng="nl">rund</trd>
<trd lng="tr">inek</trd>
</drv>
<drv mrk="bov.B0o">
  <kap><tld lit="B"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Taurus</trd>).
    </dif>
  </snc>
  <trd lng="de">Stier</trd>
  <trd lng="nl">stier</trd>
</drv>
<drv mrk="bov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Viando de <tld/>o.  
    </dif>
   </snc>
</drv>
<drv mrk="bov.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
       Familio el la ordo <ref tip="super" cel="huf.par0uloj">parhufuloj</ref>
       (<trd lng="la">Bovidae</trd>) de rema&ccirc;antaj, plej
       ofte kornoportantaj (amba&ubreve; seksoj) bestoj.
    </dif>
  </snc>
  <trd lng="de">Horntiere</trd>
</drv>
<drv mrk="bov.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Ino de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Kuh</trd>
  <trd lng="fr">vache</trd>
  <trd lng="nl">koe</trd>
</drv>
</art>
