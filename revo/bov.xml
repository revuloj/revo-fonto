    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="2">stelfiguro</ref>
      kaj
      signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref> (<trd lng="la">Taurus</trd>).
    </dif>
    <trdgrp lng="zh">
    <trd>&#x91D1;&#x725B; <pr>j&#x12B;nni&#xFA;</pr></trd>,
    <trd>&#x91D1;&#x725B;&#x5EA7; <pr>j&#x12B;nni&#xFA;zu&#xF2;</pr></trd>
