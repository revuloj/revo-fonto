<drv mrk="bov.B0isto">
  <kap><tld lit="B"/>isto
    <fnt><vrk><url ref="http://at.komputilo.org">Astronomia
  Terminaro</url></vrk></fnt>
  </kap>
  <mlg>Boo</mlg>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">Stelfiguro</ref> de la norda
      duonsfero (<trd lng="la">Bootes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_a;&c_l;&c_a;&c_p;&c_a;&c_s; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="ca">el Bover</trd>
  <trd lng="en">Bo&ouml;tes</trd>
  <trd lng="fr">Bouvier<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="it">Boote</trd>
  <trd lng="ja">&#x725B;&#x98FC;&#x3044;  <pr>&#x3046;&#x3057;&#x304B;&#x3044;</pr></trd>
  <trd lng="pl">Wolarz  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Boarul  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_o;&c_l;&c_o;&c_v;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="bov.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="bov.vir0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Virseksa <tld/>o:
      <ekz>
        prenu vir<tld/>on kaj vir&scirc;afon por pacofero
        <fnt><bib>MT</bib>, <lok>&Lev; 9:4</lok></fnt>;
      </ekz>
      <ekz>
        <ind>eksvir<tld/>o</ind> <klr>(kastrita <tld/>o)</klr>.
        <trd lng="br">ejen</trd>
        <trd lng="ca">bou</trd>
        <trd lng="fr">b&oelig;uf<klr tip="ind"> (castr&eacute;)</klr></trd>
        <trd lng="nl">os</trd>
        <trd lng="vo">hobub</trd>
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x79CD;&#x725B; <pr>zh&#x1D2;ngni&#xFA;</pr></trd>,
    <trd>&#x726F;&#x725B; <pr>g&#x1D4;ni&#xFA;</pr></trd>,
    <trd>&#x516C;&#x725B; <pr>g&#x14D;ngni&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_g;&c_a;&c_j; <klr tip="amb">(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_lata;&t_nga;&t_tsheg;</trd>
  <trd lng="br">tarv</trd>
  <trd lng="ca">bou <klr>(brau)</klr></trd>
  <trd lng="cs">b&yacute;k</trd>
  <trd lng="de">Stier</trd>
  <trd lng="en">bull</trd>
  <trd lng="es">toro</trd>
  <trd lng="fa">&f_g;&a_A;&a_w; &a_n;&a_r;</trd>
  <trd lng="fr">taureau</trd>
  <trdgrp lng="he">
    <trd>&shin;&vav;&resh;</trd>,
    <trd>&pe;&resh;</trd>
  </trdgrp>
  <trd lng="hu">bika</trd>
  <trd lng="io">bovulo</trd>
  <trd lng="it">toro</trd>
  <trd lng="ja">&#x96C4;&#x725B;  <pr>&#x304A;&#x3046;&#x3057;</pr></trd>
  <trd lng="nl">stier</trd>
  <trd lng="no">okse</trd>
  <trdgrp lng="pl">
    <trd>byk</trd>,
    <trd>buhaj</trd>
  </trdgrp>
  <trd lng="pt">touro</trd>
  <trdgrp lng="ro">
    <trd>taur</trd>,
    <trd>buhai</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">b&yacute;k</trd>
  <trd lng="uk">&c_b;&c_u;&c_g;&c_a;&c_j;</trd>
  <trdgrp lng="vo">
    <trd>tor</trd>,
    <trd>hibub</trd>
  </trdgrp>
</drv>

<drv mrk="bov.grunt0o">
  <kap>grunt<tld/>o</kap>
  <snc mrk="bov.grunt0o.ZOO">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="bov.0o.ZOO">bovo</ref>
      kun kovra hararo tre longa, kiu vivas en himalajaj
      landoj (<trd lng="la">Bos grunniens</trd>):
      <ekz>
        la hejma grunt<tld/>o diferencas de la sova&gcirc;a per siaj
        malpli grandaj korpo, hufoj kaj kornoj
        <fnt>
          <aut>-</aut>,
          <vrk>
          <url ref="&CRI;1/2005/06/23/1@26120.htm">
          Gruntbovo</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2005-06-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="poefag.0o.ZOO">poefago</ref>
    <trd lng="be">&c_ja;&c_k; <klr tip="amb">(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;)</klr></trd>
    <trd lng="bo">&t_ga;&t_ya;&t_ga;&t_tsheg;</trd>
    <trd lng="ca">iac</trd>
    <trd lng="en">yak</trd>
    <trd lng="fr">yack</trd>
    <trd lng="id">yak</trd>
    <trdgrp lng="it">
      <trd>yak</trd>,
      <trd><ind>bue</ind> tibetano</trd>
    </trdgrp>
    <trd lng="nl">jak</trd>
    <trd lng="pl">jak</trd>
    <trd lng="ro">iac</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>jak chrochtav&yacute;</trd>,
    <trd>jak divok&yacute;</trd>
  </trdgrp>
  <trd lng="sk">jak dom&aacute;ci</trd>
  <trdgrp lng="zh">
    <trd>&#x6C02;&#x725B; <pr>m&#xE1;oni&#xFA;</pr></trd>,
    <trd>&#x729B; <pr>l&#xED;</pr></trd>
  </trdgrp>
</drv>
