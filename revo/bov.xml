<drv mrk="bov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bov.0o.ZOO">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda kvarpieda, korna besto, el kiu kelkaj specioj estas
      bredataj pro viando kaj lakto, 
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Bos</trd>):
      <ekz>li &scirc;ar&gcirc;is &ccirc;aron per ligno, jungis du <tld/>ojn kaj veturis en
      la urbon
        <fnt><bib>ElektFab</bib>, <lok>Doktoro &Ccirc;ioscia</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/bov.jpg">
    e&ubreve;ropa hejmbovo
  </bld>
  <trd lng="be">&c_b;&c_y;&c_k;</trd>
  <trdgrp lng="br">
    <trd>bevin<klr tip="ind"> (loen-saout)</klr></trd>,
    <trd>loen-saout</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vaca</trd>,
    <trd>bou</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skot</trd>,
    <trd>b&yacute;k</trd>,
    <trd>v&uring;l</trd>
  </trdgrp>
  <trd lng="de">Rind</trd>
  <trd lng="en">cow</trd>
  <trd lng="es">res</trd>
  <trd lng="fa">&f_g;&a_A;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>b&oelig;uf<klr tip="ind"> (esp&egrave;ce)</klr></trd>,
    <trd>boeuf</trd>
  </trdgrp>
  <trd lng="io">bovo</trd>
  <trd lng="he">&bet;&qof;&resh;</trd>
  <trd lng="hu">szarvasmarha</trd>
  <trd lng="id">sapi</trd>
  <trdgrp lng="it">
     <trd>bovino</trd>,
     <trd>manzo</trd>,
     <trd>bue</trd>
   </trdgrp>
  <trd lng="nl">rund</trd>
  <trd lng="pl">w&oacute;&lstroke;</trd>
  <trd lng="pt">boi</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">hov&auml;do</trd>
  <trd lng="tr">bo&gbreve;a</trd>
  <trd lng="sk">hov&auml;do</trd>
  <trd lng="vo">bub</trd>
  <trdgrp lng="cs">
    <trd>hlup&aacute;k</trd>,
    <trd>vulg. v&uring;l</trd>
  </trdgrp>
</drv>

