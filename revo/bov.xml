<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bov</rad>/o
</kap>
<drv mrk="bov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Bos</trd>), el kiu kelkaj specioj estas bredataj
      pro viando kaj lakto.
    </dif>
  </snc>
  <bld lok="../bld/bov.jpg">
    e&ubreve;ropa hejmbovo
  </bld>
  <trd lng="be">&c_b;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>skot</trd>,
    <trd>b&yacute;k</trd>,
    <trd>v&#x16F;l</trd>
  </trdgrp>
  <trd lng="de">Rind</trd>
  <trd lng="en">cow</trd>
  <trd lng="es">res</trd>
  <trd lng="fr">b&oelig;uf<klr tip="ind"> (esp&egrave;ce)</klr></trd>
  <trd lng="he">&bet;&qof;&resh;</trd>
  <trd lng="hu">szarvasmarha</trd>
  <trd lng="nl">rund</trd>
  <trd lng="pl">byk</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">hov&auml;do</trd>
  <trd lng="tr">inek</trd>
</drv>

<drv mrk="bov.B0o">
  <kap><tld lit="B"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Taurus</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_y;&c_k;</trd>,
    <trd>&c_C;&c_ja;&c_l;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="de">Stier</trd>
  <trd lng="es">Tauro</trd>
  <trd lng="fr">Taureau<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="hu">Bika<klr> (csillagk&eacute;p)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &shin;&vav;&resh;</trd>
  <trd lng="nl">stier</trd>
  <trd lng="pl">byk</trd>
  <trd lng="ru">&c_T;&c_je;&c_l;&c_je;&c_c;</trd>
  <trd lng="sk">B&yacute;k</trd>
</drv>

<drv mrk="bov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Viando de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_l;&c_a;&c_v;&c_ib;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="nl">rundsvlees</trd>
  <trd lng="es">carne de <ind>res</ind></trd>
  <trd lng="fr"><klr tip="ind">(viande de) </klr>b&oelig;uf</trd>
  <trd lng="he">&bet;&shin;&resh; &bet;&qof;&resh;</trd>
  <trd lng="hu">marhah&uacute;s</trd>
  <trd lng="ru">&c_g;&c_o;&c_v;&c_ja;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">hov&auml;dzie</trd>
</drv>

<drv mrk="bov.0ido">
  <kap><tld/>id(in)o</kap>
  <snc>
    <dif>
      Ido de <tld/>o:
      <ekz>
        foriris <tld/>ido revenis <tld/>o <klr>(pli stulti&gcirc;is)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_l;&c_jo;</trd>,
    <trd>&c_c;&c_ja;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Kalb</trd>
  <trd lng="es">becerro</trd>
  <trdgrp lng="fr">
    <trd>veau</trd>,
    <trd>g&eacute;nisse</trd>
  </trdgrp>
  <trd lng="he">&ayin;&gimel;&lamed;</trd>
  <trd lng="hu">borj&uacute;</trd>
  <trd lng="sk">te&lcaron;a</trd>
</drv>

<drv mrk="bov.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Inseksa <tld/>o:
      <ekz>
        e&ccirc; plej nigra <tld/>ino donas lakton nur blankan
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_v;&c_a;</trd>
  <trd lng="de">Kuh</trd>
  <trd lng="es">vaca</trd>
  <trd lng="fr">vache</trd>
  <trd lng="hu">teh&eacute;n</trd>
  <trd lng="he">&pe;&resh;&he;</trd>
  <trd lng="nl">koe</trd>
  <trd lng="pl">krowa</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_v;&c_a;</trd>
  <trd lng="sk">krava</trd>
</drv>

<drv mrk="bov.vir0o">
  <kap>vir<tld/>o</kap>
  <snc>
    <dif>
      Virseksa <tld/>o:
      <ekz>
        prenu vir<tld/>on kaj vir&scirc;afon por pacofero
        <fnt><bib>MT</bib>, <lok>&Lev; 9:4</lok></fnt>;
      </ekz>
      <ekz>
        <ind>eksvir<tld/>o</ind> <klr>(kastrita <tld/>o)</klr>.
        <trd lng="fr">b&oelig;uf<klr tip="ind"> (castr&eacute;)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_k;</trd>
  <trd lng="de">Stier</trd>
  <trd lng="es">toro</trd>
  <trd lng="fr">taureau</trd>
  <trdgrp lng="he">
    <trd>&shin;&vav;&resh;</trd>,
    <trd>&pe;&resh;</trd>
  </trdgrp>
  <trd lng="hu">bika</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">b&yacute;k</trd>
</drv>
</art>
