<drv mrk="bov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bov.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o:
      <ekz>mi ne tre amas <tld/>a&jcirc;on, &gcirc;i ne estas sufi&ccirc;e suka
        <fnt><bib>ElektFab</bib>, <lok>Feli&ccirc;a Jo&ccirc;jo</lok></fnt>;
      </ekz>
      <ekz>li kontenti&gcirc;is per peco de kuirita <tld/>a&jcirc;o kaj porcio da
      fragoj kaj kremo
        <fnt>
          <aut>Jerome K. Jerome, trad. G. Badash</aut>
          <vrk>Tri Viroj en Boato</vrk>
          <lok>&ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_l;&c_a;&c_v;&c_ib;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="bo">&#xF42;&#xFB3;&#xF44;&#xF0B;&#xF64;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>kig <ind>bevin</ind></trd>,
    <trd>kig <ind>saout</ind></trd>
  </trdgrp>
  <trd lng="ca">carn de bou</trd>
  <trd lng="de">Rindfleisch</trd>
  <trd lng="en">beef</trd>
  <trd lng="es">carne de <ind>res</ind></trd>
  <trd lng="fa">&f_g;&a_w;&a_sh;&a_t; &f_g;&a_A;&a_w;</trd>
  <trd lng="fr"><klr tip="ind">(viande de) </klr>b&oelig;uf</trd>
  <trd lng="he">&bet;&shin;&resh; &bet;&qof;&resh;</trd>
  <trd lng="hu">marhah&uacute;s</trd>
  <trdgrp lng="it">
     <trd><ind>carne</ind> bovina</trd>,
     <trd><ind>carne</ind> di manzo</trd>
  </trdgrp>
  <trd lng="nl">rundsvlees</trd>
  <trd lng="no">oksekj&oslash;tt</trd>
  <trd lng="pl">wo&lstroke;owina</trd>
  <trd lng="ru">&c_g;&c_o;&c_v;&c_ja;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">hov&auml;dzie</trd>
  <trd lng="tr">s&inodot;&gbreve;&inodot;r eti</trd>
  <trd lng="vo">bubamit</trd>
</drv>

<drv mrk="bov.0idajxo">
  <kap><tld/>ida&jcirc;o</kap>
  <snc mrk="bov.0idajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>ido:
      <ekz>&scirc;i man&gcirc;is nek <tld/>ida&jcirc;on, nek kolombojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>&ccirc;apitro 20a</lok>
        </fnt>;
      </ekz>
      <ekz>tiu fripona hundo &gcirc;in ne tu&scirc;is, &ccirc;ar tio estis <tld/>ida&jcirc;o
            <fnt>
              <aut>Jaroslav Ha&scaron;ek, trad. Vladim&iacute;r V&aacute;&ncaron;a</aut>
              <vrk>La aventuroj de la brava soldato &Scirc;vejk dum la
              mondmilito, parto 3</vrk>
              <lok>&ccirc;apitro 1a</lok>
            </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">carn de vedella</trd>
  <trd lng="en">veal</trd>
  <trd lng="he">&#x5D1;&#x5E9;&#x5E8; &#x5D1;&#x5E7;&#x5E8;</trd>
  <trd lng="it"><ind>carne</ind> di vitello</trd>
  <trd lng="fa">&f_g;&a_w;&a_sh;&a_t; &f_g;&a_w;&a_s;&a_A;&a_l;&a_h;</trd>
  <trd lng="nl">kalfsvlees</trd>
  <trd lng="pl">ciel&eogonek;cina</trd>
  <trd lng="vo">bub&uuml;lamit</trd>
</drv>

<drv mrk="bov.0ido">
  <kap><ofc>*</ofc><tld/>ido
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -id'</lok></fnt></kap>
  <snc mrk="bov.0ido.AGR">
    <dif>
      Ido de <tld/>o:
      <ekz>
        foriris <tld/>ido revenis <tld/>o
        <klr>(pli stulti&gcirc;is)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_l;&c_jo;</trd>,
    <trd>&c_c;&c_ja;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="br">leue</trd>
  <trdgrp lng="ca">
    <trd>vedell</trd>,
    <trd>vedella</trd>
  </trdgrp>
  <trd lng="cs">tele</trd>
  <trd lng="de">Kalb</trd>
  <trd lng="en">calf</trd>
  <trd lng="es">becerro</trd>
  <trd lng="fa">&f_g;&a_w;&a_s;&a_A;&a_l;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;nisse</trd>,
    <trd>veau</trd>
  </trdgrp>
  <trd lng="he">&ayin;&gimel;&lamed;</trd>
  <trd lng="hu">borj&uacute;</trd>
  <trd lng="io">bovyuno</trd>
  <trd lng="it">vitello</trd>
  <trd lng="nl">kalf</trd>
  <trd lng="no">kalv</trd>
  <trdgrp lng="pl">
    <trd>ciel&eogonek;</trd>,
    <trd>cielak</trd>
  </trdgrp>
  <trd lng="pt">bezerro</trd>
  <trd lng="sk">te&lcaron;a</trd>
  <trd lng="tr">buza&gbreve;&inodot;</trd>
  <trd lng="vo">bub&uuml;l</trd>
</drv>

<drv mrk="bov.0ino">
  <kap><ofc>*</ofc><tld/>ino
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -in'</lok></fnt></kap>
  <snc mrk="bov.0ino.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Inseksa <tld/>o:
      <ekz>
        e&ccirc; plej nigra <tld/>ino donas lakton nur blankan
