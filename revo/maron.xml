<art mrk="$Id: maron.xml,v 1.21 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>maron</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="maron.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref> de
      greftita <ref tip="malprt" cel="kasxta.0arbo">ka&scirc;tanujo</ref>:
      <ekz>
        fileo de apro sur lito de <tld/>oj
        <fnt>
          <vrk><url ref="http://gresillon.org/rubrique26?lang=eo">Jarfina FESTego kun novjara Silvestra FESTeno 2014</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili &ccirc;irka&ubreve;is tablon trinkante la &ccirc;iutagan vinon kun <tld/>oj
        <fnt>
          <aut>Louis-Ferdinand Celine, trad. &Jcirc;ak Le Puil, Armela LeQuint</aut>
          <vrk><url ref="https://books.google.de/books?id=02exBgAAQBAJ&amp;lpg=PA254&amp;ots=b2561i0HhB&amp;dq=maronoj%20kaj%20la&amp;pg=PA254#v=onepage&amp;q=maronoj&amp;f=false">Voja&gcirc;o &gcirc;is noktofino</url></vrk>
           <lok>p. 254</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_ja;&c_d;&c_o;&c_m;&c_y; &c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="ca">castanya</trd>
  <trdgrp lng="de">
    <trd>Marone</trd>,
    <trd>Edelkastanie</trd>,
    <trd>Esskastanie</trd>
  </trdgrp>
  <trd lng="en">marron</trd>
  <trd lng="es">casta&ntilde;a</trd>
  <trd lng="fa">&a_sh;&a_A;&a_h;&zwnj;&a_b;&a_l;&a_w;&a_T;</trd>
  <trd lng="fr">marron</trd>
  <trdgrp lng="hu">
    <trd>szel&iacute;dgesztenye</trd>,
    <trd>nemesgesztenye</trd>,
    <trd>mar&oacute;ni</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6817;  <pr>&#x304F;&#x308A;</pr></trd>,
    <trd>&#x5927;&#x6817;  <pr>&#x3060;&#x3044;&#x304F;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="nl">tamme <ind>kastanje</ind></trd>
  <trd lng="pl">kasztan jadalny</trd>
  <trd lng="pt">castanha comest&iacute;vel</trd>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_s;&c_malmol;&c_je;&c_d;&c_o;&c_b;&c_n;&c_y;&c_j;
  &c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="uk">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n; (&c_ji;&c_s;&c_t;&c_ib;&c_v;&c_n;&c_i;&c_j;)</trd>
  <trd lng="zh">&#x6817;&#x5B50; <pr>l&#xEC;zi</pr></trd>
</drv>
</art>
