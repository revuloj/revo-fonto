<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: helvela.xml,v 1.8 2018/09/04 07:10:21 revo Exp $">
<kap>
  <rad>helvelac</rad>/oj
</kap>
<drv mrk="helvela.0oj">
  <kap>
    <tld/>oj
    <fnt><bib>PV</bib><lok>helvelo</lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref>
      de <ref tip="super" cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Helvellaceae</trd>):
      <ekz>
        al <tld/>oj apartenas la
        <ref tip="prt" cel="helvel.0o">helvelo</ref>
        kaj la <ref tip="prt" cel="giromitr.0o">giromitro</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lorcheln</trd>
  <trd lng="fr">Helvellac&eacute;es</trd>
  <trd lng="it">elvellacee</trd>
  <trd lng="pt">helvel&aacute;ceos</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

