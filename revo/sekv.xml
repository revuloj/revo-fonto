<drv mrk="sekv.0e">
  <kap><tld/>e</kap>
  <snc mrk="sekv.0e.ARG">
    <dif>
      Kiel <tld/>o,
      <ref tip="vid" cel="do.0">do</ref>,
      <ref tip="vid" cel="konsek1.0e">konsekvence</ref>:
      <ekz>
        la surtuto estas a&ccirc;etita de mi, <tld/>e &gcirc;i apartenas al mi
        <fnt>
          <bib>F</bib>
          <lok>&FE; 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj parencoj de mia edzino estas miaj boparencoj, <tld/>e &scirc;ia
        frato estas mia bofrato <klr>[&#x2026;]</klr>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 36</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is nun ne estis, <tld/>e ne povas esti
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili ne estis en granda nombro, ilia ordigo <tld/>e da&ubreve;ris
        mallonge
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">folglich</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>per tant</trd>,
    <trd>com a conseq&uuml;&egrave;ncia</trd>,
    <trd>tot seguit</trd>
  </trdgrp>
  <trd lng="cs">n&aacute;sledkem <klr>(n&ecaron;&ccaron;eho)</klr></trd>
  <trdgrp lng="es">
    <trd>consecuentemente</trd>,
    <trd>por consecuencia</trd>,
    <trd>por lo tanto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>par <ind>cons&eacute;quent</ind></trd>,
    <trd>donc</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;vetkez&eacute;sk&eacute;ppen</trd>,
    <trd>teh&aacute;t</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  <trd lng="sk">n&aacute;sledkom <klr>(nie&ccaron;oho)</klr></trd>
  <trd lng="uk">&#x43E;&#x442;&#x436;&#x435;</trd>
</drv>

<drv mrk="sekv.0ede">
  <kap><tld/>e de, <var><kap><tld/>e al</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="sekv.0ede.pro">
    <dif>
      En la <tld/>o de, <ref tip="vid" cel="post.0.tempe">post</ref>,
      <ref tip="vid" cel="pro.0.kial">pro</ref>...:
      <ekz>
        la servistoj, siavice, <tld/>e de geedzi&gcirc;oj <klr>[&#x2026;]</klr>
        pliigos la laborkapablojn
        <fnt>
          <aut>N. Covelli</aut>,
          <vrk><url ref="http://trento.esperantoitalia.it/doc/etruscaFamilio.pdf">
          Vivo, historio kaj legendo de la etruska familio</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        studento rajtas plenumi anta&ubreve;e aprobitan esplorlaboron kaj 
        gajni kvin poentojn <tld/>e al la pozitiva ju&gcirc;o de la 
        esplorraporto
        <fnt>
          <aut>R. Moerbeek</aut>,
          <vrk><url ref="http://www.esperanto-nederland.nl/nieuws/sciigo20060205.php">
          La Katedro pri Interlingvistiko...</url></vrk>,
          <lok>[2005]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">com a conseq&uuml;&egrave;ncia de</trd>
  <trdgrp lng="de">
    <trd>in <ind>Folge</ind> von</trd>,
    <trd>durch</trd>,
    <trd>unter der <ind>Bedingung</ind>, dass</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>en consecuencia de</trd>,
    <trd>a consecuencia de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en cons&eacute;quence de</trd>,
    <trd>par suite de</trd>
  </trdgrp>
</drv>

<drv mrk="sekv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sekv.0ajxo.KOMUNE">
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        &scirc;ia <tld/>a&jcirc;o estas maldol&ccirc;a kiel absinto <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        el ka&ubreve;zo nekompleta ne povas naski&gcirc;i sekva&jcirc;o kompleta
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        anta&ubreve;a&jcirc;o kaj <tld/>a&jcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138767">Sistemo
          L</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        for&jcirc;eta&jcirc;oj, <klr>[&#x2026;]</klr> industriaj a&ubreve; komercaj ruba&jcirc;oj,
        <tld/>a&jcirc;oj de privata agado 
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2005/008511.php">Ruba&jcirc;trafiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="sekv.0ajxo.MED">
      <uzo tip="fak">MED</uzo>
      <dif>
        Pli-malpli fruaj kaj da&ubreve;raj komplika&jcirc;oj, aperantaj
        a&ubreve; restantaj post malsano a&ubreve; operacio.
      </dif>
      <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_s;&c_t;&c_v;&c_a;</trd>
      <trdgrp lng="ca">
        <trd>xacra</trd>,
        <trd>alifac</trd>
      </trdgrp>
      <trd lng="de">Komplikation</trd>
      <trdgrp lng="fr">
        <trd>suite<klr tip="ind"> (s&eacute;quelle)</klr></trd>,
        <trd>s&eacute;quelle</trd>
      </trdgrp>
      <trd lng="hu">sz&ouml;v&odblac;dm&eacute;ny</trd>
      <trd lng="es">complicaciones<klr> (m&eacute;dicas)</klr></trd>    
      </subsnc>
      <subsnc mrk="sekv.0ajxo.BELA">
        <uzo tip="fak">BELA</uzo>
        <dif>
          <ref tip="dif" cel="dauxr.0igo">Da&ubreve;rigo</ref>:
          <ekz>
            &leftquot;Trans la spegulo kaj kion <nom>Alico</nom> trovis tie&rightquot;
            <klr>[&#x2026;]</klr> estas <tld/>a&jcirc;o de &leftquot;<nom>Alico</nom> en
            Mirlando&rightquot;
            <fnt>
              <bib>Viki</bib>
              <lok><url ref="https://eo.wikipedia.org/?curid=55542">Trans la
              spegulo</url></lok>
            </fnt>.
          </ekz>
       </dif>
    </subsnc>
    <trd lng="ca">continuaci&oacute;</trd>
    <trdgrp lng="de">
      <trd>Folgeerscheinung</trd>,
      <trd>Folge</trd>,
      <trd>Fortsetzung</trd>
    </trdgrp>
    <trd lng="fr">suite<klr tip="ind"> (le reste de qc)</klr></trd>
    <trd lng="hu">folytat&aacute;s</trd>    
    <trd lng="es">la siguiente parte</trd>    
  </snc>
  <snc mrk="sekv.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 10</lok></fnt>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="vic.0o.MAT">vico.</ref>
    <trd lng="ca">successi&oacute;</trd>
    <trd lng="es">sucesi&oacute;n</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x43E;&#x434;&#x43E;&#x432;&#x436;&#x435;&#x43D;&#x43D;&#x44F; (&#x442;&#x435;&#x43A;&#x441;&#x442;&#x443; &#x456; &#x442;.&#x43F;.)</trd>,
    <trd>&#x43C;&#x435;&#x434;. &#x43D;&#x430;&#x441;&#x43B;&#x456;&#x434;&#x43A;&#x438; (&#x445;&#x432;&#x43E;&#x440;&#x43E;&#x431;&#x438;)</trd>
  </trdgrp>
