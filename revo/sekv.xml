<drv mrk="sekv.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekv.0o.rezulto">
    <dif>
      Io, kio <ref tip="vid" cel="sekv.0i.rezulti"><tld/>as<sncref/></ref>,
      <ref tip="vid" cel="konsek1.0o">konsekvenco</ref>:
      <ekz>
        tio estas tute natura <tld/>o
        <fnt>
          <bib>LR</bib>
          <lok>8. Personaj pronomoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne <klr>[&#x2026;]</klr> pensis pri la <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 47:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne povis distingi, &ccirc;u <klr>[la varmego]</klr>
        estas <tld/>o de la efektiva fajro, &ccirc;u de lia tro
        granda ardo de amo
        <fnt><bib>Fab1</bib><lok>Persista stana soldato</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ori&ccirc;a</ind> sukceso.
        <trd lng="fr">riche de <ind>cons&eacute;quences</ind></trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="sekv.0o.vico">
    <dif>
      <ref tip="dif" cel="vic.0o">Vico</ref>:
      <ekz>
        tio fakto restos dum <tld/>o de cent jaroj
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        televida bildo<tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9504-radio.txt">Radio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Reihe</trd>
  </snc>

