</drv>

<drv mrk="moment.cxi_0e">
  <kap>
    &ccirc;i-<tld/>e, <var><kap>&ccirc;i<tld/>e</kap></var>
  </kap>
  <snc>
    <dif>
      En la nuna <tld/>o:
      <ekz>
        jes, certe, &ccirc;i-<tld/>e tio estis &gcirc;usta
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i povis &ccirc;i-<tld/>e savi sin per forkuro
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Usona konsulo, e&ccirc; se &ccirc;i<tld/>e ankora&ubreve; ne la Unua, mi efektive estas
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;uste &ccirc;i<tld/>e li estis ekrompi&gcirc;onta
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i-<tld/>e en la mondo suferas 300 milionoj da homoj pro akvomanko
       <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006866.php">La blua &leftquot;oro&#x201D; &ccirc;iam malsufi&ccirc;os</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="nun.0e">nune</ref>,
       <ref cel="pas.0e">pase</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>momentan</trd>,
    <trd><ind>gerade</ind> eben</trd>
  </trdgrp>
  <trd lng="en">currently</trd>
  <trd lng="ja">&#12371;&#x306E;&#x77AC;&#38291;&#x306B;  <pr>&#12371;&#x306E;&#12375;&#12421;&#12435;&#x304B;&#12435;&#x306B;</pr></trd>
  <trd lng="pl">w tej  <ind>chwili</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x76EE;&#x524D; <pr>m&#xF9;qi&#xE1;n</pr></trd>,
    <trd>&#x5C06;&#x624D; <pr>ji&amacron;ngc&#xE1;i</pr></trd>,
    <trd>&#x5C07;&#x624D; <pr>ji&amacron;ngc&#xE1;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="moment.cxiu0e">
  <kap>
    &ccirc;iu<tld/>e
  </kap>
  <snc mrk="moment.cxiu0e.dauxre">
    <dif>
      En &ccirc;iu ajn <tld/>o:
      <ekz>
        &ccirc;iu<tld/>e minacis lin la dan&gcirc;ero fali en la akvon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        pretigante sin &ccirc;iu<tld/>e saltegi kontra&ubreve; la atakanton
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e mi povas lin renkonti
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="moment.cxiu0e.ripete">
    <dif>
      <klr>(hiperbole)</klr>
      Ripete post nur mallongaj intertempoj:
      <ekz>
        li nur pene pa&scirc;is kaj &ccirc;iu<tld/>e haltis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e el la pli malproksimaj salonoj alkuris iu pastro a&ubreve; oficisto,
        komunikante detalojn pri la diservo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e iu &scirc;ipeto tu&scirc;egis a&ubreve; renversis alian kaj iu falis en la akvon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidi&gcirc;is sur la benko <klr>[&#8230;]</klr> kaj &ccirc;iu<tld/>e levis la okulojn,
        rigardante la najbaran &gcirc;ardenon
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="dauxr.0e">da&ubreve;re</ref>,
       <ref cel="interm.0e">intermite</ref>,
       <ref cel="cxes.sen0a">sen&ccirc;ese</ref>,
       <ref cel="halt.sen0a">senhalte</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>st&auml;ndig</trd>,
    <trd>dauernd</trd>,
    <trd>alle <ind>Augenblick</ind>e</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x523B;&#12293;&#12392;  <pr>&#12371;&#x304F;&#12371;&#x304F;&#12392;</pr></trd>,
    <trd>&#x7D76;&#12360;&#x305A;  <pr>&#x305F;&#12360;&#x305A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>co <ind>chwil&eogonek;</ind></trd>,
    <trd>w ka&zdot;dej <ind>chwili</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_o;&c_h;&c_v;&c_i;&c_l;&c_ib;</trd>,
    <trd>&c_shch;&c_o;&c_h;&c_v;&c_i;&c_l;&c_i;&c_n;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_o;&c_ji; &c_h;&c_v;&c_i;&c_l;&c_i;&c_n;&c_i;</trd>,
    <trd>&c_shch;&c_o;&c_m;&c_i;&c_t;&c_ib;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_o;&c_ji; &c_m;&c_i;&c_t;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65F6;&#x65F6;&#x523B;&#x523B; <pr>sh&#xED;sh&#xED;k&#xE8;k&#xE8;</pr></trd>,
    <trd>&#26178;&#26178;&#x523B;&#x523B; <pr>sh&#xED;sh&#xED;k&#xE8;k&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="moment.gxusta0e">
  <kap>&gcirc;usta<tld/>e,
  <var><kap>en la &gcirc;usta <tld/>o</kap></var></kap>
  <snc mrk="moment.gxusta0e.KOMUNE">
    <dif>
      En la ta&ubreve;ga, <ref cel="konven.0a">konvena</ref> a&ubreve; planita <tld/>o:
      <ekz>
        la sorto <klr>[&#8230;]</klr> alvenigas en la &gcirc;usta <tld/>o
        la ju&gcirc;antojn, kiujn mi bezonis
        <fnt>
          <aut>Moli&egrave;re, trad. L. L. Zamenhof</aut>
          <vrk>Georgo Dandin</vrk>
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        vort' en &gcirc;usta <tld/>o faras pli ol ar&gcirc;ento
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        venas prudento post la &gcirc;usta <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili e&ccirc; proponis sin, kiam venos &gcirc;usta <tld/>o, por ribeligi Aziajn gentojn, kio
        donos al ni pretekston transpa&scirc;i la limojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is tiam silenta piano &gcirc;usta<tld/>e respondas
        al la &jcirc;us soninta violon&ccirc;elo
        <fnt>
          <aut>M. Proust, trad. &Jcirc; Va&scirc;e</aut>
          <vrk><url ref="http://jxvasxe.free.fr/tradukoj/en-ombro-de-florantaj-junulinoj-eo.htm">
          En Ombro de Florantaj Junulinoj</url></vrk>
          <lok>2017-</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akurat.0e.gxustatempe">akurate<sncref/></ref>,
      <ref cel="temp.gxusta0e">&gcirc;ustatempe</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>im richtigen <ind>Moment</ind></trd>,
      <trd>im rechten <ind>Augenblick</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="fr">au bon moment</trd>
  <trdgrp lng="pl">
    <trd>we w&lstroke;a&sacute;ciwej  <ind>chwili</ind></trd>,
    <trd>we w&lstroke;a&sacute;ciwym  <ind>momencie</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36866;&#x65F6; <pr>sh&#xEC;sh&#xED;</pr></trd>,
    <trd>&#36969;&#26178; <pr>sh&#xEC;sh&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="moment.iu0e">
  <kap>
    iu<tld/>e,
    <var><kap>certa<tld/>e</kap></var>
  </kap>
  <snc>
    <dif>
      En iu <tld/>o:
      <ekz>
        iu<tld/>e mi a&ubreve;dis strangan bruon, kiu venis de sub la sofo
        <fnt>
          <bib>ChR</bib>
          <lok>La fantoma bovino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dekstra okulo fiksu unu strion, dum la maldekstra fiksu tiun apudan,
        iu<tld/>e la cerbo kombinas la striojn, naskante elstaran bildon
        <fnt>
          <aut>Gianfranco Cazzaro</aut>
          <vrk><url ref="&Monato;/1995/A950101.php">La modo de fiksokuloj</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la necesa distingo <klr>[&#8230;]</klr> certa<tld/>e mankis en tiu &ccirc;i rezolucio
        <fnt>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2008/009419.php">Rezolucio de e&ubreve;ropa instanco pri evoluismo kaj
          kreismo</url></vrk>
              <bib>Monato</bib>
          <lok>2008/10, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">in einem bestimmten <ind>Moment</ind></trd>
  <trd lng="pl">w jakim&sacute;  <ind>momencie</ind></trd>
</drv>

<drv mrk="moment.last0e">
  <kap>
    last<tld/>e
  </kap>
  <snc>
    <dif>
      En la lasta ebla <tld/>o <klr>(por eviti ion)</klr>:
      <ekz>
        li last<tld/>e subpremis tiun inklinon por eviti, ke &scirc;i <klr>[&#8230;]</klr> ekrifuzu
        paroli
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam ni atingis la kulminon de la ceremonio, li komencis eligi orel&scirc;irajn kriojn
        de teruro kaj provis last<tld/>e fu&gcirc;i tra la homamaso
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        last<tld/>e feli&ccirc;a hazardo ilin savis
        <fnt>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2001/007205.html">Kosovo &dash; Voja&gcirc;impresoj,
          somero 2000</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="jxus.0">&jcirc;us</ref>,
       <ref cel="apenau.0.jxuse">apena&ubreve;<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u; &c_a;&c_p;&c_o;&c_sh;&c_n;&c_ib; <ind>&c_m;&c_o;&c_m;&c_a;&c_n;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>im letzten <ind>Moment</ind></trd>,
    <trd>auf den letzten <ind>Dr&uuml;cker</ind></trd>
  </trdgrp>
  <trd lng="en">just in time</trd>
  <trd lng="ja">&#26368;&#x5F8C;&#x306E;&#x77AC;&#38291;&#x306B;  <pr>&#12373;&#12356;&#12372;&#x306E;&#12375;&#12421;&#12435;&#x304B;&#12435;&#x306B;</pr></trd>
  <trd lng="pl">w ostatnim  <ind>momencie</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_v; &c_o;&c_s;&c_t;&c_a;&c_n;&c_n;&c_ju; &c_m;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v; &c_o;&c_s;&c_t;&c_a;&c_n;&c_n;&c_ib;&c_j; &c_m;&c_o;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22312;&#26368;&#x540E;&#x65F6;&#x523B; <pr>z&#xE0;izu&#xEC;h&#xF2;ush&#xED;k&#xE8;</pr></trd>,
    <trd>&#22312;&#26368;&#x5F8C;&#26178;&#x523B; <pr>z&#xE0;izu&#xEC;h&#xF2;ush&#xED;k&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="moment.sam0e">
  <kap>
    sam<tld/>e
  </kap>
  <snc>
    <dif>
      En unu sama <tld/>o kiel alia okaza&jcirc;o:
      <ekz>
        li trovis &scirc;in tiel bela, ke li sam<tld/>e enami&gcirc;is en &scirc;in
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> sam<tld/>e ekparolis
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la nomoj eli&gcirc;is sam<tld/>e kaj la&ubreve; maniero, kiu amba&ubreve;flanke esprimis ne nur
        grandan surprizon, sed anka&ubreve; grandan &gcirc;ojon
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vinicius</nom> konfuzi&gcirc;is, sed <nom>Petronius</nom> sam<tld/>e rapidis lin helpi
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Johano tuj eligis kelkajn fortajn vortojn, kaj preska&ubreve; sam<tld/>e li ekpensis,
        ke li ne devas fari tion
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="koinci.0ekun">koincide kun</ref>,
       <ref tip="vid" cel="tuj.0.tempe">tuj<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u; &c_t;&c_o;&c_j; &c_s;&c_a;&c_m;&c_y; <ind>&c_m;&c_o;&c_m;&c_a;&c_n;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>im gleichen <ind>Augenblick</ind></trd>,
    <trd>zur selben <ind>Zeit</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x540C;&#26178;&#x306B;  <pr>&#12393;&#12358;&#12376;&#x306B;</pr></trd>
  <trdgrp lng="pl">
    <trd>w tym samym  <ind>momencie</ind></trd>,
    <trd>w tej samej  <ind>chwili</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x540C;&#x65F6; <pr>t&#xF3;ngsh&#xED;</pr></trd>,
    <trd>&#x540C;&#26178; <pr>t&#xF3;ngsh&#xED;</pr></trd>,
    <trd>&#36866;&#36898; <pr>sh&#xEC;f&#xE9;ng</pr></trd>,
    <trd>&#36969;&#36898; <pr>sh&#xEC;f&#xE9;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="moment.de_0o_al_0o">
  <kap>
    de <tld/>o al <tld/>o
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="subit.0e">Subite</ref>:
      <ekz>
        de <tld/>o al <tld/>o la &ccirc;ielo mallumi&gcirc;is de nuboj kaj vento, kaj fari&gcirc;is granda
        pluvo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 18:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia pozicio fari&gcirc;is de <tld/>o al <tld/>o pli turmenta
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia beleco alii&gcirc;as kaj renaski&gcirc;as de <tld/>o al <tld/>o
        <fnt>
          <bib>LGA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="iom.0post0">iom post iom</ref>,
       <ref cel="pasx.0npost0">pa&scirc;on post pa&scirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="de">von einem <ind>Moment</ind> auf den anderen</trd>
  <trd lng="pl">z <ind>chwili</ind>  na chwil&eogonek;</trd>
</drv>
