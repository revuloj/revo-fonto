<drv mrk="moment.cxiu0e">
  <kap>
    &ccirc;iu<tld/>e
  </kap>
  <snc mrk="moment.cxiu0e.dauxre">
    <dif>
      En &ccirc;iu ajn <tld/>o:
      <ekz>
        &ccirc;iu<tld/>e minacis lin la dan&gcirc;ero fali en la akvon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        pretigante sin &ccirc;iu<tld/>e saltegi kontra&ubreve; la atakanton
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e mi povas lin renkonti
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="moment.cxiu0e.ripete">
    <dif>
      <klr>(hiperbole)</klr>
      Ripete post nur mallongaj intertempoj:
      <ekz>
        li nur pene pa&scirc;is kaj &ccirc;iu<tld/>e haltis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e el la pli malproksimaj salonoj alkuris iu pastro a&ubreve; oficisto,
        komunikante detalojn pri la diservo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e iu &scirc;ipeto tu&scirc;egis a&ubreve; renversis alian kaj iu falis en la akvon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidi&gcirc;is sur la benko <klr>[&#8230;]</klr> kaj &ccirc;iu<tld/>e levis la okulojn,
        rigardante la najbaran &gcirc;ardenon
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="dauxr.0e">da&ubreve;re</ref>,
       <ref cel="interm.0e">intermite</ref>,
       <ref cel="cxes.sen0a">sen&ccirc;ese</ref>,
       <ref cel="halt.sen0a">senhalte</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_y; <ind>&c_m;&c_o;&c_m;&c_a;&c_n;&c_t;</ind></trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_a;&c_je; <ind>&c_ib;&c_m;&c_g;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>st&auml;ndig</trd>,
    <trd>dauernd</trd>,
    <trd>alle <ind>Augenblick</ind>e</trd>
  </trdgrp>
  <trd lng="en"><ind>again</ind> and again</trd>
  <trdgrp lng="ja">
    <trd>&#21051;&#12293;&#12392;  <pr>&#12371;&#12367;&#12371;&#12367;&#12392;</pr></trd>,
    <trd>&#32118;&#12360;&#12378;  <pr>&#12383;&#12360;&#12378;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>co <ind>chwil&eogonek;</ind></trd>,
    <trd>w ka&zdot;dej <ind>chwili</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_o;&c_h;&c_v;&c_i;&c_l;&c_ib;</trd>,
    <trd>&c_shch;&c_o;&c_h;&c_v;&c_i;&c_l;&c_i;&c_n;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_o;&c_ji; &c_h;&c_v;&c_i;&c_l;&c_i;&c_n;&c_i;</trd>,
    <trd>&c_shch;&c_o;&c_m;&c_i;&c_t;&c_ib;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_o;&c_ji; &c_m;&c_i;&c_t;&c_ib;</trd>
  </trdgrp>
