<drv mrk="moment.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      En unu <tld/>o; <ref tip="sin" cel="subit.0e">subite</ref>:
      <ekz>
        <tld/>e ili mortas <klr>[&#8230;]</klr> kaj malaperas
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 34:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>e ili ruini&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 73:19</lok>
        </fnt>!
      </ekz>
      <ekz>
        <nom>Marta</nom> per e&ccirc; <tld/>e ne fermi&gcirc;antaj okuloj rigardis en la mallumon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sinjorino ektimigita rapidas plenumi la ordonon kaj <tld/>e sidi&gcirc;as
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la muziko silenti&gcirc;is <tld/>e, la pordokurteno ek&jcirc;eti&gcirc;is flanken, kaj sur la
        sojlo de la pordo sin montris alta, gracia virina figuro
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="moment.0e.nun">
    <dif>
      <ref tip="dif" cel="nun.0.cxitempe">Nun</ref>, en la nuna <tld/>o, &ccirc;i-<tld/>e:
      <ekz>
        pro &scirc;ia nova funebro, &scirc;i rifuzas <tld/>e &ccirc;ian interparoladon kun iu ajn
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi opinias, ke <tld/>e kaj por tiu sola cirkonstanco ta&ubreve;gus, se ni amba&ubreve;
        reprenus nian iaman personecon
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        en tiu &ccirc;i regimento estas <tld/>e &ccirc;irka&ubreve; mil junuloj
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e supozante, tamen, ke<klr>[&#8230;]</klr>
        <fnt>
       <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2000/006847.php">Sobraj pensoj de sobrulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la alveno de la <klr>[&#8230;]</klr> vino <tld/>e interrompis nian konversacion
        <fnt>
          <vrk><url ref="&Monato;/2003/008041.html">La aristarka raporto de Albisturo
          Kvinke</url></vrk>
          <bib>Monato</bib>
          <lok>2003/07, p. 33</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">momentan</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_m;&c_e;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;</trd>,
    <trd>&c_ib;&c_m;&c_g;&c_n;&c_je;&c_n;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>f&uuml;r einen <ind>Moment</ind></trd>,
    <trd>in einem <ind>Augenblick</ind></trd>,
    <trd>pl&ouml;tzlich</trd>
  </trdgrp>
  <trd lng="en">momentarily</trd>
  <trdgrp lng="ja">
