<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ferol</rad>/o
</kap>
<drv mrk="ferol.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
       (<trd lng="la">Ferula</trd>), vivanta precipe en mediteranea regiono.
       La radiko de iuj specioj enhavas <ref cel="asafet.0o">asafetidon</ref>,
       malbonodoran guman rezinon.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Asant</trd>,
     <trd>Teufelsdreck</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>stinking gum</trd>
     <trd>devil's dung</trd>
   </trdgrp>
   <trd lng="fr">f&eacute;rule</trd>
   <trd lng="hu">&ouml;rd&ouml;ggy&ouml;k&eacute;r</trd>
   <trdgrp lng="nl">
     <trd>godenvoedsel</trd>
     <trd>duivelsdrek</trd>
   </trdgrp>
   <trd lng="ru">&c_f;&c_je;&c_r;&c_u;&c_l;&c_a;</trd>
</drv>
</art>
