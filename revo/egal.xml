<drv mrk="egal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="egal.0i.KOMUNE">
    <dif>
      Esti <tld/>a al:
      <ekz>
        lia merito ne <tld/>as al liaj pretendoj;
      </ekz>
      <ekz>
        suspekto pruvon ne <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kiam nokto vualas, &ccirc;iuj koloroj <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu el ili povus vin <tld/>i &#x2010; vi havas korpon kiel la statuo de Izido,
        skulptita en eburo
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>equiparar</trd>,
      <trd>ser igual a</trd>,
      <trd>igualar</trd>
    </trdgrp>
    <trd lng="de">gleichen</trd>
    <trd lng="es">es <ind>igual</ind> a</trd>
    <trd lng="hu">egyenl&odblac;<klr tip="ind"> (vmivel)</klr></trd>
    <trd lng="nl"><ind>gelijk</ind> zijn</trd>
    <trdgrp lng="pl">
      <trd>r&oacute;wna&cacute; si&eogonek;</trd>,
      <trd>dor&oacute;wnywa&cacute;</trd>,
      <trd>sprosta&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="egal.0i.PSI">
    <dif>
      Esti al iu samvalora, nepreferinda;
      <ref tip="vid" cel="indife.0i.io">indiferenti<sncref/></ref>:
      <ekz>
        por mi fama a&ubreve; ne fama <tld/>as
        <fnt>
          <aut>Saionji M.</aut>,
          <vrk>Esperanto por Yumeiho kaj Yumeiho por
          Esperanto</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
      <trdgrp lng="be">
      <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_r;&c_o;&c_w;&c_n;&c_y;&c_m;</ind></trd>,
      <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_a;&c_d;&c_n;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_y;&c_m;</ind></trd>,
      <trd>&c_n;&c_je; <ind>&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</ind></trd>
    </trdgrp>
<trdgrp lng="cs">
    <trd>rovnat se</trd>,
    <trd>&ccaron;init</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>equal</trd>,
    <trd>be indifferent</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;galer</trd>,
    <trd>&ecirc;tre <ind>&eacute;gal</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7B49;&#x3057;&#x3044;  [<ind>&#x3072;&#x3068;&#x3057;&#x3044;</ind>]</trd>,
    <trd>&#x540C;&#x7B49;&#x3067;&#x3042;&#x308B;  [<ind>&#x3069;&#x3046;&#x3068;&#x3046;&#x3067;&#x3042;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">rovna&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_r;&c_ib;&c_v;&c_n;&c_i;&c_m;</trd>,
