<kap>
  <ofc>*</ofc>
  <rad>&ccirc;arm</rad>/a
</kap>
<drv mrk="cxarm.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Delikate <ref tip="super" cel="placx.0a">pla&ccirc;a</ref> kaj loga pro sia agrableco:
      <ekz>
        <tld/>a virino, viza&gcirc;o, ornamo, vesto, rakonto, kamparo;
      </ekz>
      <ekz>
        <tld/>a por la okuloj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekvidis la portreton de la <tld/>a princino, 
        ornamitan per oro kaj diamantoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la krono]</klr> donos <tld/>an ornamon al via kapo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 4:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas bela, mia amato, kaj <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 1:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a, najtingala vo&ccirc;eto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> memoro <klr>[&#x2026;]</klr> ekridetis nun al la malfeli&ccirc;ulino kun
        <tld/>a maldol&ccirc;eco de perdita paradizo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur &scirc;ia viza&gcirc;o estis <tld/>a decida trankvileco
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        per neklaraj sed <tld/>aj konturoj, vidi&gcirc;is kampara dometo kun verando kovrita
        de hedero
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        rigardu tiun &ccirc;i pentrindan pejza&gcirc;on, la <tld/>an vesperon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;iu sia propra estas <tld/>a kaj kara
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agrabl.0a">agrabla</ref>,
      <ref cel="am.0inda">aminda</ref>,
      <ref cel="fresx.0a">fre&scirc;a</ref>,
      <ref cel="gracil.0a">gracila</ref>,
      <ref cel="lind.0a">linda</ref>,
      <ref cel="mir.0inda">mirinda</ref>,
      <ref cel="simpat.0a.placxa">simpatia<sncref/></ref>,
      <ref cel="vigl.0a.rapidmova">vigla<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_ga;&t_-a;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>bezaubernd</trd>,
    <trd>reizend</trd>,
    <trd>charmant</trd>,
    <trd>entz&uuml;ckend</trd>,
    <trd>anmutig</trd>,
    <trd>lieblich</trd>,
    <trd>liebreizend</trd>,
    <trd>s&uuml;&szlig;<klr tip="ind"> (niedlich)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>charming</trd>,
    <trd>cute</trd>
  </trdgrp>
  <trd lng="es">encantador</trd>
  <trdgrp lng="fr">
    <trd>charmant</trd>,
    <trd>mignon</trd>
  </trdgrp>
  <trd lng="he">&mem;&#x5B7;&qof;&samekh;&#x5B4;&yod;&fmem;</trd>
  <trd lng="hu">b&aacute;jos</trd>
  <trdgrp lng="it">
    <trd>incantevole</trd>,
    <trd>delizioso</trd>,
    <trd>aggraziato</trd>,
    <trd>avvenente</trd>, 
    <trd>carino</trd>, 
    <trd>grazioso</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9B45;&#x529B;&#x7684;&#x306A;  <pr>&#x307F;&#x308A;&#x3087;&#x304F;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x9B45;&#x60D1;&#x7684;&#x306A;  <pr>&#x307F;&#x308F;&#x304F;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x304B;&#x308F;&#x3044;&#x3044;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>charmant</trd>,
    <trd>schattig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czaruj&aogonek;cy</trd>,
    <trd>czarowny</trd>,
    <trd>szarmancki</trd>,
    <trd>nadobny</trd>,
    <trd>powabny</trd>,
    <trd>ujmuj&aogonek;cy</trd>,
    <trd>uroczy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>charmoso</trd>,
    <trd>fofo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_l;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_i;&#x43B;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_ib;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_ib;&c_v;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_ib;&c_v;&#x43D;&c_i;&c_ch;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_v;&c_a;&c_b;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43B;&c_a;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_a;&c_r;&#x43D;&c_je;&#x43D;&#x44C;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FF7;&#x4EBA; <pr>m&#xED;r&#xE9;n</pr></trd>,
    <trd>&#x7740;&#x8FF7; <pr>zh&#xE1;om&#xED;</pr></trd>,
    <trd>&#x5996; <pr>y&#x101;o</pr></trd>,
    <trd>&#x53EF;&#x611B;&#x7684; <pr>k&#x11B;&#xE0;ide</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cxarm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kvalito, logo de tio, kio estas <tld/>a:
      <ekz>
        <tld/>o estas sur viaj lipoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 45:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne atentos <tld/>on de virinoj
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 11:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;o preska&ubreve; virina, al kiu aldonis <tld/>on la kolero kaj la sunbruno
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Sara</nom>, pli kredeble viaj <tld/>oj ol via kanto dispelos mian &ccirc;agrenon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabino ne estis perfekte bela, sed &scirc;i posedis la fre&scirc;econ de dekna&ubreve; jaroj
        kaj la <tld/>on de siaj vivaj movoj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne donus cent belulinojn por &scirc;ia simpla, modesta <tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        fortepiano kaj violon&ccirc;elo <klr>[&#x2026;]</klr> &ccirc;irka&ubreve;is per ebriiganta <tld/>o la
        knabinon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">charm</trd>
  <trd lng="fr">charme</trd>
  <trd lng="de">Charme</trd>
  <trd lng="es">encanto</trd>
  <trd lng="he">&qof;&samekh;&fmem;</trd>
  <trd lng="hu">b&aacute;j</trd>
  <trdgrp lng="it">
    <trd>avvenenza</trd>,
    <trd>fascino</trd>,
    <trd>incanto<klr> (fascino)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9B45;&#x529B;  <pr>&#x307F;&#x308A;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x9B45;&#x60D1;  <pr>&#x307F;&#x308F;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">charme</trd>
  <trdgrp lng="pl">
    <trd>czar</trd>,
    <trd>szarmancja</trd>,
    <trd>nadobno&sacute;&cacute;</trd>,
    <trd>powab</trd>
  </trdgrp>
  <trd lng="pt">charme</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_l;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_m;</trd>
  </trdgrp>
  <trd lng="zh">&#x98A8;&#x97FB; <pr>f&#x113;ngy&#xF9;n</pr></trd>
</drv>

<drv mrk="cxarm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Delikate <ref tip="super" cel="placx.0i">pla&ccirc;i</ref> kaj 
      <ref tip="super" cel="log.0i">logi</ref>:
      <ekz>
        la mal&ccirc;astistino <klr>[&#x2026;]</klr> <tld/>as per sia beleco
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        muzikistaro <tld/>is la vizitantojn per lerta muzikado
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis belulo, turnis la kapojn al la virinoj <klr>[&#x2026;]</klr> beda&ubreve;rinde,
        neniun li povas <tld/>i &ccirc;i tie
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia &scirc;an&gcirc;ita animstato sendube <tld/>os vin, malgra&ubreve; via emo rigardi &ccirc;ion per la
        okuloj de praktikemulo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la beleco de la pejza&gcirc;o <tld/>is ilin pli ol &ccirc;io alia, kion ili vidis
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amuz.0i">amuzi</ref>,
      <ref cel="delekt.0i">delekti</ref>,
      <ref cel="flat.0i">flati</ref>,
      <ref cel="gxoj.0igi">&gcirc;ojigi</ref>,
      <ref cel="kajxol.0i">ka&jcirc;oli</ref>,
      <ref cel="plezur.0igi">plezurigi</ref>,
      <ref cel="tent.0i.alloge">tenti<sncref/></ref>,
      <ref cel="rav.0i">ravi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verz&uuml;cken</trd>,
    <trd>bezaubern</trd>,
    <trd>entz&uuml;cken</trd>,
    <trd>reizen <klr>(entz&uuml;cken)</klr></trd>
  </trdgrp>
  <trd lng="fr">charmer</trd>
  <trd lng="es">encantar</trd>
  <trd lng="he">&lamed;&he;&qof;&samekh;&yod;&fmem;</trd>
  <trd lng="hu">elb&aacute;jol</trd>
  <trdgrp lng="it">
    <trd>incantare</trd>,
    <trd>affascinare</trd>,
    <trd>deliziare<klr> (fascino)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x9B45;&#x60D1;&#x3059;&#x308B;  <pr>&#x307F;&#x308F;&#x304F;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">charmeren</trd>
  <trdgrp lng="pl">
    <trd>oczarowywa&cacute;</trd>,
    <trd>czarowa&cacute;</trd>,
    <trd>by&cacute; <ind>szarmanckim</ind></trd>,
    <trd>by&cacute; <ind>nadobnym</ind></trd>,
    <trd>by&cacute; <ind>powabnym</ind></trd>,
    <trd>ujmowa&cacute;</trd>,
    <trd>by&cacute; <ind>uroczym</ind></trd>
  </trdgrp>
  <trd lng="pt">encantar</trd>
  <trd lng="ro">fermeca</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43F;&#x43E;&#x43B;&#x43E;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="cxarm.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Havigi <tld/>on al io:
      <ekz>
        agrabla babilado <tld/>igis la voja&gcirc;on;
      </ekz>
      <ekz>
        kiam li <tld/>igas sian vo&ccirc;on, ne kredu al li
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_a;&c_r;&c_m;</ind></trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="fr">remplir de <ind>charme</ind></trd>
  <trdgrp lng="de">
    <trd><ind>Charm</ind> geben</trd>,
    <trd><ind>verf&uuml;hrerisch</ind> machen</trd>,
    <trd>mit <ind>Anmut</ind> versehen</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&qof;&samekh;&yod;&fmem;</trd>
  <trdgrp lng="it">
    <trd>illegiadrire</trd>,
    <trd>dare<klr> fascino</klr></trd>, 
    <trd>fornire<klr> fascino</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x9B45;&#x529B;&#x3092;&#x4E0E;&#x3048;&#x308B;  <pr>&#x307F;&#x308A;&#x3087;&#x304F;&#x3092;&#x3042;&#x305F;&#x3048;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_d;&c_a;&c_t;&c_mol;
    <ind>&c_sh;&c_a;&c_r;&c_m;</ind></trd>,
    <trd>&c_p;&c_r;&c_i;&c_d;&c_a;&c_t;&c_mol;
    <ind>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_ch;&c_a;&c_r;&c_ib;&c_v;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&#x43D;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_ch;&c_a;&c_r;&c_ib;&c_v;&#x43D;&#x43E;&c_s;&c_t;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
