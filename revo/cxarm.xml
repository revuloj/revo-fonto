<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxarm.xml,v 1.22 2021/05/29 10:56:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;arm</rad>/a
</kap>
<drv mrk="cxarm.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Delikate pla&ccirc;a kaj loga pro sia agrableco:
      <ekz>
        <tld/>a virino, viza&gcirc;o, ornamo, vesto, rakonto, kamparo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF42;&#xF60;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>charmant</trd>,
    <trd>niedlich</trd>,
    <trd>s&uuml;&szlig;<klr tip="ind"> (niedlich)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>charming</trd>,
    <trd>cute</trd>
  </trdgrp>
  <trd lng="es">encantador</trd>
  <trdgrp lng="fr">
    <trd>charmant</trd>,
    <trd>mignon</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E7;&#x5E1;&#x5B4;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">b&aacute;jos</trd>
  <trdgrp lng="it">
    <trd>incantevole</trd>,
    <trd>delizioso</trd>,
    <trd>aggraziato</trd>,
    <trd>avvenente</trd>, 
    <trd>carino</trd>, 
    <trd>grazioso</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>charmant</trd>,
    <trd>schattig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czaruj&aogonek;cy</trd>,
    <trd>czarowny</trd>,
    <trd>szarmancki</trd>,
    <trd>nadobny</trd>,
    <trd>powabny</trd>,
    <trd>ujmuj&aogonek;cy</trd>,
    <trd>uroczy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>charmoso</trd>,
    <trd>fofo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_l;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="cxarm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Logo de tio, kio estas <tld/>a.
    </dif>
  </snc>
  <trd lng="en">charm</trd>
  <trd lng="fr">charme</trd>
  <trd lng="de">Charme</trd>
  <trd lng="es">encanto</trd>
  <trd lng="he">&#x5E7;&#x5E1;&#x5DD;</trd>
  <trd lng="hu">b&aacute;j</trd>
  <trdgrp lng="it">
    <trd>avvenenza</trd>,
    <trd>fascino</trd>,
    <trd>incanto<klr> (fascino)</klr></trd>
  </trdgrp>
  <trd lng="nl">charme</trd>
  <trdgrp lng="pl">
    <trd>czar</trd>,
    <trd>szarmancja</trd>,
    <trd>nadobno&sacute;&cacute;</trd>,
    <trd>powab</trd>
  </trdgrp>
  <trd lng="pt">charme</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_l;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="cxarm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Delikate pla&ccirc;i kaj logi:
      <ekz>
        &scirc;i <tld/>as &ccirc;iujn per sia amindeco kaj spriteco;
      </ekz>
      <ekz>
        pentra&jcirc;o <tld/>anta la okulojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rav.0i">ravi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">charmant sein</trd>
  <trd lng="fr">charmer</trd>
  <trd lng="es">encantar</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E7;&#x5E1;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">elb&aacute;jol</trd>
  <trdgrp lng="it">
    <trd>incantare</trd>,
    <trd>affascinare</trd>,
    <trd>deliziare<klr> (fascino)</klr></trd>
  </trdgrp>
  <trd lng="nl">charmeren</trd>
  <trdgrp lng="pl">
    <trd>oczarowywa&cacute;</trd>,
    <trd>czarowa&cacute;</trd>,
    <trd>by&cacute; <ind>szarmanckim</ind></trd>,
    <trd>by&cacute; <ind>nadobnym</ind></trd>,
    <trd>by&cacute; <ind>powabnym</ind></trd>,
    <trd>ujmowa&cacute;</trd>,
    <trd>by&cacute; <ind>uroczym</ind></trd>
  </trdgrp>
  <trd lng="pt">encantar</trd>
  <trd lng="ro">fermeca</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="cxarm.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Havigi <tld/>on al io:
      <ekz>
        agrabla babilado <tld/>igis la voja&gcirc;on.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">remplir de <ind>charme</ind></trd>
  <trd lng="de">charmant machen</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E7;&#x5E1;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="it">
    <trd>illegiadrire</trd>,
    <trd>dare<klr> fascino</klr></trd>, 
    <trd>fornire<klr> fascino</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_d;&c_a;&c_t;&c_mol;
    <ind>&c_sh;&c_a;&c_r;&c_m;</ind></trd>,
    <trd>&c_p;&c_r;&c_i;&c_d;&c_a;&c_t;&c_mol;
    <ind>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</ind></trd>
  </trdgrp>
</drv>
</art>
