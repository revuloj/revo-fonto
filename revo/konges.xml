<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konges.xml,v 1.19 2023/12/03 09:58:55 revo Exp $">
<kap>
  <rad>kongest</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konges.0o">
  <kap><tld/>o</kap>
  <snc mrk="konges.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsaneca
      <ref tip="super" cel="flu.sangal0o">sangalfluo</ref>
      en iu korpoparto:
      <ekz>
        cerba, pulma <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">congesti&oacute;</trd>
  <trdgrp lng="de">
     <trd>Blutandrang</trd>,
     <trd>Kongestion</trd>
  </trdgrp>
  <trd lng="en">congestion</trd>
  <trd lng="es">congesti&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>congestion</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>v&eacute;rtolul&aacute;s</trd>,
     <trd>congestio</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9B31;&#x8840;  <pr>&#x3046;&#x3063;&#x3051;&#x3064;</pr></trd>,
    <trd>&#x5145;&#x8840;  <pr>&#x3058;&#x3085;&#x3046;&#x3051;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bloedaandrang</trd>,
    <trd>congestie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kongestia</trd>,
    <trd>przekrwienie</trd>
  </trdgrp>
  <trd lng="pt">congest&atilde;o</trd>
  <trd lng="ro">congestie</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_p;&c_je;&c_r;&c_je;&c_m;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_i;&c_v; &c_k;&c_r;&c_o;&c_v;&c_i;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_v;&c_o;&c_i;&c_z;&c_l;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="zh">&#x5145;&#x8840; <pr>ch&#x14D;ngxu&#xE8;</pr></trd>
</drv>

<drv mrk="konges.0a">
  <kap><tld/>a</kap>
  <snc mrk="konges.0a.MED">
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">congestiu</trd>
  <trd lng="en">congestive</trd>
  <trd lng="es">congestivo</trd>
  <trd lng="fr">congestif</trd>
  <trd lng="hu">v&eacute;rtolul&aacute;sos</trd>
  <trd lng="ja">&#x9B31;&#x8840;&#x3057;&#x305F;  <pr>&#x3046;&#x3064;&#x3061;&#x3057;&#x305F;</pr></trd>
  <trdgrp lng="nl">
    <trd><ind>bloedaandrang</ind> veroorzakend</trd>,
    <trd><ind>congestie</ind> veroorzakend</trd>,
    <trd>congestief</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zastoinowy</trd>,
    <trd>przeci&aogonek;&zdot;ony</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>congestiv&abreve;</trd>,
    <trd>staz&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_je;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="konges.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konges.0i.MED">
    <dif>
      Produkti <tld/>on en io:
      <ekz>
        la varmego <tld/>as la cerbon.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">congestionar</trd>
  <trdgrp lng="de">
     <trd>Blutandrang verursachen</trd>
  </trdgrp>
  <trd lng="en">overcharge with blood</trd>
  <trd lng="es">congestionar</trd>
  <trd lng="fr">congestionner</trd>
  <trd lng="hu">v&eacute;rtolul&aacute;st id&eacute;z el&odblac;</trd>
  <trd lng="ja">&#x9B31;&#x8840;&#x3055;&#x305B;&#x308B;  <pr>&#x3046;&#x3064;&#x3061;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd><ind>bloedaandrang</ind> veroorzaken</trd>,
    <trd><ind>congestie</ind> veroorzaken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zapcha&cacute;</trd>,
    <trd>przepe&lstroke;ni&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;nfunzi</trd>,
    <trd>umple</trd>,
    <trd>aglomera</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_p;&c_r;&c_i;&c_l;&c_i;&c_v;</ind>
  &c_k;&c_r;&c_o;&c_v;&c_i;</trd>
</drv>

<drv mrk="konges.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="konges.mal0i.MED">
    <dif>
      &Ccirc;esigi <tld/>on, defluigante troan sangon:
      <ekz>
        mal<tld/>i la pulmojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://53913456.swh.strato-hosting.eu/ina/naturkuracado19-spiraj.html">
          Malsanoj de la spira aparato</url></vrk>,
          <lok>Internacia Naturkuraca Asocio, [2015?]</lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="fr">d&eacute;congestionner</trd>
  <trd lng="pl">pogarsza&cacute; si&eogonek;</trd>     
  <trd lng="ro">se agraveaz&abreve;</trd>
</drv>
</art>
