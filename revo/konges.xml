<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kongest</rad>/o
</kap>
<drv mrk="konges.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsaneca alfluo de l' sango en iun korpoparton:
      <ekz>
        cerba, pulma <tld/>o.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="flu.sangal0o">sangalfluo</ref>
  </refgrp>
  <trd lng="ca">congesti&oacute;</trd>
  <trdgrp lng="de">
     <trd>Blutandrang</trd>,
     <trd>Kongestion</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>v&eacute;rtolul&aacute;s</trd>,
     <trd>congestio</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bloedaandrang</trd>,
    <trd>congestie</trd>
  </trdgrp>
  <trd lng="en">congestion</trd>
  <trd lng="es">congesti&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>congestion</trd>
  </trdgrp>
  <trd lng="pt">congest&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_p;&c_je;&c_r;&c_je;&c_m;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_i;&c_v; &c_k;&c_r;&c_o;&c_v;&c_i;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_v;&c_o;&c_i;&c_z;&c_l;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="konges.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">congestiu</trd>
  <trd lng="en">congestive</trd>
  <trd lng="es">congestivo</trd>
  <trdgrp lng="fr">
    <trd>congestif</trd>
  </trdgrp>
  <trd lng="hu">v&eacute;rtolul&aacute;sos</trd>
  <trdgrp lng="nl">
    <trd><ind>bloedaandrang</ind> veroorzakend</trd>,
    <trd><ind>congestie</ind> veroorzakend</trd>,
    <trd>congestief</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_je;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="konges.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Produkti <tld/>on en io:
      <ekz>
        la varmego <tld/>as la cerbon.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">congestionar</trd>
  <trdgrp lng="de">
     <trd>Blutandrang verursachen</trd>
  </trdgrp>
  <trd lng="en">overcharge with blood</trd>
  <trd lng="es">congestionar</trd>
  <trdgrp lng="fr">
    <trd>congestionner</trd>
  </trdgrp>
  <trd lng="hu">v&eacute;rtolul&aacute;st id&eacute;z el&odblac;</trd>
  <trdgrp lng="nl">
    <trd><ind>bloedaandrang</ind> veroorzaken</trd>,
    <trd><ind>congestie</ind> veroorzaken</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_p;&c_r;&c_i;&c_l;&c_i;&c_v;</ind>
  &c_k;&c_r;&c_o;&c_v;&c_i;</trd>
</drv>
</art>
