<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kepr.xml,v 1.5 2023/10/18 14:48:10 afido Exp $">
    <kap>
      <rad>kepr</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kepr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="kepr.0o.TEKS">
    <dif>
      <ref tip="super" cel="teks.0arangxo.TEKS">Teksaran&gcirc;o</ref>,
      &ccirc;e kiu la vefto superirinte unu varpan fadenon subiras du a&ubreve; pli,
      rezultiganta oblikvan surfacan strukturon:
      <ekz>
        kvarfadena vefta <tld/>o dekstra
        <fnt>
          <aut>M. Bartovska</aut>
          <vrk><url ref="https://aktuale.scienca-revuo.info/files/1117-1-1255-1-10-20140426.pdf"
              >Tredo, Tretado, Ligo</url></vrk>
          <lok>Scienca Revuo de ISAE, BEOGRAD, vol 26, n-ro 6(116), 30.12.1975</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre konata <tld/>a &scirc;tofo estas <ref tip="sub" cel="drelik.0o">dreliko</ref>;
      </ekz>
      <ekz>
        la &hcirc;er&hcirc;ena viro portis ru&gcirc;an <tld/>an tunikon kaj tartanan kruringon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=302563"
              >Tarim-mumioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">K&ouml;per</trd>
    <trd lng="en">twill</trd>
    <trd lng="fr">serge <klr>(tissu)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_v;&c_i;&c_l;</trd>,
      <trd>&c_k;&c_i;&c_p;&c_je;&c_r;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">sarja</trd>
  <trd lng="fi">toimikas</trd>
  <trd lng="hi">tuila</trd>
  <trd lng="hu">s&aacute;voly</trd>
  <trd lng="it">saia</trd>
  <trd lng="ja">&#32190;&#32340;&#12426; <pr>&#12354;&#12420;&#12362;&#12426;</pr></trd>
  <trd lng="ko">&#45733;&#51649;</trd>
  <trdgrp lng="nl">
    <trd>keper</trd>,
    <trd>tweed</trd>
  </trdgrp>
  <trd lng="sc">tweel</trd>
  <trd lng="sv">twill</trd>
  <trd lng="th">&#3612;&#3657;&#3634;&#3621;&#3634;&#3618;&#3607;&#3649;&#3618;&#3591;</trd>
  <trd lng="tr">fitilli kuma&#254;</trd>
  <trd lng="uk">&c_t;&c_v;&c_ib;&c_l;</trd>
</drv>

  </art>

