<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>furi</rad>/o
</kap>
<drv mrk="furi.0o">
  <kap><tld/>o</kap>
  <snc mrk="furi.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Infera diino de ven&gcirc;o:
      <ekz>
        la tri <tld/>oj.
      </ekz>
     </dif>
  <trd lng="pt">F&uacute;ria</trd>
  </snc>
  <snc mrk="furi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioza virino.
    </dif>
  <trdgrp lng="pt">
    <trd>f&uacute;ria</trd>,
    <trd>mulher furiosa</trd>,
  </trdgrp>
  </snc>
  <adm>
  Uskleco. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_f;&c_u;&c_r;&c_y;&c_ja;</trd>
  <trd lng="de">Furie</trd>
  <trd lng="es">furia</trd>
  <trd lng="fr">furie<klr tip="ind"> (divinit&eacute;, personne
  furieuse)</klr></trd>
  <trd lng="hu">f&uacute;ria</trd>
  <trd lng="nl">furie</trd>
  <trd lng="pl">furia</trd>
  <trd lng="ru">&c_f;&c_u;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
