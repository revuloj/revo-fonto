<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>muar</rad>/i
</kap>
<drv mrk="muar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Doni al metalo, &scirc;tofo, papero aspekton de ondemaj rebriloj:
      <ekz>
        <tld/>i rubandon.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">moireren</trd>
  <trdgrp lng="pt">
    <trd>achamalotar</trd>,
    <trd>ondear <klr tip="amb">(um tecido, um metal)</klr></trd>    
  </trdgrp>
</drv>

<drv mrk="muar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Posedanta la rebrilon de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_a;&c_r;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="nl">
    <trd>moir&eacute;</trd>,
    <trd>gevlamd</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_a;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
