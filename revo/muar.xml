<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muar.xml,v 1.22 2023/10/19 18:36:53 revo Exp $">
<kap>
  <rad>muar</rad>/i
</kap>
<drv mrk="muar.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Montri ondemajn <ref tip="super" cel="bril.re0i">rebrilo</ref>jn kiel metalo, akvosurfaco,
      olefilmo, silko:
      <ekz>
        li la&ubreve;iris la riveron, sor&ccirc;ata de la belo <klr>[&#x2026;]</klr> de la akvaflua <tld/>o
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia anta&ubreve;vido pri tio <tld/>is de sendubeco pri sukceso al malespero
        <fnt>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="http://www.ueci.it/eldonoj/verkoj/La_hknstsm.doc">La homo kiu ne sukcesis
          trompi sin mem</url></vrk>
          <lok>&Ccirc;apitro unua &dash; La kolegia vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        le&gcirc;era vo&ccirc;trilo <tld/>is &ccirc;e liaj oreloj
        <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="iriz.0i">irizi</ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="desegn.0o">desegno</ref>,
       <ref cel="marmor.0o">marmoro</ref>,
       <ref cel="opal.0o">opalo</ref>,
       <ref cel="vejn.0o.FIG">vejno<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_u;&c_a;&c_r;&c_a;&c_v;&c_y;</ind> &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  <trd lng="cs">d&aacute;t vodov&yacute; lesk</trd>
  <trdgrp lng="de">
    <trd>metallisch, &ouml;lig, seidig <ind>gl&auml;nzen</ind></trd>,
    <trd>changieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to water</trd>,
    <trd>to calendar <klr>(a watered finish)</klr></trd>
  </trdgrp>
  <trd lng="fr">moirer</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;rv&aacute;nyoz</trd>,
    <trd>moar&eacute;z</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6CE2;&#x5F62;&#x6A21;&#x69D8;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x306F;&#x3051;&#x3044;&#x3082;&#x3088;&#x3046;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x30E2;&#x30A2;&#x30EC;&#x52A0;&#x5DE5;&#x3059;&#x308B;  <pr>&#x30E2;&#x30A2;&#x30EC;&#x304B;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">moireren</trd>
  <trdgrp lng="pl">
    <trd>nadawa&cacute; falisty wygl&aogonek;d</trd>,
    <trd>pr&aogonek;&zdot;kowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>achamalotar</trd>,
    <trd>ondear <klr tip="amb">(um tecido, um metal)</klr></trd>    
  </trdgrp>
  <trd lng="ro">da un aspect ondulat</trd>
  <trd lng="sk">da&tcaron; nie&ccaron;omu vodov&yacute; lesk</trd>
</drv>

<drv mrk="muar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Posedanta tian rebrilon de <tld/>o:
      <ekz>
        la tajloro faru al vi veston el <tld/>a silka&jcirc;o
        <fnt>
          <aut>William Shakespeare</aut>
          <vrk><url ref="&DonH;Teatrajxoj/epifanio.html">La Festo de la Tri Re&gcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        neniam mi forgesos tiun banon, <klr>[&#x2026;]</klr> &scirc;mirolea &gcirc;i &scirc;ajnis, tepida kaj
        grasa, <tld/>a kun verdaj rebriloj kiel la Nigra Maro en la malnova haveno de
        <nom>Sevastopol</nom>
        <fnt>
          <aut>Henri Baupierre</aut>
          <vrk><url ref="&DonH;Revuoj/nlr/nlr45/malkovro.html">Malkovro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_a;&c_r;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>moiriert</trd>,
    <trd>gl&auml;nzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>moir&eacute;</trd>,
    <trd>moire</trd>
  </trdgrp>
  <trd lng="fr">moir&eacute;</trd>
  <trd lng="hu">moar&eacute;</trd>
  <trd lng="ja">&#x6CE2;&#x5F62;&#x6A21;&#x69D8;&#x306E;&#x3064;&#x3044;&#x305F;  <pr>&#x306F;&#x3051;&#x3044;&#x3082;&#x3088;&#x3046;&#x306E;&#x3064;&#x3044;&#x305F;</pr></trd>
  <trdgrp lng="nl">
    <trd>moir&eacute;</trd>,
    <trd>gevlamd</trd>
  </trdgrp>
  <trd lng="pl">l&sacute;ni&aogonek;cy jak mora</trd>
  <trd lng="ro">str&abreve;lucind ca un moire</trd>
  <trd lng="ru">&c_m;&c_u;&c_a;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_u;&c_a;&c_r;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_v;&c_i;&#x43B;&c_je;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
