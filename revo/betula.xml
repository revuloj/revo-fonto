<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: betula.xml,v 1.14 2017/04/17 16:10:25 revo Exp $">
<kap>
  <rad>betulac</rad>/oj
</kap>
<drv mrk="betula.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj"
      cel="famili.0o.BIO">Familio</ref> de arboj kaj arbedoj 
      el la ordo de fagoparencaj
      <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Betulaceae</trd>) kun:
      amentoj longformaj;
      viraj floroj po pluraj, en akseloj brakteaj;
      tepaloj apena&ubreve; videblaj;
      stamenoj sen harfaskoj &ccirc;e pinto;
      nuksetoj alaj, sen ujo:
      <ekz>
        amento estas nefinita floraro, ofte konussimila, 
        longforma, fleksebla kaj tipe penda <klr>[...]</klr> tipa el
        arbfamilioj 
        kiel salikacoj, fagacoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=18594'>Amento</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Rus&ndash;Kaza&hcirc;-Altajo estas kovrita <klr>[de]</klr> pingloarboj,
        poploj, <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=141503'>Altajo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_r;&c_o;&c_z;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_b;&c_r;&c_je;&c_z;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">betul&agrave;cies</trd>
  <trd lng="de">Birkengew&auml;chse</trd>
  <trd lng="es">betul&aacute;ceas</trd>
  <trd lng="en">birches</trd>
  <trd lng="fr">b&eacute;tulac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>ny&iacute;rf&eacute;l&eacute;k</trd>,
    <trd>ny&iacute;rfaf&eacute;l&eacute;k</trd></trdgrp>
  <trdgrp lng="nl">
    <trd>berken</trd>,
    <trd>heesters</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_jo;&c_z;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">bj&ouml;rkv&auml;xter</trd>
</drv>
</art>
