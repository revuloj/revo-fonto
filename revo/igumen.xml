<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>igumen</rad>/o
    </kap>

    <drv mrk="igumen.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
    	<dif>
	  En la orienta kristanismo,
	  <ref tip="super" cel="monahx.0ejestro">mona&hcirc;ejestro</ref>
	  kies rango estas malpli alta ol tiu de
	  <ref cel="arkimandrit.0o">arkimandrito</ref>.
    	</dif>
      </snc>
  <trd lng="grc">&eta_densa;&gamma;&omikron;&ypsilon_ton;&my;&epsilon;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="be">&c_ib;&c_g;&c_u;&c_m;&c_je;&c_n;</trd>
  <trd lng="ca">hegumen</trd>
  <trdgrp lng="en">
    	<trd>hegumen</trd>,
    	<trd>igumen</trd>
      </trdgrp>
  <trd lng="hu">ap&aacute;t<klr tip="ind"> (g&ouml;r&ouml;gkeleti)</klr></trd>
  <trd lng="it">egumeno</trd>
  <trd lng="pt">superior de um mosteiro</trd>
  <trd lng="ru">&c_i;&c_g;&c_u;&c_m;&c_je;&c_n;</trd>
  <trd lng="sk">igumen</trd>
</drv>

    <drv mrk="igumen.0ino">
      <kap><tld/>ino</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
    	<dif>
	  Mona&hcirc;ino de la plej alta rango, kutime
	  <ref tip="super" cel="monahx.0inejestrino">mona&hcirc;inejestrino</ref>
	  (en la orienta kristanismo).
    	</dif>
      </snc>
      <trd lng="be">&c_ib;&c_g;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
      <trd lng="ca">superiora</trd>
      <trdgrp lng="en">
    	<trd>hegumenia</trd>,
    	<trd>hegumene</trd>,
    	<trd>hegumeness</trd>
      </trdgrp>
      <trd lng="hu">ap&aacute;tn&odblac;<klr tip="ind"> (g&ouml;r&ouml;gkeleti)</klr></trd>
      <trd lng="it">egumenia</trd>
      <trd lng="ru">&c_i;&c_g;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_ja;</trd>
    </drv>

  </art>

