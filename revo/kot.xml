<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kot.0o.KOMUNE">
    <dif>
      Tero, polvo miksita kun malpuraj akvoj de stratoj a&ubreve; vojoj:
      <ekz>
        fali en <tld/>on;
      </ekz>
      <ekz>
        kiu ludas kun <tld/>o malpurigas la manojn
        <klr>(kamarado de malnoblulo fari&gcirc;as mem malnoblulo)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        trafi per la viza&gcirc;o en la <tld/>on<fnt>Z</fnt>
        <klr>(ridinde malsukcesi)</klr>;
      </ekz>
      <ekz>
        <tld/>vadi.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="marcx.0o">mar&ccirc;o</ref>,
      <ref cel="sxlim.0o">&scirc;limo</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>dirt</trd>,
      <trd>filth</trd>,
      <trd>mire</trd>,
      <trd>muck</trd>,
      <trd>mud</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schei&szlig;e</trd>,
      <trd>Dreck</trd>,
      <trd>Matsch</trd>,
      <trd>Kacke</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>lama</trd>,
      <trd>barro</trd>,
      <trd>lodo</trd>,
      <trd>vasa</trd>
    </trdgrp>
  </snc>
  <snc mrk="kot.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Honto, malnobla&jcirc;o, malgloro, malglora stato:
      <ekz>
        malpuraj animoj, kneditaj el <tld/>o;
      </ekz>
      <ekz>
        li falis en la <tld/>on de la plej malnoblaj pasioj;
      </ekz>
      <ekz>
        vivi en <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        super&scirc;uti iun per <tld/>o<fnt>Z</fnt>
        <klr>(malnoblaj insultoj)</klr>;
      </ekz>
      <ekz>
        <tld/>animulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rub.0o">rubo</ref>,
      <ref cel="sxauxm.0o.FIG">&scirc;a&ubreve;mo</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>lama</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_d;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">boue</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;r</trd>,
    <trd>piszok</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>fango</trd>,
      <trd>fanghiglia</trd>,
      <trd>melma</trd>,
      <trd>pantano</trd>
    </trdgrp>
  <trd lng="nl">slijk</trd>
  <trd lng="pl">b&lstroke;oto</trd>
  <trd lng="ru">&c_g;&c_r;&c_ja;&c_z;&c_mol;</trd>
</drv>

<drv mrk="kot.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        novembra <tld/>a tago;
      </ekz>
      <ekz>
        <tld/>a vetero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kaldrono ridas pri poto, kaj mem estas
        <tld/>a<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="en">
      <trd>muddy</trd>,
      <trd>dirty</trd>,
      <trd>filthy</trd>,
      <trd>miry</trd>,
      <trd>mucky</trd>
    </trdgrp>
    <trd lng="fr">boueux</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;ros</trd>,
    <trd>koszos</trd>,
    <trd>mocskos</trd>,
    <trd>piszkos</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>fangoso</trd>,
      <trd>di <ind>fango</ind></trd>,
      <trd>melmoso</trd>
    </trdgrp>
  <trd lng="nl">slijkerig</trd>
  <trdgrp lng="pl">
    <trd>b&lstroke;otny</trd>,
    <trd>ub&lstroke;ocony</trd>,
    <trd>s&lstroke;otny<klr> (tylko dot. pogody)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kot.0i.feki">
    <uzo tip="klr">(evitvorte)</uzo>
    <ref tip="dif" cel="fek.0i.FIZL">feki</ref>
      <ekz>
        ne <tld/>as besto en sia nesto<fnt><bib>PrV</bib></fnt>.
      </ekz>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_e;&c_c;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">chier</trd>
  <trdgrp lng="de">
    <trd>schei&szlig;en</trd>,
    <trd>kacken</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>piszk&iacute;t</trd>,
    <trd>rond&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schijten</trd>,
    <trd>kakken</trd>
  </trdgrp>
  <trd lng="pl">sra&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  </snc>
  <snc mrk="kot.0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Kovri, &scirc;miri, malpurigi per <ref cel="kot.0o.KOMUNE">koto
      </ref>.
    </dif>
    <trd lng="pl">b&lstroke;oci&cacute;</trd>
    <trd lng="it">infangare</trd>
  </snc>
</drv>

<drv mrk="kot.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc mrk="kot.en0igxi.KOMUNE">
    <dif>
      Parte sinki en <tld/>on kaj senmovi&gcirc;i pro tio:
      <ekz>
        ili devis peti helpon por movi la en<tld/>i&gcirc;intan
        &ccirc;aron
        <fnt>
        <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>embourber</ind></trd>,
    <trd>s'<ind>enliser</ind></trd>
  </trdgrp>
  <trd lng="it">infangarsi</trd>
  <trdgrp lng="pl">
    <trd>grz&aogonek;&zacute;&cacute;</trd>,
    <trd>grz&eogonek;zn&aogonek;&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kot.0sxirmilo">
  <kap><tld/>&scirc;irmilo</kap>
  <snc mrk="kot.0sxirmilo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Parto de radveturilo destinita por &scirc;irmi veturantojn 
      kontra&ubreve; <tld/>o.
    </dif>
  </snc>
  <trd lng="en">fender</trd>
  <trd lng="fr">garde-boue</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;rv&eacute;d&odblac;</trd>,
    <trd>s&aacute;rh&aacute;ny&oacute;</trd>
  </trdgrp>
  <trd lng="it">parafango</trd>
  <trd lng="nl">spatbord</trd>
  <trdgrp lng="pl">
    <trd>b&lstroke;otnik</trd>,
    <trd>chlapacz</trd>
  </trdgrp>
</drv>
</art>
