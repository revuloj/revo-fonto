    <trd>&c_g;&c_je;&c_r;&c_o;&c_ji;&c_z;&c_m;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_l;&c_je;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8C6A;&#x6C14; <pr>h&aacute;oq&igrave;</pr></trd>,
    <trd>&#x82F1;&#x52C7; <pr>y&imacron;ngy&#x1D2;ng</pr></trd>
  </trdgrp>
</drv>

