<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rem</rad>/i
</kap>
<drv mrk="rem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Movi &scirc;ipon sur akvo, anta&ubreve;enpu&scirc;ante &gcirc;in per speciala plata ligna ilo:
      <ekz>en trankvila vetero &ccirc;iu <tld/>as sen dan&gcirc;ero
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="de">rudern</trd>
  <trd lng="en">row</trd>
  <trd lng="fr">ramer</trd>
  <trd lng="nl">roeien</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_je;&c_s;&c_t;&c_i; <klr tip="amb">(&c_v;&c_jo;&c_s;&c_l;&c_a;&c_m;&c_i;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rem.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Plata ligna ilo kun longa tenilo uzata por movi &scirc;ipon.
    </dif>
  </snc>
  <trd lng="de">Ruder</trd>
  <trd lng="fr">rame</trd>
  <trdgrp lng="nl">
    <trd>roeispaan</trd>,
    <trd>riem</trd>,
    <trd>roeiriem</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_l;&c_o;</trd>
</drv>

<drv mrk="rem.0piedulo">
  <kap><tld/>piedulo</kap>
  <snc>
    <dif>
      <ref tip="dif">Palmopiedulo</ref>.
    </dif>
  </snc>
</drv>
</art>
