<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rem.xml,v 1.30 2023/10/20 19:20:37 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>rem</rad>/i
</kap>
<drv mrk="rem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref cel="mov.0i.transloki">Movi</ref> &scirc;ipon sur akvo, anta&ubreve;enpu&scirc;ante &gcirc;in per
      speciala plata ligna ilo:
      <ekz>
        kelkaj fi&scirc;istoj <tld/>is al la rifo
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is proksimume <klr>[&#x2026;]</klr> tridek stadiojn
        <fnt>
          <bib>NT</bib>
          <lok>Johano 6:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> elrigardis al Nilo, plena de &scirc;ipetoj, kies <tld/>istoj gaje
        kantis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la renegato ekveturis per rapida &scirc;ipo de ses <tld/>iloj po flanko, ekipita per
        kura&gcirc;ega <tld/>istaro
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        li trovis &scirc;ipeton, saltis en &gcirc;in kaj kun eksterordinara rapideco komencis
        <tld/>i al la alia bordo de l' rivero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virinoj kune <tld/>as, uzante specimenon de pra&ccirc;ina tiel nomata drakboato,
        kun 20 sidlokoj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2015/011437.php">Remado-rimedo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        en trankvila vetero &ccirc;iu <tld/>as sen dan&gcirc;ero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">veslovat</trd>
  <trd lng="de">rudern</trd>
  <trd lng="en">row</trd>
  <trd lng="fr">ramer</trd>
  <trd lng="hu">evez</trd>
  <trdgrp lng="ja">
    <trd>&#x821F;&#x3092;&#x3053;&#x3050;  <pr>&#x3075;&#x306D;&#x3092;&#x3053;&#x3050;</pr></trd>,
    <trd>&#x3053;&#x304E;&#x9032;&#x3080;  <pr>&#x3053;&#x304E;&#x3059;&#x3059;&#x3080;</pr></trd>,
    <trd>&#x3053;&#x3050;</trd>
  </trdgrp>
  <trd lng="nl">roeien</trd>
  <trd lng="pl">wios&lstroke;owa&cacute;</trd>
  <trd lng="pt">remar</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_s;&c_t;&c_i; <klr tip="amb">(&c_v;&c_jo;&c_s;&c_l;&c_a;&c_m;&c_i;)</klr></trd>
  <trd lng="sk">veslova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_je;&c_b;&c_t;&c_i; (&c_v;&c_je;&c_s;&#x43B;&c_a;&#x43C;&c_i;)</trd>,
    <trd>&c_v;&c_je;&c_s;&#x43B;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="rem.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Plata ligna ilo kun longa tenilo, uzata por movi &scirc;ipon:
      <ekz>
        &scirc;ipo kun <tld/>iloj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 33:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fluanta akvo <klr>[&#x2026;]</klr> en kelkaj lokoj estas tiel profunda, ke oni ne
        povas atingi la fundon per <tld/>ilo
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> sidas senmove kun la <tld/>iloj, kiujn ili senpere anta&ubreve; la
        rifo tenas flanke
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        bajdaro flosas sur la maro, sen veloj, sen <tld/>iloj, sen ia direktilo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur <klr>[&#x2026;]</klr> lago blankaj unu<tld/>ilaj boatetoj kuras, penante atingi unu la
        alian
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;is nela&ubreve;taj <tld/>ilbatoj tute apude
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        nur je kelkaj <tld/>ilsvingoj levi&gcirc;as la rokoj
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="padel.0o">padelo</ref>,
      <ref cel="pagaj.0o">pagajo</ref>,
      <ref cel="rudr.0o">rudro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_l;&c_o;</trd>
  <trd lng="cs">veslo</trd>
  <trdgrp lng="de">
    <trd>Ruder</trd>,
    <trd>Riemen <klr>(Ruder)</klr></trd>
  </trdgrp>
  <trd lng="en">oar</trd>
  <trdgrp lng="fr">
    <trd>rame<klr tip="ind"> (aviron)</klr></trd>,
    <trd>aviron<klr tip="ind"> (rame)</klr></trd>
  </trdgrp>
  <trd lng="hu">evez&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x30AA;&#x30FC;&#x30EB;  <pr>&#x304A;&#x30FC;&#x308B;</pr></trd>,
    <trd>&#x6AC2;  <pr>&#x304B;&#x3044;</pr></trd>,
    <trd>&#x6AD3;  <pr>&#x308D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>roeispaan</trd>,
    <trd>riem</trd>,
    <trd>roeiriem</trd>
  </trdgrp>
  <trd lng="pl">wios&lstroke;o</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">veslo</trd>
  <trd lng="uk">&c_v;&c_je;&c_s;&#x43B;&#x43E;</trd>
  <trd lng="vo">gub</trd>
</drv>

<drv mrk="rem.post0ilo">
  <kap>post<tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <fnt><bib>PV</bib><lok>post</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="rem.0ilo"><tld lit="R"/>ilo</ref>,
      kiun oni uzas per tordaj movoj &ccirc;e la malanta&ubreve;o
      de boato por &gcirc;in anta&ubreve;enigi kvaza&ubreve;
      per helico.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_a;&c_v;&c_o;&c_je; <ind>&c_v;&c_ja;&c_s;&c_l;&c_o;</ind></trd>
  <trd lng="de">Wriggriemen</trd>
  <trd lng="en">scull</trd>
  <trd lng="fr">godille<klr tip="ind"> (rame)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6AD3;  <pr>&#x308D;</pr></trd>,
    <trd>&#x3068;&#x3082;&#x304C;&#x3044;</trd>
  </trdgrp>
  <trd lng="pl">scull  <klr>(rodzaj  wios&lstroke;a)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_o;&c_v;&c_o;&c_je; &c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_r;&#x43C;&#x43E;&c_v;&c_je; &c_v;&c_je;&c_s;&#x43B;&#x43E;</trd>
</drv>

<drv mrk="rem.0pieda">
  <kap><tld/>pieda</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref cel="palm.0opieda" tip="dif">palmopieda</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_p;&c_ch;&c_a;&c_s;&c_t;&c_a;&c_n;&c_o;&c_g;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_l;&c_a;&c_n;&c_o;&c_g;&c_ib;</trd>
  </trdgrp>
  <trd lng="en">oar-footed</trd>
  <trd lng="fr">palmip&egrave;de<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">&uacute;sz&oacute;l&aacute;b&uacute;</trd>
  <trd lng="ja">&#x6C34;&#x304B;&#x304D;&#x306E;&#x3042;&#x308B;  <pr>&#x307F;&#x305A;&#x304B;&#x304D;&#x306E;&#x3042;&#x308B;</pr></trd>
  <trd lng="pl">wios&lstroke;onogi</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_p;&c_a;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_ch;&c_a;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_l;&c_o;&c_n;&c_o;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_je;&c_t;&c_i;&#x43D;&c_ch;&c_a;&c_s;&c_t;&#x43E;&#x43B;&c_a;&#x43F;&c_i;&c_j;</trd>
</drv>
