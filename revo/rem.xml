<kap>
<ofc>*</ofc>
<rad>rem</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="rem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref cel="mov.0i.transloki">Movi</ref> &scirc;ipon sur akvo, anta&ubreve;enpu&scirc;ante &gcirc;in per
      speciala plata ligna ilo:
      <ekz>
        kelkaj fi&scirc;istoj <tld/>is al la rifo
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is proksimume <klr>[&#8230;]</klr> tridek stadiojn
        <fnt>
          <bib>NT</bib>
          <lok>Johano 6:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> elrigardis al Nilo, plena de &scirc;ipetoj, kies <tld/>istoj gaje
        kantis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        videble flatis al la a&ubreve;gustino, ke &scirc;ian boaton <tld/>as ekskonsuloj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li posedis etan boaton, kaj li <tld/>is &gcirc;in tute senbrue en la lago
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la renegato ekveturis per rapida &scirc;ipo de ses <tld/>iloj po flanko, ekipita per
        kura&gcirc;ega <tld/>istaro
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        li trovis &scirc;ipeton, saltis en &gcirc;in kaj kun eksterordinara rapideco komencis
        <tld/>i al la alia bordo de l' rivero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie malgrandaj boatoj kun maristoj sin movadis inter ili por <tld/>i urbon a&ubreve;
        por returni sin al la &scirc;ipoj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia &scirc;ipo turni&gcirc;is meze de la rivero kaj <tld/>i&gcirc;is reen
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virinoj kune <tld/>as, uzante specimenon de pra&ccirc;ina tiel nomata drakboato,
        kun 20 sidlokoj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2015/011437.php">Remado-rimedo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        en trankvila vetero &ccirc;iu <tld/>as sen dan&gcirc;ero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">remar</trd>
  <trd lng="cs">veslovat</trd>
  <trd lng="de">rudern</trd>
  <trd lng="en">row</trd>
  <trd lng="es">remar</trd>
  <trd lng="fr">ramer</trd>
  <trd lng="hu">evez</trd>
  <trdgrp lng="ja">
    <trd>&#33311;&#12434;&#12371;&#12368;  <pr>&#12405;&#12397;&#12434;&#12371;&#12368;</pr></trd>,
    <trd>&#12371;&#12366;&#36914;&#12416;  <pr>&#12371;&#12366;&#12377;&#12377;&#12416;</pr></trd>,
    <trd>&#12371;&#12368;</trd>
  </trdgrp>
  <trd lng="nl">roeien</trd>
  <trd lng="pl">wios&lstroke;owa&cacute;</trd>
  <trd lng="pt">remar</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_s;&c_t;&c_i; <klr
  tip="amb">(&c_v;&c_jo;&c_s;&c_l;&c_a;&c_m;&c_i;)</klr></trd>
  <trd lng="sk">veslova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_je;&c_b;&c_t;&c_i; (&c_v;&c_je;&c_s;&c_l;&c_a;&c_m;&c_i;)</trd>,
    <trd>&c_v;&c_je;&c_s;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21010;&#33337; <pr>hu&aacute;chu&aacute;n</pr></trd>,
    <trd>&#19968;&#25490; <pr>y&imacron;p&aacute;i</pr></trd>,
    <trd>&#21010;&#26728; <pr>hu&agrave;ji&#462;ng</pr></trd>,
    <trd>&#21123;&#27123; <pr>hu&agrave;ji&#462;ng</pr></trd>,
    <trd>&#26987; <pr>j&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rem.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Plata ligna ilo kun longa tenilo, uzata por movi &scirc;ipon:
      <ekz>
        &scirc;ipo kun <tld/>iloj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 33:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fluanta akvo <klr>[&#8230;]</klr> en kelkaj lokoj estas tiel profunda, ke oni ne
        povas atingi la fundon per <tld/>ilo
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> sidas senmove kun la <tld/>iloj, kiujn ili senpere anta&ubreve; la
        rifo tenas flanke
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        bajdaro flosas sur la maro, sen veloj, sen <tld/>iloj, sen ia direktilo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur <klr>[&#8230;]</klr> lago blankaj unu<tld/>ilaj boatetoj kuras, penante atingi unu la
        alian
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;is nela&ubreve;taj <tld/>ilbatoj tute apude
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        nur je kelkaj <tld/>ilsvingoj levi&gcirc;as la rokoj
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="padel.0o">padelo</ref>,
      <ref cel="pagaj.0o">pagajo</ref>,
      <ref cel="rudr.0o">rudro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_l;&c_o;</trd>
  <trd lng="ca">rem</trd>
  <trd lng="cs">veslo</trd>
  <trdgrp lng="de">
    <trd>Ruder</trd>,
    <trd>Riemen <klr>(Ruder)</klr></trd>
  </trdgrp>
  <trd lng="en">oar</trd>
  <trd lng="es">remo</trd>
  <trdgrp lng="fr">
    <trd>rame<klr tip="ind"> (aviron)</klr></trd>,
    <trd>aviron<klr tip="ind"> (rame)</klr></trd>
  </trdgrp>
  <trd lng="hu">evez&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#12458;&#12540;&#12523;  <pr>&#12362;&#12540;&#12427;</pr></trd>,
    <trd>&#27330;  <pr>&#12363;&#12356;</pr></trd>,
    <trd>&#27347;  <pr>&#12429;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>roeispaan</trd>,
    <trd>riem</trd>,
    <trd>roeiriem</trd>
  </trdgrp>
  <trd lng="pl">wios&lstroke;o</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">veslo</trd>
  <trd lng="uk">&c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="vo">gub</trd>
  <trdgrp lng="zh">
    <trd>&#26555; <pr>y&igrave;</pr></trd>,
    <trd>&#26728; <pr>ji&#462;ng</pr></trd>,
    <trd>&#27123; <pr>ji&#462;ng</pr></trd>,
    <trd>&#26987; <pr>j&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rem.post0ilo">
  <kap>post<tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <fnt><bib>PV</bib><lok>post</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="rem.0ilo"><tld lit="R"/>ilo</ref>,
      kiun oni uzas per tordaj movoj &ccirc;e la malanta&ubreve;o
      de boato por &gcirc;in anta&ubreve;enigi kvaza&ubreve;
      per helico:
      <ekz>
        malanta&ubreve;e, speco de post<tld/>ilo ebligis direkti la aparaton
        <fnt>
          <aut>Jules Verne</aut>
          <vrk><url
          ref="https://miaj-tradukoj.e-monsite.com/pages/dep/apitro-xxxi-en-kiu-la-inspektoro-tubero-tre-serioze-zorgas-pri-la-interesoj-de-fileas-fogg.html"
              >&Ccirc;irka&ubreve;voja&gcirc;o tra la Mondo en 80 tagoj</url></vrk>
          <lok>&ccirc;apitro XXXI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_a;&c_v;&c_o;&c_je;
  <ind>&c_v;&c_ja;&c_s;&c_l;&c_o;</ind></trd>
  <trd lng="de">Wriggriemen</trd>
  <trd lng="en">scull</trd>
  <trd lng="fr">godille<klr tip="ind"> (rame)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#27347;  <pr>&#12429;</pr></trd>,
    <trd>&#12392;&#12418;&#12364;&#12356;</trd>
  </trdgrp>
  <trd lng="pl">scull  <klr>(rodzaj  wios&lstroke;a)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_o;&c_v;&c_o;&c_je; &c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="uk">&c_k;&c_o;&c_r;&c_m;&c_o;&c_v;&c_je; &c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#26873; <pr>zh&agrave;o</pr></trd>,
    <trd>&#27330; <pr>zh&agrave;o</pr></trd>
  </trdgrp>
</drv>

