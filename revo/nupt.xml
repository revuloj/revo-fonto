<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nupt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="nupt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Geedziga <ref tip="lst" cel="fest.0o"
      lst="voko:festoj">festo</ref>:
      <ekz>
        lia patro jam faris planon pri gefian&ccirc;igo kaj lia onklo
        projekton pri <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        399a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trd lng="de">Hochzeit</trd>
  <trdgrp lng="en">
    <trd>wedding</trd>,
    <trd>nuptials</trd>
  </trdgrp>
  <trd lng="fr">noce</trd>
  <trd lng="es">nupcias</trd>
  <trd lng="hu">n&aacute;sz</trd>
  <trd lng="nl">huwelijk</trd>
  <trd lng="pl">&sacute;lub</trd>
  <trdgrp lng="pt">
    <trd>n&uacute;pcias</trd>,
    <trd>casamento</trd>
  </trdgrp>
</drv>
</art>
