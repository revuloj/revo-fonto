<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nupt.xml,v 1.23 2025/04/04 15:46:02 revo Exp $">
<kap>
  <rad>nupt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="nupt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="nupt.0o.MOR">
    <dif>
     <ref tip="dif" cel="fest.geedzigxo0o.MOR">Geedzi&gcirc;ofesto</ref>:
      <ekz>
        lia patro jam faris planon pri gefian&ccirc;igo kaj lia onklo
        projekton pri <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        399a</lok></fnt>;
      </ekz>
      <ekz>
        mi jam kuncelebris anka&ubreve; la e&ubreve;&hcirc;aristion okaze de intereklezia <tld/>o
        <fnt>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2000/006823.html">Tro senkosta pentemo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        malfermi la oficialan <tld/>on anka&ubreve; por samseksaj paroj
        <fnt>
          <aut>Gerrit Berveling</aut>
            <vrk><url ref="&Monato;/2000/007192.html">Edzi&gcirc;oj anka&ubreve; por samseksaj paroj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        la sonoriloj vokis homojn al diservoj, <tld/>oj, enterigoj
        <fnt>
          <aut>Saliko</aut>
            <vrk><url ref="&Monato;/2001/007297.html">Kolekti sonorilojn</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 24</lok>
          </fnt>;
      </ekz>
      <ekz>
        &scirc;i preska&ubreve; forgesis, ke &scirc;i propre ja atendis la kulminon de &scirc;ia <tld/>onokto
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008357.html">La unua nokto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="matrim.0o">matrimonio</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>geedzi&#285;o</ke>, <ke>kunedzi&#285;o</ke>, <ke>kunedzini&#285;o</ke> .
  </rim>

  <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trd lng="bo">&#3910;&#3908;&#3851;&#3942;&#3851;</trd>
  <trdgrp lng="cs">
    <trd>svatba</trd>,
    <trd>svatebn&iacute; hostina</trd>,
    <trd>s&ncaron;atek</trd>
  </trdgrp>
  <trd lng="de">Hochzeitsfest</trd>
  <trdgrp lng="en">
    <trd>wedding</trd>,
    <trd>nuptials</trd>
  </trdgrp>
  <trd lng="es">nupcias</trd>
  <trd lng="fr">noce</trd>
  <trd lng="hu">n&aacute;sz</trd>
  <trd lng="nl">huwelijk</trd>
  <trd lng="pl">&sacute;lub</trd>
  <trdgrp lng="pt">
    <trd>n&uacute;pcias</trd>,
    <trd>casamento</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sob&aacute;&scaron;</trd>,
    <trd>svadba</trd>,
    <trd>veselie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_s;&c_ib;&#1083;&#1083;&#1103;</trd>,
    <trd>&c_sh;&#1083;&#1102;&c_b;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23130;&#31036; <pr>h&#363;nl&#464;</pr></trd>,
    <trd>&#23130;&#31150; <pr>h&#363;nl&#464;</pr></trd>,
    <trd>&#21916;&#37202; <pr>x&#464;ji&#468;</pr></trd>,
    <trd>&#23130;&#23476; <pr>h&#363;ny&#224;n</pr></trd>,
    <trd>&#23130;&#20856; <pr>h&#363;ndi&#462;n</pr></trd>
  </trdgrp>
</drv>
</art>
