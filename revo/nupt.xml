<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nupt.xml,v 1.19 2022/05/01 16:20:54 revo Exp $">
<kap>
  <rad>nupt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="nupt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="nupt.0o.MOR">
    <dif>
     <ref tip="dif" cel="fest.geedzigxo0o.MOR">Geedzi&gcirc;ofesto</ref>:
      <ekz>
        lia patro jam faris planon pri gefian&ccirc;igo kaj lia onklo
        projekton pri <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        399a</lok></fnt>;
      </ekz>
      <ekz>
        mi jam kuncelebris anka&ubreve; la e&ubreve;&hcirc;aristion okaze de intereklezia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Steloj;monato2/006823.html">Opinio: Tro Senkosta Pentemo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        malfermi la oficialan <tld/>on anka&ubreve; por samseksaj paroj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Steloj;monato2/007192.html">Edzi&gcirc;oj anka&ubreve; por samseksaj paroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la sonoriloj vokis homojn al diservoj, <tld/>oj, enterigoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007297.html">Kolekti sonorilojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i preska&ubreve; forgesis, ke &scirc;i propre ja atendis la kulminon de &scirc;ia <tld/>onokto
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008357.html">La unua nokto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="matrim.0o">matrimonio</ref>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trd lng="bo">&#xF46;&#xF44;&#xF0B;&#xF66;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>svatba</trd>,
    <trd>svatebn&iacute; hostina</trd>,
    <trd>s&ncaron;atek</trd>
  </trdgrp>
  <trd lng="de">Hochzeitsfest</trd>
  <trdgrp lng="en">
    <trd>wedding</trd>,
    <trd>nuptials</trd>
  </trdgrp>
  <trd lng="es">nupcias</trd>
  <trd lng="fr">noce</trd>
  <trd lng="hu">n&aacute;sz</trd>
  <trd lng="nl">huwelijk</trd>
  <trd lng="pl">&sacute;lub</trd>
  <trdgrp lng="pt">
    <trd>n&uacute;pcias</trd>,
    <trd>casamento</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sob&aacute;&scaron;</trd>,
    <trd>svadba</trd>,
    <trd>veselie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x435;&#x441;&#x456;&#x43B;&#x43B;&#x44F;</trd>,
    <trd>&#x448;&#x43B;&#x44E;&#x431;</trd>
  </trdgrp>
</drv>
</art>
