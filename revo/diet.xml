<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>diet</rad>/o
</kap>

<drv mrk="diet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Nutromaniero, obeanta difinitan regularon:
      <ekz>
        observi severan <tld/>on;
      </ekz>
      <ekz>
        vegetara, akva, lakta <tld/>o;
      </ekz>
      <ekz>
        estas ironie, ke kelkaj debatas pri <tld/>oj, dum aliaj mortas
        pro
        malsato<fnt><aut>Krys Ungar</aut>: <vrk>Vivu vigle kaj
        vegetare!</vrk>, <bib>Monato</bib><lok>jaro 1995a, numero 7a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_je;&c_t;&c_a;</trd>
  <trd lng="de">Di&auml;t</trd>
  <trd lng="es">dieta</trd>
  <trd lng="en">diet</trd>
  <trdgrp lng="fr">
    <trd>di&egrave;te<klr tip="ind"> (r&eacute;gime
    alimentaire)</klr></trd>,
    <trd>r&eacute;gime<klr tip="ind"> (alimentaire)</klr></trd>
  </trdgrp>
  <trd lng="hu">di&eacute;ta</trd>
  <trd lng="pl">dieta</trd>
  <trdgrp lng="pt">
    <trd>dieta</trd>,
    <trd>regime <klr tip="amb">(alimentar)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_je;&c_t;&c_a;</trd>
  <trd lng="sk">di&eacute;ta</trd>
</drv>

<drv mrk="diet.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sin nutri la&ubreve; moderaj reguloj koncerne tipojn, kvantojn de
      man&gcirc;a&jcirc;oj,
      a&ubreve; tempojn de man&gcirc;ado:
      <ekz>
        anstata&ubreve; frandemi, &scirc;i &ccirc;iam <tld/>is
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 53a, p.
        182a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_n;&c_a;
  <ind>&c_d;&c_y;&c_je;&c_c;&c_je;</ind></trd>
  <trd lng="de">eine <ind>Di&auml;t</ind> machen</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>di&egrave;te</ind></trd>,
    <trd>suivre un <ind>r&eacute;gime</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>diet</trd>,
    <trd>be on a <ind>diet</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>di&eacute;t&aacute;zik</trd>,
    <trd>di&eacute;t&aacute;t tart</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; na <ind>diecie</ind></trd>,
    <trd>stosowa&cacute; <ind>diet&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_d;&c_je;&c_t;&c_mol; &c_n;&c_a;
    <ind>&c_d;&c_i;&c_je;&c_t;&c_je;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_n;&c_a;
    <ind>&c_d;&c_i;&c_je;&c_t;&c_je;</ind></trd>
  </trdgrp>
</drv>
</art>
