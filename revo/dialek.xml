<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dialek.xml,v 1.32 2023/11/04 14:29:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>dialekt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dialek.0o">
  <kap><tld/>o</kap>
  <snc mrk="dialek.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Aparta formo, kiun ricevas <ref tip="super" cel="lingv.0o.homa">lingvo</ref> en
      regiono a&ubreve; socia grupo:
      <ekz>
        la homamaso kunvenis kaj miregis, &ccirc;ar &ccirc;iu aparte a&ubreve;dis ilin paroli per lia
        propra <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 2:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni devas]</klr> gardi la lingvon de pereiga disfalo je diversaj
        <tld/>oj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili skribis tion, kion ili a&ubreve;dis de popolaj rakontantoj, &scirc;an&gcirc;ante nenion, e&ccirc;
        konservante iafoje la popolan <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o en la norda parto de <nom>Groningen</nom> kaj la germana lingvo tre similas unu
        la alian
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> a&ubreve;skultis la idilian poeton, kiu en la melodia dorisa <tld/>o
        kantis la amon de pa&scirc;tistoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la angla lingvo ne nur dan&gcirc;ere, detrue poluas &ccirc;iujn lingvojn de nia planedo, sed
        mem <tld/>i&gcirc;is en 38 anglajn apartajn nuancojn ene de 8 &ccirc;efaj lingvaj grupoj
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
        </fnt>.
      </ekz>
    </dif>
    <rim mrk="dialek.0o.RIM">
      Kontraste al <ref tip="super" cel="idiom.0o">idiomo</ref>,
      <tld/>o implicas ideon de kadra lingvo, rilate al kiu &gcirc;i
      rolas kiel vario; kontraste al <ref tip="sub" cel="jxargo.0o">&jcirc;argono</ref>,
      <tld/>o estas pli kompleta lingva sistemo, kiu povas posedi
      siajn fonetikajn kaj gramatikajn aparta&jcirc;ojn; ekz-e oni
      povas paroli pri
      <ekz>
        nobela, klerula dialektoj
      </ekz>
      kaj pri iliaj aparta&jcirc;oj fonetikaj kaj gramatikaj; kaj
      pli speciale, pri
      <ekz>
        kortega, teologia &jcirc;argonoj.
      </ekz>
      <aut>Sergio Pokrovskij</aut>
    </rim>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;</trd>
  <trd lng="bo">&t_sa;&t_kata;&t_da;&t_tsheg;&t_la;&t_shabchu;&t_ga;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>dialekt</trd>,
    <trd>n&aacute;&rcaron;e&ccaron;&iacute;</trd>,
    <trd>n&aacute;&rcaron;e&ccaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dialekt</trd>,
    <trd>Mundart</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa_ton;&lambda;&epsilon;&kappa;&tau;&omikron;&sigma_fina;</trd>
  <trd lng="en">dialect</trd>
  <trd lng="es">dialecto</trd>
  <trd lng="fa">&f_g;&a_w;&f_y;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>dialecte</trd>,
    <trd>parler<klr tip="ind"> (dialecte)</klr></trd>
  </trdgrp>
  <trd lng="he">&nun;&#x5B4;&yod;&bet;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;jsz&oacute;l&aacute;s</trd>,
    <trd>dialektus</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>dialek</trd>,
    <trd>logat</trd>
  </trdgrp>
  <trd lng="ja">&#x65B9;&#x8A00;  <pr>&#x307B;&#x3046;&#x3052;&#x3093;</pr></trd>
  <trd lng="nl">dialect</trd>
  <trdgrp lng="pl">
    <trd>dialekt</trd>,
    <trd>gwara</trd>,
    <trd>narzecze</trd>
  </trdgrp>
  <trd lng="pt">dialeto</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;</trd>
  <trd lng="sk">n&aacute;re&ccaron;ie</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_ib;&c_a;&c_l;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_ib;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_ib;&c_ch;&c_ch;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5730;&#x65B9;&#x65B9;&#x8A00; <pr>d&#xEC;fangf&#x101;ngy&#xE1;n</pr></trd>,
    <trd>&#x5927;&#x767D;&#x8A71; <pr>d&#xE0;b&#xE1;ihu&#xE0;</pr></trd>,
    <trd>&#x8A71;&#x8ABF; <pr>hu&#xE0;di&#xE0;o</pr></trd>,
    <trd>&#x65B9;&#x8A00; <pr>f&#x101;ngy&#xE1;n</pr></trd>,
    <trd>&#x672C;&#x5730;&#x8A71; <pr>b&#x11B;nd&#xEC;hu&#xE0;</pr></trd>,
    <trd>&#x571F;&#x8A71; <pr>t&#x1D4;hu&#xE0;</pr></trd>,
    <trd>&#x5730;&#x65B9;&#x8A71; <pr>d&#xEC;fanghu&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dialek.etna0o">
  <kap>etna <tld/>o</kap>
  <snc mrk="dialek.etna0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="dialek.0o.LIN"><tld lit="D"/>o</ref>
      propra al aparta etno.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_a;&c_l;&c_je;&c_k;&c_t;</trd>
  <trd lng="en">ethnic dialect</trd>
  <trd lng="fr">ethnolecte</trd>
  <trd lng="id"><ind>dialek</ind> etnis</trd>
  <trd lng="pl">dialekt etniczny</trd>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;</ind></trd>
</drv>

<drv mrk="dialek.regiona0o">
  <kap>regiona <tld/>o</kap>
  <snc mrk="dialek.regiona0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="dialek.0o.LIN"><tld lit="D"/>o</ref>
      propra al aparta regiono.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_y; <ind>&c_d;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;</ind></trd>,
    <trd>&c_r;&c_e;&c_g;&c_ib;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_d;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Regionalsprache</trd>,
    <trd>Regiolekt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>local <ind>dialect</ind></trd>,
    <trd>territorial <ind>dialect</ind></trd>
  </trdgrp>
  <trd lng="fr">dialecte r&eacute;gional</trd>
  <trd lng="id"><ind>dialek</ind> regional</trd>
  <trdgrp lng="pl">
    <trd>gwara lokalna</trd>,
    <trd>lokalny <ind>dialekt</ind></trd>,
    <trd>narzecze</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;
    <ind>&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;</ind></trd>,
    <trd>&c_n;&c_a;&c_r;&c_je;&c_ch;&c_i;&c_je;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="dialek.socia0o">
  <kap>socia <tld/>o</kap>
  <snc mrk="dialek.socia0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="dialek.0o.LIN"><tld lit="D"/>o</ref>
      propra al aparta socia subgrupo;
      <ref tip="sin" cel="sociolekt.0o.LIN">sociolekto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Gruppensprache</trd>,
    <trd>Soziolekt</trd>
  </trdgrp>
  <trd lng="en">social dialect</trd>
  <trd lng="id"><ind>dialek</ind> sosial</trd>
  <trd lng="fr">sociolecte</trd>
  <trd lng="pl">dialekt spo&lstroke;eczny</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_o;-&c_g;&c_r;&c_u;&c_p;&c_p;&c_o;&c_v;&c_o;&c_j;
  <ind>&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;</ind></trd>
</drv>

<drv mrk="dialek.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <snc mrk="dialek.dis0igxi.LIN">
    <uzo tip="fak">LIN</uzo>
    <uzo tip="klr">(pri lingvo)</uzo>
    <dif>
      Disparti&gcirc;i je pluraj <tld/>oj:
      <ekz>
        la latina lingvo dis<tld/>i&gcirc;inte naskis latinidajn
        lingvojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a; &c_n;&c_a; <ind>&c_d;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;&c_y;</ind></trd>
  <trd lng="de">in <ind>Dialekt</ind>e zerfallen</trd>
  <trd lng="fr">se fragmenter en dialectes</trd>
  <trd lng="ja">&#x65B9;&#x8A00;&#x306B;&#x5206;&#x5316;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x3052;&#x3093;&#x306B;&#x3076;&#x3093;&#x304B;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>dysdialektowa&cacute;</trd>,
    <trd>dzieli&cacute; na  <ind>dialekty</ind></trd>
  </trdgrp>
</drv>
</art>
