<art mrk="$Id: kri1.xml,v 1.19 2023/10/18 17:23:25 revo Exp $">
<kap>
  <rad>kri</rad>/o
</kap>
<drv mrk="kri1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kri1.0o.ETN">
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref> el la plimulta
      indi&gcirc;ena popolo de Kanado, lo&gcirc;anta precipe en la orienta parto de
      la
      lando:
      <ekz>
        mankas en Esperanto la nomoj de la diversaj pralo&gcirc;antaj popoloj
        de
        Ameriko: la Mikmakoj, <klr>...</klr> la <tld lit="K"/>oj, la
        Inuitoj
        <fnt>
        <aut>C. Richard</aut>,
        <vrk>Indianoj kaj Pralo&gcirc;antoj de Nord-Ameriko</vrk>,
        <lok>soc.culture.esperanto, 1999-11-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kri1.0o.homonimo">
    Ne konfuzu kun <ref tip="hom" cel="kri.0o">krio</ref>,
    forta vo&ccirc;ado.
  </rim>
  <trd lng="be">&c_k;&c_r;&c_y; <klr>(&c_n;&c_a;&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="af">skree</trd>
  <trd lng="am">&#x12A5;&#x120D;&#x120D; &#x1260;&#x120D;</trd>
  <trd lng="ar">&a_S;&#x64A;&#x62D;&a_t_marbuta;</trd>
  <trd lng="az">ba&#x11F;&inodot;rmaq</trd>
  <trd lng="bn">&#x99A;&#x9BF;&#x9A4;&#x9CD;&#x995;&#x9BE;&#x9B0;</trd>
  <trd lng="ca">cree<klr> (subst.)</klr></trd>
  <trd lng="es">cree<klr> (subst.)</klr></trd>
  <trdgrp lng="fr">
    <trd>Cri<klr> (subst., indien Cree)</klr></trd>,
    <trd>Cree<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hy">&#x562;&#x572;&#x561;&#x57E;&#x565;&#x56C;</trd>
  <trdgrp lng="ja">
    <trd>&#x53EB;&#x3073;  <pr>&#x3055;&#x3051;&#x3073;</pr></trd>,
    <trd>&#x53EB;&#x3073;&#x58F0;  <pr>&#x3055;&#x3051;&#x3073;&#x3054;&#x3048;</pr></trd>
  </trdgrp>
  <trd lng="pl">Kri <klr>(indianin)</klr></trd>     
  <trd lng="ro">Cree</trd>
  <trd lng="sk">krik</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;ik</trd>,
    <trd>vol&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="sq">b&#xEB;rtas</trd>
  <trd lng="te">&#xC05;&#xC30;&#xC35;&#xC21;&#xC02;</trd>
  <trd lng="uk">&#x43A;&c_r;&c_i;&#x43A;</trd>
</drv>

<drv mrk="kri1.0a">
  <kap><tld/>a</kap>
  <snc mrk="kri1.0a.ETN">
    <dif>
      De la <tld/>oj, de ilia kulturo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y; &c_d;&c_a; <ind>&c_k;&c_r;&c_y;</ind></trd>
  <trd lng="ca">cree<klr> (adj.)</klr></trd>
  <trd lng="cs">k&rcaron;i&ccaron;&iacute;c&iacute;</trd>
  <trd lng="es">cree<klr> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>cri<klr> (adj., des indiens cree)</klr></trd>,
    <trd>cree<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="sk">krik&lcaron;av&yacute;</trd>
</drv>
</art>

