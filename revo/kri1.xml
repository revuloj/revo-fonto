<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kri</rad>/o
</kap>
<drv mrk="kri1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kri1.0o.ETN">
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref> el la plimulta
      indi&gcirc;ena popolo de Kanado, lo&gcirc;anta precipe en la orienta parto de la
      lando:
      <ekz>
        mankas en Esperanto la nomoj de la diversaj pralo&gcirc;antaj popoloj de
        Ameriko: la Mikmakoj, <klr>...</klr> la <tld lit="K"/>oj, la
        Inuitoj
        <fnt>
        <aut>C. Richard</aut>,
        <vrk>Indianoj kaj Pralo&gcirc;antoj de Nord-Ameriko</vrk>,
        <lok>soc.culture.esperanto, 1999-11-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kri1.0o.homonimo">
    Ne konfuzu kun <ref tip="hom" cel="kri.0o">krio</ref>,
    forta vo&ccirc;ado.
  </rim>
  <trdgrp lng="fr">
    <trd>Cri<klr> (subst., indien Cree)</klr></trd>,
    <trd>Cree<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kri1.0a">
  <kap><tld/>a</kap>
  <snc mrk="kri1.0a.ETN">
    <dif>
      De la <tld/>oj, de ilia kulturo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cri<klr> (adj., des indiens cree)</klr></trd>,
    <trd>cree<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

