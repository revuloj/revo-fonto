<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>poem</rad>/o
    </kap>
    <drv mrk="poem.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">POE</uzo>
      <snc>
        <dif>
          Poezia, iom longa verko:
          <ekz>
            la <tld/>oj de Homero.
          </ekz>
        </dif>
      </snc>
      <trd lng="cs">b&aacute;se&ncaron;</trd>
      <trd lng="de">Gedicht</trd>
      <trd lng="en">poem</trd>
      <trd lng="fr">po&egrave;me</trd>
      <trd lng="nl">gedicht</trd>
      <trd lng="ru">&c_p;&c_o;&c_e;&c_m;&c_a;</trd>
    </drv>

    <drv mrk="poem.0aro">
      <kap><tld/>aro</kap>
      <uzo tip="fak">TIP</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="kolekt.0o.libr">Kolekto</ref> de
          poemoj (kaj, eventuale, de aliaj verkoj poeziaj):
          <ekz>
            &leftquot;Plena <tld/>aro&rightquot; de E&ubreve;geno Mi&hcirc;alski, eldonita en Serio
            <em>Stafeto</em> (n-ro 18, 1994).
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_e;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>,
        <trd>&c_s;&c_o;&c_b;&c_r;&c_a;&c_n;&c_i;&c_je; &c_p;&c_o;&c_e;&c_m;</trd>
      </trdgrp>
    </drv>

  </art>
