<drv mrk="poem.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">TIP</uzo>
  <snc mrk="poem.0aro.poemoj">
    <dif>
      <ref tip="super" cel="kolekt.0o.libr">Kolekto</ref> da
      <tld/>oj<sncref ref="poem.0o.POE"/>:
      <ekz>
        tradicia majstroverko de orienta poeto estis
        <tld/>aro el kvin <tld/>oj, nomata <ctl>Kvinopo</ctl>.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_o;&c_b;&c_r;&c_a;&c_n;&c_i;&c_je;
    &c_p;&c_o;&c_e;&c_m;</trd>
  </snc>
  <snc mrk="poem.0aro.poeziajxoj">
    <dif>
      <ref tip="super" cel="kolekt.0o.libr">Kolekto</ref> da
      poezia&jcirc;oj:
      <ekz>
        &leftquot;Plena <tld/>aro&rightquot; de E&ubreve;geno <nom>Mi&hcirc;alski</nom>, eldonita en
        Serio
        <em>Stafeto</em> (numero 18a, 1994).
      </ekz>
    </dif>
    <trdgrp lng="de">
