<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lustr.xml,v 1.19 2023/09/02 05:33:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lustr</rad>/o
</kap>
<drv mrk="lustr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="lustr.0o.LUM">
    <dif>
      Plurbran&ccirc;a pend<ref tip="super" cel="lum.0igilo">lumigilo</ref>:
      <ekz>
        kristalaj <tld/>oj pendis de sur la plafono
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        flamis &ccirc;iuj kandeloj de <tld/>o kaj de kandelabroj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o ver&scirc;is abundan molan elektran lumon
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kandel1.0o">kandelabro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a; <klr tip="amb">(&c_s;&c_mol;&c_v;&c_ja;&c_c;&c_ib;&c_l;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="br">kantolor-skourr</trd>
  <trdgrp lng="de">
     <trd>L&uuml;ster</trd>,
     <trd>Kronleuchter</trd>
  </trdgrp>
  <trd lng="en">chandelier</trd>
  <trdgrp lng="es">
    <trd>lucerna</trd>,
    <trd>ara&ntilde;a <klr>(l&aacute;mpara)</klr></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_l;&a_w;&a_s;&a_t;&a_r;</trd>,
    <trd>&f_ch;&a_l;&f_ch;&a_r;&a_A;&a_gh;</trd>,
    <trd>&a_n;&a_w;&a_r;&a_A;&a_f;&a_sh;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">lustre<klr tip="ind"> (pour &eacute;clairer)</klr></trd>
  <trd lng="hu">csill&aacute;r</trd>
  <trd lng="ja">&#x30B7;&#x30E3;&#x30F3;&#x30C7;&#x30EA;&#x30A2;</trd>
  <trd lng="nl">luchter</trd>
  <trd lng="pl">&zdot;yrandol</trd>
  <trd lng="pt">lustre</trd>
  <trdgrp lng="ro">
    <trd>candelabru</trd>,
    <trd>lustr&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_d;&c_i;&c_l;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>
