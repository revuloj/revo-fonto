<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>&scirc;</rad> <fnt><bib>PIV1</bib></fnt>
    </kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <subart>
      <drv mrk="sx.0.konsonanto">
        <kap><tld/></kap>
        <snc>
          <dif>
            La dudek tria
            <ref tip="super" cel="liter.0o.propra">litero</ref> de la
            Esperanta alfabeto.
          </dif>
          <trd lng="fr">&scirc;<klr> (lettre)</klr></trd>
        </snc>
        <snc mrk="sx.0.FON">    
        <uzo tip="fak">FON</uzo>
          <dif>
            <ref tip="dif" cel="sxusx.0a.konsonanto">&Scirc;u&scirc;a
            konsonanto</ref>
            senvo&ccirc;a, IFAe /&#643;/.
          </dif>
           <refgrp tip="super">
            <ref cel="konson1.0o">konsonanto</ref>,
            <ref cel="frot.0sono">frotsono</ref>
          </refgrp>
         <trd lng="fr">&scirc;<klr> (consonne)</klr></trd>  
        </snc>
      </drv>
    </subart>

    <subart>
      <drv mrk="sx.0.nomo">
        <kap><ofc>* 7</ofc><tld/>o</kap>
        <snc>    
          <dif>
            La nomo de tiu
            <ref tip="super" cel="liter.0o.propra">litero</ref>.
          </dif>
        </snc>
        <trd lng="fr">ch</trd>
      </drv>
    </subart>
  </art>

