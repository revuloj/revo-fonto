<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Andre</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="andre.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
        <dif>
          Vira anta&ubreve;nomo grekdevena:
          <ekz>
            li vidis du fratojn, Simonon <klr>...</klr> kaj Andreon
            <fnt><bib>NT</bib>, <lok>&Mat; 4:18</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_A;&c_n;&c_d;&c_r;&c_e;&c_j;</trd>,
        <trd>&c_A;&c_n;&c_d;&c_r;&c_u;&c_s;&c_mol;</trd>
      </trdgrp>
      <trd lng="br">Andrev</trd>
      <trdgrp lng="de">
        <trd>Andreas</trd>,
        <trd>Andr&eacute;</trd>
      </trdgrp>         
      <trd lng="fr">Andr&eacute;</trd>
      <trdgrp lng="nl">
        <trd>Andries</trd>,
        <trd>Dries</trd>,
        <trd>Andr&eacute;</trd>
      </trdgrp>
      <trd lng="hu">Andr&aacute;s</trd>      
      <trd lng="ru">&c_A;&c_n;&c_d;&c_r;&c_je;&c_j;</trd>
    </drv>
  </art>

