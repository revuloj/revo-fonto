<drv mrk="bret.fenestro0o">
  <kap>fenestro<tld/>o</kap>
  <snc mrk="bret.fenestro0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tabulo muntita horizontale sur la fundo de fenestra
      embrazuro internen en la &ccirc;ambron:
      <ekz>
        sur la fenestro<tld/>o staras floroj;
      </ekz>
      <ekz>
        oni vidis nigran katon sidi sur fenestro<tld/>o kaj sin varmigi
        &ccirc;e la suno
        <fnt>
        <bib>MkM</bib> <lok>&ccirc;ap. 27a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_v;&c_a;&c_k;&c_o;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_k;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">ampit <klr tip="amb">(d'una finestra)</klr></trd>
  <trd lng="cs">okenn&iacute; parapet</trd>
  <trd lng="de">Fensterbrett</trd>
  <trdgrp lng="en">
    <trd>window sill</trd>, <trd>window-sill</trd>
  </trdgrp>
  <trd lng="es">alf&eacute;izar de la ventana</trd>
