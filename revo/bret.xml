    <trd>&c_p;&c_ib;&c_d;&c_v;&c_ib;&c_k;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_u;&c_t;&c_k;&c_a;</trd>
