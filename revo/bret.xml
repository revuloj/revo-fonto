<drv mrk="bret.libro0aro">
  <kap>libro<tld/>aro</kap>
  <snc mrk="bret.libro0aro.MEBL">
    <dif>
      <tld lit="B"/>aro destinita por teni librojn sur la <tld/>oj:
      <ekz>
        la altaj libro<tld/>aroj de publika biblioteko;
      </ekz>
      <ekz>
        se vi volas viziti la antikvan Romon, tiam vi iru ne al la
        fervojo,
        sed iru al via libro<tld/>aro <klr>[&#x2026;]</klr>, elektu unu a&ubreve; du
        volumojn de antikvaj verkistoj kaj eklegu
        <fnt><aut>W. Rohloff</aut>
        <vrk><url ref="&Monato;/2002/007679.html">
        Voja&gcirc;ante kun Marko Polo</url></vrk>
        <bib>Monato</bib>
        <lok>2002:12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxrank.libro0o">libro&scirc;ranko</ref>
    <trdgrp lng="zh">
    <trd>&#x65AD;&#x5C42;&#x5F0F;&#x4E66;&#x67B6; <pr>du&#xE0;nc&#xE9;ngsh&#xEC;sh&#x16B;ji&#xE0;</pr></trd>,
    <trd>&#x4E66;&#x67B6; <pr>sh&#x16B;ji&#xE0;</pr></trd>,
    <trd>&#x56FE;&#x4E66;&#x67B6; <pr>t&#xFA;sh&#x16B;ji&#xE0;</pr></trd>,
    <trd>&#x4E66;&#x67DC; <pr>sh&#x16B;gu&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_l;&c_ja;&c_zh; <klr>(&c_k;&c_n;&c_ib;&c_zh;&c_n;&c_y;)</klr></trd>
  <trd lng="cs">reg&aacute;l na knihy</trd>
  <trd lng="de">B&uuml;cherregal</trd>
  <trd lng="en">bookcase</trd>
  <trd lng="es">estanter&iacute;a para libros</trd>
