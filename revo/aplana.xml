<vortaro>
<art mrk="$Id: aplana.xml,v 1.19 2023/10/13 16:46:48 revo Exp $">
<kap>
  <rad>aplanat</rad>/a
</kap>

<drv mrk="aplana.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Estanta de tia surfaco, ke &ccirc;iuj lumaj radioj elirantaj el la
      sama punkto renkonti&gcirc;as &ccirc;e la sama fokuso:
      <ekz>
        la laborejo ricevis rekonon de lia filo <nom>Hugo Adolph</nom> (1832 &ndash; 1893) 
        kaj de lia nepo <nom>Rudolf</nom> (1865 &ndash; 1830), la unua grand-angula 
        <tld/>a objektivo estis fabrikata tie
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=598691">30837 Steinheil</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">aplanat</trd>
  <trd lng="be">&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">aplan&egrave;tic</trd>
  <trd lng="cs">aplanatick&yacute;</trd>
  <trd lng="de">aplanatisch</trd>
  <trd lng="en">aplanatic</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_fatha;&f_p;&a_kh;&a_sh;</trd>,
    <trd>&a_A_madda;&f_p;&a_l;&a_A;&a_n;&a_A;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">aplan&eacute;tique</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x904A;&#x306E;  <pr>&#x3075;&#x3042;&#x305D;&#x306E;</pr></trd>,
    <trd>&#x7121;&#x53CE;&#x5DEE;&#x306E;  <pr>&#x3080;&#x304A;&#x3055;&#x3080;&#x3055;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pl">aplanatyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">aplanatick&yacute;</trd>
</drv>

<drv mrk="aplana.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <tld lit="A"/>a objektivo:
      <ekz>
        unulensa objektivo havas distordon, periskopo, <tld/>o, anastigmato preska&ubreve; ne havas distordon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51889">Distordo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_t;</trd>
  <trd lng="ca">objectiu Aplanat</trd>
  <trd lng="de">Aplanat</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&f_y;&a_y_hamza;&f_y; &a_A;&a_fatha;&f_p;&a_kh;&a_sh;</trd>,
    <trd>&a_sh;&f_y;&a_y_hamza;&f_y; &a_A_madda;&f_p;&a_l;&a_A;&a_n;&a_A;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">aplanat</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x904A;&#x30EC;&#x30F3;&#x30BA;  <pr>&#x3075;&#x3042;&#x305D;&#x308C;&#x3093;&#x305A;</pr></trd>,
    <trd>&#x7121;&#x53CE;&#x5DEE;&#x30EC;&#x30F3;&#x30BA;  <pr>&#x3080;&#x304A;&#x3055;&#x3080;&#x3055;&#x308C;&#x3093;&#x305A;</pr></trd>,
    <trd>&#x30A2;&#x30D7;&#x30E9;&#x30CA;&#x30FC;&#x30C8;</trd>
  </trdgrp>
  <trd lng="pl">aplanat</trd>
  <trd lng="ru">&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;</trd>
</drv>

