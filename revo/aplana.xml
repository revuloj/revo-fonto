<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aplanat</rad>/a
</kap>

<drv mrk="aplana.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Estanta de tia surfaco, ke &ccirc;iuj lumaj radioj elirantaj el la
      sama punkto renkonti&gcirc;as &ccirc;e la sama fokuso.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">aplan&egrave;tic</trd>
  <trd lng="cs">aplanatick√Ω</trd>
  <trd lng="de">aplanatisch</trd>
  <trd lng="en">aplanatic</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_fatha;&f_p;&a_kh;&a_sh;</trd>,
    <trd>&a_A_madda;&f_p;&a_l;&a_A;&a_n;&a_A;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">aplan&eacute;tique</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">aplanatick&yacute;</trd>
</drv>

<drv mrk="aplana.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <tld lit="A"/>a objektivo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_t;</trd>
  <trd lng="ca">objectiu Aplanat</trd>
  <trd lng="de">Aplanat</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&f_y;&a_y_hamza;&f_y; &a_A;&a_fatha;&f_p;&a_kh;&a_sh;</trd>,
    <trd>&a_sh;&f_y;&a_y_hamza;&f_y; &a_A_madda;&f_p;&a_l;&a_A;&a_n;&a_A;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">aplanat</trd>
  <trd lng="ru">&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;</trd>
</drv>

<drv mrk="aplana.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de <tld/>a sistemo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">aplanetisme</trd>
  <trd lng="cs">aplanatizmus</trd>
  <trd lng="fa">&a_A;&a_fatha;&f_p;&a_kh;&a_sh;&f_y;</trd>
  <trd lng="fr">aplan&eacute;tisme</trd>
  <trd lng="ru">&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">aplanatizmus</trd>
</drv>

<drv mrk="aplana.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Aro da fenomenoj rezultantaj el <tld/>eco.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="fa">&a_A_madda;&f_p;&a_l;&a_A;&a_n;&a_A;&a_t;&f_y;&a_z;&a_m;</trd>
  <trd lng="ru">&c_a;&c_p;&c_l;&c_a;&c_n;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
