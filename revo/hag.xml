  <trd lng="it">L'Aia</trd>
  <trd lng="ja">&#x30CF;&#x30FC;&#x30B0;</trd>
  <trd lng="lat">Haga-Comitis</trd>
  <trdgrp lng="nl">
    <trd>Den Haag</trd>,
    <trd>'s-<ind>Gravenhage</ind></trd>
  </trdgrp>
  <trd lng="pl">Haga</trd>
  <trd lng="pt">Haia</trd>
  <trd lng="ro">Haga</trd>
  <trd lng="ru">&c_G;&c_a;&c_a;&c_g;&c_a;</trd>
