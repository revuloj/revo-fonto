<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: setl.xml,v 1.7 2023/08/05 20:25:01 afido Exp $">
    <kap>
      <rad>setl</rad>/i
    </kap>

<drv mrk="setl.0i">
  <kap><tld/>i</kap>
  <snc mrk="setl.0i.establi">
    <dif>
      <ref tip="super" cel="establ.0i">Establi</ref> lo&gcirc;ejon en nova teritorio:
      <ekz>
        ri&ccirc;aj homoj penas kadre de la le&gcirc;aro <tld/>i &ccirc;e la usona riverbordo kaj tien
        translo&gcirc;igi siajn familianojn
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2011/010441.php">Rio-granda stimulado</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is en Brazilo, kie li rekonstruis la entreprenojn helpe de
        eksterlandaj produktejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010830.php">Tom&aacute;&scaron; Ba&tcaron;a, re&gcirc;o de la
          &scirc;uistoj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        Perto, naski&gcirc;anta &ccirc;efurbo de la frua dekna&ubreve;a-jarcenta okcident-a&ubreve;stralia
        <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2012/010164.php">Panorama soro super
          kontinento</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni starigis komisionon en A&ubreve;stralio por rekomendi kiel reamikigi aborigenojn
        kaj <tld/>intojn
        <fnt>
          <aut>Ralph Harry</aut>
            <vrk><url ref="&Monato;/2000/007103.php">Pa&scirc;oj al amikeco</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 15</lok>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        amaso da neregistaraj organiza&jcirc;oj <klr>[&#8230;]</klr> <tld/>is
        <klr>(<ref cel="establ.0igxi">establi&gcirc;is</ref>)</klr> &ccirc;irka&ubreve; EU en Bruselo
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2003/008051.html">Bruselo-Babelo: lingvo-kulturo en
          E&ubreve;ropa Unio</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="logx.ek0i">eklo&gcirc;i</ref>,
       <ref cel="koloni.0i">kolonii</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>siedeln</trd>,
      <trd>sich <ind>ansiedeln</ind></trd>,
      <trd>sich <ind>niederlassen</ind></trd>
    </trdgrp>
    <trd lng="pl">osiedla&cacute; si&eogonek;</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_s;&c_je;&#x43B;&#x44F;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43E;&c_s;&c_ib;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="setl.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="establ.0ejo">Establejo</ref> de lo&gcirc;ejoj:
      <ekz>
        en <nom>Kernav&#279;</nom> trovi&gcirc;as spuroj de kulturoj, kiujn postlasis homoj anta&ubreve; 11&#x202F;000
        jaroj, inkluzive de kvar kastelmontoj, urbo, pre&gcirc;ejo, <tld/>ejo, tombejo
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2004/008390.html">Trezoroj de la
          pasinteco</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        fine de la 1a jarcento anta&ubreve; Kristo la romianoj fondis <tld/>ejon <nom>Darioritum</nom> sur
        la teritorio de kelta tribo
        <fnt>
          <bib>Monato</bib>
          <aut>Elisabeth Le Dru</aut>
          <vrk><url ref="&Monato;/2010/009948.php">Vizito al Vannes</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        proksime de la unua juda <tld/>ejo oni trovis forlasitan katolikan mona&hcirc;ejon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010574.php">Nova sinagogo ina&ubreve;gurita en
          Speyer</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela <tld/>ejo, perfekte ekipitaj lernejoj, infan&gcirc;ardeno <klr>[&#8230;]</klr> bonorde
        aran&gcirc;itaj en la vila&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010245.php">Floras enklavo komunista</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="koloni.0o.setlejo">kolonio<sncref/></ref>,
       <ref cel="kvarta.0o">kvartalo</ref>,
       <ref cel="vilagx.0o">vila&gcirc;o</ref>,
       <ref cel="urb.0o">urbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_je;&c_l;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Siedlung</trd>,
    <trd>Niederlassung</trd>,
    <trd>Ansiedlung</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolonia</trd>,
    <trd>osada</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&c_s;&c_je;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
</drv>
  </art>

