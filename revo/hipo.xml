<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hipo</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hipo.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="hipo.0.SCI">
    <ref tip="dif" cel="tro.mal0">maltro-</ref>
    <subsnc mrk="hipo.0.FIZL">
      <uzo tip="fak">FIZL</uzo>
      <dif>
        Maltro funkcianta, maltro intensa:
        <ekz>
          <tld/>tensio
          <klr>(ne sufi&ccirc;e forta sangopremo)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="ant">
      <ref cel="hiper.0.super">hiper-</ref>
    </refgrp>
  </snc>
  <trd lng="fr">hypo-</trd>
</drv>
</art>

