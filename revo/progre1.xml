<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: progre1.xml,v 1.28 2025/11/01 12:54:47 revo Exp $">
<kap>
  <rad>progresi</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="progre1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <dif>
      Tia <ref tip="super" cel="vic.0o.MAT">vico</ref> de nombroj, ke
      &ccirc;iu
      el ili esprimi&gcirc;as kiel <ctl>simpla</ctl> funkcio de la
      anta&ubreve;aj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="ca">progressi&oacute;<klr tip="amb"> (matem&agrave;tiques)</klr></trd>
  <trdgrp lng="cs">
    <trd>posloupnost</trd>,
    <trd>&ccaron;&iacute;seln&aacute; &rcaron;ada</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Progression</trd>,
    <trd>Zahlenfolge</trd>,
    <trd>fortschreitende <ind>Reihe</ind></trd>
  </trdgrp>
  <trd lng="en">progression</trd>
  <trd lng="fa">&a_t;&a_S;&a_A;&a_ayn;&a_d;</trd>
  <trd lng="fr">progression<klr tip="ind"> (math., suite)</klr></trd>
  <trdgrp lng="hu">
    <trd>sorozat</trd>,
    <trd>haladv&aacute;ny</trd>
  </trdgrp>
  <trd lng="ja">&#25968;&#21015;  <pr>&#12377;&#12358;&#x308C;&#12388;</pr></trd>
  <trdgrp lng="nl">
    <trd>rij<klr tip="ind"> (wisk.)</klr></trd>,
    <trd>reeks<klr tip="ind"> (wisk.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>post&eogonek;p</trd>,
    <trd>ci&aogonek;g</trd></trdgrp>
  <trd lng="pt">progress&atilde;o</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>postupnos&tcaron;</trd>,
    <trd>&ccaron;&iacute;seln&yacute; rad</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&c_i;&c_f;&#x43C;&c_je;&c_t;&c_i;&c_ch;&#x43D;&c_a;</trd>,
    <trd>&c_g;&c_je;&#x43E;&#x43C;&c_je;&c_t;&c_r;&c_i;&c_ch;&#x43D;&c_a; &#x43F;&c_r;&#x43E;&c_gu;&c_r;&c_je;&c_s;&c_ib;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D2F;&#x8FDB; <pr>l&#x11B;ij&#xEC;n</pr></trd>,
    <trd>&#x7D2F;&#36914; <pr>l&#x11B;ij&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="progre1.aritmetika0o">
  <kap>aritmetika <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Tia <ref tip="super" cel="progre1.0o">progresio</ref>, ke
      &ccirc;iu &gcirc;ia termo, esceptante la unuan, estas la
      aritmetika <ref cel="nombr.mez0o">meznombro</ref> de la anta&ubreve;a
      kaj de la posta:
      <ekz>
        <frm am="1, 3, 5, 7, 9, 11,...">1, 3, 5, 7, 9, 11,...</frm>;
      </ekz>
      <ekz>
        la <frm am="n">n</frm>-a termo de tia <tld/>o egalas al la sumo
        de
        konstanto <klr>(la <ref cel="difere.0o.progresio">diferenco</ref> de la
        <tld/>o)</klr> kun la anta&ubreve;a termo.
      </ekz>
    </dif>
    <ref tip="sin" cel="vic.aritmetika0o">aritmetika vico.</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_t;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">progressi&oacute; aritm&egrave;tica</trd>
  <trdgrp lng="de">
    <trd>arithmetische <ind>Folge</ind></trd>,
    <trd>arithmetische <ind>Progression</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>arithmetic <ind>sequence</ind></trd>,
    <trd>arithmetic <ind>progression</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_S;&a_A;&a_ayn;&a_d; &a_H;&a_s;&a_A;&a_b;&f_y;</trd>,
    <trd>&a_d;&a_n;&a_b;&a_A;&a_l;&a_h;&a_hamza_sure; &a_H;&a_s;&a_A;&a_b;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite arithm&eacute;tique</trd>,
    <trd>progression arithm&eacute;tique</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;mtani <ind>sorozat</ind></trd>
  <trd lng="ja">&#x7B97;&#34899;&#25968;&#21015;  <pr>&#12373;&#12435;&#12376;&#12421;&#12387;&#12377;&#12358;&#x308C;&#12388;</pr></trd>
  <trdgrp lng="nl">
    <trd>rekenkundige <ind>rij</ind></trd>,
    <trd>rekenkundige <ind>reeks</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ci&aogonek;g arytmetyczny</trd>,
    <trd>post&eogonek;p arytmetyczny</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_i;&c_f;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
    <ind>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7B49;&#x5DEE;&#25968;&#21015; <pr>d&#x11B;ngch&amacron;sh&#xF9;li&#xE8;</pr></trd>,
    <trd>&#x7B49;&#x5DEE;&#25976;&#21015; <pr>d&#x11B;ngch&amacron;sh&#xF9;li&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="progre1.geometria0o">
  <kap>geometria <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Tia <ref tip="super" cel="progre1.0o">progresio</ref>, ke
      &ccirc;iu &gcirc;ia termo, esceptante la unuan, estas la
      geometria <ref cel="nombr.mez0o">meznombro</ref> de la anta&ubreve;a
      kaj de la posta:
      <ekz>
        <frm am="1, 2, 4, 8, 16, 32,...">1, 2, 4, 8, 16, 32,...</frm>;
      </ekz>
      <ekz>
        la <frm am="n">n</frm>-a termo de tia <tld/>o egalas al la
        produto de
        konstanto <klr>(la <ref cel="kvocie.0o.progresio">kvociento</ref> de la
        <tld/>o)</klr> per la anta&ubreve;a termo.
      </ekz>
    </dif>
    <ref tip="sin" cel="vic.geometria0o">geometria vico.</ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">progressi&oacute; geom&egrave;trica</trd>
  <trd lng="cs">geometrick&aacute; posloupnost</trd>
  <trdgrp lng="de">
    <trd>geometrische <ind>Folge</ind></trd>,
    <trd>geometrische <ind>Progression</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>geometric <ind>sequence</ind></trd>,
    <trd>geometric <ind>progression</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_S;&a_A;&a_ayn;&a_d; &a_h;&a_n;&a_d;&a_s;&f_y;</trd>,
    <trd>&a_d;&a_n;&a_b;&a_A;&a_l;&a_h;&a_hamza_sure; &a_h;&a_n;&a_d;&a_s;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite g&eacute;om&eacute;trique</trd>,
    <trd>progression g&eacute;om&eacute;trique</trd>
  </trdgrp>
  <trd lng="hu">m&eacute;rtani <ind>sorozat</ind></trd>
  <trd lng="ja">&#x5E7E;&#x4F55;&#25968;&#21015;  <pr>&#x304D;&#x304B;&#12377;&#12358;&#x308C;&#12387;</pr></trd>
  <trdgrp lng="nl">
    <trd>meetkundige <ind>rij</ind></trd>,
    <trd>meetkundige <ind>reeks</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ci&aogonek;g geometryczny</trd>,
    <trd>post&eogonek;p geometryczny</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
    <ind>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="sk">geometrick&aacute; postupnos&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x7B49;&#x6BD4;&#25968;&#21015; <pr>d&#x11B;ngb&#x1D0;sh&#xF9;li&#xE8;</pr></trd>,
    <trd>&#x7B49;&#x6BD4;&#25976;&#21015; <pr>d&#x11B;ngb&#x1D0;sh&#xF9;li&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="progre1.harmona0o">
  <kap>harmona <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Tia <ref tip="super" cel="progre1.0o">progresio</ref>, ke
      &ccirc;iu &gcirc;ia termo, esceptante la unuan, estas la
      harmona <ref cel="nombr.mez0o">meznombro</ref> de la anta&ubreve;a
      kaj de la posta:
      <ekz>
        <frm am="1, 1/2, 1/3, 1/4, 1/5, 1/6,...">1, 1/2, 1/3, 1/4, 1/5, 1/6,...</frm>;
      </ekz>
      <ekz>
        la termoj de tia <tld/>o estas inversoj de la termoj de
        iu aritmetika <tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="vic.harmona0o">harmona vico.</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_m;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">progressi&oacute; harm&ograve;nica</trd>
  <trdgrp lng="de">
    <trd>harmonische <ind>Folge</ind></trd>,
    <trd>harmonische <ind>Progression</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harmonic <ind>sequence</ind></trd>,
    <trd>harmonic <ind>progression</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_S;&a_A;&a_ayn;&a_d; &a_h;&a_m;&a_s;&a_A;&a_z;</trd>,
    <trd>&a_d;&a_n;&a_b;&a_A;&a_l;&a_h;&a_hamza_sure; &a_h;&a_m;&a_s;&a_A;&a_z;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite harmonique</trd>,
    <trd>progression harmonique</trd>
  </trdgrp>
  <trd lng="hu">harmonikus <ind>sorozat</ind></trd>
  <trd lng="ja">&#x8ABF;&#x548C;&#25968;&#21015;  <pr>&#12385;&#12423;&#12358;&#x308F;&#12377;&#12358;&#x308C;&#12388;</pr></trd>
  <trdgrp lng="nl">
    <trd>harmonische <ind>rij</ind></trd>,
    <trd>harmonische <ind>reeks</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ci&aogonek;g harmoniczny</trd>,
    <trd>post&eogonek;p harmoniczny</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
    <ind>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>
</art>
