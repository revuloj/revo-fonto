<art mrk="$Id: furier.xml,v 1.18 2023/11/10 06:08:01 revo Exp $">
<kap><rad>furier</rad>/o</kap>
<drv mrk="furier.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="oficir.sub0o">Suboficiro</ref>, kiu zorgas pri la lo&gcirc;igo kaj nutrigo de
      la
      soldatoj dum la restado en fremda loko (= lo&gcirc;ejpreparisto):
      <ekz>
        lacega la laborskipo terenfalis &ccirc;irka&ubreve; la kariolo kaj tiam subite alvenis <tld/>o kun
        sia signallampo <klr>[&#x2026;]</klr> &ccirc;i dumentona simio devis meze de iu ajn &hcirc;aoso
        malkovri bestotrinkejojn
        <fnt>
          <aut>Louis-Ferdinand Celine, trad. Armela LeQuint kaj &Jcirc;ak Le Puil</aut>
          <vrk><url ref="https://books.google.de/books?id=02exBgAAQBAJ&amp;lpg=PA28&amp;ots=b2569lWFiG&amp;dq=furiero&amp;pg=PA28#v=onepage&amp;q=furiero&amp;f=false">Voja&gcirc;o
          &gcirc;is noktofino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="provia.0isto">proviantisto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_p;&c_t;&c_e;&c_n;&c_a;&c_r;&c_m;&c_u;&c_s;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_y;&c_r;&#x2019;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Quartiermacher</trd>
  <trd lng="fr">fourrier</trd>
  <trd lng="hu">sz&aacute;ll&aacute;smester</trd>
  <trdgrp lng="it">
    <trd>furiere</trd>,
    <trd>sottufficiale logistico</trd>
  </trdgrp>
  <trd lng="ja">&#x98DF;&#x7CE7;&#x30FB;&#x5BBF;&#x820E;&#x4FC2;&#x4E0B;&#x58EB;&#x5B98;  <pr>&#x3057;&#x3087;&#x304F;&#x308A;&#x3087;&#x3046;&#x30FB;&#x3057;&#x3085;&#x304F;&#x3057;&#x3083;&#x304B;&#x304B;&#x308A;&#x304B;&#x3057;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">foerier</trd>
  <trd lng="pl">kwatermistrz</trd>
  <trd lng="pt">furriel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_p;&c_t;&c_je;&c_n;&c_a;&c_r;&c_m;&c_u;&c_s;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_mol;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43F;&c_t;&c_je;&#x43D;&c_a;&c_r;&#x43C;&c_u;&c_s;</trd>,
    <trd>&#x43A;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&#x2019;&c_jeu;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#x519B;&#x9700;&#x5B98; <pr>j&#x16B;nx&#x16B;gu&#x101;n</pr></trd>
</drv>
</art>
