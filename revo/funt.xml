<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>funt</rad>/o
</kap>
<drv mrk="funt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Praktika <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="mas.0o.FIZ">maso</ref>, diversvalora la&ubreve; la landoj,
      proksimume egala al duono de <ref cel="kilogr.0o">kilogramo</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="sterli.0a">sterlinga</ref>.
    </refgrp>
    <trd lng="de">Pfund [500 g]</trd>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_n;&c_t;</trd>
  <trd lng="fr">livre<klr tip="ind"> (mesure de masse)</klr></trd>
  <trd lng="es">libra</trd>
  <trd lng="hu">font</trd>  
  <trd lng="nl">pond<klr tip="ind"> (500 g)</klr></trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_t; <klr>(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_v;&c_je;&c_s;&c_a;)</klr></trd>
</drv>

<drv mrk="funt.sterlinga0o">
  <kap>sterlinga <tld/>o, sterling<tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="pund.0o">pundo.</ref>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_n;&c_t; &c_s;&c_t;&c_e;&c_r;&c_l;&c_ib;&c_n;&c_g;&c_a;&c_w;</trd>
  <trd lng="de">Pfund <ind>Sterling</ind></trd>
  <trd lng="fr">livre sterling</trd>
  <trd lng="hu">font sterling</trd>  
  <trd lng="nl">pond sterling</trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_t; &c_s;&c_t;&c_je;&c_r;&c_l;&c_i;&c_n;&c_g;&c_o;&c_v;</trd>
</drv>
</art>
