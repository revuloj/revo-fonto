<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>funt</rad>/o
</kap>
<drv mrk="funt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="funt.0o.MEZ">
    <dif>
      Praktika <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="mas.0o.FIZ">maso</ref>, diversvalora la&ubreve; la landoj,
      proksimume egala al duono de <ref cel="kilogr.0o">kilogramo</ref>:
      <ekz>
        li havis egan tabakujon, kiu ampleksis <tld/>on da flartabako
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sterli.0a">sterlinga</ref>.
    </refgrp>
    <trd lng="de">Pfund [500 g]</trd>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_n;&c_t;</trd>
  <trd lng="en">pound</trd>
  <trd lng="es">libra</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_n;&a_d;<klr tip="ind"> (&f_y;&f_k;&a_A;&f_y; &a_j;&a_r;&a_m;)</klr></trd>,
    <trd>&f_g;&f_y;&a_r;&a_w;&a_A;&a_n;&f_k;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">livre<klr tip="ind"> (mesure de masse)</klr></trd>
  <trd lng="hu">font</trd>
  <trd lng="id">pon</trd>
  <trd lng="nl">pond<klr tip="ind"> (500 g)</klr></trd>
  <trd lng="pl">funt</trd>
  <trd lng="pt">libra</trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_t; <klr>
  (&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_v;&c_je;&c_s;&c_a;)
  </klr></trd>
</drv>

<drv mrk="funt.sterlinga0o">
  <kap>sterlinga <tld/>o, <var><kap>sterling<tld/>o</kap></var></kap>
  <snc mrk="funt.sterlinga0o.pundo">
    <uzo tip="fak">EKON</uzo>
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="pund.0o">pundo</ref>
  </snc>
  <trd lng="be">
&c_f;&c_u;&c_n;&c_t; &c_s;&c_t;&c_e;&c_r;&c_l;&c_ib;&c_n;&c_g;&c_a;&c_w;
  </trd>
  <trd lng="de">Pfund <ind>Sterling</ind></trd>
  <trd lng="en">pound sterling</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_n;&a_d; &a_A;&a_s;&a_t;&a_r;&a_l;&f_y;&a_n;&f_g;</trd>,
    <trd>&a_l;&f_y;&a_r;&a_h;&a_hamza_sure; &a_A;&a_s;&a_t;&a_r;&a_l;&f_y;&a_n;&f_g;</trd>
  </trdgrp>
  <trd lng="fr">livre sterling</trd>
  <trd lng="hu">font sterling</trd>
  <trd lng="id">pound sterling</trd>
  <trd lng="nl">pond sterling</trd>
  <trd lng="pl">funt szterling</trd>
  <trd lng="pt">libra esterlina</trd>
  <trd lng="ru">
&c_f;&c_u;&c_n;&c_t; &c_s;&c_t;&c_je;&c_r;&c_l;&c_i;&c_n;&c_g;&c_o;&c_v;
  </trd>
</drv>
</art>
