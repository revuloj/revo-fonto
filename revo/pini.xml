<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pini.xml,v 1.22 2024/02/17 06:43:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pini</rad>/o
</kap>
<drv mrk="pini.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pin.0o">pino</ref>
      (<trd lng="la">Pinus pinea</trd>), mediteranea 
      <ref tip="lst" lst="voko:koniferoj" cel="konifer.0o">konifero</ref>
      kun ombrelosimila bran&ccirc;aro kaj man&gcirc;eblaj semoj:
      <ekz>
        pinsemoj estas la man&gcirc;eblaj semoj <klr>[...]</klr> de la pinspeco pinio, kultivata &ccirc;ie en la mediteraneaj landoj, la blankeca, migdalece gusta kerno kreskas en la <tld/>strobiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127675">Pinsemo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        macunojukio <klr>(japana dol&ccirc;a&jcirc;o) </klr>simbolas ne&gcirc;on sur <tld/>arbon, t.e. longan vivon kaj grandan vivoforton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=210942">Macunojukio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_i;&c_n;&c_i;&c_ja;</trd>
  <trd lng="ca">pi pinyer</trd>
  <trdgrp lng="cs">
    <trd>pigniola</trd>,
    <trd>pignol</trd>,
    <trd>pignoli</trd>,
    <trd>pinelka</trd>,
    <trd>piniov&yacute; o&rcaron;&iacute;&scaron;ek</trd>,
    <trd>pi&ncaron;olka</trd>
  </trdgrp>
  <trd lng="de">Pinie</trd>
  <trdgrp lng="en">
    <trd>pine</trd>,
    <trd>stone pine</trd>,
    <trd>umbrella pine</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_j; &a_b;&a_A;&a_d;&a_A;&a_m;&f_y;</trd>,
    <trd>&f_k;&a_A;&a_j; &f_ch;&a_t;&a_r;&f_y;</trd>,
    <trd>&f_k;&a_A;&a_j; &a_s;&a_n;&f_g;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">pin parasol</trd>
  <trdgrp lng="hu">
    <trd>mandulafeny&odblac;</trd>,
    <trd>erny&odblac;feny&odblac;</trd>,
    <trd>p&iacute;nia</trd>,
    <trd>p&iacute;neafeny&odblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x30A4;&#x30BF;&#x30EA;&#x30A2;&#x30AB;&#x30B5;&#x30DE;&#x30C4;</trd>
  <trd lng="nl">tamme <ind>pijnboom</ind></trd>
  <trd lng="pl">pinia</trd>
  <trd lng="pt">pinheiro-manso</trd>
  <trd lng="ru">&c_p;&c_i;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">p&iacute;niov&yacute; orie&scaron;ok</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x43D;&#x456;&#x44F;</trd>,
    <trd>&#x441;&#x43E;&#x441;&#x43D;&#x430; &#x456;&#x442;&#x430;&#x43B;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x677E;&#x6811; <pr>s&#x14D;ngsh&#xF9;</pr></trd>,
    <trd>&#x677E;&#x6A39; <pr>s&#x14D;ngsh&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
