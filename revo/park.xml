<drv mrk="park.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="park.0o.plezurarbaro">
    <uzo tip="fak">HOR</uzo>
    <dif>
      &Ccirc;irka&ubreve;barita, arbori&ccirc;a, ta&ubreve;ge flegata
      terspaco, servanta kiel publika a&ubreve; privata promenejo;
      plezurarbareto:
      <ekz>
        li kuradas en la re&gcirc;a <tld/>o kaj sur la remparoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz><ekz>
        ala&ubreve;do en la Frederika <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz><ekz>
        ili elflugis tra la fenestroj de la palaco trans la <tld/>on kaj
        la arbaron
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz><ekz>
        en granda <tld/>o, longa duonmejlon, kuris cervoj, kapreoloj kaj
        leporoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz><ekz>
        Li elkuris <klr>[kaj]</klr> malaperis en la aleoj de l' <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz><ekz>
        en angulo de la <tld/>o, &ccirc;e la lago, trovi&gcirc;is eta
        benko, kiu estis la sola atestanto de iliaj varmaj kisoj
        <fnt>
          <aut>Julian Modest</aut>,
          <vrk>En la ombro de la silento</vrk>,
          <bib>Monato</bib>,
          <lok>1997/03, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxarde.0o">&gcirc;ardeno</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_r;&c_k;</trd>
    <trd lng="bg">&c_p;&c_a;&c_r;&c_k;</trd>
    <trdgrp lng="br">
      <trd>liorzh<klr tip="ind"> (park)</klr></trd>,
      <trd>park<klr tip="ind"> (liorzh)</klr></trd>
    </trdgrp>
    <trd lng="de">Park</trd>
    <trd lng="en">park</trd>
    <trd lng="fr">parc</trd>
    <trd lng="hu">park</trd>
    <trd lng="id">taman</trd>
    <trd lng="nl">park</trd>
    <trd lng="pt">parque</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_k;</trd>
    <trd lng="zh">&#x516C;&#x56ED; <pr>g&#x14D;ngyu&aacute;n</pr></trd>
  </snc>
  <snc mrk="park.0o.rezervejo">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="rezerv.natur0ejo">Naturrezervejo</ref>:
      <ekz>
        ni ser&ccirc;is en la urbo voja&gcirc;agentejon, kiu ofertus al
        ni ekskurson al la nacia <tld/>o
        <fnt>
          <aut>J. de N. Marques</aut>,
          <vrk>Voja&gcirc;o al Ushuaia</vrk>,
          <bib>Monato</bib>
          <lok>2002/02, p. 18</lok>
        </fnt>;
      </ekz><ekz>
        povas okazi, ke en la listo de mondaj kulturaj kaj naturaj
        hereda&jcirc;oj aperos anka&ubreve; nacia <tld/>o de la kur&scirc;a terlango,
        dividita inter Litovio kaj Rusio
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
            <vrk><url ref="&Monato;/2001/007273.php">Terlango -- natura valoro</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 21</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>
    <trd lng="br">park naturel</trd>
    <trd lng="de">Nationalpark</trd>
    <trdgrp lng="fr">
      <trd>parc naturel</trd>,
      <trd>r&eacute;serve naturelle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>term&eacute;szetv&eacute;delmi ter&uuml;let</trd>,
      <trd>nat&uacute;rpark</trd>
    </trdgrp>
    <trd lng="id"><ind>taman</ind> nasional</trd>
    <trdgrp lng="nl">
      <trd>natuurpark</trd>,
      <trd>park<klr tip="ind"> (natuurreservaat)</klr></trd>,
      <trd>natuurreservaat</trd>
    </trdgrp>
    <trd lng="pt">parque natural</trd>
    <trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_v;&c_je;&c_d;&c_n;&c_i;&c_k;
    </trd>
  </snc>
    <rim>
      <ref tip="hom" cel="park1.0o">Parkoj</ref> estas anka&ubreve; tri
      diinoj (= Parcoj).
    </rim>
  <trd lng="bo">&t_ga;&t_lata;&t_gigu;&t_nga;&t_tsheg;&t_ga;&t_tsheg;</trd>
  <trd lng="cs">parkovi&scaron;t&ecaron;</trd>
  <trdgrp lng="ja">
    <trd>&#x516C;&#22290;  <pr>&#12371;&#12358;&#12360;&#12435;</pr></trd>,
    <trd>&#22823;&#x5EAD;&#22290;  <pr>&#x304A;&#x304A;&#12400;&#12360;&#12435;</pr></trd>,
    <trd>&#x72E9;&#x731F;&#22290;  <pr>&#12375;&#12421;&#x308A;&#12423;&#12358;&#12360;&#12435;</pr></trd>,
    <trd>&#x96C6;&#x7A4D;&#25152;  <pr>&#12375;&#12421;&#12358;&#x305B;&#x304D;&#12376;&#12423;</pr></trd>,
    <trd>&#x7F6E;&#x304D;&#22580;  <pr>&#x304A;&#x304D;&#12400;</pr></trd>,
    <trd>&#x5EE0; <pr>&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x99D0;&#x8ECA;&#22580;  <pr>&#12385;&#12421;&#12358;&#12375;&#12419;&#12376;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">park</trd>
  <trd lng="sk">parkovisko</trd>
  <trd lng="uk">&c_p;&c_a;&c_r;&c_k;</trd>
</drv>

<drv mrk="park.amuz0o">
