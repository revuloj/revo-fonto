<drv mrk="park.0umi">
  <kap><tld/>umi <fnt><bib>PIV2</bib></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      Provizore <ref tip="super" cel="las.0i.restigi">lasi</ref>
      <ref cel="vetur.0ilo">veturilon</ref> stari en ta&ubreve;ga loko, dum
      oni &gcirc;in ne uzas:
      <ekz>
        <tld/>umu la a&ubreve;ton kaj alproksimi&gcirc;u al <klr>[la basenoj]</klr>
        <fnt><aut>Roberto Pigro</aut>: <vrk>Saturnia: Vintra magio</vrk>.
          <bib>Monato</bib></fnt>;
      </ekz><ekz>
        la &ccirc;eestantoj restis en siaj <tld/>umitaj a&ubreve;toj apud la pre&gcirc;ejo
        kaj a&ubreve;skultis radie dissendatan diservon
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2012/010520.php">Di(s)sendite</url></vrk>
        </fnt>;
      </ekz><ekz>
        en la urbocentro de lundo &gcirc;is vendredo por <tld/>umi necesas
        a&ccirc;eti <klr>[&#8230;]</klr> specialajn biletojn
        <fnt>
          <vrk><url ref="http://94uk.bialystok.pl/Podlahia-Vojevodio-kaj-Bjalistoko.html">Informilo
          pri la 94a UK en Bjalistoko</url></vrk>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;ina satelito, kiun oni &leftquot;<tld/>umos&rightquot;, se tiel diri, en la kosma
        spaco malanta&ubreve; la luno
        <fnt><aut>J.-J. Wwintraecken</aut>:
          <vrk>Kapti la kosmajn sekretojn</vrk>. <bib>Monato</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="park2.0i">parki <klr>(veturilon)</klr></ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">parkovat</trd>
  <trd lng="de">parken</trd>
  <trd lng="en"><klr>to </klr>park</trd>
  <trdgrp lng="fr">
    <trd>garer<klr> (tr)</klr></trd>,
    <trd>se <ind>garer</ind></trd>
  </trdgrp>
  <trd lng="id">parkir</trd>
  <trd lng="nl">parkeren<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">parkowa&cacute;</trd>
  <trd lng="pt">estacionar</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">parkova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_n;&c_a; &c_s;&c_t;&c_o;&c_ja;&c_n;&c_k;&c_u; (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_ib;&c_l;&c_mol;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x505C;&#x653E; <pr>t&#xED;ngf&#xE0;ng</pr></trd>,
    <trd>&#x6CCA;&#x8ECA; <pr>b&#xF3;ch&emacron;</pr></trd>,
    <trd>&#x6CCA;&#x8F66; <pr>b&#xF3;ch&emacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="park.0umejo">
