<art mrk="$Id: somateri.xml,v 1.3 2022/03/29 14:19:45 revo Exp $">
<kap>
  <rad>somateri</rad>/o
  <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="somateri.0o">
  <kap><tld/>o</kap>
  <snc mrk="somateri.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="anas.mol0o.ZOO">Molanaso</ref>:
      <ekz>
        centoj da <tld/>oj kaj hirundoj haltas por nesti, <klr>[&#x2026;]</klr> homoj tie
        neniel &gcirc;enas la kovadon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato-2012-2018">Turismo &dash; Finnlando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Eiderente</trd>
    <trd lng="en"><klr>common</klr> eider</trd>
    <trd lng="la">Somateria mollissima</trd>
    <trd lng="pl">edredon</trd>
    <trd lng="sv">ejder</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x433;&#x430; &#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x430;</trd>,
    <trd>&#x43F;&#x443;&#x445;&#x456;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>

