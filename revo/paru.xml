<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>paru</rad>/o
</kap>
<drv mrk="paru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="paru.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Parus</trd>), gracia birdo kun mallonga beko
      man&gcirc;anta insektojn kaj grengrajnojn.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ko&ncaron;adra</trd>,
    <trd>mod&rcaron;inka</trd>,
    <trd>babka</trd>
  </trdgrp>
  <trd lng="de">Meise</trd>
  <trdgrp lng="en">
    <trd>Titmouse</trd>,
    <trd>Tit</trd>
  </trdgrp>
  <trd lng="hu">cinege</trd>
  <trd lng="nl">mees</trd>
  <trd lng="pl">sikora</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_i;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_z;&c_o;&c_r;&c_je;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_s;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="paru.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Paridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Meisen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
