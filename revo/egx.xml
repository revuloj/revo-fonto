<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>e&gcirc;</rad>/o
</kap>
<drv mrk="egx.0o">
<kap><tld/>o</kap>
  <snc mrk="egx.0o.pluredro">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="super" cel="strek.0o.segmento">Streko</ref> a&ubreve;
      parto de rekto, komuna al du apudaj <ref cel="edr.0o">edroj</ref>:
      <ekz>
        la <tld/>oj de <ref cel="edr.plur0o">pluredro</ref> konsistigas 
        &ccirc;iuj <ref tip="super" cel="later.0o.streko">lateroj</ref> de 
        &gcirc;iaj <ref cel="fac.0o.pluredro">facoj</ref>;
      </ekz>
      <ekz>
        la <tld/>oj de <ref cel="edr.tri0o">triedro</ref> estas tri duonrektoj, 
        tiu de <ref cel="edr.du0o">duedro</ref> estas rekto.
      </ekz>
    </dif>
    <trd lng="de">Kante</trd>
    <trd lng="en">edge</trd>
    <trd lng="ru">&c_r;&c_je;&c_b;&c_r;&c_o;</trd>
  </snc>
  <snc num="2">
    <dif>
      Longforma alta&jcirc;o; kresto.  
    </dif>
  </snc>
  <trd lng="fr">ar&ecirc;te<klr tip="ind"> (ligne, cr&ecirc;te)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
