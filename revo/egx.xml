<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>e&gcirc;</rad>/o
</kap>
<drv mrk="egx.0o">
  <kap><tld/>o</kap>
  <snc mrk="egx.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="egx.0o.pluredro">
      <fnt><bib>MatTerm</bib><lok>p. 29</lok></fnt>
      <dif>
        <ref tip="super" cel="strek.0o.segmento">Streko</ref> a&ubreve;
        parto de rekto, komuna al du apudaj <ref cel="edr.0o">edroj</ref>:
        <ekz>
          la <tld/>ojn de <ref cel="edr.plur0o">pluredro</ref> konsistigas
          &ccirc;iuj <ref tip="super" cel="later.0o.streko">lateroj</ref> de
          &gcirc;iaj <ref cel="fac.0o.pluredro">facoj</ref>;
        </ekz>
        <ekz>
          la <tld/>oj de <ref cel="edr.tri0o">triedro</ref> estas tri 
          duonrektoj, tiu de <ref cel="edr.du0o">duedro</ref> estas rekto.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="egx.0o.grafeo">
      <fnt><bib>KompLeks</bib></fnt>
      <ref tip="vid" cel="grafe.0o">grafeo.</ref>
    </subsnc>
    <trd lng="de">Kante</trd>
    <trd lng="en">edge</trd>
    <trd lng="fr">ar&ecirc;te<klr tip="ind"> (d'un poly&egrave;dre, d'un graphe)</klr></trd>
    <trd lng="ru">&c_r;&c_je;&c_b;&c_r;&c_o;</trd>
  </snc>
  <snc>
    <dif>
      Longforma alta&jcirc;o; kresto.
    </dif>
    <trd lng="fr">ar&ecirc;te<klr tip="ind"> (ligne, cr&ecirc;te)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_mol; <klr>(&c_g;&c_o;&c_r;&c_y;, &c_h;&c_o;&c_l;&c_m;&c_a;)</klr></trd>,
      <trd>&c_h;&c_r;&c_je;&c_b;&c_je;&c_t; <klr>(&c_g;&c_o;&c_r;&c_y;)</klr></trd>
    </trdgrp>
  </snc>
</drv>
</art>
