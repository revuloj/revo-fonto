<art mrk="$Id: emulsi.xml,v 1.21 2023/11/07 20:34:10 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>emulsi</rad>/o
</kap>
<drv mrk="emulsi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="disper.0ajxo.KEM">Dispersa&jcirc;o</ref>
      de normale ne mikseblaj flua&jcirc;oj, ekz. de oleo kaj akvo:
      <ekz>
        la blanketa &scirc;a&ubreve;mo, kiun oni ofte trovas en la anguloj de la palpebroj, venas de
        tio, ke en sekvo de ofta palpebrumado la larma fluida&jcirc;o kun la eliga&jcirc;o de la
        Meibomaj glandetoj per bati&gcirc;ado fari&gcirc;as speco de &scirc;a&ubreve;ma <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        origine &ccirc;iuj fotoj estis unukoloraj <klr>[&#x2026;]</klr> pro la kemia naturo de la
        <tld/>o plakita sur fotografaj plakoj kaj filmoj, kiu enhavis lumosentivajn
        ar&gcirc;entajn salojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=237556">Nigrablanka fotografio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="suspensi.0o">suspensio</ref>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_u;&c_l;&c_mol;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="cs">vrstva</trd>
  <trdgrp lng="de">
    <trd><ofc>1</ofc>Emulsion</trd>,
    <trd><ofc>1</ofc>Samenmilch</trd>
  </trdgrp>
  <trd lng="en"><ofc>1</ofc>emulsion</trd>
  <trd lng="es">emulsi&oacute;n</trd>
  <trd lng="fr"><ofc>1</ofc>&eacute;mulsion</trd>
  <trd lng="hu">emulzi&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x4E73;&#x5264;  <pr>&#x306B;&#x3085;&#x3046;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x4E73;&#x6DB2;  <pr>&#x306B;&#x3085;&#x3046;&#x3048;&#x304D;</pr></trd>,
    <trd>&#x4E73;&#x6FC1;&#x6DB2;  <pr>&#x306B;&#x3085;&#x3046;&#x3060;&#x304F;&#x3048;&#x304D;</pr></trd>,
    <trd>&#x30A8;&#x30DE;&#x30EB;&#x30B8;&#x30E7;&#x30F3;</trd>,
    <trd>&#x611F;&#x5149;&#x4E73;&#x5264;  <pr>&#x304B;&#x3093;&#x3053;&#x3046;&#x306B;&#x3085;&#x3046;&#x3056;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">emulsie</trd>
  <trd lng="pl"><ofc>1</ofc>emulsja</trd>
  <trd lng="pt">emuls&atilde;o</trd>
  <trd lng="ru"><ofc>1</ofc>&c_e;&c_m;&c_u;&c_l;&c_mol;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">emulzia</trd>
  <trd lng="uk">&c_je;&c_m;&c_u;&c_l;&c_mol;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E73;&#x5242; <pr>r&#x1D4;j&#xEC;</pr></trd>,
    <trd>&#x4E73;&#x72B6;&#x6DB2; <pr>r&#x1D4;zhu&#xE0;ngy&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
