<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: fitis.xml,v 1.2 2023/11/09 20:48:32 afido Exp $">
    <kap>
      <rad>fitis</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="fitis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="paser.0oformaj">Paseroforma</ref>
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      (<trd lng="la">Phylloscopus trochilus</trd>)
      migrema, similanta <ref cel="cxifcxaf.0o">&ccirc;if&ccirc;afon</ref>,
      sed kun helbrunaj kuroj kaj aparta kanto:
      <ekz>
        la <tld/>o <klr>[...]</klr> unu el la plej ordinaraj birdoj en la Nordo montris
        kiom malfacile estas klare konkludi pri la rolo de birdetoj kiel detruantoj
        de insektoj
        <fnt>
          <aut>C. Stop-Bowitz</aut>
          <vrk><url ref="https://scienca-revuo.info/files/293-1-431-1-10-20140426.pdf">Birdetoj kiel ekstermantoj de insektoj</url></vrk>
          <lok>El Scienca Revuo de ISAE, vol. 20 (1969), n-ro 3,4, p. 107</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/5b/Willow_Warbler_Phylloscopus_trochilus.jpg/480px-Willow_Warbler_Phylloscopus_trochilus.jpg" prm="CC BY-SA 2.5"><tld/>o
        <fnt>
          <aut>Andreas Trepte, propra verko</aut>
          <lok><url ref="&WCW;/File:Willow_Warbler_Phylloscopus_trochilus.jpg">https://commons.wikimedia.org/wiki/File:Willow_Warbler_Phylloscopus_trochilus.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Fitis</trd>,
      <trd>Fitislaubs&auml;nger</trd>
    </trdgrp>
    <trd lng="en">willow warbler</trd>
    <trd lng="fr">pouillot fitis</trd>
  </snc>
  <trd lng="zh">&#x67F3;&#x83BA;&#x5C5E; <pr>li&#x1D4;y&#x12B;ngsh&#x1D4;</pr></trd>
</drv>

  </art>

