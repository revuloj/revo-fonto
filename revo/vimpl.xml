<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vimpl.xml,v 1.5 2022/08/18 07:30:55 revo Exp $">
<kap>
  <rad>vimpl</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="vimpl.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="vimpl.0o.SIM">
    <dif>
      <ref tip="super" cel="flag.0o.SIM">Flago</ref> kun 
      ia speciala formo: longa, triangula, dupinta....:
      <ekz>
        vento strias fumon, ventmontrilo kaj flago &scirc;iri&gcirc;as,
        <tld/>o krakas, flugas polvo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_m;&c_p;&c_e;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>korouhvi&#x10D;ka</trd>,
    <trd>vlaje&#x10D;ka</trd>
  </trdgrp>
  <trd lng="fr">fanion</trd>
  <trd lng="sk">vlajka</trd>
  <trd lng="uk">&c_v;&c_i;&#x43C;&#x43F;&c_je;&#x43B;</trd>
</drv>
</art>

