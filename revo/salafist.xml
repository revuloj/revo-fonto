<vortaro>
<art mrk="$Id$">
<kap>
  <rad>salafist</rad>/o
</kap>
<drv mrk="salafist.0o">
  <kap><tld/>o</kap>
  <snc mrk="salafist.0o.ISL">
  <uzo tip="fak">ISL</uzo>
  <ref tip="dif" cel="salafism.0ano.ISL">salafismano</ref>
    <ekz>
      la plejparto de la lo&gcirc;antaro de Kunar kaj Nurestan
      estas <tld/>oj
      <fnt>
        <aut>S. Saleem Shahzad, trad. V. Lutermano</aut>,
        <vrk><url
        ref="http://eo.mondediplo.com/article.php3?id_article=1164">
        Kiel la talibanoj revenis</url></vrk>,
        <lok>le Monde diplomatique, 2006-09</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="en">
    <trd>Salafi</trd>,
    <trd>Salafist</trd>
  </trdgrp>
  <trd lng="fr">salafiste</trd>
</drv>
</art>

