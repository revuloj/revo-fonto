<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: niks.xml,v 1.12 2022/06/10 08:35:51 revo Exp $">
<kap>
  <rad>niks</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="niks.0o">
  <kap><tld/>o</kap>
  <snc mrk="niks.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Akvo<ref tip="super" cel="fe.0o.MIT">feo</ref>:
      <ekz>
        la <tld/>o <klr>...</klr> estas maljuna, silentema, stranga
        persono, kun pantalonoj el angila ha&ubreve;to kaj kun kamizolo
        el fi&scirc;aj skvamoj ornamita per akvolilioj
        <fnt><bib>Fab4</bib>, <lok>sonorila profundo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref tip="super" cel="nimf.0o.MIT">nimfo<sncref/></ref>,
      <ref cel="siren.0o.MIT">sireno<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>vodn&iacute;k</trd>,
    <trd>zelen&yacute; mu&zcaron;&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nixe</trd>,
    <trd>Meerjungfrau</trd>
  </trdgrp>
  <trd lng="fr">nixe</trd>
  <trd lng="hu">v&iacute;zit&uuml;nd&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>nix</trd>,
    <trd>nixe</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rusa&lstroke;ka</trd>,
    <trd>niksa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>nixe</trd>,
    <trd>nix</trd>,
    <trd>nixie</trd>    
  </trdgrp>
  <trd lng="sk">vodn&iacute;k</trd>
  <trd lng="sv">sj&ouml;jungfru</trd>
  <trd lng="uk">&c_v;&#x43E;&c_d;&#x44F;&#x43D;&c_i;&#x43A;</trd>
</drv>

<drv mrk="niks.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="niks.0ino.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Ina <tld/>o:
      <ekz>
        disvasti&gcirc;adis &ccirc;iam pli obstinaj vo&ccirc;oj, ke la <tld/>inoj ne permesas transponti Drinon
        <fnt>
          <aut>I. Andri&cacute;, trad. A. Sekelj</aut>
          <vrk>La ponto super Drino</vrk>
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_s;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="fr">nixe</trd>
  <trd lng="uk">&c_r;&c_u;&c_s;&c_a;&#x43B;&#x43A;&c_a;</trd>
</drv>
</art>

