<art mrk="$Id: triod.xml,v 1.14 2018/11/03 15:10:14 revo Exp $">
<kap>
  <rad>triod</rad>/o
</kap>
<drv mrk="triod.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">RAD</uzo>
    <dif>
      Trielektroda <ref tip="vid" cel="valv.elektrona0o">valvo</ref>:
      <ekz>
        altern-kurenta <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447421">Tiristoro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la invento de <nom>Lee de Forest</nom> de 1906, nome <tld/>a tubo a&ubreve;diona, poste havis profundan efikon al elektronika muziko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50893">Elektronika muziko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_jo;&c_d;</trd>
  <trd lng="ca">tr&iacute;ode</trd>
  <trd lng="cs">trioda</trd>
  <trd lng="de">Triode</trd>
  <trd lng="en">triode</trd>
  <trd lng="fr">triode</trd>
  <trd lng="hu">tri&oacute;da</trd>
  <trd lng="nl">triode</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_o;&c_d;</trd>
  <trd lng="sk">tri&oacute;da</trd>
</drv>
</art>
