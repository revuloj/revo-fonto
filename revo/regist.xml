<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>registr</rad>/i
</kap>
<drv mrk="regist.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="regist.0i.rektasenca">
    <dif>
      Orde kaj metode skribi en tiucela libro a&ubreve; kajero ciferojn,
      notojn pri faktoj, dokumentojn ks, pri kiuj oni volas konservi
      memoron:
      <ekz>
        la resta&jcirc;o de la urboj estos malgranda kaj knabo
        povos ilin registri<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la popolon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ita letero</ind>;
        <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_z;&c_n;&c_o;&c_je; &c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;</trd>
      </ekz>
      <ekz>
        en<tld/>adi militistojn<fnt>Z</fnt>, novajn abonantojn;
      </ekz>
      <ekz>
        <tld/>igi siajn paka&jcirc;ojn en la ekspedejo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>registrieren</trd>,
      <trd>verzeichnen</trd>,
      <trd>vermerken</trd>,
      <trd>einschreiben</trd>,
      <trd>eintragen</trd>,
      <trd>notieren</trd>
    </trdgrp>
  </snc>
  <snc mrk="regist.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Atenti por memori pri io:
      <ekz>
        tiun &ccirc;i jaron ni povis <tld/>i la uzadon de Esperanto en pluraj
        internaciaj kongresoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rimark.0i">rimarki</ref>,
      <ref cel="not.0i">noti</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>registrieren</trd>,
      <trd>bemerken</trd>,
      <trd>feststellen</trd>
    </trdgrp>
  </snc>
  <snc mrk="regist.0i.TEK">
    <uzo tip="klr">(pp aparato)</uzo>
    <dif>
      Montri per skribaj signoj la variojn
      de iu fenomeno.
    </dif>
    <trdgrp lng="de">
      <trd>aufzeichnen</trd>,
      <trd>aufnehmen</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">zaznamenat</trd>
  <trd lng="en">register</trd>
  <trd lng="nl">registreren</trd>
  <trd lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="regist.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Libro, kajero, pa&gcirc;o, listo, en kiu oni <tld/>as:
      <ekz>
        alfabeta
        <tld/>o de temoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Verzeichnis</trd>,
      <trd>Register</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_je;&c_s;&c_t;&c_r;</trd>
  </snc>
  <snc>
    <dif>
      Amplekso de iu vo&ccirc;o a&ubreve; instrumento.
    </dif>
    <trdgrp lng="de">
      <trd>Register</trd>,
      <trd>Stimmumfang</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;</trd>
  </snc>
  <trd lng="nl">register</trd>
</drv>

<drv mrk="regist.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Aufzeichnung</trd>,
    <trd>Registrierung</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="regist.0olibro">
  <kap><tld/>olibro</kap>
  <snc>
    <dif>
      = <tld/>o 1.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Verzeichnis</trd>,
    <trd>Register</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_je;&c_s;&c_t;&c_r;</trd>
</drv>
</art>
