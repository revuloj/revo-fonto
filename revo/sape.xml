<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sape.xml,v 1.22 2023/10/21 06:31:28 revo Exp $">
<kap>
  <rad>sape</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sape.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fosa&jcirc;o farita sub muro por &gcirc;in renversi.
    </dif>
      <trd lng="hu">akna<klr tip="ind"> (v&aacute;r alatt)</klr></trd>
      <trdgrp lng="it">
        <trd>galleria<klr> (tecnica della mina)</klr></trd>,
        <trd>tunnel<klr> (tecnica della mina)</klr></trd>,
        <trd>tecnica della <ind>mina</ind><klr> (mil.)</klr></trd>
      </trdgrp>
      <trd lng="nl">onderaardse <ind>gang</ind></trd>
  </snc>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Ccirc;iaspeca en- a&ubreve; sub-tera vojo, permesanta proksimi&gcirc;i al la
      malamikaj pozicioj.
    </dif>
    <trd lng="de">Sappe</trd>
    <trdgrp lng="it">
      <trd>galleria<klr> (mil.)</klr></trd>,
      <trd>trincea<klr> (mil.)</klr></trd>
      <trd>tunnel<klr> (mil.)</klr></trd>,
      <trd>cunicolo<klr> (mil.)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>loopgraaf</trd>,
      <trd>loopgracht</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podkop</trd>,
    <trd>p&rcaron;ibli&zcaron;. z&aacute;kop</trd>,
    <trd>sapa</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_q;&a_b;</trd>
  <trd lng="fr">sape</trd>
  <trdgrp lng="hu">
      <trd>akna(folyos&oacute;)</trd>,
      <trd>akna&aacute;rok</trd>,
      <trd>fut&oacute;&aacute;rok</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5751;&#x9053;  <pr>&#x3053;&#x3046;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x5BFE;&#x58D5;  <pr>&#x305F;&#x3044;&#x3054;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">podkop</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podkop</trd>,
    <trd>z&aacute;kop</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&#x43F;&c_a;</trd>,
    <trd>&c_h;&c_ib;&c_d; &c_s;&#x43F;&#x43E;&#x43B;&c_u;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43A;&#x43E;&#x43F;</trd>
  </trdgrp>
</drv>

<drv mrk="sape.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="fos.sub0i.sube">Subfosi</ref> ion por &gcirc;in
      ruinigi a&ubreve; transpasi:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis certa ke oni intence <tld/>as lian sanon per la uzo de
        drogoj en lia nutra&jcirc;o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_k;&c_o;&c_p;</ind></trd>
  <trdgrp lng="cs">
    <trd>podkopat opevn&ecaron;n&iacute; nep&rcaron;&iacute;tele</trd>,
    <trd>podkopat se</trd>,
    <trd>zakopat se</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_q;&a_b; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">saper</trd>
  <trd lng="hu">akn&aacute;t &aacute;s</trd>
  <trd lng="it">minare<klr> (tecnica della mina)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6398;&#x308A;&#x5D29;&#x3059;  <pr>&#x307B;&#x308A;&#x304F;&#x305A;&#x3059;</pr></trd>,
    <trd>&#x5BFE;&#x58D5;&#x3092;&#x6398;&#x308B;  <pr>&#x305F;&#x3044;&#x3054;&#x3046;&#x3092;&#x307B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>loopgraaf</ind> maken</trd>,
    <trd>een vestingmuur <ind>ondergraven</ind></trd>
  </trdgrp>
  <trd lng="pl">podkopywa&cacute;</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; <ind>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</ind></trd>
  <trdgrp lng="sk">
    <trd>podkopa&tcaron;</trd>,
    <trd>zakopa&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43F;&c_ib;&c_d;&#x43A;&#x43E;&#x43F;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43A;&#x43E;&#x43F;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F; &#x43F;&c_ib;&c_d;
    &#x43A;&#x43E;&c_g;&#x43E;-&#x43D;.</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i; &#x43F;&c_ib;&c_d;&c_r;&c_i;&c_v;&#x43D;&c_u; &c_d;&c_ib;&#x44F;&#x43B;&#x44C;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="sape.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="soldat.0o">Soldato</ref> destinita al
      <tld/>aj
      laboroj:
      <ekz>
        turkaj soldatoj <klr>...</klr> akompanataj de tiom da
        <tld/>istoj
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 39a, p.
        296a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
  <trd lng="cs">z&aacute;kopn&iacute;k</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_q;&a_b;&#x200C;&a_z;&a_n;</trd>,
    <trd>&a_n;&a_q;&a_b;&#x200C;&a_A;&a_f;&f_k;&a_n;</trd>,
    <trd>&a_n;&a_q;&a_A;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">sapeur</trd>
  <trdgrp lng="hu">
    <trd>akn&aacute;sz</trd>,
    <trd>&aacute;rk&aacute;sz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>guastatore<klr> (mil.)</klr></trd>,
    <trd>zappatore<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x5DE5;&#x5175;  <pr>&#x3053;&#x3046;&#x3078;&#x3044;</pr></trd>
  <trd lng="nl">sappeur</trd>
  <trd lng="pl">saper</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
  <trd lng="sk">z&aacute;kopn&iacute;k</trd>
  <trd lng="uk">&c_s;&c_a;&#x43F;&c_je;&c_r;</trd>
</drv>
</art>
