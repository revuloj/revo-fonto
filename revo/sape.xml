<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sape.xml,v 1.23 2024/03/07 09:19:30 revo Exp $">
<kap>
  <rad>sape</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="sape.0o">
  <kap><tld/>o</kap>
  <snc mrk="sape.0o.submura">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fosa&jcirc;o farita sub muro por &gcirc;in renversi:
      <ekz>
        <tld/>o-ta&ccirc;mentoj eksplodigis anta&ubreve; la okuloj de la eksterlandaj militata&scirc;eoj
        parton da <tld/>itaj sablodunoj
        <fnt>
          <aut>Karel &Ccaron;apek</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0292/0019.php"
              >Milito kontra&ubreve; salamandroj</url></vrk>
          <lok>&ccirc;ap. 4, Der Nordmolch</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="min.0o.ujo">mino<sncref/></ref>
    <trd lng="de">Mine</trd>
    <trd lng="hu">akna<klr tip="ind"> (v&aacute;r alatt)</klr></trd>
    <trdgrp lng="it">
      <trd>galleria<klr> (tecnica della mina)</klr></trd>,
      <trd>tunnel<klr> (tecnica della mina)</klr></trd>,
      <trd>tecnica della <ind>mina</ind><klr> (mil.)</klr></trd>
    </trdgrp>
    <trd lng="nl">onderaardse <ind>gang</ind></trd>
  </snc>
  <snc mrk="sape.0o.alira">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Ccirc;iaspeca en- a&ubreve; sub-tera vojo, permesanta proksimi&gcirc;i al la
      malamikaj pozicioj:
      <ekz>
        estis ni kvin en <tld/>o kaj ni atendis malamikan atakon
        <fnt>
          <vrk><url ref="http://www.ikue.org/cz/arkivo/a-99-3-15.htm"
              >La Virgulino Maria Rozaria</url></vrk>
          <lok>Dio Benu, n-ro 99-3/2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sappe</trd>,
      <trd>Stichgraben</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>galleria<klr> (mil.)</klr></trd>,
      <trd>trincea<klr> (mil.)</klr></trd>
      <trd>tunnel<klr> (mil.)</klr></trd>,
      <trd>cunicolo<klr> (mil.)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>loopgraaf</trd>,
      <trd>loopgracht</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podkop</trd>,
    <trd>p&rcaron;ibli&zcaron;. z&aacute;kop</trd>,
    <trd>sapa</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_q;&a_b;</trd>
  <trd lng="fr">sape</trd>
  <trdgrp lng="hu">
      <trd>akna(folyos&oacute;)</trd>,
      <trd>akna&aacute;rok</trd>,
      <trd>fut&oacute;&aacute;rok</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22353;&#36947;  <pr>&#12371;&#12358;&#12393;&#12358;</pr></trd>,
    <trd>&#23550;&#22741;  <pr>&#12383;&#12356;&#12372;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">podkop</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podkop</trd>,
    <trd>z&aacute;kop</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>,
    <trd>&c_h;&c_ib;&c_d; &c_s;&c_p;&c_o;&c_l;&c_u;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_k;&c_o;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="sape.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sape.0i.subfosi">
    <dif>
      <ref tip="super" cel="fos.sub0i.sube">Subfosi</ref> ion por &gcirc;in
      ruinigi a&ubreve; transpasi:
      <ekz>
        de la kazerna korto forveturis tri &ccirc;aroj &scirc;ar&gcirc;itaj je <tld/>ilaro
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis certa ke oni intence <tld/>as lian sanon per la uzo de
        drogoj en lia nutra&jcirc;o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="min.0i.MIL">mini<sncref/></ref>
    <trd lng="de">unterminieren</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_k;&c_o;&c_p;</ind></trd>
  <trdgrp lng="cs">
    <trd>podkopat opevn&ecaron;n&iacute; nep&rcaron;&iacute;tele</trd>,
    <trd>podkopat se</trd>,
    <trd>zakopat se</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_q;&a_b; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">saper</trd>
  <trd lng="hu">akn&aacute;t &aacute;s</trd>
  <trd lng="it">minare<klr> (tecnica della mina)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#25496;&#12426;&#23849;&#12377;  <pr>&#12411;&#12426;&#12367;&#12378;&#12377;</pr></trd>,
    <trd>&#23550;&#22741;&#12434;&#25496;&#12427;  <pr>&#12383;&#12356;&#12372;&#12358;&#12434;&#12411;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>loopgraaf</ind> maken</trd>,
    <trd>een vestingmuur <ind>ondergraven</ind></trd>
  </trdgrp>
  <trd lng="pl">podkopywa&cacute;</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; <ind>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</ind></trd>
  <trdgrp lng="sk">
    <trd>podkopa&tcaron;</trd>,
    <trd>zakopa&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_ib;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_k;&c_o;&c_p;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_p;&c_ib;&c_d;
    &c_k;&c_o;&c_g;&c_o;-&c_n;.</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i; &c_p;&c_ib;&c_d;&c_r;&c_i;&c_v;&c_n;&c_u; &c_d;&c_ib;&c_ja;&c_l;&c_mol;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sape.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="soldat.0o">Soldato</ref> destinita al
      <tld/>aj
      laboroj:
      <ekz>
        turkaj soldatoj <klr>[&#8230;]</klr> akompanataj de tiom da
        <tld/>istoj
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 39a, p.
        296a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
  <trd lng="cs">z&aacute;kopn&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Sapeur</trd>,
    <trd>Mineur</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_q;&a_b;&zwnj;&a_z;&a_n;</trd>,
    <trd>&a_n;&a_q;&a_b;&zwnj;&a_A;&a_f;&f_k;&a_n;</trd>,
    <trd>&a_n;&a_q;&a_A;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">sapeur</trd>
  <trdgrp lng="hu">
    <trd>akn&aacute;sz</trd>,
    <trd>&aacute;rk&aacute;sz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>guastatore<klr> (mil.)</klr></trd>,
    <trd>zappatore<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#24037;&#20853;  <pr>&#12371;&#12358;&#12408;&#12356;</pr></trd>
  <trd lng="nl">sappeur</trd>
  <trd lng="pl">saper</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
  <trd lng="sk">z&aacute;kopn&iacute;k</trd>
  <trd lng="uk">&c_s;&c_a;&c_p;&c_je;&c_r;</trd>
</drv>
</art>
