<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sape</rad>/o
</kap>
<drv mrk="sape.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Fosa&jcirc;o farita sub muro por &gcirc;in renversi.
    </dif>
    <trd lng="nl">onderaardse <ind>gang</ind></trd>
    <trd lng="nl">akna<klr tip="ind"> (v&aacute;r alatt)</klr></trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;iaspeca subtera laboro, permesanta proksimi&gcirc;i al la malamikaj
      pozicioj.
    </dif>
    <trd lng="de">Sappe</trd>
    <trd lng="nl">loopgraaf</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">sape</trd>
  <trdgrp lng="hu">
      <trd>akna(folyos&oacute;)</trd>,
      <trd>akna&aacute;rok</trd>,
      <trd>fut&oacute;&aacute;rok</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sape.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="fos.sub0i.sube">Subfosi</ref> ion por &gcirc;in
      ruinigi a&ubreve; transpasi.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_k;&c_o;&c_p;</ind></trd>
  <trd lng="fr">saper</trd>
  <trd lng="hu">akn&aacute;t &aacute;s</trd>
  <trdgrp lng="nl">
    <trd>een <ind>loopgraaf</ind> maken</trd>,
    <trd>een vestingmuur <ind>ondergraven</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; <ind>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</ind></trd>
</drv>

<drv mrk="sape.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="soldat.0o">Soldato</ref> destinita al
      <tld/>aj
      laboroj:
      <ekz>
        turkaj soldatoj <klr>...</klr> akompanataj de tiom da
        <tld/>istoj
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 39a, p.
        296a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
  <trd lng="fr">sapeur</trd>
  <trdgrp lng="hu">
    <trd>akn&aacute;sz</trd>,
    <trd>&aacute;rk&aacute;sz</trd>
  </trdgrp>
  <trd lng="nl">sappeur</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;&c_jo;&c_r;</trd>
</drv>
</art>
