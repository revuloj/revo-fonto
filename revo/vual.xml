<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vual.xml,v 1.28 2022/12/21 08:30:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vual</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vual.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vual.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Peco de &scirc;tofo, <ref tip="super" cel="tuk.0o">tuko</ref>, 
      uzata por ka&scirc;i, kovri ion, precipe la viza&gcirc;on
      de virinoj:
      <ekz>
        viaj okuloj estas kolombaj, malanta&ubreve; via <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 4:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        li donis al li purpuran skarpon kaj <tld/>on el <ref cel="muslin.0o">muslino</ref>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento kaptadis &scirc;ian longan blankan <tld/>on kaj iu &scirc;in vidis
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia <ref cel="tul.0o">tula</ref> <tld/>o &scirc;in envolvis per nubi&gcirc;anta amaso
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni vidis tra la polva nebulo kvaza&ubreve; tra <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la muroj de la ne&gcirc;a kabano falis kiel aera, malpeza, nebula <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="tuk.kovro0o">kovrotuko</ref>
    <refgrp tip="vid">
      <ref cel="gaz.0o">gazo</ref>
    </refgrp>
  </snc>
  <snc mrk="vual.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;io, kio ka&scirc;as la veron a&ubreve; la realon,
      kio malhelpas rigardi ion klare:
      <ekz>
        kvaza&ubreve; <tld/>o fori&gcirc;us de tiu unue nekomprenebla afero
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam ajn oni turnas sin al la Sinjoro, la <tld/>o estas forprenata
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 3:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        an&gcirc;elo ku&scirc;igis siajn flugilojn kiel <tld/>on &ccirc;irka&ubreve; la dormanto
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj palpebroj &scirc;velis, la pupiloj kovri&gcirc;is per vitreca <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        unuaj fum<tld/>oj en la kvieta aero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        salikoj vestis sin per delikata <tld/>o el graciaj florpenda&jcirc;oj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigardante unu alian tra la <tld/>o de l&#8217; konvinko pri nia senpekeco
        <fnt>
          <bib>IKr</bib>
          <lok>Bol&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        de tempo al tempo la memoro de la pasintaj suferoj, <klr>[&#8230;]</klr> &jcirc;etis
        funebran <tld/>on sur &scirc;iajn novajn &gcirc;ojojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio malfermos viajn okulojn al la lumo kaj forigos de ili la <tld/>on
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="enigm.0o.FIG">enigmo<sncref/></ref>,
      <ref cel="iluzi.0o">iluzio</ref>,
      <ref cel="mister.0o">mistero</ref>,
      <ref cel="nebul.0o.FIG">nebulo<sncref/></ref>,
      <ref cel="nub.0o.malklara">nubo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_e;&c_l;&c_ju;&c_m;</trd>
  </trdgrp>
  <trd lng="br">gouel</trd>
  <trd lng="ca">vel</trd>
  <trdgrp lng="cs">
    <trd>plachetka</trd>,
    <trd>plena</trd>,
    <trd>rou&scaron;ka</trd>,
    <trd>&scaron;&aacute;tek</trd>
  </trdgrp>
  <trd lng="de">Schleier</trd>
  <trd lng="en">veil</trd>
  <trd lng="es">velo</trd>
  <trd lng="fr">voile<klr> (tissu)</klr></trd>
  <trd lng="hu">f&aacute;tyol</trd>
  <trd lng="nl">sluier</trd>
  <trd lng="pt">v&eacute;u</trd>
  <trd lng="ru">&c_v;&c_u;&c_a;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>plienka</trd>,
    <trd>r&uacute;&scaron;ka</trd>,
    <trd>&scaron;atka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_u;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_l;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="vual.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vual.0i.TEKS">
    <dif>
      Kovri, <ref tip="vid" cel="kasx.0i">ka&scirc;i</ref>
      per <tld/>o a&ubreve; <tld/>simila tega&jcirc;o:
      <ekz>
        densaj nuboj <tld/>is la lunon;
      </ekz>
      <ekz>
        la valo estis <tld/>ita de nebulo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind>kiam nokto <tld/>as, &ccirc;iuj koloroj egalas</ind>
        <trd lng="fr">la nuit tous les chats sont gris</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volv.en0i">envolvi</ref>
    </refgrp>    
    <trdgrp lng="ca">
      <trd>velar</trd>,
      <trd>envelar</trd>
    </trdgrp>
  </snc>
  <snc mrk="vual.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kovri, konfuzante a&ubreve; malhelpante la rigardon:
      <ekz>
        la larmoj <tld/>is liajn okulojn<fnt>B</fnt>,
      </ekz>
      <ekz>
        lian rigardon;
      </ekz>
      <ekz>
        okuloj <tld/>itaj per mal&gcirc;ojo;
      </ekz>
      <ekz>
        ne&gcirc;pluvo <tld/>is la vitrojn;
      </ekz>
      <ekz>
        la partieco <tld/>as vian prudenton<fnt>B</fnt>;
      </ekz>
      <ekz>
        frunto <tld/>ita <ind>(maltrankviligita)</ind>
        per medito<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ocultar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_u;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_a;&c_k;&c_r;&c_y;&c_c;&c_mol; 
      &c_v;&c_u;&c_a;&c_l;&c_mol;&c_l;&c_ju;</trd>
  </trdgrp>
  <trd lng="br">gouelia&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>ututlat</trd>,
    <trd>zahalit</trd>,
    <trd>zakr&yacute;t</trd>,
    <trd>zaml&zcaron;it</trd>
  </trdgrp>
  <trd lng="de">verschleiern</trd>
  <trd lng="en">veil</trd>
  <trdgrp lng="es">
    <trd>velar</trd>,
    <trd>ocultar</trd>
  </trdgrp>
  <trd lng="fr">voiler</trd>
  <trdgrp lng="hu">
    <trd>lef&aacute;tyoloz</trd>,
    <trd>elkend&odblac;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sluieren</trd>,
    <trd>omsluieren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_mol; <ind>&c_v;&c_u;&c_a;&c_l;&c_mol;&c_ju;</ind></trd>,
    <trd>&c_z;&c_a;&c_v;&c_u;&c_a;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ututla&tcaron;</trd>,
    <trd>zahali&tcaron;</trd>,
    <trd>zahmli&tcaron;</trd>,
    <trd>zakry&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_k;&c_u;&c_t;&c_a;&c_t;&c_i; &c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_v;&c_u;&c_a;&c_l;&c_l;&c_ju;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_z;&c_a;&c_v;&c_u;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="vual.sen0e">
  <kap>sen<tld/>e</kap>
  <snc mrk="vual.sen0e.FIG">
    <dif>
      Tute <ref tip="vid" cel="kasx.mal0a">malka&scirc;e</ref>:
      <ekz>
        mi sen<tld/>e diros al vi, ke...
      </ekz>      
    </dif>
  </snc>
  <trd lng="ca">obertament</trd>
  <trd lng="fr">sans fard</trd>
</drv>

<drv mrk="vual.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="vual.sen0igi.KOMUNE">
    <dif>
      Forigi la <tld/>on de iu a&ubreve; io.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_mol; 
    <ind>&c_v;&c_u;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="br">diouelia&ntilde;</trd>
  <trd lng="ca">desvelar</trd>
  <trdgrp lng="cs">
    <trd>odhalit</trd>,
    <trd>odkr&yacute;t</trd>,
    <trd>sejmout z&aacute;voj</trd>
  </trdgrp>
  <trd lng="de">entschleiern</trd>
  <trdgrp lng="en">
     <trd>unveil</trd>,
     <trd>reveal</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;voiler</trd>
  <trdgrp lng="hu">
    <trd>fellebbenti a <ind>f&aacute;tylat</ind></trd>,
    <trd>leleplez</trd>
  </trdgrp>
  <trd lng="nl">ontsluieren</trd>
  <trdgrp lng="sk">
    <trd>odhali&tcaron;</trd>,
    <trd>zbavi&tcaron; z&aacute;voja</trd>
  </trdgrp>
</drv>
</art>
