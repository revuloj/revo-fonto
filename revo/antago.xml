<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: antago.xml,v 1.19 2022/08/08 14:32:16 revo Exp $">
<kap>
  <rad>antagonism</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="antago.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="antago.0o.KOMUNE">
    <dif>
      Stato de kontra&ubreve;bataleco,
      situacio de <ref tip="vid" cel="antago1.0o.KOMUNE">antagonistoj</ref>:
      <ekz>
        estas granda <tld/>o inter la propagandistoj kiuj volas 
        kiel eble plej facilan, kaj la literaturistoj, kiuj volas kiel 
        eble plej esprimkapablan lingvon
        <fnt>
          <aut>[K. Kalocsay]</aut>,
          <vrk><url ref="http://www.tekstoj.nl/lm/lm25-1/leterkesto.html">
          Leterkesto</url></vrk>,
          <lok>Literatura Mondo, 1925-01, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_a;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>antagonizmus</trd>,
    <trd>nesmi&rcaron;iteln&yacute; protiklad</trd>,
    <trd>rozpor</trd>,
    <trd>soupe&rcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Gegensatz</trd>,
      <trd>Gegens&auml;tzlichkeit</trd>,
      <trd>Widerspruch</trd>,
      <trd>Widerspr&uuml;chlichkeit</trd>,
      <trd>Antagonismus</trd>
  </trdgrp>
  <trd lng="en">antagonism</trd>
  <trd lng="es">antagonismo</trd>
  <trd lng="fr">antagonisme</trd>
  <trdgrp lng="he">
    <trd>&#x5D9;&#x5E8;&#x5D9;&#x5D1;&#x5D5;&#x5EA;</trd>,
    <trd>&#x5E9;&#x5E0;&#x5D0;&#x5D4;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_a;&c_g;&c_o;&c_n;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>antagonizmus</trd>,
    <trd>rozpor</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43D;&c_t;&c_a;&c_g;&#x43E;&#x43D;&c_ib;&c_z;&#x43C;</trd>,
    <trd>&#x43D;&c_je;&#x43F;&c_r;&c_i;&#x43C;&c_i;&c_r;&c_je;&#x43D;&#x43D;&c_ib; &#x43F;&c_r;&#x43E;&c_t;&c_i;&c_r;&c_ib;&c_ch;&c_ch;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="antago.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="antago.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="kontraux.0a.KOMUNE">Kontra&ubreve;a</ref>,
      kontra&ubreve;batalanta:
      <ekz>
        <tld/>aj fortoj;
      </ekz>
      <ekz>
        la &ccirc;efa ka&ubreve;zo de la ekesto de &scirc;tato estas la dividi&gcirc;o de la 
        primitiva, senklasa socio en malamikajn, <tld/>ajn klasojn
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="&STEB;juro/marksisma-koncepto-pri-sxtato.htm">
          Marksisma koncepto pri &scirc;tato kaj juro</url></vrk>,
          Internacia Jura Revuo, 1976:2, p. 2a-8a<lok/>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_a;&c_g;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>antagonique</trd>,
    <trd>antagoniste<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5E2;&#x5D5;&#x5D9;&#x5D9;&#x5E0;&#x5D5;&#x5EA;</trd>
</drv>

</art>
