    <trd>&c_l;&c_je;&c_gu;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_d;&c_i;&c_n;&c_o;&c_k; &c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_ji; &c_m;&c_ib;&c_s;&c_ib;&c_ji;</trd>
  </trdgrp>
  <trdgrp lng="zh">
