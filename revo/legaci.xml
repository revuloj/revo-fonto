<kap>
  <rad>legaci</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="legaci.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Vic<ref tip="vid" cel="ambasa1.0ejo">ambasadorejo</ref>:
      <ekz>
        de Demosteno konservi&gcirc;is 69 oratora&jcirc;oj, el kiuj la plej konataj estas la
        &leftquot;Filipikoj&rightquot;, <klr>[&#8230;]</klr> &leftquot;Pri la Falsa <tld lit="L"/>o&rightquot; kaj &leftquot;Pri la Krono&rightquot;
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> havas personajn kontaktojn <klr>[&#8230;]</klr> kun eksterlandaj
        <tld/>oj kaj konsulejoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Francis-Marie Martinez Picabia</nom> estis bone konata
        pentristo kaj poeto, kiu
        naski&gcirc;is de franca patrino kaj hispana patro, kiu estis ata&scirc;eo
        &ccirc;e la kuba
        <tld/>o en Parizo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=149008">Francis Picabia</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_m;&c_ib;&c_s;&c_ib;&c_ja;</ind></trd>,
    <trd>&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_je;
    <ind>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca">legaci&oacute;</trd>
  <trd lng="de">Gesandtschaft</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;vets&eacute;g</trd>,
    <trd>diplom&aacute;ciai k&eacute;pviselet</trd>,
    <trd>misszi&oacute;<klr tip="ind"> (diplom&aacute;ciai)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#20844;&#20351;&#39208;  <pr>&#12371;&#12358;&#12375;&#12363;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>gezantschap</trd>,
    <trd>legatie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>legacja</trd>,
    <trd>poselstwo  <klr>(zagraniczne)</klr></trd>
  </trdgrp>
  <trd lng="pt">lega&ccedil;&atilde;o<klr tip="amb"> (diplomacia)</klr></trd>
  <trdgrp lng="ro">
