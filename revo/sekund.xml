<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sekund</rad>/o
</kap>
<drv mrk="sekund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sekund.0o.tempo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Baza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de tempo en la
      Internacia Sistemo de mezuroj; simb. s:
      <ekz>
        unu <tld/>o egalas al 31 556 925,9747-ono de iu tropika jaro;
      </ekz>
      <ekz>
        <tld/>o estas sesdekono de minuto.
      </ekz>
    </dif>
    <trd lng="de">Sekunde</trd>
    <adm>
    Restas por difini ie, kio estas tropika jaro... <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="sekund.0o.angulo">
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de 
      <ref cel="angul.0o.MAT">angulo</ref>, egala al sesdekono de sesdekono 
      de <ref tip="malprt" cel="grad.0o">grado</ref>.
    </dif>
    <trd lng="de">Bogensekunde</trd>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>sekunda</trd>,
    <trd>vte&rcaron;ina</trd>
  </trdgrp>
  <trd lng="en">second</trd>
  <trd lng="fr">seconde<klr tip="ind"> (unit&eacute; de temps, d'angle)</klr></trd>
  <trd lng="nl">sekonde</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;</trd>
</drv>
</art>
