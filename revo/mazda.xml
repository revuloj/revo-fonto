<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mazda.xml,v 1.16 2024/01/25 20:10:35 revo Exp $">
<kap>
  <rad>mazda</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mazda.0o">
  <kap><tld lit="M"/>o</kap>
  <snc mrk="mazda.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="sin" cel="di.0o.monoteisma">Dio</ref> en
      <ref tip="vid" cel="mazda.0ismo"><tld/>ismo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_h;&c_u;&c_r;&c_a; &c_M;&c_a;&c_z;&c_d;&c_a;</trd>,
    <trd>&c_O;&c_r;&c_m;&c_a;&c_z;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>Ahura Mazd&amacron;</trd>,
    <trd>Ormazd</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ahura Mazda</trd>,
    <trd>Ahuramazda</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Ahura Mazda</trd>,
    <trd>Oromasdes</trd>,
    <trd>Ahuramazda</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_h;&a_w;&a_r;&a_A;&a_m;&a_z;&a_d;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>Ahura Mazd&acirc;</trd>,
    <trd>Ohrmazd</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Ahura Mazd&aacute;</trd>,
    <trd>Ormuzd</trd>
  </trdgrp>
  <trd lng="pl">Ormuzd</trd>
  <trd lng="pt">Ahura-Mazd&aacute;</trd>
  <trd lng="ro">Ahura Mazda</trd>
  <trdgrp lng="ru">
    <trd>&c_A;&c_h;&c_u;&c_r;&c_a;&c_m;&c_a;&c_z;&c_d;&c_a;</trd>,
    <trd>&c_O;&c_r;&c_m;&c_u;&c_z;&c_d;</trd>,
    <trd>&c_O;&c_r;&c_m;&c_a;&c_z;&c_d;</trd>
  </trdgrp>
  <trd lng="zh">&#x7946; <pr>Xi&#x101;n</pr></trd>
</drv>

<drv mrk="mazda.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="mazda.0ano.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Homo akceptanta la doktrinon kaj praktikon de <tld/>ismo.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_r;&c_a;&c_a;&c_s;&c_t;&c_r;&c_y;&c_je;&c_c;</trd>
  <trdgrp lng="ca">
    <trd>mazdeu</trd>,
    <trd>mazdaista</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zoroastrier</trd>,
    <trd>Zarathustrier</trd>,
    <trd>Mazdaverehrer</trd>
  </trdgrp>
  <trd lng="en">Zoroastrian</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_r;&a_t;&a_sh;&a_t;&f_y;</trd>,
    <trd>&a_m;&a_z;&a_d;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gu&egrave;bre</trd>,
    <trd>parsi</trd>,
    <trd>mazd&eacute;en<klr> (subst.)</klr></trd>,
    <trd>zoroastrien</trd>
  </trdgrp>
  <trd lng="hu">Mazda-h&iacute;v&odblac;</trd>
  <trd lng="pl">mazdaista</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_b;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_s;</trd>,
    <trd>&c_z;&c_o;&c_r;&c_o;&c_a;&c_s;&c_t;&c_r;&c_i;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="mazda.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="mazda.0ismo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Persdevena <ref tip="lst" cel="religi.0o.REL" lst="voko:religioj">religio</ref>
      reformita de la profeto
      <ref tip="vid" cel="zaratusxtr.0o">Zaratu&scirc;tro</ref>
      en la 7a jarcento a. K., kies &ccirc;efa sankta libro estas la
      <ref tip="vid" cel="avest.0o">Avesto</ref>,
      kun malmultaj anoj precipe en centra Irano kaj
      Gu&gcirc;aratio:
      <ekz>
        en la antikva tempo oni pensis ke estroj, re&gcirc;oj kaj religiaj
        gvidantoj
        estas elektitaj de Dio. En tiu &ccirc;i verso <tld/>ismo aldonas
        pluan kondi&ccirc;on
        <klr>...</klr> kaj tiu kondi&ccirc;o estas virto
        <fnt>
          <aut>Be&ccirc;jo</aut>,
          <vrk><url ref="http://becxjo.blogspot.com">La mondo el mia
          vidpunkto</url></vrk>,
          <lok>2005-11-21</lok>
        </fnt>
      </ekz>      
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_a;&c_s;&c_t;&c_r;&c_y;&c_z;&c_m;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_d;&c_e;&c_ib;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">mazdaisme</trd>
  <trd lng="cs">mazdaizmus</trd>
  <trdgrp lng="de">
    <trd>Zoroastrismus</trd>,
    <trd>Zarathustrismus</trd>,
    <trd>Mazdaismus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Zoroastrianism</trd>,
    <trd>Mazdaism</trd>,
    <trd>Mazdayasna</trd>
  </trdgrp>
  <trd lng="fa">&a_A_madda;&f_y;&f_y;&a_n; <ind>&a_z;&a_r;&a_t;&a_sh;&a_t;</ind></trd>
  <trdgrp lng="fr">
    <trd>mazd&eacute;isme</trd>,
    <trd>parsisme</trd>,
    <trd>zoroastrisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mazdaizmus</trd>,
    <trd>perzsa vall&aacute;s</trd>,
    <trd>p&aacute;rszi vall&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zaratusztrianizm</trd>
    <trd>zoroastryzm</trd>
  </trdgrp>
  <trd lng="pt">masde&iacute;smo</trd>
  <trd lng="ro">zoroastrism</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_z;&c_d;&c_je;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_z;&c_o;&c_r;&c_o;&c_a;&c_s;&c_t;&c_r;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">mazdaizmus</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_z;&c_d;&c_a;&c_ji;&c_z;&c_m;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_d;&c_je;&c_ji;&c_z;&c_m; (&c_r;&c_je;&c_l;&c_ib;&c_g;&c_ib;&c_ja;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62DC;&#x706B;&#x6559; <pr>B&#xE0;ihu&#x1D2;ji&#xE0;o</pr></trd>,
    <trd>&#x6CE2;&#x65AF;&#x6559; <pr>B&#x14D;s&#x12B;ji&#xE0;o</pr></trd>,
    <trd>&#x7410;&#x7F57;&#x4E9A;&#x65AF;&#x5FB7;&#x6559; <pr>Su&#x1D2;lu&#xF3;y&#xE0;s&#x12B;d&#xE9;ji&#xE0;o</pr></trd>,
    <trd>&#x7463;&#x7F85;&#x4E9E;&#x65AF;&#x5FB7;&#x6559; <pr>Su&#x1D2;lu&#xF3;y&#xE0;s&#x12B;d&#xE9;ji&#xE0;o</pr></trd>,
    <trd>&#x7946;&#x6559; <pr>Xi&#x101;nji&#xE0;o</pr></trd>,
    <trd>&#x7946;&#x9053; <pr>Xi&#x101;nd&#xE0;o</pr></trd>,
    <trd>&#x80E1;&#x5929; <pr>H&#xFA;ti&#x101;n</pr></trd>,
    <trd>&#x8884;&#x6559; <pr>&#x1CD;oji&#xE0;o</pr></trd>,
    <trd>&#x8956;&#x6559; <pr>&#x1CD;oji&#xE0;o</pr></trd>,
    <trd>&#x7410;&#x7F57;&#x4E9A;&#x65AF;&#x5FB7;&#x6559; <pr>su&#x1D2;lu&#xF3;y&#xE0;s&#x12B;d&#xE9;ji&#xE0;o</pr></trd>,
    <trd>&#x7463;&#x7F85;&#x4E9E;&#x65AF;&#x5FB7;&#x6559; <pr>su&#x1D2;lu&#xF3;y&#xE0;s&#x12B;d&#xE9;ji&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>

