<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>deklinaci</rad>/o
</kap>
<drv mrk="deklin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Aro de la modifoj, kiujn en kelkaj lingvoj ricevas substantivo,
      pronomo a&ubreve; adjektivo la&ubreve; &gcirc;ia sekso (genro), nombro
kaj gramatika rolo;
      kazaro:
      <ekz>
        esp-o havas dukazan, kaj latino seskazan <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Deklination</trd>,
    <trd>Beugung</trd>
  </trdgrp>
  <trd lng="en">declension</trd>
  <trd lng="es">declinaci&oacute;n</trd>
  <trd lng="fr">d&eacute;clinaison</trd>
  <trdgrp lng="hu">
    <trd>n&eacute;vsz&oacute;ragoz&aacute;s</trd>,
    <trd>deklin&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">p&aacute;d <klr>(gramatick&yacute;)</klr></trd>
</drv>
<drv mrk="deklin.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Eldiri la kazaron de iu vorto.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="de">durchdeklinieren</trd>
  <trd lng="en">decline</trd>
  <trd lng="es">declinar</trd>
  <trd lng="fr">d&eacute;cliner</trd>
  <trd lng="hu">ragoz (n&eacute;vsz&oacute;t)</trd>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">sklo&ncaron;ova&tcaron;</trd>
</drv>
</art>
