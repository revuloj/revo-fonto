<kap>
  <rad>latrin</rad>/o
<fnt><bib>JED</bib></fnt></kap>

<drv mrk="latrin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plej simpla <ref tip="super" cel="neces.0ejo">necesejo</ref> kun
      necesse&gcirc;o a&ubreve; nur foso en la planko:
      <ekz>
        oni ne povas frotlavi <tld/>on, se oni tro lacas por meti la
        kuleron
        da rizo en la bu&scirc;on <fnt><bib>Mortul&Scirc;ip</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: (plej simpla) prefere diru <ke>necesejo</ke>.
  </rim>

  <trd lng="bg">&c_n;&c_u;&c_zh;&c_n;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>latr&#237;na</trd>,
    <trd>such&#253; z&#225;chod</trd>
  </trdgrp>
  <trd lng="de">Latrine</trd>
  <trd lng="en">latrine</trd>
  <trd lng="fr">latrine</trd>
  <trdgrp lng="hu">
    <trd>latrina</trd>,
    <trd>&aacute;rny&eacute;ksz&eacute;k</trd>
  </trdgrp>
  <trd lng="ja">&#31777;&#26131;&#20415;&#25152;  <pr>&#12363;&#12435;&#12356;&#12409;&#12435;&#12376;&#12423;</pr></trd>
  <trd lng="nl">latrine</trd>
  <trd lng="pl">latryna</trd>
  <trdgrp lng="pt">
    <trd>latrina</trd>,
    <trd>privada</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_u;&c_b;&c_o;&c_r;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_o;&c_t;&c_h;&c_o;&c_zh;&c_je;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_r;&c_je;&c_b;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>latr&#237;na</trd>,
    <trd>such&#253; z&#225;chod</trd>
  </trdgrp>
  <trd lng="sr">&c_l;&c_a;&c_t;&c_r;&c_i;&c_n;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#1091;&#1073;&#1080;&#1088;&#1072;&#1083;&#1100;&#1085;&#1103;</trd>,
    <trd>&#1074;&#1110;&#1076;&#1093;&#1086;&#1078;&#1077; &#1084;&#1110;&#1089;&#1094;&#1077;</trd>,
    <trd>&#1074;&#1110;&#1076;&#1093;&#1110;&#1076;&#1086;&#1082;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33541;&#22353; <pr>m&#225;ok&#275;ng</pr></trd>,
    <trd>&#33541;&#21397; <pr>m&#225;osi</pr></trd>,
    <trd>&#33541;&#24257; <pr>m&#225;osi</pr></trd>
  </trdgrp>
</drv>
