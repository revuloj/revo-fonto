<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gap</rad>/i</kap>
<drv mrk="gap.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Senpense, naive kaj iom stulte rigardadi, malfermegante okulojn kaj
      bu&scirc;on:
      <ekz>
        <tld/>i la pasantojn;
      </ekz>
      <ekz>
        <tld/>i al io, al iu;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> admiro.
        <trdgrp lng="fr">
          <trd>b&eacute;at<klr tip="ind"> (niais)</klr></trd>,
          <trd>niais</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gaffen</trd>
  <trd lng="hu">b&aacute;mul</trd>
  <trdgrp lng="fr">
    <trd>bayer</trd>,
    <trd>b&eacute;er<klr tip="ind"> (bayer)</klr></trd>
  </trdgrp>
  <trd lng="nl">gapen</trd>
  <trdgrp lng="pt">
   <trd>ficar <ind>boquiaberto</ind></trd>, 
   <trd>embasbacar-se</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_z;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_t;&c_o;&c_z;&c_je;&c_j;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gap.0vagi">
  <kap><tld/>vagi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sencele promeni interesi&gcirc;ante pri strataj bagatela&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>baguenauder</trd>,
    <trd>fl&acirc;ner</trd>,
    <trd>vadrouiller</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>flaneren</trd>,
    <trd>rondslenteren</trd>,
    <trd>slenteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_f;&c_l;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="gap.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu emas <tld/>i.
    </dif>
  </snc>
  <trd lng="de">Gaffer</trd>
  <trd lng="fr">badaud</trd>
  <trd lng="nl">gaper</trd>
  <trd lng="pt">um <ind>boquiaberta</ind></trd>
  <trd lng="ru">&c_z;&c_je;&c_v;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
