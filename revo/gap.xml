<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gap</rad>/i
</kap>
<drv mrk="gap.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Senpense, naive k iom stulte rigardadi, malfermegante okulojn k bu&scirc;on:
      <ekz><tld/>i la pasantojn;</ekz>
      <ekz><tld/>i al io, al iu;</ekz>
      <ekz><tld/>a admiro.</ekz>
    </dif>
  </snc>
  <trd lng="de">gaffen</trd>
  <trd lng="hu">b&aacute;mul</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_z;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_t;&c_o;&c_z;&c_je;&c_j;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="gap.0vagi">
  <kap><tld/>vagi</kap>
  <snc>
    <dif>
      Sencele promeni interesi&gcirc;ante pri strataj bagatela&jcirc;oj. 
    </dif>
  </snc>
</drv>
<drv mrk="gap.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu emas <tld/>i.  
    </dif>
  </snc>
  <trd lng="de">Gaffer</trd>
  <trd lng="ru">&c_z;&c_je;&c_v;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
