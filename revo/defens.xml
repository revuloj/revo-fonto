<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defens.xml,v 1.20 2023/02/13 21:24:50 revo Exp $">
<kap>
  <rad>defensiv</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="defens.0o">
  <kap><tld/>o</kap>
  <snc mrk="defens.0o.MIL">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Defendo de sia pozicio kontra&ubreve; atakanto:
      <ekz>
        en ilia <tld/>o estas bre&ccirc;o
        <fnt>
          <aut>Lubovj Lukina, E&ubreve;geno Lukin, trad. Jurij
          Finkel</aut>,
          <vrk>La misiistoj</vrk>,
          <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis2.html">parto
          2a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        multaj landaj societoj trovi&gcirc;is en <tld/>o, provante konvinki siajn registarojn
        pri nekontra&ubreve;nacieca celado de Esperanto
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>e ludantaj teamoj <klr>[&#8230;]</klr> ne ludis en tiu turniro gravan rolon
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2008/009595.php"
              >Hispanio venkis, gastigantoj seniluziigis</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="ofensi.0o">ofensivo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>defenz&iacute;va</trd>,
    <trd>obrana</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Defensive</trd>,
    <trd>Verteidigung</trd>
  </trdgrp>
  <trd lng="es">defensiva</trd>
  <trd lng="fr">d&eacute;fensive<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="he">&he;&tav;&gimel;&vav;&nun;&nun;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>defenz&iacute;va</trd>,
    <trd>v&eacute;dekez&odblac; hadm&udblac;velet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>difesa<klr> (azione o tattica)</klr></trd>,
    <trd>difensiva<klr> (azione o tattica)</klr></trd>
  </trdgrp>
  <trd lng="nl">defensief</trd>
  <trdgrp lng="pl">
    <trd>defensywa</trd>,
    <trd>obrona</trd>
  </trdgrp>
  <trd lng="pt">defensiva</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>obrana</trd>,
    <trd>defenz&iacute;va</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
</drv>
</art>
