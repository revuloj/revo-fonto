<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>defensiv</rad>/o
</kap>
<drv mrk="defens.0o">
  <kap><tld/>o</kap>
  <snc mrk="defens.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Defendo de milita pozicio kontra&ubreve; atakanta malamiko.:
      <ekz>
        en ilia <tld/>o estas bre&ccirc;o
        <fnt>
        <aut>Lubovj Lukina, E&ubreve;geno Lukin, trad. Jurij
        Finkel</aut>,
        <vrk>La misiistoj</vrk>,
        <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis2.html">parto
        2a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="ofensi.0o">ofensivo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Defensive</trd>,
    <trd>Verteidigung</trd>
  </trdgrp>
  <trd lng="es">defensiva</trd>
  <trd lng="fr">d&eacute;fensive<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>defenz&iacute;va</trd>,
    <trd>v&eacute;dekez&odblac; hadm&udblac;velet</trd>
  </trdgrp>
  <trd lng="nl">defensief</trd>
  <trdgrp lng="pl">
    <trd>defensywa</trd>,
    <trd>obrona</trd>
  </trdgrp>
  <trd lng="pt">defensiva</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>obrana</trd>,
    <trd>defenz&iacute;va</trd>
  </trdgrp>
</drv>
</art>
