<kap>
  <rad>pop</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="pop1.0o">
  <kap><tld/>o</kap>
  <snc mrk="pop1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Facila <ref tip="super" cel="stil.0o.BELA">artostilo</ref> farata por 
      amasa moda uzo, precipe en kantmuziko:
      <ekz>
        eksmembro de la franca <tld/>bando <ctl>Lolita</ctl>
        <fnt>
        <aut>F. Martorell</aut>,
        <vrk>Informletero n-ro 7</vrk>,
        <lok>soc.culture.esperanto, 2004-06-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="pop.0o">popo (pastro)</ref>
  <trd lng="ca">pop<klr> (m&uacute;s.)</klr></trd>
  <trdgrp lng="cs">
    <trd>mll. pop</trd>,
    <trd>pop-art</trd>
  </trdgrp>
  <trd lng="de">Pop<klr>(art)</klr></trd>
  <trd lng="fr">pop<klr tip="amb"> (musique)</klr></trd>
  <trd lng="nl">popmuziek</trd>
  <trd lng="pl">pop muzyka</trd>
  <trd lng="pt">pop <klr>(m&uacute;sica)</klr></trd>
  <trd lng="ro">pop muzic&abreve;</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;-&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="sk">pop-art</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&#x43F;</trd>,
    <trd>&c_s;&c_v;&#x44F;&c_shch;&c_je;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

