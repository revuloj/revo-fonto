<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grafit</rad>/o
</kap>
<drv mrk="grafit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Grizenigra, mola speco de karbono, uzata precipe por fabriki krajonojn.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_t;</trd>
  <trd lng="de">Graphit</trd>
  <trd lng="en">graphite</trd>
  <trd lng="es">grafito</trd>
  <trd lng="fr">graphite</trd>
  <trd lng="hu">grafit</trd>
  <trd lng="nl">grafiet</trd>
  <trdgrp lng="pt">
   <trd>grafite<klr tip="amb"> (tipo de carbono)</klr></trd>, 
   <trd>grafita<klr tip="amb"> (tipo de carbono)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_t;</trd>
</drv>
</art>
