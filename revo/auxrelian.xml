<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxrelian.xml,v 1.11 2022/12/14 10:30:37 revo Exp $">
<kap>
  <rad>A&ubreve;relian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="auxrelian.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxrelian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        la <klr>...</klr> festo de la <klr>...</klr>
        &leftquot;Nevenkebla Suno&rightquot;, fiksita de imperiestro
        <tld/>o
        <fnt>
          <aut>trad. B. C.</aut>,
          <vrk>Kial la 25-a de decembro?</vrk>,
          <lok>
          <url ref="http://www.esperokatolika.org/jarkolektoj/ek1982_11.htm">
          Espero katolika, 1982-11</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_w;&c_r;&c_e;&c_l;&c_ib;&c_j;</trd>,
    <trd>&c_A;&c_w;&c_r;&c_e;&c_l;&c_ib;&c_ja;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Aurelian</trd>
  <trd lng="en">Aurelian</trd>
  <trd lng="fr">Aur&eacute;lien</trd>
  <trdgrp lng="hu">
    <trd>Aurelianus</trd>,
    <trd>Aureli&aacute;n</trd>
  </trdgrp>
  <trd lng="it">Aureliano</trd>
  <trd lng="lat">Aurelianus</trd>
  <trd lng="nl">Aurelialus</trd>
  <trd lng="pl">Aurelian</trd>
  <trd lng="pt">Aureliano</trd>
  <trd lng="uk">&c_A;&c_v;&c_r;&c_je;&#x43B;&c_ib;&c_a;&#x43D;</trd>
</drv>
</art>

