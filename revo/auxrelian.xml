<vortaro>
<art mrk="$Id$">
<kap>
  <rad>A&ubreve;relian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="auxrelian.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxrelian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        la <klr>...</klr> festo de la <klr>...</klr> &leftquot;Nevenkebla Suno&rightquot;,
        fiksita de imperiestro <tld/>o
        <fnt>
          <aut>trad. B. C.</aut>,
          <vrk>Kial la 25-a de decembro?</vrk>,
          <lok><url ref="http://www.esperokatolika.org/jarkolektoj/ek1982_11.htm">Espero katolika, 1982-11</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Aurelian</trd>
  <trd lng="fr">Aur&eacute;lien</trd>
  <trdgrp lng="hu">
    <trd>Aurelianus</trd>,
    <trd>Aureli&aacute;n</trd>
  </trdgrp>
  <trd lng="pt">Aureliano</trd>
</drv>
</art>

