<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvivit.xml,v 1.13 2023/12/03 09:59:05 revo Exp $">
<kap>
<rad>kvivit</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvivit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kvivit.0i.ZOO">
    <dif>
     <ref tip="super" cel="blek.0i.beste">Bleki</ref> per disaj krietoj,
      kiel iuj birdoj:
      <ekz>
        kion birdo havas pro &ccirc;iuj siaj <tld/>oj, kiam venas la vintro
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>?
      </ekz>
      <ekz>
        de suna &gcirc;oj' /
        alte <tld/>as /
        fringoj
        <fnt>
          <aut>G. E. Ma&ubreve;ra</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr12/psalmo.html">Psalmo... 137a</url></vrk>,
          <lok>la nica literatura revuo, 1955-11 kaj 12, numero 1/2, p.
          76a-79a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxirp.0i">&ccirc;irpi</ref>,
      <ref cel="kvik.0i.ZOO">kviki</ref>,
      <ref cel="pep.0i.bleki">pepi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_ch;&c_a;&c_b;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">p&iacute;pat</trd>
  <trd lng="fr">p&eacute;pier</trd>
  <trdgrp lng="pl">
    <trd>&cacute;wierka&cacute;</trd>,
    <trd>kwili&cacute;</trd>,
    <trd>szczebiota&cacute;</trd>
    <trd>&sacute;piewa&cacute;<klr> (o ptakach)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pipilar</trd>,
    <trd>piar</trd>,
    <trd>pipiar</trd>
  </trdgrp>
  <trd lng="sk">p&iacute;pa&tcaron;</trd>
</drv>

<drv mrk="kvivit.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="kvivit.0.SON">
    <dif>
      Ripetata akuta
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>,
      kian faras iuj birdoj:
      <ekz>
        <tld/>, <tld/>! kantis la birdo kaj enflugis en la verdan arbaron
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hirundo denove preterflugas: &leftquot;<tld/>!
        <tld/>!&rightquot;
        <fnt><bib>Fab3</bib>, <lok>muta libro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_y;&c_k;</trd>
  <trd lng="de">kwiwitt</trd>
  <trd lng="fr">cui-cui</trd>
  <trd lng="pl">&cacute;wir-&cacute;wir</trd>
  <trd lng="pt">piu-piu</trd>
  <trd lng="zh">&#x557E;&#x557E; <pr>ji&#x16B;ji&#x16B;</pr></trd>
</drv>
</art>
