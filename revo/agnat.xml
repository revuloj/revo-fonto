<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agnat.xml,v 1.18 2021/02/28 14:48:40 revo Exp $">
<kap>
  <rad>agnat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="agnat.0o">
  <kap><tld/>o</kap>
  <snc mrk="agnat.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="super" cel="famili.0ano">Familiano</ref>
      el la vira linio:
      <ekz>
        la arnulfidoj estis la membroj de familio el la franka nobelaro de A&ubreve;strazio
        originanta <tld/>e de Arnulfo, episkopo de Metz de 613 &gcirc;is 626
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=511793">Arnulfidoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">agn&aacute;t</trd>
  <trd lng="de">Agnat</trd>
  <trd lng="en">agnate</trd>
  <trd lng="es">agnado</trd>
  <trd lng="fr">agnat</trd>
  <trd lng="he">&#x5D1;&#x5DF; &#x5DE;&#x5E9;&#x5E4;&#x5D7;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;rfi&aacute;gi <ind>lesz&aacute;rmazott</ind></trd>,
    <trd>agn&aacute;tus</trd>
  </trdgrp>
  <trd lng="it">agnato</trd>
  <trd lng="la">agnatus</trd>
  <trd lng="nl">agnaat</trd>
  <trd lng="pl">agnat</trd>
  <trdgrp lng="pt">
    <trd>agnato</trd>,
    <trd>agnado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_g;&c_n;&c_a;&c_t;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;
&c_p;&c_o; &c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;
&c_l;&c_i;&c_n;&c_i;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">agn&aacute;t <klr>(pr&iacute;buzn&yacute; po mu&zcaron;skom predkovi)</klr></trd>
</drv>

<drv mrk="agnat.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="agnat.0eco.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Parenceco en la vira devenlinio.
    </dif>
  </snc>
  <trd lng="cs">agnace</trd>
  <trd lng="de">Agnation</trd>
  <trdgrp lng="en">
    <trd>agnation</trd>,
    <trd>kinship</trd>
  </trdgrp>
  <trd lng="es">agnaci&oacute;n</trd>
  <trd lng="fr">agnation</trd>
  <trd lng="it">agnazione</trd>
  <trd lng="la">agnatio</trd>
  <trd lng="pl">agnacja</trd>
  <trd lng="sk">pokrvn&eacute; pr&iacute;buzenstvo v mu&zcaron;skej l&iacute;ni&iacute;</trd>
</drv>
</art>
