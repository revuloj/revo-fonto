<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: cibel.xml,v 1.2 2023/08/30 13:29:34 afido Exp $">
    <kap>
      <rad>Cibel</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="cibel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="di.0ino">Diino</ref>, origine frigia, sed anka&ubreve; glorata en
      antikva Grekujo kaj Romio kiel Granda Dipatrino:
      <ekz>
        je Jovo, Apolono, Vesto, <tld/>o, Izido kaj Oziriso, je Mitro, Baalo kaj &ccirc;iuj
        dioj de Oriento kaj Okcidento, mi petegas vin, sinjoro, lasu tiun &ccirc;i planon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial ili ne petas vin anka&ubreve; kastri vin, same kiel la pastroj de <tld/>o
        <fnt>
          <bib>StU</bib>
          <lok>La Nazaretanoj</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_ib;&c_b;&c_e;&c_l;&c_a;</trd>
    <trd lng="de">Kybele</trd>
    <trd lng="en">Cybele</trd>
    <trd lng="fr">Cyb&egrave;le</trd>
    <trd lng="grc">&Kappa;&ypsilon;&beta;&epsilon_ton;&lambda;&eta;</trd>
    <trd lng="lat">Magna Mater</trd>
  </snc>
</drv>

  </art>

