<art mrk="$Id: denomi.xml,v 1.26 2023/11/04 13:16:27 revo Exp $">
<kap>
<rad>denominator</rad>/o
</kap>
<drv mrk="denomi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      La <ref cel="divid.0anto">dividanto</ref>
      en <ref cel="frakci.kvocienta0o">kvocienta frakcio</ref>
      a&ubreve; en <ref cel="frakci.algebra0o">algebra frakcio</ref>:
      <ekz>
        en la frakcio <frm><sup>4</sup>/<sub>16</sub></frm> la nombro 
        <frm>16</frm> estas la <tld/>o kaj <frm>4</frm> estas la 
        <ref cel="numera1.0o">numeratoro</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k; <klr>(&c_d;&c_r;&c_o;&c_b;&c_u;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_a;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="cs">jmenovatel</trd>
  <trd lng="de">Nenner</trd>
  <trd lng="en">denominator</trd>
  <trd lng="es">denominador</trd>
  <trd lng="fr">d&eacute;nominateur</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5DB;&#x5B7;&#x5E0;&#x5B6;&#x5D4;</trd>
  <trd lng="hu">nevez&odblac;</trd>
  <trd lng="ja">&#x5206;&#x6BCD;  <pr>&#x3076;&#x3093;&#x307C;</pr></trd>
  <trd lng="nl">noemer</trd>
  <trd lng="pl">mianownik</trd>
  <trd lng="pt">denominador</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>delite&lcaron;</trd>,
    <trd>menovate&#x13E;</trd>
  </trdgrp>
  <trd lng="uk">&#x437;&#x43D;&#x430;&#x43C;&#x435;&#x43D;&#x43D;&#x438;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x5206;&#x6BCD; <pr>f&#x113;nm&#x1D4;</pr></trd>,
    <trd>&#x6BCD;&#x800C; <pr>m&#x1D4;&#xE9;r</pr></trd>
  </trdgrp>
</drv>
</art>
