<drv mrk="kri.ek0o">
  <kap>ek<tld/>o</kap>
  <snc mrk="kri.ek0o.ago">
    <dif>
      Ago ek<tld/>i:
      <ekz>
        je la noktomezo estis ek<tld/>o: jen la fian&ccirc;o!
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 25:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        gaja ek<tld/>o koncernis grupon da personoj, kiuj plenumis tre originalan taskon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_k;</trd>,
      <trd>&c_v;&c_o;&c_k;&c_l;&c_ib;&c_ch;</trd>
    </trdgrp>
    <trd lng="ca">exclamaci&oacute;</trd>
    <trd lng="de">Aufschrei</trd>
    <trd lng="pl">okrzyk</trd>
    <trdgrp lng="zh">
    <trd>&#21483;&#21898; <pr>ji&agrave;oh&#462;n</pr></trd>,
    <trd>&#21898;&#21483;&#22768; <pr>h&#462;nji&agrave;osh&emacron;ng</pr></trd>,
    <trd>&#21898;&#21483;&#32882; <pr>h&#462;nji&agrave;osh&emacron;ng</pr></trd>,
    <trd>&#23574;&#21483; <pr>ji&amacron;nji&agrave;o</pr></trd>,
    <trd>&#24778;&#21628; <pr>j&#299;ngh&#363;</pr></trd>,
    <trd>&#39514;&#21628; <pr>j&#299;ngh&#363;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="kri.ek0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Senfleksia vorteto esprimanta subitan ekmovon
      de la animo, emocion, senton; <ref tip="sin" cel="interj.0o.GRA">interjekcio</ref>:
      <ekz>
        la tuta letero de la komenco &gcirc;is la fine estis plenigita <klr>[&#8230;]</klr> de
        signoj de demando kaj ek<tld/>o <klr>(<ref cel="sign.kri0o">krisigno</ref>)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
       adia&ubreve;, bis, fi, ha, he, ho, hura, nu, ve  estas konsiderataj kiel oficialaj
       esperantaj ek<tld/>oj;
      </ekz>
      <ekz>
        <ctl>bis</ctl> estas la&ubreve;da ek<tld/>o,
        per kiu oni petas aktoron a&ubreve; artiston
        ripeti ion bonan
        <fnt><bib>PMEG</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_ch;&c_n;&c_ib;&c_k;</trd>
    <trd lng="ca">interjecci&oacute;</trd>
    <trd lng="cs">v&yacute;k&rcaron;ik</trd>
    <trdgrp lng="de">
       <trd>Empfindungswort</trd>,
       <trd>Ausrufewort</trd>,
       <trd>Interjektion</trd>
    </trdgrp>
    <trd lng="en">interjection</trd>
    <trd lng="es">interjecci&oacute;n</trd>
    <trd lng="fr">interjection</trd>
    <trd lng="hu">indulatsz&oacute;</trd>
    <trdgrp lng="id">
      <trd>interjeksi</trd>,
      <trd><ind>kata</ind> seru</trd>
    </trdgrp>
    <trd lng="nl">tussenwerpsel</trd>
    <trd lng="pl">wykrzyknik<klr tip="amb"> (cz&eogonek;&sacute;&cacute; mowy)</klr></trd>
    <trd lng="pt">interjei&ccedil;&atilde;o</trd>
    <trd lng="ru">&c_m;&c_je;&c_zh;&c_d;&c_o;&c_m;&c_je;&c_t;&c_i;&c_je;</trd>
    <trdgrp lng="sk">
      <trd>v&yacute;krik</trd>,
      <trd>zv&yacute;sknutie</trd>
    </trdgrp>
  </snc>
  <trd lng="it">esclamazione</trd>
  <trd lng="uk">&c_v;&c_i;&c_g;&c_u;&c_k;</trd>
</drv>

