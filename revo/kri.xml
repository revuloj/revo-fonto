    <dif>
      Akre, neagrable impresa:
      <ekz>
        kriaj koloroj, <tld/>is, <tld/>egis
        <fnt><aut>Julio Baghy</aut>,<vrk>Hura</vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schrill</trd>,
      <trd>grell</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_k;&c_l;&c_ib;&c_v;&c_y; <klr tip="amb">(&c_r;&c_e;&c_z;&c_k;&c_ib;, &c_n;&c_je;&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;)</klr></trd>
