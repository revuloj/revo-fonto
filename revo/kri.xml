<drv mrk="kri.0o">
  <kap><tld/>o</kap>
  <snc mrk="kri.0o.KOMUNE">
    <dif>
      Forta vo&ccirc;ado:
      <ekz>
        &scirc;i levis sian vo&ccirc;on per la&ubreve;ta <tld/>o, kaj
        diris...
        <fnt><bib>NT</bib>, <lok>&Luk; 1:42</lok></fnt>;
      </ekz>
      <ekz>
        li ek<tld/>is per tre granda kaj maldol&ccirc;a <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipanoj salutis lin per entuziasmaj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        komuna rida <tld/>o ruli&gcirc;is e&hcirc;e
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la anseroj revenis hejmen kaj <tld/>is sian ra&ubreve;kan <tld/>on
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>;
      </ekz>
      <ekz>
        mevoj kun longa <tld/>o sin levis super mi
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne atingos <tld/>o &gcirc;is la trono de Dio
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="kri1.0o">krio (indiano)</ref>
  <trd lng="be">&c_k;&c_r;&c_y;&c_k;</trd>
  <trd lng="ca">crit</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;ik</trd>,
    <trd>vol&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Schrei</trd>
  <trd lng="es">grito</trd>
  <trd lng="fr">cri</trd>
  <trdgrp lng="id">
    <trd><baz>teriak</baz>teriakan</trd>,
    <trd>sorak</trd>
  </trdgrp>
  <trd lng="it">grido</trd>
  <trdgrp lng="ja">
    <trd>&#21483;&#12403;  <pr>&#12373;&#12369;&#12403;</pr></trd>,
    <trd>&#21483;&#12403;&#22768;  <pr>&#12373;&#12369;&#12403;&#12372;&#12360;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schreeuw</trd>,
    <trd>kreet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>krzyk</trd>,
    <trd>wrzask</trd>
  </trdgrp>
  <trd lng="sk">krik</trd>
  <trd lng="te">&#3077;&#3120;&#3125;&#3105;&#3074;</trd>
  <trd lng="uk">&c_k;&c_r;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
