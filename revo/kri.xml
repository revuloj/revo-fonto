  <trd lng="en">battle cry</trd>
  <trd lng="es">grito de guerra</trd>
  <trd lng="fr">cri de guerre</trd>
  <trd lng="id"><baz>seru</baz>seruan <klr>(perang)</klr></trd>
  <trd lng="ja">&#12392;&#x304D;&#x306E;&#x58F0;  <pr>&#12392;&#x304D;&#x306E;&#12371;&#12360;</pr></trd>
  <trd lng="pl">okrzyk bojowy</trd>
  <trd lng="sk">bojov&yacute; pokrik</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43E;&c_j;&#x43E;&c_v;&c_i;&c_j; &#x43A;&#x43B;&c_i;&c_ch;</trd>,
