<drv mrk="kri.0ema">
  <kap><ofc>*</ofc><tld/>ema</kap>
  <snc mrk="kri.0ema.KOMUNE">
    <dif>
      Tia, kiu kutime <tld/>as:
      <ekz>
        lia edzino estas tre laborema kaj &scirc;parema, sed &scirc;i estas anka&ubreve;
        tre babilema kaj <tld/>ema
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>;
      </ekz>
      <ekz>
        li pie turnis la okulojn kaj la manojn al la &ccirc;ielo kaj per malla&ubreve;ta kaj
        <tld/>ema vo&ccirc;o pre&gcirc;is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ema memla&ubreve;dulo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la publiko <klr>[&#8230;]</klr> iras kiel anaro da &scirc;afoj post la <ind><tld/>emulo</ind>j
        <fnt>
          <bib>EE</bib>
        </fnt>.
        <trd lng="de">Schreihals</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt.0ema">afektema</ref>,
      <ref cel="agres.0ema">agresema</ref>,
      <ref cel="kverel.0ema">kverelema</ref>,
      <ref cel="lament.0i">lamenti</ref>,
      <ref cel="lauxt.0a">la&ubreve;ta</ref>,
      <ref cel="plend.0i">plendi</ref>
    </refgrp>
    <trd lng="be">&c_k;&c_r;&c_y;&c_k;&c_l;&c_ib;&c_v;&c_y; <klr tip="amb">(&c_ja;&c_k;&c_ib; &c_m;&c_n;&c_o;&c_g;&c_a; &c_k;&c_r;&c_y;&c_ch;&c_y;&c_c;&c_mol;)</klr></trd>
    <trdgrp lng="de">
      <trd>streits&uuml;chtig</trd>,
      <trd>schrill</trd>,
      <trd>oft <ind>laut</ind> werdend</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">criard</trd>
  <trdgrp lng="ja">
    <trd>&#x9A12;&#12293;&#12375;&#12356;  <pr>&#x305D;&#12358;&#x305E;&#12358;&#12375;&#12356;</pr></trd>,
    <trd>&#x53E3;&#12420;&#x304B;&#x307E;&#12375;&#12356;  <pr>&#x304F;&#12385;&#12420;&#x304B;&#x307E;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>krzykliwy</trd>,
    <trd>wrzaskliwy</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&c_r;&c_i;&#x43A;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="kri.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kri.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;i <tld/>ante:
      <ekz>
        la homamasoj &ccirc;irka&ubreve;<tld/>as nin kaj minace
        levas siajn pugnojn
        <fnt>
        <aut>J. Forge</aut>,
        <vrk>Saltego trans jarmiloj</vrk>,
        <lok>1924</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol; &c_z; &c_k;&c_r;&c_y;&c_k;&c_a;&c_m;&c_ib;</trd>
  <trd lng="hu">k&ouml;rbeki&aacute;ltoz</trd>
  <trd lng="nl">rondbrullen</trd>
  <trdgrp lng="pl">
    <trd>okrzykn&aogonek;&cacute;</trd>,
    <trd>okrzycze&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kri.ek0i">
  <kap><ofc>*</ofc>ek<tld/>i
  <fnt><bib>F</bib>, <lok>Universala Vortaro, ek'</lok></fnt></kap>
  <snc mrk="kri.ek0i.KOMUNE">
    <dif>
      Eligi iom subitan kaj emocian <tld/>on, pro
      surprizo, teruro, &gcirc;ojo, veo, protesto...:
      <ekz>
        kiam li a&ubreve;dis, ke mi ek<tld/>is per la&ubreve;ta vo&ccirc;o, li <klr>[&#8230;]</klr> forkuris
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;mi &gcirc;ojas, ke vi estas kontenta,&rightquot; ek<tld/>is lia edzino
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
