<drv mrk="kri.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="kri.0i.KOMUNE">
    <dif>
      Tre la&ubreve;te <ref tip="super" cel="vocx.0i.diri">vo&ccirc;i</ref>:
    </dif>
    <subsnc mrk="kri.0i.vocxi">
      <gra><vspec>ntr</vspec></gra>
      <dif>
        <ekz>
          iu admonis nun lamente, ke ni ne tiel la&ubreve;te <tld/>u
          <fnt>
            <aut>Raymond Schwartz</aut>
            <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
            <lok>Anta&ubreve;signoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>anta korniko
          <fnt>
            <bib>Fab1</bib>
            <lok>Galo&scirc;oj de feli&ccirc;o</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <tld/>antaj maljusta&jcirc;oj
          (evidentega kaj protestinda)
          <fnt>
            <bib>Homaranismo</bib>
            <vrk>Dogmoj de Hilelismo</vrk>
          </fnt>;
        </ekz>
        <ekz>
          la duan fojon koko <tld/>is
          <fnt>
            <bib>NT</bib>
            <lok>Marko 14:72</lok>
          </fnt>;
        </ekz>
        <ekz>
          koko <tld/>as fiere, sed ne dan&gcirc;ere
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          <ind>inter lupoj <tld/>u <klr>(bleku)</klr> lupe</ind>
          <trd lng="hu">aki farkasok k&ouml;z&eacute; ker&uuml;l, vel&uuml;k kell &uuml;v&ouml;ltenie</trd>
          <trd lng="pl">kiedy wejdziesz mi&eogonek;dzy wrony, musisz kraka&cacute; jak i one</trd>
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <trd lng="de">schreien</trd>
      <trd lng="pl">wo&lstroke;a&cacute;</trd>
    </subsnc>
    <subsnc mrk="kri.0i.paroli">
      <gra><vspec>x</vspec></gra>
      <dif>
        Diri ion per la&ubreve;ta akra vo&ccirc;o:
        <ekz>
          blovu per korno <klr>[&#8230;]</klr> kaj <tld/>u alarmon
          <fnt>
            <bib>MT</bib>
            <lok>Joel 2:1</lok>
          </fnt>;
        </ekz>
        <ekz>
          se vi volos rajdi rapide, klaku per la lango kaj <tld/>u &#8218;hop hop&#8216;
          <fnt>
            <bib>ElektFab</bib>
            <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
          </fnt>!
        </ekz>
        <ekz>
          la popolo sur la stratoj <tld/>as &#8218;hura&#8216;
          <fnt>
            <bib>Fab1</bib>
            <lok>Vojkamarado</lok>
          </fnt>;
        </ekz>
        <ekz>
          Jesuo <tld/>is per la&ubreve;ta vo&ccirc;o: <klr>[&#8230;]</klr> mia Dio, kial Vi forlasis min
          <fnt>
            <bib>NT</bib>
            <lok>Marko 15:34</lok>
          </fnt>?
        </ekz>
        <ekz>
          oni <tld/>is anta&ubreve; li: genui&gcirc;u
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 41:43</lok>
          </fnt>!
        </ekz>
        <ekz>
          famo la&ubreve;te <tld/>as, &ccirc;iuj ekscias
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">rufen</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="lament.0i">lamenti</ref>,
      <ref cel="vok.0i">voki</ref>
    </refgrp>
  </snc>
