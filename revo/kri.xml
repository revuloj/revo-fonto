  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_ja;&c_v;&c_y; <ind>&c_p;&c_o;&c_k;&c_l;&c_ib;&c_ch;</ind></trd>,
    <trd>&c_b;&c_a;&c_ja;&c_v;&c_y; <ind>&c_k;&c_l;&c_ib;&c_ch;</ind></trd>
  </trdgrp>
  <trd lng="cs">v&aacute;le&ccaron;n&yacute; pok&rcaron;ik</trd>
