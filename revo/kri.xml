<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kri</rad>/i
</kap>
<drv mrk="kri.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="kri.0i.KOMUNE">
    <dif>
      Tre la&ubreve;te vo&ccirc;i:
    </dif>
    <subsnc mrk="kri.0i.vocxi">
      <gra><vspec>ntr</vspec></gra>
      <dif>
        Eligi akrajn vo&ccirc;sonojn, sen paroloj:
        <ekz>
          <tld/>i de doloro, el la tuta gor&gcirc;o<fnt>Z</fnt>;
        </ekz>
        <ekz>
          anonc<tld/>i, pri<tld/>i gazeton;
        </ekz>
        <ekz>
          <tld/>protesti kontra&ubreve; maljusta pretendo;
        </ekz>
        <ekz>
          <ind>inter lupoj <tld/>u <klr>(bleku)</klr> lupe</ind>
          <trd lng="hu">aki farkasok k&ouml;z&eacute; ker&uuml;l, vel&uuml;k kell &uuml;v&ouml;ltenie</trd>
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="vok.0i">voki</ref>
      </refgrp>
      <trd lng="de">schreien</trd>
    </subsnc>
    <subsnc mrk="kri.0i.paroli">
      <gra><vspec>x</vspec></gra>
      <dif>
        Diri ion per la&ubreve;ta akra vo&ccirc;o:
        <ekz>
          <tld/>i alarmon<fnt>Z</fnt>,
          helpon<fnt>Z</fnt>,
          fajron;
        </ekz>
        <ekz>
          <tld/>i pri &scirc;telisto, pri la ven&gcirc;o, pro malsatego;
        </ekz>
        <ekz>
          sa&gcirc;a homo ka&scirc;as scion, sed la koro de malsa&gcirc;uloj
          el<tld/>as malsa&gcirc;egon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <tld/>anta (evidentega kaj protestinda) erarpa&scirc;o<fnt>Z</fnt>,
          maljusta&jcirc;o.
        </ekz>
      </dif>
      <trd lng="de">rufen</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">k&rcaron;i&ccaron;et</trd>
  <trd lng="en">shout</trd>
  <trdgrp lng="fr">
    <trd>crier</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;lt</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kri&ccaron;a&tcaron;</trd>
</drv>

<drv mrk="kri.0o">
  <kap><tld/>o</kap>
  <snc mrk="kri.0o.KOMUNE">
    <dif>
      Forta vo&ccirc;ado:
      <ekz>
        &scirc;i levis sian vo&ccirc;on per la&ubreve;ta <tld/>o, kaj diris...
        <fnt><bib>NT</bib>, <lok>&Luk; 1:42</lok></fnt>;
      </ekz>
      <ekz>
        la anseroj revenis hejmen kaj kriis sian ra&ubreve;kan <tld/>on
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cri</trd>
</drv>

<drv mrk="kri.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="kri.0egi.KOMUNE">
    <dif>
      Forte la&ubreve;tege <tld/>i:
      <ekz>
        &scirc;i vokis pli kaj pli la&ubreve;te, <tld/>is, <tld/>egis
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">hurler</trd>
</drv>

<drv mrk="kri.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kri.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;i <tld/>ante:
      <ekz>
        dum la homamasoj &ccirc;irka&ubreve;<tld/>as nin kaj minace
        levas siajn pugnojn, Karolo fajfigas la signalsirenon, kaj
        nia a&ubreve;tomobilo salte ekkuras
        <fnt>
        <aut>J. Forge</aut>,
        <vrk>Saltego trans jarmiloj</vrk>,
        <bib>Voja&gcirc;o</bib>,
        <lok>leciono 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">k&ouml;rbeki&aacute;ltoz</trd>
</drv>

<drv mrk="kri.ek0i">
  <kap>ek<tld/>i</kap>
  <snc mrk="kri.ek0i.KOMUNE">
    <dif>
      Eligi iom subitan kaj emocian <tld/>on, pro
      surprizo, teruro, &gcirc;ojo, veo, protesto...:
      <ekz>
        <ctl>Ho, kia malfeli&ccirc;o!</ctl> ek<tld/>is la mastro kaj
        interfrapis la manojn super la kapo
        <fnt><bib>Fab1</bib>,
        <lok>Malgranda Niko kaj granda Niko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_k;&c_l;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ausrufen</trd>,
    <trd>hinausschreien</trd>,
    <trd>losschreien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>exclaim</trd>,
    <trd>cry out</trd>,
    <trd>shout out</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;crier</ind></trd>,
    <trd>s'<ind>exclamer</ind></trd>
  </trdgrp>
  <trd lng="hu">felki&aacute;lt</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_k;&c_l;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">skr&iacute;knu&tcaron;</trd>
</drv>

<drv mrk="kri.post0i">
  <kap>post<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="kri.post0i.KOMUNE">
    <dif>
      <tld lit="K"/>i de malanta&ubreve;e, al iu malproksimi&gcirc;anta:
      <ekz>
        post<tld/>i al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        - Mi&hcirc;aelo Aleksandrovi&ccirc;! - li post<tld/>is
        al Berlioz
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>nachrufen</trd>,
    <trd>nachschreien</trd>,
    <trd>hinterher schreien</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;ler<klr tip="ind"> (de derri&egrave;re)</klr></trd>
  <trdgrp lng="hu">
    <trd>ut&aacute;nasz&oacute;l</trd>,
    <trd>ut&aacute;naki&aacute;lt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_i;&c_ch;&c_a;&c_t;&c_mol; &c_v;&c_s;&c_l;&c_je;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_ch;&c_a;&c_t;&c_mol; &c_v;&c_d;&c_o;&c_g;&c_o;&c_n;&c_k;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="kri.0signo">
  <kap><tld/>signo</kap>
  <snc mrk="kri.0signo.TIP">
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="malprt" cel="interp3.0o">Interpunkcia</ref> <ref
      cel="sign.0o.KOMP">signo</ref> (!) uzata fine de ordonaj
      kaj ek<tld/>aj frazoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Ausrufezeichen</trd>
  <trd lng="en">exclamation mark</trd>
  <trdgrp lng="fr">
    <trd>point d'exclamation</trd>
  </trdgrp>
  <trd lng="hu">felki&aacute;lt&oacute;jel</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_k;&c_l;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_z;&c_n;&c_a;&c_k;</trd>
  <trd lng="sk">v&yacute;kri&ccaron;n&iacute;k</trd>
</drv>
</art>
