<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>import</rad>/i
</kap>
<drv mrk="import.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Venigi, enporti en landon komerca&jcirc;ojn a&ubreve; aliajn a&jcirc;ojn
      el eksterlando:
      <ekz>
        <tld/>i varojn, grenon, novan industrion, modon, morojn, ideojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>importieren</trd>,
    <trd>einf&uuml;hren</trd>
  </trdgrp>
  <trd lng="en">import</trd>
  <trd lng="fr">importer</trd>
  <trdgrp lng="nl">
    <trd>importeren</trd>,
    <trd>invoeren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_m;&c_p;&c_o;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">importera</trd>
</drv>

<drv mrk="import.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>ado:
      <ekz>
        <tld/>a komerco, statistiko.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_o;&c_r;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="import.0ado">
  <kap><tld/>(ad)o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Import</trd>,
    <trd>Einfuhr</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>importation</trd>,
    <trd>import</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>import</trd>,
    <trd>invoer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_m;&c_p;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_v;&c_v;&c_o;&c_z;</trd>
  </trdgrp>
  <trd lng="sv">import</trd>
</drv>

<drv mrk="import.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <tld lit="I"/>itaj komerca&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Import</trd>,
    <trd>Importgut</trd>,
    <trd>importierte Ware</trd>,
    <trd>Einfuhr</trd>,
    <trd>Einfuhrgut</trd>,
    <trd>eingef&uuml;hrte <ind>Ware</ind></trd>
  </trdgrp>
  <trd lng="fr">importation<klr tip="ind"> (chose import&eacute;e)</klr></trd>
  <trdgrp lng="nl">
    <trd>importgoed</trd>,
    <trd>ingevoerde <ind>goederen</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_m;&c_p;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_i;&c_m;&c_p;&c_o;&c_r;&c_t;&c_n;&c_y;&c_j;
    &c_t;&c_o;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="import.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Komercisto, kiu importadas.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>importateur</trd>
  </trdgrp>
</drv>
</art>
