<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fret</rad>/o
</kap>
<drv mrk="fret.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Listelforma, ronda a&ubreve; plata ornamo, ortangule kurbigita:
      <ekz>ekzistas <tld/>oj konkavaj, konveksaj a&ubreve; nur pentrataj.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_p;&c_r;&c_ja;&c_m;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>
<drv mrk="fret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on. 
    </dif>
    <refgrp tip="sin">
      <ref cel="gilosx">gilo&scirc;o</ref>,
      <ref cel="meandr.0o">meandro</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="frajt">frajto</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol; &c_p;&c_r;&c_ja;&c_m;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m; &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;&c_o;&c_m;</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
