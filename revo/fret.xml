<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fret.xml,v 1.20 2026/02/08 08:49:38 revo Exp $">
<kap>
  <rad>fret</rad>/o
</kap>
<drv mrk="fret.0o">
  <kap><tld/>o</kap>
  <snc mrk="fret.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      Parto de
      <ref tip="malprt" cel="instru2.kord0o.MUZ">kordinstrumento</ref>,
      iu el la elstara&jcirc;oj, inter kiuj oni alpremu la kordojn
      por sonigi difinitajn tonojn:
      <ekz>
        li ludis sen<tld/>an basgitaron
        <fnt>
          <aut>M. Povorin</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/per-esperanto-muziko/message/77">
          Re: Muzik-vortareto</url></vrk>,
          <lok>per-esperanto-muziko, 2002-08-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        aparta metodo por ludi gitaron, glitigante sur la <tld/>oj metalan a&ubreve; vitran
        objekton
        <fnt>
          <bib>Kon11</bib>
          <lok>Ko-diskoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bund<klr tip="ind"> (eines Saiteninstruments)</klr></trd>
    <trd lng="en">fret<klr tip="ind"> (of stringed
    instrument)</klr></trd>
    <trd lng="nl">fret<klr tip="ind"> (van snaarinstrument)</klr></trd>
    <trd lng="pl">pr&oacute;g w gitarze</trd>
  </snc>
  <snc mrk="fret.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Listelforma, ronda a&ubreve; plata ornamo, ortangule kurbigita:
      <ekz>
        ekzistas <tld/>oj konkavaj, konveksaj a&ubreve; nur pentrataj.
      </ekz>
    </dif>
    <ref tip="sub" cel="meandr.0o.ARKI">meandro<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="ajxur.0a">a&jcirc;ura&jcirc;o</ref>,
      <ref cel="gilosx">gilo&scirc;o</ref>,
      <ref cel="plekt.0ajxo">plekta&jcirc;o</ref>
    </refgrp>
    <bld
      lok="&WCU;/f/fe/Frette.svg"
      prm="CC BY-SA 3.0"
      lrg="222"
      >blazono kun <tld/>o
        <fnt>
          <aut>Fvasconcellos, Micheletb</aut>
          <lok><url ref="&WCW;/File:Frette.svg"
              >https://commons.wikimedia.org/wiki/File:Frette.svg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Knotenkreuz</trd>,
      <trd>M&auml;ander</trd>
    </trdgrp>
    <trd lng="en">fretwork</trd>
    <trdgrp lng="nl">
      <trd>meanderfries</trd>,
      <trd>zaagtandfries</trd>
    </trdgrp>
    <trd lng="pl">ozdoba na gzymsie</trd>
    <trd lng="pt">moldura</trd>
    <trd lng="ru">&c_p;&c_r;&c_ja;&c_m;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </snc>
  <snc mrk="fret.0o.frajto">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PVEF</bib></fnt>
    <fnt><bib>KVS</bib></fnt>
    <ref tip="dif" cel="frajt.0o">frajto</ref>
  </snc>
    <trdgrp lng="cs">
    <trd>pra&zcaron;ec</trd>,
    <trd>v&yacute;stupek na hmatn&iacute;ku</trd>
  </trdgrp>
  <trd lng="fr">frette</trd>
  <trd lng="ja">&#x96F7;&#x6587;  <pr>&#x304B;&#x307F;&#x306A;&#x308A;&#x3076;&#x3093;</pr></trd>
  <trdgrp lng="sk">
    <trd>pra&zcaron;ec</trd>,
    <trd>v&yacute;stupok na hmatn&iacute;ku</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_i;&c_z;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_u;&c_ch;</trd>,
    <trd>&c_b;&c_u;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_h;&c_o;&c_m;&c_u;&c_t;</trd>
  </trdgrp>
</drv>

<!--
  manke de cita&jcirc;oj oni tute ne scias, &ccirc;u temas pri provizo per ~o, ludi per ~oj ...?
  en la franca "fretter" i.a. signifas KON: provizi per arama&jcirc;o
<drv mrk="fret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fret.0i.ARKI">
    <dif>
      Fari <tld/>on.
    </dif>
    <trd lng="fr">fretter</trd>
    <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol; &c_p;&c_r;&c_ja;&c_m;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;
    &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;&c_o;&c_m;</trd>
  </snc>
  <trd lng="ja">&#x96F7;&#x6587;&#x3067;&#x98FE;&#x308B;
  <pr>&#x304B;&#x307F;&#x306A;&#x308A;&#x3076;&#x3093;&#x3067;&#x304B;&#x3056;&#x308B;</pr></trd>
  <trd lng="pl">ozdobi&cacute;  <ind>feretem</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_ib;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_o;&c_b;&c_r;&c_u;&c_ch;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_a;&c_t;&c_i; &c_o;&c_b;&c_r;&c_u;&c_ch;&c_ib;</trd>
  </trdgrp>
</drv>
-->
</art>
