<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tunizi.xml,v 1.16 2023/09/04 19:01:55 revo Exp $">
<kap><ofc>8</ofc><rad>Tunizi</rad>/o <fnt><bib>PIV1</bib></fnt></kap>

<drv mrk="tunizi.T0o">
  <kap>
    <ofc>8</ofc><tld lit="T"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>  
  <mlg kod="ISO-3166">TN</mlg>
  <snc mrk="tunizi.T0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> en la
      norda Afriko kun &ccirc;efurbo Tunizo; Teritorio de Respubliko <tld/>o,
      limanta kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="libi.L0o.GEOG">Libio</ref> kaj
      <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteraneo</ref>:
      <ekz>
        la manifestacioj estis inspiritaj de similaj eventoj en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010284.php">Kulpantoj
          kulpigas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        de <tld/>o &gcirc;is Kongo 175 000 francoj instruas, kuracas, laboras,
        komercas, estras uzinojn, ri&ccirc;i&gcirc;as kaj havas grandan influon
        <fnt>
          <bib>Monato</bib>
          <aut>Yves Nicolas</aut>
          <vrk><url ref="&Monato;/2013/010985.php">Malio kaj la granda
          mutulo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_T;&c_u;&c_n;&c_ib;&c_s;</trd>
    <trd lng="bg">&c_T;&c_u;&c_n;&c_i;&c_s;</trd>
    <trdgrp lng="ca">
      <trd>Tun&iacute;sia</trd>,
      <trd>Rep&uacute;blica Tunisiana</trd>
    </trdgrp>
    <trd lng="de">Tunesien</trd>
    <trd lng="en">Tunisia</trd>
    <trd lng="fr">Tunisie</trd>
    <trd lng="id">Tunisia</trd>
    <trd lng="nl">Tunesi&euml;</trd>
    <trdgrp lng="pl">
      <trd>Tunezja</trd>,
      <trd>Republika Tunezji</trd>
    </trdgrp>
    <trd lng="pt">Tun&iacute;sia</trd>
    <trd lng="vo">T&uuml;nis&auml;n</trd>
  </snc>
  <trd lng="ja">&#x30C1;&#x30E5;&#x30CB;&#x30B8;&#x30A2;</trd>
  <trd lng="uk">&#x422;&#x443;&#x43D;&#x456;&#x441; (&#x434;&#x435;&#x440;&#x436;&#x430;&#x432;&#x430;)</trd>
</drv>

<drv mrk="tunizi.0a">
  <kap><tld lit="t"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld/>o:
      <ekz>
        Kartago estas <tld/>a urbo, kiu en antikva epoko estis komplete
        detruita de romianoj rezulte de longaj militoj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37694">Kartago</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_n;&c_ib;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">tunesisch</trd>
  <trd lng="en">Tunisian</trd>
  <trdgrp lng="fr"> 
    <trd>tunisien</trd>
    <trd>tunisienne<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Tunesisch</trd>
  <trd lng="vo">T&uuml;nis&auml;nik</trd>
</drv>

<drv mrk="tunizi.0ano">
  <kap><tld lit="t"/>ano</kap>
  <snc>
    <dif>
      Civitano de <tld/>o:
      <ekz>
        la lo&gcirc;antoj de <tld/>o nomi&gcirc;as <tld lit="t"/>anoj, sed la
        lo&gcirc;antoj de la &ccirc;efurbo Tunizo nomi&gcirc;as tunizanoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4194">Tunizio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_n;&c_ib;&c_s;&c_je;&c_c;</trd>
  <trd lng="de">Tunesier</trd>
  <trd lng="en">Tunisian</trd>
  <trdgrp lng="fr"> 
    <trd>Tunisien</trd>
    <trd>Tunisienne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Tunesi&euml;r</trd>
  <trd lng="vo">T&uuml;nis&auml;nan</trd>
</drv>

</art>

