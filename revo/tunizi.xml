<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tunizi</rad>/o
</kap>

<drv mrk="tunizi.0o">
  <kap>
    <tld lit="T"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="tunizi.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la norda Afriko kun &ccirc;efurbo Tunizo.
      Teritorio de <em>Respubliko Tunizio</em>.
      ISO-3166 kodo: TN.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="libi.0o.GEOG">Libio</ref>
      kaj <ref tip="vid" cel="mediterane.0o.GEOG">Mediteraneo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Tunezja</trd>,
      <trd>Republika Tunezji</trd>
    </trdgrp>
  </snc>
</drv>

</art>

