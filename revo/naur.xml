<art mrk="$Id: naur.xml,v 1.14 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>Naur</rad>/o 
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="naur.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="ISO-3166">NR</mlg>
  <snc mrk="naur.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la <ref tip="malprt" cel="mikronez.0o.GEOG">Mikronezio</ref>
      kun &ccirc;efa urbo Jareno; respubliko <tld/>o:
      <ekz>
        ekde jaroj la britoj tie produktas fosfaton, kiu abundis sur <tld/>o kaj
        nun preska&ubreve; el&ccirc;erpi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009581.php">Viziti mikro&scirc;tatojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la jaro -1200 unuaj homoj aperis sur la insulo <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=5000">12-a jarcento a.K.</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_a;&c_v;&c_u;&c_r;&c_u;</trd>
    <trdgrp lng="bg">
      <trd>&c_N;&c_a;&c_u;&c_r;&c_u;</trd>,
      <trd>&c_R;&c_je;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a; <ind>&c_N;&c_a;&c_u;&c_r;&c_u;</ind></trd>
    </trdgrp>
    <trd lng="de">Nauru</trd>
    <trdgrp lng="en">
      <trd>Nauru</trd>,
      <trd>Republic of <ind>Nauru</ind></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Nauru</trd>,
      <trd>Rep&uacute;blica de Nauru</trd>
    </trdgrp>
    <trd lng="fr">Nauru</trd>
    <trd lng="id">Nauru</trd>
    <trd lng="nl">Nauru</trd>
    <trdgrp lng="pl">
      <trd>Nauru</trd>,
      <trd>Republika <ind>Nauru</ind></trd>
    </trdgrp>
    <trd lng="pt">Nauru</trd>
    <trdgrp lng="zh">
    <trd>&#x8AFE;&#x9B6F; <pr>nu&#xF2;l&#x1D4;</pr></trd>,
    <trd>&#x8BFA;&#x9C81; <pr>nu&#xF2;l&#x1D4;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30CA;&#x30A6;&#x30EB;</trd>
  <trd lng="uk">&#x41D;&c_a;&c_u;&c_r;&c_u;</trd>
</drv>

</art>

