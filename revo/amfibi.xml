<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amfibi.xml,v 1.39 2025/04/04 14:00:21 revo Exp $">
<kap>
  <ofc>*</ofc><rad>amfibi</rad>/oj
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="amfibi.0o">
  <kap><tld/>o</kap>
  <snc mrk="amfibi.0o.ZOO">
    <dif>
      Iu el <tld/>oj:
      <ekz>
        <ref tip="sub" cel="salama.0o">salamandro</ref> estas longkorpa, vostohava, senhara <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33078">Salamandro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="ran.0o">rano</ref>,
       <ref cel="buf.0o">bufo</ref>
    </refgrp>
    <trd lng="de">Lurch</trd>
    <trdgrp lng="zh">
    <trd>&#20004;&#26646;&#31867; <pr>li&#462;ngq&#299;l&egrave;i</pr></trd>,
    <trd>&#20841;&#26866;&#39006; <pr>li&#462;ngq&#299;l&egrave;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_f;&c_ib;&c_b;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja;
    &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">amfibi</trd>
  <trd lng="cs">oboj&zcaron;iveln&iacute;k</trd>
  <trd lng="fr">amphibien</trd>
  <trd lng="he">&dalet;&vav;&#1468;&het;&#1463;&yod;</trd>
  <trd lng="pl">p&lstroke;az</trd>
  <trdgrp lng="sk">
    <trd>amf&iacute;bia</trd>,
    <trd>oboj&zcaron;iveln&iacute;k</trd>
  </trdgrp>
</drv>

<drv mrk="amfibi.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="amfibi.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref>
      de <ref tip="malprt" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Amphibia</trd>), nudha&ubreve;taj, kies larvoj
      vivas en sensala akvo spirante per brankoj kaj kies
      plenkreskuloj vivas surtere spirante per pulmoj:
      <ekz>
        la paleozoikon, &gcirc;is anta&ubreve; &ccirc;. 170 jarmilionoj, karakterizas senvertebruloj, fi&scirc;oj
        kaj <tld/>oj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Kanado sciencistoj trovis la transiran &ccirc;eneron inter fi&scirc;oj
        kaj la unuaj surteraj esta&jcirc;oj &ndash; la tiel nomatan <nom>Tiktaalik roseae</nom>,
        kvankam <nom>Tiktaalik</nom> precipe lo&gcirc;is en akvo kaj havis na&gcirc;ilojn, tamen
        &ccirc;i tiuj lastaj pli multe memorigis pri membroj de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2009/009787.php">Supervivo
          de plej ta&ubreve;ga teorio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="vost.sen0uloj">senvostuloj</ref>,
      <ref cel="urodel.0oj">urodeloj</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#20004;&#26646;&#31867; <pr>li&#462;ngq&#299;l&egrave;i</pr></trd>,
    <trd>&#20841;&#26866;&#39006; <pr>li&#462;ngq&#299;l&egrave;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_f;&c_ib;&c_b;&c_ib;&c_ib;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_y;&c_ja; <klr
    tip="amb">(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="br">amfibienned</trd>
  <trd lng="ca">amfibis</trd>
  <trd lng="de">Lurche</trd>
  <trd lng="en">amphibians</trd>
  <trd lng="fr">amphibiens<klr> (classe des)</klr></trd>
  <trd lng="he">&dalet;&vav;-&het;&yod;&yod;&fmem;</trd>
  <trd lng="hu">k&eacute;t&eacute;lt&udblac;ek</trd>
  <trd lng="ja">&#20001;&#29983;&#39006;  <pr>&#12426;&#12423;&#12358;&#12379;&#12356;&#12427;&#12356;</pr></trd>
  <trd lng="nl">amfibie&euml;n</trd>
  <trd lng="pl">p&lstroke;azy</trd>
  <trd lng="pt">anf&iacute;bio</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je;<klr tip="ind">
    (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_a;&c_m;&c_f;&c_i;&c_b;&c_i;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="sk">oboj&zcaron;iveln&iacute;ky</trd>
  <trd lng="tok">akesi</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_m;&c_f;&c_ib;&c_b;&c_ib;&c_ji;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="amfibi.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Vivanta kaj en akvo kaj surtere:
      <ekz>
        <klr>[ili]</klr> estas nek marlo&gcirc;antoj, nek <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="https://www.monato.be/2015/011355.php">Komunika&jcirc;o de Ikaro</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="amfibi.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      (pri veturilo) Povanta movi&gcirc;i kaj enakve kaj surtere.
    </dif>
    <trdgrp lng="zh">
    <trd>&#20004;&#29992; <pr>li&#462;ngy&ograve;ng</pr></trd>,
    <trd>&#20841;&#29992; <pr>li&#462;ngy&ograve;ng</pr></trd>,
    <trd>&#27700;&#38470;&#20004;&#26646; <pr>shu&#464;li&ugrave;li&#462;ngq&#299;</pr></trd>,
    <trd>&#27700;&#38520;&#20841;&#26866; <pr>shu&#464;li&ugrave;li&#462;ngq&#299;</pr></trd>,
    <trd>&#27700;&#38470;&#20004;&#29992; <pr>shu&#464;li&ugrave;li&#462;ngy&ograve;ng</pr></trd>,
    <trd>&#27700;&#38520;&#20841;&#29992; <pr>shu&#464;li&ugrave;li&#462;ngy&ograve;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_f;&c_ib;&c_b;&c_ib;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">amfibi</trd>
  <trd lng="de">amphibisch</trd>
  <trd lng="en">amphibious</trd>
  <trdgrp lng="ja">
    <trd>&#20001;&#29983;&#39006;&#12398;  <pr>&#12426;&#12423;&#12358;&#12379;&#12356;&#12427;&#12356;&#12398;</pr></trd>,
    <trd>&#27700;&#38520;&#20001;&#29992;&#12398;  <pr>&#12377;&#12356;&#12426;&#12367;&#12426;&#12423;&#12358;&#12424;&#12358;&#12398;</pr></trd>
  </trdgrp>
</drv>

</art>
