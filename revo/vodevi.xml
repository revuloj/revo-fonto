<drv mrk="vodevi.0o">
  <kap><tld/>o</kap>
  <snc mrk="vodevi.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Facila komedia <ref tip="super" cel="teatr.0ajxo">teatra&jcirc;eto</ref>
      kun strofkantoj:
