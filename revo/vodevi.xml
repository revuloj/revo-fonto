  <trd lng="de">Vaudeville</trd>
  <trd lng="en">vaudeville</trd>
  <trd lng="es">vodevil</trd>
  <trd lng="fr">vaudeville</trd>
  <trd lng="hu">zen&eacute;s <ind>boh&oacute;zat</ind></trd>
  <trd lng="nl">vaudeville</trd>
  <trdgrp lng="pt">
   <trd>vaudeville</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_je;&c_v;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>
