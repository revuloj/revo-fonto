<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vodevil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vodevi.0o">
  <kap><tld/>o</kap>
  <snc mrk="vodevi.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Facila komedia <ref tip="super" cel="teatr.0ajxo">teatra&jcirc;eto</ref>
      kun strofkantoj:
      <ekz>
        mi ja anka&ubreve; verkas diversajn <tld/>etojn
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 6a</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="vodevi.0i.RIM_ARK">
      Malnova verko prezentas la formon <em>va&ubreve;devilo</em>:
      <ekz>
        &gcirc;i estas &scirc;erca&jcirc;o, same kiel la va&ubreve;devilo
        kun mia &scirc;ajna morto
        <fnt>
         <aut>N. Hohlov</aut>,
         <vrk><url
         ref="http://esperanto.net/literaturo/dram/libr/mortdelegit.pdf">
         La Morto de la Delegito de U.E.A.</url></vrk>,
         <lok>1924</lok>
        </fnt>.
      </ekz>
    </rim>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_e;&c_v;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_je;&c_v;&c_i;&c_l;</trd>
  <trd lng="ca">vodevil</trd>
  <trdgrp lng="cs">
    <trd>popÄ›vek</trd>,
    <trd>vaudeville</trd>
  </trdgrp>
  <trd lng="de">Vaudeville</trd>
  <trd lng="en">vaudeville</trd>
  <trd lng="es">vodevil</trd>
  <trd lng="fr">vaudeville</trd>
  <trd lng="hu">zen&eacute;s <ind>boh&oacute;zat</ind></trd>
  <trd lng="nl">vaudeville</trd>
  <trdgrp lng="pt">
   <trd>vaudeville</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_je;&c_v;&c_i;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>popevok</trd>,
    <trd>vaudeville</trd>
  </trdgrp>
</drv>
</art>
