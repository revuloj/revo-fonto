<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trot</rad>/i
</kap>
<drv mrk="trot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="klr">(pp kvarpieduloj)</uzo>
    <dif>
      Kuri en aparta maniero kun rapideco meza inter pa&scirc;ado kaj galopo.
    </dif>
    <refgrp tip="sin">
      <ref cel="ambl.0o">amblo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_g;&c_ch;&c_y; &c_r;&c_y;&c_s;&c_mol;&c_s;&c_ju;</trd>
  <trdgrp lng="de">
    <trd>trotten</trd>,
    <trd>traben</trd>
  </trdgrp>
  <trd lng="en">trot</trd>
  <trd lng="hu">&uuml;get</trd>
  <trd lng="nl">draven</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol; <ind>&c_r;&c_y;&c_s;&c_mol;&c_ju;</ind></trd>,
    <trd>&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol; <ind>&c_r;&c_y;&c_s;&c_c;&c_o;&c_j;</ind></trd>
  </trdgrp>
</drv>
</art>
