<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ekspozici</rad>/o
</kap>
<drv mrk="ekspoz1.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc mrk="ekspoz1.0o.prezentado">
<dif>
Publika prezentado, elmontrado de diversaj produktoj de industrio kaj arto:
<ekz><tld/>o de pentra&jcirc;oj, de tola&jcirc;oj, de a&ubreve;tomobiloj;</ekz>
<ekz>universala <tld/>o.</ekz>
</dif>
  <trdgrp lng="de">
     <trd>Ausstellung</trd>,
     <trd>Schau</trd>
  </trdgrp>
<trd lng="hu">ki&aacute;ll&iacute;t&aacute;s</trd>
</snc>
<snc mrk="ekspoz1.0o.kolekto">
<dif>
Kolekto da <tld/>itaj objektoj. 
</dif>
  <trdgrp lng="de">
     <trd>Ausgestelltes</trd>,
     <trd>ausgestellte Ware</trd>,
     <trd>ausgestelltes Objekt</trd>,
     <trd>Schauware</trd>,
     <trd>Exponat</trd>
  </trdgrp>
<trd lng="hu">ki&aacute;ll&iacute;t&aacute;si t&aacute;rgyak</trd>
</snc>
<snc mrk="ekspoz1.0o.ejo">
<dif>
Ejo, kie oni <tld/>as. 
</dif>
  <trdgrp lng="de">
     <trd>Ausstellungsraum</trd>,
     <trd>Ausstellungshalle</trd>,
     <trd>Pr&auml;sentationsraum</trd>,
     <trd>Schauraum</trd>
  </trdgrp>
<trdgrp lng="hu">
  <trd>ki&aacute;ll&iacute;t&oacute;helyis&eacute;g</trd>,
  <trd>ki&aacute;ll&iacute;t&oacute;terem</trd>
</trdgrp>
</snc>
<trdgrp lng="be">
  <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>,
  <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_z;&c_ib;&c_c;&c_y;&c_ja;</trd>
</trdgrp>
<trd lng="en">exposition</trd>
<trd lng="es">exposici&oacute;n</trd>
<trd lng="ca">exposici&oacute;</trd>
<trd lng="fr">exposition<klr tip="ind"> (d'&oelig;uvres, de produits,...)</klr></trd>
<trdgrp lng="ru">
  <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;</trd>,
  <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
</trdgrp>
</drv>
<drv mrk="ekspoz1.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
<tld lit="E"/>e prezenti.  
</dif>
</snc>
  <trdgrp lng="de">
     <trd>ausstellen</trd>,
     <trd>zur <ind>Schau stellen</ind></trd>
  </trdgrp>
<trdgrp lng="be">
  <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
  <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
</trdgrp>
<trd lng="es">exponer<klr tip="ind"> (poner en exposici&oacute;n)</klr></trd>
<trd lng="ca">exposar<klr tip="ind"> (a un p&uacute;blic)</klr></trd>
<trd lng="fr">exposer<klr tip="ind"> (mettre en exposition)</klr></trd>
<trd lng="hu">ki&aacute;ll&iacute;t</trd>
<trdgrp lng="ru">
  <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
  <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</trdgrp>
</drv>
</art>
