<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: makul.xml,v 1.47 2024/07/29 18:33:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>makul</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="makul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="makul.0o.KOMUNE">
    <dif>
      Hazarda alikolora&jcirc;o sur io.
    </dif>
    <subsnc mrk="makul.0o.malpurajxo">
      <dif>
        Malpurigita a&ubreve; difektita loko sur io:
        <ekz>
          <ind>sang<tld/>o</ind>
          <fnt>
            <bib>IdO</bib>
            <lok>Tria Parto</lok>
          </fnt>;
          <trd lng="de">Blutfleck</trd>
        </ekz>
        <ekz>
          nigra <ind>ink<tld/>o</ind>
          <fnt>
            <bib>IdO</bib>
            <lok>Kvina Parto</lok>
          </fnt>;
          <trd lng="de">Tintenfleck</trd>
        </ekz>
        <ekz>
          iom malzorge vestita kun kelkaj <ind>gras<tld/>o</ind>j sur la ve&scirc;to
          <fnt>
            <bib>MrA</bib>
            <lok>&Ccirc;apitro XIV</lok>
          </fnt>;
          <trd lng="de">Fettfleck</trd>
        </ekz>
        <ekz>
          sur la karno de la brulvundo aperos <tld/>o ru&gcirc;ete-blanka
          <fnt>
            <bib>MT</bib>
            <lok>Levidoj 13:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          anstata&ubreve; beleco estos brul<tld/>oj
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 3:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          dis&scirc;irita, gras<tld/>ita lana &ccirc;apo
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          ne&gcirc;<tld/>oj persistis sur la bruna, <klr>[&#8230;]</klr> kaj verda tereno
          <fnt>
            <bib>KKr</bib>
            <lok>I</lok>
          </fnt>;
        </ekz>
        <ekz>
          tra la kordono videblis anka&ubreve; la du bril<tld/>oj sur la brusto de la centuriestro
          <fnt>
            <bib>MkM</bib>
            <lok>16</lok>
          </fnt>;
        </ekz>
        <ekz>
          tra amba&ubreve; fenestroj <klr>[&#8230;]</klr> brilas la malalta suno <klr>[&#8230;]</klr> kaj en
          &ccirc;i varmaj sun<tld/>oj saltas kaj dancas knabeto
          <fnt>
            <bib>Kiso</bib>
            <lok>La Brulanta Urbo</lok>
          </fnt>;
        </ekz>
        <ekz>
          vi spertis la koincidojn de la
          protuberancoj kaj de la sun<tld/>oj
          <fnt>
            <bib>VaK</bib>
            <lok>Dekoka &Ccirc;apitro</lok>
          </fnt>;
        </ekz>
        <ekz>
          e&ccirc; sur la suno trovi&gcirc;as <tld/>oj
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
      <trd lng="cs">skvrna</trd>
      <trd lng="de">Fleck</trd>
      <trdgrp lng="hu">
        <trd>folt</trd>,
        <trd>petty</trd>
      </trdgrp>
      <trd lng="nl">vlek</trd>
      <trd lng="pl">plama</trd>
    </subsnc>
    <subsnc mrk="makul.0o.natura">
      <dif>
        Natura kolora <ref tip="super" cel="punkt.0o.makuleto">punkto</ref>:
        <ekz>
          sur &scirc;iaj <klr>[&#8230;]</klr> vangoj aperis ru&gcirc;aj <tld/>oj
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          oni lin konas, kiel <klr>[&#8230;]</klr> <tld/>haran hundon
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
      <trd lng="cs">skvrna</trd>
      <trdgrp lng="de">
        <trd>Fleck</trd>,
        <trd>Klecks</trd>
      </trdgrp>
      <trd lng="es">mancha</trd>
      <trdgrp lng="hu">
        <trd>folt</trd>,
        <trd>szepl&odblac;</trd>,
      </trdgrp>
      <trd lng="nl">vlek</trd>
      <trd lng="pl">plama</trd>
    </subsnc>
  </snc>
  <snc mrk="makul.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malbona, difektita punkto:
      <ekz>
        tio &jcirc;etas <tld/>on sur lian karakteron kaj elmetas lin al puno
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio estis <klr>[&#8230;]</klr> respekto al &scirc;ia sen<tld/>a pureco kaj nobla animo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni metis <tld/>on en mian kalkulon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas por mi <tld/>o en la okulo
        <klr>(<ref cel="gxen.0a">&gcirc;eno</ref>,
        ofendeto, <ref tip="vid" cel="ost.0o">osto</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        neniu sanktulo estas sen <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_ja;&c_m;&c_a; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_g;&c_a;&c_n;&c_mol;&c_b;&c_a;</trd>,
      <trd>&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trd lng="br">tarch</trd>
    <trdgrp lng="de">
      <trd>Schaden</trd>,
      <trd>Makel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spot</trd>,
      <trd>stain</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>folt</trd>,
      <trd>makula</trd>
    </trdgrp>
    <trd lng="nl">smet</trd>
    <trd lng="pl">skaza</trd>
  </snc>
  <snc mrk="makul.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loka <ref tip="super" cel="makul.hauxt0o">ha&ubreve;t<tld/>o</ref>, sen reliefo nek penetrado,
      rezultanta el simpla
      modifi&gcirc;o de la ha&ubreve;ta koloro:
      <ekz>
        efelidoj estas etaj pigmentaj <tld/>oj
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Efelido">Efelido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="efelid.0o">efelido</ref>,
       <ref cel="hematom.0o">hematomo</ref>,
       <ref cel="lentug.0o.MED">lentugo</ref>,
       <ref cel="sang.0umo">sangumo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
    <trd lng="de">Macula</trd>
    <trd lng="en">macule</trd>
    <trd lng="fr">macule</trd>
    <trd lng="pl">plama</trd>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_n;&c_o;</trd>
  <trd lng="cs">skvrna, ka&ncaron;ka</trd>
  <trd lng="fa">&a_l;&f_k;&a_h;</trd>
  <trd lng="fr">tache</trd>
  <trdgrp lng="ja">
    <trd>&#12375;&#12415;</trd>,
    <trd>&#12424;&#12372;&#12428;</trd>,
    <trd>&#26001;&#28857;  <pr>&#12399;&#12435;&#12390;&#12435;</pr></trd>,
    <trd>&#26001;&#32011;  <pr>&#12399;&#12435;&#12418;&#12435;</pr></trd>,
    <trd>&#12406;&#12385;</trd>,
    <trd>&#27738;&#28857;  <pr>&#12362;&#12390;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#2360;&#2381;&#2341;&#2366;&#2344;</trd>
  <trdgrp lng="pl">
    <trd>plama</trd>,
    <trd>skaza</trd>,
    <trd>zaka&lstroke;a</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mancha</trd>,
    <trd>n&oacute;doa</trd>,
    <trd>m&aacute;cula</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pat&abreve;</trd>,
    <trd>defect</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_ja;&c_t;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>f&lcaron;ak</trd>,
    <trd>machu&lcaron;a</trd>,
    <trd>&scaron;kvrna</trd>
  </trdgrp>
  <trd lng="sv">fl&auml;ck</trd>
  <trd lng="tr">leke</trd>
  <trd lng="uk">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#26001; <pr>b&amacron;n</pr></trd>,
    <trd>&#26001;&#28857; <pr>b&amacron;ndi&#462;n</pr></trd>,
    <trd>&#26001;&#40670; <pr>b&amacron;ndi&#462;n</pr></trd>,
    <trd>&#29623; <pr>di&agrave;n</pr></trd>,
    <trd>&#29781; <pr>xi&aacute;</pr></trd>,
    <trd>&#30133; <pr>c&imacron;</pr></trd>,
    <trd>&#33394;&#26001; <pr>s&egrave;b&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="makul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="makul.0i.KOMUNE">
    <dif>
      Fari <tld/>ojn sur io:
      <ekz>
        iliaj vestoj estis dis&scirc;iritaj, <tld/>itaj de la strata koto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        miajn manojn ne <tld/>is sango
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne <tld/>u vian filan koron
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        ruzo kaj prudento <klr>[ne]</klr> &#11005; <tld lit="M"/>as viron, kiu sin dedi&ccirc;is &#11005; Al
        plenumado de kura&gcirc;aj faroj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se kalumnio ne brulas, &gcirc;i almena&ubreve; <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">tarcha&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>poskvrnit</trd>,
    <trd>zamazat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beflecken</trd>,
    <trd>besch&auml;digen</trd>
  </trdgrp>
  <trd lng="en">stain</trd>
  <trdgrp lng="es">
    <trd>manchar</trd>,
    <trd>ensuciar</trd>
  </trdgrp>
  <trd lng="fa">&a_l;&f_k;&a_h;&#8204;&a_d;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">tacher</trd>
  <trdgrp lng="hu">
    <trd>foltot ejt</trd>,
    <trd>bepiszkol</trd>,
    <trd>pettyez</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12424;&#12372;&#12377;</trd>,
    <trd>&#12375;&#12415;&#12434;&#12388;&#12369;&#12427;</trd>,
    <trd>&#12369;&#12364;&#12377;</trd>
  </trdgrp>
  <trd lng="nl">bevlekken</trd>
  <trdgrp lng="pl">
    <trd>brudzi&cacute;</trd>,
    <trd>plami&cacute;</trd>,
    <trd>bruka&cacute;</trd>,
    <trd>plugawi&cacute;</trd>,
    <trd>ska&zdot;a&cacute;</trd>,
    <trd>szarga&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>murdar</trd>,
    <trd>poluat</trd>,
    <trd>contaminat</trd>,
    <trd>spurca</trd>,
    <trd>p&abreve;ta</trd>,
    <trd>m&acirc;nji</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_ja;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">po&scaron;kvrni&tcaron;</trd>
  <trd lng="tr">lekelemek</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_sh;&c_a;&c_t;&c_i; &c_p;&c_l;&c_ja;&c_m;&c_u;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_m;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#29781;&#30133; <pr>xi&aacute;c&imacron;</pr></trd>
</drv>

<drv mrk="makul.0ita">
  <kap>
    <tld/>ita
  </kap>
  <snc>
    <dif>
      Havanta <tld/>ojn; <tld/>forme mikskolora (<ref tip="sub" cel="ruan.0a">ruana</ref> ks):
      <ekz>
        sang<tld/>ita glavo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        apartigu <klr>[&#8230;]</klr> &ccirc;iun &scirc;afon mikskoloran kaj <tld/>itan
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        li apartigis en tiu tago la virkaprojn striitajn kaj <tld/>itajn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis <tld/>itan tunikon kaj perukon el &scirc;afa felo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mevoj &scirc;vebis super la maro por kapti nutra&jcirc;on kaj <klr>[&#8230;]</klr> mi balda&ubreve; venos
        por forpreni la grandajn blue <tld/>itajn ovojn en la dunoj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_m;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gefleckt</trd>,
    <trd>befleckt</trd>,
    <trd>gesprenkelt</trd>,
    <trd>fleckig</trd>,
    <trd>get&uuml;pfelt</trd>
  </trdgrp>
</drv>

<drv mrk="makul.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="makul.sen0a.KOMUNE">
    <dif>
      Nehavanta <tld/>on materian a&ubreve; moralan:
      <ekz>
        <nom>Noa</nom> estis homo virta kaj sen<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        konscienco sen<tld/>a estas kuseno plej mola
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
