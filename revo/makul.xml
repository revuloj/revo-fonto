  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nepo&scaron;kvrnen&yacute;</trd>
  <trd lng="sv">obefl&auml;ckad</trd>
  <trdgrp lng="tr">
    <trd>ar&inodot;</trd>,
    <trd>lekesiz</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_o;&c_v;&c_a;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_zh;&c_o;&c_d;&c_n;&c_o;&c_ji; &c_p;&c_l;&c_ja;&c_m;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_n;&c_je;&c_p;&c_o;&c_r;&c_o;&c_ch;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26080;&#29781; <pr>w&uacute;xi&aacute;</pr></trd>,
    <trd>&#28961;&#29781; <pr>w&uacute;xi&aacute;</pr></trd>,
    <trd>&#23436;&#21892;&#30340; <pr>w&aacute;nsh&agrave;nde</pr></trd>,
    <trd>&#23436;&#32654;&#22320; <pr>w&aacute;nm&ecaron;ide</pr></trd>,
    <trd>&#23519;&#23519; <pr>ch&aacute;ch&aacute;</pr></trd>,
    <trd>&#27794;&#26377;&#32570;&#40670; <pr>m&eacute;iy&#466;uqu&emacron;di&#462;n</pr></trd>,
