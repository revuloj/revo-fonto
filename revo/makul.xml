<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>makul</rad>/o
</kap>
<drv mrk="makul.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Malpuriga a&ubreve; difekta loko sur io:
       <ekz>
         grasa, inka <tld/>o;
       </ekz>
       <ekz>
         <tld/>o en diamanto;
       </ekz>
       <ekz>
         e&ccirc; sur la suno trovi&gcirc;as <tld/>o<fnt>Z</fnt>
       </ekz>
     </dif>
     <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
     <trd lng="cs">skvrna</trd>
     <trdgrp lng="de">
       <trd>Fleck</trd>,
       <trd>Loch</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>folt</trd>,
       <trd>petty</trd>
     </trdgrp>     
     <trd lng="nl">vlek</trd>
   </snc>
   <snc>
     <dif>
       Natura kolora punkto:
       <ekz>
         <tld/>oj de la ha&ubreve;to, de la felo.
       </ekz>
     </dif>
     <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
     <trd lng="cs">skvrna</trd>
     <trdgrp lng="de">
       <trd>Fleck</trd>,
       <trd>Klecks</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>folt</trd>,
       <trd>szepl&odblac;</trd>,
     </trdgrp>   
     <trd lng="nl">vlek</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Malbona, difekta punkto:
       <ekz>neniu sanktulo estas sen <tld/>o
<fnt><bib>PrV</bib></fnt>;</ekz>
       <ekz>
         tiu &ccirc;apitro estas <tld/>o en la verko;
       </ekz>
       <ekz>
         tio &jcirc;etas <tld/>on sur lian karakteron<fnt>Z</fnt>;
       </ekz>
       <ekz>&gcirc;i estas por mi <tld/>o en la okulo
         <klr>(&gcirc;eno, ofendeto. <ref tip="vid"
         cel="ost.0o">osto</ref>)</klr>
<fnt><bib>PrV</bib></fnt>;</ekz>
       <ekz>
         la forfalo de la Romana Imperio metis en la historion
         dum mil jaroj nigran <tld/>on.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_p;&c_l;&c_ja;&c_m;&c_a; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
       <trd>&c_g;&c_a;&c_n;&c_mol;&c_b;&c_a;</trd>,
       <trd>&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_a;&c_g;&c_a;</trd>
     </trdgrp>
     <trd lng="br">tarch</trd>
     <trdgrp lng="de">
       <trd>Schaden</trd>,
       <trd>Makel</trd>
     </trdgrp>
     <trd lng="fr">tache</trd>
     <trdgrp lng="hu">
       <trd>folt</trd>,
       <trd>makula</trd>
     </trdgrp>     
     <trd lng="nl">smet</trd>
   </snc>
    <trdgrp lng="pt">
      <trd>mancha</trd>,
      <trd>n&oacute;doa</trd>,    
      <trd>m&aacute;cula</trd>
    </trdgrp>
   <trd lng="sv">fl&auml;ck</trd>
   <trd lng="ru">&c_p;&c_ja;&c_t;&c_n;&c_o;</trd>
</drv>

<drv mrk="makul.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Fari <tld/>ojn sur io:
       <ekz>
         <tld/>i sian veston
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         la tekston <tld/>as tro da preseraroj;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         <tld/>i la honoron de iu<fnt>Z</fnt>;
       </ekz>
       <ekz><uzo tip="stl">FIG</uzo>
         se kalumnio ne brulas, &gcirc;i almena&ubreve; <tld/>as
<fnt><bib>PrV</bib></fnt>;</ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         <tld/>ita konscienco.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_ib;&c_c;&c_mol;</trd>
   <trd lng="br">tarcha&ntilde;</trd>
   <trd lng="cs">poskvrnit</trd>
   <trdgrp lng="de">
     <trd>beflecken</trd>,
     <trd>besch&auml;digen</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>foltot ejt</trd>,
     <trd>bepiszkol</trd>,
     <trd>pettyez</trd>
   </trdgrp>   
   <trd lng="nl">bevlekken</trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; 
&c_p;&c_ja;&c_t;&c_n;&c_o;</trd>,
     <trd>&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="fr">tacher</trd>
</drv>

<drv mrk="makul.sen0a">
   <kap>sen<tld/>a</kap>
   <snc>
     <dif>
       Nehavanta <tld/>on materian a&ubreve; moralan:
       <ekz>
         sen<tld/>a tuko;
       </ekz>
       <ekz>
         sen<tld/>a honoro, konduto, junulino;
       </ekz>
       <ekz>
         ne <tld/>u la gloron de la patroj, kiu jam en la da&ubreve;ro de 7
         jarcentoj konservi&gcirc;is sen<tld/>a<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_je;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_l;&c_je;&c_n;&c_y;</trd>,
     <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_k;&c_o;&c_r;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="br">disaotr</trd>
   <trd lng="cs">neposkvrn&ecaron;n&yacute;</trd>
   <trdgrp lng="de">
     <trd>makellos</trd>,
     <trd>perfekt</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>vlekkeloos</trd>,
     <trd>onbevlekt</trd>
   </trdgrp>
   <trd 
lng="ru">&c_n;&c_je;&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
   <trd lng="sv">obefl&auml;ckad</trd>
   <trd lng="fr">immacul&eacute;</trd>
</drv>
</art>
