<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: makul.xml,v 1.39 2022/09/22 08:43:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>makul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="makul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="makul.0o.KOMUNE">
    <dif>
      Hazarda alikolora&jcirc;o sur io.
    </dif>
    <subsnc mrk="makul.0o.malpurajxo">
      <dif>
        Malpurigita a&ubreve; difektita loko sur io:  
        <ekz>
          sur la karno de la brulvundo aperos <tld/>o ru&gcirc;ete-blanka
          <fnt>
            <bib>MT</bib>
            <lok>Levidoj 13:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          dis&scirc;irita, gras<tld/>ita lana &ccirc;apo
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          e&ccirc; sur la suno trovi&gcirc;as <tld/>oj
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
      <trd lng="cs">skvrna</trd>
      <trd lng="de">Fleck</trd>
      <trdgrp lng="hu">
        <trd>folt</trd>,
        <trd>petty</trd>
      </trdgrp>
      <trd lng="nl">vlek</trd>
      <trd lng="pl">plama</trd>
    </subsnc>
    <subsnc mrk="makul.0o.natura">
      <dif>
        Natura kolora <ref tip="super" cel="punkt.0o.makuleto">punkto</ref>: 
        <ekz>
          sur &scirc;iaj <klr>[&#8230;]</klr> vangoj aperis ru&gcirc;aj <tld/>oj
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          oni lin konas, kiel <klr>[&#8230;]</klr> <tld/>haran hundon
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
      <trd lng="cs">skvrna</trd>
      <trdgrp lng="de">
        <trd>Fleck</trd>,
        <trd>Klecks</trd>
      </trdgrp>
      <trd lng="es">mancha</trd> 
      <trdgrp lng="hu">
        <trd>folt</trd>,
        <trd>szepl&odblac;</trd>,
      </trdgrp>
      <trd lng="nl">vlek</trd>
      <trd lng="pl">plama</trd>
    </subsnc>
  </snc>
  <snc mrk="makul.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malbona, difektita punkto:
      <ekz>
        tio &jcirc;etas <tld/>on sur lian karakteron kaj elmetas lin al puno
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio estis <klr>[&#8230;]</klr> respekto al &scirc;ia sen<tld/>a pureco kaj nobla animo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni metis <tld/>on en mian kalkulon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas por mi <tld/>o en la okulo
        <klr>(<ref cel="gxen.0a">&gcirc;eno</ref>, 
        ofendeto, <ref tip="vid" cel="ost.0o">osto</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        neniu sanktulo estas sen <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_ja;&c_m;&c_a; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_g;&c_a;&c_n;&c_mol;&c_b;&c_a;</trd>,
      <trd>&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trd lng="br">tarch</trd>
    <trdgrp lng="de">
      <trd>Schaden</trd>,
      <trd>Makel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spot</trd>,
      <trd>stain</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>folt</trd>,
      <trd>makula</trd>
    </trdgrp>
    <trd lng="nl">smet</trd>
    <trd lng="pl">skaza</trd>
  </snc>
  <snc mrk="makul.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loka <ref tip="super" cel="makul.hauxt0o">ha&ubreve;t<tld/>o</ref>, sen reliefo nek penetrado,
      rezultanta el simpla 
      modifi&gcirc;o de la ha&ubreve;ta koloro:
      <ekz>
        efelidoj estas etaj pigmentaj <tld/>oj
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Efelido">Efelido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="efelid.0o">efelido</ref>,
       <ref cel="hematom.0o">hematomo</ref>,
       <ref cel="lentug.0o.MED">lentugo</ref>,
       <ref cel="sang.0umo">sangumo</ref>
    </refgrp>
    <trd lng="de">Macula</trd>
    <trd lng="en">macule</trd>
    <trd lng="fr">macule</trd>
    <trd lng="pl">plama</trd>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_n;&c_o;</trd>
  <trd lng="cs">skvrna, ka&ncaron;ka</trd>
  <trd lng="fa">&a_l;&f_k;&a_h;</trd>
  <trd lng="fr">tache</trd>
  <trd lng="ne">&#2360;&#x94D;&#2341;&#x93E;&#2344;</trd>
  <trdgrp lng="pl">
    <trd>plama</trd>,
    <trd>skaza</trd>,
    <trd>zaka&lstroke;a</trd>
  </trdgrp>  
  <trdgrp lng="pt">
    <trd>mancha</trd>,
    <trd>n&oacute;doa</trd>,
    <trd>m&aacute;cula</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pat&abreve;</trd>,
    <trd>defect</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_ja;&c_t;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>f&lcaron;ak</trd>,
    <trd>machu&lcaron;a</trd>,
    <trd>&scaron;kvrna</trd>
  </trdgrp>
  <trd lng="sv">fl&auml;ck</trd>
  <trd lng="tr">leke</trd>
  <trd lng="uk">&#x43F;&#x43B;&#x44F;&#x43C;&c_a;</trd>
</drv>

<drv mrk="makul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="makul.0i.KOMUNE">
    <dif>
      Fari <tld/>ojn sur io:
      <ekz>
        iliaj vestoj estis dis&scirc;iritaj, <tld/>itaj de la strata koto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sang<tld/>ita glavo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        miajn manojn ne <tld/>is sango
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        apartigu <klr>[&#8230;]</klr> &ccirc;iun &scirc;afon mikskoloran kaj <ind><tld/>ita</ind>n
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:32</lok>
        </fnt>;
        <trd lng="de">gefleckt</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne <tld/>u vian filan koron
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        ruzo kaj prudento <klr>[ne]</klr> / Makulas viron, kiu sin dedi&ccirc;is / Al
        plenumado de kura&gcirc;aj faroj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se kalumnio ne brulas, &gcirc;i almena&ubreve; <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">tarcha&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>poskvrnit</trd>,
    <trd>zamazat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beflecken</trd>,
    <trd>besch&auml;digen</trd>
  </trdgrp>
  <trd lng="en">stain</trd>
  <trdgrp lng="es">
    <trd>manchar</trd>,
    <trd>ensuciar</trd>
  </trdgrp>
  <trd lng="fa">&a_l;&f_k;&a_h;&zwnj;&a_d;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">tacher</trd>
  <trdgrp lng="hu">
    <trd>foltot ejt</trd>,
    <trd>bepiszkol</trd>,
    <trd>pettyez</trd>
  </trdgrp>
  <trd lng="nl">bevlekken</trd>
  <trdgrp lng="pl">
    <trd>brudzi&cacute;</trd>,
    <trd>plami&cacute;</trd>,
    <trd>bruka&cacute;</trd>,
    <trd>plugawi&cacute;</trd>,
    <trd>ska&zdot;a&cacute;</trd>,
    <trd>szarga&cacute;</trd>
  </trdgrp> 
  <trdgrp lng="ro">
    <trd>murdar</trd>,
    <trd>poluat</trd>,
    <trd>contaminat</trd>,
    <trd>spurca</trd>,
    <trd>p&abreve;ta</trd>,
    <trd>m&acirc;nji</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_ja;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">po&scaron;kvrni&tcaron;</trd>
  <trd lng="tr">lekelemek</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_i;&c_sh;&c_a;&c_t;&c_i; &#x43F;&#x43B;&#x44F;&#x43C;&c_u;</trd>,
    <trd>&#x43F;&#x43B;&#x44F;&#x43C;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43B;&#x44F;&#x43C;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="makul.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="makul.sen0a.KOMUNE">
    <dif>
      Nehavanta <tld/>on materian a&ubreve; moralan:
      <ekz>
        <nom>Noa</nom> estis homo virta kaj sen<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        konscienco sen<tld/>a estas kuseno plej mola
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_k;&c_o;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">disaotr</trd>
  <trdgrp lng="cs">
    <trd>bez&uacute;honn&yacute;</trd>,
    <trd>neposkvrn&ecaron;n&yacute;</trd>,
    <trd>zachoval&yacute; <klr>(mravn&ecaron;, spole&ccaron;ensky)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>makellos</trd>,
    <trd>perfekt</trd>
  </trdgrp>
  <trd lng="en">spotless</trd>
  <trd lng="es">inmaculada</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y; &a_ayn;&f_y;&a_b; &a_w; &a_n;&a_q;&a_S;</trd>,
    <trd>&a_b;&a_d;&a_w;&a_n; <ind>&a_kh;&a_d;&a_sh;&a_h;</ind></trd>,
    <trd>&f_k;&a_A;&a_m;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">immacul&eacute;</trd>
  <trdgrp lng="nl">
    <trd>vlekkeloos</trd>,
    <trd>onbevlekt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niepokalany</trd>,
    <trd>nieskalany</trd>,
    <trd>nieposzlakowany</trd>
  </trdgrp> 
  <trdgrp lng="ro">
    <trd>imaculat</trd>,
    <trd>inocent</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nepo&scaron;kvrnen&yacute;</trd>
  <trd lng="sv">obefl&auml;ckad</trd>
  <trdgrp lng="tr">
    <trd>ar&inodot;</trd>,
    <trd>lekesiz</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_z;&c_a;&#x43F;&#x43B;&#x44F;&#x43C;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_zh;&#x43E;&c_d;&#x43D;&#x43E;&c_ji; &#x43F;&#x43B;&#x44F;&#x43C;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43D;. &#x43D;&c_je;&#x43F;&#x43E;&c_r;&#x43E;&c_ch;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="makul.bat0o">
  <kap>bat<tld/>o</kap>
  <snc mrk="makul.bat0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="makul.hauxt0o.MED">Ha&ubreve;tmakulo</ref>
      sekvanta 
      baton, ofte i&gcirc;anta <ref tip="vid" cel="blu.0ajxo.MED">blua&jcirc;o</ref>:
      <ekz>
        la malpli forta knabo eliris kun kelkaj bluaj bat<tld/>oj kaj 
        &scirc;vela&jcirc;oj
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url
        ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="sang.0umo">sangumo</ref>, 
       <ref cel="ehximoz.0o">e&hcirc;imozo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_n;&c_ja;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_p;&c_a;&c_d;&c_c;&c_jo;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>blauer <ind>Fleck</ind></trd>,
    <trd>Bluterguss</trd>,
    <trd>Veilchen <klr>(Bluterguss)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hematoma</trd>,
    <trd>cardenal</trd>,
    <trd>morat&oacute;n</trd>,
    <trd>moret&oacute;n</trd>,
    <trd>equimosis</trd> 
  </trdgrp> 
  <trd lng="en">bruise</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_b;&a_w;&a_d;&f_y;</trd>,
    <trd>&f_k;&a_w;&a_f;&a_t;&f_g;&f_y;</trd>,
    <trd>&a_h;&a_m;&a_A;&a_t;&a_w;&a_m;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bleu<klr> (trace de coup)</klr></trd>,
    <trd>trace de coup</trd>
  </trdgrp>
  <trd lng="pl">siniak</trd>
  <trdgrp lng="ro">
    <trd>julitur&abreve;</trd>,
    <trd>v&acirc;n&abreve;taie</trd>
  </trdgrp>
</drv>

<drv mrk="makul.hauxt0o">
  <kap>ha&ubreve;t<tld/>o</kap>
  <snc mrk="makul.hauxt0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Alikolora parto de <ref tip="malprt" cel="hauxt.0o.ANA">ha&ubreve;to</ref> 
      pro denaska difekto, vundo, <ref tip="sub" cel="ekzant.0o.MED">ekzantemo</ref>...:
      <ekz>
        li havas ha&ubreve;t<tld/>on surviza&gcirc;an kiun li klopodadas ka&scirc;i de
        &ccirc;iuj kiujn li renkontas
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Birmaj Tagoj&rightquot;,
        2007-03-11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a; &c_n;&c_a; &c_s;&c_k;&c_u;&c_r;&c_y;</trd>
  <trd lng="de">Mal <klr>(Hautfleck)</klr></trd>
  <trd lng="en">blemish</trd>
  <trd lng="fa">&a_l;&f_k;&a_h;&a_hamza_sure; &f_p;&a_w;&a_s;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>tache sur la peau</trd>,
    <trd>tache cutan&eacute;e</trd>
  </trdgrp>
  <trd lng="pl">plama <ind>sk&oacute;rna</ind></trd>
  <trd lng="ro">pata de piele</trd>
</drv>
</art>
