<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>makul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="makul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="makul.0o.KOMUNE">
    <dif>
      Hazarda alikolora&jcirc;o sur io.
    </dif>
    <subsnc mrk="makul.0o.malpurajxo">
      <dif>
        Malpurigita a&ubreve; difektita loko sur io:
        <ekz>
          grasa, inka <tld/>o;
        </ekz>
        <ekz>
          <tld/>o en diamanto;
        </ekz>
        <ekz>
          e&ccirc; sur la suno trovi&gcirc;as <tld/>o<fnt>Z</fnt>
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
      <trd lng="cs">skvrna</trd>
      <trdgrp lng="de">
        <trd>Fleck</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>folt</trd>,
        <trd>petty</trd>
      </trdgrp>
      <trd lng="nl">vlek</trd>
    </subsnc>
    <subsnc mrk="makul.0o.natura">
      <dif>
        Natura kolora punkto:
        <ekz>
          <tld/>oj de felo.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_a;</trd>
      <trd lng="cs">skvrna</trd>
      <trdgrp lng="de">
        <trd>Fleck</trd>,
        <trd>Klecks</trd>
      </trdgrp>
      <trd lng="es">mancha</trd> 
      <trdgrp lng="hu">
        <trd>folt</trd>,
        <trd>szepl&odblac;</trd>,
      </trdgrp>
      <trd lng="nl">vlek</trd>
    </subsnc>
  </snc>
  <snc mrk="makul.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malbona, difektita punkto:
      <ekz>
        neniu sanktulo estas sen <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;apitro estas <tld/>o en la verko;
      </ekz>
      <ekz>
        tio &jcirc;etas <tld/>on sur lian karakteron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas por mi <tld/>o en la okulo
        <klr>(&gcirc;eno, ofendeto. <ref tip="vid"
        cel="ost.0o">osto</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_ja;&c_m;&c_a; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_g;&c_a;&c_n;&c_mol;&c_b;&c_a;</trd>,
      <trd>&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trd lng="br">tarch</trd>
    <trdgrp lng="de">
      <trd>Schaden</trd>,
      <trd>Makel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spot</trd>,
      <trd>stain</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>folt</trd>,
      <trd>makula</trd>
    </trdgrp>
    <trd lng="nl">smet</trd>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_n;&c_o;</trd>
  <trd lng="fr">tache</trd>
  <trdgrp lng="pt">
    <trd>mancha</trd>,
    <trd>n&oacute;doa</trd>,
    <trd>m&aacute;cula</trd>
  </trdgrp>
  <trd lng="sv">fl&auml;ck</trd>
  <trd lng="ru">&c_p;&c_ja;&c_t;&c_n;&c_o;</trd>
  <trd lng="tr">leke</trd>
</drv>

<drv mrk="makul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="makul.0i.KOMUNE">
    <dif>
      Fari <tld/>ojn sur io:
      <ekz>
        <tld/>i sian veston
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tekston <tld/>as tro da preseraroj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i la honoron de iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se kalumnio ne brulas, &gcirc;i almena&ubreve; <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ita konscienco.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">tarcha&ntilde;</trd>
  <trd lng="cs">poskvrnit</trd>
  <trdgrp lng="de">
    <trd>beflecken</trd>,
    <trd>besch&auml;digen</trd>
  </trdgrp>
  <trd lng="en">stain</trd>
  <trdgrp lng="es">
    <trd>manchar</trd>,
    <trd>ensuciar</trd>
  </trdgrp>
  <trd lng="fr">tacher</trd> 
  <trdgrp lng="hu">
    <trd>foltot ejt</trd>,
    <trd>bepiszkol</trd>,
    <trd>pettyez</trd>
  </trdgrp>
  <trd lng="nl">bevlekken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_ja;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">lekelemek</trd>
</drv>

<drv mrk="makul.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="makul.sen0a.KOMUNE">
    <dif>
      Nehavanta <tld/>on materian a&ubreve; moralan:
      <ekz>
        sen<tld/>a tuko;
      </ekz>
      <ekz>
        sen<tld/>a honoro, konduto, junulino;
      </ekz>
      <ekz>
        ne <tld/>u la gloron de la patroj, kiu jam en la da&ubreve;ro de 7
        jarcentoj konservi&gcirc;is sen<tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_m;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_k;&c_o;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">disaotr</trd>
  <trd lng="cs">neposkvrn&ecaron;n&yacute;</trd>
  <trdgrp lng="de">
    <trd>makellos</trd>,
    <trd>perfekt</trd>
  </trdgrp>
  <trd lng="en">spotless</trd>
  <trd lng="es">inmaculada</trd> 
  <trd lng="fr">immacul&eacute;</trd>
  <trdgrp lng="nl">
    <trd>vlekkeloos</trd>,
    <trd>onbevlekt</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_p;&c_ja;&c_t;&c_n;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">obefl&auml;ckad</trd>
  <trdgrp lng="tr">
    <trd>ar&inodot;</trd>,
    <trd>lekesiz</trd>
  </trdgrp>
</drv>

<drv mrk="makul.bat0o">
  <kap>bat<tld/>o</kap>
  <snc mrk="makul.bat0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="makul.hauxt0o.MED">Ha&ubreve;tmakulo</ref>
      sekvanta 
      baton, ofte i&gcirc;anta <ref tip="vid"
      cel="blu.0ajxo.MED">blua&jcirc;o</ref>:
      <ekz>
        la malpli forta knabo eliris kun kelkaj bluaj bat<tld/>oj kaj 
        &scirc;vela&jcirc;oj
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url 
        ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>hematoma</trd>,
    <trd>cardenal</trd>,
    <trd>morat&oacute;n</trd>,
    <trd>moret&oacute;n</trd>,
    <trd>equimosis</trd> 
  </trdgrp> 
  <trd lng="en">bruise</trd>
  <trdgrp lng="fr">
    <trd>bleu<klr> (trace de coup)</klr></trd>,
    <trd>trace de coup</trd>
  </trdgrp>
</drv>

<drv mrk="makul.hauxt0o">
  <kap>ha&ubreve;t<tld/>o</kap>
  <snc mrk="makul.hauxt0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Alikolora parto de ha&ubreve;to pro denaska difekto, vundo,
      <ref tip="sub" cel="ekzant.0o.MED">ekzantemo</ref>...:
      <ekz>
        li havas ha&ubreve;t<tld/>on surviza&gcirc;an kiun li klopodadas ka&scirc;i de
        &ccirc;iuj kiujn li renkontas
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Birmaj Tagoj&rightquot;,
        2007-03-11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">blemish</trd>
  <trd lng="fr">tache sur la peau</trd>
</drv>
</art>
