<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korved.xml,v 1.15 2023/12/03 09:58:59 revo Exp $">
<kap>
  <rad>korved</rad>/oj
</kap>
<drv mrk="korved.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="paser.0oformaj">paseroformaj birdoj</ref>
      (<trd lng="la">Corvidae</trd>):
      <ekz>
        oni anka&ubreve; uzas la nomon &leftquot;pigo&rightquot; por iuj similaspektaj <tld/>oj, kiel ekzemple la blua pigo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26308">Pigo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="korv.0o">korvo</ref>,
      <ref cel="korak.0o">korako</ref>,
      <ref cel="frugil.0o">frugilego</ref>,
      <ref cel="kornik.0o">korniko</ref>,
      <ref cel="moned.0o">monedo</ref>,
      <ref cel="pig.0o">pigo</ref>,
      <ref cel="garol.0o">garolo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_m;&c_k;&c_a;&c_ch;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_v;&c_r;&c_a;&c_n;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">c&ograve;rvids</trd>
  <trd lng="de">Rabenv&ouml;gel</trd>
  <trdgrp lng="en">
    <trd>corvids</trd>,
    <trd>crow family</trd>
  </trdgrp>
  <trd lng="fr">corvid&eacute;s</trd>
  <trd lng="hu">varj&uacute;f&eacute;l&eacute;k</trd>
  <trdgrp lng="nl">
    <trd>kraaiachtigen</trd>,
    <trd>kraaien</trd>
  </trdgrp>
  <trd lng="pl">krukowate</trd>
  <trd lng="pt">corv&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_v;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_v;&c_r;&c_a;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x9D09;&#x79D1; <pr>y&#x101;k&#x113;</pr></trd>
</drv>
</art>

