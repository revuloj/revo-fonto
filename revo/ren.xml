<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ren</rad>/o
</kap>
<drv mrk="ren.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Duobla organo sidanta en la lumba regiono kaj sekrecianta la urinon.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_y;&c_r;&c_k;&c_a;</trd>
  <trd lng="ca">rony&oacute;</trd>
  <trd lng="de">Niere</trd>
  <trd lng="en">kidney</trd>
  <trd lng="es">ri&ntilde;&oacute;n</trd>
  <trd lng="fr">rein</trd>
  <trd lng="hu">vese</trd>
  <trd lng="nl">nier</trd>
  <trd lng="pl">nerka</trd>
  <trd lng="pt">rim</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
</drv>

<drv mrk="ren.sur0a">
  <kap>sur<tld/>a</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <ref tip="vid" cel="gland.surrena0o">sur<tld/>a glando</ref>
  </snc>
  <trd lng="fr">surr&eacute;nal</trd>
</drv>
</art>
