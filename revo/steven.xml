<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: steven.xml,v 1.10 2022/03/29 14:19:46 revo Exp $">
<kap>
  <rad>steven</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="steven.0o">
  <kap><tld/>o</kap>
  <snc mrk="steven.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Elstara firma&jcirc;o &ccirc;e la pruo a&ubreve; la po&ubreve;po
      de <ref tip="malprt" cel="sxip.0o">&scirc;ipo</ref>:
      <ekz>
        la boatisto pendigas sur ilian <tld/>on la fajreton kaj repu&scirc;as
        per la piedo for de la ponteto
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">p&rcaron;&iacute;&dcaron;</trd>
  <trd lng="de">Steven</trd>
  <trd lng="hu">orrt&odblac;ke</trd>
  <trd lng="nl">steven</trd>
  <trd lng="pl">stewa</trd>
  <trd lng="ru">&c_sh;&c_t;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">zvisl&yacute; provn&yacute; tr&aacute;m</trd>
  <trd lng="uk">&#x444;&#x43E;&#x440;&#x448;&#x442;&#x435;&#x432;&#x435;&#x43D;&#x44C;</trd>
</drv>

<drv mrk="steven.antauxa0o">
  <kap>anta&ubreve;a <tld/>o</kap>
  <snc mrk="steven.antauxa0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Elstara firma&jcirc;o &ccirc;e la pruo de
      <ref tip="malprt" cel="sxip.0o">&scirc;ipo</ref>.
    </dif>
  </snc>
  <trd lng="ca">roda<klr> (n&agrave;ut.)</klr></trd>
  <trd lng="ru">&c_f;&c_o;&c_r;&c_sh;&c_t;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="en">stem</trd>
  <trd lng="fr">&eacute;trave</trd>
  <trd lng="de">Vorsteven</trd>
  <trdgrp lng="pl">
    <trd>stewa dziobowa</trd>,
    <trd>dziobnica</trd>
  </trdgrp>
</drv>

<drv mrk="steven.posta0o">
  <kap>posta <tld/>o</kap>
  <snc mrk="steven.posta0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Firma trabo &ccirc;e la po&ubreve;po de
      <ref tip="malprt" cel="sxip.0o">&scirc;ipo</ref>.
    </dif>
  </snc>
  <trd lng="ca">codast</trd>
  <trd lng="cs">zadn&iacute; k&yacute;l lodi</trd>
  <trdgrp lng="de">
    <trd>Achtersteven</trd>,
    <trd>Hintersteven</trd>
  </trdgrp>
  <trd lng="en">stern-post</trd>
  <trd lng="fr">&eacute;tambot</trd>
  <trdgrp lng="pl">
    <trd>stewa rufowa</trd>,
    <trd>tylnica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_n;&c_p;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_h;&c_t;&c_je;&c_r;&c_sh;&c_t;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zadn&yacute; k&yacute;l lode</trd>
  <trd lng="uk">&#x430;&#x445;&#x442;&#x435;&#x440;&#x448;&#x442;&#x435;&#x432;&#x435;&#x43D;&#x44C;</trd>
</drv>
</art>
