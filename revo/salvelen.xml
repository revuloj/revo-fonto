<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salvelen.xml,v 1.8 2024/12/18 22:28:01 revo Exp $">
<kap>
  <rad>salvelen</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="i">salvelin</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="salvelen.0o">
  <kap><tld/>o,
  <var><kap><tld var="i"/>o</kap></var></kap>
  <snc mrk="salvelen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref>
      simila al salmo kun pli etaj skvamoj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="salmed.0oj">salmedoj</ref>
      (<trd lng="la">Salvelinus</trd>):
      <ekz>
        ili man&gcirc;is frita&jcirc;on de fi&scirc;o &ndash; precize de <tld/>o
        <fnt>
          <aut>R. Haupenthal</aut>
          <vrk>Dokumento pri la morto de Carlo Bourlet</vrk>
          <lok><url ref="https://esperanto-ondo.ru/Ondo/258-lode.htm">
          La Ondo de Esperanto, 2016:4&ndash;5 (258&ndash;259)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ekpla&ubreve;dis granda kaj lar&gcirc;a <tld var="i"/>o
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        li lerte manipulis per la hokkaptilo, li atente rigardis la
        nigrajn fadenojn, &ccirc;u orfi&scirc;o, tinuso, <tld var="i"/>o
        a&ubreve; pagelo
        ekavidos
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url
          ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/f9/Salvelinus_fontinalis.jpg/640px-Salvelinus_fontinalis.jpg"
      prm="PD"
      ><tld/>o (Salvelinus fontinalis)
        <fnt>
          <aut>Engbretson, Eric, U.S. Fish and Wildlife Service</aut>
          <lok><url ref="&WCW;/File:Salvelinus_fontinalis.jpg"
              >https://commons.wikimedia.org/wiki/File:Salvelinus_fontinalis.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="de">Saibling</trd>
  <trd lng="en">char<klr> (fish)</klr></trd>
  <trd lng="fr">omble</trd>
  <trdgrp lng="pl">
    <trd>golec</trd>,
    <trd>pstr&aogonek;g</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_a;&c_l;&c_ib;&c_ja;</trd>
</drv>
</art>

