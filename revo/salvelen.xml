<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salvelen.xml,v 1.6 2023/07/03 20:30:21 revo Exp $">
<kap>
  <rad>salvelen</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="salvelen.0o">
  <kap><tld/>o</kap>
  <snc mrk="salvelen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref>
      simila al salmo kun pli etaj skvamoj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="salmed.0oj">salmedoj</ref>
      (<trd lng="la">Salvelinus</trd>).
    </dif>
  </snc>
  <rim mrk="salvelen.0o.RIM_HOM">
    alia esperantigo: <em>salvelino</em>:
      <ekz>
        ekpla&ubreve;dis granda kaj lar&gcirc;a salvelino
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
  </rim>
  <trd lng="be">&c_g;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="de">Saibling</trd>
  <trd lng="en">char<klr> (fish)</klr></trd>
  <trd lng="fr">omble</trd>
  <trdgrp lng="pl">
    <trd>golec</trd>,
    <trd>pstr&aogonek;g</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&#x43B;&c_ib;&#x44F;</trd>
</drv>
</art>

