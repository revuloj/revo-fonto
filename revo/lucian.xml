<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lucian.xml,v 1.9 2022/04/01 07:29:25 revo Exp $">
<kap>
  <rad>Lucian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="lucian.0o">
  <kap><tld/>o</kap>
  <snc mrk="lucian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o opinias ke la tuta dramo meritas pliajn konsiderojn 
        post atenta relego kaj rea&ubreve;skultado
        <fnt>
        <vrk>Kvin spektantoj pridemandas a&ubreve;toron</vrk>,
        <bib>LOdE</bib>,
        <lok>2004-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_L;&c_ju;&c_c;&c_y;&c_ja;&c_n;</trd>,
    <trd>&c_L;&c_ju;&c_s;&c_mol;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lucien</trd>,
    <trd>Luciano</trd>
  </trdgrp>
  <trd lng="fr">Lucien</trd>
  <trd lng="it">Luciano</trd>
  <trd lng="nl">Lucien</trd>
  <trd lng="pl">Lucjan</trd>
  <trd lng="pt">Luciano</trd>
  <trdgrp lng="uk">
    <trd>&#x41B;&c_u;&c_c;&c_ib;&c_a;&#x43D;</trd>,
    <trd>&#x41B;&c_u;&c_ch;&c_ib;&c_a;&#x43D;</trd>,
    <trd>&#x41B;&c_u;&#x43A;&#8217;&#x44F;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

