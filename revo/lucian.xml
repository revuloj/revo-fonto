<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Lucian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="lucian.0o">
  <kap><tld/>o</kap>
  <snc mrk="lucian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        <tld/>o opinias ke la tuta dramo meritas pliajn konsiderojn 
        post atenta relego kaj rea&ubreve;skultado
        <fnt>
        <vrk>Kvin spektantoj pridemandas a&ubreve;toron</vrk>,
        <bib>LOdE</bib>,
        <lok>2004-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lucien</trd>,
    <trd>Luciano</trd>
  </trdgrp>
  <trd lng="fr">Lucien</trd>
  <trd lng="nl">Lucien</trd>
  <trd lng="pl">Lucjan</trd>
  <trd lng="pt">Luciano</trd>
</drv>
</art>

