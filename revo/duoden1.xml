<kap>
<rad>duodenit</rad>/o
</kap>
<drv mrk="duoden1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la <ref cel="duoden.0o">duodeno</ref>:
      <ekz>
        simptomoj de <tld/>o estas vomemo, premosento a&ubreve; pika doloro, senapetiteco, neregula
        digestado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_a;&c_d;&c_e;&c_n;&c_ib;&c_t;</trd>
  <trd lng="bg">&c_d;&c_u;&c_o;&c_d;&c_je;&c_n;&c_i;&c_t;</trd>
