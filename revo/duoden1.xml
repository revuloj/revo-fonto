     <trd>Duodenitis</trd>,
     <trd>Entz&uuml;ndung des Zw&ouml;lffingerdarms</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyomb&eacute;lgyullad&aacute;s</trd>,
    <trd>duodenitis</trd>
  </trdgrp>
  <trd lng="it">duodenite</trd>
  <trd lng="ja">&#x5341;&#x4E8C;&#x6307;&#x8178;&#x708E;  <pr>&#x3058;&#x3085;&#x3046;&#x306B;&#x3057;&#x3061;&#x3087;&#x3046;&#x3048;&#x3093;</pr></trd>
  <trd lng="nl">duodenitis</trd>
  <trd lng="pl">zapalenie <ind>trzustki</ind></trd>
  <trd lng="pt">duodenite</trd>
  <trd lng="ru">&c_d;&c_u;&c_o;&c_d;&c_je;&c_n;&c_i;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x443;&#x43E;&#x434;&#x435;&#x43D;&#x456;&#x442;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; &#x434;&#x432;&#x430;&#x43D;&#x430;&#x434;&#x446;&#x44F;&#x442;&#x438;&#x43F;&#x430;&#x43B;&#x43E;&#x457; &#x43A;&#x438;&#x448;&#x43A;&#x438;</trd>
