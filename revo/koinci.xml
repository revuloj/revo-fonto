<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>koincid</rad>/i
</kap>
<drv mrk="koinci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Okazi en la sama momento kiel alia fakto:
      <ekz>
        lia alveno <tld/>os kun mia deiro.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>coincide</trd>,
      <trd>concur</trd>
    </trdgrp>
  </snc>
  <snc mrk="koinci.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(elementa geometrio)</uzo>
    <dif>
      <klr>(p.p. <ref cel="lini.0o.MAT">linioj</ref>,
      <ref cel="surfac.0o.MAT">surfacoj</ref>,
      <ref cel="solid.0o.geometrio">solidoj</ref>)</klr>
      Okupi la saman lokon.
    </dif>
    <trd lng="en">coincide</trd>
  </snc>
  <trd lng="fr">co&iuml;ncider</trd>
</drv>
</art>
