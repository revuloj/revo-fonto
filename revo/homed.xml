<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: homed.xml,v 1.19 2018/11/18 16:10:14 revo Exp $">
<kap>
  <rad>homed</rad>/oj
</kap>
<drv mrk="homed.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="homed.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="simi.0oj">simioj</ref>,
      al kiu apartenas la kvar genroj
      <ref tip="prt" cel="hom.0o">homo</ref>,
      <ref tip="prt" cel="orangu.0o">orangutango</ref>,
      <ref tip="prt" cel="goril.0o">gorilo</ref> kaj
      <ref tip="prt" cel="cximpa.0o.genro">&ccirc;impanzo</ref>:
      <ekz>
        kelkaj ecoj pri <tld/>oj <klr>[estas]</klr>
        pezo: proksimume 48&#8239;&ndash;&#8239;270&#8239;kg, viroj &gcirc;enerale pli grandas ol inoj,
        fortikaj korpoj kaj disvolvitaj brakoj, polekso (man-dikfingro) kaj
        halukso (pied-dikfingro) kontra&ubreve;stareblaj, escepte en homoj, kies
        haluksoj ne estas kontra&ubreve;stareblaj,
        &ccirc;iuj fingroj havas platajn ungojn, neniu havas voston
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1752"
              >Homedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_ib;&c_n;&c_ib;&c_d;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>hominidi</trd>,
    <trd>lid&eacute; a opolid&eacute;</trd>
  </trdgrp>
  <trd lng="de">Menschenaffen</trd>
  <trd lng="es">Hom&iacute;nidos</trd>
  <trd lng="fr">hominid&eacute;s</trd>
  <trd lng="hu">emberf&eacute;l&eacute;k</trd>
  <trd lng="it">ominidi</trd>
  <trd lng="nl">mensen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pl">
    <trd>hominidy</trd>,
    <trd>cz&lstroke;owiekowate</trd>
  </trdgrp>
  <trd lng="pt">homin&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_i;&c_n;&c_i;&c_d;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>hominidi</trd>,
    <trd>&lcaron;udia a opice</trd>
  </trdgrp>
</drv>
</art>
