  <trd lng="de">Menschen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hominid&eacute;s</trd>
  <trd lng="hu">emberf&eacute;l&eacute;k</trd>
  <trd lng="nl">mensen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">homin&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_i;&c_n;&c_i;&c_d;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
