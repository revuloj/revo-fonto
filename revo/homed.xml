<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>homed</rad>/oj
</kap>
<drv mrk="homed.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="homed.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="simi.0oj">simioj</ref>,
      el kiu apartenis nur specio de la genro homo (<trd
      lng="la">Hominidae</trd>), sed nun enhavas anka&ubreve;
      <ref tip="vid" cel="orangu.0o">orangutangojn</ref>
      (orangutangenoj),
      <sup>2</sup><ref
      tip="vid" cel="goril.0oj">gorilojn</ref>, <sup>9</sup><ref
      tip="vid" cel="cximpa.0oj">&ccirc;impanzojn</ref> kaj <ref
      tip="vid" cel="bonob.0oj">bonobojn</ref> (homenoj, kiel homo).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_ib;&c_n;&c_ib;&c_d;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>hominidi</trd>,
    <trd>lid&eacute; a opolid&eacute;</trd>
  </trdgrp>
  <trd lng="de">Menschen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="es">Hom&iacute;nidos</trd>
  <trd lng="fr">hominid&eacute;s</trd>
  <trd lng="hu">emberf&eacute;l&eacute;k</trd>
  <trd lng="it">ominidi</trd>
  <trd lng="nl">mensen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pl">
    <trd>hominidy</trd>,
    <trd>cz&lstroke;owiekowate</trd>
  </trdgrp>
  <trd lng="pt">homin&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_i;&c_n;&c_i;&c_d;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>hominidi</trd>,
    <trd>&lcaron;udia a opice</trd>
  </trdgrp>
</drv>
</art>
