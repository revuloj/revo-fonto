<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>homed</rad>/oj
</kap>
<drv mrk="homed.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="homed.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Familio el la ordo <ref tip="super" cel="simi.0oj">simioj</ref>,
      el kiu plu vivas nur specio de la genro homo (<trd
      lng="la">Hominidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_ib;&c_n;&c_ib;&c_d;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="es">Hom&iacute;nidos</trd>
  <trd lng="de">Menschen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hominid&eacute;s</trd>
  <trd lng="hu">emberf&eacute;l&eacute;k</trd>
  <trd lng="it">ominidi</trd>
  <trd lng="nl">mensen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pl">
    <trd>hominidy</trd>,
    <trd>cz&lstroke;owiekowate</trd>
  </trdgrp>
  <trd lng="pt">homin&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_i;&c_n;&c_i;&c_d;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
