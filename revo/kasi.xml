<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kasi.xml,v 1.10 2023/12/03 08:00:24 revo Exp $">
<kap>
  <rad>kasi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kasi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kasi.0o.specio">
    <dif>
       <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> de <ref tip="super" cel="cinam.0ujo">cinamujo</ref> (<trd lng="la">Cinnamomum
       cassia</trd>).
    </dif>
    <trd lng="de">Zimtkassie</trd>
    <trd lng="pl">cynamon wonny</trd>
    <trd lng="zh">&#x6842;&#x76AE; <pr>gu&#xEC;p&#xED;</pr></trd>
</snc>
  <snc mrk="kasi.0o.PAR">
    <dif>
      Bonodora&jcirc;o el la &scirc;elo de <ref tip="vid" cel="kasi.0o.specio">kasio
      <sncref/></ref>, similgusta al cinamo, sed enhavanta pli da venena
      kumarino:
      <ekz>
        de mirho, aloo, kaj <tld/>o odoras &ccirc;iuj viaj vestoj
        <fnt><bib>MT</bib>, <lok>&Psa; 45:8</lok></fnt>;
      </ekz>
      <ekz>
        la <klr>...</klr> ha&ubreve;to estis frotadita per miksa&jcirc;o de 
        cedro-oleo, abelvakso, kumino kaj gumo kaj poste pudrita per 
        mirho, <tld/>o kaj aliaj bonodoraj substancoj
        <fnt>
          <aut>B. Golden</aut>,
          <vrk><url ref="http://www.literaturo.dk/profanado.htm">
          La Profanado</url></vrk>,
          <lok>Literatura Foiro, 1978-02 (47)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Cassia-Zimt</trd>
  </snc>
  <snc mrk="kasi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto el tropikaj landoj,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="fabac.0oj.BOT">fabacoj</ref> (<trd lng="la">Cassia</trd>).
    </dif>
    <trd lng="de">Kassie</trd>
    <trd lng="pl">cynamon</trd>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_je; <ind>&c_k;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;</ind> &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="en">cassia<klr> (legume)</klr></trd>
  <trd lng="fr">casse<klr> (plante)</klr></trd>
  <trd lng="it">cassia<klr> (bot.)</klr></trd>
  <trd lng="uk">&#x43A;&c_a;&c_s;&c_ib;&#x44F;</trd>
</drv>
</art>

