<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: persik.xml,v 1.32 2021/05/28 12:53:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>persik</rad>/o
</kap>

<drv mrk="persik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Bongusta <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <ref tip="malprt" cel="persik.0ujo0arbo"><tld/>ujo</ref>,
      <ref tip="super" cel="drup.0o">drupo</ref>:
      <ekz>
        en la mezo inter la folioj kronis la fruktan piramidon ananaso,
        <klr>[&#x2026;]</klr> &ccirc;irka&ubreve;is &gcirc;in rozaj <tld/>oj, flavaj abrikotoj, verdaj renklodoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi vizitis kun la princo la <tld/>ejon, ananasejon, varmigejon, kiuj estas plenaj de bonaj fruktoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la somero ni havas fragojn kaj <tld/>ojn, kiam ili estas maturaj kaj bonaj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        suka karno de <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas kiel matura <tld/>o por la okuloj de l' homo, kies langon sekigis la soifo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/persik.jpg">
    bran&ccirc;o kun <tld/>o
  </bld>
  <trd lng="be">&c_p;&c_e;&c_r;&c_s;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_s;&c_k;&c_o;&c_v;&c_a;</trd>
  <trd lng="bo">&#xF41;&#xF58;&#xF0B;&#xF56;&#xF74;&#xF0B;</trd>
  <trd lng="br">pechezenn<klr tip="ind"> (frouezhenn)</klr></trd>
  <trdgrp lng="de">
    <trd>Pfirsich <klr>(laneca)</klr></trd>,
    <trd>Nektarine <klr>(glata)[fakte mikso inter persiko kaj
    pruno]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>peach <klr>(laneca)</klr></trd>,
    <trd>nectarine <klr>(glata)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>melocot&oacute;n <klr>(laneca)</klr></trd>
    <trd>nectarina <klr>(glata)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>p&ecirc;che <klr>(laneca)</klr></trd>
    <trd>nectarine <klr>(glata)</klr></trd>
  </trdgrp>
  <trd lng="hu">&odblac;szibarack</trd>
  <trd lng="nl">perzik</trd>
  <trd lng="pl">brzoskwinia</trd>
  <trd lng="pt">p&ecirc;ssego</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_s;&c_i;&c_k;</trd>
  <trd lng="sv">persika</trd>
  <trd lng="tr">&scedil;eftali</trd>
  <trd lng="sk">brosky&ncaron;a <klr>(ovocie)</klr></trd>
  <trd lng="cs">broskev <klr>(plod)</klr></trd>
</drv>

<drv mrk="persik.0ujo0arbo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus persica</trd>), devena de Persujo:
      <ekz>
        &scirc;i el&scirc;utis veran flora&jcirc;an ne&gcirc;on sur la pomarbojn kaj <tld/>arbojn, tiel ke ili staris en plena parado
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        feli&ccirc;o en &gcirc;ardeno de florantaj <tld/>ujoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1972">Julius Zeyer</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam iu rearan&gcirc;as sian &ccirc;apelon sub la <tld/>arbo, oni emas <klr>[suspekti]</klr> tiun pri &scirc;telado de <tld/>o supre pendanta
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2006/009053.php">Prezidanto de la japana centra banko profitis</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_s;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_je;
&c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-bechez</trd>
    <trd>pechezenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trd lng="cs">broskvo&ncaron;</trd>
  <trd lng="de">Pfirsichbaum</trd>
  <trd lng="es">melocotonero</trd>
  <trd lng="fr">p&ecirc;cher</trd>
  <trd lng="hu">&odblac;szibarackfa</trd>
  <trd lng="nl">perzikboom</trd>
  <trd lng="pl">brzoskwinia</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_s;&c_i;&c_k;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">brosky&ncaron;a <klr>(strom)</klr></trd>
  <trd lng="sv">persikotr&auml;d</trd>
  <trd lng="tr">&scedil;eftali</trd>
</drv>
</art>
