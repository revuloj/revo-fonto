<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>persik</rad>/o
</kap>
<drv mrk="persik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bongusta frukto de <tld/>ujo. 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pfirsich <klr>(laneca)</klr></trd>
    <trd>Nektarine <klr>(glata)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="persik.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus persica</trd>), devena de Persujo.  
    </dif>
  </snc>
  <trd lng="de">Pfirsichbaum</trd>
</drv>
</art>
