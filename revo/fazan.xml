<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fazan.xml,v 1.23 2023/11/09 19:13:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fazan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fazan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fazan.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="super" cel="bird.0o">birdo</ref> kun longaj vostplumoj kaj belkolora plumaro
      &ccirc;e la viro; <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
      el familio <ref tip="malprt" cel="fazane.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phasianus</trd>):
      <ekz>
        ili ekvidis la <nom>minaul</nom>&#x2019;on, himalajan <tld/>on, kies belegaj koloroj brilis anta&ubreve;
        la statuo
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili havas en siaj birdejoj <tld/>ojn, sed ne man&gcirc;as ilin, akceptinte la
        principon, ke forman&gcirc;o de &ccirc;iu <tld/>o proksimigas la finon de la roma potenco
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni konstruis luksan hotelon, basenon, faris belan parkon kaj
        denove oni komencis bredi <tld/>ojn por &ccirc;asado
        <fnt>
          <aut>J. Modest</aut>,
          <vrk><url ref="http://verkoj.com/verkistoj/julian-modest/fazancasado/">
          Fazan&ccirc;asado</url></vrk>,
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/78/Common_Pheasant_RWD2.jpg/640px-Common_Pheasant_RWD2.jpg" prm="CC BY-SA 3.0"><tld/>o (Phasianus colchicus), Skotujo
        <fnt>
          <aut>DickDaniels (http://theworldbirds.org/), propra verko</aut>
          <lok><url ref="&WCW;/File:Common_Pheasant_RWD2.jpg">&WCW;/File:Common_Pheasant_RWD2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_z;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>ba&zcaron;ant</trd>,
    <trd>ba&zcaron;ant kolchick&yacute;</trd>,
    <trd>ba&zcaron;ant obecn&yacute;</trd>
  </trdgrp>
  <trd lng="de">Fasan</trd>
  <trd lng="en">pheasant</trd>
  <trd lng="fr">faisan</trd>
  <trd lng="hu">f&aacute;c&aacute;n</trd>
  <trd lng="ja">&#x30AD;&#x30B8;  <pr>&#x304D;&#x3058;</pr></trd>
  <trd lng="nl">fazant</trd>
  <trd lng="pl">ba&zdot;ant</trd>
  <trd lng="pt">fais&atilde;o</trd>
  <trd lng="ru">&c_f;&c_a;&c_z;&c_a;&c_n;</trd>
  <trd lng="sk">ba&zcaron;ant</trd>
  <trd lng="uk">&c_f;&c_a;&c_z;&c_a;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D6B; <pr>zh&#xE0;o</pr></trd>,
    <trd>&#x7FDA; <pr>hu&#x12B;</pr></trd>,
    <trd>&#x9D57; <pr>x&#x12B;</pr></trd>,
    <trd>&#x96C9;&#x96DE; <pr>zh&#xEC;j&#x12B;</pr></trd>
  </trdgrp>
</drv>

</art>
