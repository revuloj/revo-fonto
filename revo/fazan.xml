<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fazan</rad>/o
</kap>
<drv mrk="fazan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fazan.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Phasianus</trd>),
      granda birdo kun longaj vostplumoj kaj belkolora plumaro
      &ccirc;e la viro.
    </dif>
  </snc>
  <trd lng="cs">ba&zcaron;ant</trd>
  <trd lng="de">Fasan</trd>
  <trd lng="en">pheasant</trd>
  <trd lng="fr">faisan</trd>
  <trd lng="hu">f&aacute;c&aacute;n</trd>
  <trd lng="pl">ba&zdot;ant</trd>
  <trd lng="ru">&c_f;&c_a;&c_z;&c_a;&c_n;</trd>
</drv>
<drv mrk="fazan.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
   <dif>
     Familio el la ordo
     <ref tip="super" cel="kok.0oformaj">kokoformaj birdoj</ref>
     (<trd lng="la">Phasianidae</trd>).
   </dif>
  </snc>
  <trd lng="de">Fasananartige <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
