<art mrk="$Id: ekolog.xml,v 1.16 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekolog.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="ekologi.0isto.EKOL">Ekologiisto<sncref/></ref>:
      <ekz>
        &ccirc;inaj biologoj kaj <tld/>oj faris longatempan sciencan 
        esploradon en tiu loko kaj la proksima maro
        <fnt>
          <aut>-</aut>,
          <vrk>
<url ref="http://www.china.org.cn/world/shi-lvyou/chinaj_urboj/12.htm">
          Mar&ccirc;o &ccirc;e &Scirc;anhajo</url></vrk>,
          <lok>2002-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis <tld/>o kaj opiniis, ke la ekstera&jcirc;o de urboj <klr>[...]</klr> 
        devas esti &scirc;an&gcirc;ita, por ke ties lo&gcirc;antoj denove &gcirc;uu la naturharmonion
        <fnt>
          
          <aut>Evgeni Georgiev</aut>
          <vrk>A&ubreve;strio: Orfi&gcirc;inta
          beleco</vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="ca">
    <trd>ec&ograve;leg</trd>,
    <trd>ecologista</trd>
  </trdgrp>
  <trd lng="cs">ekolog</trd>
  <trd lng="de">&Ouml;kologe</trd>
  <trd lng="fr">&eacute;cologue</trd>
  <trd lng="ja">&#x751F;&#x614B;&#x5B66;&#x8005;  <pr>&#x305B;&#x3044;&#x305F;&#x3044;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">ecologist</trd>
  <trd lng="pl">ekolog</trd>
  <trdgrp lng="pt">
    <trd>ec&oacute;logo</trd>,
    <trd>ecologista</trd>
  </trdgrp>
  <trd lng="sk">ekol&oacute;g</trd>
  <trd lng="uk">&c_je;&#x43A;&#x43E;&#x43B;&#x43E;&c_g;</trd>
  <trd lng="zh">&#x751F;&#x6001;&#x5B66;&#x5BB6; <pr>sh&#x113;ngt&#xE0;ixu&#xE9;ji&#x101;</pr></trd>
</drv>
