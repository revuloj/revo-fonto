<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ma&ccirc;</rad>/i
</kap>
<drv mrk="macx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pisti en bu&scirc;o per la dentoj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia brako estis for<tld/>ita de la dentumitaj radoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="muel.0i">mueli</ref>,
      <ref cel="prem.dis0i">dispremi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">kauen</trd>
  <trdgrp lng="en">
    <trd>chew</trd>,
    <trd>masticate</trd>
  </trdgrp>
  <trd lng="nl">kauwen</trd>
  <trd lng="ru">&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">tugga</trd>
</drv>

<drv mrk="macx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Denove <tld/>i.
    </dif>
    <trd lng="de">nochmals kauen</trd>
  </snc>
  <snc>
    <dif>
      Revenigi nutra&jcirc;on el la stomako en la bu&scirc;on por denove <tld/>i &gcirc;in
      (pp bestoj, kiel bovo, kamelo).
    </dif>
    <trd lng="de">wiederk&auml;uen</trd>
    <trdgrp lng="en">
      <trd>chew the cud</trd>,
      <trd>ruminate</trd>,
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ripete pripensi:
      <ekz>
        konstante re<tld/>i projekton, reakciajn ideojn.
      </ekz>
    </dif>
    <trd lng="de">hin und her &uuml;berlegen</trd>
    <trdgrp lng="en">
      <trd>ruminate</trd>,
      <trd>reflect on</trd>,
      <trd>ponder</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">herkauwen</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_zh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="macx.krak0i">
  <kap>krak<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Rompi per la dentoj kun krakbruo.
    </dif>
  </snc>
</drv>
</art>
