<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: macx.xml,v 1.40 2024/04/07 10:53:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ma&ccirc;</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="macx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="macx.0i.ANA">
    <dif>
      <ref tip="super" cel="pist.0i">Pisti</ref> en bu&scirc;o per la dentoj:
      <ekz>
        mi ne fumas cigarojn, anka&ubreve; ne <tld/>as tabakon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        servisto sidis apud la patro kaj <tld/>is sekvinberojn
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li man&gcirc;as rapide kaj <tld/>as per la du flankoj de la bu&scirc;o
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> elkra&ccirc;is for<tld/>itan sem&scirc;elon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        li prenis la duone de<tld/>itan
        <klr>(kp <ref cel="mord.cxirkaux0i">&ccirc;irka&ubreve;mordi</ref>)</klr> viandoston
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>;
      </ekz>
      <ekz>
        la oficiro furioze kaj senpove <tld/>is la finon de sia cigaredo
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ia vermo <tld/>is iliajn nervojn
        <fnt>
          <bib>VaK</bib>
          <lok>Deksepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ricevis viandon, sed &scirc;i ne povis <tld/>i &gcirc;in, nur elsu&ccirc;i la sukon
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; li volus <tld/>i la propran langon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 48</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu amas okula&ccirc;i, ne havas kion <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prem.dis0i">dispremi</ref>,
      <ref cel="mord.0i">mordi</ref>,
      <ref cel="muel.0i">mueli</ref>
    </refgrp>
  </snc>
  <snc mrk="macx.0i.FIG">
    <dif>
      Tro <ref tip="super" cel="diskut.0i">diskuti</ref>,
      <ref tip="super" cel="trakt.pri0i">pritrakti</ref>:
      <ekz>
        &ccirc;iuj <tld/>is la malnovan temon, &ccirc;u konstrui, a&ubreve; ne konstrui la metropolitenon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi jam <klr>[&#8230;]</klr> vidis trifoje en via landa televidstacio, <klr>[&#8230;]</klr>
        kio estas anta&ubreve;<tld/>ita de la mondaj nova&jcirc;agentejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2011/010343.php">Indaj informoj</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="muel.vort0i">vortmueli</ref>
    </refgrp>
  <trd lng="de"><klr>(auf etwas) </klr>herum<ind>kauen</ind></trd>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_malmol;&c_v;&c_ch;&c_a;</trd>
  <trd lng="br">chaokat</trd>
  <trd lng="ca">mastegar</trd>
  <trd lng="cs">&zcaron;v&yacute;kat</trd>
  <trd lng="de">kauen</trd>
  <trdgrp lng="en">
    <trd>chew</trd>,
    <trd>masticate</trd>
  </trdgrp>
  <trd lng="es">masticar</trd>
  <trd lng="fr">m&acirc;cher</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;g</trd>,
    <trd>megr&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x307F;&#30741;&#x304F;  <pr>&#x304B;&#x307F;&#x304F;&#12384;&#x304F;</pr></trd>,
    <trd>&#x304B;&#12416;</trd>
  </trdgrp>
  <trd lng="nl">kauwen</trd>
  <trd lng="pl">&zdot;u&cacute;</trd>
   <trdgrp lng="pt">
    <trd>mastigar</trd>,
    <trd>mascar</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">tugga</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_zh;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_zh;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21632; <pr>j&#x1D4;</pr></trd>,
    <trd>&#21632;&#x56BC; <pr>j&#x1D4;ju&#xE9;</pr></trd>,
    <trd>&#x56BC; <pr>ji&#xE1;o</pr></trd>,
    <trd>&#x56BC; <pr>ju&#xE9;</pr></trd>,
    <trd>&#x564D; <pr>ji&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="macx.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="macx.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>ado:
      <ekz>
        <tld/>, <tld/>, <tld/>, sonis la makzeloj
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li venis
        trakosme?</vrk>, <lok>1980</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">miam</trd>
</drv>

<drv mrk="macx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="macx.re0i.ANA">
    <dif>
      Denove <tld/>i:
      <ekz>
        fosanta serpento, kiu re<tld/>as sian man&gcirc;on kaj reman&gcirc;as sian propran
        ekskrementon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_ja;</trd>
    <trd lng="cs">p&rcaron;e&zcaron;vykovat</trd>
    <trd lng="de">nochmals <ind>kauen</ind></trd>
    <trd lng="hu">&uacute;jrar&aacute;g</trd>
  </snc>
  <snc mrk="macx.re0i.ZOO">
    <dif>
      Revenigi nutra&jcirc;on el la stomako en la bu&scirc;on por denove
      <tld/>i &gcirc;in, kiel faras iuj bestoj, ekzemple bovo, kamelo:
      <ekz>
        re<tld/>antaj <tld/>ita&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_ja;</trd>
    <trd lng="br">daskiriat</trd>
    <trdgrp lng="ca">
      <trd>remugar</trd>,
      <trd>ruminar</trd>
    </trdgrp>
    <trd lng="de">wiederk&auml;uen</trd>
    <trdgrp lng="en">
      <trd>chew the cud</trd>,
      <trd>ruminate</trd>,
    </trdgrp>
    <trd lng="es">rumiar</trd>
    <trd lng="hu">k&eacute;r&odblac;dzik</trd>
  </snc>
  <snc mrk="macx.re0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ripete diskuti, <ref tip="super" cel="trakt.pri0i">pritrakti</ref>:
      <ekz>
        konstante re<tld/>i projekton, reakciajn ideojn;
      </ekz>
      <ekz>
        sendecide ili iris tien kaj reen <klr>[&#8230;]</klr>, grumblis kaj blasfemis kaj
        &ccirc;iutage centfoje re<tld/>is la samajn frazojn pri senlaboreco kaj mallumaj
        perspektivoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        britoj <klr>[&#8230;]</klr> pretas da&ubreve;re re<tld/>i <klr>[&#8230;]</klr> monstra&jcirc;ojn de
        germanoj dum la dua mondomilito
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2004/008252.html">Opinio-(s)ondado</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la japana armeo re<tld/>is diversajn planojn por tran&ccirc;i la &scirc;oseon, la solan
        trairejon
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kaj re<tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxeval.0eto.FIG">&ccirc;evaleto<sncref/></ref>,
      <ref cel="muel.0i.FIG_ripeti">mueli<sncref/></ref>
    </refgrp>
    <trdgrp lng="bg">
        <trd>&c_p;&c_r;&c_je;&c_d;&c_malmol;&c_v;&c_k;&c_v;&c_a;&c_m;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_p;&c_o;&c_v;&c_t;&c_a;&c_r;&c_ja;&c_m;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_ja;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><klr>(auf etwas) </klr>herum<ind>kauen</ind></trd>,
      <trd><klr>(etwas) </klr>wiederk&auml;uen</trd>,
      <trd><klr>(auf etwas) </klr>herum<ind>reiten</ind></trd>,
      <trd>hin und her <ind>&uuml;berlegen</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ruminate</trd>,
      <trd>reflect on</trd>,
      <trd>ponder</trd>
    </trdgrp>
    <trd lng="zh">&#x73A9;&#21619; <pr>w&#xE1;nw&#xE8;i</pr></trd>
</snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_zh;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>rem&acirc;cher</trd>,
    <trd>ruminer</trd>
  </trdgrp>
  <trd lng="hu">megr&aacute;g <klr tip="amb">(megfontol)</klr></trd>
  <trd lng="ja">&#x53CD;&#x82BB;&#12377;&#x308B;  <pr>&#x306F;&#12435;&#12377;&#12358;&#12377;&#x308B;</pr></trd>
  <trd lng="nl">herkauwen</trd>
  <trd lng="pl">prze&zdot;uwa&cacute;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_zh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="zh">&#20498;&#x56BC; <pr>d&#x1CE;oji&#xE0;o</pr></trd>
</drv>

<drv mrk="macx.re0uloj">
  <kap>re<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="macx.re0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_grupoj">Subordo</ref> de <ref tip="malprt"
      cel="huf.par0uloj">parhufuloj</ref>
      (&scirc;afoj, &gcirc;irafoj, cervedoj, bovedoj, kameloj, k.a.)
      kiuj re<tld/>as kaj havas plurpartan stomakon, plejofte kun kvar
      kameroj (<trd lng="la">Ruminantia</trd>):
      <ekz>
        <nom>Volter</nom> aludas al la <ref tip="prt" cel="alpak.0o.besto">alpako</ref>,
        al la <ref tip="prt" cel="lama.0o">lamao</ref>,
        al la vikuno, kaj aliaj re<tld/>uloj kun flavru&gcirc;aj haroj el Sud-Ameriko
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_j;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_y;)</klr></trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_n;&c_i; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_i;</trd>
  <trd lng="ca">remugants</trd>
  <trd lng="cs">p&rcaron;e&zcaron;v&yacute;kavci</trd>
  <trd lng="de">Wiederk&auml;uer</trd>
  <trd lng="en">ruminants</trd>
  <trd lng="es">rumiantes</trd>
  <trd lng="fr">ruminants</trd>
  <trd lng="ja">&#x53CD;&#x82BB;&#x52D5;&#29289;  <pr>&#x306F;&#12435;&#12377;&#12358;&#12393;&#12358;&#12406;&#12388;</pr></trd>
  <trd lng="nl">herkauwers</trd>
  <trdgrp lng="zh">
    <trd>&#x53CD;&#x520D;&#x4E9A;&#x76EE; <pr>f&#x1CE;nch&#xFA;y&#xE0;m&#xF9;</pr></trd>,
    <trd>&#x53CD;&#x82BB;&#x4E9E;&#x76EE; <pr>f&#x1CE;nch&#xFA;y&#xE0;m&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="macx.krak0i">
  <kap>krak<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="macx.krak0i.ANA">
    <dif>
      Rompi per la dentoj kun krakbruo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_r;&c_u;&c_p;&c_a;&c_c;&c_mol; <klr>(&c_zh;&c_u;&c_ju;&c_ch;&c_y;, &c_g;&c_r;&c_y;&c_z;&c_u;&c_ch;&c_y;)</klr></trd>,
    <trd>&c_h;&c_r;&c_a;&c_b;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol; <klr>(&c_zh;&c_u;&c_ju;&c_ch;&c_y;, &c_g;&c_r;&c_y;&c_z;&c_u;&c_ch;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="ca">rompre amb les dents</trd>
  <trd lng="de">zerbei&szlig;en</trd>
  <trd lng="fr">croquer</trd>
  <trd lng="hu">ropogtat <klr tip="amb">(&eacute;telt)</klr></trd>
  <trd lng="ja">&#12400;&#x308A;&#12400;&#x308A;&#x304B;&#x307F;&#30741;&#x304F; 
  <pr>&#12400;&#x308A;&#12400;&#x308A;&#x304B;&#x307F;&#x304F;&#12384;&#x304F;</pr></trd>
  <trd lng="nl">stukkauwen</trd>
  <trd lng="pl">chrupa&cacute;<klr> gry&zacute;&cacute;</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_g;&c_r;&c_y;&c_z;&c_a;&c_t;&c_mol; &c_s; &c_h;&c_r;&c_u;&c_s;&c_t;&c_o;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_i;&c_z;&c_t;&c_i;</trd>,
    <trd>&c_ji;&c_s;&c_t;&c_i; &c_z; &c_h;&c_r;&c_u;&c_s;&c_k;&c_o;&c_m;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_m;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_m;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_m;&c_t;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
