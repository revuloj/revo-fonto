<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>dosier</rad>/o
    </kap>

    <drv mrk="dosier.0o">
      <kap>
        <tld/>o
      </kap>
      <snc>
        <dif>
          Kolekto de aktoj, dokumentoj, notoj ks, rilataj al unu afero
          a&ubreve;
          unu persono, kaj ordinare entenataj en unu kovertego.
        </dif>
        <trdgrp lng="be">
          <trd>&c_d;&c_a;&c_s;&c_mol;&c_je;</trd>,
          <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="de">
           <trd>Aktenb&uuml;ndel</trd>,
           <trd>Aktensammlung</trd>,
           <trd>Akten</trd>,
           <trd>Dossier</trd>,
           <trd>gesammelte schriftliche Unterlagen</trd>
        </trdgrp>
        <trd lng="es">archivo</trd>
        <trdgrp lng="hu">
          <trd>&uuml;gyirat</trd>,
          <trd>dosszi&eacute;</trd>
        </trdgrp>
        <trd lng="fr">dossier<klr tip="ind"> (collection de
        documents)</klr></trd>
        <trd lng="nl">dossier</trd>
        <trdgrp lng="pl">
          <trd>dossier</trd>,
          <trd>akta</trd>,
          <trd>teczka<klr> (pot.)</klr></trd>,
          <trd>papiery<klr> (pot.)</klr></trd>
        </trdgrp>
        <trd lng="pt">dossi&ecirc;</trd>
        <trdgrp lng="ru">
          <trd>&c_d;&c_o;&c_s;&c_mol;&c_je;</trd>,
          <trd>&c_d;&c_je;&c_l;&c_o;</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          &Ccirc;e la polico, aparta registro de ies anta&ubreve;aj
          agoj, kondamnoj ktp.
        </dif>
        <trd lng="be">&c_d;&c_a;&c_s;&c_mol;&c_je;</trd>
        <trdgrp lng="de">
           <trd>Strafregister</trd>,
           <trd>Akten</trd>
        </trdgrp>
        <trd lng="es">expediente</trd>
        <trdgrp lng="fr">
        <trd>casier<klr tip="ind"> (judiciaire)</klr></trd>,
        <trd>dossier<klr tip="ind"> (de police)</klr></trd></trdgrp>
        <trdgrp lng="hu">
          <trd>rend&odblac;rs&eacute;gi akta</trd>,
          <trd>rend&odblac;rs&eacute;gi
          nyilv&aacute;ntart&aacute;s</trd>
        </trdgrp>
        <trd lng="nl">dossier</trd>
        <trdgrp lng="pl">
          <trd>akta</trd>,
          <trd>teczka<klr> (pot.)</klr></trd>,
          <trd>papiery<klr> (pot.)</klr></trd>
        </trdgrp>
        <trd lng="pt">ficha</trd>
        <trd lng="ru">&c_d;&c_o;&c_s;&c_mol;&c_je;</trd>
      </snc>
      <snc mrk="dosier.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Organizita kolekto da datumoj en ekstera memoro,
          &gcirc;enerale referencata 
          per &gcirc;iaj nomo kaj pozicio en <tld/>uja strukturo.
        </dif>
        <trd lng="be">&c_f;&c_a;&c_j;&c_l;</trd>
        <trd lng="bg">&c_f;&c_a;&c_j;&c_l;</trd>
        <trd lng="de">Datei</trd>
        <trd lng="nl">bestand<klr tip="ind"> (comp.)</klr></trd>
        <trd lng="en">file</trd>
        <trd lng="es">archivo</trd>
        <trd lng="fr">fichier<klr tip="ind"> (inf.)</klr></trd>
        <trdgrp lng="hu">
          <trd>f&aacute;jl</trd>,
          <trd>&aacute;llom&aacute;ny</trd>
        </trdgrp>
        <trd lng="pl">plik<klr> (danych)</klr></trd>
        <trd lng="pt">arquivo</trd>
        <trd lng="ru">&c_f;&c_a;&c_j;&c_l;</trd>
        <trd lng="tr">dosya</trd>
      </snc>
    </drv>

    <drv mrk="dosier.uj0o">
      <kap><tld/>ujo</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Organizita kolekto da <tld/>oj, &gcirc;enerale referencata per
          &gcirc;iaj 
          nomo kaj pozicio en <tld/>uja strukturo.
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;</trd>,
          <trd>&c_d;&c_y;&c_r;&c_e;&c_k;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>,
          <trd>&c_p;&c_a;&c_p;&c_k;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Verzeichnis</trd>,
          <trd>Ordner</trd>
        </trdgrp>
        <trd lng="es">directorio</trd>
        <trdgrp lng="nl">
          <trd>map</trd>,
          <trd>bestandenlijst</trd>,
          <trd>directory</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>folder</trd>,
          <trd>directory</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>r&eacute;pertoire<klr tip="ind"> (inf.)</klr></trd>,
          <trd>classeur<klr tip="ind"> (inf.)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>mappa</trd>,
          <trd>k&ouml;nyvt&aacute;r</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>katalog</trd>,
          <trd>folder</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>pasta</trd>,
          <trd>diret&oacute;rio</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>,
          <trd>&c_d;&c_i;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>,
          <trd>&c_f;&c_o;&c_l;&c_d;&c_je;&c_r;</trd>,
          <trd>&c_p;&c_a;&c_p;&c_k;&c_a;</trd>
        </trdgrp>
        <trd lng="tr">dizin</trd>
      </snc>
    </drv>

    <drv mrk="dosier.sub0ujo">
      <kap>sub<tld/>ujo</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <tld lit="D"/>ujo trovi&gcirc;anta en alia <tld/>ujo.
        </dif>
        <trdgrp lng="be">
          <trd>&c_p;&c_a;&c_d;&c_k;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;</trd>,
          <trd>&c_p;&c_a;&c_d;&c_d;&c_y;&c_r;&c_e;&c_k;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Unterverzeichnis</trd>,
          <trd>Unterordner</trd>
        </trdgrp>
        <trd lng="es">subdirectorio</trd>
        <trd lng="hu">alk&ouml;nyvt&aacute;r</trd>
        <trdgrp lng="nl">
          <trd>onderliggende <ind>map</ind></trd>,
          <trd>subdirectory</trd>
        </trdgrp>
        <trd lng="fr">sous-r&eacute;pertoire<klr tip="ind">
        (inf.)</klr></trd>
        <trd lng="en">subdirectory</trd>
        <trdgrp lng="pl">
          <trd>podkatalog</trd>,
          <trd>podfolder</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_o;&c_d;&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>,
          <trd>&c_p;&c_o;&c_d;&c_d;&c_i;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="dosier.0sistemo">
      <kap><tld/>sistemo</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          La tuto de la <tld/>oj, la <tld/>ujoj kaj administraj
          kromdatumoj registritaj sur
          <ref cel="datum.0portilo">datumportilo</ref>
          (disko, subdisko ktp):
          <ekz>
            <ind>surmeti <tld/>sistemon</ind> en la hejman <tld/>ujon;
            <trd lng="en">mount a file system</trd>
            <trd lng="hu">&aacute;llom&aacute;nyrendszert
            telep&iacute;t</trd>
            <trd lng="pl">domontowa&cacute; system<klr>
            plik&oacute;w</klr></trd>
          </ekz><ekz>
            <ind>demeti <tld/>sistemon</ind> anta&ubreve; ol depreni
            ties disketon.
            <trd lng="en">unmount a file system</trd>
            <trd lng="hu">&aacute;llom&aacute;nyrendszert
            elt&aacute;vol&iacute;t</trd>
            <trd lng="pl">odmontowa&cacute; system<klr>
            plik&oacute;w</klr></trd>
          </ekz>
        </dif>
      <trd lng="be">&c_f;&c_a;&c_j;&c_l;&c_a;&c_v;&c_a;&c_ja;
      &c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;</trd>
      <trd lng="en">file system</trd>
      <trd lng="hu">&aacute;llom&aacute;nyrendszer</trd>
      <trd lng="nl">bestandssysteem</trd>
      <trd lng="pl">system <ind>plik&oacute;w</ind></trd>
      <trd lng="ru">&c_f;&c_a;&c_j;&c_l;&c_o;&c_v;&c_a;&c_ja;
      &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;</trd>
      </snc>
    </drv>

  </art>

