<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <rad>dosier</rad>/o
</kap>
<drv mrk="dosier.0o">
 <kap>
  <tld/>o
 </kap>
 <snc num="1">
  <dif>
    Kolekto de aktoj, dokumentoj, notoj ks, rilataj al unu afero a&ubreve;
    unu persono, k ordinare entenataj en unu kovertego.
  </dif>
  <trd lng="fr">dossier<klr tip="ind"> (collection de documents)</klr></trd>
  <trd lng="nl">dossier</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_mol;&c_je;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_o;</trd>
  </trdgrp>
 </snc>
 <snc num="2">
  <dif>
    &Ccirc;e la polico, aparta registro de ies anta&ubreve;aj agoj, kondamnoj ktp.
  </dif>
  <trdgrp lng="fr">
    <trd>casier<klr tip="ind"> (judiciaire)</klr></trd>,
    <trd>dossier<klr tip="ind"> (de police)</klr></trd></trdgrp> 
  <trd lng="nl">dossier</trd>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_mol;&c_je;</trd>
 </snc>
 <snc num="3">
  <uzo tip="fak">KOMP</uzo>
  <dif>
   Organizita kolekto da datumoj en ekstera memoro, &gcirc;enerale referencata 
   per &gcirc;iaj nomo kaj pozicio en <tld/>uja strukturo.
  </dif>
  <trd lng="de">Datei</trd>
  <trd lng="nl">bestand<klr tip="ind"> (comp.)</klr></trd>
  <trd lng="en">file</trd>
  <trd lng="fr">fichier<klr tip="ind"> (inf.)</klr></trd>
  <trd lng="ru">&c_f;&c_a;&c_j;&c_l;</trd>
  <trd lng="tr">dosya</trd>
 </snc>
</drv>
<drv mrk="dosier.uj0o">
 <kap><tld/>ujo</kap>
 <snc>
  <uzo tip="fak">KOMP</uzo>
  <dif>
   Organizita kolekto da <tld/>oj, &gcirc;enerale referencata per &gcirc;iaj 
   nomo kaj pozicio en <tld/>uja strukturo.
  </dif>
 </snc>
 <trdgrp lng="de">
   <trd>Verzeichnis</trd>,
   <trd>Ordner</trd>
 </trdgrp>
 <trdgrp lng="nl">
   <trd>map</trd>,
   <trd>bestandenlijst</trd>,
   <trd>directory</trd>
 </trdgrp>
 <trdgrp lng="en">
   <trd>folder</trd>,
   <trd>directory</trd>
 </trdgrp>
 <trdgrp lng="fr">
   <trd>r&eacute;pertoire<klr tip="ind"> (inf.)</klr></trd>,
   <trd>classeur<klr tip="ind"> (inf.)</klr></trd>
 </trdgrp>
 <trdgrp lng="ru">
   <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>,
   <trd>&c_d;&c_i;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>,
   <trd>&c_f;&c_o;&c_l;&c_d;&c_je;&c_r;</trd>,
   <trd>&c_p;&c_a;&c_p;&c_k;&c_a;</trd>
 </trdgrp>
 <trd lng="tr">dizin</trd>
</drv>
<drv mrk="dosier.sub0ujo">
 <kap>sub<tld/>ujo</kap>
 <snc>
  <uzo tip="fak">KOMP</uzo>
  <dif>
   <tld lit="D"/>ujo trovi&gcirc;anta en alia <tld/>ujo.
  </dif>
 </snc>
 <trdgrp lng="de">
   <trd>Unterverzeichnis</trd>,
   <trd>Unterordner</trd>
 </trdgrp>
 <trd lng="nl">subdirectory</trd>
 <trd lng="fr">sous-r&eacute;pertoire<klr tip="ind"> (inf.)</klr></trd>
 <trd lng="en">subdirectory</trd>
 <trdgrp lng="ru">
   <trd>&c_p;&c_o;&c_d;&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>,
   <trd>&c_p;&c_o;&c_d;&c_d;&c_i;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
 </trdgrp>
</drv></art>

