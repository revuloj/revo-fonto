<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Angar</rad>/o <fnt><bib>BoER</bib></fnt>
</kap>
<drv mrk="angar.A0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero sudokcidente en la Orienta
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>,
      1779 km longa, la dekstra kaj la plej akvoplena alfluanto de
      <ref cel="jenisej.J0o">Jenisejo</ref>, fluanta el
      <ref cel="bajkal.0o">Bajkalo</ref>.
    </dif>
  </snc>
  <trd lng="en">Angara</trd>
  <trd lng="fr">Angara</trd>
  <trd lng="ru">&c_A;&c_n;&c_g;&c_a;&c_r;&c_a;</trd>
</drv>
</art>
