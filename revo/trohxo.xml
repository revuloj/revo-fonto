<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trohxo.xml,v 1.7 2019/09/28 13:10:16 revo Exp $">
  <kap>
    <rad>tro&hcirc;oid</rad>/o
  </kap>
<drv mrk="trohxo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="trohxo.0o.cikloido">
    <dif>
      <ref tip="dif" cel="cikloi.0o">Cikloido</ref>:
      <ekz>
        tro&hcirc;oido signifas ian ajn cikloidon, mallongigitan cikloidon a&ubreve; longigitan
        cikloidon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105750"
              >Cikloido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="cikloi.longigita0o">longigita cikloido</ref>,
      <ref cel="cikloi.mallongigita0o">mallongigita cikloido</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="epitro.0o">epitro&hcirc;oido</ref>,
      <ref cel="hipotr.0o">hipotro&hcirc;oido</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_h;&c_o;&c_ib;&c_d;&c_a;</trd>
  <trd lng="ca">trocoide</trd>
  <trd lng="de">Trochoide</trd>
  <trd lng="en">trochoid</trd>
  <trd lng="fr">trocho&iuml;de</trd>
  <trd lng="hu">trochoid</trd>
  <trd lng="it">trocoide<klr tip="amb"> (mat.)</klr></trd>
  <trd lng="pl">trochoida</trd>
  <trd lng="pt">troc&oacute;ide</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_h;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

