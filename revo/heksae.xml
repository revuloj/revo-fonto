  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_k;&c_s;&c_a;&c_e;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_g;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hexaeder</trd>,
    <trd>Sechsflach</trd>,
    <trd>Sechsfl&auml;chner</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&xi;&alfa_ton;&epsilon;&delta;&rho;&omikron;</trd>
  <trd lng="en">hexahedron</trd>
  <trd lng="es">hexaedro</trd>
  <trd lng="fr">hexa&egrave;dre</trd>
  <trdgrp lng="hu">
    <trd>hexa&eacute;der</trd>,
    <trd>hatlap</trd>,
    <trd>kocka</trd>
  </trdgrp>
  <trd lng="it">esaedro</trd>
  <trd lng="ja">&#x516D;&#x9762;&#x4F53;  <pr>&#x308D;&#x304F;&#x3081;&#x3093;&#x305F;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>hexa&euml;der</trd>,
    <trd>zesvlak</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>heksaedr</trd>,
    <trd>sze&sacute;cian<klr> (niekoniecznie foremny)</klr></trd>
  </trdgrp>
