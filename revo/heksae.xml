  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_k;&c_s;&c_a;&c_e;&c_d;&c_r;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_i;&c_g;&c_r;&c_a;&c_n;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x443;&#x431;</trd>,
    <trd>&#x433;&#x435;&#x43A;&#x441;&#x430;&#x435;&#x434;&#x440;</trd>,
    <trd>&#x448;&#x435;&#x441;&#x442;&#x438;&#x433;&#x440;&#x430;&#x43D;&#x43D;&#x438;&#x43A;</trd>
  </trdgrp>
  <trd lng="zh">&#x516D;&#x9762;&#x4F53; <pr>li&#xF9;mi&#xE0;nt&#x1D0;</pr></trd>
