<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: foliant.xml,v 1.8 2023/11/17 09:26:20 revo Exp $">
<kap>
  <rad>foliant</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="foliant.0o">
  <kap><tld/>o</kap>
  <snc mrk="foliant.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Bindita <ref tip="super" cel="libr.0o.presajxo">libro</ref>
      kun kvarpa&gcirc;aj kajeroj, po konsistantaj el unufoje
      faldita <ref tip="prt" cel="foli.0o.TEK">folio</ref>
      (do en formato de &ccirc;. 29 &#10005; 40&#x202F;cm):
      <ekz>
        sur multeta&gcirc;aj bretoj vici&gcirc;is dikaj <tld/>oj, ledebinditaj, korpulentaj volumoj
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        la loko de miaj luktoj kontra&ubreve; tiuj <tld/>oj
        estis la tablo, hele lumigita de malalte pendanta lampo
        <fnt>
          <aut>S. Lem, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Lem/index.html">
          Surloka Esploro</url></vrk>,
          <lok>[2003]</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la planko turi&gcirc;is stakoj de grandaj <tld/>oj, sur kelkaj tabloj ku&scirc;is montoj
        el pli malgrandaj libroj kun ledaj binda&jcirc;oj kaj ore brilantaj flankoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d2/Foliant_1806.jpg/279px-Foliant_1806.jpg" prm="CC BY-SA 3.0"><tld/>o,
    1806, bindita en porka ledo
        <fnt>
          <aut>Lysippos, propra verko</aut>
          <lok><url ref="&WCW;/File:Foliant_1806.jpg">&WCW;/File:Foliant_1806.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_ib;&c_ja;&c_n;&c_t;</trd>
  <trd lng="cs">foliant</trd>
  <trd lng="de">Foliant</trd>
  <trd lng="fr">in-folio</trd>
  <trd lng="pl">folio</trd>
  <trd lng="ru">&c_f;&c_o;&c_l;&c_i;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">foliant</trd>
  <trd lng="uk">&c_f;&#x43E;&#x43B;&c_ib;&c_a;&#x43D;&c_t;</trd>
</drv>
</art>

