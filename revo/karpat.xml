<vortaro>
  <art mrk="$Id: karpat.xml,v 1.6 2023/12/03 08:00:24 afido Exp $">
    <kap>
      <rad>Karpat</rad>/oj
    </kap>

<drv mrk="karpat.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:montaroj" cel="mont.0aro">Montaro</ref>
      etendi&gcirc;anta arkoforme de Slovakujo &gcirc;is Rumanujo:
      <ekz>
        nia grandega <klr>(rusa)</klr> regno <klr>[&#x2026;]</klr>
        etendi&gcirc;as de la <tld/>oj &gcirc;is
        la Paca Oceano
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur Slovaka flanko de l' <tld/>oj, pastroj
        skribe fiksis la nacian lingvon per
        Evangelio
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malsova&gcirc;igo de la &ccirc;evalo okazis <klr>[&#x2026;]</klr> dum
        jarcentoj <klr>[&#x2026;]</klr>
        en la limoj de la Granda Stepo, inter <tld/>o, Uralo kaj Kaza&hcirc;io
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_a;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>
    <trd lng="de">Karpaten</trd>
    <trd lng="en">Carpathian Mountains</trd>
    <trd lng="fr">Carpates</trd>
    <trd lng="nl">Karpaten</trd>
  </snc>
  <trd lng="ja">&#x30AB;&#x30EB;&#x30D1;&#x30C1;&#x30A2;&#x5C71;&#x8108; 
  <pr>&#x30AB;&#x30EB;&#x30D1;&#x30C1;&#x30A2;&#x3055;&#x3093;&#x307F;&#x3083;&#x304F;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x5580;&#x5C14;&#x5DF4;&#x9621;&#x5C71;&#x8109; <pr>K&#x101;&#x11B;rb&#x101;qi&#x101;nSh&#x101;nm&#xE0;i</pr></trd>,
    <trd>&#x54AF;&#x5C14;&#x5DF4;&#x9621;&#x5C71;&#x8109; <pr>g&#x113;&#x11B;rb&#x101;qi&#x101;nsh&#x101;nm&#xE0;i</pr></trd>,
    <trd>&#x5580;&#x5C14;&#x5DF4;&#x9621;&#x5C71;&#x8109; <pr>k&#x101;&#x11B;rb&#x101;qi&#x101;nsh&#x101;nm&#xE0;i</pr></trd>
  </trdgrp>
</drv>

  </art>

