<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plot</rad>/o
</kap>
<drv mrk="plot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Rutilus</trd>), man&gcirc;ebla riverfi&scirc;o:
      <ekz>
        la kapteblaj fi&scirc;oj povas esti perkoj, <tld/>oj, tre malofte
        koregonoj<fnt><aut>Saliko</aut>: <vrk>Fi&scirc;kapti ekscite</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 10a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Pl&ouml;tze</trd>
  <trdgrp lng="fr">
    <trd>gardon</trd>
  </trdgrp>
  <trd lng="hu">konc&eacute;r</trd>
  <trd lng="nl">voorn</trd>
  <trd lng="pt">carpa</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_v;&c_a;</trd>
</drv>
</art>
