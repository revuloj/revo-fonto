<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plot</rad>/o
</kap>
<drv mrk="plot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Man&gcirc;ebla <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>,
      kun blanka ventro kaj ru&gcirc;aj okuloj kaj na&gcirc;iloj, vivanta en nesala
      akvo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Rutilus</trd>):
      <ekz>
        la kapteblaj fi&scirc;oj povas esti perkoj, <tld/>oj, tre malofte
        koregonoj<fnt><aut>Saliko</aut>: <vrk>Fi&scirc;kapti ekscite</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 10a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Pl&ouml;tze</trd>,
    <trd>Rotauge</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gardon</trd>
  </trdgrp>
  <trd lng="hu">konc&eacute;r</trd>
  <trd lng="nl">voorn</trd>
  <trd lng="pt">carpa</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_v;&c_a;</trd>
  <trd lng="sk">plotica</trd>
</drv>
</art>
