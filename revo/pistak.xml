<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pistak</rad>/o
</kap>
<drv mrk="pistak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Verda migdaleca frukto de
      <ref tip="malprt" cel="pistak.0ujo"><tld/>ujo</ref>,
      uzata en dol&ccirc;a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Pistazie <klr tip="ind">(Frucht)</klr></trd>
  <trd lng="fr">pistache</trd>
</drv>
<drv mrk="pistak.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="anakar.0acoj">anarkadiacoj</ref>
      (<trd lng="la">Pistacia</trd>).
    </dif>
  </snc>
  <trd lng="de">Pistazie <klr tip="ind">(Pflanze)</klr></trd>
</drv>
</art>
