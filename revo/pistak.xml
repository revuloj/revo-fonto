<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>pistak</rad>/o
</kap>
<drv mrk="pistak.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Verda migdaleca frukto de
       <ref tip="malprt" cel="pistak.0ujo"><tld/>ujo</ref>,
       uzata en dol&ccirc;a&jcirc;oj.
     </dif>
   </snc>
   <trd lng="be">&c_f;&c_ib;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
   <trdgrp lng="br">
     <trd>hogan-Spagn</trd>,
     <trd>pistachezenn<klr tip="ind"> (frouezhenn)</klr></trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>festuc</trd>
     <trd>pistatxo</trd>
   </trdgrp>
   <trd lng="de">Pistazie<klr tip="ind"> (Frucht)</klr></trd>
   <trd lng="es">pistacho</trd>
   <trd lng="fr">pistache</trd>
   <trd lng="hu">piszt&aacute;cia<klr tip="ind"> (term&eacute;s)</klr></trd>
   <trd lng="nl">pistache</trd>
   <trd lng="pt">pistache</trd>
   <trd lng="ru">&c_f;&c_i;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a;</trd>
</drv>

<drv mrk="pistak.0ujo">
   <kap><tld/>ujo</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super"
       cel="anakar1.0oj">anarkadiacoj</ref>
       (<trd lng="la">Pistacia</trd>).
     </dif>
   </snc>
   <trd lng="be">&c_f;&c_ib;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
   <trdgrp lng="br">
     <trd>gwezenn-bistachez</trd>,
     <trd>pistachezenn<klr tip="ind"> (gwezenn)</klr></trd>
   </trdgrp>
   <trd lng="de">Pistazie<klr tip="ind"> (Pflanze)</klr></trd>
   <trd lng="hu">piszt&aacute;ciafa</trd>
   <trd lng="nl">pistacheboom</trd>
   <trd lng="fr">pistachier</trd>
   <trd lng="ru">&c_f;&c_i;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
