<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pistak.xml,v 1.25 2018/11/24 11:10:15 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>pistak</rad>/o
</kap>
<drv mrk="pistak.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">KUI</uzo>
   <snc>
     <dif>
       Verda migdaleca <ref tip="super" cel="frukt.0o.BOT">frukto</ref>
       de <ref tip="malprt" cel="pistak.0ujo"><tld/>ujo</ref>,
       uzata en dol&ccirc;a&jcirc;oj:
      <ekz>
        alportu al la homo donacon: iom da balzamo kaj iom da mielo, aroma&jcirc;on kaj
        mirhon, <tld/>ojn kaj migdalojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:11</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_f;&c_ib;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a;
<klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trdgrp lng="br">
     <trd>hogan-Spagn</trd>,
     <trd>pistachezenn<klr tip="ind"> (frouezhenn)</klr></trd>
   </trdgrp>
  <trdgrp lng="ca">
     <trd>festuc</trd>
     <trd>pistatxo</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>pist&aacute;cie</trd>,
    <trd>pist&aacute;ciov&yacute; o&rcaron;&iacute;&scaron;ek</trd>
  </trdgrp>
  <trd lng="de">Pistazie<klr tip="ind"> (Frucht)</klr></trd>
  <trd lng="en">pistachio</trd>
  <trd lng="es">pistacho</trd>
  <trd lng="fr">pistache</trd>
  <trd lng="hu">piszt&aacute;cia<klr tip="ind"> (term&eacute;s)</klr></trd>
  <trd lng="nl">pistache</trd>
  <trd lng="pt">pistache</trd>
  <trd lng="ru">&c_f;&c_i;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">pist&aacute;cia</trd>
  <trd lng="tr">f&inodot;st&inodot;k</trd>
</drv>

<drv mrk="pistak.0ujo">
   <kap><tld/>ujo</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref> el familio
       <ref tip="malprt" cel="anakar1.0oj">anarkadiacoj</ref>
       (<trd lng="la">Pistacia</trd>):
      <ekz>
        la <tld/>ujo, <tld/>arbo <klr>[&#8230;]</klr> estas hejma de Irano &gcirc;is Centra Azio,
        kultivata en la varmaj regionoj (e&ccirc; de Usono) por produktado de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=180484"
              >Pistakujo</url></lok>
        </fnt>.
      </ekz>
     </dif>
    <bld
      lok="&WCU;/thumb/6/6a/Pistacio_vera_Kerman_2.jpg/491px-Pistacio_vera_Kerman_2.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>oj &ccirc;e bran&ccirc;o de <tld/>ujo (Pistacia vera &leftquot;Kerman&rightquot;), Las Vegas, Nevado
        <fnt>
          <aut>Stan Shebs, propra verko</aut>
          <lok><url ref="&WCW;/File:Pistacio_vera_Kerman_2.jpg"
              >https://commons.wikimedia.org/wiki/File:Pistacio_vera_Kerman_2.jpg</url></lok>
        </fnt>
    </bld>
   </snc>
  <trd lng="be">&c_f;&c_ib;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="br">
     <trd>gwezenn-bistachez</trd>,
     <trd>pistachezenn<klr tip="ind"> (gwezenn)</klr></trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>kloko&scaron;</trd>,
    <trd>pist&aacute;cie</trd>
  </trdgrp>
  <trd lng="de">Pistazie<klr tip="ind"> (Pflanze)</klr></trd>
  <trd lng="en">pistachio</trd>
  <trd lng="fr">pistachier</trd>
  <trd lng="hu">piszt&aacute;ciafa</trd>
  <trd lng="nl">pistacheboom</trd>
  <trd lng="ru">&c_f;&c_i;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_a; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">pist&aacute;cia</trd>
</drv>
</art>
