<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barika.xml,v 1.28 2025/10/27 18:40:59 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>barikad</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="barika.0o">
  <kap><tld/>o</kap>
  <snc mrk="barika.0o.KOMUNE">
    <dif>
      Improvizita <ref tip="super" cel="bar.0ajxo">bara&jcirc;o</ref>
      por defendo a&ubreve; &scirc;topo, ekzemple en stratbatalo:
      <ekz>
        policta&ccirc;mento, alirinte la <tld/>on, subite atakas &gcirc;in
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        por tio, ke efektivi&gcirc;u la plej grandaj kaj gravaj
        &scirc;an&gcirc;oj en homara vivo, estas necesaj neniaj
        <klr>[&#x2026;]</klr> revolucioj, nek <tld/>oj
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la fenestroj estis duone &scirc;irmitaj de libro<tld/>oj kaj nur avare enlasis la
        lumon supre
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 6 (68)</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devis helpi per miaj manoj konstrui <tld/>ojn kontra&ubreve; la rusaj tankoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0o.KOMUNE">baro</ref>,
      <ref cel="bar.0ilo">barilo</ref>,
      <ref cel="barier.0o">bariero</ref>
    </refgrp>
    <trd lng="zh">&#x8DEF;&#x969C; <pr>l&ugrave;zh&agrave;ng</pr></trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="ca">barricada</trd>
  <trd lng="cs">barik&aacute;da</trd>
  <trdgrp lng="de">
    <trd>Barrikade</trd>,
    <trd>Sperre</trd>,
    <trd>Stra&szlig;ensperre</trd>
  </trdgrp>
  <trd lng="en">barricade</trd>
  <trd lng="es">barricada</trd>
  <trd lng="fr">barricade</trd>
  <trd lng="he">&mem;&#x5B4;&tav;&resh;&#x5B8;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>&uacute;ttorlasz</trd>,
    <trd>barik&aacute;d</trd>
  </trdgrp>
  <trd lng="ja">&#x30D0;&#x30EA;&#x30B1;&#x30FC;&#x30C9;</trd>
  <trd lng="nl">barricade</trd>
  <trd lng="pl">barykada</trd>
  <trd lng="pt">barricada</trd>
  <trd lng="ro">baricad&abreve;</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_r;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>barik&aacute;da</trd>,
    <trd>z&aacute;tarasy</trd>
  </trdgrp>
  <trd lng="tr">barikat</trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
</drv>

<drv mrk="barika.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="barika.0i.KOMUNE">
    <dif>
      Forte bari per <tld/>o, amasigante a&jcirc;ojn:
      <ekz>
        ili <tld/>as la pordon de ene
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u la stratojn per matracoj
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 53a,
        p. 694a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>al&ccedil;ar una barricada <klr>(a, en)</klr></trd>,
    <trd>embarrar</trd>,
    <trd>barrar</trd>,
    <trd>embarrotar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>barik&aacute;dovat</trd>,
    <trd>stav&ecaron;t barik&aacute;dy</trd>,
    <trd>zabarik&aacute;dovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sperren</trd>,
    <trd>versperren</trd>,
    <trd>verrammeln</trd>,
    <trd>verbarrikadieren</trd>
  </trdgrp>
  <trd lng="en">barricade</trd>
  <trd lng="fr">barricader</trd>
  <trd lng="he">&mem;&#x5B4;&tav;&resh;&#x5B8;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>elbarik&aacute;doz</trd>,
    <trd>eltorlaszol</trd>
  </trdgrp>
  <trd lng="ja">&#x30D0;&#x30EA;&#x30B1;&#x30FC;&#x30C9;&#x3092;&#x7BC9;&#x304F;
  <pr>&#x3070;&#x308A;&#x3051;&#x30FC;&#x3069;&#x3092;&#x304D;&#x305A;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>barricaderen</trd>,
    <trd>versperren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>barykadowa&cacute;</trd>,
    <trd>tarasowa&cacute;</trd>,
    <trd>zastawiwa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">barricar</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_r;&c_i;&c_k;&c_a;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">barik&aacute;dova&tcaron;</trd>
  <trd lng="tr">barikat kurmak</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_i;&c_k;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;
    &c_b;&c_a;&c_r;&c_i;&c_k;&c_a;&c_d;&c_o;&c_ju;</trd>
  </trdgrp>
</drv>
</art>
