<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barika.xml,v 1.25 2024/10/07 18:36:22 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>barikad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="barika.0o">
  <kap><tld/>o</kap>
  <snc mrk="barika.0o.KOMUNE">
    <dif>
      Improvizita <ref tip="super" cel="bar.0ajxo">bara&jcirc;o</ref>
      por defendo a&ubreve; &scirc;topo, ekzemple en stratbatalo:
      <ekz>
        por tio, ke efektivi&gcirc;u la plej grandaj kaj gravaj
        &scirc;an&gcirc;oj en homara vivo, estas necesaj neniaj
        <klr>...</klr> revolucioj, nek <tld/>oj
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x8DEF;&#x969C; <pr>l&#xF9;zh&#xE0;ng</pr></trd>
</snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="cs">barik&aacute;da</trd>
  <trdgrp lng="de">
    <trd>Barrikade</trd>,
    <trd>Sperre</trd>,
    <trd>Stra&szlig;ensperre</trd>
  </trdgrp>
  <trd lng="en">barricade</trd>
  <trd lng="es">barricada</trd>
  <trd lng="fr">barricade</trd>
  <trd lng="he">&#x5DE;&#x5B4;&#x5EA;&#x5E8;&#x5B8;&#x5E1;</trd>
  <trdgrp lng="hu">
    <trd>&uacute;ttorlasz</trd>,
    <trd>barik&aacute;d</trd>
  </trdgrp>
  <trd lng="ja">&#x30D0;&#x30EA;&#x30B1;&#x30FC;&#x30C9;</trd>
  <trd lng="nl">barricade</trd>
  <trd lng="pl">barykada</trd>
  <trd lng="pt">barricada</trd>
  <trd lng="ro">baricad&abreve;</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_r;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>barik&aacute;da</trd>,
    <trd>z&aacute;tarasy</trd>
  </trdgrp>
  <trd lng="tr">barikat</trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_i;&#x43A;&c_a;&c_d;&c_a;</trd>
</drv>

<drv mrk="barika.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="barika.0i.KOMUNE">
    <dif>
      Forte bari per <tld/>o, amasigante a&jcirc;ojn:
      <ekz>
        <tld/>i sian pordon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>u la stratojn per matracoj
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 53a,
        p. 694a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>barik&aacute;dovat</trd>,
    <trd>stav&ecaron;t barik&aacute;dy</trd>,
    <trd>zabarik&aacute;dovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sperren</trd>,
    <trd>versperren</trd>,
    <trd>verrammeln</trd>,
    <trd>verbarrikadieren</trd>
  </trdgrp>
  <trd lng="en">barricade</trd>
  <trd lng="fr">barricader</trd>
  <trd lng="he">&#x5DE;&#x5B4;&#x5EA;&#x5E8;&#x5B8;&#x5E1;</trd>
  <trdgrp lng="hu">
    <trd>elbarik&aacute;doz</trd>,
    <trd>eltorlaszol</trd>
  </trdgrp>
  <trd lng="ja">&#x30D0;&#x30EA;&#x30B1;&#x30FC;&#x30C9;&#12434;&#x7BC9;&#x304F; 
  <pr>&#12400;&#x308A;&#12369;&#x30FC;&#12393;&#12434;&#x304D;&#x305A;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>barricaderen</trd>,
    <trd>versperren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>barykadowa&cacute;</trd>,
    <trd>tarasowa&cacute;</trd>,
    <trd>zastawiwa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">barricar</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_r;&c_i;&c_k;&c_a;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">barik&aacute;dova&tcaron;</trd>
  <trd lng="tr">barikat kurmak</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_i;&#x43A;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&#x43E;&c_r;&#x43E;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_b;&c_a;&c_r;&c_i;&#x43A;&c_a;&c_d;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
