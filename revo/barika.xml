<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>barikad</rad>/o
</kap>
<drv mrk="barika.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Improvizita bara&jcirc;o kontra&ubreve; atako,  stratbarilo.
</dif>
</snc>
  <trd lng="en">barricade</trd>
  <trd lng="es">barricada</trd>
  <trd lng="fr">barricade</trd>
  <trdgrp lng="hu">
    <trd>&uacute;ttorlasz</trd>,
    <trd>barik&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Barrikade</trd>,
    <trd>Sperre</trd>,
    <trd>Stra&szlig;ensperre</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_r;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">barik&aacute;da</trd>
</drv>
<drv mrk="barika.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc num="1">
<dif>
Bari per <tld/>o.
</dif>
  <trdgrp lng="de">
    <trd>sperren</trd>,
    <trd>versperren</trd>
  </trdgrp>
</snc>
<snc num="2">
<dif>
Forte bari, fermi: <ekz><tld/>i sian pordon<fnt>B</fnt>.
</ekz>
</dif>
  <trdgrp lng="de">
    <trd>verrammeln</trd>,
    <trd>verbarrikadieren</trd>
  </trdgrp>
</snc>
<trd lng="en">barricade</trd>
<trd lng="fr">barricader</trd>
<trdgrp lng="hu">
  <trd>elbarik&aacute;doz</trd>,
  <trd>eltorlaszol</trd>
</trdgrp>
<trd 
lng="ru">&c_b;&c_a;&c_r;&c_r;&c_i;&c_k;&c_a;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
<trd lng="sk">barik&aacute;dova&tcaron;</trd>
</drv>
</art>
