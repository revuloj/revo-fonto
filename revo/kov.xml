<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kov</rad>/i
</kap>
<drv mrk="kov.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(pp ino de birdoj)</uzo>
    <dif>
      Sidi sufi&ccirc;an tempon sur ovoj por havigi al ili konstantan varmon kaj
      ebligi maturi&gcirc;on de la embrio:
      <ekz>
        kokino &ccirc;irka&ubreve;ita de sia <tld/>itaro.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>bebr&uuml;ten</trd>,
       <trd>ausbr&uuml;ten</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>brood</trd>,
      <trd>hatch</trd>,
      <trd>incubate</trd>,
      <trd>sit (p.p. ovoj)</trd>
    </trdgrp>
    <trd lng="sv">ruva<klr> (p&aring;)</klr></trd>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_i;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_t;&c_je;&c_n;&c_c;&c_o;&c_v;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Plej zorge gardi kaj flegi:
      <ekz>
        la patrino <tld/>as sian filon.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>bemuttern</trd>
    </trdgrp>
    <trd lng="en">watch over</trd>
    <trd lng="ru">&c_l;&c_je;&c_l;&c_je;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ka&scirc;e konservi ion por ke &gcirc;i aperu poste; ka&scirc;e prepari:
      <ekz>
        la cindroj <tld/>as la fajron;
      </ekz>
      <ekz>
        <tld/>i perfidon;
      </ekz>
      <ekz>
        mute <tld/>ata doloro;
      </ekz>
      <ekz>
        lia koro <tld/>as malamon kaj envion.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>aushecken</trd>,
       <trd>ausbr&uuml;ten</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>brood over</trd>,
      <trd>hatch up</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_l;&c_je;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_n;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">couver</trd>
</drv>

<drv mrk="kov.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie birdino <tld/>as ovojn
    </dif>
    <refgrp tip="sin">
      <ref cel="nest.0o">nesto</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>nest</trd>,
      <trd>nest box <klr>(se farita de homo)</klr></trd>,
      <trd>nesting place</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Brutplatz</trd>,
       <trd>Brutst&auml;tte</trd>
    </trdgrp>
    <trd lng="sv">rede</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Naskejo, centro, plej aktiva punkto:
      <ekz>
        <tld/>ejo de konspiro.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Brutst&auml;tte</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>birth place</trd>,
      <trd>hotbed</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">nid</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</trd>
</drv>

<drv mrk="kov.el0i">
  <kap>
    el<tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elovigi, el&scirc;eligi idojn post sufi&ccirc;a <tld/>ado.
    </dif>
    <trdgrp lng="de">
       <trd>ausbr&uuml;ten</trd>
    </trdgrp>
    <trd lng="en">hatch out</trd>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_i;&c_d;&c_je;&c_t;&c_mol; <klr>(&c_p;&c_t;&c_je;&c_n;&c_c;&c_o;&c_v;)</klr></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekaperigi, naski:
      <ekz>
        libereco el<tld/>as grandajn entreprenojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>geb&auml;ren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>give birth to</trd>,
      <trd>give rise to</trd>,
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_n;&c_a; &c_s;&c_v;&c_je;&c_t;</trd>
  </snc>
</drv>
</art>
