<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kolimb</rad>/o
</kap>
<drv mrk="kolimb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gavio</trd>), na&gcirc;birdoj kun kruroj proksimaj
      al la vosto kaj na&gcirc;ha&ubreve;to inter la piedfingroj.
    </dif>
