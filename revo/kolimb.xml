<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolimb</rad>/o
</kap>
<drv mrk="kolimb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kolimb1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gavio</trd>), na&gcirc;birdoj kun kruroj proksimaj
      al la vosto kaj na&gcirc;ha&ubreve;to inter la piedfingroj.
    </dif>
  </snc>
  <trd lng="cs">pot&aacute;plice</trd>
  <trd lng="de">Seetaucher</trd>
  <trdgrp lng="en">
    <trd>diver</trd>,
    <trd>loon</trd>
  </trdgrp>
  <trd lng="hu">b&uacute;v&aacute;r(mad&aacute;r)</trd>
  <trd lng="pl">nur</trd>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;&c_r;&c_a;</trd>
</drv>

<drv mrk="kolimb.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gaviiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Seetaucher <klr tip="ind">(Ordnung)</klr></trd>
  <trd lng="hu">b&uacute;v&aacute;ralak&uacute;ak</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_g;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_g;&c_a;&c_r;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>
