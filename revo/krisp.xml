  <rad>krisp</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="krisp.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="krisp.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ondigita a&ubreve; <ref cel="fald.0umi">faldumi</ref>ta kolumo, aparte el tiaj, kiuj dum
      mezepoko protektis la kolon kontra&ubreve; glavbatoj,
      a&ubreve; tiaj, ornamcele surmetitaj de viroj kaj virinoj dum 16-a/17-a jarcento:
      <ekz>
        maldika figuro, kies mallar&gcirc;a kolo estis blanke &ccirc;irka&ubreve;ita de delikata batista
        <tld/>o
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliajn edzojn oni vidis vestitajn en &scirc;talo a&ubreve; en <klr>[&#x2026;]</klr> multekostaj
        manteloj,
        sub&scirc;tofitaj per sciuraj feloj, kaj kun blueta kol<tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ZOO</uzo>
        karakteriza&jcirc;o de multaj <klr>[vulturoj]</klr> estas kol<tld/>o, el kiu longa
        nuda a&ubreve; mallongplumita kolo levi&gcirc;as
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Vulturo">Vulturo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[&scirc;i]</klr> kun lip<tld/>o petis pardonon
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxabot.0o">&jcirc;aboto</ref>,
      <ref cel="falbal.0o">falbalo</ref>,
      <ref cel="plis.0i">plisi</ref>
    </refgrp>
    <bld
    lok="&WCU;/thumb/6/64/Plooikraag%2C_fraise_&agrave;_la_confusion.jpg/320px-Plooikraag%2C_fraise_&agrave;_la_confusion.jpg"
    prm="CC0">kol<tld/>o, proksimume 1615 - 1635
        <fnt>
          <aut>Nekonata a&ubreve;toro, Rijksmuseum</aut>
          <lok><url
          ref="&WCW;/File:Plooikraag,_fraise_%C3%A0_la_confusion.jpg">&WCW;/File:Plooikraag,_fraise_&agrave;_la_confusion.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_y;&c_zh;&c_y;</trd>,
      <trd>&c_zh;&c_a;&c_b;&c_o;</trd>
    </trdgrp>
    <trd lng="ca">gorgera</trd>
    <trd lng="de"><klr>(Hals-)</klr>Krause</trd>
    <trdgrp lng="en">
      <trd>frill</trd>,
      <trd>frilled collar</trd>,
      <trd>ruffle</trd>
    </trdgrp>
    <trd lng="fr">fraise</trd>
    <trd lng="hu">nyakfodor</trd>
    <trd lng="it">gorgiera</trd>
    <trdgrp lng="nl">
      <trd>plooisel</trd>,
      <trd>krul</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kryza</trd>,
      <trd>falbanka</trd>
    </trdgrp>
    <trd lng="pt">gola pregueada</trd>
    <trd lng="ro">flan&scomma;&abreve;</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_y;&c_zh;&c_i;</trd>,
      <trd>&c_zh;&c_a;&c_b;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="krisp.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Refaldo de <ref tip="malprt" cel="perito.0o">peritoneo</ref>,
      kiu fiksas la diversajn partojn de intestoj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_t;&c_o;&c_ch;&c_y;&c_n;&c_k;&c_a; <klr
      tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>,
      <trd>&c_ch;&c_a;&c_p;&c_je;&c_c; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>,
      <trd>&c_b;&c_r;&c_y;&c_zh;&c_e;&c_j;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">mesenteri</trd>
    <trdgrp lng="de">
      <trd>Gekr&ouml;se</trd>,
      <trd>Mesenterium</trd>
    </trdgrp>
    <trd lng="en">mesentery</trd>
    <trd lng="fr">m&eacute;sent&egrave;re</trd>
    <trd lng="hu">b&eacute;lfodor</trd>
    <trdgrp lng="it">
      <trd>mesentere</trd>,
      <trd>mesenterio</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>darmscheil</trd>,
      <trd>mesenterium</trd>
    </trdgrp>
    <trd lng="pl">krezka</trd>
    <trd lng="pt">mesent&eacute;rio</trd>
    <trd lng="ro">flan&scomma;&abreve;</trd>
    <trd lng="ru">&c_b;&c_r;&c_y;&c_zh;&c_je;&c_j;&c_k;&c_a;</trd>
  </snc>
  <rim>
    La&ubreve; la tradukoj en UV de &F; amba&ubreve; supraj sencoj estas fundamentaj.
  </rim>
  <trdgrp lng="ja">
    <trd>&#x4E38;&#x3072;&#x3060;&#x3048;&#x308A;  <pr>&#x307E;&#x308B;&#x3072;&#x3060;&#x3048;&#x308A;</pr></trd>,
    <trd>&#x3072;&#x3060;&#x98FE;&#x308A;  <pr>&#x3072;&#x3060;&#x304B;&#x3056;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_i;&c_zh;&c_ib;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_zh;&c_ib;</trd>,
    <trd>&c_zh;&c_a;&c_b;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="krisp.0a">
  <kap>
    <tld/>a <fnt><bib>MT</bib></fnt>
  </kap>
  <snc mrk="krisp.0a.KOMUNE">
    <dif>
      <klr>(pp haroj ks)</klr> Onde,
      bukle <ref tip="super" cel="glat.mal0a">malglata</ref>:
      <ekz>
        anstata&ubreve; <tld/>aj haroj estos kalvo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 3:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam el liaj haroj fluis la akvo, ili tamen estis tre &ccirc;arme <tld/>aj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo karesis la <tld/>an kapon de l' sklavo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a plumaro
        <klr>(vd <ref cel="lanug.0o.birdo">lanugo</ref>)</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam levi&gcirc;is vento, <klr>[&#x2026;]</klr> la ondoj <tld/>i&gcirc;is kaj montris sin en vesto
        el &scirc;a&ubreve;mo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la vento blovas super la herbo, tiam &ccirc;i tiu <tld/>i&gcirc;as kiel trankvila akvo,
        sed kiam la vento blovas super la greno, tiam &ccirc;i tiu ondi&gcirc;as kiel la maro
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;tofo <klr>(krepo)</klr>;
      </ekz>
      <ekz>
        la <tld/>a mento <fnt><bib>PV</bib></fnt>
        <klr>(<nom>Mentha spicata</nom>)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">arrissat</trd>
  <trdgrp lng="de">
     <trd>kraus</trd>,
     <trd>gekr&auml;uselt</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>wavy</trd>,
     <trd>crimped</trd>,
     <trd>crinkled</trd>,
    <trd>ruffled</trd>
  </trdgrp>
  <trd lng="fr">cr&eacute;pu</trd>
  <trdgrp lng="hu">
     <trd>fodros</trd>,
     <trd>bodros</trd>,
     <trd>g&ouml;nd&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>increspato</trd>,
     <trd>riccio<klr> (agg.)</klr></trd>,
     <trd>arricciato</trd>,
     <trd>crespo</trd>.
     <trd>ondulato</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x3060;&#x306E;&#x3042;&#x308B;</trd>,
    <trd>&#x7E2E;&#x308C;&#x305F;  <pr>&#x3061;&#x3062;&#x308C;&#x305F;</pr></trd>,
    <trd>&#x3057;&#x308F;&#x306E;&#x5BC4;&#x3063;&#x305F;  <pr>&#x3057;&#x308F;&#x306E;&#x3088;&#x3063;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kroes</trd>,
    <trd>gekruld</trd>,
    <trd>gerimpeld</trd>,
    <trd>golvend</trd>
  </trdgrp>
  <trd lng="pl">k&eogonek;dzierzawy</trd>
  <trd lng="ro">buclat</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_d;&c_r;&c_ja;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_ch;&c_a;&c_v;&c_y;&c_j; <klr>(&c_o;
&c_v;&c_o;&c_l;&c_o;&c_s;&c_a;&c_h;)</klr></trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;&c_ja;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_u;&c_ch;&c_je;&c_r;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_m;&c_o;&c_r;&c_sh;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4ED3;&#x7687;&#x5931;&#x63AA; <pr>c&amacron;nghu&aacute;ngsh&imacron;cu&ograve;</pr></trd>,
    <trd>&#x5009;&#x7687;&#x5931;&#x63AA; <pr>c&amacron;nghu&aacute;ngsh&imacron;cu&ograve;</pr></trd>,
    <trd>&#x6709;&#x5377;&#x66F2; <pr>y&#x1D2;uju&#x1CE;nq&#x1D4;</pr></trd>,
    <trd>&#x6709;&#x6372;&#x66F2; <pr>y&#x1D2;uju&#x1CE;nq&#x1D4;</pr></trd>,
    <trd>&#x6709;&#x5377;&#x66F2;&#x7684; <pr>y&#x1D2;uju&#x1CE;nq&#x1D4;de</pr></trd>,
    <trd>&#x6709;&#x6372;&#x66F2;&#x7684; <pr>y&#x1D2;uju&#x1CE;nq&#x1D4;de</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krisp.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Montri, elformi <tld/>ojn:
      <ekz>
        bukloj iliaj kiel nuboj <tld/>is
        <fnt>
          <aut>Mil&aacute;n F&uuml;st, trad. K&aacute;lm&aacute;n Kalocsay</aut>
          <vrk><url ref="https://egalite.hu/kalocsay/Dissemitaj_floroj.pdf"
              >Letero Pri La Morto De Edipo</url></vrk>
          <lok>Dissemitaj Floroj, p. 122</lok>
        </fnt>;
      </ekz>
      <ekz>
        vitra&jcirc;oj makulitaj de farboj, sur kiuj <tld/>is &scirc;iritaj kaj polvoplenaj kurtenoj
        <fnt>
          <aut>Virgilio Brocchi</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Pa%C4%9Do:Literatura_Mondo_-_Julio_1935.pdf/3"
              >Edzi&gcirc;o de Zubi</url></vrk>
          <lok> Literatura Mondo, 1935 Julio, p. 105</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bukl.0i">bukli</ref>
  </snc>
  <trd lng="de">sich <ind>kr&auml;useln</ind></trd>
  <trd lng="en">curl</trd>
</drv>

<drv mrk="krisp.0igi">
  <kap>
    <tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="krisp.0igi.KOMUNE">
    <dif>
      Fari ion <tld/>a:
      <ekz>
        <tld/>igi harojn, falbalon;
      </ekz>
      <ekz>
        &scirc;i komencis enuigan <tld/>igon
        de la ekstremoj de lar&gcirc;a silka skarpo
        <fnt>
          <aut>E. Orzeszko, trad. Kabe</aut>
          <vrk><url ref="https://edoc.pub/bona-sinjorino-pdf-free.html">Bona Sinjorino</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxif.0i">&ccirc;ifi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>arrissar</trd>,
    <trd>crespar</trd>,
    <trd>encrespar</trd>
  </trdgrp>
  <trd lng="de">kr&auml;useln</trd>
  <trdgrp lng="en">
    <trd>crimp</trd>,
    <trd>curl</trd>,
    <trd>wave</trd>,
    <trd>ruffle</trd>,
    <trd>ripple</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;per</trd>
  <trdgrp lng="hu">
     <trd>fodros&iacute;t</trd>,
     <trd>bodros&iacute;t</trd>,
     <trd>g&ouml;nd&ouml;r&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>increspare</trd>,
     <trd>arricciare</trd>,
     <trd>raggrinzare</trd>.
     <trd>ondulare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x3060;&#x3092;&#x3064;&#x3051;&#x308B;</trd>,
    <trd>&#x7E2E;&#x3089;&#x305B;&#x308B;  <pr>&#x3061;&#x3062;&#x3089;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x30AE;&#x30E3;&#x30B6;&#x30FC;&#x3092;&#x3064;&#x3051;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doen <ind>krullen</ind></trd>,
    <trd>doen <ind>rimpelen</ind></trd>
  </trdgrp>
  <trd lng="pl">zmarszczy&cacute;</trd>
  <trd lng="ro">plisa</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_o;
    &c_v;&c_o;&c_l;&c_o;&c_s;&c_a;&c_h;)</klr></trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_o;&c_r;&c_k;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5377;&#x66F2; <pr>ju&#x1CE;nq&umacron;</pr></trd>,
    <trd>&#x6372;&#x66F2; <pr>ju&#x1CE;nq&umacron;</pr></trd>,
    <trd>&#x4F7F;&#x5448;&#x6CE2;&#x6D6A;&#x5F62; <pr>sh&#x1D0;ch&eacute;ngb&omacron;l&agrave;ngx&iacute;ng</pr></trd>,
    <trd>&#x4F7F;&#x5377;&#x66F2; <pr>sh&#x1D0;ju&#x1CE;nq&#x1D4;</pr></trd>,
    <trd>&#x4F7F;&#x6372;&#x66F2; <pr>sh&#x1D0;ju&#x1CE;nq&#x1D4;</pr></trd>,
    <trd>&#x4F7F;&#x8D77;&#x76B1; <pr>sh&#x1D0;q&#x1D0;zh&ograve;u</pr></trd>,
    <trd>&#x4F7F;&#x8D77;&#x76BA; <pr>sh&#x1D0;q&#x1D0;zh&ograve;u</pr></trd>,
    <trd>&#x9765; <pr>y&egrave;</pr></trd>,
    <trd>&#x9768; <pr>y&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krisp.0igilo">
  <kap>
    <tld/>igilo
  </kap>
  <snc mrk="krisp.0igilo.FRIZ">
    <dif>
      <ref tip="super" cel="friz.0ilo">Frizilo</ref>
      per kiu oni <tld/>igas hararojn:
      <ekz>
        <klr>[trovi&gcirc;is en la po&scirc;o&#x2026;]</klr> tondilo en ingo, plenmano da vitraj perloj,
        kelkaj buloj da fadeno, kudrilujo, plena kolekto de <tld/>igiloj de haroj
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_p;&c_c;&c_y; &c_d;&c_l;&c_ja;
  &c_z;&c_a;&c_v;&c_ib;&c_w;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>molls <klr tip="amb">(pl)</klr></trd>,
    <trd>rul&middot;lo</trd>,
    <trd>bigud&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lockenwickler</trd>,
    <trd>Lockenstab</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crimper</trd>,
    <trd>waver</trd>
  </trdgrp>
  <trd lng="fr">fer &agrave; friser</trd>
  <trd lng="uk">&c_shch;&c_i;&c_p;&c_c;&c_ib; &c_d;&c_l;&c_ja;
  &c_z;&c_a;&c_v;&c_i;&c_v;&c_k;&c_i;</trd>
</drv>
</art>

