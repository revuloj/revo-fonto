<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krisp</rad>/o
</kap>
<drv mrk="krisp.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
  <uzo tip="fak">ANA</uzo> 
    <dif>
      Refaldo de <ref tip="malprt" cel="perito.0o">peritoneo</ref>,
      kiu fiksas la diversajn partojn de intestoj.
    </dif>
    <trd lng="de">Gekr&ouml;se</trd>
    <trd lng="en">mesentery</trd>
    <trd lng="fr">m&eacute;sent&egrave;re</trd>
    <trd lng="hu">b&eacute;lfodor</trd>
    <trdgrp lng="it">
      <trd>mesentere</trd>,
      <trd>mesenterio</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>darmscheil</trd>,
      <trd>mesenterium</trd>
    </trdgrp>
    <trd lng="pl">krezka</trd>
    <trd lng="pt">mesent&eacute;rio</trd>
    <trd lng="ro">flan&scedil;&abreve;</trd>
    <trd lng="ru">&c_b;&c_r;&c_y;&c_zh;&c_je;&c_j;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Duobla kolumo, kun ameligitaj tubformaj faldoj, surmetita de viroj
      kaj virinoj dum 16-a jarcento:
      <ekz>
        kol<tld/>o<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="jxabot.0o">&jcirc;aboto</ref>,
      <ref cel="falbal.0o">falbalo</ref>.
    </refgrp>
    <trd lng="de">Krause</trd>
    <trdgrp lng="en">
      <trd>frill</trd>,
      <trd>frilled collar</trd>,
      <trd>ruffle</trd>
    </trdgrp>
    <trd lng="fr">fraise</trd>
    <trd lng="hu">nyakfodor</trd>
    <trd lng="it">gorgiera</trd>
    <trdgrp lng="nl">
      <trd>plooisel</trd>,
      <trd>krul</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kryza</trd>,
      <trd>falbanka</trd>
    </trdgrp>
    <trd lng="pt">gola pregueada</trd>
    <trd lng="ro">flan&scedil;&abreve;</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_y;&c_zh;&c_i;</trd>,
      <trd>&c_zh;&c_a;&c_b;&c_o;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="krisp.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Prezentanta supra&jcirc;on similan al tiu de <tld/>o a&ubreve;
      de krepo, kun
      multaj neregulaj bukletoj a&ubreve; ondetoj:
      <ekz>
        <tld/>aj kapetoj de infanoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; <tld/>aj haroj estos kalvo<fnt>Z</fnt>
        (<ref tip="sin" cel="friz">frizita</ref>);
      </ekz>
      <ekz>
        <tld/>a plumaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj folioj;
      </ekz>
      <ekz>
        timante &ccirc;ifi siajn <tld/>ajn kaj malsimplajn
        vestojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;tofo <klr>(krepo)</klr>;
      </ekz>
      <ekz>
        la <tld/>a mento<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>kraus</trd>,
     <trd>gekr&auml;uselt</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>wavy</trd>,
     <trd>crimped</trd>,
     <trd>crinkled</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;p&eacute;</trd>
  <trdgrp lng="hu">
     <trd>fodros</trd>,
     <trd>bodros</trd>,
     <trd>g&ouml;nd&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>increspato</trd>,
     <trd>riccio<klr> (agg.)</klr></trd>,
     <trd>arricciato</trd>,
     <trd>crespo</trd>.
     <trd>ondulato</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kroes</trd>,
    <trd>gekruld</trd>,
    <trd>gerimpeld</trd>,
    <trd>golvend</trd>
  </trdgrp>
  <trd lng="pl">k&eogonek;dzierzawy</trd>     
  <trd lng="ro">buclat</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_d;&c_r;&c_ja;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_ch;&c_a;&c_v;&c_y;&c_j; <klr>(&c_o;
&c_v;&c_o;&c_l;&c_o;&c_s;&c_a;&c_h;)</klr></trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="krisp.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Fari ion <tld/>a:
      <ekz>
        <tld/>igi harojn, falbalon;
      </ekz>
      <ekz>
        &scirc;i komencis enuigan <tld/>igon de la ekstremoj de
lar&gcirc;a silka
        skarpo<fnt>B</fnt>
        <klr>(sulketigi)</klr>;
      </ekz>
      <ekz>
        varma vento <tld/>igis la akvon<fnt>B</fnt>
        (<ref tip="sin" cel="fald.0i">faldi</ref>);
      </ekz>
      <ekz>
        <tld/>igi artefaritajn rozpetalojn
        (<ref tip="sin" cel="friz.0i">frizi</ref>);
      </ekz>
      <ekz>
        la ondoj <tld/>i&gcirc;is kaj &scirc;a&ubreve;mis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la oraj grenkampoj ond<tld/>i&gcirc;as.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">kr&auml;useln</trd>
  <trdgrp lng="en">
    <trd>crimp</trd>,
    <trd>curl</trd>,
    <trd>wave</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;per</trd>
  <trdgrp lng="hu">
     <trd>fodros&iacute;t</trd>,
     <trd>bodros&iacute;t</trd>,
     <trd>g&ouml;nd&ouml;r&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>increspare</trd>,
     <trd>arricciare</trd>,
     <trd>raggrinzare</trd>.
     <trd>ondulare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doen <ind>krullen</ind></trd>,
    <trd>doen <ind>rimpelen</ind></trd>
  </trdgrp>
  <trd lng="pl">zmarszczy&cacute;</trd>     
  <trd lng="ro">plisa</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_o; &c_v;&c_o;&c_l;&c_o;&c_s;&c_a;&c_h;)</klr></trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
</art>

