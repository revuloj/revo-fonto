<kap>
<rad>&jcirc;ig</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jxig1.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxig1.0o.puzlo">
    <dif>
      <ref tip="dif" cel="puzl.0o.LUD">Puzlo</ref>:
      <ekz>
        dum tridek minutoj <klr>[&#8230;]</klr> ni ne scipovis konstrui
        <tld lit="&Jcirc;"/>o-kubon, tamen finfine <nom>Vojtek</nom> sukcesis
        <fnt>
          <aut>Marzena</aut>,
          <vrk><url ref="http://www.naszawyprawa.com/relacje_es.asp">
          Voja&gcirc;raporto</url></vrk>,
          <lok>Mongolio, 2003-12-22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;&c_l;&c_o;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_z;&c_l;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>u&lstroke;adanka</trd>,
      <trd>puzzle</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>quebra-cabe&ccedil;a</trd>,
      <trd>puzzle</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd><ind>gioco</ind> di pazienza</trd>,
      <trd>puzzle</trd>
    </trdgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kunmetenigmo</ke>.
  </rim>

  <rim mrk="jxig1.0o.RIM_HOM">
    <ref tip="hom" cel="jxig.0o">&Jcirc;igo</ref>
    estas anka&ubreve; la nomo de dancospeco.
  </rim>
<trd lng="de">Puzzle</trd>
  <trd lng="en">puzzle</trd>
  <trdgrp lng="ja">
    <trd>&#12472;&#12464;</trd>,
    <trd>&#12472;&#12540;&#12464;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_gu;&c_a;</trd>,
    <trd>&c_d;&c_zh;&c_i;&c_gu;&c_a; (&c_t;&c_a;&c_n;&c_je;&c_c;&c_mol;)</trd>,
    <trd>&c_r;&c_o;&c_z;&c_r;&c_ib;&c_z;&c_n;&c_a; &c_k;&c_a;&c_r;&c_t;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25340;&#22270; <pr>p&imacron;nt&uacute;</pr></trd>,
    <trd>&#25340;&#22270;&#29609;&#20855; <pr>p&imacron;nt&uacute;w&aacute;nj&ugrave;</pr></trd>
  </trdgrp>
</drv>
