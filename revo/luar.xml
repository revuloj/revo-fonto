<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: luar.xml,v 1.5 2022/05/01 12:04:34 revo Exp $">
    <kap>
      <rad>Luar</rad>/o
    </kap>

<drv mrk="luar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="luar.0o.rivero">
    <dif>
      <ref tip="malprt" cel="franc.F0ujo">Franca</ref>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">rivero</ref>, mil kilometrojn longa,
      fontanta en Centra Montaro, fluanta norden poste okcidenten tra
      Nanto kaj enfluanta Atlantikon:
      <ekz>
        Nanto situas en okcidenta Francio proksime al Atlantiko kaj &ccirc;e
        la rivero <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Lucette &Eacute;chapp&eacute;</aut>
          <vrk><url ref="&Monato;/2013/010930.php">Vizito al Nanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Regiona Naturparko <ref cel="brier.0o">Briero</ref> situas en
        suda Bretonio, inter la elfluejoj de la
        riveroj Vileno kaj <tld/>o &ccirc;e la Atlantika Oceano
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2013/010991.php">Kie akvo kaj &ccirc;ielo
          najbaras</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Loire</trd>
    <trd lng="fr">Loire</trd>
    <trd lng="pl">Loara</trd>     
    <trd lng="ro">Loara</trd>
  </snc>
  <trd lng="be">&c_L;&c_ju;&c_a;&c_r;&c_a; <klr>(&c_r;&c_a;&c_k;&c_a;)</klr></trd>
  <trd lng="uk">&#x41B;&c_u;&c_a;&c_r;&c_a;</trd>
</drv>

  </art>

