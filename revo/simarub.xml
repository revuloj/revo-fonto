  <trd lng="de">Bitterholzgew&auml;chse</trd>
  <trd lng="nl">hemelboomfamilie</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_a;&c_r;&c_u;&c_b;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
