<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stuc</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stuc.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">HOR</uzo>
  <snc>
    <dif>
      Fortran&ccirc;i superfluajn, ekstremajn bran&ccirc;ojn, radikojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;monder<klr> (&eacute;laguer)</klr></trd>,
    <trd>&eacute;laguer</trd>,
    <trd>tailler<klr> (&eacute;laguer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megnyes<klr tip="ind">(n&ouml;v&eacute;nyt)</klr></trd>,
    <trd>lenyes<klr tip="ind">(n&ouml;v&eacute;nyt)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="stuc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metoda fortran&ccirc;o de vegeta&jcirc;aj partoj:
      <ekz>
        subite li vidis, ke Vanjo Zhu fosas &scirc;osojn kaj faras <tld/>adon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 57a, p. 255a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;lagage</trd>,
    <trd>&eacute;mondage<klr> (&eacute;lagage)</klr></trd>,
    <trd>taille<klr> (&eacute;lagage)</klr></trd>
  </trdgrp>
  <trd lng="hu">nyes&eacute;s</trd>
</drv>
</art>

