<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>le&ubreve;s</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="leuxs.0o">
  <kap><tld/>o</kap>
  <snc mrk="leuxs.0o.GRU">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref cel="sedime.0o.MIN" tip="super">Sedimento</ref>
      el la kvaternara epoko kaj kiu formas nuntempe la suprojn
      de malmulte sulki&gcirc;intaj ebenoj a&ubreve; valoj:
      <ekz>
        Nordrejn-Vestfalio <klr>[&#8230;]</klr>: estas konata kiel aparte
        fruktodona
        regiono pro ega dikeco de <tld/>o, kiun la&ubreve; &gcirc;ia grundokvalito 
        en Germanujo superas nur la <nom>Magdeburger B&ouml;rde</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=3046">Nordrejn-Vestfalio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">loess</trd>
  <trd lng="de">L&ouml;ss</trd>
  <trd lng="fa">&a_b;&a_A;&a_d;&a_r;&a_damma;&a_f;&a_t;</trd>
  <trd lng="fr">l&oelig;ss</trd>
  <trd lng="hu">l&ouml;sz</trd>
  <trd lng="nl">l&ouml;ss</trd>
  <trd lng="pl">less</trd>
  <trdgrp lng="pt">
    <trd>loess</trd>,
    <trd>loesse</trd>,
    <trd>loessito</trd>    
  </trdgrp>
  <trd lng="ro">loess</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_jo;&c_s;&c_s;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_t;&c_o;&c_z;&c_jo;&c_m;</trd>
  </trdgrp>
</drv>
</art>
