<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>le&ubreve;s</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="leuxs.0o">
  <kap><tld/>o</kap>
  <snc mrk="leuxs.0o.GRU">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref cel="sedime.0o.MIN" tip="super">Sedimento</ref>
      el la kvaternara epoko kaj kiu formas nuntempe la suprojn
      de malmulte sulki&gcirc;intaj ebenoj a&ubreve; valoj.
    </dif>
  </snc>
  <trd lng="ca">loess</trd>
  <trd lng="de">L&ouml;ss</trd>
  <trd lng="fr">l&oelig;ss</trd>
  <trd lng="hu">l&ouml;sz</trd>
  <trd lng="nl">l&ouml;ss</trd>
  <trdgrp lng="pt">
    <trd>loess</trd>,
    <trd>loesse</trd>,
    <trd>loessito</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_jo;&c_s;&c_s;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_t;&c_o;&c_z;&c_jo;&c_m;</trd>
  </trdgrp>
</drv>
</art>
