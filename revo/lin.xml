<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lin</rad>/o
</kap>
<drv mrk="lin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lin.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Linum</trd>), el kiu unu specio estas kulturata
      pro la oleo kaj la fibroj uzataj por fari tolon:
      <ekz>
        <tld/>koloraj <klr>(pale blondaj)</klr> haroj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flachs</trd>,
    <trd>Lein</trd>
  </trdgrp>
  <trd lng="es">lino</trd>
</drv>
<drv mrk="lin.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj (<trd lng="la">Linaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Leingew&auml;chse</trd>
</drv>
<drv mrk="lin.0oleo">
  <kap><tld/>oleo</kap>
  <snc>
    <dif>
      Oleo ekstraktata el semoj de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Leinsamen&ouml;l</trd>
  <trd lng="es">aceite de lino</trd>
</drv>
<drv mrk="lin.0oleatolo0oleumo">
  <kap><tld/>olea tolo, <tld/>oleumo</kap>
  <snc>
    <dif>
      Teksa&jcirc;o nepenetrebla  de akvo, dense &scirc;mirita per diversaj
      materioj kaj oleo, iam uzita por kovri plankon ktp.
    </dif>
  </snc>
  <trd lng="de">Linoleum</trd>
</drv>
</art>
