<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: reaktor.xml,v 1.18 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>reaktor</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="reaktor.0o">
  <kap><tld/>o</kap>
  <snc mrk="reaktor.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="vid" cel="reakci.0o.KEM">Reakci</ref>ujo,
      aran&gcirc;a&jcirc;o por fari &hcirc;emiajn reakciojn:
      <ekz>
        oni aliformas akvon kaj karbondioksidon en <tld/>o en miks<klr>[a&jcirc;on]</klr> da
        hidrogeno kaj da karbona monooksido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=347435">Singaso-sunreaktoro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Reaktor</trd>
    <trdgrp lng="it">
      <trd>reagente<klr> (ch.)</klr></trd>,
      <trd>reattivo<klr> (ch.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc mrk="reaktor.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Reakciujo en kiu efektivi&gcirc;as regata reakcio de
      atomkernoj:
      <ekz>
        nuklea <tld/>o estis konektita al elektra reto en Pekino
        <fnt>
          <aut>anna chu</aut>,
          <vrk><url ref="http://www.verdapekino.com/raporto/index4.htm">Nova&jcirc;oj de
          scienco</url></vrk>,
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis haltigitaj du nukleaj <tld/>oj, produktantoj de plutonio-239
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konataj rezervoj de uranio estus konsumitaj post 20&#x202F;-&#x202F;30 jaroj, e&ccirc; se oni
        ne
        atentas la mankon de akvo por malvarmigi la <tld/>ojn kaj la traktadon de
        tiom alta kvanto de radioaktiva ruba&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Pierre Bertrand</aut>
          <vrk><url ref="&Monato;/2006/008926.php">La fino de malmultekosta energio</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ia bolakva <tld/>o estas simila al tiu &ccirc;e <nom>Fuku&scirc;ima</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2011/010418.php">Adia&ubreve;o al nukleo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        por evoluigo kaj konstruado de fuzio<tld/>o <nom>Iter</nom> EU pasintjare fondis komunan
        entreprenon por la da&ubreve;ro de 35 jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009613.php">Necesas energio</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Reaktor</trd>
    <trd lng="en">reactor</trd>
    <trd lng="it">reattore</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_a;&c_k;&c_t;&c_o;&c_r;</trd>,
      <trd>&c_a;&c_t;&c_o;&c_m;&c_n;&c_y;&c_j; &c_k;&c_o;&c_t;&c_jo;&c_l;</trd>
    </trdgrp>
  </snc>
  <snc mrk="reaktor.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="dif" cel="jet.0motoro">Jetmotoro</ref>:
      <ekz>
        la juna piloto <klr>[&#x2026;]</klr> tuj inversigas la <tld/>ojn
        kaj premegas la bremsojn
        <fnt>
          <aut>Ailanto</aut>,
          <vrk><url ref="http://www.kafejo.com/humuro/07.htm">&Scirc;ercaro sepa</url></vrk>,
          <lok>2006-07-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Triebwerk</trd>
    <trd lng="it">reattore</trd>
    <trd lng="zh">&#x5F15;&#x64CE; <pr>y&#x1D0;nq&#xED;ng</pr></trd>
</snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_k;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>reaktor</trd>,
    <trd>tryskov&yacute; motor</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;acteur</trd>
  <trdgrp lng="ja">
    <trd>&#x53CD;&#x5FDC;&#x88C5;&#x7F6E;  <pr>&#x306F;&#x3093;&#x306E;&#x3046;&#x305D;&#x3046;&#x3061;</pr></trd>,
    <trd>&#x539F;&#x5B50;&#x7089;  <pr>&#x3052;&#x3093;&#x3057;&#x308D;</pr></trd>,
    <trd>&#x30B8;&#x30A7;&#x30C3;&#x30C8;&#x30A8;&#x30F3;&#x30B8;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">reactor</trd>
  <trd lng="pl">reaktor</trd>
  <trd lng="ro">reactor</trd>
  <trdgrp lng="sk">
    <trd>reaktor</trd>,
    <trd>tryskov&yacute; motor</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_a;&#x43A;&c_t;&#x43E;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_a;&#x43A;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j; &c_d;&c_v;&c_i;&c_g;&c_u;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53CD;&#x5E94;&#x7089; <pr>f&#x1CE;ny&#xEC;ngl&#xFA;</pr></trd>,
    <trd>&#x53CD;&#x61C9;&#x7210; <pr>f&#x1CE;ny&#xEC;ngl&#xFA;</pr></trd>
  </trdgrp>
</drv>
  </art>

