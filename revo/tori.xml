<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tori</rad>/o
</kap>
<drv mrk="tori.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 90, atompezo 232,0381, simbolo Th. &Ccirc;iuj
      izotopoj de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Thorium</trd>
  <trd lng="hu">t&oacute;rium</trd>
  <trd lng="nl">thorium</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
