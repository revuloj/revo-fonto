<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>tori</rad>/o
</kap>
<drv mrk="tori.0o">
   <kap><tld/>o</kap>
   <mlg>Th</mlg>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
       atomnumero 90, atompezo 232,0381, simbolo <frm>Th</frm>. &Ccirc;iuj
       <ref tip="super" cel="izotop.0o">izotopoj</ref> 
       de tiu elemento estas radioaktivaj.
     </dif>
   </snc>                       
   <trd lng="be">&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
   <trd lng="br">toriom</trd>
   <trd lng="ca">tori</trd>
   <trd lng="de">Thorium</trd>
   <trd lng="en">thorium</trd>
   <trd lng="es">torio</trd>
   <trd lng="fr">thorium</trd>
   <trd lng="hu">t&oacute;rium</trd>
   <trd lng="it">torio</trd>
   <trd lng="la">Thorium</trd>
   <trd lng="nl">thorium</trd>
   <trd lng="pl">tor</trd>
   <trd lng="pt">t&oacute;rio</trd>
   <trd lng="ru">&c_t;&c_o;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
