<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tori.xml,v 1.20 2018/11/05 07:10:20 revo Exp $">
<kap>
   <rad>tori</rad>/o
</kap>
<drv mrk="tori.0o">
   <kap><tld/>o</kap>
   <mlg kod="IUPAC">Th</mlg>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
       lst="voko:&hcirc;emiaj_elementoj" val="90">elemento</ref> kun
       atomnumero 90, atompezo 232,0381, kies &ccirc;iuj
       <ref tip="super" cel="izotop.0o">izotopoj</ref> 
       estas radioaktivaj:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr>estas ar&gcirc;ent-kolora radioaktiveta elemento,
        utiligita en lumiloj, esplorado estas iniciatata por uzi &gcirc;in kiel
        brula&jcirc;o de estontaj nukleaj centraloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28705">Torio</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_t;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="br">toriom</trd>
  <trd lng="ca">tori</trd>
  <trd lng="cs">thorium</trd>
  <trd lng="de">Thorium</trd>
  <trd lng="en">thorium</trd>
  <trd lng="es">torio</trd>
  <trd lng="fr">thorium</trd>
  <trd lng="hu">t&oacute;rium</trd>
  <trd lng="id">torium</trd>
  <trd lng="it">torio</trd>
  <trd lng="la">Thorium</trd>
  <trd lng="nl">thorium</trd>
  <trd lng="pl">tor</trd>
  <trd lng="pt">t&oacute;rio</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Th</trd>,
    <trd>t&oacute;rium</trd>
  </trdgrp>
  <trd lng="sv">torium</trd>
</drv>
</art>
