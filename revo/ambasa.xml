<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ambasa.xml,v 1.24 2021/05/28 12:11:18 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>ambasad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ambasa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc mrk="ambasa.0o.delegitaro">
    <dif>
      Ambasadora <ref tip="super" cel="deleg.0itaro">delegitaro</ref>:
      <ekz>
        kanada <tld/>o promesis al polaj enmigrontoj rekonsideron de
        iliaj
        petoj post 8 jaroj<fnt><aut>Remigiusz Mielcarek</aut>:
        <vrk>Enmigrintoj en ora ka&gcirc;o</vrk>, <bib>Monato</bib><lok>jaro
        1994a, numero 1a, p. 15a</lok></fnt>;
      </ekz>
      <ekz>
        la&ubreve; informoj de la litova <tld/>o en Britio, komence de 2000 en tiu lando
        ju&gcirc;proceson atendas kvar kriminalaferoj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006830.html">Litovio &dash; Britio: Kontrabanditaj cigaredoj invadas Brition</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_m;&c_b;&c_a;&c_s;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ambaixada</trd>,
    <trd>missi&oacute; diplom&agrave;tica</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ambas&aacute;da</trd>,
    <trd>velvyslanectv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>diplomatische <ind>Gesandtschaft</ind></trd>,
    <trd>Botschafter<klr> [pl]</klr></trd>
  </trdgrp>
  <trd lng="el">&pi;&rho;&epsilon;&sigma;&beta;&epsilon;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>embassy</trd>,
    <trd>diplomatic mission</trd>
  </trdgrp>
  <trd lng="es">embajada</trd>
  <trdgrp lng="fr">
    <trd>ambassade<klr> (mission diplomatique)</klr></trd>,
    <trd>mission <ind>diplomatique</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5E9;&#x5D2;&#x5E8;&#x5D9;&#x5E8;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">nagyk&ouml;vets&eacute;g</trd>
  <trd lng="nl">ambassade</trd>
  <trd lng="pt">embaixada <klr tip="amb">(deputa&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ve&lcaron;vyslanectvo</trd>,
    <trd>vyslanectvo <klr>(person&aacute;l)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="ambasa.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="ambasa.0ejo.ambasadorejo">
    <dif>
      <ref tip="dif" cel="ambasa1.0ejo">Ambasadorejo</ref>:
      <ekz>
        landoj, kiuj diplomatie rilatas kun la Sankta Se&gcirc;o, havas sian propran
        <tld/>ejon en Romo
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="https://www.monato.be/2009/009739.php">Vatikano diplomatie rilatas kun 177 landoj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejo de Bosnio kaj Hercegovino ne akceptis la peton peri la
        dokumentojn<fnt><aut>Spomenka &Scirc;timec</aut>: <vrk>Saluton el
        Absurdistano</vrk>, <bib>Monato</bib><lok>jaro 1993a, numero 12a,
        p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_m;&c_b;&c_a;&c_s;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF5E;&#xF74;&#xF44;&#xF0B;&#xF5A;&#xF56;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trd lng="ca">ambaixada</trd>
  <trd lng="cs">s&iacute;dlo velvyslanectv&iacute;</trd>
  <trd lng="de">Botschaft</trd>
  <trd lng="en">embassy</trd>
  <trd lng="es">embajada</trd>
  <trd lng="fr">ambassade<klr> (lieu)</klr></trd>
  <trd lng="he">&#x5E9;&#x5D2;&#x5E8;&#x5D9;&#x5E8;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">nagyk&ouml;vets&eacute;g (&eacute;p&uuml;lete)</trd>
  <trd lng="nl">ambassade</trd>
  <trd lng="pt">embaixada <klr tip="amb">(sede)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>s&iacute;dlo ve&lcaron;vyslanectva</trd>,
    <trd>ve&lcaron;vyslanectvo</trd>
  </trdgrp>
</drv>
</art>
