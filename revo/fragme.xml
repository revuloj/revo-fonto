<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fragment</rad>/o
</kap>
<drv mrk="fragme.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rompopeco de objekto:
      <ekz>
        <tld/>o de vazo, vitro, osto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Fragment</trd>,
      <trd>Bruchst&uuml;ck</trd>,
      <trd>Scherbe</trd>,
      <trd>Splitter</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_k;&c_o;&c_l;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Aparta parto, peco de literatura verko:
      <ekz>
        citi nur <tld/>ojn de poemo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="er.0o">ero</ref>,
      <ref cel="split.0o">splito</ref>,
      <ref cel="stump.0o">stumpo</ref>.
    </refgrp>
    <trd lng="de">Fragment</trd>
    <trdgrp lng="ru">
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>fragment</trd>,
    <trd>bribe</trd>,
    <trd>extrait<klr tip="ind"> (d'une &oelig;uvre)</klr></trd>,
    <trd>morceau</trd>,
    <trd>passage<klr tip="ind"> (d'une &oelig;uvre)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="fragme.0i">
  <kap><tld/>(ig)i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi en <tld/>ojn:
      <ekz>
        <tld/>igi la malfacila&jcirc;ojn por pli facile ilin solvi.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>fragmentieren</trd>,
    <trd>zerst&uuml;ckeln</trd>,
    <trd>zersplittern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fragmenter</trd>,
    <trd>morceler</trd>,
    <trd>fractionner</trd>
  </trdgrp>
</drv>
<drv mrk="fragme.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Disigita en apartajn, ne sufi&ccirc;e kunligitajn partojn;
      al kiu mankas unueco:
      <ekz>
        <tld/>aj konoj;
      </ekz>
      <ekz>
        <tld/>a (hakita) frazo, stilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>fragmentarisch</trd>,
    <trd>bruchst&uuml;ckhaft</trd>,
    <trd>unzusammenh&auml;ngend</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fragmentaire</trd>,
    <trd>morcel&eacute;<klr tip="ind"> (partiel)</klr></trd>,
    <trd>partiel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
