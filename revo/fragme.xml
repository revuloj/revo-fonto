  <rad>fragment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fragme.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fragme.0o.rompopeco">
    <dif>
      Rompo<ref tip="super" cel="pec.0o.rompajxo">peco</ref> de objekto:
      <ekz>
        <tld/>o de vazo, vitro, osto;
      </ekz>
      <ekz>
        li benis kaj dispecigis la panojn, kaj <klr>[&#x2026;]</klr> &ccirc;iuj man&gcirc;is kaj sati&gcirc;is
        kaj oni kolektis da postrestintaj <tld/>oj dek du plenajn korbojn
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 14:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj dishakis la &scirc;tonegojn, aliaj &jcirc;etis la <tld/>ojn en la kanalon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni venigis statuojn kaj grandegajn <tld/>ojn por la konstrua&jcirc;oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        elrigardis nudaj alta&jcirc;oj, kovritaj de &scirc;tonegaj <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        fendi la arbon por havi <tld/>ojn por la kuirejo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        ser&ccirc;ante &ccirc;iam la &scirc;el<tld/>ojn de la bombo, la <nom>GPU</nom> trovis en la maro libron
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj pensoj <klr>[&#x2026;]</klr> konsistis nur el <tld/>oj el delonge malaperinta tempo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj <tld/>oj de interparolado inter kvar junaj sinjoroj <klr>[&#x2026;]</klr> faris
        impreson de ia <klr>[&#x2026;]</klr> pepado
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_s;&c_k;&c_je;&c_p;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Fragment</trd>,
      <trd>Bruchst&uuml;ck</trd>,
      <trd>Scherbe</trd>,
      <trd>Splitter</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>splinter</trd>,
      <trd>bit</trd>,
      <trd>shred</trd>,
      <trd>scrap</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;red&eacute;k</trd>
    <trd lng="nl">fragment</trd>
    <trdgrp lng="pl">
      <trd>fragment</trd>,
      <trd>wyimek</trd>,
      <trd>urywek</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_k;&c_o;&c_l;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x788E;&#x5757; <pr>su&#xEC;ku&#xE0;i</pr></trd>,
    <trd>&#x8640; <pr>j&#x12B;</pr></trd>,
    <trd>&#x9F4F; <pr>j&#x12B;</pr></trd>,
    <trd>&#x65AD;&#x7247; <pr>du&#xE0;npi&#xE0;n</pr></trd>,
    <trd>&#x7247;&#x6BB5; <pr>pi&#xE0;ndu&#xE0;n</pr></trd>,
    <trd>&#x5FAE;&#x5C11; <pr>w&#x113;ish&#x1CE;o</pr></trd>,
    <trd>&#x781F; <pr>zh&#x1CE;</pr></trd>,
    <trd>&#x788E;&#x7247; <pr>su&#xEC;pi&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="fragme.0o.verkoparto">
    <dif>
      Aparta <ref tip="super" cel="part.0o.KOMUNE">parto</ref>, peco de verko:
      <ekz>
        <klr>[jen]</klr> <tld/>o el la anta&ubreve;parolo de 
        fratoj <nom>Grimm</nom> <klr>[&#x2026;]</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Petronius</nom> tamen, kiu havis mirindan memoron, komencis ripeti <tld/>ojn el la
        himno, citi apartajn versojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la cezaro legis en malgranda rondo <tld/>ojn el sia <nom>Troica</nom>
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj plej &scirc;atataj motivoj fari&gcirc;is <tld/>oj el &leftquot;Fa&ubreve;sto&rightquot;
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_u;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_d;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Fragment</trd>,
      <trd>Auszug</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&eacute;szlet</trd>,
      <trd>szemelv&eacute;ny</trd>
    </trdgrp>
    <trd lng="nl">fragment</trd>
    <trd lng="pt">fragmento</trd>
    <trdgrp lng="ru">
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="er.0o">ero</ref>,
    <ref cel="split.0o">splito</ref>,
    <ref cel="stump.0o">stumpo</ref>
  </refgrp>
  <trdgrp lng="en">
    <trd>fragment</trd>,
    <trd>piece</trd>
  </trdgrp>
  <trd lng="es">fragmento</trd>
  <trdgrp lng="fr">
    <trd>fragment</trd>,
    <trd>bribe</trd>,
    <trd>extrait<klr tip="ind"> (d'une &oelig;uvre)</klr></trd>,
    <trd>morceau</trd>,
    <trd>passage<klr tip="ind"> (d'une &oelig;uvre)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3051;&#x3089;</trd>,
    <trd>&#x7834;&#x7247;  <pr>&#x306F;&#x3078;&#x3093;</pr></trd>,
    <trd>&#x65AD;&#x7247;  <pr>&#x3060;&#x3093;&#x307A;&#x3093;</pr></trd>,
    <trd>&#x65AD;&#x7AE0;  <pr>&#x3060;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4E00;&#x7BC0;  <pr>&#x3072;&#x3068;&#x3075;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zlomok</trd>,
    <trd>&uacute;ryvok</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fragment</trd>,
    <trd>pas&aacute;&zcaron;</trd>,
    <trd>v&yacute;&ncaron;atek</trd>,
    <trd>zlomek</trd>,
    <trd>&uacute;ryvek</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_a;&c_gu;&#x43C;&c_je;&#x43D;&c_t;</trd>,
    <trd>&c_u;&c_r;&c_i;&c_v;&#x43E;&#x43A;</trd>,
    <trd>&#x43E;&c_s;&#x43A;&#x43E;&#x43B;&#x43E;&#x43A;</trd>,
    <trd>&c_u;&#x43B;&c_a;&#x43C;&#x43E;&#x43A;</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_t;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fragme.0i">
  <kap><tld/>i, <var><kap><tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="dis.0igi">Disigi</ref> en <tld/>ojn:
      <ekz>
        <tld/>igi la malfacila&jcirc;ojn por pli facile ilin solvi;
      </ekz>
      <ekz>
        la junulara E-movado en Italio <klr>[&#x2026;]</klr> aspektas tre <tld/>ita
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; la bildo devus trakuri kilometrojn longajn nervofibrojn &gcirc;is lia cerbo,
        kie la <tld/>igitaj impulsoj unu post alia denove devis rekombini&gcirc;i en bildon
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detal.0igi">detaligi</ref>,
      <ref cel="pec.dis0igi">dispecigi</ref>,
      <ref cel="divid.0i.partigi">dividi</ref>,
      <ref cel="er.0igi">erigi</ref>,
      <ref cel="part.0igi">partigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozdrobovat</trd>,
    <trd>roz&ccaron;le&ncaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fragmentieren</trd>,
    <trd>zerst&uuml;ckeln</trd>,
    <trd>zersplittern</trd>
  </trdgrp>
  <trd lng="en">fragment</trd>
  <trd lng="es">fragmentar</trd>
  <trdgrp lng="fr">
    <trd>fragmenter</trd>,
    <trd>morceler</trd>,
    <trd>fractionner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tt&ouml;rdel</trd>,
    <trd>r&eacute;szekre bont</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BF8;&#x65AD;&#x3059;&#x308B;  <pr>&#x3059;&#x3093;&#x3060;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3070;&#x3089;&#x3070;&#x3089;&#x306B;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">fragmenteren</trd>
  <trdgrp lng="pl">
    <trd>dzieli&cacute; na cz&eogonek;&sacute;ci</trd>,
    <trd>roz&lstroke;o&zdot;y&cacute; na cz&eogonek;&sacute;ci</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">roz&ccaron;le&ncaron;ova&tcaron;</trd>
  <trd lng="uk">&c_d;&c_ib;&#x43B;&c_i;&c_t;&c_i; &#x43D;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_i;</trd>
</drv>

<drv mrk="fragme.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Disigita en apartajn, ne sufi&ccirc;e kunligitajn partojn;
      al kiu mankas unueco:
      <ekz>
        <tld/>aj, supra&jcirc;aj, ne precize komprenitaj scioj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        izolita detalo, kvankam plej arte ellaborita, povas esti interesa kiel <tld/>a
        studobjekto
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detal.0a">detala</ref>,
      <ref cel="part.0a.KOMUNE">parta</ref>,
      <ref cel="op.unu0a">unuopa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_r;&c_y;&c_w;&c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fragment&aacute;rn&iacute;</trd>,
    <trd>zlomkovit&yacute;</trd>,
    <trd>&uacute;ryvkovit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fragmentarisch</trd>,
    <trd>bruchst&uuml;ckhaft</trd>,
    <trd>unzusammenh&auml;ngend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fragmentary</trd>,
    <trd>fragmented</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fragmentaire</trd>,
    <trd>morcel&eacute;<klr tip="ind"> (partiel)</klr></trd>,
    <trd>partiel</trd>
  </trdgrp>
  <trd lng="hu">t&ouml;red&eacute;kes</trd>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x7247;&#x7684;&#x306A;  <pr>&#x3060;&#x3093;&#x307A;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x3070;&#x3089;&#x3070;&#x3089;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">fragmentarisch</trd>
  <trdgrp lng="pl">
      <trd>fragmentaryczny</trd>,
      <trd>cz&aogonek;stkowy</trd>,
      <trd>urywkowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">zlomkovit&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_a;&c_gu;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x788E; <pr>su&#xEC;</pr></trd>,
    <trd>&#x96F6;&#x661F; <pr>l&#xED;ngx&#x12B;ng</pr></trd>,
    <trd>&#x767E;&#x601D;&#x4E0D;&#x89E3; <pr>b&#x1CE;is&#x12B;b&#xF9;ji&#x11B;</pr></trd>,
    <trd>&#x652F;&#x79BB;&#x7834;&#x788E; <pr>zh&#x12B;l&#xED;p&#xF2;su&#xEC;</pr></trd>,
    <trd>&#x661F;&#x661F;&#x70B9;&#x70B9; <pr>x&#x12B;ngx&#x12B;ngdi&#x1CE;ndi&#x1CE;n</pr></trd>,
    <trd>&#x96F6;&#x788E; <pr>l&#xED;ngsu&#xEC;</pr></trd>,
    <trd>&#x9593;&#x65B7;&#x5730; <pr>ji&#xE0;ndu&#xE0;nde</pr></trd>
  </trdgrp>
</drv>
