  <rad>sss</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sss.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="sss.0.SON">
