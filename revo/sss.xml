        <ctl><tld/>! <tld/>!</ctl> <klr>...</klr>
        la pomoj komencis rosti&gcirc;i
        <fnt>
          <bib>Fab4</bib>,
          <vrk>Kion la patro faras, estas &ccirc;iam &gcirc;usta</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;-&c_sh;-&c_sh;!</trd>
  <trd lng="en">shh</trd>
  <trd lng="fr">tchchch !</trd>
</drv>
