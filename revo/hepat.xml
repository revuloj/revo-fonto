<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hepat</rad>/o
</kap>
<drv mrk="hepat.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Granda glando, kiu produktas galon kaj glukozon:
      <ekz>
        &ccirc;e la homo la <tld/>o trovi&gcirc;as en la supera parto de
        la dekstra flanko de la abdomeno;
      </ekz>
      <ekz>
        oleo el <tld/>o de moruo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Leber</trd>
  <trd lng="en">liver</trd>
  <trd lng="fr">foie<klr tip="ind"> (organe)</klr></trd>
  <trd lng="nl">lever</trd>
  <trd lng="pl">w&aogonek;troba</trd>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_je;&c_n;&c_mol;</trd>
  <trd lng="tr">&ccedil;i&gbreve;er</trd>
</drv>

<drv mrk="hepat.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="H"/>o de iuj bestoj uzata kiel nutra&jcirc;o:
      <ekz>
        <tld/>a&jcirc;o de bovido;
      </ekz>
      <ekz>
        <ind>gras<tld/>a&jcirc;o</ind>
        <klr>(<tld/>o de ansero a&ubreve; anaso, tre grasigita per speciala
        nutra&jcirc;o kaj plena senmoveco)</klr>;
        <trd lng="fr">foie gras</trd>
      </ekz>
      <ekz>
        paste&ccirc;o de gras<tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Leber</trd>
  <trd lng="en">liver</trd>
  <trd lng="fr">foie<klr tip="ind"> (comme aliment)</klr></trd>
  <trd lng="nl">lever<klr tip="ind"> (gerecht)</klr></trd>
  <trd lng="pl">w&aogonek;tr&oacute;bka</trd>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_jo;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
