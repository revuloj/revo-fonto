<vortaro>
<art mrk="$Id: percep.xml,v 1.17 2022/03/25 12:53:16 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>percept</rad>/o
</kap>
<drv mrk="percep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Kono, kiun ni akiras pri la ekstera mondo pere de niaj kvin
      sentumorganoj a&ubreve; pri ni mem pere de la konscio.
    </dif>
    <rim>
      Ne konfuzu kun <em>koncepto</em>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_e;&c_r;&c_c;&c_e;&c_p;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;ijet&iacute;</trd>,
    <trd>vjem</trd>,
    <trd>vn&iacute;m&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Wahrnehmung</trd>
  <trd lng="en">perception</trd>
  <trd lng="es">percepci&oacute;n</trd>
  <trd lng="fr">perception</trd>
  <trdgrp lng="hu">
    <trd>&eacute;szlelet</trd>,
    <trd>percepci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>waarneming</trd>,
    <trd>gewaarwording</trd>
  </trdgrp>
  <trd lng="pl">percepcja</trd>
  <trd lng="pt">percep&ccedil;&atilde;o</trd>
  <trd lng="ro">percep&#x21B;ie</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>prijatie</trd>,
    <trd>vnem</trd>,
    <trd>vn&iacute;manie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43F;&#x440;&#x438;&#x439;&#x43D;&#x44F;&#x442;&#x442;&#x44F;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x446;&#x435;&#x43F;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="percep.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havi <tld/>on pri io:
      <ekz>
        apena&ubreve; <tld/>ebla klini&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>pochopit</trd>,
    <trd>vn&iacute;mat</trd>
  </trdgrp>
  <trd lng="de">wahrnehmen</trd>
  <trd lng="en">perceive</trd>
  <trd lng="es">percibir</trd>
  <trd lng="fr">percevoir</trd>
  <trd lng="hu">&eacute;szlel</trd>
  <trdgrp lng="nl">
    <trd>waarnemen</trd>,
    <trd>gewaar worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>postrzega&cacute;</trd>,
    <trd>spostrzega&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pricepe</trd>,
    <trd>p&abreve;trunde</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pochopi&tcaron;</trd>,
    <trd>vn&iacute;ma&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43F;&#x440;&#x438;&#x439;&#x43C;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x43E;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
