<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompozit.xml,v 1.9 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>kompozit</rad>/o
</kap>
<drv mrk="kompozit.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompozit.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kunmetita <ref tip="super" cel="materi1.0o.substanco">materialo</ref>
      farita el fibroj tegitaj per <ref tip="prt" cel="rezin.0o.KEM">rezino</ref>:
      <ekz>
        oni pretigas el tiuj <tld/>oj e&ccirc; boatojn a&ubreve; korektas la
        karoserion de la a&ubreve;tomobilo<klr>j</klr>
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;poliestero&rightquot;,
        2007-01-22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kompozit.0o.RIM_HOM">
    <ref tip="hom" cel="kompoz1.0oj">Kompozitoj</ref> estas
    eksa nomo de vegeta&jcirc;a familio asteracoj.
  </rim>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_p;&c_a;&c_z;&c_y;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_a;&c_z;&c_y;&c_c;&c_y;&c_j;&c_n;&c_y; <ind>&c_m;&c_a;&c_t;&c_a;&c_r;&#x2019;&c_ja;&c_l;</ind></trd>
  </trdgrp>
  <trd lng="ca">comp&ograve;sit</trd>
  <trd lng="en">composite material</trd>
  <trd lng="es">material compuesto</trd>
  <trd lng="fr">mat&eacute;riau <ind>composite</ind></trd>
  <trd lng="it">materiale <ind>composito</ind></trd>
  <trd lng="pl">kompozyt</trd>
  <trd lng="pt">material <ind>composto</ind></trd>
  <trd lng="ro">compozit</trd>
  <trdgrp lng="sk">
    <trd>kompozitum</trd>,
    <trd>zlo&zcaron;en&eacute; slovo</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kompozitum</trd>,
    <trd>slo&zcaron;enina</trd>,
    <trd>slo&zcaron;en&eacute; slovo</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x590D;&#x5408;&#x6750;&#x6599; <pr>f&#xF9;h&#xE9;c&#xE1;ili&#xE0;o</pr></trd>,
    <trd>&#x6DF7;&#x5408;&#x7269; <pr>h&#xF9;nh&#xE9;w&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>

