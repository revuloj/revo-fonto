<kap>
  <rad>direktric</rad>/o
</kap>
<drv mrk="direkt3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="direkt3.0o.linio">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="dif" cel="direkt.0anto.linio">Direktanto<sncref/></ref>:
      <ekz><tld/>o de konuso.</ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_ib;&c_r;&c_o;&c_w;&c_v;&c_a;&c_ju;&c_ch;&c_a;&c_ja;</trd>
    </trdgrp>
    <trd lng="pl">kieruj&aogonek;ca</trd>
  </snc>
  <snc mrk="direkt3.0o.koniko">
    <fnt><bib>MatTerm</bib><lok>p. 34</lok></fnt>
    <ref tip="dif" cel="direkt.0anto.koniko">direktanto<sncref/></ref>
  <trd lng="be">&c_d;&c_y;&c_r;&c_e;&c_k;&c_t;&c_r;&c_y;&c_s;&c_a;</trd>
    <trd lng="pl">kierownica</trd>
  </snc>
  <trd lng="cs">sm&#x11B;rnice <klr>(p&rcaron;&#xED;mky)</klr></trd>
  <trd lng="de">Direktrix</trd>
  <trd lng="ja">&#x6E96;&#x7DDA;  <pr>&#x3058;&#x3085;&#x3093;&#x305B;&#x3093;</pr></trd>
  <trd lng="pt">diretriz</trd>
  <trd lng="sk">smernica <klr>(priamky)</klr></trd>
  <trd lng="uk">&c_d;&c_i;&c_r;&c_je;&#x43A;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>
</drv>
