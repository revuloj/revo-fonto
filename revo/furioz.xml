<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: furioz.xml,v 1.25 2022/06/19 15:30:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>furioz</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="furioz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="furioz.0a.PSI">
    <dif>
      Pasie, freneze, senbride kolera:
      <ekz>
        <tld/>a homo, hundo, ago.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frenez.0a.pro_pasio">freneza<sncref/></ref>,
      <ref cel="rabi.0a">rabia</ref>,
      <ref cel="sovagx.0a.intensa">sova&gcirc;a<sncref/></ref>.
    </refgrp>
  </snc>
  <trd lng="lat">rabidus, a, um (rabies)</trd>
  <trdgrp lng="be">
    <trd>&c_l;&c_ju;&c_t;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_ju;&c_sh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_sh;&c_a;&c_l;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_b;&c_u;&c_ja;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&iacute;t&yacute;</trd>,
    <trd>nep&rcaron;&iacute;&ccaron;etn&yacute;</trd>,
    <trd>rozkacen&yacute;</trd>,
    <trd>rozl&iacute;cen&yacute;</trd>,
    <trd>rozl&iacute;cen&yacute;</trd>,
    <trd>zb&ecaron;sil&yacute;</trd>,
    <trd>zu&rcaron;iv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>w&uuml;tend</trd>,
    <trd>rasend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>furious</trd>,
    <trd>raging</trd>
  </trdgrp>
  <trd lng="es">furioso</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;chain&eacute;<klr tip="ind"> (furieux)</klr></trd>,
    <trd>enrag&eacute;<klr tip="ind"> (furieux)</klr></trd>,
    <trd>furieux</trd>,
    <trd>en <ind>furie</ind></trd>,
  </trdgrp>
  <trd lng="hu">&odblac;rj&ouml;ng&odblac;</trd>
  <trd lng="nl">razend</trd>
  <trdgrp lng="pl">
    <trd>w&sacute;ciek&lstroke;y</trd>,
    <trd>rozjuszony</trd>,
    <trd>zapami&eogonek;ta&lstroke;y</trd>,
    <trd>szalony</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furioso</trd>,
    <trd>raivoso</trd>,
    <trd>fulo</trd>,
    <trd>irado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_ja;&c_r;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_ja;&c_r;&c_jo;&c_n;&c_n;&c_y;&c_j;
    </trd>,
    <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>besn&yacute;</trd>,
    <trd>rozz&uacute;ren&yacute;</trd>,
    <trd>z&uacute;riv&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_o;&c_z;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="furioz.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="furioz.0eco.PSI">
    <dif>
      Stato de iu <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_l;&c_jo;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ju;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_ju;&c_sh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vzteklost</trd>,
    <trd>zu&rcaron;ivost</trd>
  </trdgrp>
  <trd lng="de">Raserei</trd>
  <trdgrp lng="en">
    <trd>fury</trd>,
    <trd>rage</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fureur</trd>,
    <trd>furie<klr tip="ind"> (acc&egrave;s de col&egrave;re)</klr>
    </trd>,
    <trd>rage<klr tip="ind"> (fureur)</klr></trd></trdgrp>
  <trd lng="hu">&odblac;rj&ouml;ng&eacute;s</trd>
  <trd lng="nl">razernij</trd>
  <trdgrp lng="pl">
    <trd>w&sacute;ciek&lstroke;o&sacute;&cacute;</trd>,
    <trd>sza&lstroke;</trd>,
    <trd>zapami&eogonek;tanie</trd>,
    <trd>napad <ind>sza&lstroke;u</ind></trd>,
    <trd>napad <ind>w&sacute;ciek&lstroke;o&sacute;ci</ind></trd>,
    <trd>napad <ind>furii</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bes</trd>,
    <trd>z&uacute;rivos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44E;&c_t;&#x44C;</trd>,
    <trd>&c_sh;&c_a;&#x43B;&c_je;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x43D;&c_je;&c_s;&c_a;&#x43C;&#x43E;&c_v;&c_i;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43E;&c_s;&#x43A;&c_a;&c_zh;&c_je;&#x43D;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_s;&#x43A;&c_a;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="furioz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="furioz.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        la komercisto <tld/>is;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ventego, la batalo, la persekutoj <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vztekat se ,, l&aacute;te&rcaron;it</trd>,
    <trd>&rcaron;&aacute;dit</trd>
  </trdgrp>
  <trd lng="de">rasen</trd>
  <trdgrp lng="de">
      <trd>rasen</trd>,
      <trd>w&uuml;ten</trd>,
      <trd>ausflippen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be furious</trd>,
    <trd>rage</trd>
  </trdgrp>
  <trd lng="es">estar <ind>furioso</ind></trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>furieux</ind></trd>,
    <trd>&ecirc;tre <ind>d&eacute;chain&eacute;</ind></trd>,
    <trd>&ecirc;tre <ind>enrag&eacute;</ind></trd>,
    <trd>faire <ind>rage</ind></trd></trdgrp>
  <trd lng="hu">&odblac;rj&ouml;ng</trd>
  <trdgrp lng="pl">
    <trd>w&sacute;cieka&cacute; si&eogonek;</trd>,
    <trd>szale&cacute;</trd>,
    <trd>wpada&cacute; w <ind>sza&lstroke;</ind></trd>,
    <trd>wpada&cacute; we
    <ind>w&sacute;ciek&lstroke;o&sacute;&cacute;</ind></trd>,
    <trd>wpada&cacute; w <ind>furi&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_ja;&c_r;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyv&aacute;dza&tcaron;</trd>,
    <trd>z&uacute;ri&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&#x43B;&c_je;&#x43D;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_a;&c_zh;&c_je;&#x43D;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="furioz.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="furioz.0igi.PSI">
    <dif>
      Igi <tld/>a, spiti, ekscitegi:
      <ekz>
        li vidu, &ccirc;u lia atako kapablas neniigi
        tion, kio lin <tld/>igas
        <fnt>
          <aut>trad. I. Chiussi</aut>,
          <vrk>La Nobla Korano</vrk>,
          <lok>1970</lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="cs">rozzu&rcaron;it</trd>
  <trd lng="fr">mettre en <ind>fureur</ind></trd>
  <trdgrp lng="sk">
    <trd>rozbesni&tcaron;</trd>,
    <trd>rozz&uacute;ri&tcaron;</trd>,
    <trd>roz&ccaron;erti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&#x43B;&#x44E;&c_t;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43B;&#x44E;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_d;&#x43E; &c_s;&#x43A;&c_a;&c_z;&c_u;</trd>
  </trdgrp>
</drv>
</art>
