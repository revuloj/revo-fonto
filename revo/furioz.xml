<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>furioz</rad>/a
</kap>
<drv mrk="furioz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Pasie, freneze, senbride kolera:
      <ekz><tld/>a homo, hundo, ago.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sovagx.0a">sova&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_ja;&c_r;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_ja;&c_r;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="furioz.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de iu <tld/>a. 
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
<drv mrk="furioz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>la komercisto <tld/>is <klr>(f)</klr>;</ekz>
      <ekz>la ventego, la batalo, la persekutoj <tld/>as.</ekz>
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_ja;&c_r;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
