<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>furioz</rad>/a
</kap>
<drv mrk="furioz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Pasie, freneze, senbride kolera:
      <ekz><tld/>a homo, hundo, ago.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frenez.0a">freneza</ref>,
      <ref cel="rabi.0a">rabia</ref>,
      <ref cel="sovagx.0a">sova&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trd lng="de">rasend</trd>
  <trdgrp lng="fr">
    <trd>furieux</trd>,
    <trd>en <ind>furie</ind></trd>,
    <trd>d&eacute;cha&icirc;n&eacute;<klr tip="ind"> (furieux)</klr></trd>,
    <trd>enrag&eacute;<klr tip="ind"> (furieux)</klr></trd></trdgrp> 
  <trd lng="nl">razend</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_ja;&c_r;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_ja;&c_r;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="furioz.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de iu <tld/>a. 
    </dif>
  </snc>
  <trd lng="de">Raserei</trd>
  <trdgrp lng="fr">
    <trd>fureur</trd>,
    <trd>furie<klr tip="ind"> (acc&egrave;s de col&egrave;re)</klr></trd>,
    <trd>rage<klr tip="ind"> (fureur)</klr></trd></trdgrp> 
  <trd lng="nl">razernij</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
<drv mrk="furioz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>la komercisto <tld/>is;</ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ventego, la batalo, la persekutoj <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">rasen</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>furieux</ind></trd>,
    <trd>&ecirc;tre <ind>d&eacute;cha&icirc;n&eacute;</ind></trd>,
    <trd>&ecirc;tre <ind>enrag&eacute;</ind></trd>,
    <trd>faire <ind>rage</ind></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_i;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_ja;&c_r;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
