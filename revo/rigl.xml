<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rigl.xml,v 1.25 2024/04/14 05:46:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rigl</rad>/i
</kap>
<drv mrk="rigl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senmovebligi <ref cel="pord.0o">pordon</ref>, 
      fenestron, klapon a&ubreve; kovrilon per speciala
      ilo konsistanta el fortika metala peco &scirc;ovebla en truon:
      <ekz>
        Grenjo per forta pu&scirc;o enigis &scirc;in profunde kaj <tld/>is la feran pordon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Rozulino iris kaj <ind>mal<tld/>i</ind>s la pordon pensante, ke &scirc;i ekvidos malri&ccirc;ulon,
        sed ekstere staris urso kaj etendis al &scirc;i sian nigran kapon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>.
        <trd lng="de">entriegeln</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_z;&c_a;&c_s;&c_a;&c_w;&c_k;&c_u;, &c_z;&c_a;&c_v;&c_a;&c_l;&c_u;)</klr></trd>
  <trdgrp lng="cs">
    <trd>ryglovat</trd>,
    <trd>r&yacute;glovat</trd>,
    <trd>zav&iacute;rat na z&aacute;str&ccaron;ku</trd>,
    <trd>zav&iacute;rat na z&aacute;voru</trd>
  </trdgrp>
  <trd lng="de">verriegeln</trd>
  <trdgrp lng="en">
    <trd>bolt<klr> (vt, latch)</klr></trd>,
    <trd>latch<klr> (vt, bolt)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>cerrar</ind> con cerrojo</trd>,
    <trd>trancar</trd>,
    <trd>acerrojar</trd>
  </trdgrp>
  <trd lng="fr">verrouiller</trd>
  <trdgrp lng="hu">
    <trd>reteszel</trd>,
    <trd>bereteszel</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5DEE;&#x3057;&#x9320;&#x3092;&#x304B;&#x3051;&#x308B;  <pr>&#x3055;&#x3057;&#x3058;&#x3087;&#x3046;&#x3092;&#x304B;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x304B;&#x3093;&#x306C;&#x304D;&#x3092;&#x304B;&#x3051;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>grendelen</trd>,
    <trd>vergrendelen</trd>,
    <trd>sluiten</trd>
  </trdgrp>
  <trd lng="pl">ryglowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>aferrolhar</trd>,
    <trd>trancar</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_t;&c_mol; &c_n;&c_a; &c_z;&c_a;&c_s;&c_o;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>uzaviera&tcaron; na z&aacute;str&ccaron;ku</trd>,
    <trd>zaviera&tcaron; na z&aacute;voru</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_i;&c_k;&c_a;&c_t;&c_i; &c_n;&c_a; &c_z;&c_a;&c_s;&c_u;&c_v;</trd>,
    <trd>&c_sh;&c_p;&c_ib;&c_n;&c_gu;&c_a;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_f;&c_ib;&c_k;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &c_s;&c_t;&c_o;&c_p;&c_o;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_b;&c_l;&c_o;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9582; <pr>shu&#x101;n</pr></trd>,
    <trd>&#x95E9; <pr>shu&#x101;n</pr></trd>,
    <trd>&#x9396;&#x9582; <pr>su&#x1D2;shu&#x101;n</pr></trd>,
    <trd>&#x9501;&#x95E9; <pr>su&#x1D2;shu&#x101;n</pr></trd>,
    <trd>&#x5957;&#x7D50; <pr>t&#xE0;oji&#xE9;</pr></trd>,
    <trd>&#x5957;&#x7ED3; <pr>t&#xE0;oji&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rigl.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="il.0o">Ilo</ref> por <tld/>i:
      <ekz>
        la meza <tld/>ilo meze de la tabuloj &scirc;ovi&gcirc;u de unu fino &gcirc;is la alia.  
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        urboj estis fortikigitaj per alta muro, per pordegoj kaj <tld/>iloj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 3:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        tenata ka&scirc;ite <klr>[&#x2026;]</klr> malanta&ubreve; seruro kaj <tld/>ilo
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="serur.0o">seruro</ref>,
      <ref cel="sxlos.0ilo">&scirc;losilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_g;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rygl</trd>,
    <trd>r&yacute;gl</trd>,
    <trd>z&aacute;str&ccaron;ka dve&rcaron;&iacute;</trd>,
    <trd>z&aacute;vora</trd>,
    <trd>&scaron;up&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Riegel</trd>
  <trdgrp lng="en">
    <trd>bolt<klr> (n, latch)</klr></trd>,
    <trd>latch<klr> (n, bolt)</klr></trd>
  </trdgrp>
  <trd lng="es">cerrojo</trd>
  <trd lng="fr">verrou</trd>
  <trdgrp lng="hu">
    <trd>retesz</trd>,
    <trd>tol&oacute;z&aacute;r</trd>,
    <trd>rigli</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5DEE;&#x3057;&#x9320;  <pr>&#x3055;&#x3057;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x304B;&#x3093;&#x306C;&#x304D;</trd>
  </trdgrp>
  <trd lng="nl">grendel</trd>
  <trd lng="pl">rygiel</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_o;&c_v;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_v;&c_i;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;vora</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_u;&c_v;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_u;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_p;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9582; <pr>shu&#x101;n</pr></trd>,
    <trd>&#x95E9; <pr>shu&#x101;n</pr></trd>,
    <trd>&#x9396;&#x9582; <pr>su&#x1D2;shu&#x101;n</pr></trd>,
    <trd>&#x9501;&#x95E9; <pr>su&#x1D2;shu&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
