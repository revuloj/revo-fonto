<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rigl</rad>/i
</kap>
<drv mrk="rigl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <dif>
      Senmovebligi pordon, fenestron, klapon a&ubreve; kovrilon per speciala
      ilo
      konsistanta el fortika metala peco &scirc;ovebla en truon.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_z;&c_a;&c_s;&c_a;&c_w;&c_k;&c_u;, &c_z;&c_a;&c_v;&c_a;&c_l;&c_u;)</klr></trd>
  <trdgrp lng="cs">
    <trd>ryglovat</trd>,
    <trd>r&yacute;glovat</trd>,
    <trd>zav&iacute;rat na z&aacute;str&ccaron;ku</trd>,
    <trd>zav&iacute;rat na z&aacute;voru</trd>
  </trdgrp>
  <trd lng="de">verriegeln</trd>
  <trd lng="en">fasten</trd>
  <trdgrp lng="es">
    <trd><ind>cerrar</ind> con cerrojo</trd>,
    <trd>trancar</trd>,
    <trd>acerrojar</trd>
  </trdgrp>
  <trd lng="fr">verrouiller</trd>
  <trdgrp lng="hu">
    <trd>reteszel</trd>,
    <trd>bereteszel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>grendelen</trd>,
    <trd>vergrendelen</trd>,
    <trd>sluiten</trd>
  </trdgrp>
  <trd lng="pl">ryglowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>aferrolhar</trd>,
    <trd>trancar</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_t;&c_mol; &c_n;&c_a; &c_z;&c_a;&c_s;&c_o;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>uzaviera&tcaron; na z&aacute;str&ccaron;ku</trd>,
    <trd>zaviera&tcaron; na z&aacute;voru</trd>
  </trdgrp>
</drv>

<drv mrk="rigl.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5183">rigl'il'</url></lok></fnt></kap>
  <snc>
    <dif>
      Ilo por <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="serur.0o">seruro</ref>,
      <ref cel="sxlos.0ilo">&scirc;losilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_g;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rygl</trd>,
    <trd>r&yacute;gl</trd>,
    <trd>z&aacute;str&ccaron;ka dve&rcaron;&iacute;</trd>,
    <trd>z&aacute;vora</trd>,
    <trd>&scaron;up&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Riegel</trd>
  <trd lng="es">cerrojo</trd>
  <trd lng="fr">verrou</trd>
  <trdgrp lng="hu">
    <trd>retesz</trd>,
    <trd>tol&oacute;z&aacute;r</trd>,
    <trd>rigli</trd>
  </trdgrp>
  <trd lng="nl">grendel</trd>
  <trd lng="pl">rygiel</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_o;&c_v;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_v;&c_i;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;vora</trd>
</drv>
</art>
