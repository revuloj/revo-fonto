<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>ia</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ia1.0i">
   <kap><tld/>i</kap>
   <snc>
     <dif>
       <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel <ref tip="vid"
       cel="azen.0o">azeno</ref>.
     </dif>
   </snc>
   <trd lng="ca">bramar</trd>
   <trd lng="es">rebuznar</trd>
   <trd lng="fr">braire</trd>
   <trd lng="hu">i&aacute;zik</trd>
</drv>

<drv mrk="ia1.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Azena bleko:
       <ekz>
         pli multis la <tld/>oj <klr>...</klr> de la azeno,
         ol la henoj de la &ccirc;evalo
         <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 8a, p.
         435a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="ca">bram</trd>
   <trd lng="es">rebuzno</trd>
   <trd lng="fr">braiment</trd>
   <trd lng="hu">i&aacute;z&aacute;s</trd>
</drv>
</art>

