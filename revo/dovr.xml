<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: dovr.xml,v 1.4 2023/12/29 06:31:45 revo Exp $">
    <kap>
      <rad>Dovr</rad>/o
    </kap>

<drv mrk="dovr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="dovr.0o.norvega">
    <dif>
      Montara loko (<frm>9&deg;15&#8217;E, 61&deg;59&#8217;N</frm>), &ccirc;e la trajnlinio inter Oslo kaj Trondhejmo:
      <ekz>
        maljuna gnomo el Norvegujo, kiu lo&gcirc;as en la malnova roko de <tld/>o kaj posedas
        multe da rokaj kasteloj kaj orminejon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Dovrefjell</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="dover.0o.brita">Dovero a&ubreve; Dovro</ref> estas anka&ubreve; brita havenurbo.
  </rim>
  <trd lng="be">&c_D;&c_o;&c_w;&c_r;&c_e;&c_f;&#8217;&c_je;&c_l;&c_mol; <klr>(&c_g;&c_o;&c_r;&c_y; &c_w; &c_N;&c_a;&c_r;&c_v;&c_e;&c_g;&c_ib;&c_ib;)</klr></trd>
  <trd lng="uk">&c_g;&#x43E;&c_r;&c_i; &c_D;&#x43E;&c_v;&c_r;&c_je;</trd>
</drv>

  </art>

