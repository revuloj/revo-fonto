<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: dovr.xml,v 1.3 2022/03/16 11:42:20 revo Exp $">
    <kap>
      <rad>Dovr</rad>/o
    </kap>

<drv mrk="dovr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="dovr.0o.norvega">
    <dif>
      Montara loko (<frm>9&deg;15&#x2019;E, 61&deg;59&#x2019;N</frm>), &ccirc;e la trajnlinio inter Oslo kaj Trondhejmo:
      <ekz>
        maljuna gnomo el Norvegujo, kiu lo&gcirc;as en la malnova roko de <tld/>o kaj posedas
        multe da rokaj kasteloj kaj orminejon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>.
      </ekz>      
    </dif>
    <trd lng="de">Dovrefjell</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="dover.0o.brita">Dovero a&ubreve; Dovro</ref> estas anka&ubreve; brita havenurbo.
  </rim>
  <trd lng="uk">&#x433;&#x43E;&#x440;&#x438; &#x414;&#x43E;&#x432;&#x440;&#x435;</trd>
</drv>

  </art>

