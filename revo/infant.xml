<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>infanteri</rad>/o
</kap>
<drv mrk="infant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Parto de la militistaro, kiu batalas kaj ordinare transloki&gcirc;as piede. 
    </dif>
  </snc>
  <trd lng="de">Infanterie</trd>
  <trd lng="fr">infanterie</trd>
  <trd lng="nl">infanterie</trd>
  <trd lng="ru">&c_p;&c_je;&c_h;&c_o;&c_t;&c_a;</trd>
  <trd lng="sv">infanteri</trd>
</drv>
<drv mrk="infant.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Soldato de <tld/>o.  
    </dif>
  </snc>
  <trd lng="de">Infanterist</trd>
  <trd lng="fr">fantassin</trd>
  <trd lng="nl">infanterist</trd>
  <trd lng="ru">&c_p;&c_je;&c_h;&c_o;&c_t;&c_i;&c_n;&c_je;&c_c;</trd>
  <trd lng="sv">infanterist</trd>
</drv>
</art>
