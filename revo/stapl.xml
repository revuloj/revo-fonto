<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stapl.xml,v 1.22 2022/05/05 14:43:48 revo Exp $">
<kap>
  <rad>stapl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="stapl.0o">
  <kap><tld/>o</kap>
  <snc mrk="stapl.0o.FORM">
    <dif>
      <ref tip="super" cel="amas.0o">Amaso</ref> da 
      objektoj (precipe konstrumaterialoj, hejtligno
      ktp), metitaj unuj sur aliajn kaj aran&gcirc;itaj en <ref tip="super" cel="vic.0o.spaca">vico</ref>n: 
      <ekz>
        <tld/>o da traboj, tabuloj, brikoj, libroj;
      </ekz>
      <ekz>
        dika <tld/>o da masinskribitaj paper-folioj
        <fnt>
          <vrk><url ref="http://esperanto.es:8080/jspui/bitstream/11013/4496/1/Cervantes_Pri%20la%20libero%2c%20kun%20Don-Ki%C4%A5oto%20donis%20al%20multaj%20malfeli%C4%89uloj_1949.pdf">La Genia Kavaliro Don-Ki&hcirc;oto De Mancujo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        iom rekolektinte la spiron, li ku&scirc;igis unu el la
        lignopecoj surbraken por meti &gcirc;in sur la <tld/>on
        <fnt>
          <aut>Jan Frideg&aring;rd, tr. Ferenc Szil&aacute;gyi</aut>:
          <vrk>La Arbino</vrk>.
          <lok>Vespera ru&gcirc;o anoncas ventegon. Inko Tyres&ouml;,
          2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stak.0o.vico">stako<sncref/></ref>,
      <ref cel="kolon.0o.staplo">kolono<sncref/></ref>
    </refgrp>    
  </snc>
  <snc mrk="stapl.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Dinamika <ref tip="super" cel="datum.0strukturo">datumstrukturo</ref>,
      ofte rekte realigita per <ref cel="proceso.0o">procesor</ref>aj
      komandoj, &ccirc;e kiu oni povas aldoni kaj relegi novajn objektojn nur supre:
      <ekz>
        procesoroj liveritaj sur kartetoj, sur kiuj estas la <tld/>omemoro
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="&Steloj;monato2/007114.html">Intel alfrontas
          konkurencon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la kerno uzis teknologion de ekstervica plenumo de komandoj, novan skemon de
        komutado de <tld/>o kaj nova &ccirc;efbuso
        <fnt>
          <vrk><url ref="&Viki;/Alpha">Alpha</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas memorilo, kiu staras inter procesoro kaj pli granda, sed malpli
        rapida memorilo
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Monato;/2012/010794.php">Distra kaj kleriga vortaro</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="stak.0o.KOMP">stako<sncref/></ref>
    <refgrp tip="vid">
       <ref cel="vic.atendo0o">atendovico</ref>,
       <ref cel="bufr.0o.KOMP">bufro<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Stapelspeicher</trd>,
      <trd>Stack</trd>,
      <trd>Kellerspeicher</trd>
    </trdgrp>
    <trd lng="en">stack</trd>
    <trd lng="fr">pile</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_o;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pila</trd>,
    <trd>munt</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sklad zbo&zcaron;&iacute;</trd>,
    <trd>skladi&scaron;t&ecaron;</trd>,
    <trd>srovnan&aacute; hromada</trd>
  </trdgrp>
  <trd lng="de">Stapel</trd>
  <trdgrp lng="en">
    <trd>pile</trd>,
    <trd>stack</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mont&oacute;n</trd>,
    <trd>dep&oacute;sito</trd>,
    <trd>pila</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>empilement</trd>,
    <trd>pile<klr> (empilement)</klr></trd>
  </trdgrp>
  <trd lng="hu">rak&aacute;s</trd>
  <trd lng="nl">stapel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">sklad pre stohov&yacute; tovar<klr>
      (&ccaron;akaj&uacute;ci na predaj alebo n&aacute;vrat)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x43B;&#x430;&#x434;</trd>,
    <trd>&#x43A;&#x43E;&#x43C;&#x43E;&#x440;&#x430;</trd>,
    <trd>&#x43F;&#x430;&#x43A;&#x433;&#x430;&#x443;&#x437;</trd>,
    <trd>&#x431;&#x430;&#x437;&#x430;</trd>,
    <trd>&#x43F;&#x430;&#x43A;&#x435;&#x442; (&#x43F;&#x440;&#x43E;&#x433;&#x440;&#x430;&#x43C; &#x434;&#x43B;&#x44F; &#x432;&#x438;&#x43A;&#x43E;&#x43D;&#x430;&#x43D;&#x43D;&#x44F;)</trd>
  </trdgrp>
</drv>

<drv mrk="stapl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stapl.0i.FORM">
    <dif>
      Surmetadi en <tld/>on:
      <ekz>
        ankora&ubreve; kompletaj brikoj <klr>(en postbombada ruino)</klr>
        estis <tld/>itaj
        <fnt>
          <aut>G&uuml;nter Grass, tr. Wolfgang Kirschstein</aut>:
          <vrk>Tri &ccirc;apitroj el la romano de Grass</vrk>.
          <bib>LOdE</bib>, <lok>n-ro 61 (1999:11), p. 11.</lok>
        </fnt>;
      </ekz><ekz>
        alte kvalifikita teknikisto ekzemple nun en la arkivo
        <tld/>as &jcirc;urnalojn
        <fnt>
          <aut>Stefan Maul</aut>:
          <vrk>Estimataj gelegantoj</vrk>. <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        ligna <tld/>ebla kofro, kiu ta&ubreve;gas hotel&ccirc;ambre kiel tirkesto
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj
          progreso</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la svisa registaro <tld/>as <klr>(stokas)</klr> vakcinon por proksimume ok milionoj da
        personoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2007/009259.php">Planoj senmaskigitaj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        en lia memoro estis <tld/>ita neel&ccirc;erpebla
        sortimento de instruaj historioj
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>narovnat do skladu</trd>,
    <trd>skladovat</trd>,
    <trd>stohovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stapeln</trd>,
    <trd>aufschichten</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>almacenar</trd>,
    <trd>amontonar</trd>,
    <trd>apilar</trd>
  </trdgrp>
  <trd lng="fr">empiler</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_a;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_v; &c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skladova&tcaron;</trd>,
    <trd>stohovite nauklada&tcaron;</trd>,
    <trd>uklada&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x431;&#x435;&#x440;&#x456;&#x433;&#x430;&#x442;&#x438; &#x442;&#x43E;&#x432;&#x430;&#x440;&#x438; &#x43D;&#x430; &#x441;&#x43A;&#x43B;&#x430;&#x434;&#x456;</trd>,
    <trd>&#x441;&#x43A;&#x43B;&#x430;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
