<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stapl</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stapl.0o">
  <kap><tld/>o</kap>
  <snc mrk="stapl.0o.FORM">
    <dif>
      Amaso da objektoj, &gcirc;enerale plataj, metitaj unuj sur aliaj,
      <ref tip="vid" cel="stak.0o.vico">stako<sncref/></ref>,
      <ref tip="sin" cel="kolon.0o.staplo">kolono<sncref/></ref>:
      <ekz>
        <tld/>o da libroj, teleroj.
      </ekz>
    </dif>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_o;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pila</trd>,
    <trd>munt</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sklad zboží</trd>,
    <trd>skladiště</trd>,
    <trd>srovnaná hromada</trd>
  </trdgrp>
  <trd lng="de">Stapel</trd>
  <trdgrp lng="en">
    <trd>pile</trd>,
    <trd>stack</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>empilement</trd>,
    <trd>pile<klr> (empilement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rak&aacute;s</trd>,
    <trd>halom</trd>
  </trdgrp>
  <trd lng="nl">stapel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">sklad pre stohový tovar <klr>(čakajúci na predaj alebo návrat)</klr></trd>
</drv>

<drv mrk="stapl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stapl.0i.FORM">
    <dif>
      Supermetadi en <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en lia memoro estis <tld/>ita neel&ccirc;erpebla
        sortimento de instruaj historioj
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>narovnat do skladu</trd>,
    <trd>skladovat</trd>,
    <trd>stohovat</trd>
  </trdgrp>
  <trd lng="fr">empiler</trd>
  <trdgrp lng="sk">
    <trd>skladovať</trd>,
    <trd>stohovite naukladať</trd>,
    <trd>ukladať</trd>
  </trdgrp>
</drv>
</art>
