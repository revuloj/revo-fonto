<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stapl</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="stapl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Amaso da objektoj, &gcirc;enerale plataj, metitaj unuj sur aliaj:
      <ekz>
        <tld/>o da libroj, teleroj.
      </ekz>
    </dif>
    <ref tip="sin" cel="kolon.0o.staplo">kolono</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_o;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Stapel</trd>
  <trdgrp lng="fr">
    <trd>empilement</trd>,
    <trd>pile<klr> (empilement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rak&aacute;s</trd>,
    <trd>halom</trd>
  </trdgrp>
  <trd lng="nl">stapel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
