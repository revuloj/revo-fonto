<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grapfrukt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="grapfrukt.0o">
  <kap><tld/>o</kap>
  <snc mrk="grapfrukt.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>arbo, iom granda, globa:
      <ekz>
        salato el <tld/>oj kaj cepoj
        <fnt>
          <aut>Donjo</aut>,
          <lok>en: <url
          ref="http://eo.wikibooks.org/wiki/Kuirlibro">Kuirlibro</url>,
          2005-02-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="grapfrukt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="grapfrukt.0arbo.BOT"><tld/>arbo</ref>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_je;&c_j;&c_p;&c_f;&c_r;&c_u;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Grapefruit</trd>,
    <trd>Grapefrucht</trd>
  </trdgrp>
  <trd lng="en">grapefruit</trd>
  <trd lng="fa">&f_g;&a_r;&f_y;&f_p;&zwnj;&a_f;&a_r;&a_w;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>pamplemousse rose</trd>,
    <trd>pomelo</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>grejpfrut</trd>,
    <trd>grapefruit</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>toranja</trd>,
    <trd>grapefruit</trd>
  </trdgrp>
  <trd lng="sv">grapefrukt</trd>
</drv>

<drv mrk="grapfrukt.0arbo">
  <kap><tld/>arbo</kap>
  <snc mrk="grapfrukt.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="citrus.0o.BOT">Citruso</ref>,
      miksospeco de <ref tip="vid"
      cel="pampelmus.0arbo.BOT">pampelmusarbo</ref> kaj
      <ref tip="vid" cel="orangx.0arbo.BOT">oran&gcirc;arbo</ref>
      (<trd lng="la">Citrus paradisi</trd>):
      <ekz>
        la <tld/>arbo plejofte fari&gcirc;as 5 &gcirc;is 6 m alta
        <fnt><bib>Viki</bib>, <lok>artikolo
        &leftquot;grapfrukto&rightquot;,
        2006-08-22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Grapefruitbaum</trd>
  <trd lng="pl">grejpfrut<klr> (drzewo)</klr></trd>
  <trd lng="sv">grapefrukttr&auml;d</trd>
</drv>
</art>

