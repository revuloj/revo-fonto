<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grapfrukt.xml,v 1.20 2023/11/11 11:36:36 revo Exp $">
<kap>
  <rad>grapfrukt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="grapfrukt.0o">
  <kap><tld/>o</kap>
  <snc mrk="grapfrukt.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>arbo, iom granda, globa:
      <ekz>
        salato el <tld/>oj kaj cepoj
        <fnt>
          <aut>Donjo</aut>,
          <lok>en: <url ref="http://eo.wikibooks.org/wiki/Kuirlibro">Kuirlibro</url>,
          2005-02-27</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_g;&c_r;&c_e;&c_j;&c_p;&c_f;&c_r;&c_u;&c_t; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
    <trd lng="ca">aranja</trd>
    <trdgrp lng="fr">
      <trd>pom&eacute;lo</trd>,
      <trd>grape-fruit</trd>
    </trdgrp>
     <trdgrp lng="nl">
    <trd>pomelo</trd>
    </trdgrp>
  </snc>
  <snc mrk="grapfrukt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="grapfrukt.0arbo.BOT"><tld/>arbo</ref>
 <trd lng="be">&c_g;&c_r;&c_e;&c_j;&c_p;&c_f;&c_r;&c_u;&c_t; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
      <trd lng="ca">aranger</trd>
     <trdgrp lng="nl">
    <trd>pomeloboom</trd>
  </trdgrp>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_je;&c_j;&c_p;&c_f;&c_r;&c_u;&c_t;</trd>
  <trd lng="ca">aranja</trd>
  <trdgrp lng="cs">
    <trd>grapefruit</trd>,
    <trd>grep</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Grapefruit</trd>,
    <trd>Grapefrucht</trd>
  </trdgrp>
  <trd lng="en">grapefruit</trd>
  <trd lng="es">pomelo</trd>
  <trd lng="fa">&f_g;&a_r;&f_y;&f_p;&zwnj;&a_f;&a_r;&a_w;&a_t;</trd>
  <trd lng="ja">&#x30B0;&#x30EC;&#x30FC;&#x30D7;&#x30D5;&#x30EB;&#x30FC;&#x30C4;</trd>
  <trdgrp lng="pl">
    <trd>grejpfrut</trd>,
    <trd>grapefruit</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>toranja</trd>,
    <trd>grapefruit</trd>
  </trdgrp>
  <trd lng="sk">grapefruit</trd>
  <trd lng="sv">grapefrukt</trd>
  <trd lng="tr">greyfurt</trd>
  <trd lng="uk">&c_g;&c_r;&c_je;&c_j;&#x43F;&c_f;&c_r;&c_u;&c_t;</trd>
  <trd lng="zh">&#x6935; <pr>ji&#x1CE;</pr></trd>
</drv>

<drv mrk="grapfrukt.0arbo">
  <kap><tld/>arbo</kap>
  <snc mrk="grapfrukt.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="citrus.0o.BOT">Citruso</ref>,
      miksospeco de <ref tip="vid" cel="pampelmus.0arbo.BOT">pampelmusarbo</ref> kaj
      <ref tip="vid" cel="orangx.0arbo.BOT">oran&gcirc;arbo</ref>
      (<trd lng="la">Citrus paradisi</trd>):
      <ekz>
        la <tld/>arbo plejofte fari&gcirc;as 5 &gcirc;is 6 m alta
        <fnt><bib>Viki</bib>, <lok>artikolo
        &leftquot;grapfrukto&rightquot;,
        2006-08-22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_e;&c_j;&c_p;&c_f;&c_r;&c_u;&c_t; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="ca">aranger</trd>
  <trdgrp lng="cs">
    <trd>grapefruitov&yacute; strom</trd>,
    <trd>grepov&yacute; strom</trd>
  </trdgrp>
  <trd lng="de">Grapefruitbaum</trd>
  <trdgrp lng="fr">
    <trd>chad&egrave;gue</trd>,
    <trd>pom&eacute;lo</trd>
  </trdgrp>
  <trd lng="ja">&#x30B0;&#x30EC;&#x30FC;&#x30D7;&#x30D5;&#x30EB;&#x30FC;&#x30C4;</trd>
  <trd lng="nl">pomeloboom</trd>
  <trd lng="pl">grejpfrut<klr> (drzewo)</klr></trd>
  <trd lng="sk">grepov&yacute; strom</trd>
  <trd lng="sv">grapefrukttr&auml;d</trd>
  <trd lng="tr">greyfurt a&gbreve;ac&inodot;</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_r;&c_je;&c_j;&#x43F;&c_f;&c_r;&c_u;&c_t;</trd>,
    <trd>&c_gu;&c_r;&c_je;&c_j;&#x43F;&c_f;&c_r;&c_u;&c_t;&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>
  </trdgrp>
  <trd lng="zh">&#x8461;&#x8404;&#x67DA; <pr>p&#xFA;t&#xE1;oy&#xF2;u</pr></trd>
</drv>
</art>

