<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>bolt</rad>/o <fnt>Z</fnt></kap>
<drv mrk="bolt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala <ref tip="super" cel="sxraux.0o">&scirc;ra&ubreve;bo</ref>
      a&ubreve; najlego kun lar&gcirc;a kapo kaj cilindra korpo,
      uzata por kunfiksi du objektojn.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schraube</trd>,
    <trd>Metallschraube</trd>
  </trdgrp>
  <trd lng="en">bolt</trd>
  <trd lng="es">tornillo</trd>
  <trdgrp lng="fr">
    <trd>rivet</trd>,
    <trd>boulon <klr tip="ind">(vis cylindrique)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_t;</trd>
  <trd lng="sv">bult</trd>
</drv>

<drv mrk="bolt.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunligi per <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>schrauben</trd>,
    <trd>verschrauben</trd>
  </trdgrp>
  <trd lng="en">bolt</trd>
  <trd lng="es">atornillar</trd>
  <trd lng="fr">boulonner</trd>
  <trd lng="ru">&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol; &c_b;&c_o;&c_l;&c_t;&c_a;&c_m;&c_i;</trd>
</drv>

<drv mrk="bolt.0ingo">
  <kap><tld/>ingo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Spirale truita metalpeco, kiun oni &scirc;ra&ubreve;bas sur la <tld/>o por &gcirc;in
      alfiksi.
    </dif>
    <ref tip="super" cel="sxraux.0ingo">&scirc;ra&ubreve;bingo</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="de">Unterlegscheibe</trd>
  <trd lng="en">nut</trd>
  <trd lng="es">tuerca</trd>
  <trd lng="fr">&eacute;crou</trd>
  <trd lng="ru">&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
</drv>
</art>
