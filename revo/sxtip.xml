<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxtip.xml,v 1.32 2023/12/26 01:31:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;tip</rad>/o
</kap>
<drv mrk="sxtip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxtip.0o.KOMUNE">
    <dif>
      Mallonga dika peco da ligno:
      <ekz>
        <tld/>o <klr>(kolbo, tenilo)</klr> de pafilo;
      </ekz><ekz>
        &scirc;i sidis rigida, kvaza&ubreve; <tld/>o
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>;
      </ekz><ekz>
        Vi metis miajn piedojn en <tld/>on,  Vi observas &ccirc;iujn miajn vojojn
        <fnt><bib>MT</bib>, <lok>Ijob 13:27</lok></fnt>;
      </ekz><ekz>
        apud la fre&scirc;a herbobedo ku&scirc;is la bastono de la patro, alligita
        al <tld/>o
        <klr>(bastono metita je unu fino en la teron kian oni uzas por
        alligi &scirc;afojn)</klr>
        <fnt><bib>Fab2</bib><lok>Sambuka virineto</lok></fnt>;
      </ekz><ekz>
        jen na&gcirc;as peco da ligno, &gcirc;i <klr>[&#8230;]</klr> estas ja <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Flikilo</lok></fnt>;
      </ekz><ekz>
        ili veturis <ind>trans <tld/>ojn kaj &scirc;tonojn</ind> &ccirc;iam pli
        profunden en la arbaron
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
        <trd lng="de">&uuml;ber Stock und Stein</trd>
      </ekz><ekz>
        <klr>[&gcirc;i]</klr> kaptis per mallertaj manoj la pezan stangon, kiu estis
        anta&ubreve;&scirc;ovita anta&ubreve; la pordo, senbrue &gcirc;i eltiris la stangon, singarde
        &gcirc;i forprenis la <tld/>eton, kiu estis &scirc;ovita sub la anson
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz><ekz>
        la glaseto kun la blukolora ligna piedo ne dronis, la ligna
        <tld/>o tenis &gcirc;in supre
        <fnt><bib>Fab4</bib>, <lok>Anneto</lok></fnt>;
      </ekz><ekz>
        &scirc;i sidis rigida, kvaza&ubreve; <tld/>o, kaj ne movi&gcirc;is
        <fnt><bib>ElektFab</bib>, <lok>Fi&scirc;kaptisto kaj lia edzino</lok></fnt>;
      </ekz><ekz>
        e&ccirc; <tld/>o estos bela, se vi &gcirc;in ornamos <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kan.0o">kano</ref>,
      <ref cel="stip.0o.tigo">stipo<sncref/></ref>,
      <ref cel="trab.0o">trabo</ref>,
      <ref cel="trunk.0o">trunko</ref>,
      <ref cel="verg.0o">vergo</ref>
    </refgrp>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_r;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_u;&c_r;&c_b;&c_a;&c_l;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">tronc</trd>
  <trdgrp lng="de">
      <trd>Scheit</trd>,
      <trd>Holzscheit</trd>,
      <trd>Holzklotz</trd>,
      <trd>Pflock</trd>,
      <trd>Stock</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>log</trd>,
    <trd>block</trd>
  </trdgrp>
  <trd lng="fr">buche</trd>
  <trdgrp lng="hu">
    <trd>has&aacute;b</trd>,
    <trd>tusk&oacute;</trd>,
    <trd>t&odblac;ke</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>pezzo<klr> (di legno)</klr></trd>,
    <trd>ciocco<klr> (di legno)</klr></trd>,
    <trd>ceppo<klr> (di legno)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>blok</trd>,
    <trd>houtblok</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kloc</trd>,
    <trd>pniak</trd>,
    <trd>k&lstroke;oda</trd>,
    <trd>polano</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_je;&c_n;&c_o;</trd>,
    <trd>&c_ch;&c_u;&c_r;&c_b;&c_a;&c_n;</trd>
  </trdgrp>
  </snc>
  <snc mrk="sxtip.0o.malsagxulo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malsa&gcirc;ulo:
      <ekz>
        malsa&gcirc;a kiel <tld/>o <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        ne timu, kaj via koro ne senkura&gcirc;i&gcirc;u pro la du fumantaj brul<tld/>oj,
        pro la furiozo de <nom>Recin</nom> <klr>[&#8230;]</klr> kaj de la
        filo de <nom>Remalja</nom>
        <fnt><bib>MT</bib>, <lok>Jesaja 7:4</lok></fnt>;
      </ekz><ekz>
        <ref tip="vid" cel="kap.sxtip0ulo.KOMUNE">&scirc;tipkapulo</ref>.
      </ekz>
    </dif>
    <trd lng="de">Holzkopf</trd>
    <trd lng="ru">&c_d;&c_u;&c_b;&c_i;&c_n;&c_a;<klr> (&c_d;&c_u;&c_r;&c_a;&c_k;)</klr></trd>
  </snc>
  <snc mrk="sxtip.0o.stumpo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="stump.0o.FIG">Stumpo<sncref/></ref>:
      <ekz>
        la a&gcirc;on de la arbo oni difinis per la nombro
        de la tavoloj sur la <tld/>o
        <fnt><bib>FK</bib>, <lok>el la vivo kaj sciencoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">soca</trd>
    <trdgrp lng="de">
      <trd>Stumpf</trd>,
      <trd>Baumstumpf</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>moncone</trd>,
      <trd>ceppaia</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#22823;&#x304D;&#12356;&#26408;&#x584A; 
    <pr>&#x304A;&#x304A;&#x304D;&#12356;&#x304D;&#x304B;&#x305F;&#x307E;&#x308A;</pr></trd>,
    <trd>&#x4E38;&#x592A;  <pr>&#x307E;&#x308B;&#x305F;</pr></trd>,
    <trd>&#x85AA;  <pr>&#x307E;&#x304D;</pr></trd>,
    <trd>&#12391;&#x304F;&#x306E;&#x574A;  <pr>&#12391;&#x304F;&#x306E;&#x307C;&#12358;</pr></trd>,
    <trd>&#12358;&#12377;&#x306E;&#x308D;</trd>
  </trdgrp>
  <trd lng="ne">&#2354;&#2327;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&#x43D;&#x43E;</trd>,
    <trd>&#x43F;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&#x43E;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtip.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="sxtip.0aro.lignofajro">
    <dif>
      Amaso da <tld/>oj por fari
      <ref cel="fajr.ligno0o">lignofajron</ref> kaj la lignofajro mem:
      <ekz>
        sekbran&ccirc;etojn kaj trunkojn &jcirc;eti en <tld/>aron
        <fnt><aut>A. Mickiewicz tr. A. Grabowski</aut>:
          <vrk>Sinjoro Tadeo</vrk>, <lok>3:811.</lok></fnt>;
      </ekz><ekz>
        &ccirc;e la kripto oni bruligis kelkajn pe&ccirc;tor&ccirc;ojn,
        kiujn oni kunmetis en malgrandan <tld/>aron
       <fnt>
         <aut>H. Sienkewicz, tr. Lidia Zamenhof</aut>:
         <vrk>Quo vadis</vrk>, <lok>&Ccirc;ap. 20&#xAA;</lok>
       </fnt>;
     </ekz><ekz>
       mi rigardis de malproksime, dum &scirc;ia patro flamigis la
       <tld/>aron
       <fnt><aut>A. L&ouml;wenstein</aut>: <vrk>La &scirc;tona urbo</vrk>,
         <lok>1999</lok></fnt>.
     </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_o;&c_v;&c_a;<klr> (&c_d;&c_l;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_r;&c_a;)</klr></trd>,
      <trd>&c_k;&c_o;&c_s;&c_t;&c_jo;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxtip.0aro.funebra">
    <dif>
      <ref cel="fajr.ligno0o">Lignofajro</ref> por rite cindrigi
      mortinto(j)n
      <ekz>
        oni malpermesis ekzemple bruligi la vidvinojn sur la <tld/>aro
        de ilia edzo
        <fnt><lok>Le Monde diplomatique en Esperanto</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kremaci.0o">kremacio</ref>
    <trd lng="en">pyre</trd>
    <trd lng="ru">&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k;&c_o;&c_s;&c_t;&c_jo;&c_r;</trd>
  </snc>
  <snc mrk="sxtip.0aro.auxtodafeo">
    <dif>
      <ref cel="fajr.ligno0o">Lignofajro</ref> por
      <ref cel="ekzeku.0i">ekzekuti</ref> (brulmortigi,
      <ref cel="sxtip.0arumi"><tld/>arumi</ref>) kondamniton:
      <ekz>
        ne plu konvenas starigi gigantan inkvizician brul<tld/>aron
        <klr>[&#8230;]</klr> same kiel oni iam karbonadis herezulojn
        <fnt><bib>Monato</bib></fnt>;
      </ekz><ekz>
        la ekzekutisto ekbruligis la <tld/>aron
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>;
      </ekz><ekz>
        post proceso en &Gcirc;enevo, li estis kiel herezulo kondamnita al
        la morto sur brul<tld/>aro
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2011/010364.php">Mikaelo Serveto (1511-1553) 500-jara</url></vrk>
        </fnt>;
      </ekz><ekz>
        la &gcirc;eneva civito, kiu en la 16a jarcento bruligis Serveton sur
        la <tld/>aro de maltoleremo
        <fnt>
       <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2000/006847.php">Sobraj pensoj de sobrulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="auxtod.0o">a&ubreve;todafeo</ref>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_jo;&c_r;<klr> (&c_d;&c_l;&c_ja; &c_k;&c_a;&c_z;&c_n;&c_i;)</klr></trd>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>pira</trd>,
    <trd>foguera</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Scheiterhaufen</trd>,
    <trd>Holzstapel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>woodpile</trd>,
    <trd>pyre</trd>
  </trdgrp>
  <trd lng="fr">b&ucirc;cher</trd>
  <trdgrp lng="hu">
    <trd>farak&aacute;s</trd>,
    <trd>m&aacute;glya</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rogo</trd>,
    <trd>pira</trd>,
    <trd><ind>catasta</ind> di legna<klr>(per il fuoco)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x307E;&#x304D;&#x306E;&#x5C71;  <pr>&#x307E;&#x304D;&#x306E;&#12373;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>houtstapel</trd>,
    <trd>brandstapel</trd>
  </trdgrp>
  <trd lng="pl">stos drewna</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43E;&c_g;&#x43D;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&c_t;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtip.0arumi">
  <kap><tld/>arumi <fnt><bib>PIV1</bib></fnt></kap>
  <snc>
    <dif>
      Ekzekute brulmortigi sur
      <ref cel="sxtip.0aro.auxtodafeo"><tld/>aro<sncref/></ref>.
    </dif>
  </snc>
  <ref tip="vid" cel="auxtod.0o">a&ubreve;todafeo</ref>
  <trd lng="be">&c_s;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_v;&c_o;&c_g;&c_n;&c_ib;&c_sh;&c_ch;&c_y; &c_w; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="ja">&#x706B;&#21009;&#x306B;&#x51E6;&#12377;  <pr>&#12402;&#12369;&#12356;&#x306B;&#12375;&#12423;&#12377;</pr></trd>
  <trd lng="pl">spali&cacute; na <ind>stosie</ind></trd>
  <trd lng="ru">&c_s;&c_zh;&c_je;&c_ch;&c_mol; &c_n;&c_a; &c_k;&c_o;&c_s;&c_t;&c_r;&c_je;<klr> (&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
  <trd lng="uk">&c_s;&#x43F;&c_a;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_v;&#x43E;&c_g;&#x43D;&c_i;&c_shch;&c_ib;</trd>
</drv>
</art>
