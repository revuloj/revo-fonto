<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kork.xml,v 1.22 2023/12/03 09:58:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kork</rad>/o
</kap>
<drv mrk="kork.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kork.0o.KOMUNE">
    <dif>
      Spongeca kaj malpeza &scirc;elo de speciala kverko, uzata por fari
      &scirc;topilojn, flosilojn, &scirc;uplandojn kaj aliajn:
      <ekz>
        flosi kiel <tld/>o;
      </ekz>
      <ekz>
        la kavo estis plenigita per <tld/>o kaj diversaj aliaj substancoj, kies celo
        ver&scirc;ajne estis protekti la kofron kontra&ubreve; malsekeco
        <fnt>
          <aut>C. Piron</aut>
          <vrk>Gerda Malaperis!</vrk>
          <lok>1983</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_r;&c_k;</trd>
    <trd lng="ca">suro</trd>
    <trd lng="de">Kork</trd>
    <trd lng="en">cork</trd>
    <trd lng="es">corcho</trd>
    <trd lng="fr">li&egrave;ge</trd>
    <trd lng="hu">parafa</trd>
    <trd lng="pl">korek</trd>
    <trd lng="pt">corti&ccedil;a</trd>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_a; &c_p;&c_r;&c_o;&c_b;&c_k;&c_o;&c_v;&c_o;&c_g;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#x8EDF;&#x6728;&#x585E; <pr>ru&#x1CE;nm&#xF9;s&#x101;i</pr></trd>,
    <trd>&#x8EDF;&#x6728; <pr>ru&#x1CE;nm&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="kork.0o.sxtopilo">
    <dif>
      &Scirc;topilo el tiu substanco:
      <ekz>
        <ref tip="vid" cel="tir.korko0ilo.KUI">korktirilo</ref>;
      </ekz>
      <ekz>
        la botelkolo <klr>[&#x2026;]</klr> estis provizita per <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <vrk>Kolo de botelo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>cork</trd>,
      <trd>stopper</trd>
    </trdgrp>
    <trd lng="bg">&c_t;&c_a;&c_p;&c_a;</trd>
    <trd lng="ca">tap</trd>
    <trd lng="de">Korken</trd>
    <trd lng="es">corcho<klr> (tap&oacute;n)</klr></trd>
    <trd lng="fr">bouchon</trd>
    <trd lng="hu">parafadug&oacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_r;&c_a;&c_k;</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30EB;&#x30AF;</trd>,
    <trd>&#x30B3;&#x30EB;&#x30AF;&#x6813;  <pr>&#x30B3;&#x30EB;&#x30AF;&#x305B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">kurk</trd>
  <trd lng="pl">korek</trd>
  <trdgrp lng="ro">
    <trd>dop</trd>,
    <trd>plut&abreve;</trd>
  </trdgrp>
</drv>

<drv mrk="kork.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kork.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="sxtop.0i.fermi">&Scirc;topi<sncref/></ref> per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_a;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_u;&c_sh;&c_v;&c_a;&c_m; &c_s; &c_t;&c_a;&c_p;&c_a;</trd>
  <trd lng="de">verkorken</trd>
  <trd lng="en">cork (up)</trd>
  <trd lng="es">tapar con un tap&oacute;n de corcho</trd>
  <trd lng="fr">boucher<klr> (une bouteille)</klr></trd>
  <trd lng="hu">bedug <klr tip="ind">(dug&oacute;val)</klr></trd>
  <trd lng="ja">&#x30B3;&#x30EB;&#x30AF;&#x6813;&#x3092;&#x3059;&#x308B;  <pr>&#x30B3;&#x30EB;&#x30AF;&#x305B;&#x3093;&#x3092;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">kurken</trd>
  <trdgrp lng="pl">
    <trd>zakorkowa&cacute;</trd>,
    <trd>zatka&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>astupa</trd>,
    <trd>&icirc;nfunda</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol; &c_p;&c_r;&c_o;&c_b;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#x585E; <pr>s&#x101;i</pr></trd>
</drv>

<drv mrk="kork.mal0i">
  <kap>
    mal<tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kork.mal0i.KOMUNE">
    <dif>
      <ref tip="super" cel="sxtop.mal0i">Mal&scirc;topi</ref>, forigante <tld/>on:
      <ekz>
        &leftquot;je la sano de miaj karaj gastoj!&rightquot; &mdash; deklaris la mastro, mal<tld/>ante la duan
        botelon
        <fnt>
          <aut>A. Pu&scirc;kin, trad. N. Lozga&ccirc;ev</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Rn-pusk1.htm">&Ccirc;erkisto</url></vrk>
          <lok>La Ondo de Esperanto, 1999:6 (56)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_u;&c_s;&c_k;&c_a;&c_m;</trd>
  <trd lng="de">entkorken</trd>
  <trd lng="en">uncork</trd>
  <trd lng="es">descorchar</trd>
  <trd lng="fr">d&eacute;boucher</trd>
  <trd lng="ja">&#x30B3;&#x30EB;&#x30AF;&#x6813;&#x3092;&#x629C;&#x304F;  <pr>&#x30B3;&#x30EB;&#x30AF;&#x305B;&#x3093;&#x3092;&#x306C;&#x304F;</pr></trd>
  <trd lng="nl">ontkurken</trd>
  <trdgrp lng="pl">
    <trd>odkorkowa&cacute;</trd>,
    <trd>napocz&aogonek;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;ncepe</trd>,
    <trd>bro&#x219;&abreve;</trd>
  </trdgrp>
</drv>
</art>
