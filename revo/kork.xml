<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kork.xml,v 1.18 2019/08/15 22:10:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kork</rad>/o
</kap>
<drv mrk="kork.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Spongeca kaj malpeza &scirc;elo de speciala kverko, uzata por fari
      &scirc;topilojn, flosilojn, &scirc;uplandojn ktp.:
      <ekz>
        flosi kiel <tld/>o.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_r;&c_k;</trd>
    <trd lng="ca">suro</trd>
    <trd lng="de">Kork</trd>
    <trd lng="en">cork</trd>
    <trd lng="es">corcho</trd>
    <trd lng="fr">li&egrave;ge</trd>
    <trd lng="hu">parafa</trd>
    <trd lng="pl">korek</trd>
    <trd lng="pt">corti&ccedil;a</trd>    
    <trd lng="ru">&c_k;&c_o;&c_r;&c_a; &c_p;&c_r;&c_o;&c_b;&c_k;&c_o;&c_v;&c_o;&c_g;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      &Scirc;topilo el tiu materio:
      <ekz>
        <ind><tld/>tirilo</ind>.
        <trd lng="hu">dug&oacute;h&uacute;z&oacute;</trd>
        <trd lng="nl">kurkentrekker</trd>
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>cork</trd>,
      <trd>stopper</trd>
    </trdgrp>
    <trd lng="bg">&c_t;&c_a;&c_p;&c_a;</trd>
    <trd lng="ca">tap</trd>
    <trd lng="de">Korken</trd>
    <trd lng="es">corcho<klr> (tap&oacute;n)</klr></trd>
    <trd lng="fr">bouchon</trd>
    <trd lng="hu">parafadug&oacute;</trd>        
    <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_r;&c_a;&c_k;</trd>
  <trd lng="nl">kurk</trd>
  <trd lng="pl">korek</trd>
  <trdgrp lng="ro">
    <trd>dop</trd>,
    <trd>plut&abreve;</trd>
  </trdgrp>
</drv>

<drv mrk="kork.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_a;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_u;&c_sh;&c_v;&c_a;&c_m; &c_s; &c_t;&c_a;&c_p;&c_a;</trd>
  <trd lng="de">verkorken</trd>
  <trd lng="en">cork (up)</trd>
  <trd lng="es">tapar con un tap&oacute;n de corcho</trd>
  <trd lng="fr">boucher<klr> (une bouteille)</klr></trd>
  <trd lng="hu">bedug <klr tip="ind">(dug&oacute;val)</klr></trd> 
  <trd lng="nl">kurken</trd>
  <trdgrp lng="pl">
    <trd>zakorkowa&cacute;</trd>,
    <trd>zatka&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>astupa</trd>,
    <trd>&icirc;nfunda</trd>
  </trdgrp>     
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol; &c_p;&c_r;&c_o;&c_b;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kork.mal0i">
  <kap>
    mal<tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mal&scirc;topi la <tld/>on.
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_u;&c_s;&c_k;&c_a;&c_m;</trd>
  <trd lng="de">entkorken</trd>
  <trd lng="en">uncork</trd>
  <trd lng="es">descorchar</trd>
  <trd lng="fr">d&eacute;boucher</trd>
  <trd lng="nl">ontkurken</trd>
  <trdgrp lng="pl">
    <trd>odkorkowa&cacute;</trd>,
    <trd>napocz&aogonek;&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;ncepe</trd>,
    <trd>bro&#537;&abreve;</trd>
  </trdgrp>     
</drv>
</art>
