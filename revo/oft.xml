<drv mrk="oft.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="oft.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a, pli facile trovebla ol anta&ubreve;e:
      <ekz>
        kazoj de la malsano <tld/>i&gcirc;is anka&ubreve; en e&ubreve;ropaj landoj
        <fnt><aut>V. Lemelev</aut>:
          <vrk>Nova infekta malsano postulanta internacian
          kunlaboron</vrk>,
          <bib>Monato</bib>,
          <lok>2001:5, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_sh;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">aliesaat<klr tip="ind"> (v.g.)</klr></trd>
  <trdgrp lng="de">
    <trd>h&auml;ufig <ind>werden</ind></trd>,
    <trd><klr>sich </klr>h&auml;ufen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devenir plus <ind>fr&eacute;quent</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyakoriv&aacute; v&aacute;lik</trd>,
    <trd>s&udblac;r&udblac;s&ouml;dik <klr tip="ind">(id&odblac;ben)</klr></trd>
  </trdgrp>
  <trd lng="pl">powtarza&cacute; si&eogonek; <klr tip="amb">(cz&eogonek;sto)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="oft.mal0a">
  <kap>mal<tld/>a</kap>
