    <trd>&#x983B;&#x767A;  <pr>&#x3072;&#x3093;&#x3071;&#x3064;</pr></trd>,
    <trd>&#x983B;&#x5EA6;  <pr>&#x3072;&#x3093;&#x3069;</pr></trd>
  </trdgrp>
  <trd lng="nl">frequentie</trd>
