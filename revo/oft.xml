<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>oft</rad>/e
</kap>
<drv mrk="oft.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;multe da fojoj&rightquot;:
      <ekz>
        mi <tld/>e ricevas leterojn el eksterlando;
      </ekz>
      <ekz>
        li vizitas nin <tld/>e, preska&ubreve; &ccirc;iutage;
      </ekz>
      <ekz>
        post infekfa malsano oni <tld/>e bruligas la vestojn de la
        malsanulo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_a;</trd>,
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_k;&c_r;&c_o;&c_c;&c_mol;</trd>
  </trdgrp>                 
  <trd lng="br">alies</trd>
  <trd lng="cs">&ccaron;asto</trd>
  <trd lng="de">oft</trd>
  <trd lng="el">&sigma;&ypsilon;&chi;&ny;&alfa_ton;</trd>
  <trd lng="en">often</trd>
  <trdgrp lng="fr">
    <trd>souvent</trd>
  </trdgrp>
  <trd lng="hu">gyakran</trd>  
  <trd lng="nl">dikwijls</trd>
  <trd lng="pl">cz&eogonek;sto</trd>
  <trdgrp lng="pt">
    <trd>freq&uuml;entemente</trd>,
    <trd>a <ind>mi&uacute;do</ind></trd>,
    <trd>muitas <ind>vez</ind>s</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>adesea</trd>,
    <trd>deseori</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_o;</trd>,
    <trd>&c_m;&c_n;&c_o;&c_g;&c_o; &c_r;&c_a;&c_z;</trd>
  </trdgrp>
  <trd lng="sv">ofta</trd>
</drv>

<drv mrk="oft.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta a&ubreve; trovebla multe da fojoj:
      <ekz>
        <tld/>a ka&ubreve;zo de fatalaj program-eraroj<fnt><aut>Klivo</aut>:
        <vrk>Kio estas Javo?</vrk>, <bib>Monato</bib><lok>jaro 1998a,
        numero 5a, p. 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>        
  <trd lng="br">stank<klr tip="ind"> (ag. en amzer)</klr></trd>
  <trd lng="de">h&auml;ufig</trd>
  <trd lng="el">&sigma;&ypsilon;&chi;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">fr&eacute;quent</trd>
  <trdgrp lng="hu">
    <trd>gyakori</trd>,
    <trd>s&udblac;r&udblac; <klr tip="ind">(gyakori)</klr></trd>
  </trdgrp>
  <trd lng="pl">cz&eogonek;sty</trd>
  <trd lng="pt">freq&uuml;ente</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_n;&c_o;&c_g;&c_o;&c_k;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="oft.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Relativa abundo de okazado:
      <ekz>
        man&gcirc;kutimo alta je graso, aparte bestdevena graso, 
ligi&gcirc;as kun
        plia <tld/>eco de arteriosklerozo
        <fnt><aut>Krys Ungar</aut>: <vrk>Vivu vigle kaj
        vegetare!</vrk>, <bib>Monato</bib><lok>jaro 1995a, numero 7a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frekve.0o.ofteco">frekvenco <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_a;&c_t;&c_a;</trd>    
  <trd lng="br">aliested</trd>
  <trd lng="de">H&auml;ufigkeit</trd>
  <trdgrp lng="fr">
    <trd>fr&eacute;quence</trd>
  </trdgrp>
  <trd lng="hu">gyakoris&aacute;g</trd>  
  <trd lng="pl">cz&eogonek;sto&sacute;&cacute;</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_o;&c_t;&c_a;</trd>
</drv>

<drv mrk="oft.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi ion <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="mult">multigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
  </trdgrp>
  <trd lng="de"><klr>die </klr>H&auml;ufigkeit <ind>erh&ouml;hen</ind></trd>
  <trdgrp lng="fr">
    <trd>intensifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyakoriv&aacute; tesz</trd>,
    <trd>s&udblac;r&iacute;t <klr tip="ind">(id&odblac;ben)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="oft.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      I&gcirc;i <tld/>a, pli facile trovebla ol anta&ubreve;e:
      <ekz>
        kazoj de la malsano <tld/>i&gcirc;is anka&ubreve; en e&ubreve;ropaj
        landoj<fnt><aut>Vladimir Lemelev</aut>: <vrk>Nova infekta malsano
        postulanta internacian kunlaboron</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 5a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_sh;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>        
  <trd lng="br">aliesaat<klr tip="ind"> (v.g.)</klr></trd>
  <trdgrp lng="de">
     <trd>h&auml;ufig <ind>werden</ind></trd>,
     <trd><klr>sich </klr>h&auml;ufen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devenir plus <ind>fr&eacute;quent</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyakoriv&aacute; v&aacute;lik</trd>,
    <trd>s&udblac;r&udblac;s&ouml;dik <klr 
tip="ind">(id&odblac;ben)</klr></trd>
  </trdgrp>
  <trd lng="pl">powtarza&cacute; si&eogonek; <klr 
tip="amb">(cz&eogonek;sto)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="oft.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Okazanta malmultajn fojojn:
      <ekz>
        tria rimarko pri <ctl>njo-knabo</ctl> estas &gcirc;ia relative
        mal<tld/>a uzo en la lingvo<fnt><aut>Bernard Golden</aut>:
        <vrk>Katamito: seksologia faktermino</vrk>,
        <bib>Monato</bib><lok>jaro 1996a, numero 12a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mult.mal0a">malmulta</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_d;&c_k;&c_ib;</trd>  
  <trd lng="br">ral<klr tip="ind"> (ag.)</klr></trd>
  <trdgrp lng="de">
     <trd>selten</trd>,
     <trd>rar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rare<klr> (peu fr&eacute;quent)</klr></trd>
  </trdgrp>
  <trd lng="hu">ritka</trd>
  <trd lng="pl">rzadki</trd>  
  <trd lng="ru">&c_r;&c_je;&c_d;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="oft.mal0e">
  <kap>mal<tld/>e</kap>
  <snc>
    <dif>
      Malmulte da fojoj:
      <ekz>
        al liaj leteroj &scirc;i respondis mal<tld/>e<fnt><aut>Spomenka
        &Scirc;timec</aut>: <vrk>Morto en Melburna tramo</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 8a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_d;&c_k;&c_a;</trd>    
  <trdgrp lng="br">
     <trd>ral<klr tip="ind"> (adv.)</klr></trd>,
     <trd>ral a wezh</trd>
  </trdgrp>
  <trd lng="de">selten</trd>
  <trd lng="el">&sigma;&pi;&alfa_ton;&ny;&jota;&alfa;</trd>
  <trdgrp lng="fr">
    <trd>rarement</trd>
  </trdgrp>
  <trd lng="hu">ritk&aacute;n</trd>  
  <trd lng="nl">zelden</trd>
  <trd lng="pl">rzadko</trd>
  <trd lng="pt">raramente</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_k;&c_o;</trd>
  <trd lng="sv">s&auml;llan</trd>
</drv>

<drv mrk="oft.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io mal<tld/>e trovi&gcirc;ebla, okazanta:
      <ekz>
        sed, malofta&jcirc;o inter la samtempaj poetoj, la lingvo restas 
klara
        kaj plene komprenebla<fnt><aut>Carlo
        Minnaja</aut>: <vrk>Meritanta ne nur reeldonon (Joh&aacute;n Valano,
        Malmalice)</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 7a, p.
        28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>                            
  <trd lng="be">&c_r;&c_e;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="br">ralete</trd>
  <trdgrp lng="de">
     <trd>Rarit&auml;t</trd>,
     <trd>seltenes <ind>St&uuml;ck</ind></trd>
  </trdgrp>
  <trd lng="el">&sigma;&pi;&alfa_ton;&ny;&jota;&omikron;</trd>
  <trdgrp lng="fr">
    <trd>raret&eacute;</trd>
  </trdgrp>
  <trd lng="hu">ritkas&aacute;g</trd>  
  <trdgrp lng="pl">
     <trd>rarytas</trd>,
     <trd>rzadko&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">raridade</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="oft.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      I&gcirc;i malpli <tld/>e trovebla, malpli abundi:
      <ekz>
        por la mal<tld/>i&gcirc;inta varo oni devis pagi pli<fnt><aut>Roland
        Rotsaert</aut>: <vrk>Piramidaj investoj: ne nur en Albanio!</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 7a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; 
<ind>&c_r;&c_e;&c_d;&c_k;&c_ib;&c_m;</ind></trd>,
    <trd>&c_r;&c_a;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>selten werden</trd>,
     <trd>rar werden</trd>,
     <trd>verschwinden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>rar&eacute;fier</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ritkul</trd>,
    <trd>megritkul</trd>
  </trdgrp>
  <trd lng="pl">zmniejszy&cacute; <klr 
tip="amb">(cz&eogonek;stotliwo&sacute;&cacute;)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol; 
<ind>&c_r;&c_je;&c_d;&c_k;&c_i;&c_m;</ind></trd>
</drv>

<drv mrk="oft.plej0e">
  <kap>plej<tld/>e</kap>
  <snc>
    <dif>
      En plimulto de okazoj:
      <ekz>
        la temoj de la kantoj estas plej<tld/>e anarkiismo, liberecanoj
        kaj simile<fnt><aut>Manuel Pancorbo Castro</aut>: <vrk>Al la
        barikadoj! (JoMo kaj Liberecanoj)</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 7a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_n;&c_a;&c_j;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_j;</trd>     
  <trd lng="br">peurliesa&ntilde;</trd>
  <trdgrp lng="de">
     <trd>meistens</trd>,
     <trd>meist</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>le plus <ind>souvent</ind></trd>
  </trdgrp>
  <trd lng="hu">leggyakrabban</trd>  
  <trd lng="pl">najcz&eogonek;&sacute;ciej</trd>
  <trd lng="ru">&c_ch;&c_a;&c_shch;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>
</drv>

<drv mrk="oft.pli0igxi">
  <kap>pli<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pli kaj pli abunde, <tld/>e aperi:
      <ekz>
        falsaj monbiletoj pliofti&gcirc;as<fnt><aut>Mu Binghua</aut>:
        <vrk>Falsaj monbiletoj pliofti&gcirc;as</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 11a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_sh;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>                    
  <trd lng="br">aliesaat<klr tip="ind"> (v.g.)</klr></trd>
  <trdgrp lng="de">
     <trd>h&auml;ufiger <ind>werden</ind></trd>,
     <trd><klr>sich </klr>zunehmend <ind>h&auml;ufen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>intensifier</ind></trd>,
    <trd>se <ind>multiplier</ind><klr> (devenir 
fr&eacute;quent)</klr></trd>,
    <trd>se <ind>r&eacute;pandre</ind><klr> (devenir 
fr&eacute;quent)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyakoribb&aacute; v&aacute;lik</trd>,
    <trd>szaparodik <klr tip="ind">(id&odblac;ben)</klr></trd>
  </trdgrp>
  <trd lng="pl">zwi&eogonek;kszy&cacute; <klr 
tip="amb">(cz&eogonek;stotliwo&sacute;&cacute;)</klr></trd>
  <trd lng="ru">&c_u;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
