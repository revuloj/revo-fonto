  <trd lng="cs">zev&scaron;ednit</trd>
  <trd lng="de"><klr>die </klr>H&auml;ufigkeit <ind>erh&ouml;hen</ind></trd>
  <trdgrp lng="fr">
    <trd>intensifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
