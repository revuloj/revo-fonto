  <trd lng="fr">fr&eacute;quent</trd>
  <trdgrp lng="hu">
    <trd>gyakori</trd>,
    <trd>s&udblac;r&udblac;<klr tip="ind"> (gyakori)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
