<drv mrk="oft.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="oft.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i malpli <tld/>e trovebla, malpli abundi:
      <ekz>
        por la mal<tld/>i&gcirc;inta varo oni devis pagi pli
        <fnt>
          <aut>R. Rotsaert</aut>,
          <vrk>Piramidaj investoj: ne nur en Albanio!</vrk>,
          <bib>Monato</bib>,
          <lok>1997:7, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;
      <ind>&c_r;&c_e;&c_d;&c_k;&c_ib;&c_m;</ind></trd>,
    <trd>&c_r;&c_a;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>selten werden</trd>,
    <trd>rar werden</trd>,
    <trd>verschwinden</trd>
  </trdgrp>
  <trd lng="fr">se <ind>rar&eacute;fier</ind></trd>
  <trdgrp lng="hu">
    <trd>ritkul</trd>,
    <trd>megritkul</trd>
  </trdgrp>
  <trd lng="nl"><ind>zeldzaam</ind> worden</trd>
  <trd lng="pl">zmniejszy&cacute; <klr tip="amb">(cz&eogonek;stotliwo&sacute;&cacute;)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol;
    <ind>&c_r;&c_je;&c_d;&c_k;&c_i;&c_m;</ind></trd>
</drv>

<drv mrk="oft.plej0e">
  <kap>plej<tld/>e</kap>
  <snc mrk="oft.plej0e.KOMUNE">
    <dif>
      En plimulto de okazoj:
      <ekz>
        la temoj de la kantoj estas plej<tld/>e anarkiismo,
        liberecanoj kaj simile
        <fnt><aut>M. Pancorbo Castro</aut>:
          <vrk>Al la barikadoj!</vrk>,
          <bib>Monato</bib>,
          <lok>1999:7, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_j;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_j;</trd>
  <trd lng="br">peurliesa&ntilde;</trd>
  <trdgrp lng="de">
    <trd>meistens</trd>,
    <trd>meist</trd>
  </trdgrp>
  <trd lng="fr">le plus <ind>souvent</ind></trd>
  <trd lng="hu">leggyakrabban</trd>
  <trd lng="es">mayoritariamente</trd>
  <trd lng="nl">meestal</trd>
  <trd lng="pl">najcz&eogonek;&sacute;ciej</trd>
  <trd lng="ru">&c_ch;&c_a;&c_shch;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>
  <trd lng="sv">oftast</trd>
</drv>

<drv mrk="oft.pli0igxi">
  <kap>pli<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="oft.pli0igxi.KOMUNE">
    <dif>
      Pli kaj pli abunde, <tld/>e aperi:
      <ekz>
        falsaj monbiletoj pli<tld/>i&gcirc;as
        <fnt><aut>Mu Binghua</aut>,
          <vrk>Falsaj monbiletoj pliofti&gcirc;as</vrk>,
          <bib>Monato</bib>,
          <lok>1999:11, p. 9a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_sh;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">aliesaat<klr tip="ind"> (v.g.)</klr></trd>
  <trdgrp lng="de">
    <trd>h&auml;ufiger <ind>werden</ind></trd>,
    <trd><klr>sich </klr>zunehmend <ind>h&auml;ufen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>intensifier</ind></trd>,
    <trd>se <ind>multiplier</ind><klr> (devenir
        fr&eacute;quent)</klr></trd>,
    <trd>se <ind>r&eacute;pandre</ind><klr> (devenir
        fr&eacute;quent)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyakoribb&aacute; v&aacute;lik</trd>,
    <trd>szaparodik <klr tip="ind">(id&odblac;ben)</klr></trd>
  </trdgrp>
  <trd lng="nl">meer <ind>algemeen</ind> worden</trd>
  <trd lng="pl">zwi&eogonek;kszy&cacute; <klr tip="amb">(cz&eogonek;stotliwo&sacute;&cacute;)</klr></trd>
  <trd lng="ru">&c_u;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
