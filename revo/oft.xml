<drv mrk="oft.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="oft.mal0ajxo.KOMUNE">
    <dif>
      Io mal<tld/>e trovebla, malabunde okazanta:
      <ekz>
        sed, mal<tld/>a&jcirc;o inter la samtempaj poetoj, la lingvo restas
        klara kaj plene komprenebla
        <fnt>
          <aut>C. Minnaja</aut>,
          <vrk>Meritanta ne nur reeldonon</vrk>,
          <bib>Monato</bib>,
          <lok>2001:7, p. 28a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="br">ralete</trd>
  <trdgrp lng="de">
    <trd>Rarit&auml;t</trd>,
    <trd>seltenes <ind>St&uuml;ck</ind></trd>
  </trdgrp>
  <trd lng="el">&sigma;&pi;&alfa_ton;&ny;&jota;&omikron;</trd>
  <trd lng="en">rarity</trd>
  <trd lng="fr">raret&eacute;</trd>
  <trd lng="hu">ritkas&aacute;g</trd>
  <trdgrp lng="nl">
    <trd>rariteit</trd>,
    <trd>iets <ind>zeldzaam</ind>s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rarytas</trd>,
    <trd>rzadko&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">raridade</trd>
  <trd lng="es">rareza<klr> (cosa)</klr></trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sv">
    <trd>ovanlighet</trd>,
    <trd>raritet</trd>
  </trdgrp>
