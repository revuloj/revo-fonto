<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gazet</rad>/o
</kap>
<drv mrk="gazet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Perioda presa&jcirc;o sciiganta la aktuala&jcirc;ojn:
      <ekz>
        duonmonata, semajna <tld/>o;
      </ekz>
      <ekz>
        &ccirc;iutaga <tld/>o, tag<tld/>o;
      </ekz>
      <ekz>
        mensogas kiel <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="jxurna.0o">&jcirc;urnalo</ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="revu.0o">revuo</ref>,
      <ref cel="bulten.0o">bulteno</ref>,
      <ref cel="inform.0ilo">informilo</ref>.
    </refgrp>
  </snc>
  <trd lng="ar">&jim;&fatha;&ra;&kasra_ya;&dal;&fatha;&ta_marbuta;&dammatan; &jim; &jim;&fatha;&ra;&fatha_alif;&ya_hamza;&kasra;&dal;&damma;</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_v;&c_y;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_e;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_o;&c_p;&c_ib;&c_s;</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;asopis</trd>
  <trdgrp lng="de">
    <trd>Zeitung</trd>,
    <trd>Zeitschrift</trd>
  </trdgrp>
  <trd lng="en">gazette</trd>
  <trd lng="es">revista</trd>
  <trdgrp lng="fr">
    <trd>gazette</trd>,
    <trd>journal</trd>,
    <trd>p&eacute;riodique<klr tip="ind"> (publication)</klr></trd>,
    <trd>revue</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>krant</trd>,
    <trd>tijdschrift</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="gazet.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>oj en iu loko a&ubreve; lando:
      <ekz>
        la <tld/>aro amike akceptis la aperon de nia revuo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_a; <klr>(&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_y;&c_ja; &c_v;&c_y;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd>Presse</trd>,
    <trd>Zeitungslandschaft</trd>
  </trdgrp>
  <trd lng="es">prensa</trd>
  <trd lng="fr">presse<klr tip="ind"> (les journaux)</klr></trd>
  <trd lng="nl">pers<klr tip="ind"> (de kranten)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_s;&c_a;</trd>
</drv>

<drv mrk="gazet.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="jxurnalist.0o">&Jcirc;urnalisto</ref>, kiu publikigas en <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Journalist</trd>,
    <trd>Zeitungsschreiber</trd>
  </trdgrp>
  <trd lng="es">periodista</trd>
  <trd lng="fr">journaliste de presse</trd>
  <trdgrp lng="nl">
    <trd>redacteur</trd>,
    <trd>journalist</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_je;&c_t;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="gazet.0istaro">
  <kap><tld/>istaro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>istoj en iu loko a&ubreve; lando.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_a; <klr>(&c_k;&c_o;&c_l;&c_a; &c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_a;&c_w;)</klr></trd>
  <trd lng="es">prensa</trd>
  <trd lng="fr">presse<klr tip="ind"> (les journalistes)</klr></trd>
  <trd lng="nl">pers<klr tip="ind"> (de journalisten)</klr></trd>
</drv>

<drv mrk="gazet.viva0o">
  <kap>viva <tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Improviza teatro, sceniganta aktuala&jcirc;ojn, plej ofte satire.
    </dif>
    <trd lng="es"><ind>revista</ind> teatral</trd>
  </snc>
</drv>
</art>
