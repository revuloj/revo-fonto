<drv mrk="gazet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gazet.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref cel="period.0a">Perioda</ref> eldona&jcirc;o sciiganta
      la <ref cel="aktual.0a">aktuala&jcirc;ojn</ref>:
     <ekz>
        la <tld/>o Heroldo de Esperanto
        <fnt>
          <bib>LSB</bib>
          <lok>Telegramo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&ccirc;iutaga <tld/>o</ind>
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
        <trd lng="de">Tageszeitung</trd>
      </ekz>
      <ekz>
        la urba <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>regiona <tld/>o</ind>
        <fnt>
          <bib>Kon11</bib>
          <lok>Aventuro</lok>
        </fnt>;
        <trd lng="de">Regionalzeitung</trd>
      </ekz>
      <ekz>
        la hodia&ubreve;a <ind>vespera <tld/>o</ind>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
        <trd lng="de">Abendblatt</trd>
      </ekz>
      <ekz>
        <tld/>o trimonata
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        porjunulara <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eldonadi <tld/>ojn kaj &jcirc;urnalojn en la lingvo internacia
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> donis intervjuon al la <ind>loka <tld/>o</ind>
        <fnt>
          <bib>KKr</bib>
          <lok>II</lok>
        </fnt>;
        <trd lng="de">Lokalzeitung</trd>
      </ekz>
      <ekz>
        la societo eldonas monatan <tld/>on
        <ctl><nom>La Graphologie</nom></ctl>, kiu inter aliaj donas
        <ctl>portretojn grafologiajn</ctl>
        <fnt>
          <aut>J. Janowski</aut>
          <vrk>El la historio de la grafologio</vrk>
          <lok>en: Fundamenta Krestomatio de la lingvo Esperanto,
          1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> fari&gcirc;is kunredaktoro de la nacia <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>La ora a&gcirc;o de Esperanto (demandoj: Stela Besenyei-Merger, respondoj: Joanne Johns - &Gcirc;o)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aprobantoj de la ideo de hilelismo estas petataj, sendi sian adreson al la
        redakcio de tiu <tld/>o, kiu afable publikigis nian nunan leteron
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj raportis pri la lasta krimo
        <fnt>
          <bib>ChM</bib>
          <lok>8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u &ccirc;io tio staris <klr>(estis presita)</klr> en <tld/>o
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        mensogas kiel <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bulten.0o">bulteno</ref>,
      <ref cel="magazin.0o">magazino</ref>,
      <ref cel="revu.0o">revuo</ref>
