<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gazet</rad>/o
</kap>
<drv mrk="gazet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Perioda presa&jcirc;o sciiganta la aktuala&jcirc;ojn:
      <ekz>duonmonata, semajna <tld/>o;</ekz>
      <ekz>
        &ccirc;iutaga <tld/>o
        <klr>(= <ref cel="jxurna.0o">&jcirc;urnalo</ref>)</klr>;
      </ekz> 
      <ekz>
        mensogas kiel <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="revu.0o">revuo</ref>,
      <ref cel="bulten.0o">bulteno</ref>,
      <ref cel="inform.0ilo">informilo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">&ccaron;asopis</trd>
  <trdgrp lng="de">
    <trd>Zeitung</trd>,
    <trd>Zeitschrift</trd></trdgrp>
  <trd lng="en">gazette</trd>
  <trdgrp lng="fr">
    <trd>gazette</trd>,
    <trd>journal</trd>,
    <trd>p&eacute;riodique<klr tip="ind"> (publication)</klr></trd>,
    <trd>revue</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>krant</trd>,
   <trd>tijdschrift</trd>
       </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="gazet.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>oj en iu loko a&ubreve; lando:
      <ekz>
        la <tld/>aro amike akceptis la aperon de nia revuo<fnt>B</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Presse</trd>,
    <trd>Zeitungslandschaft</trd></trdgrp>
  <trd lng="fr">presse<klr tip="ind"> (les journaux)</klr></trd>
  <trd lng="nl">pers<klr tip="ind"> (de kranten)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_s;&c_a;</trd>
</drv>
<drv mrk="gazet.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu profesie skribas en <tld/>oj. 
    </dif>
    <ref tip="vid" cel="jxurna.0isto">&jcirc;urnalisto.</ref>
  </snc>
  <adm>
  Precizigi eventualan diferencon disde "jxurnalisto".
  <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
   <trd>Journalist</trd>,
   <trd>Zeitungsschreiber</trd></trdgrp>
  <trd lng="fr">journaliste</trd>
  <trdgrp lng="nl">
   <trd>redacteur</trd>,
   <trd>journalist</trd>
       </trdgrp>
  <trd lng="ru">&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>
<drv mrk="gazet.0istaro">
  <kap><tld/>istaro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>istoj en iu loko a&ubreve; lando. 
    </dif>
  </snc>
  <trd lng="fr">presse<klr tip="ind"> (les journalistes)</klr></trd>
<trd lng="nl">pers<klr tip="ind"> (de journalisten)</klr></trd>
</drv>
<drv mrk="gazet.viva0o">
  <kap>viva <tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Improviza teatro, sceniganta aktuala&jcirc;ojn, plej ofte satire. 
    </dif>
  </snc>
</drv>
</art>
