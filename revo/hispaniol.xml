<vortaro>
  <art mrk="$Id: hispaniol.xml,v 1.3 2023/11/15 17:08:01 afido Exp $">
    <kap>
      <rad>Hispaniol</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="hispaniol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Dua plej granda <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">insulo</ref> de
      Grandaj <ref tip="malprt" cel="antil.0oj">Antiloj</ref>, sur kiu situas <ref tip="prt" cel="haiti.0o">Haitio</ref>
      kaj <ref tip="prt" cel="dominik2.0o">Dominika Respubliko</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> situas sudoriente de Kubo kaj okcidente de Puerto-Riko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1688">Hispaniolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_E;&c_s;&c_p;&c_a;&c_n;&c_mol;&c_jo;&c_l;&c_a; <klr tip="amb">(&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;)</klr></trd>
    <trd lng="de">Hispaniola</trd>
    <trd lng="en">Hispaniola</trd>
    <trd lng="fr">Hispaniola</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x4F0A;&#x65AF;&#x5E15;&#x5C3C;&#x5965;&#x62C9;&#x5C9B; <pr>y&#x12B;s&#x12B;p&#xE0;n&#xED;&#xE0;ol&#x101;d&#x1CE;o</pr></trd>,
    <trd>&#x4F0A;&#x65AF;&#x5E15;&#x5C3C;&#x5965;&#x62C9; <pr>Y&#x12B;s&#x12B;p&#xE0;n&#xED;&#xE0;ol&#x101;</pr></trd>,
    <trd>&#x6D77;&#x5730;&#x5C9B; <pr>H&#x1CE;id&#xEC;D&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

  </art>

