<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: hispaniol.xml,v 1.2 2023/08/28 14:28:39 afido Exp $">
    <kap>
      <rad>Hispaniol</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="hispaniol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Dua plej granda <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">insulo</ref> de
      Grandaj <ref tip="malprt" cel="antil.0oj">Antiloj</ref>, sur kiu situas <ref tip="prt"
      cel="haiti.0o">Haitio</ref>
      kaj <ref tip="prt" cel="dominik2.0o">Dominika Respubliko</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> situas sudoriente de Kubo kaj okcidente de Puerto-Riko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1688"
              >Hispaniolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_E;&c_s;&c_p;&c_a;&c_n;&c_mol;&c_jo;&c_l;&c_a; <klr tip="amb">(&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;)</klr></trd>
    <trd lng="de">Hispaniola</trd>
    <trd lng="en">Hispaniola</trd>
    <trd lng="fr">Hispaniola</trd>
  </snc>
</drv>

  </art>

