<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kafeteri.xml,v 1.5 2022/04/14 07:30:02 revo Exp $">
<kap>
  <rad>kafeteri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kafeteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="kafeteri.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="mangx.0ejo.KUI">Man&gcirc;ejo</ref>
      kun <ref tip="prt" cel="bufed.0o.servo">bufedo</ref> a&ubreve; a&ubreve;tomatoj, kie oni ricevas dum la tuta
      tago
      varmajn a&ubreve; malvarmajn trinka&jcirc;ojn kaj kelkajn man&gcirc;a&jcirc;ojn:
      <ekz>
        lernantoj &gcirc;uas man&gcirc;a&jcirc;ojn en la universitata <tld/>o
        <fnt>
          <aut>Zhang Shuang, Liu Zijia</aut>,
          <vrk><url ref="http://www.espero.com.cn/se/txt/2013-04/26/content_537382.htm">
          Mezlernejanoj el la tertremo-trafita regiono rekomencis
          lernadon en Chengdu</url></vrk>,
          <lok>espero.com.cn, 2013-04-26</lok>
        </fnt>.
      </ekz>
      <ekz>
        bildo, farita en unu el <tld/>oj de <nom>Jekaterinburg</nom>
        <fnt>
          <bib>LOdE</bib>
          <aut>Valentin Melnikov</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">Gasto el
          Pollando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="bufed.0ejo">bufedejo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="bufed.0o.ejo">bufedo<sncref/></ref>,
      <ref cel="kantin.0o.KUI">kantino</ref>,
      <ref cel="kaf.0ejo">kafejo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_f;&c_e;&c_t;&c_e;&c_r;&c_y;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Cafeteria</trd>,
    <trd>Imbisstube</trd>
  </trdgrp>
  <trd lng="fr">caf&eacute;t&eacute;ria</trd>
  <trd lng="pl">kafeteria</trd>     
  <trd lng="ro">cofet&abreve;rie</trd>
  <trd lng="uk">&#x43A;&c_a;&c_f;&c_je;&c_t;&c_je;&c_r;&c_ib;&c_j;</trd>
</drv>
</art>

