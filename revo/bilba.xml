<vortaro>
  <art mrk="$Id: bilba.xml,v 1.1 2023/08/06 12:37:44 afido Exp $">
    <kap>
      <rad>Bilba</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="bilba.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="bilba.0o.urbo">
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> en
      la nordo de <ref tip="malprt" cel="hispan.H0ujo">Hispanujo</ref>,
      &ccirc;. 325&#8239;km norde de Madrido:
      <ekz>
        post kongreso en Bilbao, mia edzino kaj mi buse veturis <klr>[&#8230;]</klr> tra unu
        el la plej imponaj montaraj pejza&gcirc;oj en Hispanio
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2004/008467.html"
              >Kampoj de Soria Antonio Machado, universala poeto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la UNESKO-Centro de Vaskaj Landoj (E&ubreve;skio) en <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 5 (55)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bilbao</trd>
    <trd lng="en">Bilbao</trd>
    <trd lng="eu">Bilbo</trd>
    <trd lng="fr">Bilbao</trd>
    <trd lng="hi">Bilbao</trd>
  </snc>
</drv>

