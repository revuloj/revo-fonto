<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Konfuce</rad>/o</kap>
<drv mrk="konfuce.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      &Ccirc;inlingve Kong-Fu-ce (Kong la Majstro), proks. 551 a.K. &ndash; 479 a.K.
      Antikva &ccirc;ina pensulo, fondinto de <tld lit="k"/>anismo.
    </dif>
  </snc>
  <trd lng="de">Konfuzius</trd>
  <trd lng="en">Confucius</trd>
  <trdgrp lng="hu">
    <trd>Konfuciusz</trd>,
    <trd>Kung Fu-ce</trd></trdgrp>
  <trd lng="nl">Confucius</trd>
  <trdgrp lng="ru">
    <trd>&c_K;&c_o;&c_n;&c_f;&c_u;&c_c;&c_i;&c_j;</trd>,
    <trd>&c_K;&c_u;&c_n;-&c_c;&c_z;&c_y;</trd>
  </trdgrp>
  <trd lng="sk">Konfucius</trd>
  <trdgrp lng="zh">
    <trd>&#23380;&#22827;&#23376; <klr>[K&#466;ngf&#363;z&#464;]</klr></trd>
    <trd>&#23380;&#23376; <klr>[K&#466;ngz&#464;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="konfuce.0ano">
  <kap><tld lit="k"/>ano</kap>
  <snc>
    <dif>
      Kredanto je <tld lit="k"/>anismo.
    </dif>
  </snc>
  <trd lng="de">Konfuzianer</trd>
  <trd lng="en">Confucian</trd>
  <trd lng="hu">konfuci&aacute;nus</trd>
  <trd lng="nl">confucianist</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_f;&c_u;&c_c;&c_i;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">konfuci&aacute;n</trd>
</drv>

<drv mrk="konfuce.0anismo">
  <kap><tld lit="k"/>anismo</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Etika, socia kaj religia doktrino, aperinta en antikva &Ccirc;inujo
      kaj ege influinta evoluon de kulturo, politiko kaj socia vivo
      de &Ccirc;inujo dum pli ol du jarmiloj. La&ubreve; <tld lit="k"/>anismo,
      la potenco de regnestro estas sankta kaj donacita de la &ccirc;ielo,
      kaj disdivido de homoj je superaj kaj malsuperaj estas universala
      le&gcirc;o de justeco. <tld/>anismo opinias bazo de socio moralan
      perfekti&gcirc;on kaj obeadon al normoj de etiketo.
    </dif>
  </snc>
  <trd lng="de">Konfuzianismus</trd>
  <trd lng="en">Confucianism</trd>
  <trd lng="hu">konfucianizmus</trd>
  <trd lng="nl">confucianisme</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_f;&c_u;&c_c;&c_i;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">konfucianizmus</trd>
</drv>
</art>

