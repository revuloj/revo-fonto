<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>halo</rad>/o
</kap>
<drv mrk="halo.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">AST</uzo>
    <dif>
      Luma cirklo, ringo, kiu &ccirc;irka&ubreve;as ofte astrojn:
      <ekz>suno, luna <tld/>o.</ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Hof</trd>,
      <trd>Halo</trd>
    </trdgrp>
    <trd lng="nl">halo</trd>
    <trd lng="ru">&c_g;&c_a;&c_l;&c_o;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Luma difuzo, kiu &ccirc;irka&ubreve;as la fotografan figuron de brilanta
      punkto ricevita sur impresebla plako.
    </dif>
    <trd lng="de">Lichthof</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_o;</trd>
  <trd lng="en">halo</trd>
  <trd lng="fr">halo</trd>
  <trdgrp lng="hu">
    <trd>halo</trd>,
    <trd>f&eacute;nyudvar</trd>
  </trdgrp>
  <trd lng="pt">halo</trd>
</drv>

<drv mrk="halo.kontraux0ilo">
  <kap>kontra&ubreve;<tld/>ilo</kap>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      &Scirc;irmilo kontra&ubreve; la <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">antihalo<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">f&eacute;nyudvar-mentes&iacute;t&odblac;</trd>
</drv>
</art>
