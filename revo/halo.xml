<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>halo</rad>/o
</kap>
<drv mrk="halo.0o">
  <kap><tld/>o</kap>
  <snc mrk="halo.0o.KOMUNE">
    <dif>
      Nebula heleco &ccirc;irka&ubreve;anta lumfonton:
      <ekz>
        la ru&gcirc;a lumo de la neonlampo brilis kun <tld/>o en silka
        kurteno de printempa pluvo
        <fnt><aut>Ueyama M.</aut>, <vrk>Pardonon!</vrk>, <lok>l'
        omnibuso, 1970</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="halo.0o.AST">
      <uzo tip="fak">AST</uzo>
      <dif>
        Luma cirklo, ringo, kiu &ccirc;irka&ubreve;as ofte astrojn:
        <ekz>
          suna, luna <tld/>o.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Hof</trd>,
        <trd>Halo</trd>
      </trdgrp>
      <trd lng="nl">halo</trd>
      <trd lng="ru">&c_g;&c_a;&c_l;&c_o;</trd>
    </subsnc>
    <subsnc mrk="halo.0o.FOT">
      <uzo tip="fak">FOT</uzo>
      <dif>
        Luma difuzo, kiu &ccirc;irka&ubreve;as la fotografan figuron de
        brilanta punkto ricevita sur impresebla plako.
      </dif>
      <trd lng="de">Lichthof</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="nimb.0o">nimbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_o;</trd>
  <trd lng="ca">halo</trd>
  <trd lng="en">halo</trd>
  <trd lng="es">halo</trd>
  <trd lng="fa">&a_h;&a_A;&a_l;&a_h;</trd>
  <trd lng="fr">halo</trd>
  <trdgrp lng="hu">
    <trd>halo</trd>,
    <trd>f&eacute;nyudvar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>halo</trd>,
    <trd>aureola</trd>
  </trdgrp>
  <trd lng="pt">halo</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_o;</trd>,
    <trd>&c_o;&c_r;&c_je;&c_o;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="halo.kontraux0ilo">
  <kap>kontra&ubreve;<tld/>ilo</kap>
  <snc mrk="halo.kontraux0ilo.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      &Scirc;irmilo kontra&ubreve; la <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">antihalo<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">f&eacute;nyudvar-mentes&iacute;t&odblac;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_o;&c_r;&c_je;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>
</drv>
</art>
