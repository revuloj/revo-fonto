<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>stimul</rad>/i
</kap>
<drv mrk="stimul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Peni por ke iu jam aganta agu pli vigle.
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Plivigligi la aktivecon de iu a&ubreve; io:
      <ekz>
        tiu ideo estas la <ind><tld/>o</ind> de ilia laborado<fnt>Z</fnt>.
        <trd lng="ru">&c_s;&c_t;&c_i;&c_m;&c_u;&c_l;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekscit.0i">eksciti</ref>,
      <ref cel="incit.0i">inciti</ref>,
      <ref cel="instig.0i">instigi</ref>,
      <ref cel="fervor.0igi">fervorigi</ref>,
      <ref cel="spron.0i">sproni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>stimulieren</trd>,
    <trd>anregen</trd>,
    <trd>anreizen</trd>,
    <trd>antreiben</trd>,
    <trd>vorantreiben</trd>,
    <trd>anstacheln</trd>,
    <trd>anspornen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stimuleren</trd>,
    <trd>aansporen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_i;&c_m;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
