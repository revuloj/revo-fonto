<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: navig.xml,v 1.22 2022/04/25 07:30:25 revo Exp $">
    <kap>
      <rad>navig</rad>/i <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="navig.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="navig.0i.veturi">
        <uzo tip="fak">MAR</uzo>
        <dif>
          Sin transporti per tiucela veturilo (precipe &scirc;ipo) tra maro,
          rivero, lago; tra aero a&ubreve; kosmo:
          <ekz>
            la&ubreve; irlanda tradicio, la sanktulo Brendan <tld/>is per
            koraklo al
            la ekstrema&jcirc;o de la oceano
            <fnt><aut>G. MacAoidh</aut>:
              <vrk>Frua irlanda migrado al Islando</vrk>,
              <bib>Monato</bib>
              <lok>jaro 1998a, numero 7a, p. 22a</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="sxip.0i">&scirc;ipi</ref>,
          <ref cel="ir.sxip0i">&scirc;ipiri</ref>
        </refgrp>
      </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>navigovat</trd>,
    <trd>plavit se</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>zur See fahren</trd>,
        <trd>Fl&uuml;sse befahren</trd>,
        <trd>Seen befahren</trd>,
        <trd>Meere befahren</trd>
      </trdgrp>
  <trd lng="en">navigate</trd>
  <trd lng="es">navegar</trd>
  <trdgrp lng="fr">
        <trd>naviguer</trd>,
        <trd>voguer</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>navigeren</trd>,
        <trd>varen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawigowa&cacute;</trd>,
    <trd>&zdot;eglowa&cacute;</trd>
  </trdgrp>    
  <trd lng="pt">navegar</trd>
  <trd lng="ro">naviga</trd>
  <trd lng="ru">&c_p;&c_l;&c_y;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>navigova&tcaron;</trd>,
    <trd>plavi&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_i;&c_s;&c_t;&c_i; (&#x43D;&c_a; &#x43A;&#x43E;&c_r;&c_a;&c_b;&#x43B;&c_ib;)</trd>,
    <trd>&#x43B;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_d;&c_ib;&c_j;&c_s;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43E;&#x43B;&#x44C;&#x43E;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="navig.0ado">
      <kap><tld/>ado</kap>
      <snc mrk="navig.0ado.veturado">
        <dif>
          La ago <tld/>i<sncref ref="navig.0i.veturi"/>:
          <ekz>
            post longa mar<tld/>ado
            <fnt><aut>R. Roze</aut>:
              <vrk>&leftquot;Bravado&rightquot; en Saint-Tropez</vrk>,
              <bib>Monato</bib><lok>jaro 1997a, numero 2a, p.
              28a</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_n;&c_a;&c_v;&c_ib;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>,
          <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Fahrt</trd>,
          <trd>Navigation</trd>
        </trdgrp>
        <trd lng="en">navigation</trd>
        <trd lng="es">navegaci&oacute;n</trd>
        <trd lng="fr">navigation</trd>
        <trdgrp lng="nl">
          <trd>navigeren<klr tip="ind"> (zn.)</klr></trd>,
          <trd>varen<klr tip="ind"> (zn.)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_s;&c_u;&c_d;&c_o;&c_h;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
          <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_je;<klr> (&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr></trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>lodn&iacute; doprava</trd>,
    <trd>navigace</trd>,
    <trd>plavba</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawigacja</trd>,
    <trd>&zdot;egluga</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>orientare</trd>,
    <trd>naviga&#x21B;ie</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lodn&aacute; doprava</trd>,
    <trd>navig&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_v;&c_ib;&c_gu;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43F;&#x43B;&c_a;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&#x44C;&#x43E;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="navig.0ebla">
      <kap><tld/>ebla</kap>
      <snc mrk="navig.0ebla.MAR">
        <dif>
          &Scirc;ipirebla:
          <ekz>
            la fora Stanley-lago, de kie la Kongo-riverego estas
            <tld/>ebla
            <fnt><aut>W. M. A. De Smet</aut>:
              <vrk>Anta&ubreve; cent jaroj</vrk>, <bib>Monato</bib>
              <lok>jaro 1998a, numero 5a, p. 25a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_s;&c_u;&c_d;&c_a;&c_h;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="cs">splavn&yacute;</trd>
  <trdgrp lng="de">
        <trd>befahrbar</trd>,
        <trd>schiffbar</trd>
      </trdgrp>
  <trd lng="en">navigable</trd>
  <trd lng="es">navegable</trd>
  <trd lng="fr">navigable</trd>
  <trd lng="nl">bevaarbaar</trd>
  <trd lng="pl">&zdot;eglowny</trd>
  <trd lng="ro">navigabil</trd>
  <trd lng="ru">&c_s;&c_u;&c_d;&c_o;&c_h;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">splavn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_d;&#x43D;&#x43E;&#x43F;&#x43B;&c_a;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_d;&c_a;&c_t;&#x43D;&c_i;&c_j; &c_d;&#x43E; &#x43F;&#x43B;&c_a;&c_v;&c_a;&#x43D;&#x43D;&#x44F; (&#x43F;&c_r;&#x43E;
    &c_v;&#x43E;&c_d;&#x43D;&c_i;&c_j; &c_sh;&#x43B;&#x44F;&c_h;)</trd>
  </trdgrp>
</drv>

    <drv mrk="navig.0isto">
      <kap><tld/>isto</kap>
      <snc mrk="navig.0isto.MAR">
        <uzo tip="fak">MAR</uzo>
        <dif>
          Homo, kiu profesie <tld/>as:
          <ekz>
            La irlanda kronikisto Adamnan (&ccirc;. 624&ndash;704 p.K.) asertas, ke
            la <tld/>isto estis ne Brandan
            <fnt><aut>G. MacAoidh</aut>:
              <vrk>Frua irlanda migrado al Islando</vrk>,
              <bib>Monato</bib><lok>jaro 1998a, numero 7a, p.
              22a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_ib;&c_g;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">navig&aacute;tor</trd>
  <trd lng="de">Navigator</trd>
  <trd lng="en">navigator</trd>
  <trd lng="es">navegador</trd>
  <trd lng="fr">navigateur</trd>
  <trd lng="nl">zeevaarder</trd>
  <trd lng="pl">nawigator</trd>
  <trd lng="ro">navigator</trd>
  <trdgrp lng="ru">
        <trd>&c_m;&c_o;&c_r;&c_je;&c_p;&c_l;&c_a;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
        <trd>&c_m;&c_o;&c_r;&c_je;&c_h;&c_o;&c_d;</trd>
      </trdgrp>
  <trd lng="sk">navig&aacute;tor</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&c_c;&#x43C;&c_a;&#x43D;</trd>,
    <trd>&#x43F;&c_ib;&#x43B;&#x43E;&c_t;</trd>
  </trdgrp>
</drv>

    <drv mrk="navig.0ilo">
      <kap><tld/>ilo</kap>
      <snc mrk="navig.0ilo.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Rimedo por <tld/>i en la Interreto (la Kiberkosmo), en
          komplikita datumstrukturo ktp:
          <ekz>
            Mozilla evoluis el Neckejp-<tld/>ilo <klr>(Netscape
            Navigator)</klr>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="navig.0ilo.bira">
        <ref tip="dif" cel="naviga.0ilo.birilo">navigaciilo<sncref/></ref>
      </snc>
      <trd lng="be">&c_n;&c_a;&c_v;&c_ib;&c_g;&c_a;&c_t;&c_a;&c_r; <klr>(&c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;, &c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="cs">plavidlo</trd>
  <trd lng="en">navigator</trd>
  <trd lng="pl">przegl&aogonek;darka sieciowa</trd>
  <trd lng="ro">browser web</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">plavidlo</trd>
</drv>

  </art>
