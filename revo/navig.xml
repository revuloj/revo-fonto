    &c_v;&c_o;&c_d;&c_n;&c_i;&c_j; &c_sh;&c_l;&c_ja;&c_h;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#x822A;&#x884C; <pr>k&#x11B;h&#xE1;ngx&#xED;ng</pr></trd>,
    <trd>&#x53EF;&#x901A;&#x8239; <pr>k&#x11B;t&#x14D;ngchu&#xE1;n</pr></trd>,
