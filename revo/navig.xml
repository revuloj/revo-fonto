<drv mrk="navig.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="navig.0i.veturi">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Sin transporti per tiucela veturilo (precipe &scirc;ipo) tra maro,
      rivero, lago (tra aero, kosmo):
      <ekz>
        la procesio de la nebulaj konturoj jam <tld/>is en la golfon, <klr>[&#x2026;]</klr> la
        remiloj ritme batis
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ekfunkciigis la motoron kaj mi el<tld/>is al la maro
        <fnt>
          <bib>VaK</bib>
          <lok>Dekna&ubreve;a &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; irlanda tradicio, la sanktulo <nom>Brendan</nom> <tld/>is per
        koraklo al la ekstrema&jcirc;o de la oceano
        <fnt>
          <aut>G. MacAoidh</aut>:
          <vrk>Frua irlanda migrado al Islando</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1998a, numero 7a, p. 22a</lok>
        </fnt>;
      </ekz>
      <ekz>
        perflose <tld/>i <klr>[&#x2026;]</klr> sur la rivero
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2001/007186.html">Vakeroj de Alpoj</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 10</lok>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kristandemokratoj <klr>[&#x2026;]</klr> <tld/>is sur ondego da simpatio
        <fnt>
       <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2000/006835.php">Pekoj de patriarko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 9</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="naviga.0i.veturi">navigacii<sncref/></ref>,
      <ref cel="sxip.0i">&scirc;ipi</ref>,
      <ref cel="ir.sxip0i">&scirc;ipiri</ref>
    </refgrp>
    <ref tip="super" cel="direkt.0i.stiri">direkti<sncref/></ref>
    <refgrp tip="sub">
      <ref cel="board.0i">boardi</ref>,
      <ref cel="driv.0i.MAR">drivi<sncref/></ref>,
      <ref cel="flos.0i">flosi</ref>,
      <ref cel="vel.0i">veli</ref>
    </refgrp>
  </snc>
  <snc mrk="navig.0i.retumi">
    <uzo tip="fak">RET</uzo>
    <dif>
      <ref tip="dif" cel="ret.0umi">Retumi</ref>:
      <ekz>
        <klr>[la retumilo]</klr> inkluzivas <klr>[&#x2026;]</klr> Guglo-ser&ccirc;ejon kaj la eblon
        <tld/>i per langetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008483.php">Foliumiloj denove interbatalas</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">surfen <klr>(Web)</klr></trd>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>navigovat</trd>,
    <trd>plavit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zur See fahren</trd>,
    <trd>Fl&uuml;sse befahren</trd>,
    <trd>Seen befahren</trd>,
    <trd>Meere befahren</trd>
  </trdgrp>
  <trd lng="en">navigate</trd>
  <trd lng="es">navegar</trd>
  <trdgrp lng="fr">
    <trd>naviguer</trd>,
    <trd>voguer</trd>
  </trdgrp>
  <trd lng="ja">&#x822A;&#x884C;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>navigeren</trd>,
    <trd>varen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawigowa&cacute;</trd>,
    <trd>&zdot;eglowa&cacute;</trd>
  </trdgrp>    
  <trd lng="pt">navegar</trd>
  <trd lng="ro">naviga</trd>
  <trd lng="ru">&c_p;&c_l;&c_y;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>navigova&tcaron;</trd>,
    <trd>plavi&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_i;&c_s;&c_t;&c_i; (&c_n;&c_a; &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_ib;)</trd>,
    <trd>&c_l;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_d;&c_ib;&c_j;&c_s;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_p;&c_o;&c_l;&c_mol;&c_o;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9818;&#x822A; <pr>l&#x1D0;ngh&#xE1;ng</pr></trd>,
    <trd>&#x9886;&#x822A; <pr>l&#x1D0;ngh&#xE1;ng</pr></trd>,
    <trd>&#x822A; <pr>h&#xE1;ng</pr></trd>,
    <trd>&#x822A;&#x884C; <pr>h&#xE1;ngx&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="navig.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="navig.0o.veturado">
    <dif>
      La ago <tld/>i<sncref ref="navig.0i.veturi"/>:
      <ekz>
        generalo <nom>S&eacute;bert</nom> <klr>[&#x2026;]</klr> partoprenis pro malsano 
        nek la <tld/>on, nek la
        kongreson
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/008392.html">Reveno de heroo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas du profesioj, al kiuj angla homo jam 
        denaske inklinas: la <tld/>ado kaj
        la teksindustrio
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vasili Ostrov</nom>, granda insulo inter la du brakoj 
        de <nom>Neva</nom>, sur kiu komerco kaj
        <tld/>ado en Peterburgo havas sian &ccirc;efan lokon
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>En Peterburgo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post longa mar<tld/>ado
        <fnt><aut>R. Roze</aut>:
          <vrk>&leftquot;Bravado&rightquot; en Saint-Tropez</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 2a, p.
          28a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_v;&c_ib;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Fahrt</trd>,
      <trd>Navigation</trd>
    </trdgrp>
    <trd lng="en">navigation</trd>
    <trd lng="es">navegaci&oacute;n</trd>
    <trd lng="fr">navigation</trd>
    <trdgrp lng="nl">
      <trd>navigeren<klr tip="ind"> (zn.)</klr></trd>,
      <trd>varen<klr tip="ind"> (zn.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_d;&c_o;&c_h;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_je;<klr> (&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x9818;&#x822A; <pr>l&#x1D0;ngh&#xE1;ng</pr></trd>,
    <trd>&#x9886;&#x822A; <pr>l&#x1D0;ngh&#xE1;ng</pr></trd>,
    <trd>&#x5BFC;&#x822A; <pr>d&#x1CE;oh&#xE1;ng</pr></trd>,
    <trd>&#x5C0E;&#x822A; <pr>d&#x1CE;oh&#xE1;ng</pr></trd>,
    <trd>&#x822A; <pr>h&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>lodn&iacute; doprava</trd>,
    <trd>navigace</trd>,
    <trd>plavba</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x822A;&#x884C;  <pr>&#x3053;&#x3046;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x822A;&#x6D77;  <pr>&#x3053;&#x3046;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x822A;&#x7A7A;  <pr>&#x3053;&#x3046;&#x304F;&#x3046;</pr></trd>,
    <trd>&#x822A;&#x6CD5;  <pr>&#x3053;&#x3046;&#x307B;&#x3046;</pr></trd>
