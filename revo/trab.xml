<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>trab</rad>/o
</kap>
<drv mrk="trab.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Longa, dika kvadratigita lignopeco servanta ordinare por subteni
       partojn de konstruo:
       <ekz>
         meti <tld/>ojn sur la vojon
         <klr>(f: estigi malhelpa&jcirc;ojn.
         <ref tip="vid" cel="baston.0o">bastono</ref>)</klr>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         en fremda okulo ni vidas ligneron, en nia ni <tld/>on
         ne vidas <klr>(ni estas pli indulgaj por niaj difektoj
         ol por tiuj de la aliaj homoj)</klr>
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_b;&c_je;&c_r;&c_v;&c_ja;&c_n;&c_o;</trd>,
     <trd>&c_b;&c_e;&c_l;&c_mol;&c_k;&c_a;</trd>,
     <trd>&c_b;&c_r;&c_u;&c_s;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>poutre</trd>,
     <trd>charpente</trd>
   </trdgrp>
   <trd lng="de">Balken</trd>
   <trd lng="en">beam</trd>
   <trdgrp lng="hu">
     <trd>gerenda</trd>,
     <trd>tart&oacute;</trd>
   </trdgrp>
   <trd lng="nl">balk</trd>
  <trdgrp lng="pt">
    <trd>trave</trd>,
    <trd>viga</trd>,
    <trd>barrote</trd>
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_b;&c_a;&c_l;&c_k;&c_a;</trd>,
     <trd>&c_b;&c_r;&c_u;&c_s;</trd>
   </trdgrp>
</drv>

<drv mrk="trab.0ajxo0aro">
   <kap><tld/>a&jcirc;o, <tld/>aro</kap>
   <snc>
     <dif>
       Kuna&jcirc;o da <tld/>oj, &ccirc;arpenta&jcirc;o.
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_r;&c_u;&c_b;</trd>
   <trdgrp lng="fr"><trd>poutrage</trd><trd>charpente</trd></trdgrp>
   <trdgrp lng="hu">
     <trd>gerend&aacute;zat</trd>,
     <trd>&aacute;csolat</trd>
   </trdgrp>
   <trd lng="ru">&c_s;&c_r;&c_u;&c_b;</trd>
</drv>
</art>
