<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kom</rad>/o
</kap>
<drv mrk="kom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Interpunkcia <ref tip="super" cel="sign.0o">signo</ref> (,)
      indikanta, ke oni devas apartigi partojn de propozicio a&ubreve;
      propoziciojn unu de la alia, intermetante tre mallongan pa&ubreve;zon. 
    </dif>
  </snc>
  <trd lng="cs">&ccaron;&aacute;rka</trd>
  <trd lng="en">comma</trd>
  <trd lng="nl">komma</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_ja;&c_t;&c_a;&c_ja;</trd>
</drv>
<drv mrk="kom.punkto0o">
  <kap>punkto<tld/>o</kap>
  <snc>
    <dif>
      Interpunkcia <ref tip="super" cel="sign.0o">signo</ref> (;)
      indikanta iom pli longan pa&ubreve;zon ol <tld/>o inter partoj de frazo.
    </dif>
  </snc>
  <trd lng="en">semicolon</trd>
  <trd lng="nl">kommapunt</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_k;&c_a; &c_s; &c_z;&c_a;&c_p;&c_ja;&c_t;&c_o;&c_j;</trd>
</drv>
</art>
