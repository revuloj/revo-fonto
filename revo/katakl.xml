<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kataklism</rad>/o
</kap>
<drv mrk="katakl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Naturkatastrofo, ka&ubreve;zanta tre gravajn difektojn kaj &scirc;an&gcirc;ojn
      sur la surfaco de la tero.  
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>catastrophe</trd>,
    <trd>cataclysm</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>natuurramp</trd>,
    <trd>cataclysme</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_k;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
