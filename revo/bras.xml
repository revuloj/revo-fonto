<vortaro>
  <art mrk="$Id: bras.xml,v 1.5 2025/06/01 20:00:21 afido Exp $">
    <kap>
      <rad>bras</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="bras.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SHI</uzo>
  <snc>
    <dif>
      Unu el la <ref tip="super" cel="sxnur.0o.KOMUNE">&scirc;nuro</ref>j amba&ubreve;flanke fiksitaj
      &ccirc;e velstango (jardo, gafo) por turni &gcirc;in la&ubreve; la vento:
      <ekz>
        la pigra &scirc;ipanaro <klr>[&#8230;]</klr> malamis laboron kun &scirc;nuraro, kvaza&ubreve; <tld/>oj,
        kargo&scirc;nuroj kaj &scirc;kotoj estis personaj malamikoj de &ccirc;iu matroso
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="Noveloj, Cutty
          Sark">https://eduinf.waw.pl/esp/lern/librejo/0207/0010.php</url></vrk>
          <lok>skunbarko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Brasse</trd>
    <trd lng="en">brace <klr>(sailing)</klr></trd>
    <trd lng="fr">br&egrave;me</trd>
    <trd lng="pl">bras</trd>
  </snc>
<trd lng="be">&c_b;&c_r;&c_a;&c_s; <klr
tip="amb">(&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  <trd lng="ca">bra&ccedil;a <klr tip="amb">(cap o corda)</klr></trd>
</drv>
