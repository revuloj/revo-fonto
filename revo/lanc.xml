<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lanc</rad>/o
</kap>
<drv mrk="lanc.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Batalilo konsistanta el longa ligna stango kun akra ferpinto:
      <ekz>
	<ind><tld/>isto</ind>.
	<trd lng="hu">l&aacute;ndzs&aacute;s</trd>
	<trd lng="pl">oszczepnik</trd>
	<trd lng="ru">&c_k;&c_o;&c_p;&c_je;&c_j;&c_shch;&c_i;&c_k;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_d;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Lanze</trd>,
     <trd>Spie&szlig;</trd>,
     <trd>Speer<klr> [por &jcirc;etado]</klr></trd>
  </trdgrp>
  <trd lng="el">&delta;&omikron_ton;&rho;&ypsilon;</trd>
  <trdgrp lng="en">
    <trd>lance</trd>,
    <trd>spear</trd>
  </trdgrp>
  <trd lng="es">lanza</trd>
  <trd lng="hu">l&aacute;ndzsa</trd>
  <trd lng="nl">lans</trd>
  <trdgrp lng="pl">
    <trd>lanca</trd>,
    <trd>dzida</trd>,
    <trd>oszczep</trd>
  </trdgrp>
  <trd lng="pt">lan&ccedil;a</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_p;&c_mol;&c_jo;</trd>,
    <trd>&c_p;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>lans</trd>,
    <trd>spjut</trd>
  </trdgrp>
</drv>

<drv mrk="lanc.0eto">
  <kap>
    <tld/>eto<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      &Hcirc;irurgia speciala tran&ccirc;ileto, por piki vejnon,
      entran&ccirc;i absceson, kc..
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_n;&c_c;&c_e;&c_t;</trd>
  <trd lng="de">Lanzette</trd>
  <trd lng="el">&ny;&ypsilon;&sigma;&tau;&epsilon_ton;&rho;&jota;</trd>
  <trdgrp lng="en">
    <trd>lancet</trd>,
    <trd>skalpel</trd>
  </trdgrp>
  <trd lng="hu">lancetta</trd>
  <trd lng="nl">lancet</trd>
  <trdgrp lng="pl">
    <trd>lancet</trd>,
    <trd>skalpel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bisturi</trd>,
    <trd>escalpelo (&ecirc;)</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_c;&c_je;&c_t;</trd>
</drv>
</art>
