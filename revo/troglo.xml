<kap>
  <rad>troglodit</rad>/o
</kap>
<drv mrk="troglo.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">PRA</uzo>
    <dif>
      <ref tip="super" cel="hom.pra0o">Prahomo</ref>
      lo&gcirc;anta en kavernoj; kavernulo:
      <ekz>
        <tld/>a arkitekturo ekzistas en diversaj landoj de la mondo i.a. en
        Francio (Luar-valo), Nederlando, Irano, Italio, Tunizio, &Ccirc;inio, Hispanio,
        temas pri lokoj tute a&ubreve; parte elfositaj el &scirc;tono a&ubreve; marno, povas esti anka&ubreve; rifu&gcirc;ejo en kaverno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=566419">Troglodita (arkitekturo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Oni nomas anka&ubreve; malgrandan birdogenron <ref tip="hom" cel="troglo1">troglodito</ref>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_y;&c_t;</trd>,
    <trd>&c_p;&c_ja;&c_ch;&c_o;&c_r;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_r;&c_o;&c_g;&c_l;&c_o;&c_d;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_shch;&c_je;&c_r;&c_je;&c_n; <ind>&c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;</ind></trd>
  </trdgrp>
  <trd lng="ca">troglodita</trd>
  <trdgrp lng="cs">
    <trd>jeskynn&iacute; &ccaron;lov&ecaron;k</trd>,
    <trd>troglodyt</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>H&ouml;hlenmensch</trd>,
    <trd>H&ouml;hlenbewohner</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_n;&a_s;&a_A;&a_n; <ind>&a_gh;&a_A;&a_r;&a_n;&a_sh;&f_y;&a_n;</ind></trd>
  <trd lng="fr">troglodyte</trd>
  <trd lng="hu">barlanglak&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x7A74;&#x5C45;&#x4EBA;  [<ind>&#x3051;&#x3063;&#x304D;&#x3087;&#x3058;&#x3093;</ind>]</trd>,
    <trd>&#x30DF;&#x30BD;&#x30B5;&#x30B6;&#x30A4;</trd>
  </trdgrp>
  <trd lng="nl">holbewoner</trd>
  <trd lng="pt">troglodita</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_g;&c_l;&c_o;&c_d;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_shch;&c_je;&c_r;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pravek&yacute; jaskynn&yacute; &ccaron;lovek</trd>,
    <trd>troglodyt</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x440;&#x43E;&#x433;&#x43B;&#x43E;&#x434;&#x438;&#x442;</trd>,
    <trd>&#x43F;&#x435;&#x447;&#x435;&#x440;&#x43D;&#x430; &#x43B;&#x44E;&#x434;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x432;&#x43E;&#x43B;&#x43E;&#x432;&#x435; &#x43E;&#x447;&#x43A;&#x43E;</trd>
  </trdgrp>
</drv>
