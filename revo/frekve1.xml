<kap><rad>frekvencometr</rad>/o</kap>
<drv mrk="frekve1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref cel="aparat.0o.TEK">Aparato</ref> por mezuri principe la
      ondlongon kaj fakte la <ref cel="frekve.0o.TEK">frekvenco</ref>n de radioelektraj ondoj:
      <ekz>
        frekvencometro <klr>[&#x2026; kutime]</klr> mezuras anka&ubreve; periodojn, tempon, impulsoda&ubreve;ron kaj
        aliajn parametrojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1337">Frekvencometro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_a;&c_t;&c_a;&c_m;&c_je;&c_r;</trd>
  <trd lng="cs">m&ecaron;&rcaron;i&ccaron; frekvence</trd>
  <trd lng="de">Frequenzmesser</trd>
  <trd lng="es">frecuenci&oacute;metro</trd>
  <trd lng="fr">fr&eacute;quencem&egrave;tre</trd>
  <trd lng="hu">frekvenciam&eacute;r&odblac;</trd>
  <trd lng="nl">frequentiemeter</trd>
  <trdgrp lng="pt">
    <trd>freq&uuml;enc&iacute;metro</trd>,
    <trd>freq&uuml;enci&ocirc;metro</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_o;&c_t;&c_o;&c_m;&c_je;&c_r;</trd>
  <trd lng="sk">mera&ccaron; frekvencie</trd>
  <trd lng="uk">&#x447;&#x430;&#x441;&#x442;&#x43E;&#x442;&#x43E;&#x43C;&#x456;&#x440;</trd>
</drv>
