<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sumo</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sumo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Japan-devena lukto-<ref tip="super" cel="sport.0o">sporto</ref>,
      en kiu per diversaj &jcirc;etoj kaj pu&scirc;oj oni forigu la
      kontra&ubreve;ulon el la areno, a&ubreve; tu&scirc;igu la grundon
      per alia korpoparto ol plandoj.
    </dif>
  </snc>
  <trd lng="de">Sumo</trd>
  <trdgrp lng="fr">
    <trd>sumo</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_o;</trd>
</drv>
</art>

