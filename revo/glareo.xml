<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glareo.xml,v 1.15 2018/12/25 13:10:14 revo Exp $">
<kap>
<rad>glareol</rad>/o
</kap>
<drv mrk="glareo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="glareo1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Glareola</trd>):
      <ekz>
        la <tld/>oj <klr>[&#8230;]</klr> havas mallongajn krurojn, tre longajn pintajn
        flugilojn kaj longajn forkoformajn vostojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=197231"
              >Glareolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/07/Glareola_maldivarum_-_Beung_Borapet.jpg/640px-Glareola_maldivarum_-_Beung_Borapet.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Glareola maldivarum), Beung Borapet, Nahkon Sawan, Tajlado
        <fnt>
          <aut>JJ Harrison (jjharrison89@facebook.com), propra verko</aut>
          <lok><url ref="&WCW;/File:Glareola_maldivarum_-_Beung_Borapet.jpg"
              >https://commons.wikimedia.org/wiki/File:Glareola_maldivarum_-_Beung_Borapet.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="ca">
    <trd>glar&egrave;ola</trd>,
    <trd>perdiu de mar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ouhorlik</trd>,
    <trd>ouhorl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Brachschwalbe</trd>
  <trd lng="en">pratincole</trd>
  <trd lng="fr">glar&eacute;ole</trd>
  <trd lng="hu">sz&eacute;kics&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>pluvier</trd>,
    <trd>vorkstaartpluvier</trd>,
    <trd>zwaluwpluvier</trd>
  </trdgrp>
  <trd lng="pl">&zdot;wirowiec</trd>
  <trd lng="pt">glar&eacute;ola</trd>
  <trd lng="ru">&c_t;&c_i;&c_r;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">prielo&zcaron;n&iacute;k</trd>
</drv>

</art>

