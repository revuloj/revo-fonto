<vortaro>
<art mrk="$Id$">
<kap>
<rad>glareol</rad>/o
</kap>
<drv mrk="glareo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="glareo.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Glareola</trd>).
    </dif>
  </snc>
  <trd lng="cs">ouhorlik</trd>
  <trd lng="de">Brachschwalbe</trd>
  <trd lng="en">pratincole</trd>
  <trd lng="hu">sz&eacute;kics&eacute;r</trd>
  <trd lng="pl">zwirowiec</trd>
  <trd lng="ru">&c_t;&c_i;&c_r;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
</drv>
<drv mrk="glareo.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="hxarad.0oformaj">&hcirc;aradrioformaj
      birdoj</ref> (<trd lng="la">Glareolidae</trd>
    </dif>
  </snc>
  <trd lng="de">Brachschwalben <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>

