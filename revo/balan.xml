<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>balan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="balan.0o">
  <kap><tld/>o</kap>
  <snc mrk="balan.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="glan.0o.ANA">glano</ref>
    <ekz>
      por bone purigi la penison, la prepucio (la ha&ubreve;to &ccirc;irka&ubreve; la
      <tld/>o) devas esti tute repu&scirc;ita
      <fnt>
        <aut>trad. Pa&ubreve;lin'</aut>,
        <vrk>Adoleskantinaj Demandoj</vrk>,
        <lok>1999</lok>
      </fnt>.
    </ekz>
  </snc>
  <rim mrk="balan.0o.RIM_HOM">
    <ref tip="hom" cel="balan1.0o">Balano</ref> anka&ubreve; estas nomo de iu
    krustulo.
  </rim>
  <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a; <klr tip="amb">(&c_p;&c_e;&c_n;&c_ib;&c_s;&c_a;)</klr></trd>
  <trd lng="ca">gland</trd>
  <trdgrp lng="cs">
    <trd>balanus</trd>,
    <trd>&zcaron;alud <klr>(penisu)</klr></trd>
  </trdgrp>
  <trd lng="en">glans</trd>
  <trd lng="es">glande</trd>
  <trd lng="fr">gland<klr> (anatomie)</klr></trd>
  <trd lng="hu">h&iacute;mvessz&odblac;makk</trd>
  <trd lng="it">glande</trd>
  <trd lng="iu">&#5125;&#5333;&#5469;&#5465;&#5509;</trd>
  <trd lng="nl">eikel</trd>
  <trd lng="pl">&zdot;o&lstroke;&aogonek;d&zacute;</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">morsk&yacute; &zcaron;alu&dcaron;</trd>
</drv>
</art>

