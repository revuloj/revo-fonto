    <refgrp tip="vid">
      <ref cel="auxgur.0o">a&ubreve;guro</ref>,
      <ref cel="profet.0ajxo">profeta&jcirc;o</ref>.
    </refgrp>
    <ref tip="malprt" cel="astrol2.0o">astrologio</ref>
