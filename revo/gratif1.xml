<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gratif1.xml,v 1.11 2019/07/06 08:10:17 revo Exp $">
<kap>
<rad>gratifikaci</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="gratif1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="gratif.0o">Gratifiko</ref>:
      <ekz>
        kiel anta&ubreve;pagon de &ccirc;i tiu <tld/>o oni partepagis al la personaro 25% de l'
        apartenanta monsumo
        <fnt>
          <vrk><url ref="http://ifef.free.fr/bultenoj/FE%20V%201928-7.pdf"
              >Fervojoj en Hispanujo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gratificaci&oacute;</trd>,
    <trd>propina</trd>
  </trdgrp>
  <trd lng="en">gratification</trd>
  <trd lng="fr">gratification</trd>
  <trdgrp lng="pl">
    <trd>gratyfikacja</trd>,
    <trd>napiwek</trd>
  </trdgrp>
</drv>
</art>
