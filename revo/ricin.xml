<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ricin</rad>/o
</kap>
<drv mrk="ricin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>
      (<trd lng="la">Ricinus</trd>), el kies grajnoj oni ekstraktas
      laksigan oleon.
    </dif>
  </snc>
  <trd lng="de">Rizinus</trd>
  <trd lng="en">castor-oil plant</trd>
  <trd lng="fr">ricin</trd>
  <trdgrp lng="nl">
    <trd>ricinus</trd>,
    <trd>wonderboom</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_shch;&c_je;&c_v;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
