<art mrk="$Id: ricin.xml,v 1.23 2024/04/14 05:46:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ricin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ricin.0o">
  <kap><tld/>o</kap>
  <snc mrk="ricin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>;
      <ref tip="lst" cel="veget.0ajxo" lst="voko:malpigioparencoj">vegeta&jcirc;o</ref>,
      el kies <ref cel="iksod.0o">iksodo</ref>similaj
      grajnoj oni ekstraktas laksigan oleon (<trd lng="la">Ricinus</trd>):
      <ekz>
        &scirc;i disponis ankora&ubreve; tempon sufi&ccirc;an por okupi&gcirc;i pri la infanoj,
        por vidi sur ilia okul-blanko, ke ili bezonas dozon da
        <tld/>-oleo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_c;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>ricin</trd>,
    <trd>sko&rcaron;ec</trd>
  </trdgrp>
  <trd lng="de">Rizinus</trd>
  <trd lng="en">castor-oil plant</trd>
  <trd lng="fr">ricin</trd>
  <trd lng="hu">ricinus</trd>
  <trdgrp lng="ja">
    <trd>&#x30D2;&#x30DE;</trd>,
    <trd>&#x30C8;&#x30A6;&#x30B4;&#x30DE;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ricinus</trd>,
    <trd>wonderboom</trd>
  </trdgrp>
  <trd lng="pl">rycyna</trd>
  <trd lng="pt">r&iacute;cino</trd>
  <trd lng="ro">ricin&abreve;</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_shch;&c_je;&c_v;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">ric&#xED;n</trd>
  <trd lng="uk">&c_r;&c_i;&c_c;&c_i;&#x43D;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x84D6; <pr>b&#xEC;</pr></trd>,
    <trd>&#x84D6;&#x9EBB; <pr>b&#xEC;m&#xE1;</pr></trd>
  </trdgrp>
</drv>
</art>
