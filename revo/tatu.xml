<art mrk="$Id: tatu.xml,v 1.20 2018/09/02 22:10:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tatu</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tatu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tatu.0i.KOS">
    <dif>
      Enpiki sub ies ha&ubreve;to neforigeblajn desegna&jcirc;ojn per koloraj
      pigmentoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
    </trd>,
    <trd>
    &c_v;&c_y;&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
    </trd>,
    <trd>
    &c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_o;&c_w;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="cs">tetovat</trd>
  <trd lng="de">t&auml;towieren</trd>
  <trd lng="en">tattoo<klr tip="ind"> (verb)</klr></trd>
  <trd lng="es">tatuar</trd>
  <trd lng="fr">tatouer</trd>
  <trd lng="hu">tetov&aacute;l</trd>
  <trd lng="ja">&#20837;&#12428;&#22696;&#12377;&#12427; [<ind>&#12356;&#12428;&#12378;&#12415;&#12377;&#12427;</ind>]</trd>
  <trd lng="nl">tatoe&euml;ren</trd>
  <trd lng="pl">tatuowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>tatuar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_t;&c_u;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_t;&c_a;&c_t;&c_u;&c_i;&c_r;&c_o;&c_v;&c_k;&c_u;
    </trd>
  </trdgrp>
  <trd lng="sk">vytetova&tcaron;</trd>
  <trd lng="sv">tatuera</trd>
</drv>

<drv mrk="tatu.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="tatu.0ajxo.KOS">
    <dif>
      Neforigebla <tld/>ita desegna&jcirc;o:
      <ekz>
        tio estas <tld/>a&jcirc;o de nia tribo
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">tattoo<klr tip="ind"> (noun)</klr></trd>
  <trd lng="fr">tatouage</trd>
  <trdgrp lng="ja">
    <trd>&#20837;&#12428;&#22696; [<ind>&#12356;&#12428;&#12378;&#12415;</ind>]</trd>
    <trd>&#12479;&#12488;&#12453;&#12540; [<ind>&#12383;&#12392;&#12357;&#12540;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tatoeage</trd>
    <trd>tattoo</trd>
  </trdgrp>
  <trd lng="es">tatuaje</trd>
  <trd lng="pl">tatua&zdot;</trd>
  <trd lng="sv">tatuering</trd>
</drv>

</art>
