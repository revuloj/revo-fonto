<kap>
  <ofc>1</ofc>
  <rad>tatu</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tatu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tatu.0i.KOS">
    <dif>
      Enpiki sub ies ha&ubreve;to neforigeblajn desegna&jcirc;ojn per koloraj
      pigmentoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
    </trd>,
    <trd>
    &c_v;&c_y;&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
    </trd>,
    <trd>
    &c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_o;&c_w;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="cs">tetovat</trd>
  <trd lng="de">t&auml;towieren</trd>
  <trd lng="en">tattoo<klr tip="ind"> (verb)</klr></trd>
  <trd lng="es">tatuar</trd>
  <trd lng="fr">tatouer</trd>
  <trd lng="hu">tetov&aacute;l</trd>
  <trd lng="ja">&#x5165;&#x308C;&#x58A8;&#x3059;&#x308B; <pr>&#x3044;&#x308C;&#x305A;&#x307F;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">tatoe&euml;ren</trd>
  <trd lng="pl">tatuowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>tatuar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_t;&c_u;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_t;&c_a;&c_t;&c_u;&c_i;&c_r;&c_o;&c_v;&c_k;&c_u;
    </trd>
  </trdgrp>
  <trd lng="sk">vytetova&tcaron;</trd>
  <trd lng="sv">tatuera</trd>
  <trdgrp lng="zh">
    <trd>&#x523A;&#x82B1; <pr>c&#xEC;hu&#x101;</pr></trd>,
    <trd>&#x7D0B;&#x8EAB; <pr>w&#xE9;nsh&#x113;n</pr></trd>,
    <trd>&#x7EB9;&#x8EAB; <pr>w&#xE9;nsh&#x113;n</pr></trd>,
    <trd>&#x523A;&#x82B1;&#x7D0B; <pr>c&#xEC;hu&#x101;w&#xE9;n</pr></trd>,
    <trd>&#x523A;&#x82B1;&#x7EB9; <pr>c&#xEC;hu&#x101;w&#xE9;n</pr></trd>,
    <trd>&#x523A;&#x9751; <pr>c&#xEC;q&#x12B;ng</pr></trd>,
    <trd>&#x523A;&#x9752; <pr>c&#xEC;q&#x12B;ng</pr></trd>,
    <trd>&#x6587;&#x8EAB; <pr>w&#xE9;nsh&#x113;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tatu.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="tatu.0ajxo.KOS">
    <dif>
      Neforigebla <tld/>ita desegna&jcirc;o:
      <ekz>
        tio estas <tld/>a&jcirc;o de nia tribo
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_t;&c_u;</trd>,
    <trd>&c_t;&c_a;&c_t;&c_u;&c_ib;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_t;&c_u;&c_a;&c_zh;</trd>
  </trdgrp>
  <trd lng="en">tattoo<klr tip="ind"> (noun)</klr></trd>
  <trd lng="fr">tatouage</trd>
  <trdgrp lng="ja">
    <trd>&#x5165;&#x308C;&#x58A8; <pr>&#x3044;&#x308C;&#x305A;&#x307F;</pr></trd>
    <trd>&#x30BF;&#x30C8;&#x30A5;&#x30FC; <pr>&#x305F;&#x3068;&#x3045;&#x30FC;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tatoeage</trd>
    <trd>tattoo</trd>
  </trdgrp>
  <trd lng="es">tatuaje</trd>
  <trd lng="pl">tatua&zdot;</trd>
  <trd lng="sv">tatuering</trd>
  <trdgrp lng="zh">
    <trd>&#x523A;&#x82B1; <pr>c&#xEC;hu&#x101;</pr></trd>,
    <trd>&#x7D0B;&#x8EAB; <pr>w&#xE9;nsh&#x113;n</pr></trd>,
    <trd>&#x7EB9;&#x8EAB; <pr>w&#xE9;nsh&#x113;n</pr></trd>
  </trdgrp>
</drv>

