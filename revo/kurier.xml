<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kurier</rad>/o
</kap>
<drv mrk="kurier.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kurier.0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Homo, kies okupo estas rapide porti koresponda&jcirc;ojn,
      piede, sur&ccirc;evale a&ubreve; alie:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        atendi bonan veteron kaj laman <tld/>on
        <klr>(malrapidi por agi)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kolero de re&gcirc;o estas <tld/>o de morto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="herold.0o">heroldo</ref>,
      <ref cel="port.leter0isto">leterportisto</ref>,
      <ref cel="stafet.0o">stafeto</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_u;&c_r;'&c_je;&c_r;</trd>,
      <trd>&c_v;&c_ja;&c_s;&c_t;&c_u;&c_n;</trd>,
      <trd>&c_g;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_s;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>           
    <trd lng="br">poster<klr tip="ind"> (a gas lizhero&ugrave;, pakado&ugrave;)</klr></trd>
    <trdgrp lng="en">
      <trd>courier</trd>,
      <trd>messenger</trd>,
      <trd>runner</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kurier</trd>,
      <trd>Eilbote</trd>
    </trdgrp>
    <trd lng="es">correo</trd>
    <trdgrp lng="fr">
      <trd>courrier<klr> (coursier)</klr></trd>,
      <trd>coursier</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fut&aacute;r</trd>,
      <trd>h&iacute;rn&ouml;k</trd>,
      <trd>k&uuml;ld&ouml;nc</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;</trd>,
      <trd>&c_g;&c_o;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_v;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_s;&c_y;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="nl">koerier</trd>
  </snc>
  <snc mrk="kurier.0o.sxaka">
    <dif>
      &Scirc;akpeco movebla diagonale tra la tuta &scirc;aktabulo.
    </dif>
    <trd lng="be">&c_s;&c_l;&c_o;&c_n; <klr tip="amb">(&c_u; &c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_a;&c_h;)</klr></trd>
    <trd lng="br">furlukin<klr tip="ind"> (echedo&ugrave;)</klr></trd>
    <trd lng="en">bishop</trd>
    <trd lng="es">alfil</trd>
    <trd lng="fr">fou<klr> (&eacute;checs)</klr></trd>
    <trdgrp lng="hu">
      <trd>fut&aacute;r <klr>(sakk)</klr></trd>,
      <trd>fut&oacute; <klr>(sakk)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_l;&c_o;&c_n; <klr>(&c_sh;&c_a;&c_h;&c_m;.)</klr></trd>
    <trd lng="nl">loper <klr>(schaakspel)</klr></trd>
  </snc>
  <trd lng="de">L&auml;ufer</trd>
</drv>
</art>
