<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fum.xml,v 1.48 2023/08/31 13:11:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fum.0o.brulo">
    <dif>
      Miksa&jcirc;o de brulresta&jcirc;oj &scirc;vebantaj en aero, t.e. 
      <ref tip="super" cel="aerosol.0o">aerosolo</ref>
      de cindreroj, fulgo, gutetoj de vaporo, oleo ktp:
      <ekz>
        <tld/>o sin levadis el la kamentuboj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjoro <nom>Britain</nom> kaj prenis la pipon returne en la bu&scirc;on <klr>[kaj
        elspirante]</klr> elblovis grandan nubon da <tld/>o
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        el ar&gcirc;entaj incensujoj <klr>[&#x2026;]</klr> fluis malpezaj <klr>[&#x2026;]</klr> rubandoj de
        <tld/>o
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o parfumita per mirho kaj olibano
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        komencis levi&gcirc;adi el la urbo kolono da <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili malaperos kiel <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 37:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        cigara <tld/>o tute konfuzis la kapon de l&#x2019; maljunulino
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        per blueta nubo levi&gcirc;is <klr>[&#x2026;]</klr> de la altaro 
        <klr>[&#x2026;]</klr> ofer<tld/>o de danko
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistas <tld/>o sen brulo
        <klr>(ne estas efiko sen ka&ubreve;zo)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="haladz.0o">haladzo</ref>,
      <ref cel="nebul.0o">nebulo</ref>,
      <ref cel="vapor.0o">vaporo</ref>
    </refgrp>
    <trd lng="id">asap</trd>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_m;</trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_m;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_lata;&t_nga;&t_sa;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>d&yacute;m</trd>,
    <trd>kou&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rauch</trd>,
    <trd>Qualm</trd>
  </trdgrp>
  <trd lng="en">smoke</trd>
  <trd lng="es">humo</trd>
  <trd lng="fr">fum&eacute;e</trd>
  <trd lng="hu">f&uuml;st</trd>
  <trd lng="id">asap</trd>
  <trdgrp lng="ja">
    <trd>&#x7159;  [<ind>&#x3051;&#x3080;&#x308A;</ind>]</trd>,
    <trd>&#x6E6F;&#x6C17;  [<ind>&#x3086;&#x3052;</ind>]</trd>,
    <trd>&#x306F;&#x304B;&#x306A;&#x3044;&#x3082;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">rook</trd>
  <trd lng="pl">dym</trd>
  <trdgrp lng="pt">
    <trd>fuma&ccedil;a</trd>,
    <trd>fumo</trd>
  </trdgrp>
  <trd lng="ro">fum</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;</trd>
  <trd lng="sk">fum&eacute;</trd>
  <trd lng="uk">&c_d;&c_i;&c_m;</trd>
</drv>

<drv mrk="fum.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="fum.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Ellasi <tld/>on:
      <ekz>
        &ccirc;iuj kamentuboj de la urbo <tld/>is kaj odoris
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lampo <tld/>is, kaj la muroj <klr>[&#x2026;]</klr> fari&gcirc;is pli nigraj
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> <tld/>is brulfajroj kaj fi&scirc;istoj kuiris terpomojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kial <tld/>as Via kolero kontra&ubreve; la &scirc;afoj de Via pa&scirc;tejo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 74:1</lok>
        </fnt>?
      </ekz>
      <ekz>
        <ind><tld/>igi incenson</ind>
        <fnt><bib>MT</bib>, <lok>Eliro 30:1</lok></fnt>.
        <trd lng="hu">t&ouml;mj&eacute;nt f&uuml;st&ouml;l</trd>
        <trd lng="ru">&c_k;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_m;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="fr">fumer<klr> (produire de la fum&eacute;e, &ecirc;tre en
    col&egrave;re)</klr></trd>
    <trd lng="id">ber<ind>asap</ind></trd>
    <trd lng="pl">pali&cacute;</trd>
    <trd lng="ru">&c_d;&c_y;&c_m;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="fum.0i.tabakon">
    <gra><vspec>tr</vspec></gra>
    <dif>
      En- kaj elspiri tabakan a&ubreve; aroman <tld/>on:
      <ekz>
        pla&ccirc;as al mi la blueta <tld/>o de l' cigaro, kiun vi <tld/>as
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i pipon da tabako
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i pipon de opio
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ridis kaj <tld/>is kiel kamentubo
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patro ku&scirc;is sur la sofo, <tld/>is cigaredojn kaj &gcirc;uis
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        varma cindro elfalis sur la kruron de la <tld/>into
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_c;&c_y;&c_g;&c_a;&c_r;&c_e;&c_t;&c_u;)</klr></trd>
    <trd lng="es">fumar</trd>
    <trd lng="fr">fumer<klr> (du tabac)</klr></trd>
    <trdgrp lng="hu">
      <trd>f&uuml;st&ouml;l<klr tip="amb"> (doh&aacute;nyt)</klr></trd>,
      <trd>doh&aacute;nyzik</trd></trdgrp>
    <trd lng="id">me<ind>rokok</ind></trd>
    <trd lng="pl">pali&cacute;</trd>
    <trd lng="pt">fumar</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_m;&c_ja;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_tsheg;&t_ma;&t_ga;&t_tsheg;&t_-a;&t_tha;&t_drengbu;&t_na;&t_tsheg;</trd>
  <trd lng="cs">kou&rcaron;it</trd>
  <trd lng="de">rauchen</trd>
  <trd lng="en">smoke</trd>
  <trdgrp lng="ja">
    <trd>&#x7159;&#x3092;&#x51FA;&#x3059;  [<ind>&#x3051;&#x3080;&#x308A;&#x3092;&#x3060;&#x3059;</ind>]</trd>,
    <trd>&#x55AB;&#x7159;&#x3059;&#x308B;  [<ind>&#x304D;&#x3064;&#x3048;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5438;&#x3046;  [<ind>&#x3059;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">roken</trd>
  <trdgrp lng="pl">
    <trd>pali&cacute;</trd>,
    <trd>&cacute;mi&cacute;<klr> (o papierosach)</klr></trd>,
    <trd>kurzy&cacute;</trd>,
  </trdgrp>
  <trd lng="ro">arde</trd>
  <trdgrp lng="sk">
    <trd>dymi&tcaron;</trd>,
    <trd>faj&ccaron;i&tcaron;</trd>,
    <trd>&ccaron;adi&tcaron;</trd>,
    <trd>&ccaron;mudi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_m;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_i;&c_m;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_p;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_i;&c_t;&c_i; (&c_t;&c_ju;&c_t;&c_ju;&c_n;)</trd>
  </trdgrp>
</drv>

<drv mrk="fum.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <tld lit="F"/>i<sncref ref="fum.0i.ntr"/>:
      <ekz>
        piramida monto, <klr>[&#x2026;]</klr> kiu havas strangajn kutimojn, post longaj jaroj
        de l' trankvilo subite &gcirc;i komencas <tld/>i&gcirc;i, tremi, brui
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;skultis tion en la drinkejo, kie <tld/>i&gcirc;is la seba kandelo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la supo jam <tld/>i&gcirc;is <klr>(odore vaporis)</klr> en teleroj: 
        tio estis tre alloga
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">f&uuml;st&ouml;ni kezd<klr tip="amb"> (vulk&aacute;n)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_m;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">rauchen <klr>(intr)</klr></trd>
  <trd lng="id">ber<ind>asap</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x7159;&#x306B;&#x306A;&#x308B;  [<ind>&#x3051;&#x3080;&#x308A;&#x306B;&#x306A;&#x308B;</ind>]</trd>,
    <trd>&#x767A;&#x7159;&#x3059;&#x308B;  [<ind>&#x306F;&#x3064;&#x3048;&#x3093;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">pali&cacute; si&eogonek;</trd>
  <trd lng="pt">fumegar</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fum.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        la farmodomo tuta brulis kiel grandega tor&ccirc;o, <klr>[kolono]</klr> tridekmetra,
        rekte supreniris al la &ccirc;ielo, kiun &gcirc;i mallumigis per sia nigra <tld/>ado
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> predikis kontra&ubreve; la moro de <tld/>ado
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tute dekutimi&gcirc;is de <tld/>ado
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">kou&rcaron;en&iacute;</trd>
  <trd lng="de">Rauchen</trd>
  <trd lng="fr">fumage</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;st&ouml;l&eacute;s</trd>;
    <trd>doh&aacute;nyz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x767A;&#x7159;  [<ind>&#x306F;&#x3064;&#x3048;&#x3093;</ind>]</trd>,
    <trd>&#x55AB;&#x7159;  [<ind>&#x304D;&#x3064;&#x3048;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">het <ind>roken</ind></trd>
  <trd lng="pl">palenie</trd>
  <trd lng="pt">ato de fumar</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>;
    <trd>&c_k;&c_u;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dymenie</trd>,
    <trd>faj&ccaron;enie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_m;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fum.0ajxi">
  <kap><tld/>a&jcirc;i,
  <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fum.0ajxi.KOMUNE">
    <dif>
      Trakti per fumo.
    </dif>
    <subsnc mrk="fum.0ajxi.konserve">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Saturi per <tld/>o por konservado:
        <ekz>
          <tld/>a&jcirc;itaj fi&scirc;oj
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XIX</lok>
          </fnt>;
        </ekz>
        <ekz>
          Marta a&ccirc;etis funton da <ind><tld/>a&jcirc;ita &scirc;inko</ind>
          <fnt>
            <bib>Marta</bib>
          </fnt>;
          <trd lng="de">R&auml;ucherschinken</trd>
        </ekz>
        <ekz>
          la fi&scirc;on oni tuj salas, sekigas en la sunbrilo, <tld/>a&jcirc;as super fajro
          <fnt>
            <bib>&Ccirc;uk&ccirc;oj</bib>
            <lok>Unua skizo:</lok>
          </fnt>;
        </ekz>
        <ekz>
          matenman&gcirc;on unue: &scirc;elkuiritajn terpomojn kaj <tld/>a&jcirc;itan haringon
          <fnt>
            <bib>Mortul&Scirc;ip</bib>
            <lok>&Ccirc;apitro 48</lok>
          </fnt>;
        </ekz>
        <ekz>
          fazeola supo kun <tld/>a&jcirc;itaj porkaj ripa&jcirc;etoj
          <fnt>
            <bib>LOdE</bib>
            <lok>2001, &#x2116;2</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_v;&c_e;&c_n;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
      <trd lng="de">r&auml;uchern</trd>
      <trd lng="es">ahumar</trd>
      <trdgrp lng="fr">
        <trd>boucaner</trd>,
        <trd>fumer<klr> (boucaner)</klr></trd>
      </trdgrp>
      <trd lng="hu">f&uuml;st&oacute;l<klr tip="amb"> (h&uacute;st)</klr></trd>
      <trd lng="id">meng<ind>asap</ind>i</trd>
      <trd lng="nl">roken<klr tip="ind"> (v.voedsel)</klr></trd>
      <trd lng="pl">w&eogonek;dzi&cacute;</trd>
      <trd lng="pt">defumar</trd>
      <trd lng="ru">&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_r;&c_y;&c_b;&c_u; &c_i; &c_t;.&c_p;.)</klr></trd>
    </subsnc>
    <subsnc mrk="fum.0ajxi.nigrigi">
      <uzo tip="fak">TEK</uzo>
      <dif>
        Nigrigi per <tld/>o:
        <ekz>
          paperoj, nigri&gcirc;intaj pro la malnova polvo, kvaza&ubreve; <tld/>a&jcirc;itaj, okupis la tutan
          spacon
          <fnt>
            <bib>PatrojFiloj</bib>
            <lok>XX</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a&jcirc;ita vitra&jcirc;o.
        </ekz>
      </dif>
      <trd lng="be">&c_z;&c_a;&c_k;&c_u;&c_r;&c_o;&c_d;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">ru&szlig;en</trd>
      <trd lng="hu">bekormol</trd>
      <trd lng="nl">blakeren</trd>
      <trd lng="pl">opala&cacute; <klr>(ogniem)</klr> </trd>
      <trd lng="pt">esfuma&ccedil;ar</trd>
      <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_mol; &c_k;&c_o;&c_p;&c_o;&c_t;&c_mol;&c_ju;)</klr></trd>
    </subsnc>
    <subsnc mrk="fum.0i.sanigi">
      <dif>
        Sanigi per <tld/>o:
        <ekz>
          <klr>[artemizion]</klr> mia patrino bruligis kaj 
          per &gcirc;i <tld/>umis &ccirc;iujn angulojn kaj mallumajn 
          ni&ccirc;ojn de la domo
          <fnt>
            <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
            <vrk>Montara Vila&gcirc;o</vrk>,
            <lok>1984</lok>
          </fnt>.
        </ekz>
      </dif>
    <trd lng="de">ausr&auml;uchern</trd>
    </subsnc>
  </snc>
  <trd lng="cs">udit</trd>
