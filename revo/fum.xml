<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fum</rad>/o
</kap>
<drv mrk="fum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fum.0o.brulo">
    <dif>
      Speco de vapora nebulo enhavanta materiajn solidajn erojn kaj
      eli&gcirc;anta el brulanta korpo:
      <ekz>
        <tld/>o de ligno, pajlo, tabako;
      </ekz>
      <ekz>
        <tld/>o de pipo, de brulego;
      </ekz>
      <ekz>
        sen<tld/>a karbo, pulvo;
      </ekz>
      <ekz>
        ne ekzistas <tld/>o sen brulo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(ne estas efiko sen ka&ubreve;zo.
        <ref tip="vid" cel="tern">terno</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Rauch</trd>
  </snc>
  <snc mrk="fum.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvalora efemera&jcirc;o:
      <ekz>
        gloro estas nur <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">humo</trd>
  <trd lng="en">smoke</trd>
  <trd lng="nl">rook</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;</trd>
</drv>
<drv mrk="fum.0i">
  <kap><tld/>i</kap>
  <snc mrk="fum.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Ellasi <tld/>on:
      <ekz>
        ligno, kameno<fnt>Z</fnt>, lampo, vulkano <tld/>as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>as <klr>(bolas, eksplodas, fulmas)</klr>
        via kolero kontra&ubreve; ili<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>igi incenson</ind>
        <fnt>(<aut>&tZ;</aut>: <vrk>&X;</vrk>,
        <lok>Eliro 30:1</lok>)</fnt>.
        <trd lng="ru">&c_k;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
    <trd lng="fr">fumer<klr tip="ind"> (produire de la fum&eacute;e, &ecirc;tre en col&egrave;re)</klr></trd>
    <trd lng="ru">&c_d;&c_y;&c_m;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="fum.0i.tabakon">
    <gra><vspec>tr</vspec></gra>
    <dif>
      En- kaj elspiri tabakan a&ubreve; aroman <tld/>on:
      <ekz>
        <tld/>i cigaron, pipon, el pipo;
      </ekz>
      <ekz>
        estas malpermesite <tld/>i &ccirc;i tie.
      </ekz>
    </dif>
    <trd lng="es">fumar</trd>
    <trd lng="fr">fumer<klr tip="ind"> (du tabac)</klr></trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">kou&rcaron;it</trd>
  <trd lng="de">rauchen</trd>
  <trd lng="nl">roken</trd>
</drv>
<drv mrk="fum.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <tld lit="F"/>i <sncref ref="fum.0i.ntr"/>:
      <ekz>
        post longaj jaroj da kvieto la vulkano rekomencis
        <tld/>i&gcirc;i.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="fum.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="nl">het <ind>roken</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>;
    <trd>&c_k;&c_u;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="fum.0ajxi">
  <kap><tld/>a&jcirc;i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fum.0ajxi.konserve">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Saturi per <tld/>o por konservado:
      <ekz>
        <tld/>a&jcirc;ita &scirc;inko, haringo.
      </ekz>
    </dif>
    <trd lng="de">r&auml;uchern</trd>
    <trd lng="es">ahumar</trd>
    <trdgrp lng="fr">
      <trd>boucaner</trd>,
      <trd>fumer<klr tip="ind"> (boucaner)</klr></trd>
    </trdgrp>
    <trd lng="nl">roken<klr tip="ind"> (v.voedsel)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_r;&c_y;&c_b;&c_u; &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <snc mrk="fum.0ajxi.nigrigi">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Nigrigi per <tld/>o:
      <ekz>
        <tld/>a&jcirc;ita vitra&jcirc;o.
      </ekz>
    </dif>
    <trd lng="de">ru&szlig;en</trd>
    <trd lng="nl">blakeren</trd>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_mol; &c_k;&c_o;&c_p;&c_o;&c_t;&c_mol;&c_ju;)</klr></trd>
  </snc>
</drv>
<drv mrk="fum.0tubo">
  <kap><tld/>tubo</kap>
  <snc>
    <dif>
      Tubo, tra kiu eli&gcirc;as la <tld/>o de fajrujo, forno, kameno ktp.
    </dif>
  </snc>
  <trd lng="de">Ofenrohr</trd>
  <trd lng="fr">conduit de fum&eacute;e</trd>
  <trd lng="nl">schoorsteen</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_o;&c_v;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_a;</trd>
</drv>
<drv mrk="fum.ter0o">
  <kap>ter<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="fumari.0o">fumario.</ref>
  </snc>
</drv>
</art>
