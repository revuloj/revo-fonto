<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fum.xml,v 1.39 2021/05/29 14:32:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fum.0o.brulo">
    <dif>
      Speco de vapora nebulo enhavanta materiajn solidajn erojn kaj
      eli&gcirc;anta el brulanta korpo:
      <ekz>
        <tld/>o de ligno, pajlo, tabako;
      </ekz>
      <ekz>
        <tld/>o de pipo, de brulego;
      </ekz>
      <ekz>
        sen<tld/>a karbo, pulvo;
      </ekz>
      <ekz>ne ekzistas <tld/>o sen brulo
        <klr>(ne estas efiko sen ka&ubreve;zo.
        <ref tip="vid" cel="tern">terno</ref>)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="de">Rauch</trd>
    <trd lng="id">asap</trd>
  </snc>
  <snc mrk="fum.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvalora efemera&jcirc;o:
      <ekz>
        gloro estas nur <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Schall und Rauch</trd>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_m;</trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_m;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xFB3;&#xF44;&#xF66;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>d&yacute;m</trd>,
    <trd>kou&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Rauch</trd>
  <trd lng="en">smoke</trd>
  <trd lng="es">humo</trd>
  <trd lng="fr">fum&eacute;e</trd>
  <trd lng="hu">f&uuml;st</trd>
  <trd lng="id">asap</trd>
  <trd lng="nl">rook</trd>
  <trd lng="pl">dym</trd>
  <trdgrp lng="pt">
    <trd>fuma&ccedil;a</trd>,
    <trd>fumo</trd>
  </trdgrp>
  <trd lng="ro">fum</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;</trd>
  <trd lng="sk">fum&eacute;</trd>
</drv>

<drv mrk="fum.0i">
  <kap><ofc>*</ofc><tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5032">fum'i</url></lok></fnt></kap>
  <snc mrk="fum.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Ellasi <tld/>on:
      <ekz>
        ligno, kameno<fnt>Z</fnt>, lampo, vulkano <tld/>as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>as <klr>(bolas, eksplodas, fulmas)</klr>
        via kolero kontra&ubreve; ili<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>igi incenson</ind>
        <fnt><bib>MT</bib>, <lok>Eliro 30:1</lok></fnt>.
        <trd lng="hu">t&ouml;mj&eacute;nt f&uuml;st&ouml;l</trd>
        <trd lng="ru">&c_k;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_m;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="fr">fumer<klr> (produire de la fum&eacute;e, &ecirc;tre en
    col&egrave;re)</klr></trd>
    <trd lng="id">ber<ind>asap</ind></trd>
    <trd lng="pl">pali&cacute;</trd>
    <trd lng="ru">&c_d;&c_y;&c_m;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="fum.0i.tabakon">
    <gra><vspec>tr</vspec></gra>
    <dif>
      En- kaj elspiri tabakan a&ubreve; aroman <tld/>on:
      <ekz>
        <tld/>i cigaron, pipon, el pipo;
      </ekz>
      <ekz>
        estas malpermesite <tld/>i &ccirc;i tie.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_c;&c_y;&c_g;&c_a;&c_r;&c_e;&c_t;&c_u;)</klr></trd>
    <trd lng="es">fumar</trd>
    <trd lng="fr">fumer<klr> (du tabac)</klr></trd>
    <trdgrp lng="hu">
      <trd>f&uuml;st&ouml;l<klr tip="amb"> (doh&aacute;nyt)</klr></trd>,
      <trd>doh&aacute;nyzik</trd></trdgrp>
    <trd lng="id">me<ind>rokok</ind></trd>
    <trd lng="pl">pali&cacute;</trd>
    <trd lng="pt">fumar</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_m;&c_ja;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF50;&#xF0B;&#xF58;&#xF42;&#xF0B;&#xF60;&#xF50;&#xF7A;&#xF53;&#xF0B;</trd>
  <trd lng="cs">kou&rcaron;it</trd>
  <trd lng="de">rauchen</trd>
  <trd lng="en">smoke</trd>
  <trd lng="nl">roken</trd>
  <trdgrp lng="pl">
    <trd>pali&cacute;</trd>,
    <trd>&cacute;mi&cacute;<klr> (o papierosach)</klr></trd>,
    <trd>kurzy&cacute;</trd>,
  </trdgrp>
  <trd lng="ro">arde</trd>
  <trdgrp lng="sk">
    <trd>dymi&tcaron;</trd>,
    <trd>faj&ccaron;i&tcaron;</trd>,
    <trd>&ccaron;adi&tcaron;</trd>,
    <trd>&ccaron;mudi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fum.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <tld lit="F"/>i<sncref ref="fum.0i.ntr"/>:
      <ekz>
        post longaj jaroj da kvieto la vulkano rekomencis
        <tld/>i&gcirc;i.
      </ekz>
    </dif>
    <trd lng="hu">f&uuml;st&ouml;ni kezd<klr tip="amb"> (vulk&aacute;n)</klr></trd>
  </snc>
  <trd lng="id">ber<ind>asap</ind></trd>
  <trd lng="pl">pali&cacute; si&eogonek;</trd>
  <trd lng="pt">fumegar</trd>

  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fum.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">kou&rcaron;en&iacute;</trd>
  <trd lng="de">Rauchen</trd>
  <trd lng="fr">fumage</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;st&ouml;l&eacute;s</trd>;
    <trd>doh&aacute;nyz&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">het <ind>roken</ind></trd>
  <trd lng="pl">palenie</trd>
  <trd lng="pt">ato de fumar</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>;
    <trd>&c_k;&c_u;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dymenie</trd>,
    <trd>faj&ccaron;enie</trd>
  </trdgrp>
</drv>

<drv mrk="fum.0ajxi">
  <kap><tld/>a&jcirc;i<fnt>Z</fnt>,
  <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fum.0ajxi.KOMUNE">
    <dif>
      Trakti per fumo.
    </dif>
    <subsnc mrk="fum.0ajxi.konserve">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Saturi per <tld/>o por konservado:
        <ekz>
          <tld/>a&jcirc;ita &scirc;inko, haringo.
        </ekz>
      </dif>
      <trd lng="be">&c_v;&c_e;&c_n;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
      <trd lng="de">r&auml;uchern</trd>
      <trd lng="es">ahumar</trd>
      <trdgrp lng="fr">
        <trd>boucaner</trd>,
        <trd>fumer<klr> (boucaner)</klr></trd>
      </trdgrp>
      <trd lng="hu">f&uuml;st&oacute;l<klr tip="amb"> (h&uacute;st)</klr></trd>
      <trd lng="id">meng<ind>asap</ind>i</trd>
      <trd lng="nl">roken<klr tip="ind"> (v.voedsel)</klr></trd>
      <trd lng="pl">w&eogonek;dzi&cacute;</trd>
      <trd lng="pt">defumar</trd>
      <trd lng="ru">&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_r;&c_y;&c_b;&c_u; &c_i; &c_t;.&c_p;.)</klr></trd>
    </subsnc>
    <subsnc mrk="fum.0ajxi.nigrigi">
      <uzo tip="fak">TEK</uzo>
      <dif>
        Nigrigi per <tld/>o:
        <ekz>
          <tld/>a&jcirc;ita vitra&jcirc;o.
        </ekz>
      </dif>
      <trd lng="be">&c_z;&c_a;&c_k;&c_u;&c_r;&c_o;&c_d;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">ru&szlig;en</trd>
      <trd lng="hu">bekormol</trd>
      <trd lng="nl">blakeren</trd>
      <trd lng="pl">opala&cacute; <klr>(ogniem)</klr> </trd>
      <trd lng="pt">esfuma&ccedil;ar</trd>
      <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_p;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_mol; &c_k;&c_o;&c_p;&c_o;&c_t;&c_mol;&c_ju;)</klr></trd>
    </subsnc>
    <subsnc mrk="fum.0i.sanigi">
      <dif>
        Sanigi per <tld/>o:
        <ekz>
          <klr>[artemizion]</klr> mia patrino bruligis kaj 
          per &gcirc;i <tld/>umis &ccirc;iujn angulojn kaj mallumajn 
          ni&ccirc;ojn de la domo
          <fnt>
            <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
            <vrk>Montara Vila&gcirc;o</vrk>,
            <lok>1984</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="cs">udit</trd>
  <trd lng="sk">vy&uacute;di&tcaron;</trd>
  <trd lng="pt">fumigar</trd>
</drv>

<drv mrk="fum.0tubo">
  <kap><tld/>tubo</kap>
  <snc>
    <dif>
      Tubo, tra kiu eli&gcirc;as la <tld/>o de fajrujo, forno, kameno ktp.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_m;&c_ib;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kom&iacute;n</trd>,
    <trd>kou&rcaron;ovod</trd>,
    <trd>roura od kamen</trd>
  </trdgrp>
  <trd lng="de">Ofenrohr</trd>
  <trd lng="fr">conduit de <ind>fum&eacute;e</ind></trd>
  <trd lng="hu">k&eacute;m&eacute;ny</trd>
  <trd lng="id">cerobong</trd>
  <trd lng="nl">schoorsteen</trd>
  <trd lng="pl">komin</trd>
  <trd lng="pt">chamin&eacute;</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_o;&c_v;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dymovod kozuba</trd>,
    <trd>kom&iacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="fum.ter0o">
  <kap>ter<tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="fumari.0o">fumario.</ref>
  </snc>
  <trd lng="cs">dymnivka</trd>
  <trd lng="pl">dymnica</trd>
  <trd lng="pt">fum&aacute;ria</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">zemedym</trd>
</drv>
</art>
