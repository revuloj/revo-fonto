<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>bufr</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="bufr.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">FER</uzo>
      <snc mrk="bufr.0o.TEK">
        <uzo tip="fak">TEK</uzo>
        <dif>
          Pu&scirc;risortego por moderigi la kunpu&scirc;i&gcirc;ojn de vagonoj.  
        </dif>
        <trd lng="fr">tampon <klr tip="ind">(amortisseur)</klr></trd>
      </snc>
      <snc mrk="bufr.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="super" cel="memor.0o.KOMP">Memoro</ref> por
          provizora konservado de datumoj, precipe &ccirc;e kunlaboro de
          diversrapidaj traktiloj.          
        </dif>
      </snc>
      <trd lng="en">buffer</trd>
      <trd lng="ru">&c_b;&c_u;&c_f;&c_je;&c_r;</trd>
    </drv>
  </art>
