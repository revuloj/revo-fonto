<art mrk="$Id: solan.xml,v 1.26 2024/03/25 06:40:51 revo Exp $">
<kap>
<rad>solan</rad>/o
</kap>
<drv mrk="solan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="solana.0oj"><tld/>acoj</ref> (<trd lng="la">Solanum</trd>),
      kies &ccirc;efaj specioj estas:
      <refgrp tip="sub">
        <ref cel="dolcxa.0o">dol&ccirc;amaro</ref>,
        <ref cel="melong.0o">melongeno</ref>,
        <ref cel="ter.0pomujo">terpomujo</ref>:
      </refgrp>
      <ekz>
        melongeno estas specio de <klr>[&#x2026;]</klr> <tld/>o, kiu portas grandajn fruktojn
        <klr>[kiuj]</klr> estas violkoloraj a&ubreve; anka&ubreve; blankaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20032">Melongeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1d/Solanum_dulcamara_01.jpg/360px-Solanum_dulcamara_01.jpg" prm="CC     BY-SA 3.0">Dol&ccirc;amara <tld/>o (Solanum dulcamara), Hurum, Norvegujo
        <fnt>
          <aut>Bjoertvedt, propra verko</aut>
          <lok><url ref="&WCW;/File:Solanum_dulcamara_01.jpg">&WCW;/File:Solanum_dulcamara_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_l;&c_jo;&c_n;</trd>,
    <trd>&c_l;&c_ib;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">g&egrave;nere Solanum</trd>
  <trd lng="cs">lilek</trd>
  <trd lng="de">Nachtschatten</trd>
  <trdgrp lng="hu">
    <trd>csucsor</trd>,
    <trd>ebsz&odblac;l&odblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x30CA;&#x30B9;  <pr>&#x306A;&#x3059;</pr></trd>
  <trd lng="nl">nachtschade</trd>
  <trd lng="pl">psianka s&lstroke;odkog&oacute;rz</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_l;&c_jo;&c_n;</trd>
  <trd lng="sk">&lcaron;u&lcaron;ok</trd>
  <trd lng="uk">&#x43F;&c_a;&c_s;&#x43B;&c_ib;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x8304;&#x5C5E; <pr>qi&#xE9;sh&#x1D4;</pr></trd>,
    <trd>&#x8304;&#x5C6C; <pr>qi&#xE9;sh&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
