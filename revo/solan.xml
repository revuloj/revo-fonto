<vortaro>
<art mrk="$Id$">
<kap>
  <rad>solan</rad>/o
</kap>
<drv mrk="solan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>acoj (<trd lng="la">Solanum</trd>),
      kies &ccirc;efaj specioj estas:
      <refgrp tip="sub">
        <ref cel="dolcxa.0o">dol&ccirc;amaro</ref>,
        <ref cel="melong.0o">melongeno</ref>,
        <ref cel="ter.0pomo">terpomo</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="de">Nachtschatten</trd>
  <trd lng="nl">nachtschade</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_l;&c_jo;&c_n;</trd>
</drv>
</art>
