<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>solan</rad>/o
</kap>
<drv mrk="solan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="solanac.0oj"><tld/>acoj</ref> (<trd
      lng="la">Solanum</trd>),
      kies &ccirc;efaj specioj estas:
      <refgrp tip="sub">
        <ref cel="dolcxa.0o">dol&ccirc;amaro</ref>,
        <ref cel="melong.0o">melongeno</ref>,
        <ref cel="ter.0pomujo">terpomujo</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_l;&c_jo;&c_n;</trd>
  <trd lng="ca">g&egrave;nere Solanum</trd>
  <trd lng="de">Nachtschatten</trd>
  <trdgrp lng="hu">
    <trd>csucsor</trd>,
    <trd>ebsz&odblac;l&odblac;</trd>
  </trdgrp>
  <trd lng="nl">nachtschade</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_l;&c_jo;&c_n;</trd>
</drv>
</art>
