<vortaro>
<art mrk="$Id: mancin.xml,v 1.15 2020/05/20 20:10:19 revo Exp $">
<kap>
  <rad>mancinel</rad>/o
</kap>
<drv mrk="mancin.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:botanikaj_specioj">Specio</ref>
       el familio
      <ref tip="malprt" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>
      (<trd lng="la">Hippomane mancinella</trd>), 
      kun folioj kaj fruktoj similaj al 
      pom<ref tip="lst" cel="arb.0o"
      lst="voko:malpigioparencoj">arbo</ref>, 
      sed kies lakteca suko estas tre venena:
      <ekz>
        <tld/>o trovi&gcirc;as proksime <klr>[de]</klr> &#8208; kaj sur &#8208; akvobordaj
        pla&gcirc;oj,
        &gcirc;i certigas bonegan naturan ventoprotekton kaj &gcirc;iaj radikoj
        stabiligas la sablon,
        evitante la erozion de la strando
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=596619">Mancinelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
       <tld/>frukto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Manschinellenbaum</trd>
  <trd lng="en">manchineel</trd>
  <trd lng="es">Manzanilla de la muerte</trd>
  <trd lng="fr">mancenillier</trd>
  <trd lng="hu">manz&aacute;nilla-almafa</trd>
  <trd lng="nl">manzanillaboom</trd>
  <trdgrp lng="pl">
    <trd>manchineel</trd>,
    <trd>jab&lstroke;usznik</trd>,
    <trd>konisza&lstroke;</trd>
    <trd>jab&lstroke;o&nacute; &sacute;mierci</trd>
  </trdgrp>
  <trd lng="pt">mancenilheira</trd>
  <trd lng="ro">copacul mor&#539;ii</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_n;&c_c;&c_i;&c_n;&c_je;&c_l;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_sh;&c_i;&c_n;&c_je;&c_l;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

