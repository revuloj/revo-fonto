<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mancinel</rad>/o
</kap>
<drv mrk="mancin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio
      <ref tip="super" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>
      (<trd lng="la">Hippomane mancinella</trd>), kies lakteca suko
      estas tre venena.
    </dif>
  </snc>
  <trd lng="de">Manschinellenbaum</trd>
  <trd lng="en">manchineel</trd>
</drv>
<drv mrk="mancin.0frukto">
  <kap><tld/>frukto</kap>
  <snc>
    <dif>
      Frukto de la <tld/>o, iomete simila al malgranda pomo.
    </dif>
  </snc>
</drv>
</art>

