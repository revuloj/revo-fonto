<kap>
<rad>stereometri</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="stereo1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Parto de <ref tip="malprt" cel="geomet1.0o.MAT">geometrio</ref>,
      kiu pli speciale okupi&gcirc;as
      pri la ecoj de <ref cel="solid.0o.geometrio">solidoj</ref>:
      <ekz>
        Nova <tld/>o de vinbareloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2105">Keplero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="planim.0o">planimetrio.</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_t;&c_je;&c_r;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">estereometria</trd>
  <trd lng="de">Stereometrie</trd>
  <trd lng="en">stereometry</trd>
  <trd lng="es">estereometr&iacute;a</trd>
  <trd lng="fr">st&eacute;r&eacute;om&eacute;trie</trd>
  <trd lng="nl">stereometrie</trd>
  <trd lng="pl">stereometria</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x441;&#x442;&#x435;&#x440;&#x435;&#x43E;&#x43C;&#x435;&#x442;&#x440;&#x456;&#x44F;</trd>
</drv>
</art>
