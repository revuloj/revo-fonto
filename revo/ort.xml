<drv mrk="ort.0ilo">
  <kap><tld/>ilo,
     <var><kap><tld/>angulilo</kap></var>
  </kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="fak">TEK</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="ort.0ilo.ilo">
    <dif>
      Ilo por desegni <ref cel="ort.0a.figuroj"><tld/>ajn
      <sncref/></ref>
      rektojn:
      <ekz>
        &ccirc;iu havis en la mano dikan bastonon a&ubreve; lignan cirkelon, a&ubreve;
        <tld/>ilon
        <fnt>
          <bib>Far1</bib>,
          <lok>&ccirc;apitro 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        niveli kun helpo de du tabuletoj kaj <tld/>angulilo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>

    </dif>
    <rim>
      <tld lit="O"/>iloj povas havi diversajn formojn la&ubreve; la intencita
      uzo, ekz-e formo de <tld/>a triangulo a&ubreve; de litero T.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_u;&c_g;&c_l;&c_a;&c_v;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cartab&oacute;</trd>,
    <trd>escaire</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>troj&uacute;heln&iacute;k</trd>,
    <trd>&uacute;heln&iacute;k <klr>(kresli&ccaron;sk&yacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geodreieck</trd>,
    <trd>Rei&szlig;dreieck</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>set square</trd>,
    <trd>triangle<klr tip="ind"> (set square)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cartab&oacute;n</trd>,
    <trd>escuadra</trd>
  </trdgrp>
  <trd lng="fr">&eacute;querre</trd>
  <trd lng="hu">der&eacute;ksz&ouml;g&udblac; <ind>vonalz&oacute;</ind></trd>
  <trd lng="pl">ekierka</trd>
  <trd lng="ro">echer</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">uholn&iacute;k</trd>
  <trd lng="zh">&#x4E09;&#x89D2;&#x5C3A; <pr>s&#x101;nji&#x1CE;och&#x1D0;</pr></trd>
</drv>

