<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>gem</rad>/o
</kap>
<drv mrk="gem.0o">
  <kap><tld/>o</kap>
  <snc mrk="gem.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      &Ccirc;iuspeca multvalora kruda &scirc;tono uzebla por fari <ref tip="vid"
      cel="juvel.0o.BELA">juvelon</ref>.
    </dif>
    <trd lng="hu">dr&aacute;gak&odblac;</trd>
    <trd lng="nl">edelsteen</trd>
    <trd lng="pt">gema<klr tip="amb"> (qualquer pedra preciosa)</klr></trd>
  </snc>
  <snc mrk="gem.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="trezor.0o.aprezindajxo">trezoro <sncref/></ref>
    <trd lng="nl">parel<klr tip="ind"> (fig.)</klr></trd>
    <trd lng="pt">j&oacute;ia<klr tip="amb"> (f)</klr></trd>
    <trd lng="hu">gy&ouml;ngy<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Gemme</trd>,
    <trd>Edelstein</trd>
  </trdgrp>
  <trd lng="es">gema</trd>
  <trdgrp lng="fr">
    <trd>gemme</trd>,
    <trd>pierre pr&eacute;cieuse</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_j; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
</drv>

<drv mrk="gem.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu komercas <tld/>ojn.
    </dif>
  </snc>
  <adm>
    &Scirc;ajnas temi pri sinonimo de juvelisto. Sed PIV1 ne konsentas
    kaj rezervas la terminon al prilaboristoj de ~oj. <aut>MB</aut>
  </adm>
  <trd lng="hu">dr&aacute;gak&odblac;-keresked&odblac;</trd>
  <trd lng="nl">juwelier</trd>
  <trd lng="pt">vendedor de gemas</trd>
</drv>
</art>
