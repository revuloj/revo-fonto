<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>2</ofc>
<rad>gem</rad>/o</kap>
<drv mrk="gem.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      &Ccirc;iuspeca multvalora juvel&scirc;tono.
    </dif>
    <trd lng="nl">edelsteen</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="perl.0o">Perlo</ref> 3.
    </dif>
    <trd lng="nl">parel<klr tip="ind"> (fig.)</klr></trd>
  </snc>
  <trdgrp lng="de">
    <trd>Gemme</trd>,
    <trd>Edelstein</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gemme</trd>,
    <trd>pierre pr&eacute;cieuse</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_j; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
</drv>

<drv mrk="gem.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu komercas <tld/>ojn.
    </dif>
  </snc>
  <adm>
    Sxajnas temi pri sinonimo de juvelisto. Sed PIV1 ne konsentas
    kaj rezervas la terminon al prilaboristoj de ~oj. <aut>MB</aut>
  </adm>
  <trd lng="nl">juwelier</trd>
</drv>
</art>
