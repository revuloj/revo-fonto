<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>degn</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="degn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="degn.0i.KOMUNE">
    <dif>
      Bonvole komplezi al subulo:
      <ekz>
        vi tion, ver&scirc;ajne, ne <tld/>as kompreni
        <fnt>
        <aut>Fjodor Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
        <vrk>Noticoj el la kelo</vrk>,
        <bib>LOdE</bib>,
        <lok>2002, numero 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur grandaj sinjoroj scias esti tiom <tld/>aj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 2a, &ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>condescendre</trd>,
    <trd>daigner</trd>
  </trdgrp>
</drv>
</art>

