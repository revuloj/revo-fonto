<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: degn.xml,v 1.12 2024/10/22 10:33:29 revo Exp $">
<kap>
  <rad>degn</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="degn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="degn.0i.KOMUNE">
    <dif>
      Bonvole <ref tip="super" cel="komple4.0i">komplezi</ref> al subulo:
      <ekz>
        nur grandaj sinjoroj scias esti tiom <ind><tld/>a</ind>j
        <fnt>
          <bib>Salamandroj</bib>,
          <lok>libro 2a, &ccirc;apitro 1a</lok>
        </fnt>;
        <trd lng="ca">digne</trd>
      </ekz>
      <ekz>
        vi tion, ver&scirc;ajne, ne <tld/>as kompreni
        <fnt>
        <aut>F. Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
          <vrk>Noticoj el la kelo</vrk>,
          <bib>LOdE</bib>,
          <lok>2002:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>is lin per respondo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        kata mo&scirc;to, <tld/>u veni &ccirc;i tien
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="dign.0a">digna</ref>,
    <ref cel="favor.0i">favori</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_l;&c_a;&c_s;&c_k;&c_u;</ind></trd>,
    <trd>&c_p;&c_a;&c_b;&c_l;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_a; &c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dignar-se</trd>,
    <trd>condescendir</trd>
  </trdgrp>
  <trd lng="cs">r&aacute;&ccaron;it</trd>
  <trdgrp lng="de">
    <trd>so <ind>gn&auml;dig</ind> sein</trd>,
    <trd>geruhen</trd>,
    <trd>sich <ind>herablassen</ind></trd>,
    <trd>sich <ind>herbeilassen</ind></trd>,
    <trd><klr>(einer Gef&auml;lligkeit) </klr>w&uuml;rdigen</trd>
  </trdgrp>
  <trd lng="en">deign</trd>
  <trdgrp lng="fr">
    <trd>condescendre</trd>,
    <trd>daigner</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&vav;&alef;&yod;&lamed; &bet;&tet;&vav;&bet;&vav;</trd>
  <trdgrp lng="nl">
    <trd>het <ind>nodig</ind> vinden</trd>,
    <trd>het <ind>waard</ind> vinden</trd>
  </trdgrp>
  <trd lng="pt">dignar-se</trd>
  <trdgrp lng="sk">
    <trd>preuk&aacute;za&tcaron; l&aacute;skavos&tcaron; ni&zcaron;&scaron;ie postaven&eacute;mu</trd>,
    <trd>upon&iacute;&zcaron;i&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_l;&c_o;&c_v;&c_zh;&c_i;&c_v;. &c_z;&c_v;&c_o;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_l;&c_a;&c_zh;&c_l;&c_i;&c_v;&c_o; &c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_g;&c_l;&c_ja;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

