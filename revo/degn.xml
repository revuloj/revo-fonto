<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: degn.xml,v 1.10 2023/10/05 08:30:57 revo Exp $">
<kap>
  <rad>degn</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="degn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="degn.0i.KOMUNE">
    <dif>
      Bonvole komplezi al subulo:
      <ekz>
        nur grandaj sinjoroj scias esti tiom <ind><tld/>a</ind>j
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 2a, &ccirc;apitro 1a</lok>
        </fnt>;
        <trd lng="ca">digne</trd>
      </ekz>
      <ekz>
        vi tion, ver&scirc;ajne, ne <tld/>as kompreni
        <fnt>
        <aut>F. Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
        <vrk>Noticoj el la kelo</vrk>,
        <bib>LOdE</bib>,
        <lok>2002:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>is lin per respondo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>dignar-se</trd>,
    <trd>condescendir</trd>
  </trdgrp>
  <trd lng="cs">r&#xE1;&#x10D;it</trd>
  <trd lng="en">deign</trd>
  <trdgrp lng="fr">
    <trd>condescendre</trd>,
    <trd>daigner</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D5;&#x5D0;&#x5D9;&#x5DC; &#x5D1;&#x5D8;&#x5D5;&#x5D1;&#x5D5;</trd>
  <trdgrp lng="nl">
    <trd>het <ind>nodig</ind> vinden</trd>,
    <trd>het <ind>waard</ind> vinden</trd>
  </trdgrp>
  <trd lng="pt">dignar-se</trd>
  <trdgrp lng="sk">
    <trd>preuk&#xE1;za&tcaron; l&#xE1;skavos&tcaron; ni&#x17E;&scaron;ie postaven&#xE9;mu</trd>,
    <trd>upon&#xED;&#x17E;i&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&#x43B;&#x43E;&c_v;&c_zh;&c_i;&c_v;. &c_z;&c_v;&#x43E;&#x43B;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&c_b;&#x43B;&c_a;&c_zh;&#x43B;&c_i;&c_v;&#x43E; &#x43F;&#x43E;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_g;&#x43B;&#x44F;&#x43D;&c_u;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

