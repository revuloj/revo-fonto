<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>abel</rad>/o
</kap>

<drv mrk="abel.0o">
  <kap><ofc>*</ofc><tld/>o</kap><fnt>F</fnt>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref>
       el la familio
      <ref tip="malprt" cel="abeled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Apis</trd>), el kiu unu specio
      (<trd lng="la">Apis mellifera</trd>) estas bredata
      pro la <ref cel="miel.0o">mielo</ref>.
    </dif>
  </snc>
  <bld lok="../bld/abel.jpg">
    <tld/>o sur rutfloro
  </bld>
  <trd lng="be">&c_p;&c_ch;&c_a;&c_l;&c_a;</trd>
  <trd lng="bg">&c_p;&c_ch;&c_je;&c_l;&c_a;</trd>
  <trd lng="br">gwenanenn</trd>
  <trd lng="ca">abella</trd>
  <trd lng="de">Biene</trd>
   <trd lng="en">&my;&epsilon_ton;&lambda;&jota;&sigma;&sigma;&alfa;</trd>
  <trd lng="en">bee</trd>
  <trd lng="es">abeja</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_n;&a_b;&a_w;&a_r; &a_ayn;&a_s;&a_l;</trd>,
    <trd>&a_z;&a_n;&a_b;&a_w;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">abeille</trd>
  <trd lng="he">&dalet;&bet;&vav;&resh;&he;</trd>
  <trd lng="hu">m&eacute;h</trd>
  <trd lng="ja">&#12511;&#12484;&#12496;&#12481; [<ind>&#12415;&#12388;&#12400;&#12385;</ind>]</trd>
  <trd lng="nl">bij</trd>
  <trd lng="it">ape</trd>
  <trd lng="oc">abelha</trd>
  <trd lng="pl">pszczo&lstroke;a</trd>
  <trd lng="pt">abelha</trd>
  <trd lng="ru">&c_p;&c_ch;&c_je;&c_l;&c_a;</trd>
  <trd lng="sk">v&ccaron;ela</trd>
  <trd lng="sv">bi</trd>
  <trd lng="uk">&c_b;&c_d;&c_zh;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="tr">
    <trd>bal ar&inodot;s&inodot;</trd>,
    <trd>ar&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="abel.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Dometo konstruita de <tld/>oj, a&ubreve; korbo a&ubreve; kesteto
konstruita de homo por lo&gcirc;igi <tld/>ojn:
      <ekz>
        en <tld/>ujon ne blovu
        <klr>(ne incitu koleremulon)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_sh;&c_je;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Bienenkorb</trd>, <trd>Bienenstock</trd>
  </trdgrp>
  <trd lng="be">&c_v;&c_u;&c_l;&c_je;&c_j;</trd>
  <trdgrp lng="br">
    <trd>ruskenn</trd>, <trd>kestenn</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>rusc</trd>, 
    <trd>buc</trd>,
    <trd>seguer</trd>,
    <trd>bresca</trd>
    <trd>arna</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>beehive</trd>, <trd>hive</trd>
  </trdgrp>
  <trd lng="es">colmena</trd>
  <trd lng="fa">&f_k;&a_n;&a_d;&a_w;</trd>
  <trd lng="fr">ruche</trd>
  <trd lng="he">&kaf;&vav;&vav;&resh;&tav;</trd>
  <trd lng="hu">kapt&aacute;r</trd>
  <trd lng="it">alveare</trd>
  <trd lng="ja">&#12511;&#12484;&#12496;&#12481;&#12398;&#24035;&#31665;
  [<ind>&#12415;&#12388;&#12400;&#12385;&#12398;&#12377;&#12400;&#12371;</ind>]</trd>
  <trd lng="nl">bijenkorf</trd>
  <trdgrp lng="oc">
    <trd>bornat</trd>, 
    <trd>bornhon</trd>,
    <trd>buc</trd>,
    <trd>brusc</trd>
  </trdgrp>
  <trd lng="pl">ul</trd>
  <trdgrp lng="pt">
    <trd>colm&eacute;ia</trd>,
    <trd>corti&ccedil;o</trd>
  </trdgrp>
  <trd lng="tr">kovan</trd>
  <trd lng="ru">&c_u;&c_l;&c_je;&c_j;</trd>
  <trd lng="sk">&uacute;&lcaron;</trd>
  <trd lng="sv">bikupa</trd>
</drv>

<drv mrk="abel.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie <tld/>bredisto aran&gcirc;is <tld/>ujojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_o;&c_sh;&c_je;&c_r;</trd>
  <trd lng="br">gwenaneg</trd>
  <trd lng="ca">abellar</trd>
  <trd lng="de">Bienenstand</trd>
  <trd lng="en">apiary</trd>
  <trdgrp lng="es">
    <trd>colmenar</trd>,
    <trd>abejera</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_n;&a_d;&a_w;&a_kh;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">rucher</trd>
  <trd lng="he">&mem;&kaf;&vav;&vav;&resh;&tav;</trd>
  <trd lng="hu">m&eacute;hes</trd>
  <trd lng="it">arnia</trd>
  <trd lng="ja">&#39178;&#34562;&#22580; [<ind>&#12424;&#12358;&#12411;&#12358;&#12376;&#12423;&#12358;</ind>]</trd>
    <trdgrp lng="nl">
    <trd>bijenschans</trd>,
    <trd>bijenstal</trd>
  </trdgrp>
  <trd lng="pl">pasieka</trd>
  <trdgrp lng="pt">
    <trd>colmeal</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_je;&c_k;&c_a;</trd>
  <trd lng="sk">v&ccaron;el&iacute;n</trd>
</drv>

<drv mrk="abel.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo kiu prizorgas <tld/>ujojn kaj
      forprenas mielon kaj <ref cel="vaks.0o">vakson</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ch;&c_a;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_ch;&c_je;&c_l;&c_a;&c_r;</trd>
  <trd lng="br">gwenaner</trd>
  <trdgrp lng="ca">
    <trd>apicultor</trd>,
    <trd>abellaire</trd>
  </trdgrp>
  <trd lng="de">Imker</trd>
  <trd lng="en">beekeeper</trd>
  <trd lng="es">apicultor</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_n;&a_d;&a_w;&a_d;&a_A;&a_r;</trd>,
    <trd>&a_z;&a_n;&a_b;&a_w;&a_r;&a_d;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">apiculteur</trd>
  <trd lng="he">&kaf;&vav;&vav;&resh;&fnun;</trd>
  <trd lng="hu">m&eacute;h&eacute;sz</trd>
  <trd lng="it">apicoltore</trd>
  <trd lng="ja">&#39178;&#34562;&#23478; [<ind>&#12424;&#12358;&#12411;&#12358;&#12363;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>imker</trd>,
    <trd>bijenhouder</trd>
  </trdgrp>
  <trdgrp lng="oc">
    <trd>apicultor</trd>,
    <trd>abelhaire</trd>
  </trdgrp>
  <trd lng="pl">pszczelarz</trd>
  <trdgrp lng="pt">
     <trd>apicultor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">v&ccaron;el&aacute;r</trd>
</drv>

<drv mrk="abel.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="abel.0ino.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Inseksa individuo el la genro <tld/>oj.
    </dif>
    <refgrp tip="sub">
      <ref cel="abel.0regxino"><tld/>re&gcirc;ino</ref>,
      <ref cel="abel.labor0o">labor<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="ca">abella femella</trd>
  <trd lng="fa">&a_z;&a_n;&a_b;&a_w;&a_r; &a_m;&a_A;&a_d;&a_h;</trd>
  <trd lng="fr">abeille femelle</trd>
  <trd lng="hu">n&odblac;st&eacute;ny <ind>m&eacute;h</ind></trd>
  <trd lng="nl">wijfjes<ind>bij</ind></trd>
  <trdgrp lng="pt">
     <trd>abelha f&ecirc;mea</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_k;&c_a;
<ind>&c_p;&c_ch;&c_je;&c_l;&c_y;</ind>
  </trd>
</drv>

<drv mrk="abel.0regxino">
  <kap><tld/>re&gcirc;ino</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      En <tld/>a familio, la sola <tld/>
      ino kun plene evoluintaj
      seksaj organoj:
      <ekz>
        kaj tien flugis la <tld/>
        re&gcirc;ino kun sia svarmo kaj demetis
        vakson kaj mielon
        <fnt><bib>Fab3</bib>, <lok>Elfo de la rozo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_ch;&c_je;&c_l;&c_a; &c_m;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="br">mamm-wenan</trd>
  <trd lng="ca">abella reina</trd>
  <trd lng="de">Bienenk&ouml;nigin</trd>
  <trd lng="en">queen</trd>
  <trd lng="es">reina</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_l;&f_k;&a_h;<klr tip="ind"> (&a_z;&a_n;&a_b;&a_w;&a_r;)</klr></trd>,
    <trd>&a_z;&a_n;&a_b;&a_w;&a_r; &a_m;&a_l;&f_k;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">reine<klr tip="ind"> (abeille)</klr></trd>
  <trd lng="hu">m&eacute;hkir&aacute;lyn&odblac;</trd>
  <trd lng="it"><ind>ape</ind> regina</trd>
  <trd lng="nl">bijenkoningin</trd>
  <trd lng="oc">abelha reina</trd>
  <trdgrp lng="pl">
    <trd>kr&oacute;lowa</trd>, <trd>pszczo&lstroke;a <ind>matka</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abelha-mestra</trd>,
    <trd>rainha</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_ch;&c_je;&c_l;&c_i;&c_n;&c_a;&c_ja;
<ind>&c_m;&c_a;&c_t;&c_k;&c_a;</ind></trd>
</drv>

<drv mrk="abel.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="abel.vir0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Virseksa individuo el la genro <tld/>oj, kiu ne laboras kaj kies
nura funkcio estas fekundigi la re&gcirc;inon:
      <ekz>
        la <tld/>oj &ccirc;iam laboremas, sed la vir<tld/>o sufi&ccirc;e
bone
        man&gcirc;adas, kaj tute ne laboras
        <fnt>
        <aut>M. K. Gandhi tr. A Giridhar Rao</aut>:
        <vrk>La A&ubreve;tobiografio</vrk>, <lok><url
ref="http://www.geocities.com/agrao99/gandhi/eksperimentoj/unua/singardemo.htm"
        >18. Singardemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_c;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_t;&c_malmol;&c_r;&c_t;&c_je;&c_j;</trd>
  <trd lng="br">tad-gwenan</trd>
  <trd lng="ca">abellot</trd>  
  <trd lng="de">Drohne</trd>
  <trd lng="en">drone</trd>
  <trd lng="es">z&aacute;ngano</trd>
  <trd lng="fa">&a_z;&a_n;&a_b;&a_w;&a_r; &a_n;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>abeille m&acirc;le</trd>,
    <trd>faux-bourdon</trd>
  </trdgrp>
  <trd lng="hu">herem&eacute;h</trd>
  <trd lng="it">fuco</trd>
  <trd lng="nl">mannetjes<ind>bij</ind></trd>
  <trdgrp lng="oc">
    <trd>abelha mascla</trd>,
    <trd>abelhard</trd>,
    <trd>fals <ind>bordon</ind></trd>
  </trdgrp>
  <trd lng="pl">trute&nacute;</trd>
  <trdgrp lng="pt">
    <trd>zang&atilde;o</trd>,
    <trd>z&acirc;ng&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_t;&c_je;&c_n;&c_mol;</trd>
</drv>

<drv mrk="abel.labor0o">
  <kap>labor<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      En <tld/>a familio, <tld/>ino sen la reprodukta funkcio, kiu
sen&ccirc;ese laboras:
      <ekz>
        interesa aspekto de la evoluado estas, ke la distingo
        inter labor<tld/>oj kaj <tld/>re&gcirc;inoj bazi&gcirc;as nur sur
nutra&jcirc;o
        <fnt><aut>John Adcock, tr. Donald Rogers</aut>:
        <vrk><url ref="http://www.esperanto.org.nz/kiekiel.html"
        >De kie? Kien? Kaj kiel?</url></vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">labourerez<klr tip="ind"> (gwenanenn)</klr></trd>
  <trd lng="ca">abella <klr tip="ind">obrera</klr></trd>
  <trd lng="de">Arbeiterin<klr tip="ind"> (Biene)</klr></trd>
  <trd lng="en">worker<klr tip="ind"> (bee)</klr></trd>
  <trd lng="es"><klr tip="ind">(abeja) </klr>obrera</trd>
  <trd lng="fa">&a_z;&a_n;&a_b;&a_w;&a_r; &f_k;&a_A;&a_r;&f_g;&a_r;</trd>
  <trd lng="fr">ouvri&egrave;re<klr tip="ind"> (abeille)</klr></trd>
  <trd lng="hu">dolgoz&oacute;<klr tip="ind"> (m&eacute;h)</klr></trd>
  <trd lng="it"><ind>ape</ind> operaia</trd>
  <trd lng="nl">werkster<klr tip="ind"> (bij)</klr></trd>
  <trd lng="pl">pszczo&lstroke;a <ind>robotnica</ind></trd>
  <trdgrp lng="pt">
    <trd>abelha-oper&aacute;ria</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_b;&c_o;&c_ch;&c_a;&c_ja;
&c_p;&c_ch;&c_je;&c_l;&c_a;</trd>
</drv>
</art>
