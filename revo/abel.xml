<drv mrk="abel.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="abel.vir0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Virseksa individuo el la genro <tld/>oj,
      kiu ne laboras kaj kies
      nura funkcio estas fekundigi la re&gcirc;inon:
      <ekz>
        la <tld/>oj &ccirc;iam laboremas, sed la vir<tld/>o sufi&ccirc;e
        bone man&gcirc;adas, kaj tute ne laboras
        <fnt>
        <aut>M. K. Gandhi, trad. A Giridhar Rao</aut>,
        <vrk><url ref="http://esperanto.enxarxa.com/GANDHI%20Eksperimentoj%20kun%20la%20Vero.pdf">
        Eksperimentoj kun la Vero</url></vrk>,
        <lok>[vidita en 2017]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE1C;&#xE36;&#xE49;&#xE07;&#xE15;&#xE31;&#xE27;&#xE1C;&#xE39;&#xE49;</trd>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_c;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_t;&c_malmol;&c_r;&c_t;&c_je;&c_j;</trd>
  <trd lng="br">tad-gwenan</trd>
  <trd lng="ca">abellot</trd>
  <trd lng="cs">trubec</trd>
  <trd lng="da">drone</trd>
  <trd lng="de">Drohne</trd>
  <trd lng="en">drone</trd>
  <trd lng="es">z&aacute;ngano</trd>
  <trd lng="fa">&a_z;&a_n;&a_b;&a_w;&a_r; &a_n;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>abeille m&acirc;le</trd>,
    <trd>faux-bourdon</trd>
