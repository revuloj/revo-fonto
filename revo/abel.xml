<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>abel</rad>/o
</kap>
<drv mrk="abel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio
      <ref tip="super" cel="abel.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Apis</trd>), el kiu unu specio
      (<trd lng="la">Apis mellifera</trd>) estas bredata
      pro la mielo.
    </dif>
  </snc>
  <bld lok="../bld/abel.jpg">
    <tld/>o sur rutfloro
  </bld>
  <trd lng="de">Biene</trd>
  <trd lng="en">bee</trd>
  <trd lng="es">abeja</trd>
  <trd lng="fr">abeille</trd>
  <trd lng="nl">bij</trd>
  <trd lng="tr">ar&inodot;</trd>
  <trd lng="it">ape</trd>
  <trd lng="ru">&#1087;&#1095;&#1077;&#1083;&#1072;</trd>
</drv>
<drv mrk="abel.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Korbo a&ubreve; kesteto por lo&gcirc;igi <tld/>ojn:
      <ekz>
        en <tld/>ujon ne blovu
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(ne incitu koleremulon)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bienenkorb</trd>, <trd>Bienenstock</trd>
  </trdgrp>
  <trd lng="de">Bienenstand</trd>
  <trd lng="es">panal</trd>
  <trd lng="fr">ruche</trd>
  <trd lng="nl">bijenkorf</trd>
  <trd lng="it">alveare</trd>
  <trd lng="tr">kovan</trd>
  <trd lng="ru">&#1091;&#1083;&#1077;&#1081;</trd>
</drv>
<drv mrk="abel.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie <tld/>bredisto aran&gcirc;is <tld/>ujojn.
    </dif>
  </snc>
  <trd lng="fr">rucher</trd>
  <trd lng="ru">&#1087;&#1072;&#1089;&#1077;&#1082;&#1072;</trd>
</drv>
<drv mrk="abel.0edoj">
  <kap><tld/>edoj</kap>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Apidae</trd><trd lng="de">Apoidea</trd>).
    </dif>
  </snc>
  <trd lng="fr">apid&eacute;s</trd>
</drv>
</art>
