    <trd>rainha</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_ch;&c_je;&c_l;&c_i;&c_n;&c_a;&c_ja;
<ind>&c_m;&c_a;&c_t;&c_k;&c_a;</ind></trd>
  <trdgrp lng="th">
