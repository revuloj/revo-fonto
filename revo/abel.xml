    <trd>buc</trd>,
    <trd>seguer</trd>,
    <trd>bresca</trd>
    <trd>arna</trd>
  </trdgrp>
  <trd lng="cs">&uacute;l</trd>
  <trdgrp lng="da">
    <trd>bistade</trd>,
    <trd>bikube</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bienenkorb</trd>, <trd>Bienenstock</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>beehive</trd>, <trd>hive</trd>
  </trdgrp>
  <trd lng="es">colmena</trd>
  <trd lng="fa">&f_k;&a_n;&a_d;&a_w;</trd>
  <trd lng="fr">ruche</trd>
  <trd lng="he">&kaf;&vav;&vav;&resh;&tav;</trd>
  <trd lng="hu">kapt&aacute;r</trd>
  <trd lng="io">abeluyo</trd>
  <trd lng="it">alveare</trd>
  <trd lng="ja">&#x30DF;&#x30C4;&#x30D0;&#x30C1;&#x306E;&#x5DE3;&#x7BB1;
  <pr>&#x3088;&#x3046;&#x307B;&#x3046;&#x3070;&#x3053;</pr></trd>
  <trd lng="nl">bijenkorf</trd>
  <trdgrp lng="oc">
    <trd>bornat</trd>,
    <trd>bornhon</trd>,
    <trd>buc</trd>,
    <trd>brusc</trd>
  </trdgrp>
  <trd lng="pl">ul</trd>
  <trdgrp lng="pt">
    <trd>colm&eacute;ia</trd>,
    <trd>corti&ccedil;o</trd>
  </trdgrp>
