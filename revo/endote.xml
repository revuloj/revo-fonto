<art mrk="$Id: endote.xml,v 1.18 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>endoterm</rad>/a
</kap>
<drv mrk="endote.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Karakterizanta &hcirc;emian <ref cel="reakci.0o.KEM">reakcion</ref>, &ccirc;e kiu la varmo estas
      konsumata.
      <ekz>
        la koncentrigita energio de la sunlumo povas servi por <klr>[&#x2026;]</klr> <tld/>aj
        reakcioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=270149">Sunforno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kalia sulfato anka&ubreve; prepareblas per <tld/>a reakcio de la kalia bisulfato kaj
        kalia klorido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=591000">Kalia sulfato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="ekzoterm.0a">ekzoterma</ref>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">endot&egrave;rmic</trd>
  <trdgrp lng="de">
     <trd>endotherm</trd>,
     <trd>endothermisch</trd>,
     <trd>unter W&auml;rmezufuhr</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>endothermic</trd>,
    <trd>endothermal</trd>
  </trdgrp>
  <trd lng="fr">endotherme</trd>
  <trd lng="ja">&#x5438;&#x71B1;&#x306E; <pr>&#x304D;&#x3085;&#x3046;&#x306D;&#x3064;&#x306E;</pr></trd>
  <trd lng="pl">endotermiczny</trd>
  <trd lng="pt">endot&eacute;rmico</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_n;&c_d;&c_o;&c_t;&c_je;&c_r;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_l;&c_o;&c_shch;&c_a;&c_ju;&c_shch;&c_i;&c_j;
    &c_t;&c_je;&c_p;&c_l;&c_o;&c_t;&c_u;</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_n;&c_d;&c_o;&c_t;&c_je;&c_r;&c_m;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trd lng="zh">&#x5438;&#x70ED; <pr>x&#x12B;r&#xE8;</pr></trd>
</drv>
</art>
