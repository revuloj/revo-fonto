<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>farise</rad>/o, farizeo<fnt>B</fnt> 
<fnt>Z</fnt>
</kap>
<drv mrk="farise.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Ano de juda sekto, kiu afektis eksteran piegecon. 
    </dif>
    <refgrp tip="sin">
      <ref cel="pietis">pietisto</ref>.
    </refgrp>
    <trd lng="ru">&c_f;&c_a;&c_r;&c_i;&c_s;&c_je;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo afektanta eksteran, nur &scirc;ajnan piecon k morseverecon:
      <ekz>mi ne toleros tian <tld/>ismon k mensogemon<fnt>B</fnt>.</ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="hipokr.0ulo">hipokritulo</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_r;&c_i;&c_s;&c_je;&c_j;</trd>,
      <trd>&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;</trd>,
      <trd>&c_h;&c_a;&c_n;&c_zh;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
