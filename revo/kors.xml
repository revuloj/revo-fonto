<kap>
  <rad>kors</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kors.0o">
  <kap><tld/>o</kap>
  <snc mrk="kors.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Lar&gcirc;a strato a&ubreve; <ref tip="sin" cel="ale.0o">aleo</ref>
      plantita per arboj kaj servanta kiel publika
      promenejo en urbo:
      <ekz>
        homoj amasi&gcirc;is &ccirc;e la du flankoj de la <tld/>o
        <fnt>
          <aut>L. Gabrielli</aut>,
          <vrk>La Granda Skandalo</vrk>,
          <lok>Karnavalo, 1973</lok>
        </fnt>.
      </ekz>      
    </dif>
    <ref tip="vid" cel="bulvar.0o">bulvardo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_je;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korzo</trd>,
    <trd>promen&aacute;da</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Korso</trd>,
     <trd>Flanierstra&szlig;e</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>corso</trd>,
    <trd>cours</trd>,
    <trd>promenade<klr> (avenue)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>korz&oacute;</trd>,
    <trd>s&eacute;t&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6563;&#x6B69;&#x9053;  <pr>&#x3055;&#x3093;&#x307D;&#x307F;&#x3061;</pr></trd>,
    <trd>&#x5927;&#x901A;&#x308A;  <pr>&#x304A;&#x304A;&#x3069;&#x304A;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>corso</trd>,
    <trd>promenade</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>corso</trd>,
    <trd>deptak</trd>,
    <trd>ulica spacerowa</trd>
  </trdgrp>
  <trd lng="pt">alameda <klr tip="amb">(&ecirc;)</klr></trd>
  <trdgrp lng="ro">
    <trd>corso</trd>,
    <trd>promenada</trd>,
    <trd>mersul pe strad&abreve;</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_l;&c_je;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">korzo</trd>
</drv>
</art>

