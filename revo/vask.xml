<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vask</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vask.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="euxsk.0o">e&ubreve;sko</ref>
    <trd lng="be">&c_b;&c_a;&c_s;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>&egrave;uscar</trd>,
      <trd>basc<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="cs">n&aacute;dr&zcaron; font&aacute;ny</trd>
    <trd lng="de">Baske</trd>
    <trd lng="en">Basque</trd>
    <trd lng="es">vasco</trd>
    <trd lng="eu">euskaldun</trd>
    <trd lng="fr">Basque<klr> (subst.)</klr></trd>
    <trd lng="fy">Bask</trd>
    <trd lng="hu">baszk</trd>
    <trdgrp lng="io">
      <trd>Baskiano</trd>,
      <trd>Basko</trd>
    </trdgrp>
    <trd lng="nl">Bask</trd>
    <trdgrp lng="pl">
      <trd>Bask</trd>,
      <trd>Baskijczyk</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>euscaro</trd>,
      <trd>basco</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_s;&c_k;</trd>
    <trd lng="tp">jan Esuka</trd>
    <trd lng="sk">n&aacute;dr&zcaron; vodometu</trd>
  </snc>
</drv>

<drv mrk="vask.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="euxsk.0a">e&ubreve;ska</ref>
    <ekz>
      en la kale&scirc;o sidis <klr>...</klr> <tld/>a
      sinjorino
      <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 8a, p.
      69a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>&egrave;uscar</trd>,
    <trd>basc<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="cs">baskick&yacute;</trd>
  <trd lng="de">baskisch</trd>
  <trd lng="en">Basque</trd>
  <trd lng="es">vasco</trd>
  <trd lng="eu">euskal</trd>
  <trd lng="fr">basque<klr> (adj.)</klr></trd>
  <trd lng="fy">Baskysk</trd>
  <trd lng="hu">baszk</trd>
  <trdgrp lng="io">
    <trd>Baskiana</trd>,
    <trd>Baska</trd>
  </trdgrp>
  <trd lng="nl">Baskisch</trd>
  <trd lng="pl">baskijski</trd>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_k;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">baskick&yacute;</trd>
</drv>
</art>
