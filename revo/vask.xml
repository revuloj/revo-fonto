<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vask.xml,v 1.24 2023/10/21 16:50:35 revo Exp $">
<kap>
  <rad>vask</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vask.0o">
  <kap><tld/>o</kap>
  <snc mrk="vask.0o.eusko">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="euxsk.0o">E&ubreve;sko</ref>:
      <ekz>
        &scirc;tata registaro devus modifi la hispanan konstitucion kaj starigi referendumon,
        por ke la <tld/>oj decidu pri sia propra estonteco
        <fnt>
          <bib>Monato</bib>
          <aut>Madridano</aut>
          <vrk><url ref="&Monato;/2003/008182.html">Vaskoj kaj ETA</url></vrk>
          <lok>2003/10, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_s;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>&egrave;uscar</trd>,
      <trd>basc<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="cs">n&aacute;dr&zcaron; font&aacute;ny</trd>
    <trd lng="de">Baske</trd>
    <trd lng="en">Basque</trd>
    <trd lng="es">vasco</trd>
    <trd lng="eu">euskaldun</trd>
    <trd lng="fr">Basque<klr> (subst.)</klr></trd>
    <trd lng="fy">Bask</trd>
    <trd lng="hu">baszk</trd>
    <trdgrp lng="io">
      <trd>Baskiano</trd>,
      <trd>Basko</trd>
    </trdgrp>
    <trd lng="nl">Bask</trd>
    <trdgrp lng="pl">
      <trd>Bask</trd>,
      <trd>Baskijczyk</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>euscaro</trd>,
      <trd>basco</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_s;&c_k;</trd>
    <trd lng="tok">jan Esuka</trd>
    <trd lng="sk">n&aacute;dr&zcaron; vodometu</trd>
  </snc>
  <ref tip="hom" cel="vask1.0o">vasko (baseneto)</ref>
  <trd lng="ja">&#x30D0;&#x30B9;&#x30AF;&#x4EBA;  <pr>&#x30D0;&#x30B9;&#x30AF;&#x3058;&#x3093;</pr></trd>
  <trd lng="uk">&c_r;&c_a;&#x43A;&#x43E;&c_v;&c_i;&#x43D;&c_a; (&c_f;&#x43E;&#x43D;&c_t;&c_a;&#x43D;&c_a; &c_ib; &c_t;.&#x43F;.)</trd>
</drv>

<drv mrk="vask.0a">
  <kap><tld/>a</kap>
  <snc mrk="vask.0a.euska">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="euxsk.0a">E&ubreve;ska</ref>:
      <ekz>
        en la kale&scirc;o sidis <klr>[&#x2026;]</klr> <tld/>a
        sinjorino
        <fnt><bib>DonKihxoto</bib>, 
        <lok>parto 1a, &ccirc;apitro 8a, p.
        69a</lok></fnt>.
      </ekz>
      <ekz>
        en la hispana urbo Bilbao <klr>[&#x2026;]</klr> mi legis indikojn en la <tld/>a (e&ubreve;ska)
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj' (Garbhan MacAoidh)</aut>
          <vrk><url ref="&Monato;/2013/010673.php">Malgravaj lingvoj gravas</url></vrk>
          <lok>2013/01, p. 26-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>&egrave;uscar</trd>,
    <trd>basc<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="cs">baskick&yacute;</trd>
  <trd lng="de">baskisch</trd>
  <trd lng="en">Basque</trd>
  <trd lng="es">vasco</trd>
  <trd lng="eu">euskal</trd>
  <trd lng="fr">basque<klr> (adj.)</klr></trd>
  <trd lng="fy">Baskysk</trd>
  <trd lng="hu">baszk</trd>
  <trdgrp lng="io">
    <trd>Baskiana</trd>,
    <trd>Baska</trd>
  </trdgrp>
  <trd lng="nl">Baskisch</trd>
  <trd lng="pl">baskijski</trd>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_k;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">baskick&yacute;</trd>
  <trd lng="uk">&c_b;&c_a;&c_s;&#x43A;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>
</art>
