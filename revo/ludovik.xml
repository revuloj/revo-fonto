    <kap>
      <ofc>*</ofc><ofc>7</ofc><rad>Ludovik</rad>/o
    </kap>

    <drv mrk="ludovik.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
          &gcirc;ermandevena:
          <ekz>
            <tld/>o, donu al mi panon
            <fnt><bib>F</bib>, <lok>&FE; 8</lok></fnt>.
          </ekz>
        </dif>
      </snc>       
      <trdgrp lng="be">
        <trd>&c_L;&c_ju;&c_d;&c_v;&c_ib;&c_k;</trd>,
        <trd>&c_L;&c_ju;&c_d;&c_v;&c_ib;&c_g;</trd>,
        <trd>&c_L;&c_ju;&c_d;&c_o;&c_v;&c_ib;&c_k;</trd>
      </trdgrp>
      <trd lng="br">Loeiz</trd>
      <trd lng="de">Ludwig</trd>
      <trdgrp lng="es">
        <trd>Luis</trd>,
        <trd>Ludovico</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>Louis</trd>,
        <trd>Ludovic</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>Luigi</trd>,
        <trd>Ludovico</trd>
      </trdgrp>
      <trd lng="hu">Lajos</trd>
      <trdgrp lng="ja">
    <trd>&#x30EB;&#x30C9;&#x30D3;&#x30AF;</trd>,
    <trd>&#x30EB;&#x30FC;&#x30C8;&#x30D3;&#x30D2;</trd>
  </trdgrp>
  <trd lng="nl">Ludo</trd>
      <trd lng="ru">&c_L;&c_ju;&c_d;&c_o;&c_v;&c_i;&c_k;</trd>
      <trdgrp lng="uk">
    <trd>&#x41B;&#x44E;&c_d;&#x43E;&c_v;&c_ib;&#x43A;</trd>,
    <trd>&#x41B;&#x44E;&c_d;&c_v;&c_ib;&c_gu;</trd>
