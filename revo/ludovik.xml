<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>Ludovik</rad>/o
    </kap>

    <drv mrk="ludovik.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
        <dif>
          Vira anta&ubreve;nomo &gcirc;ermandevena:
          <ekz>
            <tld/>o, donu al mi panon
            <fnt><bib>F</bib>, <lok>&FE; 8</lok></fnt>.
          </ekz>
        </dif>
      </snc>       
      <trd lng="br">Loeiz</trd>
      <trd lng="de">Ludwig</trd>
      <trdgrp lng="es">
        <trd>Luis</trd>,
        <trd>Ludovico</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>Louis</trd>,
        <trd>Ludovic</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>Luigi</trd>,
        <trd>Ludovico</trd>
      </trdgrp>
      <trd lng="hu">Lajos</trd>
      <trd lng="nl">Ludo</trd>
      <trd lng="ru">&c_L;&c_ju;&c_d;&c_o;&c_v;&c_i;&c_k;</trd>
    </drv>
  </art>

