<art mrk="$Id: ciklam.xml,v 1.21 2021/03/06 06:20:00 revo Exp $">
<kap>
<rad>ciklamen</rad>/o
</kap>
<drv mrk="ciklam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="primol1.0oj">primolacoj</ref>
      (<trd lng="la">Cyclamen</trd>), plurjaraj tuberaj herboj
      kun ru&gcirc;aj a&ubreve; blankaj floroj,
      hejmaj precipe en Alpoj, kun multaj kulturataj
      varia&jcirc;oj, precipe de persa <tld/>o
      (<trd lng="la">Cyclamen persicum</trd>):
      <ekz>
        malvarma forcejo: nokta temperaturo malplii&gcirc;as je 7&#x202F;&ndash;&#x202F;10&#x202F;&deg;C <klr>[ta&ubreve;gas
        por]</klr> azaleo, <tld/>o, dianto, pelargonio, tulipo ktp.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128759">Plantdomo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/ciklam.jpg">florpote kulturata <tld/>o</bld>
  <trd lng="bg">&c_c;&c_i;&c_k;&c_l;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">ciclamen</trd>
  <trd lng="de">Alpenveilchen</trd>
  <trd lng="el">&kappa;&ypsilon;&kappa;&lambda;&alfa_ton;&my;&jota;&ny;&omikron;</trd>
  <trd lng="fr">cyclamen</trd>
  <trd lng="he">&#x5E8;&#x5B7;&#x5E7;&#x5B6;&#x5E4;&#x5B6;&#x5EA;</trd>
  <trd lng="hu">cikl&aacute;men</trd>
  <trd lng="it">ciclamino</trd>
  <trdgrp lng="pl">
    <trd>cyklamen</trd>,
    <trd>fio&lstroke;ek alpejski</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cicl&acirc;men</trd>,
    <trd>c&iacute;clame</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_i;&c_k;&c_l;&c_a;&c_m;&c_je;&c_n;</trd>,
    <trd>&c_d;&c_r;&c_ja;&c_k;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_p;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja;
    &c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">cyklamen</trd>
  <trd lng="sk">cykl&aacute;men</trd>
  <trdgrp lng="cs">
    <trd>brambo&rcaron;&iacute;k</trd>,
    <trd>cykl&aacute;men</trd>
  </trdgrp>
</drv>
</art>
