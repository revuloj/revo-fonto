<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ciklamen</rad>/o
</kap>
<drv mrk="ciklam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="primol.0acoj">primolacoj</ref> 
      (<trd lng="la">Cyclamen</trd>), plurjaraj tuberaj herboj
      kun ru&gcirc;aj a&ubreve; blankaj floroj,
      hejmaj precipe en Alpoj, kun multaj kulturataj
      varia&jcirc;oj, precipe de persa <tld/>o
      (<trd lng="la">Cyclamen persicum</trd>).
    </dif>
  </snc>
  <bld lok="../bld/ciklam.jpg">florpote kulturata <tld/>o</bld>
  <trd lng="de">Alpenveilchen</trd>
  <trd lng="fr">cyclamen</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_i;&c_k;&c_l;&c_a;&c_m;&c_je;&c_n;</trd>,
    <trd>&c_d;&c_r;&c_ja;&c_k;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_p;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
