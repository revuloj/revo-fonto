<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>friz</rad>/i
</kap>
<drv mrk="friz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Krispe, bukle kombi harojn, volvante ilin &ccirc;irka&ubreve; varmigita ilo:
      <ekz><tld/>i harojn, barbon, plumojn;</ekz>
      <ekz>arte <tld/>ita kapo, virino.</ekz>
    </dif>
  </snc>
  <trd lng="de">frisieren</trd>
  <trd lng="en">curl</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;&c_y;)</klr></trd>
</drv>
<drv mrk="friz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="F"/>ita buklo. 
    </dif>
  </snc>
  <trd lng="de">Frisur</trd>
</drv>
<drv mrk="friz.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Fera ilo prenilforma, por <tld/>i. 
    </dif>
  </snc>
  <trd lng="de">Frisierstab</trd>
  <trd lng="ru">&c_shch;&c_i;&c_p;&c_c;&c_y; &c_d;&c_l;&c_ja; &c_z;&c_a;&c_v;&c_i;&c_v;&c_k;&c_i;</trd>
</drv>
<drv mrk="friz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Kombisto, barbiro.  
    </dif>
  </snc>
  <trd lng="de">Fris&ouml;r</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_k;&c_m;&c_a;&c_h;&c_je;&c_r;</trd>
</drv>
</art>
