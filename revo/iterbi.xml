<vortaro>
<art mrk="$Id$">
<kap>
  <rad>iterbi</rad>/o
</kap>
<drv mrk="iterbi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Yb</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="70">elemento</ref> kun
      atomnumero 70, atompezo 173,04; malofta ar&gcirc;entgriza 
      <ref tip="super" cel="metal.0o">metalo</ref>, lantanoido.
    </dif>
  </snc>                            
  <trd lng="be">&c_ib;&c_t;&c_e;&c_r;&c_b;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_i;&c_t;&c_je;&c_r;&c_b;&c_i;&c_j;</trd>
  <trd lng="br">iterbiom</trd>
  <trd lng="ca">iterbi</trd>
  <trd lng="de">Ytterbium</trd>
  <trd lng="en">ytterbium</trd>
  <trd lng="es">iterbio</trd>
  <trd lng="fr">ytterbium</trd>
  <trd lng="hu">itterbium</trd>
  <trd lng="id">iterbium</trd>
  <trd lng="it">itterbio</trd>
  <trd lng="la">Ytterbium</trd>
  <trd lng="nl">ytterbium</trd>
  <trd lng="pl">iterb</trd>
  <trd lng="pt">it&eacute;rbio<klr tip="ind"> (qu&iacute;mica)</klr></trd>
  <trd lng="ru">&c_i;&c_t;&c_t;&c_je;&c_r;&c_b;&c_i;&c_j;</trd>
  <trd lng="sv">ytterbium</trd>
</drv>
</art>
