<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skiz.xml,v 1.24 2024/03/25 06:40:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skiz</rad>/o
</kap>
<drv mrk="skiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skiz.0o.KOMUNE">
    <dif>
      <ref tip="vid" cel="net.mal0o">Malneta</ref>, rapide farita bildo
      a&ubreve;
      prezento:
    </dif>
    <subsnc mrk="skiz.0o.BELA_desegno">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Desegno prezentanta nur malmultajn &ccirc;efajn a&ubreve;
        karakterizajn
        liniojn:
        <ekz>
          jen estas <tld/>o, desegnita de fama desegnisto, oni komisiis
          al
          vi kopii &gcirc;in
          <fnt><bib>Marta</bib></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="kontur.0o">konturo</ref>,
        <ref cel="siluet.0o">silueto</ref>,
        <ref cel="skem.0o">skemo</ref>
      </refgrp>
      <trdgrp lng="es">
        <trd>bosquejo</trd>,
        <trd>esquema</trd>
      </trdgrp>
      <trd lng="nl">krabbel</trd>
      <trdgrp lng="pt">
        <trd>esbo&ccedil;o</trd>,
        <trd>rascunho</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="skiz.0o.BELA_artajxo">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Nefinita pentra&jcirc;o a&ubreve; skulpta&jcirc;o prezentanta
        nur krudan provan
        malneton de la projektata verko:
        <ekz>
          &gcirc;i &scirc;ajnis al mi bela <tld/>o, el kiu oni povas
          fari belan
          pentra&jcirc;on
          <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
          2a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="es">boceto</trd>
    </subsnc>
    <subsnc mrk="skiz.0o.BELE">
      <uzo tip="fak">BELE</uzo>
      <dif>
        Literatura verko ne prezentanta plendetale la temon, sed
        resumante
        &gcirc;in per &ccirc;efaj trajtoj:
        <ekz>
          la <tld/>o estis skribita jam en junio de 1934, sed mi perdis
          la
          manuskripton
          <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova
          &scirc;akproblemo, (tria skizo
          el la &ccirc;uk&ccirc;a vivo), anta&ubreve;parolo de la
          a&ubreve;toro</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_n;&c_a;&c_r;&c_y;&c_s;</trd>
      <trd lng="es">esbozo</trd>
      <trd lng="ru">&c_o;&c_ch;&c_je;&c_r;&c_k;</trd>
    </subsnc>
    <subsnc mrk="skiz.0o.plano">
      <dif>
        Resumo de plano, de projekto:
        <ekz>
          jen <tld/>o de miaj projektoj;
        </ekz>
        <ekz>
          tio estis nur teoriaj <tld/>oj, palaj senkorpaj fantomoj en la
          regiono de revoj<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trd lng="es">croquis</trd>
    </subsnc>
    <refgrp tip="sin">
      <ref cel="kanvas.0o.FIG">kanvaso <sncref/></ref>,
      <ref cel="skelet.0o.FIG">skeleto <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_k;&c_ib;&c_d;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_ib;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;&ccaron;rt</trd>,
    <trd>skica</trd>,
    <trd>&ccaron;rta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Skizze</trd>
    <trd>Entwurf</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>croquis</trd>,
    <trd>&eacute;bauche</trd>,
    <trd>esquisse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;zlat</trd>,
    <trd>skicc</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30B1;&#x30C3;&#x30C1;  <pr>&#x3059;&#x3051;&#x3063;&#x3061;</pr></trd>,
    <trd>&#x4E0B;&#x7D75;  <pr>&#x3057;&#x305F;&#x3048;</pr></trd>,
    <trd>&#x7565;&#x56F3;  <pr>&#x308A;&#x3083;&#x304F;&#x305A;</pr></trd>,
    <trd>&#x30AF;&#x30ED;&#x30C3;&#x30AD;&#x30FC;</trd>,
    <trd>&#x6982;&#x7565;  <pr>&#x304C;&#x3044;&#x308A;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x8349;&#x6848;  <pr>&#x305D;&#x3046;&#x3042;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schets</trd>,
    <trd>aanleg</trd>,
    <trd>ontwerp</trd>,
    <trd>concept</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szkic</trd>,
    <trd>zarys</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_r;&c_o;&c_s;&c_o;&c_k;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_i;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;&ccaron;rt</trd>,
    <trd>skica</trd>,
    <trd>&ccaron;rta</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_s;&#x43A;&c_ib;&c_z;</trd>,
    <trd>&c_sh;&#x43A;&c_ib;&c_c;</trd>,
    <trd>&#x43D;&c_a;&c_ch;&c_je;&c_r;&#x43A;</trd>,
    <trd>&#x43D;&c_a;&c_r;&c_i;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8349;&#x7A3F; <pr>c&#x1CE;og&#x1CE;o</pr></trd>,
    <trd>&#x901F;&#x5199; <pr>s&#xF9;xi&#x11B;</pr></trd>,
    <trd>&#x901F;&#x5BEB; <pr>s&#xF9;xi&#x11B;</pr></trd>,
    <trd>&#x8349;&#x56FE; <pr>c&#x1CE;ot&#xFA;</pr></trd>,
    <trd>&#x8349;&#x5716; <pr>c&#x1CE;ot&#xFA;</pr></trd>,
    <trd>&#x8349;&#x6848; <pr>c&#x1CE;o&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skiz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skiz.0i.KOMUNE">
    <dif>
      Fari <tld/>on pri io:
      <ekz>
        li <tld/>is al mi interesan planon;
      </ekz>
      <ekz>
        la scienculoj rapide <tld/>is <klr>...</klr> la historion de la
        du
        princinoj
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    &c_e;&c_s;&c_k;&c_ib;&c_z;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_ib;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nast&iacute;nit</trd>,
    <trd>na&ccaron;rtnout</trd>
  </trdgrp>
  <trd lng="de">skizzieren</trd>
  <trd lng="en">sketch</trd>
  <trdgrp lng="es">
    <trd>dise&ntilde;ar</trd>,
    <trd>esbozar</trd>,
    <trd>bosquejar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;baucher</trd>,
    <trd>esquisser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felv&aacute;zol</trd>,
    <trd>v&aacute;zlatot k&eacute;sz&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30B1;&#x30C3;&#x30C1;&#x3059;&#x308B;  <pr>&#x3059;&#x3051;&#x3063;&#x3061;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4E0B;&#x7D75;&#x3092;&#x63CF;&#x304F;  <pr>&#x3057;&#x305F;&#x3048;&#x3092;&#x3048;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x6982;&#x7565;&#x3092;&#x793A;&#x3059;  <pr>&#x304C;&#x3044;&#x308A;&#x3083;&#x304F;&#x3092;&#x3057;&#x3081;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schetsen</trd>,
    <trd>ontwerpen</trd>,
    <trd>uitstippelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szkicowa&cacute;</trd>,
    <trd>kre&sacute;li&cacute;</trd>,
    <trd>zarysowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_n;&c_a;&c_b;&c_r;&c_o;&c_s;&c_o;&c_k;</ind></trd>,
    <trd>&c_n;&c_a;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>na&ccaron;rta&tcaron;</trd>,
    <trd>&ccaron;rta&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_je;&c_s;&#x43A;&c_ib;&c_z;</trd>,
    <trd>&c_sh;&#x43A;&c_ib;&c_c;</trd>,
    <trd>&#x43D;&c_a;&c_ch;&c_je;&c_r;&#x43A;</trd>,
    <trd>&#x43D;&c_a;&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_ib;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D20;&#x63CF; <pr>s&#xF9;mi&#xE1;o</pr></trd>,
    <trd>&#x52FE;&#x52D2; <pr>g&#x14D;ul&#xE8;</pr></trd>,
    <trd>&#x52FE;&#x5212; <pr>g&#x14D;uhu&#xE0;</pr></trd>,
    <trd>&#x52FE;&#x5283; <pr>g&#x14D;uhu&#xE0;</pr></trd>,
    <trd>&#x901F;&#x5199; <pr>s&#xF9;xi&#x11B;</pr></trd>,
    <trd>&#x901F;&#x5BEB; <pr>s&#xF9;xi&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skiz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="skiz.0isto.KOMUNE">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Homo kiu profesie <tld/>as prepare por posta plena ellaborado:
      <ekz>
        juna <tld/>isto Nikolajev ekkura&gcirc;is: &ndash; Mi proponas
        unu horon
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_w;&c_c;&c_a; <ind>&c_e;&c_s;&c_k;&c_ib;&c_z;&c_a;&c_w;</ind></trd>,
    <trd>&c_ch;&c_a;&c_r;&c_c;&c_jo;&c_zh;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_r;&c_y;&c_s;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="es">dise&ntilde;ador</trd>
  <trd lng="fr">roughman</trd>
  <trdgrp lng="pl">
    <trd>kre&sacute;larz</trd>,
    <trd>szkicownik</trd>
  </trdgrp>
</drv>
</art>
