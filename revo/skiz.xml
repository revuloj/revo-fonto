<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skiz</rad>/o
</kap>
<drv mrk="skiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Rapida desegno prezentanta nur malmultajn &ccirc;efajn a&ubreve; karakterizajn
      liniojn.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Nefinita pentra&jcirc;o a&ubreve; skulpta&jcirc;o prezentanta nur krudan provan
      malneton de la projektata verko.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Literatura verko ne prezentanta plendetale la temon, sed resumante
      &gcirc;in per &ccirc;efaj trajtoj.
    </dif>
    <trd lng="ru">&c_o;&c_ch;&c_je;&c_r;&c_k;</trd>
  </snc>
  <snc>
    <dif>
      Resumo de plano, de projekto:
      <ekz>
        jen <tld/>o de miaj projektoj;
      </ekz>
      <ekz>
        tio estis nur teoriaj <tld/>oj, palaj senkorpaj fantomoj en la
        regiono de revoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="skem.0o">skemo</ref>,
      <ref cel="siluet.0o">silueto</ref>,
      <ref cel="kontur.0o">konturo</ref>,
      <ref cel="skelet.0o">skeleto</ref>,
      <ref cel="kanvas.0o">kanvaso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Skizze</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_r;&c_o;&c_s;&c_o;&c_k;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_i;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="skiz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on pri io:
      <ekz>
        li <tld/>is al mi interesan planon;
      </ekz>
      <ekz>
        <tld/>i la biografion, la vivon de Z.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">skizzieren</trd>
  <trd lng="en">sketch</trd>
  <trd lng="nl">schets</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_n;&c_a;&c_b;&c_r;&c_o;&c_s;&c_o;&c_k;</ind></trd>,
    <trd>&c_n;&c_a;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
