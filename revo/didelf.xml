<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>didelf</rad>/o
</kap>
<drv mrk="didelf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="didelf.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Didelphis</trd>).
    </dif>
  </snc>
  <trd lng="de">Opossum</trd>
</drv>
<drv mrk="didelf.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="marsup.0oj">marsupialoj</ref>
      (<trd lng="la">Didelphidae</trd>),
      de malgrandaj amerikaj arbogrimpaj mamuloj, kies inoj havas
      sur ventro po&scirc;on por porti la idojn.
    </dif>
  </snc>
  <trd lng="de">Beutelratten</trd>
</drv>
</art>
