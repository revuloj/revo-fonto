<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>didelf</rad>/o
</kap>
<drv mrk="didelf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="didelf1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Didelphis</trd>).
    </dif>
  </snc>
  <trd lng="de">Opossum</trd>
  <trd lng="fr">didelphe</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_u;&c_u;&c_t;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_s;&c_s;&c_u;&c_m;</trd>
  </trdgrp>
</drv>

</art>
