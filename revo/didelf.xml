<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>didelf</rad>/o
</kap>
<drv mrk="didelf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="marsup.0o"
      lst="voko:marsupiuloj">Marsupiulo</ref>
      &gcirc;is inter 32&nbsp;cm kaj 50&nbsp;cm longa kun samlonga nuda vosto,
      per kiu &gcirc;i kapablas bone grimpi,
      ofte kun tri nigraj strioj sur la kapo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio 
      <ref tip="malprt" cel="didelf1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Didelphis</trd>).
    </dif>
  </snc>
  <trd lng="cs">va&ccaron;ice</trd>
  <trd lng="de">Opossum</trd>
  <trd lng="fr">opossum, sarigue</trd>
  <trd lng="hu">ersz&eacute;nyes <ind>patk&aacute;ny</ind></trd>
  <trdgrp lng="nl">
    <trd>buidelrat</trd>,
    <trd>opossum</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dydelf</trd>,
    <trd>szczur workowaty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gamb&aacute;</trd>,
    <trd>didelfo</trd>,
    <trd>sering&uuml;&ecirc;</trd>,
    <trd>timbu</trd>,
    <trd>sarag&uuml;&eacute;ia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_u;&c_u;&c_t;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_s;&c_s;&c_u;&c_m;</trd>
  </trdgrp>
</drv>

</art>
