<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>federaci</rad>/o
</kap>
<drv mrk="federa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Ligo, unui&gcirc;o, kungrupi&gcirc;o de memstaraj &scirc;tatoj, societoj
      a&ubreve; organizoj por komuna celo.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>F&ouml;deration</trd>,
    <trd>Staatenbund</trd>
  </trdgrp>
  <trd lng="es">federaci&oacute;n</trd>
  <trd lng="fr">f&eacute;d&eacute;ration</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;g</trd>,
    <trd>&aacute;llamsz&ouml;vets&eacute;g</trd>,
    <trd>f&ouml;der&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">federatie</trd>
  <trd lng="ru">&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="federa.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncerna al la funkciado de grupo de institucioj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">f&eacute;d&eacute;ral</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;gi</trd>,
    <trd>f&ouml;der&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="federa.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
  <trdgrp lng="de">
    <trd>f&ouml;derieren</trd>,
    <trd>zu einem Bund vereinigen</trd>
  </trdgrp>
  <trd lng="fr">f&eacute;d&eacute;rer</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;gben egyes&iacute;t</trd>,
    <trd>f&ouml;deraliz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">federeren</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol; &c_v; <ind>&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</ind></trd>
</drv>
</art>
