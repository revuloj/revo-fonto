<art mrk="$Id$">
<kap>
  <rad>aster</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aster.0o">
  <kap><tld/>o</kap>
  <snc mrk="aster.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj"><tld/>acoj</ref>
      (<trd lng="la">Aster</trd>):
      <ekz>
        balda&ubreve; ekfloros <tld/>oj &ndash; la plej amataj floroj miaj
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 8a</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="de">Aster</trd>
  <trd lng="en">aster</trd>
  <trdgrp lng="fr">
    <trd>aster</trd>,
    <trd>vendangeuse<!-- PLI --></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&odblac;szir&oacute;zsa</trd>,
    <trd>gerebcsin</trd>,
    <trd>aszter</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="sv">aster</trd>
</drv>
</art>
