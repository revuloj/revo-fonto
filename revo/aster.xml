<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>aster</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="aster.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="aster.0acoj"><tld/>acoj</ref>
       (<trd lng="la">Aster</trd>).
     </dif>
   </snc>
   <trd lng="de">Aster</trd>
   <trdgrp lng="fr">
     <trd>aster</trd>,
     <trd>vendangeuse<!-- PLI --></trd></trdgrp>
   <trd lng="ru">&#1072;&#1089;&#1090;&#1088;&#1072;</trd>
</drv>
<drv mrk="aster.0acoj">
   <kap><tld/>acoj</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Familio de dukotiledonaj vegeta&jcirc;oj
       (<trd lng="la">Asteraceae</trd>), kies floroj estas
       dense kunigitaj kvaza&ubreve;e en kapeto.
     </dif>
   </snc>
   <trd lng="de">Korbbl&uuml;tengew&auml;chse</trd>
</drv>
</art>
