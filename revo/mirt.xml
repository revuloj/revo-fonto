<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>mirt</rad>/o
</kap>
<drv mrk="mirt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mirt.0acoj"><tld/>acoj</ref> 
      (<trd lng="la">Myrtus</trd>), arbeto kun &ccirc;iamverdaj ledecaj
      folioj kaj bonodoraj blankaj floretoj, dedi&ccirc;ita al la
      militherooj &ccirc;e la Grekoj kaj al Venuso &ccirc;e la Romanoj.  
   </dif>
  </snc>
  <trd lng="de">Myrte</trd>
</drv>
<drv mrk="mirt.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de arboj kaj arbedoj 
      (<trd lng="la">Myrtaceae</trd>).
    </dif>
    <refgrp tip="sub">
      <ref cel="kariof.0ujo">kariofilujo</ref>,
      <ref cel="euxkal.0o">e&ubreve;kalipto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Myrtengew&auml;chse</trd>
</drv>
</art>
