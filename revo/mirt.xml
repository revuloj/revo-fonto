<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mirt</rad>/o
</kap>
<drv mrk="mirt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mirtac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Myrtus</trd>), arbeto kun &ccirc;iamverdaj ledecaj
      folioj kaj bonodoraj blankaj floretoj, dedi&ccirc;ita al la
      militherooj &ccirc;e la grekoj kaj al Venuso &ccirc;e la romanoj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_t;</trd>
  <trd lng="de">Myrte</trd>
  <trd lng="hu">mirtusz</trd>
  <trdgrp lng="nl">
    <trd>gagel</trd>,
    <trd>mirte</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_t;</trd>,
    <trd>&c_m;&c_i;&c_r;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
