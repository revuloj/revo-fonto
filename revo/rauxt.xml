<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rauxt.xml,v 1.6 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>ra&ubreve;t</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rauxt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="rauxt.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="dif" cel="kurs.0o.voja">Kurso<sncref/></ref>:
      <ekz>
        sur kartego <klr>...</klr> ru&gcirc;a rubando
        montris la <tld/>on
        <fnt>
          <aut>J. Francis</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/noveloj/13.html">
          La Tu&scirc;o de Morto</url></vrk>,
          <lok>en: Vitralo, 1960</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MAT</uzo>
        <tld/>o: vico de e&gcirc;oj kaj verticoj de unu vertico al alia,
        iu ajn donita e&gcirc;o a&ubreve; vertico povus esti uzata pli ol unufoje
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=104312">Glosaro de grafeteorio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="itiner.0o.TRA">itinero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_r;&c_s;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Route</trd>,
    <trd>Weg</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>route</trd>,
    <trd>itin&eacute;raire</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trasa</trd>,
    <trd>kurs<klr>  (np. autobusu)</klr></trd>
  </trdgrp>
  <trd lng="ro">traseu</trd>
  <trdgrp lng="zh">
    <trd>&#x8DEF;&#x7DDA; <pr>l&#xF9;xi&#xE0;n</pr></trd>,
    <trd>&#x8DEF;&#x7EBF; <pr>l&#xF9;xi&#xE0;n</pr></trd>,
    <trd>&#x5F84;&#x8DEF; <pr>j&#xEC;ngl&#xF9;</pr></trd>,
    <trd>&#x5F91;&#x8DEF; <pr>j&#xEC;ngl&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>

