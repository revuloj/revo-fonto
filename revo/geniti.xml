<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>genitiv</rad>/o
</kap>
<drv mrk="geniti.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Deklinacia kazo, kiu montras posedon, kvaliton, la tuton,
      de kiu oni prenas parton ktp, ordinare tradukita en Esperanto
      per la prepozicioj <ekz>de</ekz> kaj <ekz>da.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_d;&c_n;&c_y; &c_s;&c_k;&c_l;&c_o;&c_n;</trd>   
  <trdgrp lng="br">
    <trd>genitiv</trd>,
    <trd>tro-c'henel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Genitiv</trd>,
    <trd>Wesfall</trd>,
    <trd>zweiter Fall</trd>
  </trdgrp>
  <trd lng="en">genitive</trd>
  <trd lng="es">genitivo</trd>
  <trd lng="fr">g&eacute;nitif</trd>
  <trdgrp lng="hu">
    <trd>birtokos eset</trd>,
    <trd>genitivus</trd>
  </trdgrp>
  <trd lng="nl">genitief</trd>
  <trd lng="pt">genitivo<klr tip="amb"> (gram&aacute;tica)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  <trd lng="sv">genitiv</trd>
</drv>
</art>
