<kap>
  <ofc>*</ofc>
  <rad>genitiv</rad>/o
</kap>
<drv mrk="geniti.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      Deklinacia <ref cel="kaz.0o.GRA">kazo</ref>, kiu montras posedon,
      kvaliton, la tuton,
      de kiu oni prenas parton ktp, ordinare tradukita en Esperanto
      per la prepozicioj <ekz>de</ekz> kaj <ekz>da.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_d;&c_n;&c_y; &c_s;&c_k;&c_l;&c_o;&c_n;</trd>
  <trdgrp lng="br">
    <trd>genitiv</trd>,
    <trd>tro-c'henel</trd>
  </trdgrp>
  <trd lng="ca">genitiu</trd>
  <trdgrp lng="cs">
    <trd>2.p&aacute;d</trd>,
    <trd>genitiv</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Genitiv</trd>,
    <trd>Wesfall</trd>,
    <trd>zweiter Fall</trd>
  </trdgrp>
  <trd lng="en">genitive</trd>
  <trd lng="es">genitivo</trd>
  <trd lng="fr">g&eacute;nitif</trd>
  <trdgrp lng="hu">
    <trd>birtokos eset</trd>,
    <trd>genitivus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C5E;&#x683C;  <pr>&#x305E;&#x3063;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x6240;&#x6709;&#x683C;  <pr>&#x3057;&#x3087;&#x3086;&#x3046;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">genitief</trd>
  <trd lng="pl">dope&lstroke;niacz</trd>
  <trd lng="pt">genitivo<klr tip="amb"> (gram&aacute;tica)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>2. p&aacute;d</trd>,
    <trd>genit&iacute;v</trd>
  </trdgrp>
  <trd lng="sv">genitiv</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x434;&#x43E;&#x432;&#x438;&#x439; &#x432;&#x456;&#x434;&#x43C;&#x456;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&#x491;&#x435;&#x43D;&#x456;&#x442;&#x438;&#x432;</trd>
  </trdgrp>
  <trd lng="zh">&#x7B2C;&#x4E8C;&#x683C; <pr>d&#xEC;&#xE8;rg&#xE9;</pr></trd>
</drv>
