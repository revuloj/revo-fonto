<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ma&scirc;</rad>/o
</kap>
<drv mrk="masx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bukleto formita per interplekti&gcirc;o de la fadenoj en trikita teksa&jcirc;o
      a&ubreve; en reto
    </dif>
    <refgrp tip="sin">
      <ref cel="nod.0o">nodo</ref>,
      <ref cel="bant.0o">banto</ref>.
    </refgrp>
    <trd lng="de">Masche</trd>
    <trd lng="hu">szem <klr tip="ind">(hurok)</klr></trd>
  </snc>
  <snc>
    <dif>
      Malplena spaceto inter la plektitaj fadenoj de teksa&jcirc;o a&ubreve; reto.
    </dif>
  </snc>
  <snc>
    <dif>
      Buklo, la&ubreve;vole stre&ccirc;ebla a&ubreve; malstre&ccirc;ebla,
      formita per &scirc;nuro kaj
      povanta servi por &ccirc;irka&ubreve;kapti, &ccirc;irka&ubreve;lavi,
      premsufoki kc.
    </dif>
    <trd lng="de">Schlinge</trd>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_t;&c_l;&c_ja;</trd>
  <trd lng="en">mesh</trd>
  <trd lng="fr">maille</trd>
  <trd lng="hu">hurok</trd>
  <trd lng="nl">steek<klr tip="ind"> (v.weefsel)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_t;&c_l;&c_ja;</trd>
</drv>

<drv mrk="masx.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Reto:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>aro de tiu organizo implikas la tutan mondon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Netz</trd>
  <trd lng="fr">maillage</trd>
  <trd lng="hu">h&aacute;l&oacute;</trd>
  <trd lng="nl">net</trd>
  <trd lng="ru">&c_s;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="masx.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc>
    <dif>
      Malstre&ccirc;i a&ubreve; malfari la <tld/>ojn de io:
      <ekz>
        dis<tld/>igi &scirc;trumpojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>felfejt <klr tip="ind">(sz&ouml;vetet)</klr></trd>,
    <trd>felbont <klr tip="ind">(k&ouml;t&eacute;st)</klr></trd>,
    <trd>sz&eacute;tbont <klr tip="ind">(hurkot)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_p;&c_je;&c_t;&c_l;&c_i; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="masx.0kiraso">
  <kap><tld/>kiraso</kap>
  <snc>
    <dif>
      Mezepoka arma&jcirc;o farita el feraj interplektitaj ringetoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_ch;&c_u;&c_g;&c_a;</trd>
  <trd lng="en">chainmail</trd>
  <trd lng="fr">c&ocirc;te de maille</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;nc&eacute;ling</trd>,
    <trd>ingv&eacute;rt</trd>
  </trdgrp>
  <trd lng="nl">mali&euml;nkolder</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_u;&c_g;&c_a;</trd>
</drv>
</art>
