<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nebul.xml,v 1.31 2020/05/03 04:10:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nebul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="nebul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="nebul.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Amaso da densa akva vaporo formi&gcirc;anta en malsupra atmosfero kaj
      malklariganta la vidon:
      <ekz>
        griza <tld/>o kovras la valon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &gcirc;i ankora&ubreve; ne eliris el malproksima <tld/>o<fnt>Z</fnt>
        <klr>(e&ccirc; ne estas ankora&ubreve; aperanta, estas tute necerta.
        <refgrp tip="vid">
          <ref cel="paser.0o">pasero</ref>,
          <ref cel="hok.0o">hoko</ref>
        </refgrp>)</klr>
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>niebla</trd>,
      <trd>neblina</trd>
    </trdgrp>
  </snc>
  <snc mrk="nebul.0o.nebulecajxo">
    <dif>
      Io simila al <tld/>o:
      <ekz>
        polva <tld/>o;
      </ekz>
      <ekz>
        okuloj vualataj de larma <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
  <trd lng="es">niebla</trd>
  </snc>
  <snc mrk="nebul.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malklara&jcirc;o, malpreciza&jcirc;o:
      <ekz>
        en la <tld/>o de jam longe pasintaj jaroj;
      </ekz>
      <ekz>
        la instruo dispelis la kvincentjaran <tld/>on
        <klr>(senklerecon)</klr>;
      </ekz>
      <ekz>
        ia <tld/>a&jcirc;o ankora&ubreve; &ccirc;irka&ubreve;&scirc;vebas la demandon.
      </ekz>
    </dif>
  <trd lng="es">niebla</trd>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_m;&c_a;&c_n;</trd>
  <trd lng="bg">&c_m;&c_malmol;&c_g;&c_l;&c_a;</trd>
  <trdgrp lng="br">  
    <trd>latar</trd>,
    <trd>lusenn</trd>,
    <trd>nivlenn<klr tip="ind"> (latar)</klr></trd>,
    <trd>mogidell</trd>,
    <trd>mougenn</trd>,
    <trd>morenn</trd>
  </trdgrp>
  <trd lng="cs">mlha</trd>
  <trd lng="de">Nebel</trd>
  <trd lng="en">fog</trd>
  <trdgrp lng="fr">
    <trd>brouillard</trd>,
    <trd>brume</trd>
  </trdgrp>
  <trd lng="he">&ayin;&resh;&pe;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;d</trd>,
    <trd>p&aacute;ra</trd>
  </trdgrp>
  <trd lng="id">kabut</trd>
  <trdgrp lng="nl">
    <trd>nevel</trd>,
    <trd>mist</trd>
  </trdgrp>
  <trd lng="pl">mg&lstroke;a</trd>
  <trdgrp lng="pt">
    <trd>n&eacute;voa</trd>,
    <trd>neblina</trd>,
    <trd>nevoeira</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_m;&c_a;&c_n;</trd>
  <trd lng="sk">hmla</trd>
  <trd lng="sv">dimma</trd>
</drv>

<drv mrk="nebul.0a">
  <kap><tld/>a</kap>
  <snc mrk="nebul.0a.MET">
    <dif>
      <tld lit="N"/>oplena, <tld/>aspekta:
      <ekz>
        <tld/>a &ccirc;ielo.
      </ekz>
    </dif>                       
    <trdgrp lng="es">
      <trd>nublado</trd>,
      <trd>neblinoso</trd>,
      <trd>nebuloso</trd>
    </trdgrp>
    <trd lng="br">latarek</trd>
    <trd lng="de">neblig</trd>
    <trd lng="he">&ayin;&resh;&pe;&yod;&lamed;&yod;</trd>
    <trd lng="pl">mglisty</trd>
    <trd lng="sv">dimmig</trd>
  </snc>
  <snc mrk="nebul.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malklara, malpreciza:
      <ekz>
        <tld/>aj pensoj, konturoj;
      </ekz>
      <ekz>
        revadi pri ia tre malproksima <tld/>a tempo<fnt>Z</fnt>.
      </ekz>
    </dif>                       
    <trd lng="br">koumoulek<klr tip="ind"> (luziet)</klr></trd>
    <trd lng="de">nebul&ouml;s</trd>
    <trd lng="es">nebuloso</trd>
    <trd lng="he">&mem;&ayin;&vav;&resh;&pe;&lamed;</trd>
    <trdgrp lng="nl">
      <trd>nevelig</trd>,
      <trd>mistig</trd>
    </trdgrp>
    <trdgrp lng="pl">
    <trd>mglisty</trd>,
    <trd>zamglony</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>dimmig</trd>,
      <trd>oklar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_m;&c_a;&c_n;&c_n;&c_y;</trd>
  <trd lng="bg">&c_m;&c_malmol;&c_g;&c_l;&c_i;&c_v;</trd>
  <trdgrp lng="cs">
    <trd>mlhav&yacute;</trd>,
    <trd>nejasn&yacute;</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;buleux</trd>
  <trd lng="hu">k&ouml;d&ouml;s</trd>
  <trd lng="id">ber<ind>kabut</ind></trd>
  <trd lng="pt">nebuloso</trd>
  <trd lng="ru">&c_t;&c_u;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hmlist&yacute;</trd>,
    <trd>nejasn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="nebul.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="nebul.0igi.MET">
    <dif>
      Igi <tld/>aspekta:
      <ekz>
        larmoj <tld/>igis &scirc;iajn okulojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">embrumer</trd>
    <trdgrp lng="pl">
    <trd>zasnuwa&cacute; mg&lstroke;&aogonek;</trd>,
    <trd>zamgli&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="nebul.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Igi malklara,
      <ref tip="vid" cel="konfuz.0i">konfuzi</ref>:
      <ekz>
        la fumo <tld/>igis la vidon;
      </ekz>
      <ekz>
        la kolero <tld/>igis vian sa&gcirc;on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi la okulojn al iu<fnt>Z</fnt>
        <klr>(trompi per brila &scirc;ajno)</klr>;
      </ekz>
      <ekz>
        jam <tld/>i&gcirc;as la memoro pri tiuj okaza&jcirc;oj;
      </ekz>
      <ekz>
        ilian intelekton <tld/>igas multaj supersti&ccirc;oj.
      </ekz>
    </dif>
    <trd lng="pl">zamgli&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_t;&c_u;&c_m;&c_a;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_n;&c_ja;&c_ja;&c_s;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">zaml&zcaron;ovat</trd>
  <trdgrp lng="fr">
    <trd>brouiller</trd>,
    <trd>troubler</trd>
  </trdgrp>
  <trd lng="sk">zahmlieva&tcaron;</trd>
</drv>

<drv mrk="nebul.fum0o">
  <kap>fum<tld/>o</kap>
  <snc mrk="nebul.fum0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Densa, sanmalfavora <tld/>o parte a&ubreve; tute
      &scirc;uldata al polucio:
      <ekz>
        arbara fajro de Sumatra ka&ubreve;zis gravan fum<tld/>on 
        transliman en la sudorientazia regiono
        <fnt>
          <aut>Xiong</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/581/2013/06/30/177s151325.htm">
          Indonezia fumnebulo...</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2013-06-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">smog</trd>
  <trd lng="fr">smog</trd>
</drv>
</art>
