<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ungven.xml,v 1.27 2024/01/06 10:28:54 revo Exp $">
<kap>
   <rad>ungvent</rad>/o
   <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ungven.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kuraca oleeca <ref tip="super" cel="sxmir.0ajxo">&scirc;mira&jcirc;o</ref>,
      etendita sur tuko kaj kiun oni tradicie metis sur abscesojn:
      <ekz>
        la ha&ubreve;ton de la frunto korodis ulcero, &scirc;mirita per <tld/>o
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        du-tri junulinoj havis sufi&ccirc;e a&ccirc;ajn cikatrojn, do &scirc;i alportis etan vazon da
        <tld/>o per kiu &scirc;miri ilin
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
         necesa&jcirc;oj, kiel &ccirc;arpioj kaj <tld/>oj, por
         kuracado<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 3a, p.
         40a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_mol;
      <klr>(&c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr>
   </trd>
  <trd lng="bg">&c_m;&c_je;&c_h;&c_l;&c_je;&c_m;</trd>
  <trd lng="ca">ung&uuml;ent</trd>
  <trdgrp lng="cs">
    <trd>mast</trd>,
    <trd>unguentum</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Zugsalbe</trd>,
     <trd>Heilsalbe</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>ointment</trd>,
     <trd>unguent</trd>
   </trdgrp>
  <trd lng="es">ung&uuml;ento</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_r;&a_h;&a_m;</trd>,
     <trd>&f_p;&a_m;&a_A;&a_d;</trd>
   </trdgrp>
  <trdgrp lng="fi">
     <trd>pasta<klr> (l&auml;&auml;ke)</klr></trd>,
     <trd>salva</trd>
  </trdgrp>
  <trd lng="fr">onguent</trd>
  <trd lng="he">&mem;&shin;&het;&he;</trd>
  <trdgrp lng="hu">
     <trd>gy&oacute;gy&iacute;r</trd>,
     <trd>gy&oacute;gyken&odblac;cs</trd>
   </trdgrp>
  <trd lng="it">unguento</trd>
  <trdgrp lng="ja">
    <trd>&#36575;&#33167;  <pr>&#12394;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#33167;&#34220;  <pr>&#12371;&#12358;&#12420;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">zalf</trd>
  <trd lng="pl">ma&sacute;&cacute;</trd>
  <trd lng="pt">ung&uuml;ento</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_h;&c_t;&c_i;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_m;&c_a;&c_z;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_mol;  <klr>(&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="sk">lie&ccaron;iv&aacute; mas&tcaron;</trd>
  <trd lng="uk">&c_m;&c_a;&c_z;&c_mol;</trd>
</drv>
</art>
