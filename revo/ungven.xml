<kap>
   <rad>ungvent</rad>/o
</kap>

<drv mrk="ungven.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="medika1.0o">Medikamenta</ref>
&scirc;mira&jcirc;o,
       etendita sur tuko kaj kiun oni metas sur vundon:
       <ekz>
         necesa&jcirc;oj, kiel &ccirc;arpioj kaj <tld/>oj, por
         kuracado<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 3a, p.
         40a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_mol;
      <klr>(&c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr>
   </trd>
  <trd lng="bg">&c_m;&c_je;&c_h;&c_l;&c_je;&c_m;</trd>
  <trd lng="ca">ung&uuml;ent</trd>
  <trdgrp lng="cs">
    <trd>mast</trd>,
    <trd>unguentum</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Salbe</trd>,
     <trd>Heilsalbe</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>ointment</trd>,
     <trd>unguent</trd>
   </trdgrp>
  <trd lng="es">ung&uuml;ento</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_r;&a_h;&a_m;</trd>,
     <trd>&f_p;&a_m;&a_A;&a_d;</trd>
   </trdgrp>
  <trdgrp lng="fi">
     <trd>pasta<klr> (l&auml;&auml;ke)</klr></trd>,
     <trd>salva</trd>
   </trdgrp>
  <trd lng="fr">onguent</trd>
  <trd lng="he">&mem;&shin;&het;&he;</trd>
  <trdgrp lng="hu">
     <trd>gy&oacute;gy&iacute;r</trd>,
     <trd>gy&oacute;gyken&odblac;cs</trd>
   </trdgrp>
  <trd lng="it">unguento</trd>
  <trdgrp lng="ja">
    <trd>&#x8EDF;&#x818F;  [<ind>&#x306A;&#x3093;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x818F;&#x85AC;  [<ind>&#x3053;&#x3046;&#x3084;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">zalf</trd>
  <trd lng="pl">ma&sacute;&cacute;</trd>
  <trd lng="pt">ung&uuml;ento</trd>
  <trd lng="ru">&c_m;&c_a;&c_z;&c_mol;
<klr>(&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">lie&#x10D;iv&#xE1; mas&#x165;</trd>
  <trd lng="uk">&#x43C;&#x430;&#x437;&#x44C;</trd>
</drv>
</art>
