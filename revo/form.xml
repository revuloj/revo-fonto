<drv mrk="form.vivo0o">
  <kap>vivo<tld/>o</kap>
  <snc mrk="form.vivo0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Grupo da vegeta&jcirc;oj a&ubreve; bestoj, parencaj a&ubreve; ne,
      prezentanta similajn eksterajn trajtojn, esti&gcirc;intajn per
      konver&gcirc;o ka&ubreve;zita de adapti&gcirc;o al unu sama
      vivmedio:
      <ekz>
        la diversspecaj herboj estas vegetala vivo<tld/>o;
      </ekz>
      <ekz>
        la vespertoj kaj birdoj apartenas al unu animala
        vivo<tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Lebensform</trd>
    <trd lng="en">life form</trd>
    <trd lng="id"><ind>bentuk</ind> kehidupan</trd>
    <trdgrp lng="ru">
