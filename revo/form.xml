  <trd lng="is">l&#xF6;gun</trd>
  <trd lng="ja">&#x5F62;&#x72B6;</trd>
  <trd lng="jw">wangun</trd>
  <trd lng="ka">&#x10E4;&#x10DD;&#x10E0;&#x10DB;&#x10D0;</trd>
  <trd lng="km">&#x179A;&#x17BC;&#x1794;&#x179A;&#x17B6;&#x1784;</trd>
  <trd lng="ko">&#xBAA8;&#xC591;</trd>
  <trd lng="lo">&#xEAE;&#xEB9;&#xE9A;&#xEAE;&#xEC8;&#xEB2;&#xE87;</trd>
