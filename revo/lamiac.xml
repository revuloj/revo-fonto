<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lamiac.xml,v 1.17 2023/10/19 05:15:34 revo Exp $">
<kap>
  <rad>lamiac</rad>/oj <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="lamiac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="lamiac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de 
      <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj, kies korolo estas <ref tip="prt" cel="labi.0o">labio</ref>
      (<trd lng="la">Lamiaceae</trd>):
      <ekz>
        la <tld/>oj komunlingve nomataj menta familio entenas &ccirc;irka&ubreve; 3
500 speciojn en
        &ccirc;irka&ubreve; 200 genroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18066">Lamiacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_a;&c_t;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_u;&c_s;&c_t;&c_n;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>lami&agrave;cies</trd>,
    <trd>labiades</trd>
  </trdgrp>
  <trd lng="cs">hluchavkovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Lippenbl&uuml;tengew&auml;chse</trd>
  <trd lng="fr">lamiac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>ajakosak</trd>,
    <trd>&aacute;rvacsal&aacute;nf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="ja">&#x30B7;&#x30BD;&#x79D1;  <pr>&#x30B7;&#x30BD;&#x304B;</pr></trd>
  <trd lng="nl">lipbloemigen</trd>
  <trd lng="pl">jasnotowate</trd>
  <trdgrp lng="pt">
    <trd>labiadas <klr tip="amb">(bot.)</klr></trd>,
    <trd>lami&aacute;ceas <klr tip="amb">(bot.)</klr></trd>
  </trdgrp>
  <trd lng="ro">Lamiaceae</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_s;&c_n;&c_o;&c_t;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hluchavkovit&eacute;</trd>,
    <trd>pyskat&eacute;</trd>
  </trdgrp>
  <trd lng="sv">kransblommiga v&auml;xter</trd>
  <trd lng="uk">&#x433;&#x443;&#x431;&#x43E;&#x446;&#x432;&#x456;&#x442;&#x456;</trd>
</drv>
</art>
