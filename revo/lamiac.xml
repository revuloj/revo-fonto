<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lamiac</rad>/oj
</kap>
<drv mrk="lamiac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Lamiaceae</trd>),
      kies korolo estas <ref tip="prt" cel="labi.0o">labio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_a;&c_t;&c_k;&c_ib;</trd>
  <trd lng="de">Lippenbl&uuml;tengew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>ajakosak</trd>,
    <trd>&aacute;rvacsal&aacute;nf&eacute;l&eacute;k</trd></trdgrp>
  <trd lng="nl">lipbloemigen</trd>
  <trdgrp lng="pt">
    <trd>labiadas <klr tip="amb">(bot.)</klr></trd>,
    <trd>lami&aacute;ceas <klr tip="amb">(bot.)</klr></trd>
  </trdgrp>

  <trdgrp lng="ru">
    <trd>&c_ja;&c_s;&c_n;&c_o;&c_t;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">kransblommiga v&auml;xter</trd>
</drv>
</art>
