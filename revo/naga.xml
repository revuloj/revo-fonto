<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: naga.xml,v 1.6 2024/02/04 09:20:01 revo Exp $">
<kap>
  <rad>naga</rad>/o
</kap>
<drv mrk="naga.0o">
  <kap><tld/>o</kap>
  <snc mrk="naga.0o.ETN">
    <dif>
      Ano de <ref tip="lst" cel="popol.0o.etno" lst="voko:popolanoj">popolo</ref> plurgenta,
      vivanta en nordokcidenta Birmo kaj nordorienta
      Hindujo, precipe en <tld lit="N"/>lando:
      <ekz>
        anka&ubreve; inter la &jcirc;uangoj trovi&gcirc;as tiu
        bambua danco, kiun mi anta&ubreve;e vidis inter la
        aborigenoj de la <tld/>a nacieco
        <fnt>
          <aut>[P. Dasgupta]</aut>,
          <vrk><url ref="&CRI;362/2008/12/05/1s89595.htm">
          3-an de decembro</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2008-12-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_g;&c_a; <klr tip="amb">(&c_n;&c_a;&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="de">Naga</trd>
  <trd lng="fr">naga</trd>
  <trd lng="pl">Naga <klr>(lud)</klr></trd>
  <trd lng="ro">naga</trd>
</drv>

<drv mrk="naga.0lando">
  <kap><tld lit="N"/>lando <fnt><bib>NePiv</bib></fnt>
  <fnt><bib>LanLin</bib></fnt></kap>
  <snc mrk="naga.0lando.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la orienta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        tiu regiono en 1962 fari&gcirc;is federacia &scirc;tato <tld lit="N"/>lando
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Nagaoj">
          nagaoj</url>, 2013-03-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_a;&c_g;&c_a;&c_l;&c_e;&c_n;&c_d;</trd>
  <trd lng="de">Nagaland</trd>
  <trd lng="fr">Nagaland</trd>
  <trd lng="pl">Nagaland</trd>
  <trd lng="ro">Nagaland</trd>
  <trdgrp lng="zh">
    <trd>&#37027;&#21152;&#20848;&#37030; <pr>n&agrave;ji&amacron;l&aacute;nb&amacron;ng</pr></trd>,
    <trd>&#37027;&#21152;&#34349;&#37030; <pr>n&agrave;ji&amacron;l&aacute;nb&amacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>

