<art mrk="$Id: svisi.xml,v 1.20 2024/03/25 07:28:38 revo Exp $">
<kap>
  <rad>Svisi</rad>/o
</kap>
<drv mrk="svisi.0o">
  <kap><tld/>o</kap>
  <snc mrk="svisi.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="svis.S0ujo">Svisujo</ref>:
      <ekz>
        dum la 2a kaj 3a jarmiloj anta&ubreve; nia erao, la galloj, a&ubreve; keltoj
        (estas same), okupis vastegan teritorion, kiu iris de Britio kaj
        Hispanio &gcirc;is Turkio, pasante tra Francio, <tld/>o, la sudo de
        Germanio,
        Hungario kaj parto de Pollando, de Rumanio kaj de Bulgario.
        <fnt><aut>Andr&eacute; CHERPILLOD</aut>:
          <vrk><url ref="&Monato;/1998/006035.html">La galla, &ccirc;u
          malaperinta?</url></vrk>,
          <bib>Monato</bib>,
          <lok>1998/06, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas en <tld/>o kvar lingvaj komunumoj, kaj tri oficialaj lingvoj: la germana,
        la franca kaj la itala
        <fnt>
          
          <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2001/007201.php">Invadas la
          angla</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Sh;&c_v;&c_a;&c_j;&c_c;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_Sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">Su&iuml;ssa</trd>
  <trd lng="cs">&Scaron;v&yacute;carsko</trd>
  <trd lng="da">Svejts</trd>
  <trd lng="de">Schweiz</trd>
  <trd lng="el">&Epsilon;&lambda;&beta;&epsilon;&tau;&jota_ton;&alfa;</trd>
  <trd lng="en">Switzerland</trd>
  <trd lng="es">Suiza<klr tip="amb"> (naci&oacute;n)</klr></trd>
  <trdgrp lng="fr">
    <trd>Suisse</trd>,
    <trd>Conf&eacute;d&eacute;ration suisse</trd>
  </trdgrp>
  <trd lng="fy">Switserl&acirc;n</trd>
  <trd lng="gd">an <ind>Eilv&eacute;is</ind></trd>
  <trd lng="hu">Sv&aacute;jc</trd>
  <trd lng="id">Swiss</trd>  
  <trd lng="io">Suisia</trd>  
  <trd lng="is">Sviss</trd>
  <trd lng="it">Svizzera</trd>
  <trd lng="lat">Helvetia</trd>
  <trd lng="lt">&Scaron;veicarija</trd>
  <trd lng="lv">&Scaron;veice</trd>
  <trd lng="nl">Zwitserland</trd>
  <trd lng="no">Sveits</trd>
  <trd lng="pl">Szwajcaria</trd>
  <trd lng="pt">Su&iacute;&ccedil;a</trd>
