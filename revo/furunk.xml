<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: furunk.xml,v 1.16 2017/07/02 18:10:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>furunk</rad>/o
</kap>
<drv mrk="furunk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="furunk.0o.MED">
    <dif>
      Malgranda dolora <ref tip="super" 
      cel="absces.0o.MED">absceso</ref> de la ha&ubreve;to:
      <ekz>
        li frotetis sian dekstran sidvangon kaj konstatis simple: &leftquot;mi 
        kredas, ke mi akiris <tld/>on sur maloportuna loko&rightquot;
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_u;&c_n;&c_k;&c_u;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>furunkl</trd>,
    <trd>furunkul</trd>,
    <trd>ne&zcaron;it</trd>
  </trdgrp>
  <trd lng="de">Furunkel</trd>
  <trdgrp lng="en">
    <trd>furuncle</trd>,
    <trd>boil</trd>
  </trdgrp>
  <trd lng="fr">furoncle</trd>
  <trdgrp lng="hu">
    <trd>kel&eacute;s</trd>,
    <trd>furunkulus</trd>
  </trdgrp>
  <trd lng="nl">steenpuist</trd>
  <trdgrp lng="pl">
    <trd>furunku&lstroke;</trd>,
    <trd>czyrak</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fur&uacute;nculo</trd>,
    <trd>leicen&ccedil;o</trd>,
    <trd>nascida</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>furuncul</trd>,
    <trd>absces</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_u;&c_n;&c_k;&c_u;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hluch&yacute; vred</trd>,
    <trd>pupenec</trd>,
    <trd>vriedok</trd>
  </trdgrp>
</drv>
</art>
