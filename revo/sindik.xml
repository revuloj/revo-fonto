<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sindik.xml,v 1.17 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>sindik</rad>/o
</kap>
<drv mrk="sindik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Persono rajtigita zorgi pri la aferoj de
      <ref cel="korpor.0o">korporacio</ref>, kies membro
      &gcirc;i estas:
      <ekz>
        <tld/>o de notarioj;
      </ekz>
      <ekz>
        la balkono de l&#x2019; <tld/>o estis objekto de
        multaj revoj de l&#x2019; viroj kaj virinoj
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        letero de <nom>Rossi</nom> al <tld/>o <nom>Rigaud</nom>
        <fnt>
          <bib>FSp</bib>
        </fnt>;
      </ekz>
      <ekz>
        estinte sindiko dum iom da tempo, li komencis grasi&gcirc;i
        <fnt>
          <bib>VRA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prokur.0isto">prokuristo</ref>,
      <ref cel="urb.0estro">urbestro</ref>
    </refgrp>
    <trd lng="ca">s&iacute;ndic</trd>
    <trd lng="de">Syndikus</trd>
    <trdgrp lng="fr">
      <trd>administrateur</trd>,
      <trd>syndic</trd>
    </trdgrp>
    <trd lng="hu">szindikus</trd>
    <trd lng="nl">gemachtigde</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_n;&c_d;&c_i;&c_k;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_r;&c_sh;&c_i;&c_n;&c_a; &c_g;&c_i;&c_l;&c_mol;&c_d;&c_i;&c_i;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Persono, elektita de komerca tribunalo, por administri
      <ref cel="bankro.0i.EKON">bankrotintan</ref> entreprenon
      &gcirc;is fina aran&gcirc;o de la afero.
    </dif>
    <trd lng="ca">comissari de la fallida</trd>
    <trd lng="de">Konkursverwalter</trd>
    <trd lng="hu">cs&odblac;dt&ouml;meggondnok</trd>
    <trd lng="nl">curator</trd>
    <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;
    <ind>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>spr&aacute;vce</trd>,
    <trd>spr&aacute;vce konkursn&iacute; podstaty podniku</trd>,
    <trd>syndik</trd>,
    <trd>zplnomocn&ecaron;nec</trd>,
    <trd>z&aacute;stupce</trd>
  </trdgrp>
  <trd lng="en">syndic</trd>
  <trd lng="fr">administrateur judiciaire</trd>
  <trdgrp lng="ja">
    <trd>&#x7BA1;&#x7406;&#x4EBA;  <pr>&#x304B;&#x3093;&#x308A;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x5171;&#x76CA;&#x59D4;&#x54E1;  <pr>&#x3068;&#x3082;&#x3048;&#x304D;&#x3044;&#x3044;&#x3093;</pr></trd>,
    <trd>&#x7BA1;&#x8CA1;&#x4EBA;  <pr>&#x304B;&#x3093;&#x3056;&#x3044;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x6E05;&#x7B97;&#x4EBA;  <pr>&#x305B;&#x3044;&#x3055;&#x3093;&#x306B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">syndyk</trd>
  <trdgrp lng="sk">
    <trd>splnomocnenec</trd>,
    <trd>spr&aacute;vca konkurznej podstaty</trd>,
    <trd>z&aacute;stupca</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#x43D;&c_d;&c_i;&#x43A;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_sh;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_ch;&#x43B;&c_je;&#x43D; &#x43C;&c_a;&c_gu;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_t;&c_u;</trd>
  </trdgrp>
</drv>
</art>
