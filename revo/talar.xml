    <trd>&c_m;&c_a;&c_n;&c_t;&c_y;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Talar</trd>
  <trd lng="nl">toga</trd>
