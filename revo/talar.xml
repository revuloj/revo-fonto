<kap>
  <rad>talar</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="talar.0o">
  <kap><tld/>o</kap>
  <snc mrk="talar.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Longa lar&gcirc;a <ref tip="super" cel="rob.0o.surtuto">robo</ref>
      en iuj landoj ofice portata de ju&gcirc;istoj, profesoroj,
      advokatoj...:
      <ekz>
        elito, kiu nuntempe vestas sin en la majesta <tld/>o de la supera kortumo por
        pozi teatre anta&ubreve; la televidaj kameraoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2012/010894.php">Me&ccirc;o kaj bombo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="taler.0o">talaro (monero)</ref>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_t;&c_y;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klerika</trd>,
    <trd>tal&aacute;r</trd>
  </trdgrp>
  <trd lng="de">Talar</trd>
  <trdgrp lng="en">
    <trd>gown</trd>,
    <trd>robe</trd>
  </trdgrp>
  <trd lng="es">toga</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_d;&a_A;</trd>,
    <trd>&a_j;&a_b;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">toge<klr tip="amb"> (d'avocat etc.)</klr></trd>
  <trd lng="hu">tal&aacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#x6CD5;&#x670D;  <pr>&#x307B;&#x3046;&#x3075;&#x304F;</pr></trd>,
    <trd>&#x5F0F;&#x670D;  <pr>&#x3057;&#x304D;&#x3075;&#x304F;</pr></trd>,
    <trd>&#x50E7;&#x670D;  <pr>&#x305D;&#x3046;&#x3075;&#x304F;</pr></trd>,
    <trd>&#x30AC;&#x30A6;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">toga</trd>
  <trd lng="pl">toga<klr> (np. s&eogonek;dziowska)</klr></trd>
  <trdgrp lng="pt">
   <trd>toga</trd>,
   <trd>beca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_n;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pl&aacute;&scaron;&tcaron;</trd>,
    <trd>tal&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x43D;&#x442;&#x456;&#x44F;</trd>,
    <trd>&#x442;&#x43E;&#x491;&#x430;</trd>
  </trdgrp>
</drv>
