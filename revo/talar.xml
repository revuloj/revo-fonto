<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>talar</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="talar.0o">
  <kap><tld/>o</kap>
  <snc mrk="talar.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Longa lar&gcirc;a <ref tip="super" cel="rob.0o.surtuto">robo</ref>
      en iuj landoj ofice portata de ju&gcirc;istoj, profesoroj,
      advokatoj...
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_t;&c_y;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Talar</trd>
  <trdgrp lng="en">
    <trd>gown</trd>,
    <trd>robe</trd>
  </trdgrp>
  <trd lng="fr">toge<klr tip="amb"> (d'avocat etc.)</klr></trd>
  <trd lng="hu">tal&aacute;r</trd>
  <trd lng="nl">toga</trd>
  <trd lng="pl">toga<klr> (np. s&eogonek;dziowska)</klr></trd>
  <trdgrp lng="pt">
   <trd>toga</trd>,
   <trd>beca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_n;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">tal&aacute;r</trd>
</drv>
</art>
