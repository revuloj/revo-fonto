<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kazern.xml,v 1.24 2023/09/01 13:58:58 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kazern</rad>/o
</kap>
<drv mrk="kazern.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konstr.0ajxo">Konstrua&jcirc;o</ref>
      destinita por lo&gcirc;igi <ref tip="prt" cel="soldat.0o.militisto">soldatojn</ref>:
      <ekz>
        neniu ta&ccirc;mento havis la rajton lo&gcirc;i en kamparoj, &ccirc;iu devis esti 
        en la <tld/>oj a&ubreve; en la tendaro
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        en printempo 1992 la armeo metis siajn tankojn kaj
        kanonegojn sur la montojn &ccirc;irka&ubreve; Sarajevo kaj poste dum monatoj
        pafadis al la urbo &dash; ne kontra&ubreve; <tld/>oj de malamikaj soldatoj (&ccirc;ar 
        en la komenco tiaj absolute ne ekzistis), sed rekte kontra&ubreve; civilulaj domoj
        <fnt>
	<aut>Zlatko Ti&scaron;ljar</aut>
	<vrk><url ref="&Monato;/2000/006811.php">Nekredeblaj malveroj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 12-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la polico a&ccirc;etis veran 5000-litran &scirc;prucma&scirc;inon, kiu staris en <tld/>o iom for de la viena
        urbocentro
        <fnt>
	<aut>Walter Klag</aut>
	<vrk><url ref="&Monato;/2000/006891.php">Batita kremo kaj akvo&jcirc;etiloj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/04, p. 34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
  <trd lng="br">kazarn</trd>
  <trdgrp lng="cs">
    <trd>kas&aacute;rna</trd>,
    <trd>kas&aacute;rny</trd>
  </trdgrp>
  <trd lng="de">Kaserne</trd>
  <trdgrp lng="en">
    <trd>barracks</trd>,
    <trd>soldiers' quarters</trd>
  </trdgrp>
  <trd lng="fr">caserne</trd>
  <trdgrp lng="hu">
    <trd>laktanya</trd>,
    <trd>kasz&uacute;rnya</trd>
  </trdgrp>
  <trd lng="it">caserma</trd>
  <trdgrp lng="ja">
    <trd>&#x5175;&#x55B6;  [<ind>&#x3078;&#x3044;&#x3048;&#x3044;</ind>]</trd>,
    <trd>&#x5175;&#x820E;  [<ind>&#x3078;&#x3044;&#x3057;&#x3083;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">kazerne</trd>
  <trd lng="pl">koszary</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
  <trd lng="sk">kas&aacute;re&ncaron;</trd>
  <trd lng="uk">&#x43A;&c_a;&c_z;&c_a;&c_r;&#x43C;&c_a;</trd>
</drv>
</art>
