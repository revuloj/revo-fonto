<art mrk="$Id: vitral.xml,v 1.23 2023/09/05 09:43:56 revo Exp $">
<kap>
  <rad>vitral</rad>/o
</kap>
<drv mrk="vitral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Kuna&jcirc;o el diverskoloraj pecoj de <ref tip="prt" cel="vitr.0o">vitro</ref>, 
      kunigitaj per plumbostrioj por formi desegnojn:
      <ekz>
        videblas belega <tld/>o, aparte en la kapelo de fondinto de la
        lernejo por produktantoj de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html">Impresoj
          pri Portugalio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        portalo en karakteriza stilo: katedralturo kun <tld/>o por la
        kristanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011082.php">Elegante
          ellevante</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mozaik.0o">mozaiko</ref>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trd lng="bg">&c_s;&c_t;&c_malmol;&c_k;&c_l;&c_o;&c_p;&c_i;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>mozaikov&eacute; okno</trd>,
    <trd>vitrail</trd>
  </trdgrp>
  <trd lng="de">Bleiglas(fenster)</trd>
  <trd lng="el">&beta;&jota;&tau;&rho;&omikron_ton;</trd>
  <trdgrp lng="es">
     <trd>vidiera de colores</trd>
     <trd>vitral</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>leadlight</trd>,
    <trd>leaded light</trd>,
    <trd>stained glass window</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vitrail</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nes <ind>&uuml;vegablak</ind></trd>
  <trd lng="ja">&#x30B9;&#x30C6;&#x30F3;&#x30C9;&#x30B0;&#x30E9;&#x30B9;</trd>
  <trd lng="nl">loodglas</trd>
  <trd lng="pl">witra&zdot;</trd>
  <trd lng="pt">vitral</trd>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>mozaikov&eacute; farebn&eacute; okno</trd>,
    <trd>vitr&aacute;&zcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x456;&#x442;&#x440;&#x430;&#x436;</trd>
</drv>
</art>
