<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vitral</rad>/o
</kap>
<drv mrk="vitral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Kuna&jcirc;o el diverskoloraj pecoj de vitro, kunigitaj per
      plumbostrioj
      por formi desegnojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trd lng="bg">&c_s;&c_t;&c_malmol;&c_k;&c_l;&c_o;&c_p;&c_i;&c_s;</trd>
  <trd lng="de">Bleiglas(fenster)</trd>
  <trd lng="el">&beta;&jota;&tau;&rho;&omikron_ton;</trd>
  <trdgrp lng="fr">
    <trd>vitrail</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nes <ind>&uuml;vegablak</ind></trd>
  <trd lng="nl">loodglas</trd>
  <trd lng="pt">vitral</trd>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>
</drv>
</art>
