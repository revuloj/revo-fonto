<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Sambr</rad>/o
</kap>
<drv mrk="sambr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="river.0o">Rivero</ref>, fluanta el
      <ref tip="malprt" cel="franc.F0ujo">Francujo</ref> al
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref>,
      kie &gcirc;i fluas en la riveron <ref tip="vid" cel="moz.0o">Mozo</ref>
      apud la urbo
      <ref tip="vid" cel="namur.0o.urb">Namuro</ref>:
      <ekz>
        kunfluejo, aludo al la loko kie, en Namur, Mozo kaj <tld/>o
        kunfluas.
        <fnt><aut>Claude PIRON</aut>:
        <vrk>Renkonte al Esperanto</vrk>,
        <bib>OndEsp</bib>,
        <lok>n-ro 04/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sambre</trd>
  <trd lng="fr">Sambre</trd>
  <trd lng="nl">Samber</trd>
  <trd lng="ru">&c_S;&c_a;&c_m;&c_b;&c_r;</trd>
</drv>
</art>
