<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sambr.xml,v 1.11 2023/07/15 21:24:48 revo Exp $">
<kap>
  <rad>Sambr</rad>/o
</kap>
<drv mrk="sambr.S0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref>, fluanta el
      <ref tip="malprt" cel="franc.F0ujo">Francujo</ref> al
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref>,
      kie &gcirc;i fluas en la riveron <ref tip="vid" cel="moz.M0o">Mozo</ref>
      apud la urbo
      <ref tip="vid" cel="namur.N0o.urb">Namuro</ref>:
      <ekz>
        kunfluejo, aludo al la loko kie, en Namur, Mozo kaj <tld/>o
        kunfluas.
        <fnt><aut>Claude PIRON</aut>:
        <vrk>Renkonte al Esperanto</vrk>,
        <bib>LOdE</bib>,
        <lok>n-ro 04/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_m;&c_b;&c_r;</trd>
  <trd lng="ca">Sambre</trd>
  <trd lng="de">Sambre</trd>
  <trd lng="fr">Sambre</trd>
  <trd lng="hu">Sambre</trd>
  <trd lng="it">Sambre</trd>
  <trd lng="nl">Samber</trd>
  <trd lng="pl">Sambra</trd>
  <trd lng="ru">&c_S;&c_a;&c_m;&c_b;&c_r;</trd>
</drv>
</art>
