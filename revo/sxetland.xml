<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sxetland.xml,v 1.6 2024/03/25 07:28:39 afido Exp $">
    <kap>
      <ofc>10</ofc>
      <rad>&Scirc;etland</rad>/o
    </kap>

<drv mrk="sxetland.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="insul.0aro">Insularo</ref>
      en la nordo de <ref tip="malprt" cel="skot.0lando">Skotlando</ref>:
      <ekz>
        sur la &scirc;ildo bildi&gcirc;as la blazono de <tld/>o:
        vikinga &scirc;ipo kun devizo en la
        antikva normana lingvo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008323.html">Le&gcirc;oj landon prosperigas</url></vrk>
              <bib>Monato</bib>
          <lok>2004/07, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio okazos al <tld/>o, se la Skota Nacia Partio
        sukcesos akiri sendependecon en
        venonta referendumo, estas neklare
        <fnt>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2014/011096.php">Skandinava veksilologio</url></vrk>
              <bib>Monato</bib>
          <lok>2014/03, p. 19</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="orkad.0o">Orkado</ref>
    <trd lng="de">Shetlandinseln</trd>
    <trd lng="en">Shetland</trd>
    <trd lng="fr">Shetland</trd>
    <trd lng="nl">Shetlandeilanden</trd>
    <trd lng="pl">Szetlandy {pl}</trd>
  </snc>
<trd lng="be">&c_Sh;&c_e;&c_t;&c_l;&c_a;&c_n;&c_d;&c_z;&c_k;&c_ib;&c_ja; &c_a;&c_s;&c_t;&c_r;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="zh">
    <trd>&#x8A2D;&#x5F97;&#x862D;&#x7FA4;&#x5CF6; <pr>Sh&#xE8;d&#xE9;l&#xE1;nQ&#xFA;nd&#x1CE;o</pr></trd>,
    <trd>&#x8BBE;&#x5F97;&#x5170;&#x7FA4;&#x5C9B; <pr>Sh&#xE8;d&#xE9;l&#xE1;nQ&#xFA;nd&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
  </art>

