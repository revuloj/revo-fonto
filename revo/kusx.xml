<drv mrk="kusx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kusx.0igxi.KOMUNE">
    <dif>
      Sin <tld/>igi:
      <ekz>
	<tld/>i&gcirc;i sur pajlo;
      </ekz>
      <ekz>
	hundo <tld/>i&gcirc;as &ccirc;e la piedoj de sia mastro;
      </ekz>
      <ekz>
	ili teren<tld/>i&gcirc;is anta&ubreve; la re&gcirc;o <klr>(adorsigne)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_ch;&c_y;</trd>,
    <trd>&c_u;&c_l;&c_je;&c_g;&c_ch;&c_y;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd><klr>sich </klr>hinlegen</trd>,
     <trd><klr>sich </klr>niederlegen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tumbarse</trd>,
    <trd>acostarse</trd>
  </trdgrp> 
  <trd lng="fr">se coucher<klr> (s'&eacute;tendre)</klr></trd>
  <trd lng="hu">lefekszik</trd>
  <trd lng="nl">gaan <ind>liggen</ind></trd>
  <trdgrp lng="pl">
    <trd>po&lstroke;o&zdot;y&cacute; si&eogonek;</trd>,
    <trd>k&lstroke;a&sacute;&cacute; si&eogonek;</trd>,
    <trd>lec</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>stai jos</trd>,
    <trd>culc&abreve;-te</trd>,
    <trd>du-te</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
