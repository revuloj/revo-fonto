<drv mrk="kusx.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <snc mrk="kusx.0acxi.KOMUNE">
    <dif>
       Malformale, dismembre, tro-komforte <tld/>i:
      <ekz>
        vi <tld/>a&ccirc;as, kvaza&ubreve; sako, suferante pro varmego
        <fnt>
          <aut>Czin &Scirc;entanj (= Jin Shengtan), trad. P. Mo&jcirc;ajev</aut>
          <vrk><url ref="http://esperantio.narod.ru/ekster/19ghojo.htm">Dek na&ubreve; &gcirc;ojoj de la vivo</url></vrk>
          <lok>[vidita en 2019]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>vautrer</ind></trd>
</drv>

<drv mrk="kusx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="kusx.0ejo.KOMUNE">
    <dif>
      Loko, kie iu <tld/>as:
      <ekz>
	mi parfumis mian <tld/>ejon
        <fnt><bib>MT</bib> <lok>&Sen; 7:17</lok></fnt>;
      </ekz>
      <ekz>
	&scirc;i ripozis en <tld/>ejo de floroj;
      </ekz>
      <ekz>
	<tld/>ejo de sova&gcirc;a besto.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_o;&c_zh;&c_a;</trd>,
      <trd>&c_l;&c_ja;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_l;&c_je;&c_zh;&c_n;&c_ja;</trd>,
      <trd>&c_l;&c_o;&c_g;&c_a;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Lager</trd>,
      <trd>Liegestatt</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>lecho</trd>,
      <trd>cama</trd>
    </trdgrp>
    <trd lng="fr">couche</trd>
    <trd lng="hu">fekhely</trd>
    <trd lng="nl">ligplaats</trd>
    <trdgrp lng="pl">
      <trd>&lstroke;o&zdot;e</trd>,
      <trd>koja</trd>,
      <trd>legowisko</trd>
    </trdgrp>   
    <trd lng="ro">strat</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_zh;&c_je;</trd>,
      <trd>&c_l;&c_je;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_l;&c_je;&c_zh;&c_b;&c_i;&c_shch;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kusx.0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="flu.0ejo.GEOG">fluejo</ref>
    <trd lng="be">&c_r;&c_e;&c_ch;&c_y;&c_sh;&c_ch;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Bett</trd>,
      <trd>Flussbett</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>lecho<klr> (de r&iacute;o)</klr></trd>,
      <trd>cauce<klr> (de r&iacute;o)</klr></trd>
    </trdgrp>
    <trd lng="fr">lit<klr> (de rivi&egrave;re)</klr></trd>
    <trd lng="hu">foly&oacute;&aacute;gy</trd>
    <trd lng="nl">bedding</trd>
    <trd lng="pl">koryto <klr> (rzeki)</klr> </trd>     
    <trd lng="ro">r&acirc;u <klr> (r&acirc;uri)</klr> </trd>
    <trd lng="ru">&c_r;&c_u;&c_s;&c_l;&c_o;</trd>
  </snc>
  <snc mrk="kusx.0ejo.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Mintavolo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_k;&c_l;&c_a;&c_d;</trd>,
      <trd>&c_z;&c_a;&c_l;&c_je;&c_zh;</trd>
    </trdgrp>
    <trd lng="de">Lagerst&auml;tte</trd>
    <trd lng="es">fil&oacute;n</trd>
    <trd lng="fr">filon</trd>
    <trdgrp lng="hu">
      <trd>tel&eacute;r</trd>,
      <trd>telep <klr tip="ind">(&aacute;sv&aacute;ny&eacute;)</klr></trd>,
      <trd>&aacute;sv&aacute;nyr&eacute;teg</trd>,
      <trd>lel&odblac;hely <klr tip="ind">(&aacute;sv&aacute;ny&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="nl">mijnader</trd>
    <trd lng="pl">z&lstroke;o&zdot;e</trd> 
    <trd lng="ro">strat</trd>
    <trd lng="ru">&c_z;&c_a;&c_l;&c_je;&c_zh;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="kusx.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="kusx.0ema.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="vid" cel="labor.mal0ema.KOMUNE">Mallaborema</ref>, 
      malpenema:
      <ekz>
	hundoj mutaj <klr>...</klr> dormemaj, <tld/>emaj
        <fnt><bib>MT</bib> <lok>&Jes; 56:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_ja;&c_v;&c_y;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_ja;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_n;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="de">tr&auml;ge</trd>
  <trdgrp lng="es">
    <trd>perezoso</trd>,
    <trd>vago</trd>
  </trdgrp>
  <trd lng="fr">flemmard</trd>
  <trdgrp lng="hu">
    <trd>helye</trd>,
    <trd>hever&eacute;sz&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hangerig</trd>,
    <trd>vadsig</trd>
  </trdgrp>
  <trd lng="pl">leniwy</trd>     
  <trd lng="ro">lene&scedil;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ja;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_n;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kusx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kusx.0igi.KOMUNE">
    <dif>
      Meti iun a&ubreve; ion, tiel ke &gcirc;i <tld/>u:
      <ekz>
	<tld/>igi infanon en lulilon;
      </ekz>
      <ekz>
	<tld/>igi botelon sur la tablo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stern.0i">sterni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>hinlegen</trd>,
     <trd>niederlegen</trd>
  </trdgrp>
 <trdgrp lng="es">
   <trd>tumbar</trd>,
   <trd>acostar</trd>
 </trdgrp> 
  <trd lng="fr">coucher<klr> (mettre &agrave; l'horizontal)</klr></trd>
  <trd lng="hu">lefektet</trd>
  <trd lng="nl">neerleggen</trd>
  <trd lng="pl">po&lstroke;o&zdot;y&cacute;</trd>     
  <trd lng="ro">pune</trd>
  <trdgrp lng="ru">
