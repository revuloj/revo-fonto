<drv mrk="kusx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kusx.0igi.KOMUNE">
    <dif>
      <ref tip="super" cel="met.0i.movi">Meti</ref> iun a&ubreve; ion,
      tiel ke &gcirc;i <tld/>u:
      <ekz>
        Doroteo ekkisis la infanon ankora&ubreve; unu fojon kaj volis <tld/>igi &gcirc;in ree en la
        lulilon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la duan nokton la admiralo ne <tld/>igas sin, la duan tagon turmentas lin unu
        sama premanta penso
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>igis lian veston apud si
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; blindulo ne <tld/>igu falilon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kolektis la koturnojn <klr>[&#8230;]</klr> kaj ili dis<tld/>igis ilin
        al si &ccirc;irka&ubreve; la tendaro
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 11:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro plej malgranda bagatelo oni <tld/>igas min sur la tero kaj batas kiel
        papiruson
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stern.0i">sterni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
