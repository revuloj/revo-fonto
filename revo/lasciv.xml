    <trd>&c_p;&c_o;&c_h;&c_ib;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_s;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28139;&#33633; <pr>y&iacute;nd&agrave;ng</pr></trd>,
    <trd>&#28139;&#34153; <pr>y&iacute;nd&agrave;ng</pr></trd>,
    <trd>&#20918;&#33633; <pr>y&ecaron;d&agrave;ng</pr></trd>,
    <trd>&#20918;&#34153; <pr>y&ecaron;d&agrave;ng</pr></trd>,
    <trd>&#22909;&#33394; <pr>h&agrave;os&egrave;</pr></trd>,
    <trd>&#28139;&#39080; <pr>y&iacute;nf&emacron;ng</pr></trd>,
