  </trdgrp>
</drv>

<drv mrk="lasciv.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Sinteno sugestanta mal&ccirc;aston, provanta instigi &gcirc;in &ccirc;e aliaj:
      <ekz>
        timi la sintrudon de amoremuloj kaj la atencojn de la <tld/>o
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 11a, p.
        82a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ju;&c_r;</trd>,
    <trd>&c_ju;&c_r;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_zh;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>lux&uacute;ria</trd>,
    <trd>lasc&iacute;via</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lascivita</trd>,
    <trd>lascivnost</trd>,
    <trd>necudnost</trd>
  </trdgrp>
  <trd lng="de">Laszivit&auml;t</trd>
  <trdgrp lng="fr">
    <trd>lascivet&eacute;</trd>,
    <trd>lascivit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bujas&aacute;g</trd>,
    <trd>&eacute;rz&eacute;kis&eacute;g</trd>,
    <trd>k&eacute;jv&aacute;gy</trd>
  </trdgrp>
  <trd lng="nl">wellustigheid<klr tip="ind"> (zeldz.)</klr></trd>
  <trd lng="pl">lubie&zdot;no&sacute;&cacute;</trd>
  <trd lng="ro">lascivitate</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_h;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">necudnos&tcaron;</trd>
</drv>
