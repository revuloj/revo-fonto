<kap>
  <rad>lasciv</rad>/a <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="lasciv.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Kondute <ref tip="super" cel="provok.0a">provoka</ref> al seksa volupto.
      <ekz>
        bela ru&gcirc;harulino sciencoplene demetadas siajn seksallogajn subvestojn je la
        ritmo de <tld/>a muziko
        <fnt>
          <bib>KrB</bib>
          <lok>La &gcirc;oja podio</lok>
        </fnt>;
      </ekz>
      <ekz>
        juna virino <klr>[&#8230;]</klr> volupta kaj <tld/>a
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tion oni nomas amo?! oni simple venas, <tld/>e alku&scirc;as kaj malkompleze uza&ccirc;as
        vin
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Konfeso de la Potifar-edzino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frivol.0a.maldeca">frivola<sncref/></ref>,
      <ref cel="cxast.mal0a">mal&ccirc;asta</ref>,
      <ref cel="dec.mal0a">maldeca</ref>,
      <ref cel="prud.mal0a">malpruda</ref>,
      <ref cel="obscen.0a">obscena</ref>,
      <ref cel="volupt.0a">volupta</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>voluptema</ke>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_ju;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_zh;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>lasciu</trd>,
    <trd>luxuri&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chl&iacute;pn&yacute;</trd>,
    <trd>lascivn&iacute;</trd>,
    <trd>necudn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>lasziv</trd>,
    <trd>zweideutig <klr>(lasziv)</klr></trd>,
    <trd>schl&uuml;pfrig</trd>,
    <trd>unz&uuml;chtig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lascivious</trd>,
    <trd>lustful</trd>,
    <trd>lewd</trd>
  </trdgrp>
  <trd lng="fr">lascif</trd>
  <trdgrp lng="hu">
    <trd>buja</trd>,
    <trd>&eacute;rz&eacute;ki</trd>,
    <trd>k&eacute;js&oacute;v&aacute;r</trd>,
    <trd>k&eacute;jv&aacute;gy&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wellustig<klr tip="ind"> (zeldz.)</klr></trd>,
    <trd>lascief<klr tip="ind"> (zeldz.)</klr></trd>
  </trdgrp>
  <trd lng="pl">lubie&zdot;ny</trd>
  <trdgrp lng="pt">
    <trd>lascivo</trd>,
    <trd>libidinoso</trd>,
    <trd>l&uacute;brico</trd>
  </trdgrp>
  <trd lng="ro">concupiscent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_h;&c_o;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
