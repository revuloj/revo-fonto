<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: monoklin.xml,v 1.2 2026/02/25 03:15:13 afido Exp $">
    <kap>
      <rad>monoklin</rad>/a
    </kap>

<drv mrk="monoklin.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="klr">(pri plantoj)</uzo>
  <snc mrk="monoklin.0a.BOT">
    <dif>
      Havanta duseksajn florojn:
      <ekz>
        rizo estas tipa mempolenanta planto <tld/>a
        <fnt>
          <bib>CRI</bib>
          <vrk><url ref="https://esperanto.cri.cn/1/2004/09/16/1@15023.htm"
              >Esploro kaj disvastigo de hibridigita rizo</url></vrk>
          <lok>2004-09-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="diklin.0a">diklina</ref>
    <trdgrp lng="de">
      <trd>zweigeschlechtlich</trd>,
      <trd>monoklin</trd>
    </trdgrp>
    <trd lng="pl">hermafrodytyczny</trd>
  </snc>
</drv>

  </art>

