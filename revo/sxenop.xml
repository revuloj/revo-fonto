<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxenop.xml,v 1.22 2018/10/07 19:10:21 revo Exp $">
<kap>
  <ofc>9</ofc><rad>&scirc;enopraz</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxenop.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref>
       de <ref tip="super" cel="ajl.0o.genro">ajlo</ref>
      (<trd lng="la">Allium schoenoprasum</trd>),
      uzata kiel <ref tip="lst" cel="spic.0o"
      lst="voko:spicoj">spicherbo</ref>;
      kun mallar&gcirc;e bulbforma radiko, el kiu kreskas unu a&ubreve; du, 5 &gcirc;is
      50&nbsp;cm   
      altaj, ronde tubetoformaj folioj kun diametro inter 2 kaj
      6&nbsp;mm,
      kun globformaj floraroj el 30 &gcirc;is 50 trinombraj, duseksaj,
      klo&scirc;formaj, hele 
      violkoloraj floroj, kies ses folietoj havas kontrastan malhelan
      mezan 
      strion:
      <ekz>
        prepari <klr>[...]</klr> ova&jcirc;on kun <tld/>o
        <fnt><bib>Fab4</bib>, <lok>Kion la patro faras, estas &ccirc;iam   
        &gcirc;usta</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o fajnigas &ccirc;efe ova&jcirc;ojn, salatojn, kazeon kaj buterpanon,
        por bona gusto necesas rikolti la planton anta&ubreve; la apero de
        floroj
        <fnt>
          <bib>Viki</bib>,
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/%C5%9Cenoprazo">&Scirc;enoprazo</url></vrk>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld
     lok="&WCU;/thumb/2/22/Allium_schoenoprasum_a1.jpg/640px-Allium_schoenoprasum_a1.jpg"
     prm="CC-BY-SA-3.0"
     >floranta <tld/>o (Allium schoenoprasum)
       <fnt>
         <aut>Jerzy Opio&lstroke;a, propra verko</aut>
         <lok><url ref="&WCW;/File:Allium_schoenoprasum_a1.jpg"
             >&WCW;/File:Allium_schoenoprasum_a1.jpg</url></lok>
       </fnt>
  </bld>
  <trdgrp lng="be">
    <trd>&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja; &c_s;&c_k;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja; &c_r;&c_e;&c_z;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_i;&c_v; &c_l;&c_u;&c_k;</trd>
  <trd lng="de">Schnittlauch</trd>
  <trd lng="en">chives</trd>
  <trd lng="fr">ciboulette</trd>
  <trdgrp lng="hu">
    <trd>met&eacute;l&odblac;hagyma</trd>,
    <trd>snidling</trd>
  </trdgrp>
  <trd lng="it">erba cipollina</trd>
  <trd lng="nl">bieslook</trd>
  <trd lng="pl">szczypiorek</trd>
  <trd lng="pt">cebolinha</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_n;&c_i;&c_t;&c_t;-&c_l;&c_u;&c_k;</trd>,
    <trd>&c_l;&c_u;&c_k;-&c_r;&c_je;&c_z;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_l;&c_u;&c_k;-&c_s;&c_k;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">gr&auml;sl&ouml;k</trd>
</drv>
</art>
