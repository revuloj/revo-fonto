<vortaro>
<art mrk="$Id$">
<kap>
<rad>&scirc;enopraz</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="sxenop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ajl.0o.genro">ajlo</ref>
      (<trd lng="la">Allium schoenoprasum</trd>),
      uzata kiel spica&jcirc;o.
    </dif>
  </snc>
<bld lok="../bld/sxenop.jpg">&gcirc;ardene kulturata <tld/>o</bld>
  <trd lng="de">Schnittlauch</trd>
  <trd lng="fr">ciboulette</trd>
  <trd lng="nl">bieslook</trd>
  <trd lng="pl">szczypiorek</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_n;&c_i;&c_t;-&c_l;&c_u;&c_k;</trd>,
    <trd>&c_l;&c_u;&c_k;-&c_r;&c_je;&c_z;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_l;&c_u;&c_k;-&c_s;&c_k;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>
