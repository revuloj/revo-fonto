<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: litv.xml,v 1.6 2023/09/01 19:11:00 revo Exp $">
    <kap>
      <rad>Litv</rad>/o
        <fnt>
          <bib>FK</bib>
        </fnt>
    </kap>

<drv mrk="litv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Ekde mezepoko, grandduklando ampleksinta <ref cel="litov.L0ujo">Litovujon</ref>, Belorusujon
      kaj Ukrainujon, &gcirc;is
      1795 unuigita kun Pollando:
      <ekz>
        la sendependa pola Respubliko konsistis el du &ccirc;efaj partoj: Pola
        krono kaj
        <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o! Patrujo mia! simila al sano, Vian grandan valoron
        ekkonas litvano, Vin
        perdinte
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>     
      <ekz>
        <tld lit="l"/>a pola poeto <nom>Mickievi&ccirc;</nom>, el Vilno
        forpelita de la rusa registaro
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz> 
      <ekz>
        <tld lit="l"/>a popola kanto
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gro&szlig;f&uuml;rstentum <ind>Litauen</ind></trd>
    <trd lng="pl">Litwa</trd>     
    <trd lng="ro">Lituania</trd>
  </snc>
  <trd lng="be">&c_L;&c_ib;&c_t;&c_v;&c_a; <klr>(&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="ja">&#x30EA;&#x30C8;&#x30A2;&#x30CB;&#x30A2;&#x5927;&#x516C;&#x56FD;  [<ind>&#x30EA;&#x30C8;&#x30A2;&#x30CB;&#x30A2;&#x3060;&#x3044;&#x304A;&#x304A;&#x3084;&#x3051;&#x3053;&#x304F;</ind>]</trd>
  <trd lng="uk">&#x41B;&c_i;&c_t;&c_v;&c_a;</trd>
</drv>

  </art>

