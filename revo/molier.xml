    <kap>
      <rad>Molier</rad>/o
    </kap>

<drv mrk="molier.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      <nom>Jean-Baptiste Poquelin</nom>, alinome <trd lng="fr">Moli&egrave;re</trd>
      (1622&#x202F;&#8208;&#x202F;1673), franca
      aktoro kaj
      komedi-<ref tip="lst" lst="voko:historiaj_personoj" cel="verk.0isto">verkisto</ref>:
      <ekz>
        Georgo <nom>Dandin</nom> estas 3-akta komedio de <tld/>o, verkita en 1668
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16274">Georgo Dandin</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tradicioj de <tld/>o en la komedio de <nom>Dancourt</nom>: &leftquot;Somero de Koketulinoj&rightquot;
        <fnt>
          <aut>L. Mursalijeva</aut>
          <lok><url ref="https://scienca-revuo.info/issue/view/162">Scienca Revuo, vol. 57, nr-o.
          204 (2006)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_a;&c_l;&c_mol;&c_je;&c_r;</trd>
    <trd lng="de">Moli&egrave;re</trd>
    <trd lng="fr">Moli&egrave;re</trd>
    <trd lng="nl">Moli&egrave;re</trd>
    <trd lng="pl">Molier</trd>
    <trd lng="ro">Moli&egrave;re</trd>
  </snc>
  <trd lng="ja">&#x30E2;&#x30EA;&#x30A8;&#x30FC;&#x30EB;</trd>
  <trd lng="uk">&#x41C;&#x43E;&#x43B;&#x44C;&c_jeu;&c_r; (&#x43A;&#x43E;&#x43C;&c_je;&c_d;&c_ib;&#x43E;&c_g;&c_r;&c_a;&c_f;)</trd>
</drv>

  </art>

