    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="figur.stil0o">Stil<tld/>o</ref>:
      <ekz>
        <ref tip="sub" cel="ironi.0o">ironio</ref> estas vort<tld/>o
        a&ubreve; <ref tip="sin" cel="trop.0o">tropo</ref> kiu konsistas je diri la malon a&ubreve; kontra&ubreve;on de
        tio kion oni volas diri
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50925">Ironio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a; &c_m;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;
      &c_s;&c_t;&c_y;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;
      &c_r;&c_y;&c_t;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>slovn&iacute; figura</trd>,
    <trd>slovn&iacute; &uacute;tvar ze s&eacute;mantick&eacute;ho hlediska</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stilfigur</trd>
  </trdgrp>
  <trd lng="es">figura literaria</trd>
  <trdgrp lng="fr">
    <trd>figure de style</trd>,
    <trd>trope</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&oacute;k&eacute;p</trd>,
    <trd>k&eacute;pes kifejez&eacute;s</trd>,
    <trd>tr&oacute;pus</trd>
  </trdgrp>
  <trd lng="it">figura<klr> stilistica</klr></trd>
  <trd lng="nl">stijlfiguur</trd>
  <trdgrp lng="pl">
    <trd>figura retoryczna</trd>,
    <trd>przeno&sacute;nia</trd>,
    <trd>paralela</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;
  &c_r;&c_je;&c_ch;&c_i;</trd>
  <trd lng="sk">slovn&yacute; &uacute;tvar</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_l;&c_o;&c_v;&c_je;&c_s;&c_n;&c_i;&c_j; &c_o;&c_b;&c_r;&c_a;&c_z;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_v;&c_je;&c_s;&c_n;&c_a; &c_f;&c_ib;&c_gu;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
