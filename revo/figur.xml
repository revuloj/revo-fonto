  <trdgrp lng="cs">
    <trd>figura</trd>,
    <trd>obrazec</trd>,
    <trd>postava</trd>,
    <trd>&uacute;tvar</trd>
  </trdgrp>
  <trd lng="cy">ffigur</trd>
