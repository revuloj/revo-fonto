<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>figur</rad>/o
</kap>
<drv mrk="figur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="figur.0o.aspekto">
    <dif>
      Videbla ekstera formo de esta&jcirc;o a&ubreve; objekto:
      <ekz>
        subite virina <tld/>o ekstaris anta&ubreve; mi;
      </ekz>
      <ekz>
        &scirc;ia tuta <tld/>o estis senmova<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nun &scirc;i per sia tuta <tld/>o el&scirc;ovi&gcirc;is
        en la lumon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas grava <tld/>o de la angla literaturo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>Figur</trd>
    </trdgrp>
    <trd lng="hu">alak</trd>
  </snc>
  <snc mrk="figur.0o.BELA">
    <dif>
      Belarta prezenta&jcirc;o de objekto a&ubreve; estulo:
      <ekz>
        <tld/>oj de dioj;
      </ekz>
      <ekz>
        sankt<tld/>o<fnt>Z</fnt>;
      </ekz>
    </dif>
    <subsnc mrk="figur.0o.sxaka">
      <uzo tip="fak">SPO</uzo>
      <dif>
        <ref tip="super" cel="sxak.0peco">&Scirc;akpeco</ref> pli grava
        ol peono (kaj tial skulptita malpli abstrakte ol peono):
        <ref tip="sub" cel="regx.0o.sxaka">re&gcirc;o</ref> (R, &#x2654;),
        <ref tip="sub" cel="dam.0o.sxaka">damo</ref> (D, &#x2655;),
        <ref tip="sub" cel="kurier.0o.sxaka">kuriero</ref> (K, &#x2657;),
        <ref tip="sub" cel="cxeval.0o.sxaka">&ccirc;evalo</ref> (&Ccirc;, &#x2658;),
        <ref tip="sub" cel="tur.0o.sxaka">turo</ref> (T, &#x2656;).
      </dif>
      <trdgrp lng="de">
        <trd>Figur</trd>,
        <trd>Schachfigur</trd>
      </trdgrp>
      <trd lng="hu">figura</trd>
    </subsnc>
    <subsnc mrk="figur.0o.karta">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Altranga <ref tip="super" cel="kart.lud0o">ludkarto</ref>,
        kutime kun bildo:
        <ref tip="sub" cel="regx.0o.karta">re&gcirc;o</ref>,
        <ref tip="sub" cel="dam.0o.karta">damo</ref>,
        <ref tip="sub" cel="fant.0o">fanto</ref>,
        <ref tip="sub" cel="jxoker.0o.karto">&jcirc;okero</ref>.
      </dif>
      <trd lng="de">Bild</trd>
    </subsnc>
    <trd lng="de">Zeichen</trd>
  </snc>
  <snc mrk="figur.0o.geometria">
    <ref tip="dif" cel="figur.geometria0o">geometria figuro</ref>
    <trd lng="de">geometrische Figur</trd>
    <trd lng="hu">alakzat</trd>
  </snc>
  <snc mrk="figur.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">LIN</uzo>
    <ref tip="dif" cel="figur.stil0o">stil<tld/>o</ref>
    <ekz>
      retorikaj <tld/>oj.
    </ekz>
    <trdgrp lng="de">
      <trd>Stilfigur</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>figure de style</trd>,
      <trd>trope</trd>
    </trdgrp>
    <trd lng="nl">stijlfiguur</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Figur</trd>
  </trdgrp>
  <trd lng="es">figura</trd>
  <trd lng="fr">figure<klr tip="ind"> (tous sens sauf visage)</klr></trd>
  <trd lng="nl">figuur</trd>
  <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="figur.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">LIN</uzo>
  <snc mrk="figur.0a.LIN">
    <dif>
      Metafora:
      <ekz>
        vorto uzata la&ubreve; la <tld/>a senco.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>fig&uuml;rlich</trd>,
      <trd>&uuml;bertragen</trd>
    </trdgrp>
    <trd lng="fr">figur&eacute;</trd>
    <trd lng="nl">figuurlijk</trd>
    <trdgrp lng="hu">
      <trd>&aacute;tvitt</trd>,
      <trd>k&eacute;pes</trd>
    </trdgrp>
    <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>

<drv mrk="figur.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Aperi en iu formo, ludi rolon:
      <ekz>
        en la kongreso li <tld/>is kiel interpretisto
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        en la skandalo <tld/>as gravaj politikistoj;
      </ekz>
      <ekz>
        li <tld/>as kiel estro, sed efektive estas nur subulo.
      </ekz>
    </dif>
    <adm>
      Estas strange, ke PIV1 konsideras tiun uzon transitiva. <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>eine <ind>Rolle spielen</ind></trd>,
      <trd>eine <ind>Figur abgeben</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>figurer<klr tip="ind"> (intr.)</klr></trd>,
      <trd>faire <ind>figure</ind> (de)</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>een <ind>rol</ind> spelen</trd>,
      <trd>figureren</trd>
    </trdgrp>
    <trd lng="hu">szerepel</trd>
    <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Prezenti, reprodukti formon, <tld/>on de esta&jcirc;o a&ubreve; objekto:
      <ekz>
        la statuo, pentra&jcirc;o, bildo <tld/>as personon,
        leonon, floron, vazon;
      </ekz>
      <ekz>
        <uzo tip="fak">MAT</uzo>
        oni <tld/>as per kurbo la ekvacion 
        <frm><k>f</k>(<k>x</k>,<k>y</k>) = 0</frm>
        <fnt><bib>MatTerm</bib><lok>p. 16</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">represent</trd>
    <trdgrp lng="de">
      <trd>bilden</trd>,
      <trd>formen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>figurer<klr tip="ind"> (tr.)</klr></trd>,
      <trd>repr&eacute;senter<klr tip="ind"> (figurer)</klr></trd>
    </trdgrp>
    <trd lng="hu">&aacute;br&aacute;zol</trd>
    <trd lng="pl">przedstawia&cacute;</trd>
    <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="figur.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Akcesora neparolanta aktoro.
    </dif>
  </snc>
  <trd lng="fr">figurant</trd>
  <trd lng="nl">figurant</trd>
  <trdgrp lng="hu">
    <trd>statiszta</trd>,
    <trd>mell&eacute;kszerepl&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;(&c_k;&c_a;) <ind>&c_k;&c_o;&c_r;&c_d;&c_je;&c_b;&c_a;&c_l;&c_je;&c_t;&c_a;</ind></trd>;
    <trd>&c_s;&c_t;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="figur.geometria0o">
  <kap>geometria <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 6</lok></fnt>
  <snc>
    <dif>
      <ref cel="ar.0o.MAT">Aro</ref> da punktoj, konsiderata de la
      vidpunkto de &gcirc;iaj geometriaj ecoj:
      <ekz>
        ebena <tld/>o <klr>(inkluzivata de ebeno)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="geomet1.0o">geometrio</ref>.
    </refgrp>
    <refgrp tip="sub">
      Specifaj figuroj:
      <ref cel="punkt.0o.elementa">punkto</ref>,
      <ref cel="lini.0o.MAT">linio</ref>,
      <ref cel="surfac.0o.MAT">surfaco</ref>,
      <ref cel="solid.0o.geometrio">solido</ref>.
    </refgrp>
  </snc>
  <bld tip="svg" lok="../bld/e_elementaj_figuroj.svg" alt="600" lrg="600">
    elementaj geometriaj <tld/>oj
  </bld>
  <trd lng="de">Figur</trd>
  <trd lng="en">figure<klr tip="ind"> (geometric shape)</klr></trd>
  <trd lng="fr">figure g&eacute;om&eacute;trique</trd>
  <trd lng="nl">figuur<klr tip="ind"> (geometrie)</klr></trd>
  <trd lng="hu">alakzat<klr tip="ind"> (m&eacute;rtani)</klr></trd>
  <trd lng="pl">figura geometryczna</trd>
  <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="figur.stil0o">
  <kap>stil<tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Esprimo, kiu uzas vortojn neordinare
      por atentiga a&ubreve; pla&ccirc;a efekto.
    </dif>
    <refgrp tip="sin">
      <ref cel="figur.0o.BELE">figuro</ref>,
      <ref cel="trop.0o">tropo</ref>,
      <ref cel="figur.vort0o">vort<tld/>o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Stilfigur</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>figure de style</trd>,
    <trd>trope</trd>
  </trdgrp>
  <trd lng="nl">stijlfiguur</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;k&eacute;p</trd>,
    <trd>k&eacute;pes kifejez&eacute;s</trd>,
    <trd>tr&oacute;pus</trd>
  </trdgrp>
</drv>

<drv mrk="figur.vort0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <ref tip="dif" cel="figur.stil0o">stil<tld/>o</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Stilfigur</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>figure de style</trd>,
    <trd>trope</trd>
  </trdgrp>
  <trd lng="nl">stijlfiguur</trd>
</drv>
</art>
