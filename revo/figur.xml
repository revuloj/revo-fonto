<vortaro>
<art mrk="$Id: figur.xml,v 1.133 2023/08/29 16:35:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>figur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="figur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="figur.0o.aspekto">
    <dif>
      Videbla ekstera formo de esta&jcirc;o a&ubreve; objekto:
      <ekz>
        subite virina <tld/>o ekstaris anta&ubreve; mi;
      </ekz>
      <ekz>
        &scirc;ia tuta <tld/>o estis senmova
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun &scirc;i per sia tuta <tld/>o el&scirc;ovi&gcirc;is
        en la lumon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas grava <tld/>o de la angla literaturo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="person.0o.rolanto">persono<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>Figur</trd>
    </trdgrp>
    <trd lng="hu">alak</trd>
    <trdgrp lng="pl">
      <trd>figura</trd>,
      <trd>posta&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="figur.0o.BELA">
    <dif>
      Belarta prezenta&jcirc;o de objekto a&ubreve; estulo:
      <ekz>
        <tld/>oj de dioj;
      </ekz>
      <ekz>
        sankt<tld/>ojn ornamas, kaj homojn malamas
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
    </dif>
    <subsnc mrk="figur.0o.sxaka">
      <uzo tip="fak">SPO</uzo>
      <dif>
        <ref tip="super" cel="sxak.0peco">&Scirc;akpeco</ref> pli
        grava
        ol peono kaj ordinare skulptita malpli abstrakte ol peono:
        <ekz>
          sur la tablo estis granda &scirc;aktabulo kun mirinde
          fajne faritaj <tld/>etoj
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 22a, p.
          219a</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="regx.0o.sxaka">re&gcirc;o</ref> (R,
        &#9812;),
        <ref cel="dam.0o.sxaka">damo</ref> (D, &#9813;),
        <ref cel="kurier.0o.sxaka">kuriero</ref> (K,
        &#9815;),
        <ref cel="cxeval.0o.sxaka">&ccirc;evalo</ref>
        (&Ccirc;, &#9816;),
        <ref cel="tur.0o.sxaka">turo</ref> (T, &#9814;)
      </refgrp>
      <trdgrp lng="de">
        <trd>Figur</trd>,
        <trd>Schachfigur</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>b&aacute;bu</trd>,
        <trd>figura</trd>
      </trdgrp>
      <trd lng="pl">figura<klr> szachowa</klr></trd>
    </subsnc>
    <subsnc mrk="figur.0o.karta">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Altranga <ref tip="super" cel="kart.lud0o">ludkarto</ref>,
        kutime kun bildo:
        <ekz>
          en la kartludoj, la fanto (a&ubreve; bubo) estas la <tld/>o malsupera al la damo
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=228079">Fanto</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="regx.0o.karta">re&gcirc;o</ref>,
        <ref cel="dam.0o.karta">damo</ref>,
        <ref cel="fant.0o">fanto</ref>,
        <ref cel="jxoker.0o.karto">&jcirc;okero</ref>
      </refgrp>
      <trd lng="de">Bild</trd>
      <trd lng="pl">figura<klr> karciana</klr></trd>
    </subsnc>
    <trd lng="es">imagen</trd>
    <trd lng="de">Zeichen</trd>
    <trd lng="hu">alak</trd>
    <trdgrp lng="pl">
      <trd>figura</trd>,
      <trd>figurka</trd>
    </trdgrp>
  </snc>
  <snc mrk="figur.0o.geometria">
    <ref tip="dif" cel="figur.geometria0o">geometria figuro</ref>
    <trd lng="de">geometrische Figur</trd>
    <trd lng="es">figura geom&eacute;trica</trd>
    <trd lng="hu">alakzat</trd>
    <trd lng="pl">figura geometryczna</trd>
  </snc>
  <snc mrk="figur.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="figur.stil0o">Stil<tld/>o</ref>:
      <ekz>
        ri&ccirc;a stilo kaj abundaj retorikaj <tld/>oj
       <fnt>
          <bib>Monato</bib>
          <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2000/007031.php">Katolikismo:
           Johano Hus &#8208; &ccirc;u ne plu herezulo?</url></vrk>
           <lok>2000/08, p. 19</lok>
           </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Stilfigur</trd>
    </trdgrp>
    <trd lng="es">figura literaria</trd>
    <trdgrp lng="fr">
      <trd>figure de style</trd>,
      <trd>trope</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&oacute;k&eacute;p</trd>,
      <trd>alakzat</trd>,
      <trd>tr&oacute;pus</trd>
    </trdgrp>
    <trd lng="nl">stijlfiguur</trd>
    <trdgrp lng="pl">
      <trd>figura retoryczna</trd>,
      <trd>przeno&sacute;nia</trd>
    </trdgrp>
  </snc>
  <trd lng="af">figuur</trd>
  <trd lng="am">&#4637;&#4661;&#4621;</trd>
  <trd lng="ar">&a_A;&a_l;&a_r;&a_q;&a_m;</trd>
