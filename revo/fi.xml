  <trdgrp lng="cs">
    <trd>fuj</trd>,
    <trd>jak nevhodn&eacute; slovo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pfui!</trd>,
    <trd>ih!</trd>,
    <trd>igitt!</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fie!</trd>,
    <trd>for shame!</trd>
  </trdgrp>
  <trd lng="es">Â¡puaf, qu&eacute; asco!</trd>
  <trd lng="hu">pfuj!</trd>
  <trdgrp lng="fr">
    <trd>fi!</trd>,
    <trd>beurk!</trd>,
    <trd>pouah!</trd>
  </trdgrp>
  <trd lng="hu">pfuj!</trd>
  <trd lng="ja">&#12481;&#12455;&#12483;&#65281;</trd>
  <trd lng="nl">foei</trd>
