<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>fi</rad> <fnt><bib>PV</bib></fnt>
</kap>
<subart>

  <drv mrk="fi.0">
    <kap><tld/></kap>
    <snc mrk="fi.0.interjekcio">
      <gra><vspec>ekkrio</vspec></gra>
	<dif>
	  Malestiminde, abomene, na&ubreve;ze!
	  <ekz>
	    <tld/>, kiel abomene!
	    <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 26</lok></fnt>!
	  </ekz>
      <ekz>
	    <ind><tld/> al vi!</ind>
	    <trd lng="en">shame on you!</trd>
	    <trd lng="hu">sz&eacute;gyeld magad!</trd>
	  </ekz>
	</dif>
    </snc>
    <trdgrp lng="cs">
    <trd>fuj</trd>,
    <trd>jak nevhodn&eacute; slovo</trd>
    </trdgrp>
    <trdgrp lng="de">
    <trd>pfui!</trd>,
    <trd>ih!</trd>
    </trdgrp>
    <trdgrp lng="en">
    <trd>fie!</trd>,
    <trd>for shame!</trd>
    </trdgrp>
    <trd lng="es">&#161;puaf, qu&eacute; asco!</trd>
    <trd lng="hu">pfuj!</trd>
    <trdgrp lng="fr">
    <trd>fi!</trd>,
    <trd>beurk!</trd>,
    <trd>pouah!</trd>
    </trdgrp>
    <trd lng="hu">pfuj!</trd>
    <trdgrp lng="pt">
    <trd>fu</trd>,
    <trd>abren&uacute;ncio</trd>,
    <trd>apre</trd>,
    <trd>chi</trd>,
    <trd>credo</trd>,
    <trd>cruzes</trd>,
    <trd>fora</trd>,
    <trd>irra</trd>
    </trdgrp>
    <trdgrp lng="ru">
    <trd>&c_f;&c_i;!</trd>,
    <trd>&c_f;&c_u;!</trd>
    </trdgrp>
    <trdgrp lng="sk">
    <trd>ak&eacute; nevhodn&eacute; slovo</trd>,
    <trd>fuj</trd>
    </trdgrp>
  </drv>
</subart>
<subart>
  <drv mrk="fi.0.prefikso">
    <kap><tld/>-</kap>
    <snc>
    <dif>
	<ref tip="dif" cel="prefik.0o">Prefikso</ref> esprimanta malestimon:
	<ekz>
	  <ind><tld/>komercisto</ind><fnt>Z</fnt>;
	  <trd lng="de">dubioser Gesch&auml;ftsmann</trd>
	  <trd lng="es">comerciante mezquino</trd>
	  <trd lng="fr">mercanti</trd>
      <trd lng="hu">nyer&eacute;szked&odblac;</trd>
	</ekz>
      <ekz>
	  <ind><tld/>bu&scirc;o</ind>;
	  <trd lng="vo">lumud</trd>
	  <fnt>Z</fnt>
	  <trd lng="en">dirty mouth</trd>
	  <trd lng="de">Schandmaul</trd>
	  <trd lng="es">malhablado</trd>
	  <trd lng="hu">mocskos sz&aacute;j</trd>
	</ekz>
      <ekz>
	  <ind><tld/>insektoj</ind>
	  <trd lng="de">Ungeziefer</trd>
	  <trd lng="en">pests</trd>
	  <trd lng="es">plaga</trd>
	  <trd lng="fr">insecte nuisible</trd>
	  <trd lng="hu">f&eacute;reg<klr tip="ind"> (k&aacute;rt&eacute;kony rovar)</klr></trd>
	  <fnt><bib>MT</bib>, <lok>&Eli; 8:24</lok></fnt>.
	</ekz>
  </dif>
  </snc>
  <rim mrk="fi.0.RIM">
  Prefikseca <em><tld/>-</em> kaj la sufikso <ref tip="sin"
  cel="acx">-a&ccirc;</ref> estas similaj kaj povas en multaj okazoj esti
  preska&ubreve; indiferente uzataj. Ili tamen diferencas inter si per
  tio, ke <em>-a&ccirc;</em> estas prefere objektiva, dum <em><tld/>-</em>
  estas prefere subjektiva; <em>-a&ccirc;</em> povas montri simple, ke iu
  a&ubreve; io estas malbonkvalita, neta&ubreve;ga, a&ubreve; ke oni
  opinias &gcirc;in tia; <em><tld/>-</em> esprimas negativan ju&gcirc;on el
  morala vidpunkto:
  <ekz>
	&ccirc;evala&ccirc;o ne estas <tld/>a &ccirc;evalo, sed tre malfeli&ccirc;a;
  </ekz>
  <ekz>
	tiu <tld/>ulo estas tre lerta kaj bela viro.
  </ekz>
</rim>
  <trd lng="vo">lu-</trd>
  </drv>
</subart>

<subart>
  <dif>
	Samsignifa memstara vortero:
  </dif>
  <drv mrk="fi.0a">
	<kap><tld/>a</kap>
	<snc>
	  <dif>
	    <refgrp tip="sin">
	      <ref cel="nobl.mal0a">Malnobla</ref>,
	      <ref cel="abomen.0inda">abomeninda</ref>
	    </refgrp>:
	    <ekz>
	      jen la terure malbela <tld/>a flanko de l' afero
	      <fnt>
		<aut>H. Harrison, trad. W. Auld</aut>,
		<vrk>La stratoj de A&scirc;kelono</vrk>,
                <lok>1994</lok>
	      </fnt>;
	    </ekz>
            <ekz>
	      ne trovi&gcirc;as akuzo, ekde la rabado, &gcirc;is
	      la patromurdo, a&ubreve; de la vanteco &gcirc;is la plej <tld/>a
	      krueleco, kiu ne trafus ilin tute prave
	      <fnt><bib>SatirRak</bib>, <vrk>Logos</vrk></fnt>;
	    </ekz>
            <ekz>
	      <tld/>a nuanco, senco, tono.
	    </ekz>
	  </dif>
	</snc>
    <trdgrp lng="cs">
    <trd>b&iacute;dn&yacute;</trd>,
    <trd>hanebn&yacute;</trd>,
    <trd>mrzk&yacute;</trd>,
    <trd>ni&ccaron;emn&yacute;</trd>,
    <trd>podl&yacute;</trd>
    </trdgrp>
    <trdgrp lng="en">
    <trd>vile</trd>,
    <trd>filthy</trd>,
    <trd>dirty</trd>,
    <trd>base</trd>,
    <trd>shameful</trd>,
    <trd>disgusting</trd>
    </trdgrp>
    <trdgrp lng="es">
    <trd>abyecto</trd>,
    <trd>vil</trd>,
    <trd>indigno</trd>,
    <trd>despreciable</trd>
    </trdgrp>
    <trdgrp lng="fr">
    <trd>abject<klr tip="ind"> (m&eacute;prisable)</klr></trd>,
    <trd>bas<klr tip="ind"> (m&eacute;prisable)</klr></trd>,
    <trd>indigne</trd>,
    <trd>vilain</trd>,
    <trd>m&eacute;prisable</trd>
    </trdgrp>
    <trdgrp lng="hu">
    <trd>aljas</trd>,
    <trd>ocsm&aacute;ny</trd>,
    <trd>gal&aacute;d</trd>
    </trdgrp>
    <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_z;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sk">hanebn&yacute;</trd>
  </drv>

  <drv mrk="fi.0ajxo">
	<kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
	<snc>
	  <dif>
	    <tld lit="F"/>a ago a&ubreve; abomeninda objekto.
	  </dif>
	</snc>
	<trdgrp lng="es">
	  <trd>bajeza</trd>,
	  <trd>vileza</trd>
	</trdgrp>
	<trdgrp lng="fr">
	  <trd>bassesse<klr> (acte bas)</klr></trd>,
	  <trd>cochonnerie<klr> (acte, propos)</klr></trd>
	</trdgrp>
	<trdgrp lng="hu">
	  <trd>aljas tett</trd>,
	  <trd>gal&aacute;ds&aacute;g</trd>
	</trdgrp>
	<trd lng="ru">&c_p;&c_o;&c_d;&c_l;&c_o;&c_s;&c_t;&c_mol; <klr>(&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;)</klr></trd>
  </drv>

  <drv mrk="fi.0eco">
	<kap><tld/>eco</kap>
	<snc>
    <dif>
     Karakterizo de io a&ubreve; iu <tld/>a.
    </dif>
    </snc>
<trd lng="cs">podlost</trd>
<trd lng="es">mezquindad</trd>
<trd lng="fr">bassesse<klr tip="ind"> (caract&egrave;re
	      m&eacute;prisable)</klr></trd>
<trd lng="ru">&c_p;&c_o;&c_d;&c_l;&c_o;&c_s;&c_t;&c_mol; <klr>(&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
<trd lng="sk">oplanstvo</trd>
</drv>

    <drv mrk="fi.0ulo">
	<kap><tld/>ulo</kap>
	<snc>
    <dif>
     Persono malestiminda.
    </dif>
    </snc>
<trdgrp lng="cs">
<trd>dareb&aacute;k</trd>,
<trd>padouch</trd>,
<trd>v&scaron;iv&aacute;k</trd>
</trdgrp>
<trdgrp lng="es">
<trd>despreciable</trd>,
<trd>indigno</trd>,
<trd>bellaco</trd>,
<trd>abyecto</trd>
</trdgrp>
<trd lng="fr">salaud</trd>
<trdgrp lng="sk">
<trd>dareb&aacute;k</trd>,
<trd>podliak</trd>,
<trd>v&scaron;iv&aacute;k</trd>
</trdgrp>
</drv>
</subart>
 
<adm>
  La rimarko ne kontentigas min. Mi sentas, ke "fi" referencas al ia
  socia morala normo, dum "a&ccirc;", kvankam &gcirc;i celas kaj materiajn kaj
  moralajn ecojn estas pli subjektiva. Ekzemploj helpu. <aut>MB</aut>
</adm>
<adm>
  &Ccirc;u mia ekzemplo pri &ccirc;evala&ccirc;o sufi&ccirc;as?
  <aut>Sergio</aut>
</adm>
</art>
