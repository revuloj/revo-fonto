<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tres.xml,v 1.4 2024/12/04 08:38:03 revo Exp $">
<kap>
  <rad>tres</rad>/o
</kap>
<drv mrk="tres.0o">
  <kap><tld/>o</kap>
  <snc mrk="tres.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kubdevena
      <ref tip="super" cel="instru2.kord0o.MUZ">kordinstrumento</ref>,
      <ref tip="vid" cel="gitar.0o.MUZ">gitar</ref>eto origine
      trikorda, evoluinta al tri kordo-duoj:
      <ekz>
        aliaj muzikistoj ludas gitaron, fluton,
        marakon, perkutilon kaj <tld/>on
        <fnt>
          <aut>&Lstroke;. &Zdot;ebrowski</aut>,
          <vrk><url
          ref="http://pl-pl.facebook.com/note.php?note_id=111571585566049">
          Amindaj estas kubanoj</url></vrk>,
          <lok>Esperanto, [2010?], 1239 (9)&#65279;</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_s; <klr>(&c_v;&c_ib;&c_d; &c_g;&c_ib;&c_t;&c_a;&c_r;&c_y;)</klr></trd>
  <trd lng="de">Tres</trd>
  <trd lng="fr">tres<klr> (inst. de musique)</klr></trd>
  <trd lng="pl">tres  <klr>(gitara)</klr></trd>
</drv>
</art>

