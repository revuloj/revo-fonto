<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kutr</rad>/o
  <fnt>Z</fnt>
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kutr.0o">
  <kap><tld/>o</kap>
  <snc mrk="kutr.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Malgranda unumasta <ref tip="super" 
      cel="sxip.vel0o.MAR">vel&scirc;ipo</ref>:
      <ekz>
        la boato, malgranda <tld/>o <klr>...</klr> &gcirc;uste pasis la 
        longecan rifan promontoron
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url
          ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_r;</trd>
  <trd lng="cs">kutr</trd>
  <trd lng="de">Kutter</trd>
  <trd lng="en">cutter</trd>
  <trd lng="es">c&uacute;ter</trd>
  <trd lng="fr">cotre</trd>
  <trd lng="hu">kutter</trd>
  <trd lng="nl">kotter</trd>
  <trd lng="pt">c&uacute;ter</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jednosťažník</trd>,
    <trd>kuter</trd>
  </trdgrp>
</drv>
</art>
