<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kutr.xml,v 1.21 2023/12/03 09:59:04 revo Exp $">
<kap>
  <rad>kutr</rad>/o <fnt><bib>JED</bib></fnt>,
  <var><kap><rad var="e">kuter</rad>/o</kap></var>
</kap>
<drv mrk="kutr.0o">
  <kap><tld/>o, <var><kap><tld var="e"/>o</kap></var></kap>
  <snc mrk="kutr.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Malgranda unumasta <ref tip="super" cel="sxip.vel0o.SHI">vel&scirc;ipo</ref>:
      <ekz>
        la &gcirc;endarmoj balda&ubreve; aperis sur eta <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la boato, malgranda <tld/>o <klr>[&#x2026;]</klr> &gcirc;uste pasis la
        longecan rifan promontoron
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>
          <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>
          <lok>inko, 2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tuj povis diri, &ccirc;u &scirc;ipo estas brigo, <tld var="e"/>o, skuno
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni uzas la <tld/>ojn komune, ni &ccirc;asas &ccirc;iam kolektive
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>4. La fatala sa&gcirc;eco.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_r;</trd>
  <trd lng="cs">kutr</trd>
  <trd lng="de">Kutter</trd>
  <trd lng="en">cutter</trd>
  <trd lng="es">c&uacute;ter</trd>
  <trd lng="fr">cotre</trd>
  <trd lng="hu">kutter</trd>
  <trd lng="ja">&#x30AB;&#x30C3;&#x30BF;&#x30FC;</trd>
  <trd lng="nl">kotter</trd>
  <trdgrp lng="pl">
    <trd>jacht</trd>,
    <trd>kuter</trd>
  </trdgrp>
  <trd lng="pt">c&uacute;ter</trd>
  <trdgrp lng="ro">
    <trd>iaht</trd>,
    <trd>vas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jednos&tcaron;a&zcaron;n&iacute;k</trd>,
    <trd>kuter</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&c_n;&c_d;&c_je;&c_r; (&c_o;&c_d;&c_n;&c_o;&c_shch;&c_o;&c_g;&c_l;&c_o;&c_v;&c_a; &c_v;&c_ib;&c_t;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_a; &c_ja;&c_h;&c_t;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5DE1;&#x903B;&#x8239; <pr>x&#xFA;nlu&#xF3;chu&#xE1;n</pr></trd>,
    <trd>&#x5DE1;&#x908F;&#x8239; <pr>x&#xFA;nlu&#xF3;chu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
