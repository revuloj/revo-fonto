<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>papav</rad>/o
</kap>
<drv mrk="papav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el <ref tip="super" cel="papav.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Papaver</trd>),
      kun belaj floroj, el kiu oni ekstraktas opion kaj kies semoj
      liveras bonan man&gcirc;eblan oleon.
    </dif>
  </snc>
  <trd lng="de">Mohn</trd>
  <trd lng="fr">pavot</trd>
</drv>
<drv mrk="papav.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="papav.0o"><tld/>o</ref>
      (<trd lng="la">Papaver rhoeas</trd>),
      kun skarlataj floroj, tre ofta en grenkampoj.
    </dif>
  </snc>
  <bld lok="../bld/papav.jpg">
    floro
  </bld>
  <trd lng="de">Klatschmohn</trd>
  <trd lng="fr">coquelicot</trd>
</drv>
<drv mrk="papav.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Papaveraceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Mohngew&auml;chse</trd>
</drv>
</art>
