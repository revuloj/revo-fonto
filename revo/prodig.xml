<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prodig.xml,v 1.15 2022/05/27 07:34:46 revo Exp $">
<kap>
  <rad>prodig</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="prodig.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prodig.0i.KOMUNE">
    <dif>
      Malavari:
      <ekz>
        &ccirc;u pli <tld/>as iu por sia dam' ol mi
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>?
      </ekz>
      <ekz>
        <nom>&Ccirc;aplickij</nom> <klr>[&#8230;]</klr> mortis en mizero 
        <tld/>inte siajn milionojn
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        tradicia <tld/>ado de mono
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011179.php">Abenomiko, a&ubreve;daca defio</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sinsekvaj registaroj <tld/>adis amason da mono por stimuli la ekonomion la&ubreve;
        la postuloj de la popolo
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2012/010776.php">Konkurado al &scirc;tata paneo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">prodigar</trd>
  <trdgrp lng="cs">
    <trd>mrhat</trd>,
    <trd>pl&yacute;tvat</trd>,
    <trd>promarnit</trd>
  </trdgrp>
  <trd lng="de">verschwenden</trd>
  <trd lng="fr">prodiguer</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;kozol</trd>,
    <trd>pazarol</trd>
  </trdgrp>
  <trd lng="nl"><ind>gul</ind> zijn<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pt">
    <trd>prodigalizar</trd>,  
    <trd>ser <ind>pr&oacute;digo</ind></trd>,      
    <trd>esbanjar</trd>,
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>porozhadzova&tcaron;</trd>,
    <trd>premrha&tcaron;</trd>,
    <trd>prem&aacute;rni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_r;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_a;&c_r;&#x43D;&#x43E;&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_t;&c_r;&c_i;&#x43D;&#x44C;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="prodig.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prodig.0a.KOMUNE">
    <dif>
     <ref tip="dif" cel="avar.mal0a">Malavara</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        preni de la bran&ccirc;oj de fortikaj kaj <tld/>aj kverkoj ties dol&ccirc;an
        kaj maturan frukton
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 11a, p.
        82a</lok></fnt>;
      </ekz>
      <ekz>
        fifamaj uzuristoj kolektas &scirc;uldon de <tld/>a filo el la familianoj, kompreneble
        kontra&ubreve;le&gcirc;e
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2005/008691.php">Familia nomo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pr&ograve;dig</trd>,
    <trd>gener&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>marnotratn&yacute;</trd>,
    <trd>rozma&rcaron;il&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verschwenderisch</trd>,
  </trdgrp>
  <trd lng="fr">prodigue</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;kozl&oacute;</trd>,
    <trd>pazarl&oacute;</trd>
  </trdgrp>
  <trd lng="nl">gul<klr tip="ind"> (zeldz.)</klr></trd>
  <trd lng="pt">pr&oacute;digo</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_shch;&c_je;&c_d;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">m&aacute;rnotratn&yacute;</trd>
  <trd lng="uk">&#x43C;&c_a;&c_r;&#x43D;&#x43E;&c_t;&c_r;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="prodig.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prodig.0o.KOMUNE">
    <dif>
      Malavaro:
      <ekz>
        li decidis senigi sin de sia regalemo kaj <tld/>o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 39a, p.
        293a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">prodigalitat</trd>
  <trd lng="de">Verschwendung</trd>
  <trd lng="fr">prodigalit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;kozl&aacute;s</trd>,
    <trd>pazarl&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">gulheid<klr tip="ind"> (zeldz.)</klr></trd>
  <trd lng="pt">prodigalidade</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_shch;&c_je;&c_d;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

