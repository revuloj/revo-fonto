<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pis.xml,v 1.22 2024/02/17 06:43:08 revo Exp $">
<kap>
  <ofc>9</ofc><rad>pis</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pis.0i">
  <kap><ofc>9</ofc><tld/>i <fnt><bib>HejmVort</bib></fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="pis.0i.FIZL">
    <uzo tip="fak">FIZL</uzo> 
    <dif>
      <ref tip="dif" cel="urin.0i">Urini</ref>:
      <ekz>
        kiu ses- a&ubreve; sep-jara infano ne kelkafoje <tld/>as en la lito,
        dumdorme?
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/009002.php">Memoroj
          neniam perdas sian mal&gcirc;ojon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la dioj prenis la felon de la virbovo kaj ejakulis a&ubreve; <tld/>is
        sur &gcirc;i,
        subterigante poste la rezulton apud la tombejo de la edzino de
        Hirieo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=388222">Oriono
          (mitologio)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la re&gcirc;o <tld/>is stare (kiel viro) a&ubreve; side (kiel virino)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=457840">Francisko de
          Asizo de Burbono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_c;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&ccaron;urat</trd>,
    <trd>&ccaron;&uring;rat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pinkeln</trd>,
    <trd>pissen</trd>,
    <trd>pullern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><klr>(take a) </klr>piss</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mear</trd>,
    <trd>hacer pipi</trd>
  </trdgrp>
  <trd lng="fr">pisser</trd>
  <trd lng="hu">pisil</trd>
  <trdgrp lng="id">
    <trd><ind>buang air</ind> kecil</trd>,
    <trd>kencing</trd>,
    <trd>pipis</trd>
  </trdgrp>
  <trd lng="ja">&#x304A;&#x3057;&#x3063;&#x3053;&#x3059;&#x308B;</trd>
  <trdgrp lng="nl">
    <trd>plassen</trd>,
    <trd>pipi doen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mocz oddawa&cacute;</trd>,
    <trd>siusia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>urinar</trd>,
    <trd>mijar</trd>,
    <trd>fazer <ind>xixi</ind></trd>    
  </trdgrp>
  <trdgrp lng="ro">
    <trd>urina</trd>,
    <trd>pi&#x219;a</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>
  <trd lng="sk">cika&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_s;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_c;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x4FBF; <pr>xi&#x1CE;obi&#xE0;n</pr></trd>,
    <trd>&#x6492;&#x5C3F; <pr>s&#x101;ni&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pis.0o">
  <kap><ofc>9</ofc><tld/>o </kap> 
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="pis.0o.FIZL">
    <uzo tip="fak">FIZL</uzo> 
    <dif>
      Ago <tld/>i kaj ties rezulto, t.e. <ref tip="dif" cel="pis.0ajxo"><tld/>a&jcirc;o</ref>:
      <ekz>
        apartamento <esc>plenplena</esc> da libroj odorantaj kat<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La
          lastaj tagoj de Lillo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        diabla <tld/>o ... &ndash; li grumblis
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2013/010910.php">Dobos
          Delight</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Brus</nom> <klr>[&#x2026;]</klr> finis la
        &leftquot;teatra&jcirc;on&rightquot; trinkante sian <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=588367">G&uuml;nter
          Brus</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_ch;&c_a;&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&ccaron;ur&aacute;n&iacute;</trd>,
    <trd>&ccaron;&uring;r&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="en">urine</trd>
  <trd lng="fr">urine</trd>
  <trd lng="ja">&#x304A;&#x3057;&#x3063;&#x3053;</trd>
  <trdgrp lng="pl">
    <trd>siusianie</trd>,
    <trd>szczanie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pisand</trd>,
    <trd>urinatul</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C3F; <pr>ni&#xE0;o</pr></trd>,
    <trd>&#x5C3F;&#x6DB2; <pr>ni&#xE0;oy&#xE8;</pr></trd>,
    <trd>&#x5C3F; <pr>su&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pis.0ajxo">
  <kap><ofc>9</ofc><tld/>a&jcirc;o </kap> 
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="pis.0ajxo.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="dif" cel="urin.0o">Urino</ref>:
      <ekz>
        Ze&ubreve;so <klr>[&#x2026;]</klr> diris, ke, kiam ilia <tld/>a&jcirc;o fluos kiel
        rivero, 
        tiuj suferoj seni&gcirc;os,<klr>[&#x2026;]</klr> tial nun, se nur unu azeno
        <tld/>as, 
        aliaj azenoj anka&ubreve; <tld/>as &ccirc;irka&ubreve; &gcirc;i
        <fnt>
          <vrk><url ref="&EduInf;lern/librejo/0030/0010.php">440 
         Fabloj de Ezopo &#x2010; 410. Azenoj Petis al Ze&ubreve;so</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pisse</trd>,
      <trd>Pulle</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_ch;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ur&aacute;n&iacute;</trd>,
    <trd>&ccaron;&uring;r&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="en">urine</trd>
  <trdgrp lng="fr">
    <trd>pisse</trd>,
    <trd>urine</trd>
  </trdgrp>
  <trd lng="ja">&#x304A;&#x3057;&#x3063;&#x3053;</trd>
  <trdgrp lng="pl">
    <trd>mocz</trd>,
    <trd>szczyny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pipi</trd>,
    <trd>urin&abreve;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_je;&c_ch;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C3F;&#x6837; <pr>ni&#xE0;oy&#xE0;ng</pr></trd>,
    <trd>&#x5C3F;&#x6A23; <pr>ni&#xE0;oy&#xE0;ng</pr></trd>,
    <trd>&#x5C3F; <pr>ni&#xE0;o</pr></trd>,
    <trd>&#x5C3F;&#x6DB2; <pr>ni&#xE0;oy&#xE8;</pr></trd>,
    <trd>&#x5C3F; <pr>su&#x12B;</pr></trd>,
    <trd>&#x5C0F;&#x606D; <pr>xi&#x1CE;og&#x14D;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pis.for0igxi">
  <kap>
    for<tld/>i&gcirc;i,
    <var><kap>sin for<tld/>i</kap></var>
  </kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhoneste, pretekste, fu&gcirc;i el malkonvena situacio:
      <ekz>
        <nom>Ka&ccirc;inski</nom> kun sia partio <nom>PiS</nom> for<tld/>is
        sin el la registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009365.php">Du mil
          sep</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
       <tld lit="P"/>ado en milito ofte estis la sola ebleco fori&gcirc;i por
       iom da tempo de la fronto.
    </rim>
  </snc>
  <trd lng="be">&c_u;&c_c;&c_ja;&c_ch;&c_y;</trd>
  <trd lng="de">sich <ind>verpissen</ind></trd>
  <trdgrp lng="pl">
    <trd>wysiusia&cacute; si&eogonek;</trd>,
    <trd>odla&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>se urina</trd>,
    <trd>face pipi</trd>
  </trdgrp>
</drv>

<drv mrk="pis.0poto">
  <kap><tld/>poto <fnt><bib>HejmVort</bib></fnt></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
     <dif>
       <ref tip="dif" cel="vaz.nokto0o">Noktovazo</ref>,
       precipe tia por malgrandaj infanoj, kiuj ankora&ubreve;
       ne povas uzi necesejon.
     </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_n;&c_y; <ind>&c_g;&c_a;&c_r;&c_sh;&c_ch;&c_o;&c_k;</ind></trd>
  <trd lng="ca">orinal</trd>
  <trd lng="cs">no&ccaron;n&iacute;k</trd>
  <trdgrp lng="de">
    <trd>T&ouml;pfchen <klr>(zum Pullern)</klr></trd>,
    <trd>Pisspott</trd>
  </trdgrp>
  <trd lng="en">toilet</trd>
  <trd lng="es">orinal</trd>
  <trd lng="fr">pot<klr> (urinoir)</klr></trd>
  <trd lng="hu">bili</trd>
  <trd lng="id">pispot</trd>
  <trd lng="ja">&#x304A;&#x307E;&#x308B;</trd>
  <trdgrp lng="nl">
    <trd>pispot</trd>,
    <trd>po</trd>
  </trdgrp>
  <trd lng="pl">nocnik</trd>
  <trdgrp lng="pt">
    <trd>urinol</trd>,
    <trd>penico</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>oal&abreve; de noapte</trd>,
    <trd>oli&#x21B;&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_n;&c_o;&c_j;
  <ind>&c_g;&c_o;&c_r;&c_sh;&c_o;&c_k;</ind></trd>
  <trd lng="sk">no&ccaron;n&iacute;k</trd>
  <trd lng="uk">&#x43D;&c_ib;&c_ch;&#x43D;&c_i;&c_j; &c_g;&#x43E;&c_r;&c_shch;&#x43E;&#x43A;</trd>
</drv>
