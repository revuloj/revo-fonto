<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>pis</rad>/i
</kap>
<drv mrk="pis.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Familiara vorto por <ref tip="sin" cel="urin.0i">urini</ref>.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>plassen</trd>,
    <trd>pipi doen</trd>
  </trdgrp>
</drv>
<drv mrk="pis.0poto">
  <kap><tld/>poto</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Vazo, en kiun oni kakas kaj <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>pispot</trd>,
    <trd>po</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
