<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pis</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pis.0i">
  <kap><tld/>i</kap> <fnt><bib>HejmVort</bib></fnt>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">KOMUNE</uzo>
  <uzo tip="fak">FIZL</uzo>
  <snc mrk="pis.0i.FIZL">
    <ref tip="dif" cel="urin.0i">urini</ref>
  </snc>
  <trdgrp lng="en">
    <trd><klr>(take a) </klr>piss</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mear</trd>,
    <trd>hacer pipi</trd>
  </trdgrp>
  <trd lng="fr">pisser</trd>
  <trd lng="hu">pisil</trd>
  <trdgrp lng="nl">
    <trd>plassen</trd>,
    <trd>pipi doen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>urinar</trd>,
    <trd>mijar</trd>,
    <trd>fazer <ind>xixi</ind></trd>    
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>
</drv>

<drv mrk="pis.0poto">
  <kap><tld/>poto</kap>
  <fnt><bib>HejmVort</bib></fnt>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <dif>
      Vazo, en kiun oni kakas kaj <tld/>as.
    </dif>
  </snc>
  <trd lng="ca">orinal</trd>
  <trd lng="en">toilet</trd>
  <trd lng="es">orinal</trd>
  <trd lng="fr">pot<klr> (urinoir)</klr></trd>
  <trd lng="hu">bili</trd>
  <trdgrp lng="nl">
    <trd>pispot</trd>,
    <trd>po</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>urinol</trd>,
    <trd>penico</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_n;&c_o;&c_j; <ind>&c_g;&c_o;&c_r;&c_sh;&c_o;&c_k;</ind></trd>
</drv>
</art>
