<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pis</rad>/i
</kap>
<drv mrk="pis.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Familiara vorto por <ref tip="sin" cel="urin.0i">urini</ref>.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>plassen</trd>,
    <trd>pipi doen</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>
</drv>

<drv mrk="pis.0poto">
  <kap><tld/>poto</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Vazo, en kiun oni kakas kaj <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>pispot</trd>,
    <trd>po</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_n;&c_o;&c_j; <ind>&c_g;&c_o;&c_r;&c_sh;&c_o;&c_k;</ind></trd>
</drv>
</art>
