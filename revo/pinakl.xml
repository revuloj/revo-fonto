<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pinakl.xml,v 1.15 2023/09/03 12:45:07 revo Exp $">
<kap>
  <rad>pinakl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pinakl.0o">
  <kap><tld/>o</kap>
  <snc mrk="pinakl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pinta&jcirc;o staranta sur <ref tip="malprt" cel="tegmen.0o">tegmento</ref>, arka&jcirc;o a&ubreve; apoga&jcirc;o,
      stabilige a&ubreve; orname:
      <ekz>
        &gcirc;i prezentis sin al li en formo de kastelo kun siaj kvar turoj
        kaj
        <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 2a, p.
        34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_n;&c_a;&c_k;&c_l;&c_mol;</trd>
  <trd lng="ca">pinacle</trd>
  <trdgrp lng="cs">
    <trd>cimbu&rcaron;&iacute;</trd>,
    <trd>pinakl</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pinakel</trd>,
    <trd>Spitzs&auml;ule</trd>,
    <trd>Zinne</trd>
  </trdgrp>
  <trd lng="es">pin&aacute;culo</trd>
  <trd lng="fr">pinacle</trd>
  <trd lng="hu">fiatorony</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x5C16;&#x5854;  [<ind>&#x3057;&#x3087;&#x3046;&#x305B;&#x3093;&#x3068;&#x3046;</ind>]</trd>,
    <trd>&#x30D4;&#x30CA;&#x30AF;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">pinakel</trd>
  <trdgrp lng="pl">
    <trd>pinakiel</trd>,
    <trd>fiala</trd>,
    <trd>sterczyna</trd>
  </trdgrp>
  <trd lng="pt">pin&aacute;culo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_n;&c_a;&c_k;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_k;&c_o;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_sh;&c_je;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">ozdoba oporn&yacute;ch obl&uacute;kov striech</trd>
  <trd lng="uk">&#x43F;&#x456;&#x43D;&#x430;&#x43A;&#x43B;&#x44C;</trd>
</drv>
</art>
