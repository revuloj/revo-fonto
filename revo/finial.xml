<kap>
<rad>finial</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="finial.0o">
  <kap><tld/>o</kap>
  <snc mrk="finial.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Florsimila <ref tip="super" cel="ornam.0o">ornamo</ref> finanta pinta&jcirc;on:
      <ekz>
        Pandiono <klr>[&#8230;]</klr> ekvidis meze de la halo marmoran kubon,
        ornamitan per bluaj <tld/>oj kaj volva&jcirc;oj el vitro
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_r;&c_o;&c_n; <klr tip="amb">(&c_a;&c_r;&c_h;.)</klr></trd>
  <trdgrp lng="cs">
    <trd>fi&aacute;la</trd>,
    <trd>kv&ecaron;t</trd>,
    <trd>ozdoba</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kreuzblume</trd>,
     <trd>Bl&auml;tterknauf</trd></trdgrp>
  <trd lng="fr">finial</trd>
  <trdgrp lng="hu">
     <trd>keresztr&oacute;zsa</trd>,
     <trd>oromd&iacute;sz</trd></trdgrp>
  <trd lng="ja">&#38914;&#33775; <pr>&#12385;&#12423;&#12358;&#12363;</pr></trd>
  <trd lng="nl">fleuron</trd>
  <trdgrp lng="pt">
    <trd>remate</trd>,
    <trd>flor&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_z;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_mol;&c_je;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">kvetov&aacute; ozdoba hrotu</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_je;&c_b;&c_ib;&c_n;&c_mol; (&c_a;&c_b;&c_o; &c_k;&c_o;&c_n;&c_mol;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_i;&c_k;) &c_sh;&c_p;&c_i;&c_l;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
