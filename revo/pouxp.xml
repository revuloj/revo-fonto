<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pouxp.xml,v 1.19 2024/02/17 07:52:35 revo Exp $">
<kap>
  <rad>po&ubreve;p</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pouxp.0o">
  <kap><tld/>o</kap>
  <snc mrk="pouxp.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="dif" cel="pob.0o.MAR">Pobo</ref>:
      <ekz>
        tiam la ondoj venis kontra&ubreve; nian <tld/>on
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la &scirc;ipo estis elstaranta super la kajo
        <fnt>
          <aut>Aleksandro Grin</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Grin/kuranta_04.html">Kuranta sur
          ondoj</url></vrk>
          <lok>IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o kaj steveno estas bruo, kvaza&ubreve; tie laboras mueliloj
        <fnt>
          <vrk><url ref="https://wikilivres.org/wiki/La_submarinistoj">La submarinistoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="sxip.0o.MAR">&scirc;ipo</ref>
    <ref tip="ant" cel="pru.0o">pruo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_a;
  <klr>(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
  <trd lng="ca">popa</trd>
  <trdgrp lng="de">
    <trd>Heck</trd>,
    <trd>Achternschiff</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stern</trd>,
    <trd>poop</trd>
  </trdgrp>
  <trd lng="es">popa</trd>
  <trd lng="fr">poupe</trd>
  <trdgrp lng="hu">
    <trd>tat</trd>,
    <trd>haj&oacute;far</trd>
  </trdgrp>
  <trd lng="nl">achtersteven</trd>
  <trd lng="pl">rufa</trd>
  <trd lng="pt">popa</trd>
  <trd lng="ro">pup&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x8233; <pr>zh&#xFA;</pr></trd>,
    <trd>&#x8244; <pr>sh&#x101;o</pr></trd>,
    <trd>&#x8239;&#x8244; <pr>chu&#xE1;nsh&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
