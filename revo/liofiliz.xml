<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: liofiliz.xml,v 1.11 2025/04/12 20:27:21 revo Exp $">
<kap>
  <rad>liofiliz</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="liofiliz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="liofiliz.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="sek.frosto0igi.TEK">Frostosekigi</ref>:
      <ekz>
        oni <tld/>as &gcirc;in, t.e. samtempe malsekigas kaj malvarmigas
        &gcirc;in je minus 60&deg;C
        <fnt>
          <aut>R. Roze</aut>,
          <vrk>Vivisekcio de polenero</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 11a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas transplanti anka&ubreve; <tld/>atan histon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=585579">Keratokonuso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la unuaj akvodifektitaj libroj estis portataj al <tld/>ejo en la Centro por
        Librokonservado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=317032">Librejo Dukino Anna
          Amalia</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_c;&c_y;&c_j;&c_n;&c_u;&c_ju;</ind> &c_s;&c_u;&c_sh;&c_k;&c_u;</trd>
  <trd lng="cs">lyofilizovat</trd>
  <trd lng="de">gefriertrocknen</trd>
  <trdgrp lng="en">
    <trd>lyophilize</trd>,
    <trd><ind>freeze</ind>-dry</trd>
  </trdgrp>
  <trd lng="fr">lyophiliser</trd>
  <trd lng="pl">liofilizowa&cacute;</trd>
  <trd lng="sk">lyofilizova&tcaron;</trd>
  <trd lng="uk">&c_o;&c_b;&c_je;&c_z;&c_v;&c_o;&c_d;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_z;&c_v;&c_je;&c_r;&c_h;&c_n;&c_i;&c_z;&c_mol;&c_k;&c_o;&c_ju;
  &c_t;&c_je;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_o;&c_ju;</trd>
</drv>
</art>

