<kap>
  <ofc>4</ofc>
  <rad>kla&ccirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klacx.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="babil.0i.babilacxi">Babili</ref> dissemante malica&jcirc;ojn a&ubreve; banala&jcirc;ojn:
      <ekz>
        oni tro <tld/>adis, ke esperanto estas &leftquot;tiel facile lernebla!&rightquot;, ke
        <klr>[&#8230;]</klr> la laboristoj tiele trompitaj forlasas la kurson je la unuaj
        malfaciloj
        <fnt>
        </fnt>;
      </ekz>
      <ekz>
        jen venis la <tld/>ado, kiu siblis tra &ccirc;iuj domoj, stratoj kaj stratetoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        estu trankvila, mi ne estas <tld/>ulo
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la supra aparta&jcirc;o estis gvidanta <tld/>istino <klr>[&#8230;]</klr>, neinteligenta
        verdeta virino, kiu anta&ubreve;flaradis &ccirc;iun malfeli&ccirc;on
        <fnt>
          <bib>RBa</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glor.mal0i">malglori</ref>,
      <ref cel="fam.mis0igi">misfamigi</ref>,
      <ref cel="nigr.0igi.FIG_malbonfamigi">nigrigi<sncref/></ref>,
      <ref cel="kredit.sen0igi.konfido">senkreditigi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_k;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd
  lng="bo">&#xF62;&#xF92;&#xFB1;&#xF56;&#xF0B;&#xF42;&#xF64;&#xF7A;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>malparlar</trd>,
    <trd>xafardejar</trd>,
    <trd>maldir</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>klatschen</trd>,
    <trd>tratschen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gossip</trd>,
    <trd>clack</trd>,
    <trd>prate</trd>,
    <trd>prattle</trd>,
    <trd> talk small talk</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>chambrear</trd>,
    <trd>chambriar</trd>,
    <trd>chismorrear</trd>,
    <trd>chismear</trd>,
    <trd>cotillear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancaner<klr tip="ind"> (m&eacute;dire)</klr></trd>,
    <trd>m&eacute;dire</trd>
  </trdgrp>
  <trd lng="hu">pletyk&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x53E3;&#12434;&#12356;&#12356;&#12405;&#12425;&#12377;  <pr>&#12354;&#x304F;&#12371;&#12358;&#12434;&#12356;&#12356;&#12405;&#12425;&#12377;</pr></trd>,
    <trd>&#38512;&#x53E3;&#12434;&#x304D;&#x304F;  <pr>&#x304B;&#12370;&#12368;&#12385;&#12434;&#x304D;&#x304F;</pr></trd>,
    <trd>&#x60AA;&#12356;&#12358;&#x308F;&#12373;&#x8A71;&#12434;&#12377;&#x308B; 
    <pr>&#x308F;&#x308B;&#12356;&#12358;&#x308F;&#12373;&#x306F;&#x306A;&#12375;&#12434;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">roddelen</trd>
  <trd lng="pl">plotkowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">dedikodu yapmak</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&c_t;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i; &#x43F;&#x43B;&c_ib;&c_t;&#x43A;&c_i;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&c_t;&c_i;/&#x43F;&#x43B;&c_je;&c_s;&#x43A;&c_a;&c_t;&c_i; &#x44F;&c_z;&c_i;&#x43A;&#x43E;&#x43C;</trd>,
    <trd>&#x43E;&c_b;&#x43C;&#x43E;&c_v;&#x43B;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8AAA;&#38263;&#36947;&#x77ED; <pr>shu&#x14D;ch&#xE1;ngd&#xE0;odu&#x1CE;n</pr></trd>,
    <trd>&#21593; <pr>w&#x1D2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klacx.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="klacx.0o.KOMUNE">
    <dif>
      Dissciigita malbonvola <ref tip="super" cel="dir.0o">dira&jcirc;o</ref>:
      <ekz>
        mi ne fidas al la urbaj <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p.
        78a</lok></fnt>;
      </ekz>
      <ekz>
        la konversacio ne estis unuflanka, la erotika <tld/>o kun apartena komento havis
        sian lokon
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie babila&jcirc;o kaj <tld/>a&jcirc;o!
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o kaj &ccirc;ikana&jcirc;o, netoleremaj kaj persiste subtenataj asertoj, kiuj ne
        valoris e&ccirc; putran haringon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fam.fi0a">fifamo</ref>,
       <ref cel="dir.oni0o">onidiro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>maldien&ccedil;a</trd>,
    <trd>xafardeig</trd>,
    <trd>comareig</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geschw&auml;tz</trd>,
    <trd>Klatsch</trd>,
    <trd>Tratsch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>babble</trd>,
    <trd>chitchat</trd>,
    <trd>gossip</trd>,
    <trd>prattle</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>chisme</trd>,
    <trd>chambre</trd>,
    <trd>cotilleo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancan</trd>,
    <trd>comm&eacute;rage</trd>,
    <trd>m&eacute;disance</trd>,
    <trd>potin</trd>,
    <trd>racontar</trd>,
    <trd>ragot</trd>
  </trdgrp>
  <trd lng="hu">pletyka</trd>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x53E3;  <pr>&#x308F;&#x308B;&#12368;&#12385;</pr></trd>,
    <trd>&#38512;&#x53E3;  <pr>&#x304B;&#12370;&#12368;&#12385;</pr></trd>,
    <trd>&#x30B4;&#x30B7;&#x30C3;&#x30D7;</trd>
  </trdgrp>
  <trd lng="nl">roddelpraat</trd>
  <trd lng="pl">plotka</trd>
  <trdgrp lng="ro">
    <trd>b&acirc;rf&abreve;</trd>,
    <trd>zvon</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_ja;</trd>
  <trd lng="tr">dedikodu</trd>
  <trd lng="uk">&#x43F;&#x43B;&c_ib;&c_t;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D41;&#x8A00; <pr>li&#xFA;y&#xE1;n</pr></trd>,
    <trd>&#x6D41;&#x8A00;&#x871A;&#x8A9E; <pr>li&#xFA;y&#xE1;nf&emacron;iy&#x1D4;</pr></trd>,
    <trd>&#x80CC;&#x540E;&#x8BAE;&#x8BBA; <pr>b&#xE8;ih&#xF2;uy&#xEC;l&#xF9;n</pr></trd>,
    <trd>&#x4EB2;&#22068;&#x58F0; <pr>q&#x12B;nzu&#x1D0;sh&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klacx.0ema">
  <kap><tld/>ema, <var><kap><tld/>ama</kap></var></kap>
  <snc mrk="klacx.0ema.KOMUNE">
    <dif>
      Ema al malica parolado pri homoj:
      <ekz>
        la&ubreve;taj kaj multvortaj, babilemaj kaj <tld/>emaj
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> gardu sin de perfido, hipokriteco kaj <tld/>amo
        <fnt>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>emuloj gustumos kiel franda&jcirc;on la elementojn de kalumnio, kiujn vi al
        ili prezentos
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="babil.0ema">babilema</ref>,
       <ref cel="diskre.mal0a">maldiskreta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_z;&c_y;&c_ch;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_t;&c_k;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">tratschig</trd>
  <trdgrp lng="es">
    <trd>cotilla</trd>,
    <trd>chismoso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancanier</trd>,
    <trd>potinier</trd>
  </trdgrp>
  <trd lng="hu">pletyk&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#38512;&#x53E3;&#x306E;&#x597D;&#x304D;&#x306A; 
    <pr>&#x304B;&#12370;&#12368;&#12385;&#x306E;&#12377;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x30B4;&#x30B7;&#x30C3;&#x30D7;&#x597D;&#x304D;&#x306E; 
    <pr>&#12372;&#12375;&#12387;&#12407;&#12377;&#x304D;&#x306E;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klacx.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="klacx.pri0i.KOMUNE">
    <dif>
      Malice <ref tip="super" cel="parol.pri0i">priparoli</ref>:
      <ekz>
        &scirc;i ne plu povos viziti la pre&gcirc;domon, kaj <klr>[&#8230;]</klr> estos tute pri<tld/>ata
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kuris al la &ccirc;ambro de iu kunlaboranto kaj detale kaj malame pri<tld/>is la
        re&gcirc;isoron, eksedzon
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>;
      </ekz>
      <ekz>
        certe la bienuloj a&ubreve;dis pri tio, kio estis
        pri<tld/>ata
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_k;&c_a;&c_r;&c_y;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trd lng="de">sich &uuml;ber etwas <ind>auslassen</ind></trd>
  <trdgrp lng="es">
    <trd>cotillear de</trd>,
    <trd>chismorrear sobre</trd>
  </trdgrp>
  <trd lng="fr">jaser sur</trd>
  <trdgrp lng="zh">
    <trd>&#x558B;&#x558B;&#x4E0D;&#x4F11; <pr>di&#xE9;di&#xE9;b&#xF9;xi&#x16B;</pr></trd>,
    <trd>&#x8AAA;&#38263;&#36947;&#x77ED; <pr>shu&#x14D;ch&#xE1;ngd&#xE0;odu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klacx.kafo0o">
  <kap>
    kafo<tld/>o
  </kap>
  <snc mrk="klacx.kafo0o.kunveneto">
    <uzo tip="fak">POL</uzo><uzo tip="fak">ESP</uzo>
    <dif>
      <ref tip="super" cel="ven.kun0o">Kunveneto</ref> sen aparta programo krom
      kafotrinkado kaj babilado:
      <ekz>
        en semajnfina kafo<tld/>o de Esperanto-Junularo <nom>Frankfurt</nom>, mi trovas bonegan
        okazon apliki mian relernitan kapablon
        <fnt>
          <bib>FSI</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve;tagmeze <klr>[&#8230;]</klr> modera sunbruni&gcirc;ado (anka&ubreve; en la proksima naturista
        strando), sa&ubreve;nado, amika kafo<tld/>o
        <fnt>
          <bib>LOdE</bib>
          <aut>Ljubomir Trifon&ccirc;ovski</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">&Ccirc;ebalta Esperantista
          Printempo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mangx.inter0eto">interman&gcirc;eto</ref>,
      <ref cel="konver2.0o">konversacio</ref>,
      <ref cel="mangx.te0o">teman&gcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Kaffeeklatsch</trd>,
    <trd>Kaffeekr&auml;nzchen</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_s;&c_i;&c_d;&c_je;&#x43D;&#x44C;&#x43A;&c_i; &c_z;&c_a; &#x43A;&c_a;&c_v;&#x43E;&#x44E;</trd>,
    <trd>&#x43F;&#x43B;&c_ib;&c_t;&#x43A;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F; &c_z;&c_a; &#x43A;&c_a;&c_v;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

