<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kla&ccirc;</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="klacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Babili dissemante malica&jcirc;ojn: 
      <ekz><tld/>i kontra&ubreve; iu;</ekz>
      <ekz>&scirc;i estas terure <tld/>ema.</ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>gossip</trd>,
    <trd>clack</trd>,
    <trd>prate</trd>,
    <trd>prattle</trd>,
    <trd> talk small talk</trd>
  </trdgrp>
  <trd lng="nl">roddelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="klacx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dissciigita malbonvola dira&jcirc;o:
      <ekz>mi ne konfidas al la urbaj <tld/>oj<fnt>B</fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>babble</trd>,
    <trd>chitchat</trd>,
    <trd>gossip</trd>,
    <trd>prattle</trd>
  </trdgrp>
  <trd lng="nl">roddelpraat</trd>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_ja;</trd>
</drv>
</art>
