<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kla&ccirc;</rad>/i <fnt>Z</fnt>
</kap>
<drv mrk="klacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Babili dissemante malica&jcirc;ojn:
      <ekz>
        <tld/>i kontra&ubreve; iu;
      </ekz>
      <ekz>
        &scirc;i estas terure <ind><tld/>ema</ind>.
        <trd lng="hu">pletyk&aacute;s</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_k;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>klatschen</trd>,
    <trd>tratschen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gossip</trd>,
    <trd>clack</trd>,
    <trd>prate</trd>,
    <trd>prattle</trd>,
    <trd> talk small talk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancaner<klr tip="ind"> (m&eacute;dire)</klr></trd>,
    <trd>m&eacute;dire</trd>
  </trdgrp>
  <trd lng="hu">pletyk&aacute;l</trd>
  <trd lng="nl">roddelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="klacx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dissciigita malbonvola dira&jcirc;o:
      <ekz>
        mi ne fidas al la urbaj <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p. 78a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Klatsch</trd>,
    <trd>Tratsch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>babble</trd>,
    <trd>chitchat</trd>,
    <trd>gossip</trd>,
    <trd>prattle</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancan</trd>,
    <trd>comm&eacute;rage</trd>,
    <trd>m&eacute;disance</trd>,
    <trd>potin</trd>,
    <trd>racontar</trd>,
    <trd>ragot</trd>
  </trdgrp>
  <trd lng="hu">pletyka</trd>
  <trd lng="nl">roddelpraat</trd>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_ja;</trd>
</drv>
</art>
