<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klacx.xml,v 1.21 2019/09/08 18:10:16 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>kla&ccirc;</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klacx.0i.KOMUNE">
    <dif>
      Babili dissemante malica&jcirc;ojn:
      <ekz>
        <tld/>i kontra&ubreve; iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_k;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>malparlar</trd>,
    <trd>xafardejar</trd>,
    <trd>maldir</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>klatschen</trd>,
    <trd>tratschen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gossip</trd>,
    <trd>clack</trd>,
    <trd>prate</trd>,
    <trd>prattle</trd>,
    <trd> talk small talk</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>chambrear</trd>,
    <trd>chambriar</trd>,
    <trd>chismorrear</trd>,
    <trd>chismear</trd>,
    <trd>cotillear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancaner<klr tip="ind"> (m&eacute;dire)</klr></trd>,
    <trd>m&eacute;dire</trd>
  </trdgrp>
  <trd lng="hu">pletyk&aacute;l</trd>
  <trd lng="nl">roddelen</trd>
  <trd lng="pl">plotkowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">dedikodu yapmak</trd>
</drv>

<drv mrk="klacx.0o">
  <kap><tld/>o</kap>
  <snc mrk="klacx.0o.KOMUNE">
    <dif>
      Dissciigita malbonvola dira&jcirc;o:
      <ekz>
        mi ne fidas al la urbaj <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p.
        78a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>maldien&ccedil;a</trd>,
    <trd>xafardeig</trd>,
    <trd>comareig</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Klatsch</trd>,
    <trd>Tratsch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>babble</trd>,
    <trd>chitchat</trd>,
    <trd>gossip</trd>,
    <trd>prattle</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>chisme</trd>,
    <trd>chambre</trd>,
    <trd>cotilleo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancan</trd>,
    <trd>comm&eacute;rage</trd>,
    <trd>m&eacute;disance</trd>,
    <trd>potin</trd>,
    <trd>racontar</trd>,
    <trd>ragot</trd>
  </trdgrp>
  <trd lng="hu">pletyka</trd>
  <trd lng="nl">roddelpraat</trd>
  <trd lng="pl">plotka</trd>
  <trdgrp lng="ro">
    <trd>b&acirc;rf&abreve;</trd>,
    <trd>zvon</trd>
  </trdgrp> 
  <trd lng="ru">&c_s;&c_p;&c_l;&c_je;&c_t;&c_n;&c_ja;</trd>
  <trd lng="tr">dedikodu</trd>
</drv>

<drv mrk="klacx.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="klacx.0ema.KOMUNE">
    <dif>
      Ema al malica parolado pri homoj:
      <ekz>
        &scirc;i estas terure <tld/>ema.       
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>cotilla</trd>,
    <trd>chismoso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cancanier</trd>,
    <trd>potinier</trd>
  </trdgrp>
  <trd lng="hu">pletyk&aacute;s</trd>
</drv>

<drv mrk="klacx.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="klacx.pri0i.KOMUNE">
    <dif>
      Malice priparoli:
      <ekz>
        certe la bienuloj a&ubreve;dis pri tio, kio estis 
        pri<tld/>ata
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>cotillear de</trd>,
    <trd>chismorrear sobre</trd>
  </trdgrp>
  <trd lng="fr">jaser sur</trd>
</drv>
</art>
