<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: puebl.xml,v 1.11 2015/07/04 08:37:42 revo Exp $">
<kap><rad>puebl</rad>/o</kap>
<drv mrk="puebl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref> el vila&gcirc;-lo&gcirc;a
      popolo vivanta per &ccirc;aso, kultivo de maizo, fazeolo, kotono
      en Novmeksiko kaj Arizono:
      <ekz>
        <tld/>oj [...] ricevis sian nomon pro la konstruado de specialaj
        vila&gcirc;oj (hispane <ctl>pueblos</ctl>)<fnt><aut>Tazio
        Carlevaro</aut>: <vrk>Skize pri indianaj kulturoj</vrk>,
        <lok>Literatura foiro, numero 115a, 1998-10, p.
        32-37a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">indi pueblo</trd>
  <trd lng="cs">pueblo</trd>
  <trd lng="de">Puebloindianer</trd>
  <trd lng="en">Pueblo</trd>
  <trdgrp lng="fr">
    <trd>Pueblo<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">puebl&oacute;indi&aacute;n</trd>
  <trd lng="nl">Pueblo-indiaan</trd>
  <trd lng="pt">pueblo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_e;&c_b;&c_l;&c_o;<klr tip="amb"> (&c_i;&c_n;&c_d;&c_je;&c_je;&c_c;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pueblo</trd>
</drv>

<drv mrk="puebl.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">puebloindianisch</trd>
  <trd lng="en">pueblo</trd>
  <trdgrp lng="fr">
    <trd>pueblo<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">puebl&oacute;indi&aacute;n</trd>
  <trd lng="nl">Pueblo-indiaans</trd>
  <trd lng="pt">do <ind>pueblo</ind></trd>
</drv>
</art>
