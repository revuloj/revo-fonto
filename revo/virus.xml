<drv mrk="virus.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="virus.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      <ref tip="super" cel="mikrorganism.0o">Mikroorganismo</ref>
      ka&ubreve;zanta infektajn malsanojn &ccirc;e animaloj, plantoj kaj bakterioj,
      ne povanta ekzisti sen gastiganta &ccirc;elo kaj ne havanta propran
      metabolon:
      <ekz>
        la baza vojo de infektado de narkota&jcirc;emuloj estas la vasta
        aplikado de injektilo kune uzata kun personoj infektitaj de la
        <tld/>o de aidoso
        <fnt>
          <aut>V. Lemelev</aut>,
          <vrk><url ref="&Monato;/2001/pdf/monato200105.pdf#page=14">Novaj strategioj en
          Rusio</url></vrk>,
          <bib>Monato</bib>,
          <lok>2001/05, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <tld/>oj uzas ribonukleatan acidon por konservi la
        genetikan informon
        <fnt>
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>;
      </ekz>
      <ekz>
        sur la supra&jcirc;o de la <tld/>oj estas tubereca kovrilo, simila al
        krono a&ubreve; korono
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php">De kie
          venis kaj kial &ccirc;esis netipa pne&ubreve;monio?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malsanulino ricevis kontra&ubreve;<tld/>ajn medikamentojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; la plej sa&gcirc;aj kaj ekvilibraj homoj ne sukcesas eskapi la
        <tld/>on de naciismo
        <fnt>
	<aut>Uwe Joachim Moritz/Aldo de' Giorgi</aut>
	<vrk><url ref="&Monato;/2000/007043.php">Du opinioj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/07, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="he">&nun;&gimel;&yod;&fpe;</trd>
    <trd lng="th">&#xE44;&#xE27;&#xE23;&#xE31;&#xE2A;</trd>
  </snc>
  <snc mrk="virus.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Komputila <ref tip="super" cel="progra.0o.komp">programo</ref>,
      kiu kapablas krei (ne necese tute indentajn) kopiojn de si mem kaj
      enigas tiujn en komputilajn dosierojn, sistemojn a&ubreve; aliajn
      programojn; la kopio plej ofte mem havas la kapablon krei pliajn
      kopiojn:
      <ekz>
        mian personan komputilon neniam trafis <tld/>o, kvankam mi
        sendis kaj ricevis centojn da retpo&scirc;taj leteroj, bultenoj kaj
        cirkuleroj
        <fnt>
          <aut>Claude Rouget</aut>,
          <vrk><url ref="&Monato;/1997/005659.html">Internet ne (tiom)
          malbonas</url></vrk>,
          <bib>Monato</bib>,
          <lok>1997/08, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la plej venenaj <tld/>oj, kiujn mi ricevis, venis ene de
        listo de membroj
        de UEA
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008036.html">Damnita
          komputilo! Diabla Interreto!</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="he">&vav;&yod;&resh;&vav;&samekh;</trd>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_r;&c_u;&c_s;</trd>
  <trd lng="bg">&c_v;&c_i;&c_r;&c_u;&c_s;</trd>
  <trd lng="br">viruz</trd>
  <trd lng="ca">virus</trd>
  <trd lng="cs">virus</trd>
  <trd lng="da">virus</trd>
  <trd lng="de">Virus</trd>
  <trd lng="en">virus</trd>
  <trd lng="es">virus</trd>
  <trdgrp lng="el">
    <trd>&jota_ton;&omikron;&sigma_fina;</trd>,
    <trd>&my;&jota_ton;&alfa;&sigma;&my;&alfa;</trd>
  </trdgrp>
  <trd lng="eu">birus</trd>
  <trd lng="fa">&a_w;&f_y;&a_r;&a_w;&a_s;</trd>
  <trd lng="fi">virus</trd>
  <trd lng="fr">virus</trd>
  <trd lng="fy">firus</trd>
  <trd lng="gl">virus</trd>
  <trd lng="hu">v&iacute;rus</trd>
  <trd lng="id">virus</trd>
  <trd lng="io">viruso</trd>
  <trd lng="it">virus</trd>
  <trdgrp lng="ja">
    <trd>&#x30A6;&#x30A4;&#x30EB;&#x30B9;  <pr>&#x3046;&#x3044;&#x308B;&#x3059;</pr></trd>,
    <trd>&#x30D3;&#x30FC;&#x30EB;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">virus</trd>
  <trd lng="no">virus</trd>
  <trd lng="oc">virus</trd>
  <trd lng="pl">wirus</trd>
  <trd lng="pt">v&iacute;rus</trd>
  <trd lng="ro">virus</trd>
  <trd lng="ru">&c_v;&c_i;&c_r;&c_u;&c_s;</trd>
  <trd lng="sk">v&iacute;rus</trd>
  <trd lng="sv">virus</trd>
  <trd lng="uk">&c_v;&c_ib;&c_r;&c_u;&c_s;</trd>
  <trd lng="zh">&#x75C5;&#x6BD2;</trd>
</drv>

<drv mrk="virus.kontraux0ilo">
  <kap>kontra&ubreve;<tld/>ilo</kap>
  <snc mrk="virus.kontraux0ilo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Komputila <ref tip="super" cel="progra.0o.komp">programo</ref> kiu
      kontra&ubreve;as <tld/>ojn en komputilaj dosieroj, sistemoj a&ubreve; aliaj
      programoj:
      <ekz>
        la kontra&ubreve;<tld/>ilo de <nom>Norton</nom> detruis kelkajn malpli
        gravajn <tld/>ojn kaj
        riparis du-tri difektitajn dosierojn: sur la plej gravajn &gcirc;i ne
        efikis
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008036.html">Damnita
          komputilo! Diabla Interreto!</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>European Bernt Fix</nom> (a&ubreve; <nom>Bernd</nom>) estas la
        unua kontra&ubreve;<tld/>ilo en 1987
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Kontra%C5%ADvirusilo">Kontra&ubreve;virusilo</url>,
          2014-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE42;&#xE1B;&#xE23;&#xE41;&#xE01;&#xE23;&#xE21;&#xE1B;&#xE49;&#xE2D;&#xE07;&#xE01;&#xE31;&#xE19;&#xE44;&#xE27;&#xE23;&#xE31;&#xE2A;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_y;&c_v;&c_ib;&c_r;&c_u;&c_s;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_y;&c_v;&c_ib;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_a; &c_o;&c_t; <klr tip="ind">&c_v;&c_i;&c_r;&c_u;&c_s;&c_i;</klr></trd>,
    <trd>&c_v;&c_i;&c_r;&c_u;&c_s;&c_je;&c_n; &c_s;&c_k;&c_je;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Virenschutz</trd>
  <trd lng="en">antivirus</trd>
  <trd lng="fr">antivirus</trd>
  <trdgrp lng="es">
    <trd>antivirus</trd>,
    <trd>aplicaci&oacute;n antivirus</trd>
  </trdgrp>
  <trd lng="id">antivirus</trd>
  <trd lng="it">antivirus</trd>
  <trd lng="nl">antivirus-programma</trd>
  <trdgrp lng="zh">
    <trd>&#x6740;&#x6BD2;&#x8F6F;&#x4EF6; <pr>sh&#x101;d&#xFA;ru&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x6BBA;&#x6BD2;&#x8EDF;&#x4EF6; <pr>sh&#x101;d&#xFA;ru&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x6740;&#x6BD2; <pr>sh&#x101;d&#xFA;</pr></trd>,
    <trd>&#x6BBA;&#x6BD2; <pr>sh&#x101;d&#xFA;</pr></trd>,
    <trd>&#x9632;&#x75C5;&#x6BD2; <pr>f&#xE1;ngb&#xEC;ngd&#xFA;</pr></trd>,
    <trd>&#x53CD;&#x75C5;&#x6BD2; <pr>f&#x1CE;nb&#xEC;ngd&#xFA;</pr></trd>,
    <trd>&#x6740;&#x8F6F; <pr>sh&#x101;ru&#x1CE;n</pr></trd>,
    <trd>&#x6BBA;&#x8EDF; <pr>sh&#x101;ru&#x1CE;n</pr></trd>,
    <trd>&#x9632;&#x6BD2;&#x8EDF;&#x4EF6; <pr>f&#xE1;ngd&#xFA;ru&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x9632;&#x6BD2;&#x8F6F;&#x4EF6; <pr>f&#xE1;ngd&#xFA;ru&#x1CE;nji&#xE0;n</pr></trd>
  </trdgrp>
