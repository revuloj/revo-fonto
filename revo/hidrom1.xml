<vortaro>
<art mrk="$Id: hidrom1.xml,v 1.14 2019/03/15 21:10:14 revo Exp $">
<kap>
  <rad>hidrometri</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="hidrom1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      pri la mezurado de akva&jcirc;oj:
      <ekz>
        realtempaj <tld/>aj datumoj de la akvokolekta areo Rodano-Mediteraneo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=513884"
              >Var (rivero)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Wassermesskunde</trd>,
    <trd>Hydrometrie</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&my;&epsilon;&tau;&rho;&jota_ton;&alfa;</trd>
  <trd lng="es">hidrometr&iacute;a</trd>
  <trd lng="fa">&a_A_madda;&a_b;&zwnj;&a_s;&a_n;&a_j;&f_y;</trd>
  <trd lng="fr">hydrom&eacute;trie</trd>
  <trdgrp lng="hu">
    <trd>hidrometria</trd>,
    <trd>v&iacute;zm&eacute;r&eacute;stan</trd>
  </trdgrp>
  <trd lng="it">idrometria</trd>
  <trdgrp lng="nl">
    <trd>watermeetkunde</trd>,
    <trd>hydrometrie</trd>
  </trdgrp>
  <trd lng="pt">hidrometria</trd>
</drv>
</art>
