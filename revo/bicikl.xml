<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bicikl.xml,v 1.38 2023/10/29 19:11:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bicikl</rad>/o
</kap>

<drv mrk="bicikl.0o">
  <kap><tld/>o</kap>
  <snc mrk="bicikl.0o.velocipedo">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="lst" lst="voko:veturiloj" cel="veloci.0o">Velocipedo</ref> kun du
      egalgrandaj radoj:
      <ekz>
        oni min venigis por kuraci junulon, kiu, falinte el <tld/>eto, grave vundis sian
        genuon
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel longa rubando kuras <tld/>oj, per sonorego plenigante la aeron
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj estas la solaj vere hom- kaj medi-konvenaj veturiloj
        <fnt>
          <aut>Maul Stefan</aut>
            <vrk><url ref="&Monato;/2000/007121.php">Malbona venko</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 30</lok>
          </fnt>;
      </ekz>
      <ekz>
        senkostaj publikaj <tld/>oj jam sukcese uzeblas <klr>[&#x2026;]</klr> ekzemple
        en la &ccirc;efurbo de Finnlando, Helsinko
        <fnt>
        <aut>R. Pyh&auml;l&auml;</aut>
        <vrk>&leftquot;Stelo&rightquot; &ccirc;e la urbodomo</vrk>
        <bib>Monato</bib>
        <lok>2002:7, p. 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas &jcirc;ongli, veturi sur unurada <tld/>o kaj regi aliajn plezurigajn arta&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Cezar</aut>
          <vrk><url ref="&Monato;/2003/008090.html">Amuzi atendantojn</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="tricik.0o">triciklo</ref>,
       <ref cel="tandem.0o.BIC">tandemo<sncref/></ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x81EA;&#x884C;&#x8ECA; <pr>z&#xEC;x&#xED;ngch&#x113;</pr></trd>,
    <trd>&#x811A;&#x8E0F;&#x8F66; <pr>ji&#x1CE;ot&#xE0;ch&#x113;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_l;&c_a;&c_s;&c_ib;&c_p;&c_je;&c_d;</trd>,
    <trd>&c_r;&c_o;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_l;&c_o;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_kata;&t_nga;&t_tsheg;&t_sa;&t_gata;&t_tsheg;&t_ra;&t_gigu;&t_la;&t_tsheg;</trd>
  <trd lng="ca">bicicleta</trd>
  <trdgrp lng="cs">
    <trd>bicykl</trd>,
    <trd>j&iacute;zdn&iacute; kolo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fahrrad</trd>,
    <trd>Rad <klr>(Fahrrad)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bicycle</trd>,
    <trd>bike</trd>
  </trdgrp>
  <trd lng="es">bicicleta</trd>
  <trdgrp lng="fr">
    <trd>bicyclette</trd>,
    <trd>v&eacute;lo</trd>
  </trdgrp>
  <trd lng="he">&alef;&vav;&pe;&nun;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>ker&eacute;kp&aacute;r</trd>,
    <trd>bicikli</trd>
  </trdgrp>
  <trd lng="id">sepeda</trd>
  <trd lng="it">bicicletta</trd>
  <trd lng="ja">&#x81EA;&#x8EE2;&#x8ECA;  <pr>&#x3058;&#x3066;&#x3093;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">fiets</trd>
  <trd lng="oc">bicicleta</trd>
  <trdgrp lng="pl">
    <trd>rower</trd>,
    <trd>bicykl<klr> (przest. &zdot;art.)</klr></trd>
  </trdgrp>
  <trd lng="pt">bicicleta</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;
  <klr>(&c_d;&c_v;&c_u;&c_h;&c_k;&c_o;&c_l;&c_jo;&c_s;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">bicykel</trd>
  <trd lng="sv">cykel</trd>
  <trd lng="tr">bisiklet</trd>
  <trd lng="uk">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;&nbsp;(&c_d;&c_v;&c_o;&c_k;&c_o;&c_l;&c_ib;&c_s;&c_n;&c_i;&c_j;)</trd>
</drv>

<drv mrk="bicikl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bicikl.0i.BIC">
    <dif>
      <ref tip="super" cel="vetur.0i.TRA">Veturi<sncref/></ref> per <tld/>o:
      <ekz>
        la vojo al lia laborloko ne estis tro longa por <tld/>i matene kaj vespere
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        li foje <klr>[&#x2026;]</klr> <tld/>is al apuda vila&gcirc;o por spekti futbalan
        mat&ccirc;on
        <fnt><aut>S. &Scirc;timec</aut> <vrk>Morto en Melburna
        tramo</vrk> <bib>Monato</bib> <lok>1997:8, p.
        24a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x9A0E;&#x81EA;&#x884C;&#x8ECA; <pr>q&#xED;z&#xEC;x&#xED;ngch&#x113;</pr></trd>,
    <trd>&#x9A0E;&#x8ECA; <pr>q&#xED;ch&#x113;</pr></trd>,
    <trd>&#x9A0E; <pr>q&#xED;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_je;&c_h;&c_a;&c_c;&c_mol; &c_n;&c_a;
    <ind>&c_v;&c_je;&c_l;&c_a;&c_s;&c_ib;&c_p;&c_je;&c_d;&c_z;&c_je;</ind></trd>,
    <trd>&c_je;&c_h;&c_a;&c_c;&c_mol; &c_n;&c_a;
    <ind>&c_r;&c_o;&c_v;&c_a;&c_r;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jet na kole</trd>,
    <trd>jezdit na kole</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fahrrad <ind>fahren</ind></trd>,
    <trd>radeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><klr>(travel on a) </klr>bicycle</trd>,
    <trd>bike</trd>
  </trdgrp>
  <trd lng="es">andar en <ind>bicicleta</ind></trd>
  <trd lng="fr">faire du <ind>v&eacute;lo</ind></trd>
  <trd lng="he">&lamed;&resh;&kaf;&vav;&bet; &ayin;&lamed; &alef;&vav;&pe;&nun;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>ker&eacute;kp&aacute;rozik</trd>,
    <trd>biciklizik</trd>
  </trdgrp>
  <trd lng="id">ber<ind>sepeda</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#x8EE2;&#x8ECA;&#x3067;&#x884C;&#x304F;  <pr>&#x3058;&#x3066;&#x3093;&#x3057;&#x3083;&#x3067;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x30B5;&#x30A4;&#x30AF;&#x30EA;&#x30F3;&#x30B0;&#x3059;&#x308B;  <pr>&#x3055;&#x3044;&#x304F;&#x308A;&#x3093;&#x3050;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>je&zacute;dzi&cacute; <ind>rowerem</ind></trd>,
    <trd>je&zacute;dzi&cacute; na <ind>rowerze</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_je;&c_h;&c_a;&c_t;&c_mol; &c_n;&c_a;
  &c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>bicyklova&tcaron; (sa)</trd>,
    <trd>bicyklova&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">cykla</trd>
</drv>

<drv mrk="bicikl.elektro0o">
  <kap>
    elektro<tld/>o,
    <var><kap>elektra <tld/>o</kap></var>
  </kap>
  <snc mrk="bicikl.elektro0o.BIC">
    <dif>
      <tld lit="B"/>o pelata per elektra motoro:
      <ekz>
        Svisaj Federaciaj Fervojoj (SBB) <klr>[&#x2026;]</klr> enkondukis novan abonon
        <klr>[&#x2026;]</klr> en &gcirc;i estas entenata anka&ubreve; jara ludono de elektro<tld/>o
        <fnt>
          <aut>J. Tom&iacute;&scaron;ek</aut>
          <vrk><url ref="http://esperanto-aeh.eu/wp-content/uploads/Informilo_1_2018.pdf">Neniu volas voja&gcirc;i nur per la fervojo</url></vrk>
          <lok>Esperanto-informilo, 2018-01</lok>
        </fnt>;
      </ekz>
      <ekz>
        mian unuan elektran <tld/>on mi a&ccirc;etis en 2015
        <fnt>
          <aut>E. Van den Bosch</aut>
          <vrk><url ref="https://www.youtube.com/watch?v=3I9RBxHckEY">Elektraj Bicikloj (prelego)</url></vrk>
          <lok>Youtube, 2020-09-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_r;&c_o;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_m;&c_a;&c_p;&c_e;&c_d;</trd>
  </trdgrp>
  <trd lng="fr">v&eacute;lo &eacute;lectrique</trd>
</drv>

<drv mrk="bicikl.motor0o">
  <kap>
    motor<tld/>o, <var><kap>zum<tld/>o</kap></var>
  </kap>
  <snc mrk="bicikl.motor0o.BIC">
    <dif>
      Biciklo provizita per malforta motoro, <ref tip="sin" cel="moped.0o">mopedo</ref>:
      <ekz>
        printempe ni espereble sukcesos a&ccirc;eti motor<tld/>on
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili havis zum<tld/>ojn kaj ilia viza&gcirc;o estis ka&scirc;ita per motorciklista kasko kaj
        koltuko metita &gcirc;is la okuloj.
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="motoci.0o">motociklo</ref>,
      <ref cel="skoter.0o">skotero</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x8F15;&#x4FBF;&#x6469;&#x6258;&#x8ECA; <pr>q&#x12B;ngbi&#xE0;nm&#xF3;tu&#x14D;ch&#x113;</pr></trd>,
    <trd>&#x8F15;&#x578B;&#x6469;&#x6258;&#x8ECA; <pr>q&#x12B;ngx&#xED;ngm&#xF3;tu&#x14D;ch&#x113;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_p;&c_e;&c_d;</trd>,
    <trd>&c_m;&c_o;&c_f;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Moped</trd>,
    <trd>Mofa</trd>
  </trdgrp>
  <trd lng="fr">v&eacute;lomoteur</trd>
  <trdgrp lng="ja">
    <trd>&#x539F;&#x52D5;&#x6A5F;&#x4ED8;&#x304D;&#x81EA;&#x8EE2;&#x8ECA;  <pr>&#x3052;&#x3093;&#x3069;&#x3046;&#x304D;&#x3064;&#x304D;&#x3058;&#x3067;&#x3093;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x30E2;&#x30FC;&#x30BF;&#x30FC;&#x30B5;&#x30A4;&#x30AF;&#x30EB;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_p;&c_je;&c_d;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_o;&c_r;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
