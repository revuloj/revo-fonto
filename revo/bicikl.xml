<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bicikl</rad>/o
</kap>
<drv mrk="bicikl.0o">
  <kap><tld/>o</kap>
  <snc>
   <dif>
    <ref tip="super" cel="veloci.0o">Velocipedo</ref> kun du egalgrandaj
    radoj.
   </dif>
  </snc>
  <trd lng="de">Fahrrad</trd>
  <trd lng="en">bicycle</trd>
  <trdgrp lng="fr">
    <trd>bicyclette</trd>,
    <trd>v&eacute;lo</trd></trdgrp>
  <trd lng="nl">fiets</trd>
  <trd lng="pl">rower</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d; 
<klr>(&c_d;&c_v;&c_u;&c_h;&c_k;&c_o;&c_l;&#1105;&c_s;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="tr">bisiklet</trd>
</drv>

<drv mrk="bicikl.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Veturi per <tld/>o.
     </dif>
   </snc>
   <trd lng="fr">faire du <ind>v&eacute;lo</ind></trd>
</drv>

</art>
