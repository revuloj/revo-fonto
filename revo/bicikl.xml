<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bicikl</rad>/o
</kap>

<drv mrk="bicikl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="veloci.0o">Velocipedo</ref> kun du egalgrandaj
      radoj:
      <ekz>
        senkostaj publikaj <tld/>oj jam sukcese uzeblas [...] ekzemple en
        la &ccirc;efurbo de Finnlando, Helsinko<fnt><aut>Raita Pyh&auml;l&auml;</aut>:
        <vrk>&leftquot;Stelo&rightquot; &ccirc;e la urbodomo</vrk>, <bib>Monato</bib><lok>jaro
        2002a, numero 7a, p. 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_l;&c_a;&c_s;&c_ib;&c_p;&c_je;&c_d;</trd>,
    <trd>&c_r;&c_o;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_l;&c_o;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Fahrrad</trd>
  <trdgrp lng="en">
    <trd>bicycle</trd>,
    <trd>bike</trd>
  </trdgrp>
  <trd lng="es">bicicleta</trd>
  <trdgrp lng="fr">
    <trd>bicyclette</trd>,
    <trd>v&eacute;lo</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ker&eacute;kp&aacute;r</trd>,
    <trd>bicikli</trd>
  </trdgrp>
  <trd lng="nl">fiets</trd>
  <trd lng="pl">rower</trd>
  <trd lng="pt">bicicleta</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;
<klr>(&c_d;&c_v;&c_u;&c_h;&c_k;&c_o;&c_l;&c_jo;&c_s;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">bicykel</trd>
  <trd lng="sv">cykel</trd>
  <trd lng="tr">bisiklet</trd>
</drv>

<drv mrk="bicikl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Veturi per <tld/>o:
      <ekz>
        li foje [..] <tld/>is al apuda vila&gcirc;o por spekti futbalan
        mat&ccirc;on<fnt><aut>Spomenka &Scirc;timec</aut>: <vrk>Morto en Melburna
        tramo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 8a, p.
        24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_je;&c_h;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_v;&c_je;&c_l;&c_a;&c_s;&c_ib;&c_p;&c_je;&c_d;&c_z;&c_je;</ind></trd>,
    <trd>&c_je;&c_h;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_r;&c_o;&c_v;&c_a;&c_r;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="de">Fahrrad fahren</trd>
  <trd lng="es">andar en <ind>bicicleta</ind></trd>
  <trdgrp lng="en">
    <trd><klr>(travel on a) </klr>bicycle</trd>,
    <trd>bike</trd>
  </trdgrp>
  <trd lng="fr">faire du <ind>v&eacute;lo</ind></trd>
  <trdgrp lng="hu">
    <trd>ker&eacute;kp&aacute;rozik</trd>,
    <trd>biciklizik</trd>
  </trdgrp>
  <trd lng="ru">&c_je;&c_h;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;&c_je;</trd>
  <trd lng="sk">bicyklova&tcaron; (sa)</trd>
  <trd lng="sv">cykla</trd>
</drv>
</art>
