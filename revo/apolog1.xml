<kap>
  <rad>apologi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="apolog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parola a&ubreve; skriba defendo kaj pravigo de io a&ubreve; iu, solene
      farita:
      <ekz>
        fari <tld/>on de sia politiko;
      </ekz>
      <ekz>
        la <tld/>o de Sokrato.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pled">pledado</ref>,
      <ref cel="laux">la&ubreve;do</ref>,
      <ref cel="kulp">senkulpigo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">apologia</trd>
  <trd lng="he">&#x5D4;&#x5B4;&#x5EA;&#x5E0;&#x5B7;&#x5E6;&#x5B0;&#x5DC;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trd lng="ms">permohonan maaf</trd>
  <trdgrp lng="cs">
    <trd>obhajoba</trd>,
    <trd>obrana</trd>,
    <trd>obrann&aacute; &rcaron;e&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Apologie</trd>,
     <trd>Verteidigungsrede</trd>,
     <trd>Verteidigungsschrift</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apology</trd>
  </trdgrp>
  <trd lng="es">apolog&iacute;a</trd>
  <trd lng="fa">&a_d;&a_f;&a_A;&a_ayn;&f_y;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>apologie</trd>,
    <trd>&eacute;loge</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>v&eacute;d&odblac;besz&eacute;d</trd>,
     <trd>v&eacute;d&odblac;irat</trd>,
     <trd>apol&oacute;gia</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>apologia</trd>,
    <trd>obrona</trd>
  </trdgrp>
  <trd lng="ro">ap&abreve;rare</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_h;&c_v;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apol&oacute;gia</trd>,
    <trd>apol&oacute;gia</trd>,
    <trd>obrana</trd>,
    <trd>obrann&aacute; re&ccaron;</trd>
  </trdgrp>
</drv>
</art>
