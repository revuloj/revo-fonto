<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apolog1.xml,v 1.25 2023/10/13 16:46:48 revo Exp $">
<kap>
  <rad>apologi</rad>/o
</kap>
<drv mrk="apolog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parola a&ubreve; skriba <ref tip="super" cel="defend.0o">defendo</ref> 
      kaj <ref cel="prav.0igi">pravigo</ref> de io a&ubreve; iu, solene
      farita:
      <ekz>
        la parolado de <nom>Brutus</nom> estas abstrakta <tld/>o de altprincipa homo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni jure persekutu la konteston a&ubreve; <tld/>on de la holoka&ubreve;sto kaj la disvastigon
        de antisemitismo
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008494.php">Veneno antisemita</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; fidindas <ind><tld/>isto</ind>j de energio-industrioj kiel la nuklea, la nafta
        <klr>[&#x2026;]</klr> gravas evoluigi ekologie pla&ccirc;ajn energi-fontojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010345.php">Ne menciu la n-vorton</url></vrk>
          <lok>2011</lok>
        </fnt>.
        <trd lng="de">Apologet</trd>
        <trd lng="grc">&alfa;&pi;&omikron;&lambda;&omikron;&gamma;&eta;&tau;&eta_ton;&sigma_fina;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pled">pledado</ref>,
      <ref cel="laux">la&ubreve;do</ref>,
      <ref cel="kulp">senkulpigo</ref>
    </refgrp>
    <ref tip="vid" cel="apologetik.0o">apologetiko</ref>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">apologia</trd>
  <trd lng="grc">&alfa;&pi;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="he">&he;&#x5B4;&tav;&nun;&#x5B7;&tsadi;&#x5B0;&lamed;&vav;&#x5BC;&tav;</trd>
  <trd lng="ja">&#x5F01;&#x660E;  <pr>&#x3079;&#x3093;&#x3081;&#x3044;</pr></trd>
  <trd lng="ms">permohonan maaf</trd>
  <trdgrp lng="cs">
    <trd>obhajoba</trd>,
    <trd>obrana</trd>,
    <trd>obrann&aacute; &rcaron;e&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Apologie</trd>,
     <trd>Verteidigungsrede</trd>,
     <trd>Verteidigungsschrift</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apology</trd>
  </trdgrp>
  <trd lng="es">apolog&iacute;a</trd>
  <trd lng="fa">&a_d;&a_f;&a_A;&a_ayn;&f_y;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>apologie</trd>,
    <trd>&eacute;loge</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>v&eacute;d&odblac;besz&eacute;d</trd>,
     <trd>v&eacute;d&odblac;irat</trd>,
     <trd>apol&oacute;gia</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>apologia</trd>,
    <trd>obrona</trd>
  </trdgrp>
  <trd lng="ro">ap&abreve;rare</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_h;&c_v;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apol&oacute;gia</trd>,
    <trd>apol&oacute;gia</trd>,
    <trd>obrana</trd>,
    <trd>obrann&aacute; re&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43F;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x445;&#x432;&#x430;&#x43B;&#x44F;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
