<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apologi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="apolog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parola a&ubreve; skriba defendo kaj pravigo de io a&ubreve; iu, solene
      farita:
      <ekz>
        fari <tld/>on de sia politiko;
      </ekz>
      <ekz>
        la <tld/>o de Sokrato.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pled">pledado</ref>,
      <ref cel="laux">la&ubreve;do</ref>,
      <ref cel="kulp">senkulpigo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>apologie</trd>,
    <trd>&eacute;loge</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_h;&c_v;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
