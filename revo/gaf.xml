<art mrk="$Id: gaf.xml,v 1.18 2022/03/19 13:55:50 revo Exp $">
<kap>
  <rad>gaf</rad>/o
</kap>
<drv mrk="gaf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Oblikva <ref tip="super" cel="stang.vel0o">velstango</ref>, 
      kiu surtenas la <ref tip="vid" cel="vel.skun0o">skunvelojn</ref>:
      <ekz>
        sekvis longa vico da malliberuloj, inter si ligitaj per speco da ligna <tld/>o,
        ili surhavis &ccirc;irka&ubreve;kole feran ringon, kiu estis &scirc;nurita al li a&ubreve; &scirc;i kiu
        anta&ubreve;iris
        <fnt>
          <vrk><url ref="http://esperokatolika.org/ek19661970/ek1967_11.htm">Afrika rakonto &dash; Pro la Kara Iome</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">perxa</trd>
  <trd lng="cs">vratir&aacute;hno</trd>
  <trd lng="de">Gaffel</trd>
  <trdgrp lng="en">
    <trd>gaff</trd>,
    <trd>spar</trd>
  </trdgrp>
  <trd lng="fr">espar</trd>
  <trdgrp lng="hu">
    <trd>csonkar&uacute;d<klr tip="amb">
    (vitorlar&uacute;d)</klr></trd>,
    <trd>csonkafa<klr tip="amb"> (vitorlar&uacute;d)</klr></trd>
  </trdgrp>
  <trd lng="nl">gaffel</trd>
  <trd lng="pl">gafel</trd>
  <trdgrp lng="pt">
    <trd>verg&ocirc;ntea</trd>,
    <trd>verga</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_f;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">vratirahno</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x444;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x440;&#x430;&#x43D;&#x433;&#x43E;&#x443;&#x442;&#x43D;&#x435; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
