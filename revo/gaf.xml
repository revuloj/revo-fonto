<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gaf</rad>/o
</kap>
<drv mrk="gaf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Oblikva <ref tip="super" cel="stang.vel0o">velstango</ref>, 
      kiu surtenas la <ref tip="vid" cel="vel.skun0o">skunvelojn</ref>.
    </dif>
  </snc>
  <trd lng="de">Gaffel</trd>
  <trdgrp lng="en">
    <trd>gaff</trd>,
    <trd>spar</trd>
  </trdgrp>
  <trd lng="fr">espar</trd>
  <trdgrp lng="hu">
    <trd>csonkar&uacute;d<klr tip="amb">
    (vitorlar&uacute;d)</klr></trd>,
    <trd>csonkafa<klr tip="amb"> (vitorlar&uacute;d)</klr></trd>
  </trdgrp>
  <trd lng="nl">gaffel</trd>
  <trd lng="pl">gafel</trd>
  <trdgrp lng="pt">
    <trd>verg&#244;ntea</trd>,
    <trd>verga</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_f;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
