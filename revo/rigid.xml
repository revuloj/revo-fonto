<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rigid.xml,v 1.19 2021/03/12 19:23:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rigid</rad>/a
</kap>
<drv mrk="rigid.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="rigid.0a.neflekseble">
    <dif>
      Malfacile <ref tip="ant" cel="fleks.0ebla">fleksebla</ref>:
      <ekz>
        <tld/>a tola jupo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis kapdoloron, lia kolo estis tute <tld/>a
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        predikistoj <klr>[&#8230;]</klr> kun la <tld/>aj kolumoj kaj longaj nigraj roboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Gerda</nom> estis tiel <tld/>a de malvarmo, ke &scirc;i ne povis paroli
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dorso fari&gcirc;is <tld/>a, la brakoj kaj manoj estis <tld/>aj, &scirc;ia tuta korpo
        estis kiel statuo
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> sidi&gcirc;is en la apogse&gcirc;o malproksime de la tor&ccirc;o kaj pasigis
        kelke da horoj en duondorma <tld/>eco
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fiks.0a">fiksa</ref>,
       <ref cel="firm.0a.fiksita">firma<sncref/></ref>,
      <ref cel="katale.0o">katalepsio</ref>,
      <ref cel="parali">paralizita</ref>,
      <ref cel="mov.sen0a">senmova</ref>,
      <ref cel="sxton">&scirc;tonigita</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>test</trd>,
      <trd>ert</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>steif</trd>,
      <trd>starr</trd>
    </trdgrp>
  </snc>
  <snc mrk="rigid.0a.necedeme">
    <dif>
      Obstine sama, necedema, ne adapti&gcirc;a al la bezonoj kaj
      cirkonstancoj:
      <ekz>
        <tld/>a karaktero;
      </ekz>
      <ekz>
        tio &ccirc;i povus naski la penson, ke nia lingvo restos &ccirc;iam <tld/>a
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>miena mastrino de la oficejo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a stilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="rigor.0a">rigora</ref>,
       <ref cel="sever.0a">severa</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>caparrut</trd>,
      <trd>obstinat</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>streng</trd>,
      <trd>hart <klr>(unnachgiebig)</klr></trd>,
      <trd>unnachgiebig</trd>,
      <trd>unbeweglich</trd>,
      <trd>unflexibel</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_g;&c_ib;</trd>,
    <trd>&c_a;&c_d;&c_u;&c_b;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_n;&c_je;&c_e;&c_l;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">r&iacute;gid</trd>
  <trdgrp lng="cs">
    <trd>neohebn&yacute;</trd>,
    <trd>nepoddajn&yacute;</trd>,
    <trd>rigidn&iacute;</trd>,
    <trd>strnul&yacute;</trd>,
    <trd>tuh&yacute;</trd>,
    <trd>tvrdo&scaron;&iacute;jn&yacute;</trd>,
    <trd>ztuhl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raide</trd>,
    <trd>rigide</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>merev</trd>,
    <trd>rugalmatlan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stug</trd>,
    <trd>stijf</trd>
  </trdgrp>
  <trd lng="pt">r&iacute;gido</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_g;&c_o;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_d;&c_a;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_n;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_ch;&c_je;&c_n;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>merav&yacute;</trd>,
    <trd>neprisp&ocirc;sobiv&yacute;</trd>,
    <trd>skrehnut&yacute;</trd>,
    <trd>stuhnut&yacute;</trd>,
    <trd>tuh&yacute;</trd>,
    <trd>ustrnut&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="rigid.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      En fiksa, necedema maniero:
      <ekz>
        tie sidis hundo <klr>[&#8230;]</klr> kaj <tld/>e lin rigardis
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la supro de la tegmento <tld/>e kaj senmove staris la cikonio
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> tenis la kapon alte kaj staris fiere kaj <tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsano venas rapide, foriras <tld/>e
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        promesi mi ne povas, respondis la mastrino monotone, &gcirc;entile, sed <tld/>e
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">starr</trd>
</drv>


<drv mrk="rigid.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rigid.0igxi.neflekseble">
    <dif>
      I&gcirc;i malelasta, sengesta, malfacile fleksebla:
      <ekz>
        la dika &scirc;afa pelto, malsekigita de sango, <tld/>i&gcirc;is de frosto
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la terura spektaklo <nom>Adamson</nom> <tld/>i&gcirc;is kaj muti&gcirc;is
        <fnt><bib>SatirRak</bib>, <lok>la barbaro</lok></fnt>;
      </ekz>
      <ekz>
        la haroj sur mia korpo <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 4:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia edzino rakontis al li tion, kio okazis, tiam <tld/>i&gcirc;is en li lia koro, kaj
        li fari&gcirc;is kiel &scirc;tono
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 25:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        la birdo ne estis mortinta, &gcirc;i estis nur <tld/>i&gcirc;inta, nun &gcirc;i varmi&gcirc;is kaj
        denove ekvivis
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_s;&c_t;&c_y;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_k;&c_o;&c_ch;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_o;&c_s;&c_t;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_v;&c_je;&c_r;&c_d;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rigid.0igxi.necedeme">
    <dif>
      <ref cel="persis.0i">Persisti</ref> en sia necedemo:
      <ekz>
        en la decid&#8217; kruela <tld/>i&gcirc;is
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia popolo <tld/>i&gcirc;is en la defalo de mi
        <fnt><bib>MT</bib>, <lok>&Hos; 11:7</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_o;&c_s;&c_t;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_o;&c_s;&c_n;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_v;&c_je;&c_r;&c_d;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">posar-se r&iacute;gid</trd>
  <trdgrp lng="de">
    <trd>erstarren</trd>,
    <trd>versteifen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>figer</ind></trd>,
    <trd>se <ind>raidir</ind></trd>
  </trdgrp>
</drv>

<drv mrk="rigid.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Firme, spasme fiksi&gcirc;i &ccirc;e io:
      <ekz>
        li levi&gcirc;is kaj frapis la Fili&scirc;tojn, &gcirc;is lia mano laci&gcirc;is kaj
        al<tld/>i&gcirc;is al la glavo
        <fnt><bib>MT</bib>, <lok>&Sa2; 23:10</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krocx.0igxi">kro&ccirc;i&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>cramponner</ind></trd>,
    <trd>se <ind>crisper</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_o; <ind>&c_v;&c_c;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>
</drv>

<drv mrk="rigid.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fleks.0ebla">Fleksebla</ref>, 
      <ref tip="sin" cel="elast.0a">elasta</ref>:
      <ekz>
        junkoj <klr>[&#8230;]</klr> ekstreme <klr>[&#8230;]</klr> mal<tld/>aj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        signoj de mal<tld/>igo de la planata ekonomio
        <fnt>
          <bib>Monato</bib>
          <aut>Ulrich Lins</aut>
          <vrk><url ref="https://www.monato.be/2005/008536.php">Nokta aventuro en Pjongjango</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_e;&c_l;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flexible</trd>,
    <trd>el&agrave;stic</trd>
  </trdgrp>
  <trd lng="cs">ohebn&yacute;</trd>
  <trdgrp lng="de">
    <trd>beweglich</trd>,
    <trd>flexibel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souple</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rugalms</trd>,
    <trd>hajl&eacute;kony</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buigzaam</trd>,
    <trd>flexibel</trd>,
    <trd>elastisch</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_e;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ohybn&yacute;</trd>
</drv>

<drv mrk="rigid.strecx0eco">
  <kap>stre&ccirc;<tld/>eco</kap>
  <snc>
    <ref tip="dif" cel="kontra4.0o">kontrakturo</ref>
  </snc>
  <trd lng="ca">contractura</trd>
  <trd lng="de">Kontraktur</trd>
  <trdgrp lng="hu">
    <trd>izomr&ouml;vid&uuml;l&eacute;s</trd>,
    <trd>izomzsugorod&aacute;s</trd>,
    <trd>kontrakt&uacute;ra</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>
</art>
