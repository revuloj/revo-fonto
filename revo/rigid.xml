<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rigid</rad>/a
</kap>
<drv mrk="rigid.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      Nefleksebla:
      <ekz>
        <tld/>a bran&ccirc;o, &scirc;tofo, membro;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i pro frosto;
      </ekz>
      <ekz>
        <tld/>i&gcirc;o de la morto;
      </ekz>
      <ekz>
        <tld/>e (fikse) rigardi iun<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mov.sen0a">senmova</ref>,
      <ref cel="parali">paralizita</ref>,
      <ref cel="sxton">&scirc;tonigita</ref>,
      <ref cel="katale.0o">katalepsio</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>steif</trd>,
      <trd>starr</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(f. iu)</uzo>
    <dif>
      Necedema, neobeema, obstina:
      <ekz>
        <tld/>a karaktero;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i en sia decido<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mia popolo <tld/>i&gcirc;is en la defalo de mi<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>streng</trd>,
      <trd>hart</trd>,
      <trd>unnachgiebig</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(f. io)</uzo>
    <dif>
      Ne adapti&gcirc;anta al la bezonoj, kaj cirkonstancoj:
      <ekz>
        <tld/>a stilo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>unbeweglich</trd>,
      <trd>unflexibel</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">strnul&yacute;</trd>
  <trdgrp lng="fr">
    <trd>rigide</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stug</trd>,
    <trd>stijf</trd>
  </trdgrp>
</drv>

<drv mrk="rigid.mal0a">
  <kap>
    mal<tld/>a
  </kap>
  <snc>
    <dif>
      Fleksebla, elasta.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>beweglich</trd>,
    <trd>flexibel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souple</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buigzaam</trd>,
    <trd>flexibel</trd>,
    <trd>elastisch</trd>
  </trdgrp>
</drv>

<drv mrk="rigid.strecx0eco">
  <kap>
    stre&ccirc;<tld/>eco
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="kontra4.0o">Kontrakturo</ref>.
    </dif>
  </snc>
</drv>
</art>
