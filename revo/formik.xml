<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>formik</rad>/o
</kap>
<drv mrk="formik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el la familio <ref tip="malprt" cel="formiked.0oj"><tld/>edoj</ref>:
    </dif>
    <ekz>
      <tld/>a energio, persisto;
    </ekz>
    <ekz>
      meti bastonon en la <ind><tld/>ejo</ind>n
      <klr>(maltrankviligi senzorgan anaron)</klr>.
      <trd lng="fr">fourmili&egrave;re</trd>
    </ekz>
    <trd lng="de">Ameise</trd>
    <trd lng="fr">fourmi</trd>
    <trd lng="ru">&c_m;&c_u;&c_r;&c_a;&c_v;&c_je;&c_j;</trd>
    <trd lng="tr">kar&inodot;nca</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio
      <ref tip="super" cel="formiked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Formica</trd>).
    </dif>
  </snc>
</drv>

</art>
