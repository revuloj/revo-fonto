<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>formik</rad>/o
</kap>
<drv mrk="formik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el la familio <ref tip="malprt" cel="formik.0edoj"><tld/>edoj</ref>:
    </dif>
    <ekz>
      <tld/>a energio, persisto;
    </ekz>
    <ekz>
      meti bastonon en la <tld/>ejon
      <klr>(f. maltrankviligi senzorgan anaron)</klr>.
    </ekz>
    <trd lng="de">Ameise</trd>
    <trd lng="fr">fourmi</trd>
    <trd lng="tr">kar&inodot;nca</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio
      <ref tip="super" cel="formik.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Formica</trd>).
    </dif>
  </snc>
  <trd lng="de">Waldameise</trd>
</drv>
<drv mrk="formik.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Formica</trd>), vivanta en socioj.
    </dif>
  </snc>
  <trd lng="de">Ameisen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
