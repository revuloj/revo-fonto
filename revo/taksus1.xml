<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: taksus1.xml,v 1.15 2019/03/31 19:10:19 revo Exp $">
<kap>
  <rad>taksusac</rad>/oj
</kap>
<drv mrk="taksus1.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="konifer.0oj">koniferoj</ref>
      (<trd lng="la">Taxaceae</trd>):
      <ekz>
        anta&ubreve; la kastelo anta&ubreve;parko etendi&gcirc;as kun labirinto de buksacoj kaj <tld/>oj
        (parte kiel he&gcirc;o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=567043"
              >Kastelo de Tiszadob</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="taksus.0o">taksuso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_ib;&c_s;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">tax&agrave;cies</trd>
  <trd lng="cs">tisovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Eibengew&auml;chse</trd>
  <trd lng="fr">Taxac&eacute;es</trd>
  <trd lng="hu">tiszafaf&eacute;l&eacute;k</trd>
  <trd lng="nl">taxusfamilie</trd>
  <trd lng="pl">cisowate</trd>
  <trdgrp lng="pt">
   <trd>tax&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_s;&c_s;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">tisovit&eacute;</trd>
  <trd lng="sv">idegransv&auml;xter</trd>
</drv>
</art>
