<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>polus</rad>/o</kap>
<drv mrk="polus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="polus.0o.de_sfero">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">GEOG</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      &Ccirc;iu el la du punktoj, je kiuj
      <ref cel="aks.0o.rotacia">akso</ref> de sfero sekcas &gcirc;ian
      surfacon:
      <ekz>
        suda <tld/>o de la tero.
      </ekz>
    </dif>
    <trd lng="de">Pol</trd>
    <trd lng="en">pole</trd>
    <trd lng="fr">p&ocirc;le<klr tip="ind"> (g&eacute;og.)</klr></trd>
    <trdgrp lng="hu">
      <trd>sark</trd>,
      <trd>p&oacute;lus</trd>
    </trdgrp>
    <trd lng="pl">biegun</trd>
  </snc>
  <snc mrk="polus.0o.FIZ">
    <dif>
      &Ccirc;iu el la du punktoj de pilo, baterio, magneto, &ccirc;e kiuj
      aperas kontra&ubreve;aj fortoj.
    </dif>
    <trd lng="fr">p&ocirc;le<klr tip="ind"> (phys.)</klr></trd>
    <trdgrp lng="hu">
      <trd>sarok</trd>,
      <trd>p&oacute;lus</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="polus.0o.koordinatsistemo">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(de <tld/>a <ref
        cel="koordi.0sistemo">koordinatsistemo</ref>)</klr>
        &Gcirc;ia <ref cel="origin.0o.de_polusa_koordinatsistemo">origino</ref>.
      </dif>
      <trdgrp lng="hu">
        <trd>p&oacute;luspont</trd>,
        <trd>orig&oacute;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="polus.0o.de_funkcio">
      <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
      <dif>
        <klr>(de kompleksa <ref cel="funkci.0o.MAT">funkcio</ref>
        <frm><k>f</k></frm>)</klr>
        Tia <ref cel="ordina.ne0a.punkto_de_funkcio">neordinara</ref>
        punkto <frm><k>a</k></frm> de <frm><k>f</k></frm>, ke por iu
        entjero <frm><k>n</k></frm> la funkcio
        <frm>(<k>z</k>-<k>a</k>)<sup><k>n</k></sup><k>f</k>(<k>z</k>)</frm>
        estas <ref cel="holomo.0a.cxepunkto">holomorfa</ref>
        &ccirc;e punkto <frm><k>a</k></frm>.
      </dif>
      <ref tip="vid" cel="meromo.0a">meromorfa.</ref>
      <trd lng="hu">p&oacute;lus<klr tip="ind"> (f&uuml;ggv&eacute;ny&eacute;)</klr></trd>
    </subsnc>
    <trd lng="de">Pol</trd>
    <trd lng="en">pole</trd>
    <trd lng="fr">p&ocirc;le<klr tip="ind"> (math.)</klr></trd>
    <trd lng="pl">biegun</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_ju;&c_s;</trd>
  </snc>
  <trd lng="nl">pool</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_ju;&c_s;</trd>
</drv>

<drv mrk="polus.P0astelo">
  <kap><tld lit="P"/>a stelo</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Stelo en la stelaro
      <ref tip="malprt" cel="urs.MalgrandaU0ino">Malgranda Ursino</ref>,
      <trd lng="la">&alfa; Ursae Minoris</trd>,
      el la helaj steloj la plej proksima al la &ccirc;iela nord<tld/>o
      (je proks. 1&ring;).
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>North star</trd>,
    <trd>polestar</trd>,
    <trd>Cynosura</trd>,
    <trd>Polaris</trd>
  </trdgrp>
  <trd lng="fr">&eacute;toile polaire</trd>
  <trdgrp lng="hu">
    <trd>Sarkcsillag</trd>,
    <trd>Polaris</trd>
  </trdgrp>
  <trd lng="nl">Poolster</trd>
  <trd lng="ru">&c_P;&c_o;&c_l;&c_ja;&c_r;&c_n;&c_a;&c_ja; &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
</drv>
</art>
