<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: septet.xml,v 1.21 2023/10/21 06:31:44 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>septet</rad>/o
</kap>
<drv mrk="septet.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika <ref tip="super" cel="verk.0o.propra">verko</ref>
      por sep vo&ccirc;oj a&ubreve; instrumentoj:
      <ekz>
        la <tld/>o de <nom>Beethoven</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287108">Instrumentado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Igor Stravinski</nom> <klr>[&#x2026;]</klr> unuafoje eksperimentis per ne-dekdutonaj
        seriteknikoj en malgrand-skalaj kaj vo&ccirc;aj kaj &ccirc;ambraj verkoj kiaj la
        <klr>[&#x2026;]</klr> <tld lit="S"/>o (1953) kaj la Tri kantoj el <nom>Shakespeare</nom> (1953)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21609">Igor Stravinski</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_p;&c_t;&c_e;&c_t;</trd>
  <trd lng="bg">&c_s;&c_je;&c_p;&c_t;&c_je;&c_t;</trd>
  <trd lng="ca">septet</trd>
  <trdgrp lng="cs">
    <trd>septet</trd>,
    <trd>septuor</trd>
  </trdgrp>
  <trd lng="de">Septett</trd>
  <trd lng="en">septet</trd>
  <trd lng="es">septeto</trd>
  <trd lng="fr">septuor</trd>
  <trd lng="hu">septett</trd>
  <trdgrp lng="ja">
    <trd>&#x4E03;&#x91CD;&#x594F;  <pr>&#x306A;&#x306A;&#x3058;&#x3085;&#x3046;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x4E03;&#x91CD;&#x5531;  <pr>&#x306A;&#x306A;&#x3058;&#x3085;&#x3046;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">septet</trd>
  <trd lng="pl">septet</trd>
  <trd lng="ru">&c_s;&c_je;&c_p;&c_t;&c_je;&c_t;</trd>
  <trd lng="sk">septeto</trd>
  <trd lng="uk">&c_s;&c_je;&c_p;&c_t;&c_je;&c_t;</trd>
</drv>
</art>
