<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tring.xml,v 1.7 2024/05/10 18:27:19 afido Exp $">
<kap>
  <rad>tring</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="tring.0o">
  <kap><tld/>o</kap>
  <snc mrk="tring.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:birdoj" cel="bird.vad0oj">Vadbirdo</ref> kun mezlonga beko
      kaj longaj brilkoloraj kruroj, vivanta en mezvarmaj landoj;
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref> el
      familio <ref tip="malprt" cel="skolop2.0oj">skolopedoj</ref>
      (<trd lng="la">Tringa</trd>):
      <ekz>
        iuj <tld/>aj paroj superflugis el la oriento
        <fnt>
          <aut>H. Laxness</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        Blankventra <tld/>o venis flugante de la nordo al la boathaveno
        <fnt>
          <aut>Jan Hellstr&ouml;m</aut>
          <vrk><url ref="https://www.esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_41.pdf"
              >Vesperaj sonoj de birdoj &ccirc;e rivero G&ouml;ta &Auml;lv</url></vrk>
          <lok>Ora &Jcirc;urnalo, n.ro 41, Junio&ndash;Julio 2021, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/9/9a/Wood_sandpiper_2.jpg/640px-Wood_sandpiper_2.jpg"
      prm="CC BY-SA 4.0"
      ><tld/>o (Tringa glareola)
        <fnt>
          <aut>Artemy Voikhansky, propra verko</aut>
          <lok><url ref="&WCW;/File:Wood_sandpiper_2.jpg"
              >https://commons.wikimedia.org/wiki/File:Wood_sandpiper_2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_ib;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_u;&c_ch;&c_a;&c_j;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Wasserl&auml;ufer</trd>
  <trd lng="fr">chevalier<klr> (oiseau)</klr></trd>
  <trd lng="pl">brodziec</trd>
  <trd lng="uk">&c_k;&c_o;&c_l;&c_o;&c_v;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#23567;&#40643;&#33139;&#40440; <pr>xi&#462;ohu&aacute;ngji&#462;oy&ugrave;</pr></trd>,
    <trd>&#23567;&#40644;&#33050;&#40556; <pr>xi&#462;ohu&aacute;ngji&#462;oy&ugrave;</pr></trd>,
    <trd>&#28418;&#40440; <pr>pi&amacron;oy&ugrave;</pr></trd>,
    <trd>&#28418;&#40556; <pr>pi&amacron;oy&ugrave;</pr></trd>,
    <trd>&#26519;&#40440; <pr>l&iacute;ny&ugrave;</pr></trd>,
    <trd>&#26519;&#40556; <pr>l&iacute;ny&ugrave;</pr></trd>,
    <trd>&#38738;&#33050;&#40556; <pr>q&imacron;ngji&#462;oy&ugrave;</pr></trd>,
    <trd>&#38738;&#33139;&#40440; <pr>q&imacron;ngji&#462;oy&ugrave;</pr></trd>,
    <trd>&#32005;&#33139;&#40440; <pr>h&oacute;ngji&#462;oy&ugrave;</pr></trd>,
    <trd>&#32418;&#33050;&#40556; <pr>h&oacute;ngji&#462;oy&ugrave;</pr></trd>
  </trdgrp>
</drv>
</art>

