<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><ofc>*</ofc><rad>akir</rad>/i</kap>
<drv mrk="akir.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Havigi al si:
       <ekz>
         mi <tld/>is tiun pentra&jcirc;on je malalta prezo;
       </ekz>
       <ekz>
         peke <tld/>ita ne estas profita
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         lasu min re<tld/>i mian spiron;
       </ekz>
       <ekz>
         <tld/>i domon, prozelitojn, amikon, scion, sa&gcirc;on,
         potencon, sukceson<fnt>Z</fnt>, influon;
       </ekz>
       <ekz>
         la parolanto <tld/>is la plimulton de la vo&ccirc;oj;
       </ekz>
       <ekz>
         nenia ofero estus tro granda, se ni povus per &gcirc;i
         <tld/>i al ni lingvon  komunhoman<fnt>Z</fnt>;
       </ekz>
       <ekz>
         tiu propono <tld/>is la aprobon de &ccirc;iuj;
       </ekz>
       <ekz>
         mi <tld/>is la konvinkon, ke...;
       </ekz>
       <ekz>
         vi devas <tld/>i por tio la permeson de la Akademio;
       </ekz>
       <ekz>
         tiu legendo <tld/>is grandan amplekson;
       </ekz>
       <ekz>
         labor<tld/>i
         <fnt>(<aut>Zamenhof</aut>: <vrk>Biblio</vrk>,
         <lok>Je&hcirc;ezkel 23, 29</lok>)</fnt>;
       </ekz>
       <ekz>
         milit<tld/>a&jcirc;o
         <fnt>(<aut>Zamenhof</aut>: <vrk>Biblio</vrk>,
         <lok>Samuel 15, 21</lok>)</fnt>;
       </ekz>
       <ekz>
         rab<tld/>o
         <fnt>(<aut>Zamenhof</aut>: <vrk>Biblio</vrk>,
         <lok>Jesaja 10, 6</lok>)</fnt>;
       </ekz>
       <ekz>
         &ccirc;as<tld/>o<fnt>Z</fnt>;
       </ekz>
       <ekz>
         batal<tld/>o<fnt>Z</fnt>;
       </ekz>
       <ekz>
         iun plen<tld/>i <klr>(pla&ccirc;i tute al iu,  gajni ies
         amikecon, favoron: <ekz> la preleganto plen<tld/>is siajn
         a&ubreve;dantojn</ekz>)</klr>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="ating.0i">atingi</ref>,
       <ref cel="gajn.0i">gajni</ref>,
       <ref cel="konker.0i">konkeri</ref>,
       <ref cel="kapt.0i">kapti</ref>,
       <ref cel="pren.0i">preni</ref>,
       <ref cel="ricev.0i">ricevi</ref>,
       <ref cel="rikolt.0i">rikolti</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_b;&c_y;&c_c;&c_mol;</trd>, 
     <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>, 
     <trd>&c_z;&c_a;&c_j;&c_m;&c_je;&c_c;&c_mol;</trd>, 
     <trd>&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>, 
     <trd>&c_z;&c_d;&c_a;&c_b;&c_y;&c_c;&c_mol;</trd> 
   </trdgrp>
   <trdgrp lng="de">
     <trd>erlangen</trd>,
     <trd>erwerben</trd>,
     <trd>anschaffen</trd>,
     <trd>erbeuten</trd>
   </trdgrp>
   <trd lng="en">acquire</trd>
   <trdgrp lng="fr">
     <trd>acqu&eacute;rir</trd>,
     <trd>obtenir</trd>,
     <trd>se <ind>procurer</ind></trd></trdgrp>
   <trdgrp lng="it">
     <trd>acquisire</trd>,
     <trd>ottenere</trd>,
     <trd>conseguire</trd>
   </trdgrp>
   <trdgrp lng="nl">
   <trd>verwerven</trd>,
   <trd>verkrijgen</trd>
       </trdgrp>
   <trdgrp lng="pt">
     <trd>adquirir</trd>,
     <trd>obter</trd>,
     <trd>arranjar</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_d;&c_o;&c_b;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="sv">
      <trd>f&ouml;rv&auml;rva</trd>,
      <trd><ind>skaffa</ind> sig</trd>
   </trdgrp>
</drv>
</art>
