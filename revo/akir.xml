<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akir.xml,v 1.37 2021/05/28 12:11:18 revo Exp $">
<kap>
  <ofc>*</ofc><rad>akir</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akir.0i.KOMUNE">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Havigi al si:
      <ekz>
        mi <tld/>is tiun pentra&jcirc;on je malalta prezo;
      </ekz>
      <ekz>
        peke <tld/>ita ne estas profita<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        lasu min re<tld/>i mian spiron;
      </ekz>
      <ekz>
        <tld/>i domon, prozelitojn, amikon, scion, sa&gcirc;on,
        potencon, sukceson<fnt>Z</fnt>, influon;
      </ekz>
      <ekz>
        la parolanto <tld/>is la plimulton de la vo&ccirc;oj;
      </ekz>
      <ekz>
        nenia ofero estus tro granda, se ni povus per &gcirc;i
        <tld/>i al ni lingvon komunhoman<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu propono <tld/>is la aprobon de &ccirc;iuj;
      </ekz>
      <ekz>
        mi <tld/>is la konvinkon, ke...;
      </ekz>
      <ekz>
        vi devas <tld/>i por tio la permeson de la Akademio;
      </ekz>
      <ekz>
        tiu legendo <tld/>is grandan amplekson;
      </ekz>
      <ekz>
        labor<tld/>i
        <fnt><bib>MT</bib>, <lok>&Jeh; 23:29</lok></fnt>;
      </ekz>
      <ekz>
        milit<tld/>a&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Sa1; 15:21</lok></fnt>;
      </ekz>
      <ekz>
        rab<tld/>o
        <fnt><bib>MT</bib>, <lok>&Jes; 10:6</lok></fnt>;
      </ekz>
      <ekz>
        batal<tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        iun plen<tld/>i <klr>(pla&ccirc;i tute al iu, gajni ies
        amikecon, favoron:
        <ekz>la preleganto plen<tld/>is siajn a&ubreve;dantojn</ekz>)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ating.0i">atingi</ref>,
      <ref cel="gajn.0i">gajni</ref>,
      <ref cel="konker.0i">konkeri</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="pren.0i">preni</ref>,
      <ref cel="ricev.0i">ricevi</ref>,
      <ref cel="rikolt.0i">rikolti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_b;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_b;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xF42;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>akuizita&ntilde;</trd>,
    <trd>perc'henna&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adquirir</trd>,
    <trd>guanyar</trd>,
    <trd>obtenir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dos&aacute;hnout n&ecaron;&ccaron;eho</trd>,
    <trd>nab&yacute;t</trd>,
    <trd>opat&rcaron;ovat si</trd>,
    <trd>vydob&yacute;vat</trd>,
    <trd>z&iacute;skat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erlangen</trd>,
    <trd>erwerben</trd>,
    <trd>anschaffen</trd>,
    <trd>erbeuten</trd>,
    <trd>erarbeiten</trd>,
    <trd>akquirieren</trd>,
    <trd>an sich <ind>bringen</ind></trd>,
    <trd>in Besitz <ind>kommen</ind></trd>
  </trdgrp>
  <trd lng="en">acquire</trd>
  <trdgrp lng="es">
    <trd>adquirir</trd>,
    <trd>obtener</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acqu&eacute;rir</trd>,
    <trd>obtenir</trd>,
    <trd>se <ind>procurer</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5E8;&#x5DB;&#x5D5;&#x5E9;</trd>,
    <trd>&#x5DC;&#x5D4;&#x5E9;&#x5D9;&#x5D2;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>men<ind>dapat</ind></trd>,
    <trd>memper<ind>oleh</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>acquisire</trd>,
    <trd>ottenere</trd>,
    <trd>conseguire</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwerven</trd>,
    <trd>verkrijgen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zdoby&cacute;</trd>,
    <trd>uzyska&cacute;</trd>,
    <trd>osi&aogonek;gn&aogonek;&cacute;</trd>,
    <trd>naby&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adquirir</trd>,
    <trd>obter</trd>,
    <trd>arranjar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>atinge</trd>,
    <trd>cump&abreve;ra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadobudn&uacute;&tcaron;</trd>,
    <trd>z&iacute;ska&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>f&ouml;rv&auml;rva</trd>,
    <trd><ind>skaffa</ind> sig</trd>
  </trdgrp>
  <trd lng="tp">kama <ind>jo</ind></trd>
</drv>

<drv mrk="akir.cxas0i">
  <kap>&ccirc;as<tld/>i</kap>
  <snc mrk="akir.cxas0i.CXAS">
    <dif>
      <tld lit="A"/>i, kapti per &ccirc;asado:
      <ekz>
        &ccirc;as<tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni diris, ke li povas &ccirc;as<tld/>i pli multe da bestoj
        <klr>...</klr> ol ordinaraj &ccirc;asistoj
        <fnt>
        <aut>LU Fuvej</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/epc/epc6104/oceanou.html">
        La &ccirc;asisto en arba oceano</url></vrk>,
        <lok>El popola &Ccirc;inio, 1961:4, p. 185a-187a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>capturar</trd>,
    <trd>atrapar</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>capturar <klr>(una presa)</klr></trd>,
    <trd>dar caza</trd>
  </trdgrp>
  <trd lng="fr">attraper<klr> (&agrave; la chasse)</klr></trd>
  <trd lng="pl">upolowa&cacute;</trd>
</drv>
</art>
