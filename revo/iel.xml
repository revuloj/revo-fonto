<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>iel</rad>
</kap>
<drv mrk="iel.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;la&ubreve; ia maniero&rightquot;:
      <ekz>
        &ccirc;u mi povas <tld/> helpi al vi?;
      </ekz>
      <ekz>
        neniu havas la rajton <tld/> tu&scirc;i vian kapon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  <trd lng="de">irgendwie</trd>
  <trdgrp lng="fr">
    <trd>d'une certaine <ind>mani&egrave;re</ind></trd>,
    <trd>d'une <ind>mani&egrave;re</ind> ou d'une autre</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>valahogy</trd>,
    <trd>valamik&eacute;ppen</trd>
  </trdgrp>
  <trd lng="nl">op een of andere <ind>wijze</ind></trd>
  <trdgrp lng="pl">
   <trd>jako&sacute;</trd>, 
   <trd>jakim&sacute; sposobem</trd> 
  </trdgrp>
  <trdgrp lng="pt">
   <trd>de algum <ind>modo</ind></trd>, 
   <trd>de alguma <ind>maneira</ind></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_k;-&c_t;&c_o;</trd>,
    <trd>&c_k;&c_a;&c_k;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  </trdgrp>
  <trd lng="vo">semlik</trd>
</drv>

<drv mrk="iel.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Emfaza samsenca formo:
      <ekz><tld/>e, iome, duone malbone
        <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>d'une certaine <ind>mani&egrave;re</ind></trd>,
    <trd>d'une <ind>mani&egrave;re</ind> ou d'une autre</trd></trdgrp>
  <trd lng="hu">&iacute;gy vagy &uacute;gy</trd>
  <trd lng="nl">op een of andere <ind>wijze</ind></trd>
  <trdgrp lng="pl">
   <trd>jako&sacute;</trd>, 
   <trd>jakim&sacute; sposobem</trd> 
  </trdgrp>
  <trdgrp lng="pt">
   <trd>de algum <ind>modo</ind></trd>, 
   <trd>de alguma <ind>maneira</ind></trd> 
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_k;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
</drv>
</art>
