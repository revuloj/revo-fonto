<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>iel</rad></kap>
<drv mrk="iel.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <trd lng="de">irgendwie</trd>
    <dif>
      Adverbo signifanta "la&ubreve; ia maniero":
      <ekz>
        &ccirc;u mi povas <tld/> helpi al vi?
      </ekz>
      <ekz>
        neniu havas la rajton <tld/> tu&scirc;i vian kapon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">irgendwie</trd>
</drv>
<drv mrk="iel.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Emfaza samsenca formo:
      <ekz>
        <tld/>e, iome, duone malbone
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
