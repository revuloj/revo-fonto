<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iel.xml,v 1.26 2025/11/28 22:30:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>iel</rad>
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="iel.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;la&ubreve; ia maniero&rightquot;:
      <ekz>
        &Ccirc;u vi ne povus traduki la vorton &rightquot;gento&#x201D; <tld/> alie?
        <fnt>
          <bib>LR</bib>
          <aut>L. L. Zamenhof</aut>
          <lok>Unua parto, 1: Vortfarado, 28: Pri &rightquot;gento&#x201D;</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iam <tld/> forgesadis
        <fnt>
          <bib>Revizoro</bib>
          <aut>L. L. Zamenhof</aut>
          <lok>Akto 1, Sceno 1</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo Petrovi&ccirc; rigardis &scirc;in <tld/> strange
        <fnt>
          <bib>PatrojFiloj</bib>
          <aut>Kabe</aut>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        sentis sin <tld/> malvigla
        <fnt>
          <bib>Fab3</bib>
          <aut>L. L. Zamenhof</aut>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u mi povas <tld/> helpi al vi?;
      </ekz>
      <ekz>
        neniu havas la rajton <tld/> tu&scirc;i vian kapon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  <trd lng="de">irgendwie</trd>
  <trdgrp lng="en">
    <trd>in some way</trd>,
    <trd>in any way</trd>,
    <trd>somehow</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>de alg&uacute;n <ind>modo</ind></trd>,
   <trd>de alguna <ind>manera</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'une certaine <ind>mani&egrave;re</ind></trd>,
    <trd>d'une <ind>mani&egrave;re</ind> ou d'une autre</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>valahogy</trd>,
    <trd>valamik&eacute;ppen</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>in qualche <ind>modo</ind></trd>,
   <trd>in qualche <ind>maniera</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x3069;&#x3046;&#x306B;&#x304B;&#x3057;&#x3066;</trd>
  <trd lng="nl">op een of andere <ind>wijze</ind></trd>
  <trdgrp lng="pl">
   <trd>jako&sacute;</trd>,
   <trd>jakim&sacute; sposobem</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>de algum <ind>modo</ind></trd>,
   <trd>de alguma <ind>maneira</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
   <trd>intr-un mod anume</trd>,
   <trd>intr-un fel anume</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_k;-&c_t;&c_o;</trd>,
    <trd>&c_k;&c_a;&c_k;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">p&aring; n&aring;got <ind>s&auml;tt</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_mol;</trd>,
    <trd>&c_ja;&c_k;&c_o;&c_s;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_z;&c_n;&c_a;-&c_ja;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_n;&c_a;</trd>,
    <trd>&c_ja;&c_k;</trd>,
    <trd>&c_h;&c_t;&c_o;&c_z;&c_n;&c_a;-&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="vo">semlik</trd>
  <trd lng="zh">&#x4E0D;&#x77E5;&#x4F55;&#x6545; <pr>b&ugrave;zh&imacron;h&eacute;g&ugrave;</pr></trd>
</drv>

<drv mrk="iel.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Emfaza samsenca formo:
      <ekz><tld/>e, iome, duone malbone
        <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>in some way</trd>,
    <trd>in any way</trd>,
    <trd>somehow</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>de un <ind>modo</ind> u otro</trd>,
   <trd>de una <ind>manera</ind> u otra</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'une certaine <ind>mani&egrave;re</ind></trd>,
    <trd>d'une <ind>mani&egrave;re</ind> ou d'une autre</trd></trdgrp>
  <trd lng="hu">&iacute;gy vagy &uacute;gy</trd>
  <trdgrp lng="it">
   <trd>in un <ind>modo</ind> o nell'altro</trd>
  </trdgrp>
  <trd lng="ja">&#x3069;&#x3046;&#x306B;&#x304B;&#x3057;&#x3066;</trd>
  <trd lng="nl">op een of andere <ind>wijze</ind></trd>
  <trdgrp lng="pl">
   <trd>jako&sacute;</trd>,
   <trd>jakim&sacute; sposobem</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>de algum <ind>modo</ind></trd>,
   <trd>de alguma <ind>maneira</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_k;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  <trd lng="sv">p&aring; n&aring;got <ind>s&auml;tt</ind></trd>
</drv>
</art>
