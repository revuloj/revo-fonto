<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iel.xml,v 1.21 2022/03/21 15:27:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>iel</rad>
</kap>
<drv mrk="iel.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;la&ubreve; ia maniero&rightquot;:
      <ekz>
        &ccirc;u mi povas <tld/> helpi al vi?;
      </ekz>
      <ekz>
        neniu havas la rajton <tld/> tu&scirc;i vian kapon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  <trd lng="de">irgendwie</trd>
  <trdgrp lng="es">
   <trd>de alg&uacute;n <ind>modo</ind></trd>, 
   <trd>de alguna <ind>manera</ind></trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'une certaine <ind>mani&egrave;re</ind></trd>,
    <trd>d'une <ind>mani&egrave;re</ind> ou d'une autre</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>valahogy</trd>,
    <trd>valamik&eacute;ppen</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>in qualche <ind>modo</ind></trd>, 
   <trd>in qualche <ind>maniera</ind></trd> 
  </trdgrp>
  <trd lng="nl">op een of andere <ind>wijze</ind></trd>
  <trdgrp lng="pl">
   <trd>jako&sacute;</trd>, 
   <trd>jakim&sacute; sposobem</trd> 
  </trdgrp>
  <trdgrp lng="pt">
   <trd>de algum <ind>modo</ind></trd>, 
   <trd>de alguma <ind>maneira</ind></trd> 
  </trdgrp>
  <trdgrp lng="ro">
   <trd>intr-un mod anume</trd>, 
   <trd>intr-un fel anume</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_k;-&c_t;&c_o;</trd>,
    <trd>&c_k;&c_a;&c_k;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">p&aring; n&aring;got <ind>s&auml;tt</ind></trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x43A;-&#x43D;&#x435;&#x431;&#x443;&#x434;&#x44C;</trd>,
    <trd>&#x44F;&#x43A;&#x43E;&#x441;&#x44C;</trd>,
    <trd>&#x431;&#x43E;&#x437;&#x43D;&#x430;-&#x44F;&#x43A;</trd>,
    <trd>&#x43A;&#x430;&#x437;&#x43D;&#x430;</trd>,
    <trd>&#x44F;&#x43A;</trd>,
    <trd>&#x445;&#x442;&#x43E;&#x437;&#x43D;&#x430;-&#x44F;&#x43A;</trd>
  </trdgrp>
  <trd lng="vo">semlik</trd>
</drv>

<drv mrk="iel.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Emfaza samsenca formo:
      <ekz><tld/>e, iome, duone malbone
        <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  <trdgrp lng="es">
   <trd>de un <ind>modo</ind> u otro</trd>, 
   <trd>de una <ind>manera</ind> u otra</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'une certaine <ind>mani&egrave;re</ind></trd>,
    <trd>d'une <ind>mani&egrave;re</ind> ou d'une autre</trd></trdgrp>
  <trd lng="hu">&iacute;gy vagy &uacute;gy</trd>
  <trdgrp lng="it">
   <trd>in un <ind>modo</ind> o nell'altro</trd>
  </trdgrp>
  <trd lng="nl">op een of andere <ind>wijze</ind></trd>
  <trdgrp lng="pl">
   <trd>jako&sacute;</trd>, 
   <trd>jakim&sacute; sposobem</trd> 
  </trdgrp>
  <trdgrp lng="pt">
   <trd>de algum <ind>modo</ind></trd>, 
   <trd>de alguma <ind>maneira</ind></trd> 
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_k;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  <trd lng="sv">p&aring; n&aring;got <ind>s&auml;tt</ind></trd>
</drv>
</art>
