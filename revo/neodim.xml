<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: neodim.xml,v 1.22 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>neodim</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="neodim.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Nd</mlg>
  <snc mrk="neodim.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="60">elemento</ref>,
      malofta ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>
      kun atomnumero 60, atompezo 144,24; lantanoido:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> rara metalo, <klr>[&#x2026;]</klr> brila
        ar&gcirc;ent-kolora lantanoida elemento, tamen &gcirc;i senbrili&gcirc;as rapide en aero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28668">Neodimo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_a;&c_d;&c_y;&c_m;</trd>
  <trd lng="bg">&c_n;&c_je;&c_o;&c_d;&c_i;&c_m;</trd>
  <trd lng="br">neodim</trd>
  <trd lng="ca">neodimi</trd>
  <trd lng="cs">neodym <klr>(Nd)</klr></trd>
  <trd lng="de">Neodym</trd>
  <trd lng="en">neodymium</trd>
  <trd lng="es">neodimio</trd>
  <trd lng="fr">n&eacute;odyme</trd>
  <trd lng="hu">neod&iacute;mium</trd>
  <trd lng="id">neodimium</trd>
  <trd lng="it">neodimio</trd>
  <trd lng="la">Neodymium</trd>
  <trd lng="nl">neodymium</trd>
  <trd lng="pl">neodym</trd>
  <trd lng="pt">neod&iacute;mio</trd>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_d;&c_i;&c_m;</trd>
  <trd lng="sk">neodym <klr>(Nd)</klr></trd>
  <trd lng="sv">neodym</trd>
  <trd lng="uk">&#x43D;&#x435;&#x43E;&#x434;&#x438;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x91F9; <pr>n&#x1D4;:</pr></trd>,
    <trd>&#x9495; <pr>n&#x1D4;:</pr></trd>,
    <trd>&#x91E2; <pr>n&#x1CE;i</pr></trd>,
    <trd>&#x91F9; <pr>n&#x1DA;</pr></trd>,
    <trd>&#x9495; <pr>n&#x1DA;</pr></trd>
  </trdgrp>
</drv>
</art>
