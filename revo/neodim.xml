<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>neodim</rad>/o
</kap>
<drv mrk="neodim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 60, atompezo 144,24, simbolo Nd. Malofta <ref tip="super"
      cel="metal.0o">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_a;&c_d;&c_y;&c_m;</trd>
  <trd lng="de">Neodym</trd>
  <trd lng="en">neodymium</trd>
  <trd lng="hu">neod&iacute;mium</trd>
  <trd lng="nl">neodymium</trd>
  <trd lng="pt">neod&iacute;mio</trd>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_d;&c_i;&c_m;</trd>
</drv>
</art>
