<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>5</ofc>
      <rad>kabinet</rad>/o <fnt>Z</fnt>
    </kap>
    <drv mrk="kabine.0">
      <kap><tld/>o</kap>
      <snc mrk="kabine.0.pensejo">
        <dif>
          &Ccirc;ambro destinita al intelekta laboro:
          <ekz>
            <tld/>o de kuracisto<fnt>B</fnt>,</ekz><ekz>
            oficisto;</ekz><ekz>
            anta&ubreve; la apero de Esperanto multaj asertis, ke lingvo ne povas
            esti kreita en <tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>aj provoj<fnt>Z</fnt>
            <klr>(<refgrp tip="vid">
              <ref cel="libr">libra</ref>,
              <ref cel="retort">retorta</ref></refgrp>)</klr>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Arbeitszimmer</trd>,
          <trd>Studierzimmer</trd>,
          <trd>Sprechzimmer</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>bureau<klr> (pi&egrave;ce pour travailler)</klr></trd>,
          <trd>cabinet d'&eacute;tude</trd>
        </trdgrp>
      </snc>
      <snc mrk="kabine.0.ekspozicio">
        <dif>
          Speciala &ccirc;ambro, kie oni ekspozicias sciencajn a&ubreve; artajn
          kolektojn por studo:
          <ekz>
            <tld/>o de natura&jcirc;oj<fnt>Z</fnt></ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="galeri.0o.ekspoziciejo">galerio</ref>.
        </refgrp>
        <trdgrp lng="de">
          <trd>Ausstellungsraum</trd>,
          <trd>Aufbewahrungsort</trd>,
          <trd>Kabinett</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>cabinet de curiosit&eacute;s</trd>
        </trdgrp>
      </snc>
      <snc mrk="kabine.0o.ministraro">
        <dif>
          <ref tip="dif" cel="minist1.0aro">Ministraro</ref>.
          <ekz>
            la <tld/>o eksi&gcirc;is.</ekz>
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>Ministerkabinett</trd>,
        <trd>Kabinett</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>cabinet</trd>
      </trdgrp>
      <trd lng="nl">kabinet</trd>
      <trd lng="ru">&c_k;&c_a;&c_b;&c_i;&c_n;&c_je;&c_t;</trd>
    </drv>
  </art>
