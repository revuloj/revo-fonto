<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Fortunat</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fortunat.0o">
  <kap><tld/>o</kap>
  <snc mrk="fortunat.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        kaj mi &gcirc;ojas pro la alveno de <klr>...</klr> Fortunato kaj A&hcirc;aiko
        <fnt><bib>NT</bib>, <lok>&Ko1; 16:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Fortunatus<klr tip="amb"> (Vorname)</klr></trd>
  <trd lng="fr">Fortun&eacute;<klr tip="amb"> (pr&eacute;nom)</klr></trd>
  <trd lng="pt">Fortunato</trd>
</drv>
</art>

