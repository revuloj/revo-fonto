<vortaro>
<art mrk="$Id: zingib1.xml,v 1.14 2018/08/13 18:10:14 revo Exp $">
<kap>
   <rad>zingibrac</rad>/oj
</kap>
<drv mrk="zingib1.0oj">
   <kap>
     <tld/>oj
   </kap>
   <snc>
   <uzo tip="fak">BOT</uzo>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO"
       lst="voko:botanikaj_familioj">Familio</ref>  
       de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
       tropikaj plantoj (<trd lng="la">Zingiberaceae</trd>):
      <ekz>
        la kurkumo botanike apartenas al la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2387">Kurkumo</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">
&c_ib;&c_m;&c_b;&c_ib;&c_r;&c_n;&c_y;&c_ja; <klr tip="amb">
(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
   <trd lng="ca">zingiber&agrave;cies</trd>
   <trd lng="de">Ingwergew&auml;chse</trd>
   <trd lng="de">zingib&eacute;rac&eacute;es</trd>
   <trd lng="es">cingiber&aacute;ceas</trd>
   <trd lng="fr">Zingib&eacute;rac&eacute;es</trd>
   <trd lng="hu">gy&ouml;mb&eacute;rf&eacute;l&eacute;k</trd>
   <trd lng="nl">gemberfamilie</trd>
   <trd lng="pt">zingiber&aacute;ceas</trd>
   <trd lng="ru">&c_i;&c_m;&c_b;&c_i;&c_r;&c_n;&c_y;&c_je; 
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
   <trd lng="it">Zingiberaceae</trd>
</drv>
</art>
