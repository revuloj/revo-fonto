<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apepsi.xml,v 1.5 2007/06/10 16:32:20 revo Exp $">
<kap>
  <rad>apepsi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="apepsi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malrapidi&gcirc;o, malfacili&gcirc;o de digesto:
      <ekz>
       timante <tld/>on mi ne riskas frue enliti&gcirc;i
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p. 357a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_p;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>mangelhaftes Verdauungsverm&ouml;gen</trd>,
    <trd>Apepsie</trd>
  </trdgrp>                  
  <trd lng="es">apepsia</trd>
  <trdgrp lng="fr">
    <trd>apepsie</trd>
  </trdgrp>
  <trd lng="hu">apepszia</trd>
</drv>
</art>

