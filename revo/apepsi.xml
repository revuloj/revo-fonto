<art mrk="$Id: apepsi.xml,v 1.7 2022/03/12 10:47:30 revo Exp $">
<kap>
  <rad>apepsi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="apepsi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malrapidi&gcirc;o, malfacili&gcirc;o de digesto:
      <ekz>
       timante <tld/>on mi ne riskas frue enliti&gcirc;i
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p. 357a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_p;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>mangelhaftes Verdauungsverm&ouml;gen</trd>,
    <trd>Apepsie</trd>
  </trdgrp>                  
  <trd lng="es">apepsia</trd>
  <trdgrp lng="fr">
    <trd>apepsie</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5D7;&#x5DC;&#x5EA; &#x5E2;&#x5D9;&#x5DB;&#x5D5;&#x5DC;</trd>
  <trd lng="hu">apepszia</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43F;&#x435;&#x43F;&#x441;&#x456;&#x44F;</trd>,
    <trd>&#x43D;&#x435;&#x441;&#x442;&#x440;&#x430;&#x432;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

