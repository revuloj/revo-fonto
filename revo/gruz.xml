<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gruz.xml,v 1.23 2023/11/11 11:36:37 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>gruz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gruz.0o">
  <kap><tld/>o</kap>
  <snc mrk="gruz.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref cel="sabl.0o">Sabl</ref>ego el dikaj grajnoj, dispecigitaj &scirc;tonetoj:
      <ekz>
	la <tld/>o en la vojetoj estis hajle blanka
	<fnt>
	<aut>G. Bomans, trad. B. Leopold</aut>,
	<vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr53/sekreto.html">
	La sekreto</url></vrk>,
	<lok>Nica Literatura Revuo, 1960-01 &gcirc;is 02, numero 5/3, p.
	108a-110a</lok>
	</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="silik.0o">siliko</ref>,
      <ref cel="pirit.0o">pirito</ref>
    </refgrp>
  </snc>
  <adm>
    Mi ne komprenas la ref al &leftquot;pirito&rightquot;. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_z;</trd>,
    <trd>&c_zh;&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ib;&c_j;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_data;&t_naro;&t_tsheg;&t_ha;&t_rata;&t_shabchu;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">grava</trd>
  <trdgrp lng="de">
    <trd>Grus</trd>,
    <trd>Kies</trd>
  </trdgrp>
  <trd lng="en">gravel</trd>
  <trd lng="fr">gravier</trd>
  <trd lng="ja">&#x30B0;&#x30EB;&#x30B8;&#x30A2;&#x4EBA;  <pr>&#x30B0;&#x30EB;&#x30B8;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>kiezel</trd>,
    <trd>grind</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>murva</trd>,
    <trd>kavics</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&zdot;wir</trd>,
    <trd>t&lstroke;ucze&nacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cascalho</trd>,
    <trd>brita</trd>,
    <trd>areia grossa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">Gruz&iacute;nec</trd>
  <trd lng="cs">Gruz&iacute;nec</trd>
  <trdgrp lng="zh">
    <trd>&#x7802;&#x783E; <pr>sh&#x101;l&#xEC;</pr></trd>,
    <trd>&#x783E; <pr>l&#xEC;</pr></trd>,
    <trd>&#x7802; <pr>sh&#x101;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gruz.0ero">
  <kap>
    <tld/>ero
  </kap>
  <snc mrk="gruz.0ero.HOR">
    <dif>
      Grajno, &scirc;toneto de <ref tip="malprt" cel="gruz.0o.HOR"><tld/>o</ref>:
      <ekz>
        la radoj de la kamionoj dis&jcirc;etadis la moveblajn <tld/>erojn
        <fnt>
          <aut>T. Steele</aut>
          <vrk>La Sunlevi&gcirc;a Ceremonio</vrk>
          <lok>en: Australia Felix, 1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_v;&c_ib;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_z;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">gravier<klr> (grain de)</klr></trd>
</drv>
</art>
