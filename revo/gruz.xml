<art mrk="$Id: gruz.xml,v 1.18 2021/05/28 12:15:00 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>gruz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gruz.0o">
  <kap><tld/>o</kap>
  <snc mrk="gruz.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Sablego el dikaj grajnoj, dispecigitaj &scirc;tonetoj:
      <ekz>
	la <tld/>o en la vojetoj estis hajle blanka
	<fnt>
	<aut>G. Bomans, trad. B. Leopold</aut>,
	<vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr53/sekreto.html">

	La sekreto</url></vrk>,
	<lok>Nica Literatura Revuo, 1960-01 &gcirc;is 02, numero 5/3, p.
	108a-110a</lok>
	</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="silik.0o">siliko</ref>,
      <ref cel="pirit.0o">pirito</ref>
    </refgrp>
  </snc>
  <adm>
    Mi ne komprenas la ref al &leftquot;pirito&rightquot;. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_z;</trd>,
    <trd>&c_zh;&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ib;&c_j;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xFA1;&#xF7C;&#xF0B;&#xF67;&#xFB2;&#xF74;&#xF42;&#xF0B;</trd>
  <trd lng="ca">grava</trd>
  <trdgrp lng="de">
    <trd>Grus</trd>,
    <trd>Kies</trd>
  </trdgrp>
  <trd lng="en">gravel</trd>
  <trd lng="fr">gravier</trd>
  <trdgrp lng="nl">
    <trd>kiezel</trd>,
    <trd>grind</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>murva</trd>,
    <trd>kavics</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&zdot;wir</trd>,
    <trd>t&lstroke;ucze&nacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cascalho</trd>,
    <trd>brita</trd>,
    <trd>areia grossa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">Gruz&#xED;nec</trd>
  <trd lng="cs">Gruz&#xED;nec</trd>
</drv>
</art>
