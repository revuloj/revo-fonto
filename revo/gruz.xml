<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>gruz</rad>/o
</kap>
<drv mrk="gruz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sablego el dikaj grajnoj, dispecigitaj &scirc;tonetoj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="silik.0o">siliko</ref>,
      <ref cel="pirit.0o">pirito</ref>.
    </refgrp>
  </snc>
  <adm>
  Mi ne komprenas la ref al "pirito". <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
     <trd>Grus</trd>,
     <trd>Kies</trd></trdgrp>
  <trd lng="fr">gravier</trd>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
