<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>3</ofc>
<rad>gruz</rad>/o</kap>
<drv mrk="gruz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sablego el dikaj grajnoj, dispecigitaj &scirc;tonetoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="silik.0o">siliko</ref>,
      <ref cel="pirit.0o">pirito</ref>.
    </refgrp>
  </snc>
  <adm>
    Mi ne komprenas la ref al &leftquot;pirito&rightquot;. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_z;</trd>,
    <trd>&c_zh;&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ib;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Grus</trd>,
    <trd>Kies</trd>
  </trdgrp>
  <trd lng="fr">gravier</trd>
  <trdgrp lng="nl">
    <trd>kiezel</trd>,
    <trd>grind</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>murva</trd>,
    <trd>kavics</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>cascalho</trd>, 
   <trd>brita</trd>, 
   <trd>areia grossa</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
