<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>3</ofc>
<rad>erupci</rad>/o
</kap>
<drv mrk="erupci.0o">
<kap><tld/>o</kap>
<snc num="1">
<dif>
Vulkana elsputo. 
</dif>
<trdgrp lng="de">
   <trd>Vulkanausbruch</trd>,
   <trd>Eruption</trd></trdgrp>
<trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
</snc>
<snc num="2">
<dif>
Apero sur la ha&ubreve;to de makuloj, aknoj, veziketoj kc. 
</dif>
</snc>
<trdgrp lng="de">
   <trd>Ausbruch</trd>,
   <trd>Auftreten</trd></trdgrp>
<trd lng="ru">&c_p;&c_o;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je; &c_s;&c_y;&c_p;&c_i;</trd>
</drv>
<drv mrk="erupci.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
Fari <tld/>on.  
</dif>
</snc>
<trdgrp lng="de">
   <trd>hervorbrechen</trd>,
   <trd>ausbrechen</trd>,
   <trd>eruptieren</trd></trdgrp>
<trdgrp lng="ru">
  <trd>&c_i;&c_z;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
  <trd>&c_p;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o; &c_s;&c_y;&c_p;&c_i;)</klr></trd>
</trdgrp>
</drv>
</art>
