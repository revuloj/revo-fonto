<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abak</rad>/o
</kap>
<drv mrk="abak.0o">
  <kap><tld/>o</kap>
  <snc mrk="abak.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Supera plata parto de kolona kapitelo.
    </dif>
    <trdgrp lng="de">
      <trd>S&auml;ulenplatte</trd>,
      <trd>Kapitelldeckplatte</trd>
    </trdgrp>
    <trd lng="en">abacus</trd>
    <trd lng="fr">abaque</trd>
    <trdgrp lng="hu">
      <trd>abakusz</trd>,
      <trd>fejlemez <klr>(oszlopf&odblac;n)</klr></trd>
    </trdgrp>
    <trd lng="nl">abacus</trd>
    <trd lng="ru">&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="abak.0o.kalkulilo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Speco de kalkulilo por plenumi aritmetikajn operaciojn,
      konsistanta &ccirc;efe el buletoj &scirc;oveblaj sur
      stangetoj.
    </dif>
    <trdgrp lng="de">
      <trd>Rechenbrett</trd>,
      <trd>Abakus</trd>
    </trdgrp>
    <trd lng="es">&aacute;baco</trd>
    <trdgrp lng="en">
      <trd>abacus</trd>,
      <trd>calculating frame</trd></trdgrp> 
    <trdgrp lng="fr">
      <trd>abaque</trd>,
      <trd>boulier</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>abakusz</trd>,
      <trd>sz&aacute;mol&oacute;g&eacute;p</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>abacus</trd>,
      <trd>telraam</trd>
    </trdgrp>
    <trd lng="pt">&aacute;baco</trd>
    <trd lng="ru">&c_s;&c_ch;&c_jo;&c_t;&c_y;</trd>
  </snc>
  <snc mrk="abak.0o.grafika.tabela">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Grafika&jcirc;o a&ubreve; tabelo, uzata por anstata&ubreve;i
      kalkulon en la solvo de kelkaj matematikaj
      a&ubreve; te&hcirc;nikaj problemoj.
    </dif>
    <trd lng="de">Rechentafel</trd>
    <trdgrp lng="en">
      <trd>nomogram</trd>,
      <trd>chart</trd></trdgrp> 
    <trdgrp lng="fr">
      <trd>abaque</trd>,
      <trd>nomogramme</trd>,
      <trd>table</trd></trdgrp> 
    <trdgrp lng="hu">
      <trd>nomogram</trd>,
      <trd>sz&aacute;mol&oacute;t&aacute;bla</trd>
    </trdgrp>
    <trd lng="nl">abacus</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_m;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_je; &c_t;&c_a;&c_b;&c_l;&c_i;&c_c;&c_y;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
