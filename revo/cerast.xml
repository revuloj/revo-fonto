<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cerast.xml,v 1.10 2022/03/14 15:25:39 revo Exp $">
<kap>
  <rad>cerast</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="cerast.0o">
  <kap><tld/>o</kap>
  <snc mrk="cerast.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="serpen.0o" lst="voko:serpentoj">Serpento</ref>
      el la sahara kaj arabuja sablaj dezertoj, kun du
      kornsimilaj pintoj sur la kapo;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="vipure.0oj">viperedoj</ref>
      (<trd lng="la">Cerastes</trd>):
      <ekz>
        serpento &ccirc;e la vojo, <tld lit="C"/>o &ccirc;e la irejo
        <fnt><bib>MT</bib>, <lok>&Gen; 49:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">escur&ccedil;&oacute; banyut del desert</trd>
  <trd lng="de">Hornviper</trd>
  <trdgrp lng="en">
    <trd>Saharan horned viper</trd>,
    <trd>horned desert viper</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&eacute;raste</trd>,
    <trd>vip&egrave;re &agrave; cornes</trd>
  </trdgrp>
  <trd lng="nl">hoornadder</trd>
  <trd lng="it">ceraste cornuta</trd>
  <trd lng="pl">&zdot;mija rogata</trd>
  <trd lng="uk">&#x440;&#x43E;&#x433;&#x430;&#x442;&#x430; &#x433;&#x430;&#x434;&#x44E;&#x43A;&#x430;</trd>
</drv>
</art>

