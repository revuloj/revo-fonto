<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sik.0o">
  <kap><tld/>o</kap>
  <snc mrk="sik.0o.REL">
    <dif>
      Ano de <tld/>ismo.
    </dif>
  </snc>
  <trd lng="fr">sikh</trd>
</drv>

<drv mrk="sik.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="sik.0ismo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Unudia <ref tip="super" cel="religi.0o.REL">religio</ref> fondita en
      la 16a jarcento en Pan&gcirc;abo, kun doktrino origine difinita de &gcirc;iaj
      sinsekvaj dek majstroj:
      <ekz>
        la guruo ordonis, ke <ctl>Guru Granth Sahib</ctl>,
        la sankta libro de <tld/>ismo, estu la spirita a&ubreve;toritato
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;sikismo&rightquot;,
        2005-10-18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sikhismus</trd>
  <trd lng="en">sikhism</trd>
  <trd lng="es">sikhismo</trd>
  <trd lng="fr">sikhisme</trd>
  <trd lng="it">sikhismo</trd>
  <trd lng="nl">sikhisme</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>