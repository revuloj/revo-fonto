<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sirok</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sirok.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Varmega, desudorienta <ref tip="super" cel="vent.0o.MET">vento</ref>
      el Saharo, kiu trablovas la Mediteranean Afrikan bordon:
      <ekz>
        ne frapos ilin <tld/>o nek la suno<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;&c_o;&c_k;&c_a;</trd>
  <trd lng="de">Schirokko</trd>
  <trdgrp lng="fr">
    <trd>sirocco</trd>
  </trdgrp>
  <trd lng="nl">sirocco</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_o;&c_k;&c_k;&c_o;</trd>
</drv>
</art>
