  <trd lng="hu">k&ouml;telez&odblac;</trd>
  <trd lng="id">wajib</trd>
  <trdgrp lng="pl">
    <trd>obowi&aogonek;zkowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
