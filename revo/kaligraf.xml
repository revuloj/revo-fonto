<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaligraf.xml,v 1.11 2022/03/21 17:51:56 revo Exp $">
<kap>
  <rad>kaligraf</rad>/o <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="kaligraf.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaligraf.0o.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="dif" cel="skrib.bel0isto.BELA">Belskribisto</ref>:
      <ekz>
        la Luksemburga ju&gcirc;istaro oficiale konfesis la ekzistadon de la grafologio,
        turnante sin al la redaktoro de la dirita gazeto en unu ju&gcirc;a afero, en kiu
        ordinaraj <tld/>oj ne povis helpi
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zhong You</nom> (151&#x202F;&ndash;&#x202F;230), fama <tld/>o el la Periodo de Tri Regnoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 70a, p. 496a,
        noto</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kaligrafi.0isto">kaligrafiisto</ref>
  </snc>
  <trd lng="ca">cal&middot;l&iacute;graf</trd>
  <trdgrp lng="cs">
    <trd>kaligraf</trd>,
    <trd>krasopisec</trd>
  </trdgrp>
  <trd lng="de">Kalligraf</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_sh;&a_n;&a_w;&f_y;&a_s;</trd>,
    <trd>&a_kh;&a_T;&a_A;&a_T;</trd>
  </trdgrp>
  <trd lng="fr">calligraphe</trd>
  <trd lng="nl">kalligraaf</trd>
  <trd lng="pl">kaligraf</trd>
  <trd lng="pt">cal&iacute;grafo</trd>
  <trd lng="ro">caligraf</trd>
  <trd lng="sk">krasopisec</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43B;&#x456;&#x433;&#x440;&#x430;&#x444;</trd>,
    <trd>&#x43A;&#x440;&#x430;&#x441;&#x43D;&#x43E;&#x43F;&#x438;&#x441;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

