<drv mrk="strecx.0a">
  <kap><tld/>a</kap>
  <snc mrk="strecx.0a.strikta">
    <dif>
      <tld lit="S"/>ita, <ref cel="strikt.0a.TEKS">strikta</ref>:
      <ekz>
        <tld/>a vesto;
      </ekz>
      <ekz>
        ekvidante lian ornaman epoleton mi rigidi&gcirc;is en <tld/>a salutpozo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strikt.0a">strikta</ref>
    </refgrp>
    <trd lng="de">gespannt</trd>
    <trdgrp lng="hu">
      <trd>feszes</trd>,
      <trd>szoros</trd>
    </trdgrp>
    <trd lng="nl">strak</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="strecx.0a.postulema">
    <dif>
      Ekstreme energia, forto<ref cel="postul.0ema">postulema</ref>:
      <ekz>
        <tld/>a laboro
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a batalo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a historio
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> a&ubreve;skultis la interparoladon kun <tld/>a atento
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la cirkonstancoj <klr>[&#8230;]</klr> ka&ubreve;zis al li multajn horojn de malgaja kaj
        <tld/>a medito
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        min ekkaptis <tld/>a scivolemo
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kapt.atento0a">atentokapta</ref>,
       <ref cel="spir.0kapta">spirkapta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anstrengend</trd>,
      <trd>spannend</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fesz&uuml;lt</trd>,
      <trd>meger&odblac;ltet&odblac;</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bajo tensi&oacute;n</trd>,
    <trd>tenso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sous tension</trd>,
    <trd>tendu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43F;&c_r;&c_u;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_a;&c_t;&#x44F;&c_g;&#x43D;&c_u;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="strecx.0o">
  <kap><tld/>o</kap>
  <snc mrk="strecx.0o.stato">
    <dif>
      Stato en kiu oni forte uzas siajn korpajn a&ubreve; spiritajn fortojn:
      <ekz>
        teni la okulojn en <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu transmovas &scirc;tonojn, tiu faras al si difekton per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 10:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i laboris kun granda <tld/>o de sia cerbo kaj de sia volo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de &scirc;ia viza&gcirc;o malgrandi&gcirc;is
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la leganto pluiras facile, sen la <tld/>o de malfacila kompreno
        <fnt>
          <aut>C. Minnaja</aut>,
          <vrk><url ref="&Monato;/rec/malm.html">Meritanta ne nur
          reeldonon</url></vrk>,
          <bib>Monato</bib>,
          <lok>2001:07</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i gravitas &ccirc;irka&ubreve; malsana 70-jarulo, kiu per granda
        forto<tld/>o decidas spiti
        sian malsanon por influi la decidon pri la akcepto a&ubreve;
        malakcepto de nova fabriko
        <fnt>
          <aut>Pejno S.</aut>:
          <vrk><url ref="&Monato;/rec/kviv.html">Balda&ubreve;
          &ccirc;i-teatre</url></vrk>,
          <bib>Monato</bib>,
          <lok>1999:05</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="tensi.0o.strecxo">tensio<sncref/></ref>
    </refgrp>
  <trdgrp lng="de">
    <trd>Anstrengung</trd>,
    <trd>Anspannung</trd>
  </trdgrp>
    <trd lng="en">stress</trd>
    <trdgrp lng="nl">
      <trd>spanning</trd>,
      <trd>stress</trd>
    </trdgrp>
    <trd lng="hu">er&odblac;fesz&iacute;t&eacute;s</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_je;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_u;&c_g;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_ib;&c_l;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estiramiento</trd>,
    <trd>tensi&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;tirement</trd>,
    <trd>tension</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43F;&c_r;&c_u;&c_g;&c_a;</trd>,
    <trd>&#x43D;&c_a;&#x43F;&c_r;&c_u;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_t;&#x44F;&c_g;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_t;&#x44F;&c_g;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&c_i;&c_t;&#x44F;&c_g;&c_a;&#x43D;&#x43D;&#x44F; (&c_sh;&c_i;&c_ji;)</trd>,
    <trd>&c_v;&c_i;&#x43F;&c_i;&#x43D;&c_a;&#x43D;&#x43D;&#x44F; (&c_g;&c_r;&c_u;&c_d;&c_je;&c_j;)</trd>,
    <trd>&#x43D;&c_a;&#x43A;&c_r;&c_u;&c_ch;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F; (&c_g;&#x43E;&c_d;&c_i;&#x43D;&#x43D;&c_i;&#x43A;&c_a;)</trd>,
    <trd>&c_z;&c_u;&c_s;&c_i;&#x43B;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="strecx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="strecx.0igxi.KOMUNE">
    <dif>
      Pro forto <ref cel="etend.0igxi">etendi&gcirc;i</ref> trans sia normala formo,
      distiri&gcirc;i:
      <ekz>
        lia kolo kaj kapo <tld/>i&gcirc;is anta&ubreve;en
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj &gcirc;ermoj rapide elkreskis el la profunda grundo <klr>[kaj]</klr> <tld/>i&gcirc;is
        supren
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la po&scirc;testro, eniras <tld/>i&gcirc;inte, en uniformo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la na&gcirc;membrano inter la fingroj tiel <tld/>i&gcirc;is, ke &gcirc;i dis&scirc;iri&gcirc;is kaj la sango
        komencis flui
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ielarko el<tld/>i&gcirc;is el la nigra nubo en la bluan aeron
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i&gcirc;i el la ha&ubreve;to</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">aus der Haut fahren</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="es"><ind>estirar</ind>se</trd>
  <trd lng="fr">s'<ind>&eacute;tirer</ind></trd>
  <trd lng="uk">&#x43D;&c_a;&#x43F;&c_r;&c_u;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="strecx.0ita">
  <kap><tld/>ita</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="strecx.0ita.angulo">
    <dif>
      <klr>(p.p. <ref cel="angul.0o.figuro">angulo</ref>)</klr>
      Kies <ref cel="later.0o.angulo">lateroj</ref> estas
      <ref cel="kontraux.0a.duonrekto">kontra&ubreve;aj</ref>:
      <ekz>
        <tld/>ita angulo egalas al du ortoj, alidire &gcirc;ia mezuro
        estas <frm>180&#x2DA;</frm>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>flat<klr tip="ind"> (angle)</klr></trd>,
      <trd>straight<klr tip="ind"> (angle)</klr></trd>
    </trdgrp>
    <trd lng="de">gestreckt<klr tip="ind"> (-er Winkel)</klr></trd>
    <trd lng="es"><klr>(&aacute;ngulo) </klr><ind>llano</ind></trd>
    <trd lng="fr"><klr tip="ind">(angle) </klr><ind>plat</ind></trd>
    <trd lng="hu">egyenes<klr tip="ind"> (sz&ouml;g)</klr></trd>
    <trd lng="nl">gestrekt<klr tip="ind"> (v.hoek)</klr></trd>    
    <trd lng="pl"><klr tip="ind">(k&aogonek;t)
    </klr><ind>p&oacute;&lstroke;pe&lstroke;ny</ind></trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_y;&c_j;<klr tip="ind">
    (&c_u;&c_g;&c_o;&c_l;)</klr></trd>
    <trdgrp lng="zh">
      <trd>&#x5E73;&#30340;</trd>
      <trd>&#x76F4;&#30340;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_ja;&c_t;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_n;&c_u;&c_t;&c_y; <klr>(&c_v;&c_u;&c_g;&c_a;&c_l;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43F;&c_r;&c_u;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_a;&c_t;&#x44F;&c_g;&#x43D;&c_u;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="strecx.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="strecx.mal0i.KOMUNE">
    <dif>
      Igi malrigida, ne tirata, pli mola, pli komforta:
      <ekz>
        la rimeno de lia tornistro lin premas <klr>[&#8230;]</klr>, li mal<tld/>is &gcirc;in, sed
        nenio helpis
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la laringo trovi&gcirc;as pluraj muskoloj, kies tasko estas <tld/>i a&ubreve; mal<tld/>i,
        proksimigi a&ubreve; malproksimigi la vo&ccirc;ajn kordojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        longa blanka tuko mal<tld/>e kaj malpeze kovris &scirc;iajn belajn membrojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipa kapelano vizitas lin konsole kaj kamarado iom
        mal<tld/>as al li la premegantajn katenojn
        <fnt>
          <aut>H. Melville, trad. B. Gahler</aut>,
          <vrk><url ref="&DonH;Revuoj/np/np5703/budd.html">
          Billy -- katenita</url></vrk>,
          <lok>Norda Prismo, 1957:3, p. 138a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_l;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>,
    <trd>&c_a;&c_d;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>distender</trd>,
    <trd>aflojar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;serrer</trd>,
    <trd>d&eacute;tendre</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_s;&#x43B;&c_a;&c_b;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_s;&#x43B;&c_a;&c_b;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i; (&#x43A;&c_u;&c_r;&#x43E;&#x43A;)</trd>
  </trdgrp>
</drv>

<drv mrk="strecx.mal0igxi">
  <kap>mal<tld/>i&gcirc;i,
  <var><kap>sen<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="strecx.mal0igxi.FIZ">
    <dif>
      Elasti&gcirc;i, moli&gcirc;i, etendi&gcirc;i la&ubreve; sia normala, ne perforte vastigita 
      formo:
      <ekz>
        miaj tendenoj mal<tld/>i&gcirc;as, la ponardo falas el miaj manoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis kiel instrumento, kies kordoj mal<tld/>i&gcirc;is kaj ne povas plu soni
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempo <klr>[&#8230;]</klr> entenas unuecan longecon,
        sed en nia penso &gcirc;i <tld/>i&gcirc;as kaj mal<tld/>i&gcirc;as
        la&ubreve; nia deziro a&ubreve; penso
        <fnt>
          <aut>F. Sammut, trad. C. Mallia</aut>,
          <vrk>La Malta Revo</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="strecx.mal0igxi.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="malprt" cel="ripoz.0i.nenifari">Ripozi</ref>, 
      <ref tip="vid" cel="trankv.0igxi">trankvili&gcirc;i</ref>,
      forigante animajn <tld/>ojn:
      <ekz>
        estos por mi pli refre&scirc;ige <klr>[&#8230;]</klr> mal<tld/>i&gcirc;i en via banujo
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni devas foje-foje sen<tld/>i&gcirc;i, &ccirc;ar aliokaze fari&gcirc;os idioto
        <fnt><aut>J. S&aacute;rk&ouml;zi</aut>, <vrk><url ref="http://mek.niif.hu/04100/04194/04194.htm"/>Filmo
        por 
        pli ol 16 jaruloj kaj aliaj noveloj</vrk>, 
        <lok>1995</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_l;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>entspannen</ind></trd>
  <trdgrp lng="es">
    <trd><ind>relajar</ind>se</trd>,
    <trd><ind>tranquilizar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;tendre</ind></trd>,
    <trd>se <ind>rel&acirc;cher</ind></trd>,
    <trd>se <ind>relaxer</ind></trd>
  </trdgrp>
</drv>

<drv mrk="strecx.man0o">
  <kap>
    man<tld/>o
  </kap>
  <snc mrk="strecx.man0o.spano">
    <dif>
      <ref tip="dif" cel="span.0o.KOMUNE">Spano<sncref/></ref>:
      <ekz>
        ili faris la surbrusta&jcirc;on duobla, man<tld/>o estis &gcirc;ia longo, kaj man<tld/>o &gcirc;ia
        lar&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 39:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mezo de tiu &scirc;tono <klr>[&#8230;]</klr> estas farita truo, ulnon profunda kaj
        malpli ol man<tld/>on lar&gcirc;a
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">Pri la centra loko de la
          Tero, kie Kristo estis krucumita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_d;&c_z;&c_ja;</trd>
  <trd lng="de">Handspanne</trd>
</drv>


<drv mrk="strecx.sub0i">
  <kap>sub<tld/>i</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <uzo tip="klr">(fakula &jcirc;argono)</uzo>
  <snc mrk="strecx.sub0i.MAT">
    <dif>
      <klr>(p.p. <ref cel="ark.0o.segmento">arko</ref> kaj
      <ref cel="sxnur.0o.cirklo">&scirc;nuro</ref> de la sama kurbo)</klr>
      Diri, ke la &scirc;nuro sub<tld/>as la arkon, signifas, ke
      ili havas samajn <ref cel="rand.0o.figuro">randojn</ref>:
      <ekz>
        diametro sub<tld/>as duonon de la cirklo;
      </ekz>
      <ekz>
        du <ref cel="egal.0a.izometria">egalaj</ref> &scirc;nuroj
        sub<tld/>as egalajn arkojn.
      </ekz>
    </dif>
    <rim>
      Travidebla metaforo: la &scirc;nuro <tld/>as la arkon de sube,
      kvaza&ubreve; temus pri pafarko pafanta al flugantaj birdoj.
    </rim>
    <trd lng="de">unterspannen</trd>
    <trd lng="en">subtend<klr tip="ind"> (an arc)</klr></trd>
    <trd lng="es">subtender<klr tip="ind"> un arco (math.)</klr></trd>
    <trd lng="fr">sous-tendre<klr tip="ind"> (math.)</klr></trd>
    <trd lng="hu">bez&aacute;r<klr tip="ind"> (&iacute;vet)</klr></trd>
    <trd lng="ru">&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="ind">
    (&c_d;&c_u;&c_g;&c_u;)</klr></trd>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_d;&c_u;&c_g;&c_u;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&#x44F;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#8217;&c_jeu;&c_d;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="strecx.forto0o">
  <kap>forto<tld/>o</kap>
  <snc mrk="strecx.forto0o.KOMUNE">
    <dif>
      Momenta
      <ref tip="vid" cel="pen.0o.KOMUNE">peno</ref>,
      per kiu oni provas superi malfacila&jcirc;on:
      <ekz>
        oni sukcesis nur per lasta, granda forto<tld/>o
        <fnt>
          <aut>J. S. Larsen </aut>,
          <vrk><url
          ref="http://www.liberafolio.org/Members/jens_s_larsen/kiu-efektive-estis-blinkenberg">
          Kiu efektive estis Blinkenberg?</url></vrk>,
          <lok>libera folio, 2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj publika&jcirc;oj <klr>[&#8230;]</klr> estas pli bonaj kaj bezonas malpli da
        forto<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Salomon</aut>
          <vrk><url ref="&Monato;/2003/008016.html">La mondolingvo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        hejmecaj drinkejoj igas forgesi la biciklajn forto<tld/>ojn dumtagajn
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2004/008266.html">Romantika itinero por spertaj
          biciklantoj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pen.0o">peno</ref>,
      <ref cel="streb.0o.KOMUNE">strebo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_s;&c_ib;&c_l;&c_a;&c_w;)</klr></trd>,
    <trd>&c_v;&c_y;&c_s;&c_ib;&c_l;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="es">esfuerzo</trd>
  <trd lng="fr">effort</trd>
  <trd lng="uk">&c_z;&c_u;&c_s;&c_i;&#x43B;&#x43B;&#x44F;</trd>
</drv>
