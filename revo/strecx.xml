<drv mrk="strecx.0a">
  <kap><tld/>a</kap>
  <snc mrk="strecx.0a.strikta">
    <dif>
      <tld lit="S"/>ita, <ref cel="strikt.0a.TEKS">strikta</ref>:
      <ekz>
        <tld/>a vesto;
      </ekz>
      <ekz>
        ekvidante lian ornaman epoleton mi rigidi&gcirc;is en <tld/>a salutpozo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strikt.0a">strikta</ref>
    </refgrp>
    <trd lng="de">gespannt</trd>
    <trdgrp lng="hu">
      <trd>feszes</trd>,
      <trd>szoros</trd>
    </trdgrp>
    <trd lng="nl">strak</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="strecx.0a.postulema">
    <dif>
      Ekstreme energia, forto<ref cel="postul.0ema">postulema</ref>:
      <ekz>
        <tld/>a laboro
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a batalo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a historio
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> a&ubreve;skultis la interparoladon kun <tld/>a atento
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la cirkonstancoj <klr>[&#8230;]</klr> ka&ubreve;zis al li multajn horojn de malgaja kaj
        <tld/>a medito
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        min ekkaptis <tld/>a scivolemo
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kapt.atento0a">atentokapta</ref>,
       <ref cel="spir.0kapta">spirkapta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anstrengend</trd>,
      <trd>spannend</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fesz&uuml;lt</trd>,
      <trd>meger&odblac;ltet&odblac;</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bajo tensi&oacute;n</trd>,
    <trd>tenso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sous tension</trd>,
    <trd>tendu</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24373;&#12426;&#12388;&#12417;&#12383;  <pr>&#12399;&#12426;&#12388;&#12417;&#12383;</pr></trd>,
    <trd>&#32202;&#24373;&#12375;&#12383;  <pr>&#12365;&#12435;&#12385;&#12423;&#12358;&#12375;&#12383;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
