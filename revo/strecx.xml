    <trd>napr&eogonek;&zdot;ony</trd>,
    <trd>wyt&eogonek;&zdot;ony</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21507;&#32039; <pr>ch&imacron;j&#464;n</pr></trd>,
    <trd>&#21507;&#32202; <pr>ch&imacron;j&#464;n</pr></trd>,
    <trd>&#32039;&#24352; <pr>j&#464;nzh&amacron;ng</pr></trd>,
    <trd>&#32202;&#24373; <pr>j&#464;nzh&amacron;ng</pr></trd>,
    <trd>&#32039;&#32503; <pr>j&#464;nb&emacron;ng</pr></trd>,
    <trd>&#32202;&#32323; <pr>j&#464;nb&emacron;ng</pr></trd>,
    <trd>&#32039;&#32503;&#32503; <pr>j&#464;nb&emacron;ngb&emacron;ng</pr></trd>,
    <trd>&#32202;&#32323;&#32323; <pr>j&#464;nb&emacron;ngb&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="strecx.0o">
  <kap><tld/>o</kap>
  <snc mrk="strecx.0o.stato">
    <dif>
      Ago <tld/>i kaj ties rezulta stato:
      <ekz>
        la <tld/>o de muskoloj sur lia viza&gcirc;o malpliforti&gcirc;as
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu transmovas &scirc;tonojn, tiu faras al si difekton per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Pre; 10:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiris min anta&ubreve;en subita <tld/>o de la &ccirc;eno
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fortoj de la salamandro faris lastan <tld/>on, la ha&ubreve;to subite ekkrakis
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de &scirc;ia viza&gcirc;o malgrandi&gcirc;is
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        malsana 70-jarulo, kiu per granda forto<tld/>o decidas spiti
        sian malsanon
        <fnt>
          <aut>Pejno S.</aut>,
          <vrk><url ref="&Monato;/1999/kviv.html">
          Balda&ubreve; &ccirc;i-teatre</url></vrk>,
          <bib>Monato</bib>
          <lok>1999/05, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        teni la okulojn en <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Spannung</trd>,
      <trd>Anspannung</trd>
    </trdgrp>
    <trd lng="en">stress</trd>
    <trd lng="hu">er&odblac;fesz&iacute;t&eacute;s</trd>
    <trdgrp lng="nl">
      <trd>spanning</trd>,
      <trd>stress</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_je;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_u;&c_g;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="strecx.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Psika <tld/>o; <ref tip="dif" cel="strecx.anim0o">anim<tld/>o</ref>:
      <ekz>
        &scirc;i laboris kun granda <tld/>o de sia cerbo kaj de sia volo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la leganto pluiras facile, sen la <tld/>o de malfacila kompreno
        <fnt>
          <aut>C. Minnaja</aut>,
          <vrk><url ref="&Monato;/2001/malm.html">Meritanta ne nur
          reeldonon</url></vrk>,
          <bib>Monato</bib>,
          <lok>2001/07, 28-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum du horoj &scirc;i estis sensukcese ser&ccirc;anta <klr>[&#8230;]</klr>
        fine, ru&gcirc;a pro <tld/>o kaj honto, &scirc;i senpove konfesis:
        <klr>[&#8230;]</klr> mi ne povas trovi
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
      <ekz>
        delegitoj, kies hejmaj lingvoj ne estas oficialaj, kun tremanta vo&ccirc;o rapide
        tralegas sian anta&ubreve;e preparitan tekston kaj post tiu <tld/>o trankvile
        eksidas kaj ne plu intervenas
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la triobla relparo de la oratora intelekta aktiveco <klr>[&#8230;]</klr> postulas
        grandan intelektan <tld/>on
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu kun <tld/>o atendis, kiu gajnos la kokinon
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="stres.0o">streso</ref>,
      <ref cel="suspens.0o">suspenso</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>psychiche Anspannung</trd>,
      <trd>Stress</trd>,
      <trd>Nervenanspannung</trd>,
      <trd>Gespanntheit</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
