<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gravur</rad>/i
</kap>
<drv mrk="gravur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Desegni sur ligno, &scirc;tono, metalo kc per pinta &scirc;tala ilo a&ubreve; per acido. 
    </dif>
    <refgrp tip="vid">
      <ref cel="skrib.en0i">enskribi</ref>,
      <ref cel="trancx">entran&ccirc;i</ref>,
      <ref cel="cxiz.0i">&ccirc;izi</ref>,
      <ref cel="gilosx.0i">gilo&scirc;i</ref>,
      <ref cel="strek.0i">streki</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>gravieren</trd>,
     <trd>stechen</trd>,
     <trd>ritzen</trd>,
     <trd>einschneiden</trd></trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fari fortan kaj longeda&ubreve;ran impreson sur la spiriton:
      <ekz><tld/>i ion en sian memoron, en la koro;</ekz>
      <ekz>versoj <tld/>i&gcirc;as pli facile en la memoro ol prozo.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrib.en0i">enskribi</ref>,
      <ref cel="not.0i">noti</ref>,
      <ref cel="gard.0i">gardi</ref>,
      <ref cel="konser.0i">konservi</ref>,
      <ref cel="stamp">enstampi</ref>,
      <ref cel="radik.en0i">enradiki</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>einbrennen</trd>,
     <trd>einmei&szlig;eln</trd></trdgrp>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;., &c_o; &c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_i;)</klr></trd>
  </snc>
  <trd lng="en">engrave</trd>
  <trd lng="fr">graver</trd>
  <trd lng="sv">gravera</trd>
</drv>
<drv mrk="gravur.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Desegna&jcirc;o per <tld/>ado. 
    </dif>
  </snc>
    <trdgrp lng="de">
     <trd>Gravur</trd>,
     <trd>Grav&uuml;re</trd>,
     <trd>Stich</trd>,
     <trd>eingeschnittene Schrift</trd></trdgrp>
  <trd lng="fr">gravure</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a;</trd>
</drv>
<drv mrk="gravur.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="grifel.0o.gravurilo">grifelo.</ref>
  </snc>
  <adm>
  Gxi jam estas cxe Kabe, kiu donas alian sencon al grifelo. 
  Povas esti, ke aperis pli novaj uzoj (ekz-e "lumdiska gravurilo"?). 
  <aut>MB</aut>
  </adm>
    <trdgrp lng="de">
     <trd>Flacheisen</trd>,
     <trd>Stechmeisel</trd>,
     <trd>Graviermesser</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_je;&c_c; <klr>(&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r;&c_a;)</klr></trd>,
    <trd>&c_sh;&c_t;&c_i;&c_h;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
