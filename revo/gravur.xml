<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gravur.xml,v 1.27 2022/04/30 08:42:40 revo Exp $">
<kap><ofc>*</ofc>
<rad>gravur</rad>/i</kap>
<drv mrk="gravur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gravur.0i.desegni">
    <dif>
      <ref tip="super" cel="desegn.0i.KOMUNE">Desegni</ref> sur ligno, &scirc;tono, metalo kc per
      pinta malmola ilo a&ubreve; per
      acido:
      <ekz>
        prenu du oniksajn &scirc;tonojn, kaj <tld/>u sur ili la nomojn de la filoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[sur]</klr> la fingringo <klr>[&#x2026;]</klr> sin trovas <tld/>ita sentenco
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        honorante lian piecon <klr>[ni]</klr> <tld/>os la jenan surskribon en la tomboj
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hakilo en<tld/>is siajn runojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrib.en0i">enskribi</ref>,
      <ref cel="trancx">entran&ccirc;i</ref>,
      <ref cel="cxiz.0i">&ccirc;izi</ref>,
      <ref cel="gilosx.0i">gilo&scirc;i</ref>,
      <ref cel="ksilog.0i">ksilografi</ref>,
      <ref cel="strek.0i">streki</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gravieren</trd>,
      <trd>stechen</trd>,
      <trd>ritzen</trd>,
      <trd>einschneiden</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>graveren</trd>,
      <trd>griffen</trd>
    </trdgrp>
    <trd lng="pl">grawerowa&cacute;</trd>
    <trd lng="pt">gravar<klr tip="amb"> (em madeira, pedra, metal
    etc)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gravur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fari fortan kaj longeda&ubreve;ran <ref cel="impres.0o">impreso</ref>n a&ubreve; bildon:
      <ekz>
        tiajn ravantajn pentra&jcirc;ojn povis <tld/>i nur la tempo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>igis en sian cerbon la figuron
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        via &ccirc;arma viza&gcirc;o &ccirc;iam restos <tld/>itaj en mia memoro
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiun]</klr> seriozecon de lia mieno <klr>[&#x2026;]</klr> akraj animdoloroj
        <tld/>is sur lia vireca viza&gcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        post malmultaj monatoj japanaj <nom>Hitachi</nom>-teknikistoj <tld/>is la
        bondeziron &leftquot;<nom>Peace '91</nom>&rightquot; forportante kelkdekon da
        sulfuratomoj el kristalo da molibdena disulfato
        <fnt><aut>Gianfranco CAZZARO</aut>
        <vrk><url ref="&Monato;/005286.html">Mikroskope vastaj
        horizontoj</url></vrk><bib>Monato</bib><lok>n-ro
        02/1997</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> bildoj de sakura-floroj estas jam
        en<tld/>i&gcirc;intaj en iliaj cerboj kun la sentoj kaj traviva&jcirc;oj
        de &gcirc;ojo kaj mal&gcirc;ojo
        <fnt><aut>a&ubreve;toro nekonata</aut>
        <vrk><url ref="&Monato;/006633.html">Sakura-floroj</url></vrk>
        <bib>Monato</bib><lok>n-ro 03/2000</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrib.en0i">enskribi</ref>,
      <ref cel="intern.0igi.KOMUNE">internigi<sncref/></ref>,
      <ref cel="not.0i">noti</ref>,
      <ref cel="gard.0i">gardi</ref>,
      <ref cel="konser.0i">konservi</ref>,
      <ref cel="stamp.0i">enstampi</ref>,
      <ref cel="radik.en0i">enradiki</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>einbrennen</trd>,
      <trd>einmei&szlig;eln</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bev&eacute;s <klr tip="ind">(&aacute;tv. is)</klr></trd>,
      <trd>grav&iacute;roz</trd>
    </trdgrp>
    <trd lng="nl">griffen</trd>
    <trd lng="pt">gravar<klr tip="amb"> (f)</klr></trd>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;
    <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;., &c_o;
    &c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_i;)</klr></trd>
  </snc>
  <trd lng="ca">gravar</trd>
  <trdgrp lng="cs">
    <trd>grav&iacute;rovat</trd>,
    <trd>vr&yacute;vat</trd>
  </trdgrp>
  <trd lng="en">engrave</trd>
  <trd lng="es">grabar</trd>
  <trd lng="fr">graver</trd>
  <trd lng="sk">vyry&tcaron;</trd>
  <trd lng="sv">gravera</trd>
  <trdgrp lng="uk">
    <trd>&#x491;&#x440;&#x430;&#x432;&#x456;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x491;&#x440;&#x430;&#x432;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="gravur.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="desegn.0ajxo">Desegna&jcirc;o</ref> per <tld/>ado:
      <ekz>
        ligno<tld/>oj de <nom>Christian Winther</nom>
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        ligno<tld/>o <klr>[&#x2026;]</klr> prezentis aeran fenomenon, kiu aperis en la urbo
        Kolonjo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povoscias fari objektojn el oro, ar&gcirc;ento, kupro, fero, &scirc;tono, kaj ligno
        <klr>[&#x2026;]</klr>, fari &ccirc;iajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 2:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> devas ne opinii la Diecon simila al <klr>[&#x2026;]</klr> <tld/>a&jcirc;o de
        homa arto
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 17:29</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="epigra.0o.ARKE">epigrafo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="ca">gravat</trd>
  <trdgrp lng="de">
    <trd>Gravur</trd>,
    <trd>Grav&uuml;re</trd>,
    <trd>Stich</trd>,
    <trd>eingeschnittene Schrift</trd>
  </trdgrp>
  <trd lng="en">engraving</trd>
  <trd lng="es">grabado</trd>
  <trd lng="fr">gravure</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;set</trd>,
    <trd>metszet</trd>
  </trdgrp>
  <trd lng="nl">gravure</trd>
  <trd lng="pt">gravura<klr tip="amb"> (coisa gravada)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a;</trd>
  <trd lng="uk">&#x491;&#x440;&#x430;&#x432;&#x44E;&#x440;&#x430;</trd>
</drv>

<drv mrk="gravur.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="gravur.0ilo.grifelo">
    <dif>
      Ilo kun malmola pinto por <tld/>ado; <ref tip="dif" cel="grifel.0o.gravurilo">grifelo</ref>.
    </dif>
    <refgrp tip="vid">
       <ref cel="cxiz.0ileto">&ccirc;izileto</ref>,
       <ref cel="cizel.0ilo">cizelilo</ref>,
       <ref cel="skalpe.0o">skalpelo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/c/c7/Stichel_2a.jpg/320px-Stichel_2a.jpg" prm="CC-BY-SA-3.0"><tld/>iloj
        <fnt>
          <aut>Manfred Br&uuml;ckels</aut>
          <lok><url ref="&WCW;/File:Stichel_2a.jpg">&WCW;/File:Stichel_2a.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">bur&iacute;</trd>
    <trdgrp lng="de">
      <trd>Stichel</trd>,
      <trd>Graviermesser</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>engraving tool</trd>,
      <trd>burin</trd>,
      <trd>graver</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>karcol&oacute;t&udblac;</trd>,
      <trd>v&eacute;s&odblac;t&udblac;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>graveernaald</trd>,
      <trd>graveerstift</trd>
    </trdgrp>
    <trd lng="pt">buril</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_z;&c_je;&c_c; <klr>(&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r;&c_a;)</klr></trd>,
      <trd>&c_sh;&c_t;&c_i;&c_h;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">rydlo</trd>
    <trdgrp lng="cs">
      <trd>ryc&iacute; jehla</trd>,
      <trd>rydlo</trd>
    </trdgrp>
  </snc>
  <snc mrk="gravur.0ilo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por registri datumojn
      sur <ref cel="disk.lum0o">lumdisko</ref> per lasero:
      <ekz>
        la apero de la kompakta disko plifaciligis ilian taskon, sufi&ccirc;as ja disponi pri
        komputilo kun <tld/>ilo kaj presilo por eldoni tiom, kiom oni bezonas
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2006/008921.php">Kontra&ubreve;le&gcirc;a
          kopiado</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Brenner <klr>(CD)</klr></trd>
    <trd lng="en">optical writer <klr>(CD)</klr></trd>
    <trd lng="fr">graveur de disque optique</trd>
  </snc>
  <trd lng="uk">&#x440;&#x456;&#x437;&#x435;&#x446;&#x44C;</trd>
</drv>
</art>
