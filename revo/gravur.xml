<kap><ofc>*</ofc>
<rad>gravur</rad>/i</kap>
<drv mrk="gravur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gravur.0i.desegni">
    <dif>
      <ref tip="super" cel="desegn.0i.KOMUNE">Desegni</ref> sur ligno, &scirc;tono, metalo kc per
      pinta malmola ilo a&ubreve; per acido:
      <ekz>
        prenu du oniksajn &scirc;tonojn, kaj <tld/>u sur ili la nomojn de la filoj
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 28:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[sur]</klr> la fingringo <klr>[&#8230;]</klr> sin trovas <tld/>ita sentenco
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        honorante lian piecon <klr>[ni]</klr> <tld/>os la jenan surskribon en la tomboj
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hakilo en<tld/>is siajn runojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="cxiz.0i">&ccirc;izi</ref>,
      <ref cel="skrib.en0i">enskribi</ref>,
      <ref cel="trancx">entran&ccirc;i</ref>,
      <ref cel="gilosx.0i">gilo&scirc;i</ref>,
      <ref cel="ksilog.0i">ksilografi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gravieren</trd>,
      <trd>stechen</trd>,
      <trd>ritzen</trd>,
      <trd>einschneiden</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>graveren</trd>,
      <trd>griffen</trd>
    </trdgrp>
    <trd lng="pl">grawerowa&cacute;</trd>
    <trd lng="pt">gravar<klr tip="amb"> (em madeira, pedra, metal
    etc)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_r;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gravur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fari fortan kaj longeda&ubreve;ran <ref cel="impres.0o">impreso</ref>n a&ubreve; bildon:
      <ekz>
        tiajn ravantajn pentra&jcirc;ojn povis <tld/>i nur la tempo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>igis en sian cerbon la figuron
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        via &ccirc;arma viza&gcirc;o &ccirc;iam restos <tld/>itaj en mia memoro
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiun]</klr> seriozecon de lia mieno <klr>[&#8230;]</klr> akraj animdoloroj
        <tld/>is sur lia vireca viza&gcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        post malmultaj monatoj japanaj <nom>Hitachi</nom>-teknikistoj <tld/>is la
        bondeziron &leftquot;<nom>Peace '91</nom>&rightquot; forportante kelkdekon da
        sulfuratomoj el kristalo da molibdena disulfato
        <fnt><aut>G. Cazzaro</aut>
        <vrk><url ref="&Monato;/1997/005286.html">Mikroskope vastaj
        horizontoj</url></vrk> <bib>Monato</bib>
        <lok>1997/02</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> bildoj de sakura-floroj estas jam
        en<tld/>i&gcirc;intaj en iliaj cerboj kun la sentoj kaj traviva&jcirc;oj
        de &gcirc;ojo kaj mal&gcirc;ojo
        <fnt>
        <aut>-</aut>
        <vrk><url ref="&Monato;/2000/006633.html">Sakura-floroj</url></vrk>
        <bib>Monato</bib><lok>2000/03, p. 26</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="radik.en0i">enradiki</ref>,
      <ref cel="stamp.0i">enstampi</ref>,
      <ref cel="konser.0i">konservi</ref>,
      <ref cel="not.0i">noti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>einbrennen</trd>,
      <trd>einmei&szlig;eln</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bev&eacute;s <klr tip="ind">(&aacute;tv. is)</klr></trd>,
      <trd>grav&iacute;roz</trd>
    </trdgrp>
    <trd lng="nl">griffen</trd>
    <trd lng="pt">gravar<klr tip="amb"> (f)</klr></trd>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;
    <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;., &c_o;
    &c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_i;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#24427;&#29256; <pr>di&amacron;ob&#462;n</pr></trd>,
    <trd>&#37931; <pr>ju&amacron;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_v;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">gravar</trd>
  <trdgrp lng="cs">
    <trd>grav&iacute;rovat</trd>,
    <trd>vr&yacute;vat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>engrave</trd>,
    <trd>etch</trd>
  </trdgrp>
  <trd lng="es">grabar</trd>
  <trd lng="fr">graver</trd>
  <trdgrp lng="ja">
    <trd>&#24427;&#12427;  <pr>&#12411;&#12427;</pr></trd>,
    <trd>&#21051;&#12416;  <pr>&#12365;&#12374;&#12416;</pr></trd>,
    <trd>&#24427;&#12426;&#36796;&#12416;  <pr>&#12411;&#12426;&#12371;&#12416;</pr></trd>,
    <trd>&#21051;&#12415;&#36796;&#12416;  <pr>&#12365;&#12374;&#12415;&#12371;&#12416;</pr></trd>
  </trdgrp>
  <trd lng="sk">vyry&tcaron;</trd>
  <trd lng="sv">gravera</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_r;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_gu;&c_r;&c_a;&c_v;&c_ib;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="gravur.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="gravur.0ajxo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="desegn.0ajxo">Desegna&jcirc;o</ref> farita per <tld/>ado:
      <ekz>
        ligno<tld/>oj de <nom>Christian Winther</nom>
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povoscias fari objektojn el oro, ar&gcirc;ento, kupro, fero, &scirc;tono, kaj ligno
        <klr>[&#8230;]</klr>, fari &ccirc;iajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>&Kr2; 2:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> devas ne opinii la Diecon simila al <klr>[&#8230;]</klr> <tld/>a&jcirc;o de
        homa arto
        <fnt>
          <bib>NT</bib>
          <lok>&Ago; 17:29</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="epigra.0o.ARKE">epigrafo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a;</trd>
  <trd lng="ca">gravat</trd>
  <trdgrp lng="de">
    <trd>Gravur</trd>,
    <trd>Grav&uuml;re</trd>,
    <trd>Stich</trd>,
    <trd>eingeschnittene Schrift</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>engraving</trd>,
    <trd>etching</trd>
  </trdgrp>
  <trd lng="es">grabado</trd>
  <trd lng="fr">gravure</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;set</trd>,
    <trd>metszet</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21051;&#12415;&#30446;  <pr>&#12365;&#12374;&#12415;&#12417;</pr></trd>,
    <trd>&#24427;&#12426;&#30446;  <pr>&#12411;&#12426;&#12417;</pr></trd>
  </trdgrp>
  <trd lng="nl">gravure</trd>
  <trd lng="pt">gravura<klr tip="amb"> (coisa gravada)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a;</trd>
  <trd lng="uk">&c_gu;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a;</trd>
  <trd lng="zh">&#30707;&#21051; <pr>sh&iacute;k&egrave;</pr></trd>
</drv>

<drv mrk="gravur.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="gravur.0ilo.grifelo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo kun malmola pinto por <tld/>ado; <ref tip="dif" cel="grifel.0o.gravurilo">grifelo</ref>.
    </dif>
    <refgrp tip="vid">
       <ref cel="cxiz.0ileto">&ccirc;izileto</ref>,
       <ref cel="cizel.0ilo">cizelilo</ref>,
       <ref cel="skalpe.0o">skalpelo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/c/c7/Stichel_2a.jpg/320px-Stichel_2a.jpg" prm="CC-BY-SA-3.0"><tld/>iloj
        <fnt>
          <aut>Manfred Br&uuml;ckels</aut>
          <lok><url ref="&WCW;/File:Stichel_2a.jpg">&WCW;/File:Stichel_2a.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_je;&c_c; <klr>(&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r;&c_s;&c_k;&c_ib;)</klr></trd>,
      <trd>&c_sh;&c_t;&c_y;&c_h;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">bur&iacute;</trd>
    <trdgrp lng="de">
      <trd>Stichel</trd>,
      <trd>Graviermesser</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>engraving tool</trd>,
      <trd>burin</trd>,
      <trd>graver</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>karcol&oacute;t&udblac;</trd>,
      <trd>v&eacute;s&odblac;t&udblac;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>graveernaald</trd>,
      <trd>graveerstift</trd>
    </trdgrp>
    <trd lng="pt">buril</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_z;&c_je;&c_c; <klr>(&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r;&c_a;)</klr></trd>,
      <trd>&c_sh;&c_t;&c_i;&c_h;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">rydlo</trd>
    <trdgrp lng="cs">
      <trd>ryc&iacute; jehla</trd>,
      <trd>rydlo</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#25361;&#36887; <pr>ti&#462;od&ograve;u</pr></trd>,
    <trd>&#25590;&#25540; <pr>y&eacute;y&uacute;</pr></trd>,
    <trd>&#36887;&#24825; <pr>d&ograve;ur&ecaron;</pr></trd>,
    <trd>&#25103;&#24324; <pr>x&igrave;n&ograve;ng</pr></trd>,
    <trd>&#25366;&#33510; <pr>w&amacron;ku</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="gravur.0ilo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por registri datumojn
      sur <ref cel="disk.lum0o">lumdisko</ref> per lasero:
      <ekz>
        la apero de la kompakta disko plifaciligis ilian taskon, sufi&ccirc;as ja disponi pri
        komputilo kun <tld/>ilo kaj presilo por eldoni tiom, kiom oni bezonas
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2006/008921.php">Kontra&ubreve;le&gcirc;a
          kopiado</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_p;&c_a;&c_r;&c_a;&c_t; &c_d;&c_l;&c_ja; &c_z;&c_a;&c_p;&c_ib;&c_s;&c_u; &c_a;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;&c_h; &c_d;&c_y;&c_s;&c_k;&c_a;&c_w;</trd>
    <trd lng="de">Brenner <klr>(CD)</klr></trd>
    <trd lng="en">optical writer <klr>(CD)</klr></trd>
    <trd lng="fr">graveur de disque optique</trd>
    <trd lng="zh">&#28903;&#24405;&#26426; <pr>sh&amacron;ol&ugrave;j&imacron;</pr></trd>
</snc>
  <trd lng="ja">&#24427;&#21051;&#20992;  <pr>&#12385;&#12423;&#12358;&#12371;&#12367;&#12392;&#12358;</pr></trd>
  <trd lng="uk">&c_r;&c_ib;&c_z;&c_je;&c_c;&c_mol;</trd>
</drv>

<drv mrk="gravur.kupro0ajxo">
  <kap>kupro<tld/>a&jcirc;o,
  <var><kap>kupro<tld/>o</kap></var></kap>
  <snc mrk="gravur.kupro0ajxo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Bilda a&ubreve; teksta verko presita per reliefa <ref tip="vid" cel="matric.0o.TEK">matrico</ref>
      <tld/>ita en kupro:
      <ekz>
        tia magia kvadrato trovi&gcirc;as sur la kupro<tld/>a&jcirc;o &#8218;La melankolio&#8216; de <nom>Albrecht
        D&uuml;rer</nom>
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://lingvovojo.wordpress.com/2013/09/28/magiaj-nombro-kvadratoj/">
          Magiaj nombro-kvadratoj</url></vrk>
          <lok>lingvovojo, 2013-09-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        kupro<tld/>ojn de bataloj bazitaj sur desegna&jcirc;oj de <nom>Georg Philipp Rugendas</nom>
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Goswinus_Uilenbroek">
          Goswinus Uilenbroek</url></vrk>
          <lok>Vikipedio, 2024-01-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>copper engraving</trd>,
    <trd>copper plate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chalcographie</trd>,
    <trd>gravure sur cuivre</trd>
  </trdgrp>
</drv>

<drv mrk="gravur.kupro0isto">
  <kap>kupro<tld/>isto</kap>
  <snc mrk="gravur.kupro0isto.BELA">
    <dif>
      <tld lit="G"/>isto laboranta per kupro:
      <ekz>
        nepo de <nom>Rentsch</nom> estis la saksa kortegopentristo kaj kupro<tld/>isto
       <nom>Christian Wilhelm Ernst Dietrich</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=343155">Johann Ernst Rentsch (la pli
          maljuna)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;dirytec</trd>
  <trd lng="de">Kupferstecher</trd>
  <trd lng="en">copper plate lithographer</trd>
  <trd lng="fr">graveur sur cuivre</trd>
  <trd lng="hu">r&eacute;zmetsz&odblac;</trd>
  <trd lng="nl">kopergraveur</trd>
  <trd lng="pl">miedziorytnik</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r; &c_n;&c_a; &c_m;&c_je;&c_d;&c_i;</trd>
  <trd lng="sk">medirytec</trd>
  <trdgrp lng="zh">
    <trd>&#37509;&#29256;&#24427;&#21051;&#27231; <pr>t&oacute;ngb&#462;ndi&amacron;ok&egrave;j&imacron;</pr></trd>,
    <trd>&#38108;&#29256;&#38613;&#21051;&#26426; <pr>t&oacute;ngb&#462;ndi&amacron;ok&egrave;j&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gravur.ligno0ajxo">
  <kap>ligno<tld/>a&jcirc;o,
  <var><kap>ligno<tld/>o</kap></var></kap>
  <snc mrk="gravur.ligno0ajxo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Bilda a&ubreve; teksta verko presita per reliefa <ref tip="vid" cel="matric.0o.TEK">matrico</ref>
      <tld/>ita en ligno;
      <ref tip="sin" cel="ksilog.0ajxo.BELA">ksilografa&jcirc;o</ref>:
      <ekz>
        kolekto de klasikaj japanaj ligno<tld/>a&jcirc;oj
        <fnt>
          <aut>I. Lapenna, T. Carlevaro kaj U. Lins</aut>
          <vrk>Esperanto en Perspektivo</vrk>
          <lok>1974</lok>
        </fnt>;
      </ekz>
      <ekz>
        ligno<tld/>o <klr>[&#8230;]</klr> prezentis aeran fenomenon, kiu aperis en la urbo
        Kolonjo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Holzschnitt</trd>
  <trd lng="en">woodcut</trd>
  <trdgrp lng="fr">
    <trd>gravure sur bois</trd>,
    <trd>xylographie<klr> (&oelig;uvre)</klr></trd>
  </trdgrp>
  <trd lng="nl">xylografie<klr tip="ind"> (het gedrukte)</klr></trd>
  <trd lng="pt">xilogravura</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a; &c_p;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26408;&#21051; <pr>m&ugrave;k&egrave;</pr></trd>,
    <trd>&#26408;&#21051;&#29256;&#30011; <pr>m&ugrave;k&egrave;b&#462;nhu&agrave;</pr></trd>,
    <trd>&#26408;&#21051;&#29256;&#30059; <pr>m&ugrave;k&egrave;b&#462;nhu&agrave;</pr></trd>,
    <trd>&#26408;&#21051;&#30011; <pr>m&ugrave;k&emacron;hu&agrave;</pr></trd>,
    <trd>&#26408;&#21051;&#30059; <pr>m&ugrave;k&emacron;hu&agrave;</pr></trd>,
    <trd>&#38613;&#29256; <pr>di&amacron;ob&#462;n</pr></trd>,
    <trd>&#26408;&#29256;&#30011; <pr>m&ugrave;b&#462;nhu&agrave;</pr></trd>,
    <trd>&#26408;&#29256;&#30059; <pr>m&ugrave;b&#462;nhu&agrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gravur.ligno0isto">
  <kap>ligno<tld/>isto</kap>
  <snc mrk="gravur.ligno0isto.BELA">
    <dif>
      <tld lit="G"/>isto laboranta per ligno; <ref tip="sin"
      cel="ksilog.0isto.TIP">ksilografisto</ref>:
      <ekz>
