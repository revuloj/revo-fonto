<vortaro>
<art mrk="$Id$">
<kap>
  <rad>geofizik</rad>/o
</kap>
<drv mrk="geofiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri la fizikaj 
      le&gcirc;oj de la tero.  
    </dif>
  </snc>
  <trd lng="de">Geophysik</trd>
  <trd lng="fr">g&eacute;ophysique</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_f;&c_i;&c_z;&c_i;&c_k;&c_a;</trd>
</drv>
<drv mrk="geofiz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">g&eacute;ophysicien</trd>
</drv>
</art>
