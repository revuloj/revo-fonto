<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geofiz.xml,v 1.22 2025/08/12 22:32:17 revo Exp $">
<kap>
  <rad>geofizik</rad>/o
</kap>

<drv mrk="geofiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref>
      pri la fizikaj le&gcirc;oj de la tero:
      <ekz>
        <nom>Georg Christoph Lichtenberg</nom> <klr>[&#8230;]</klr> estis konsiderata grava fakulo i.a.
        pri astronomio, kemio, <tld/>o, meteorologio kaj statistiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6666">Georg Christoph
          Lichtenberg</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        geodezio estas fako de la <tld/>o, per kiu oni studas la formon kaj dimensiojn
        de la Tero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28987">Geodezio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_f;&c_ib;&c_z;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">geof&iacute;sica</trd>
  <trd lng="de">Geophysik</trd>
  <trd lng="fr">g&eacute;ophysique</trd>
  <trd lng="hu">geofizika</trd>
  <trd lng="id">geofisika</trd>
  <trd lng="ja">&#22320;&#29699;&#29289;&#29702;&#x5B66;  <pr>&#12385;&#x304D;&#12421;&#12358;&#12406;&#12388;&#x308A;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">geofysica</trd>
  <trd lng="pl">geofizyka</trd>
  <trd lng="pt">geof&iacute;sica</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_f;&c_i;&c_z;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">geofysik</trd>
  <trd lng="tr">jeofizik</trd>
  <trd lng="uk">&c_g;&c_je;&#x43E;&c_f;&c_ib;&c_z;&c_i;&#x43A;&c_a;</trd>
  <trd lng="zh">&#22320;&#29699;&#29289;&#29702;&#x5B66; <pr>d&#xEC;qi&#xFA;w&#xF9;l&#x1D0;xu&#xE9;</pr></trd>
</drv>

<drv mrk="geofiz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        <nom>Adolf Schmidt</nom> <klr>[&#8230;]</klr> estis germana <tld/>isto kaj esperantisto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=170">Adolf Schmidt</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_f;&c_ib;&c_z;&c_ib;&c_k;</trd>
  <trd lng="ca">geof&iacute;sic</trd>
  <trd lng="de">Geophysiker</trd>
  <trd lng="fr">g&eacute;ophysicien</trd>
  <trd lng="hu">geofizikus</trd>
  <trd lng="id">geofisikawan</trd>
  <trd lng="nl">geofysicus</trd>
  <trd lng="pl">geofizyk</trd>
  <trd lng="pt">geof&iacute;sico</trd>
  <trd lng="sv">geofysiker</trd>
  <trd lng="uk">&c_g;&c_je;&#x43E;&c_f;&c_ib;&c_z;&c_i;&#x43A;</trd>
</drv>
</art>
