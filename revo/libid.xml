  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_b;&c_i;&c_d;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_je; &c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
