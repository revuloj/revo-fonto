<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxtor.xml,v 1.31 2025/03/27 16:35:08 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>a&ubreve;tor</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="auxtor.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxtor.0o.JUR">
    <dif>
      Verkinto (de libro a&ubreve; de scienca, literatura, belarta verko).
    </dif>
    <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_r;</trd>
    <trdgrp lng="br">
      <trd>aozer</trd>,
      <trd>oberour</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Autor</trd>,
      <trd>Verfasser</trd>
    </trdgrp>
    <trd lng="nl">auteur</trd>
    <trdgrp lng="hu">
      <trd>&iacute;r&oacute;</trd>,
      <trd>szerz&odblac;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#33879;&#20316;&#26435;&#20154; <pr>zh&#249;zu&#242;qu&#225;nr&#233;n</pr></trd>,
      <trd>&#33879;&#20316;&#27402;&#20154; <pr>zh&#249;zu&#242;qu&#225;nr&#233;n</pr></trd>,
      <trd>&#20316;&#32773; <pr>zu&#242;zh&#283;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="auxtor.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Farinto, kreinto, elpensinto:
      <ekz>
        li estas <tld/>o de sia propra
        malfeli&ccirc;o;
      </ekz>
      <ekz>
        la kunkulpulo estas punata kiel la <tld/>o mem.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Urheber</trd>,
      <trd>Sch&ouml;pfer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szerz&odblac;</trd>,
      <trd>okoz&oacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#21019;&#36896;&#20027; <pr>chu&#224;ngz&#224;ozh&#468;</pr></trd>,
      <trd>&#21109;&#36896;&#20027; <pr>chu&#224;ngz&#224;ozh&#468;</pr></trd>,
      <trd>&#21019;&#22987;&#32773; <pr>chu&#224;ngsh&#464;zh&#283;</pr></trd>,
      <trd>&#21109;&#22987;&#32773; <pr>chu&#224;ngsh&#464;zh&#283;</pr></trd>,
      <trd>&#30528;&#20316;&#20154; <pr>zh&#249;zu&#242;r&#233;n</pr></trd>,
      <trd>&#33879;&#20316;&#20154; <pr>zh&#249;zu&#242;r&#233;n</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>verkinto</ke>, <ke>kreinto</ke>.
  </rim>

  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_r;</trd>
  <trd lng="bo">&#3938;&#4009;&#3964;&#3928;&#3851;&#3924;&#3851;&#3924;&#3964;&#3851;</trd>
  <trd lng="en">author</trd>
  <trd lng="es">autor</trd>
  <trd lng="fr">auteur</trd>
  <trd lng="he">&#1502;&#1456;&#1495;&#1463;&#1489;&#1468;&#1461;&#1512;</trd>
  <trd lng="it">autore</trd>
  <trdgrp lng="pl">
    <trd>autor</trd>,
    <trd>tw&oacute;rca</trd>
  </trdgrp>
  <trd lng="pt">autor</trd>
  <trd lng="ro">autor</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">autor</trd>
  <trd lng="uk">&c_a;&c_v;&c_t;&#1086;&c_r;</trd>
</drv>

<drv mrk="auxtor.0a">
  <kap><tld/>a</kap>
  <snc mrk="auxtor.0a.JUR">
    <dif>
      Rilata al a&ubreve;toro:
      <ekz>
        <ind><tld/>aj rajtoj</ind>.
        <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_r;&c_s;&c_k;&c_a;&c_je; &c_p;&c_r;&c_a;&c_v;&c_a;</trd>
        <trd lng="es">derechos de autor</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="nl">auteurs-</trd>
  <trd lng="hu">szerz&odblac;i</trd>
  <trd lng="it">d'<ind>autore</ind></trd>
  <trdgrp lng="pl">
    <trd>autorski</trd>,
    <trd>pisarski</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">autorsk&yacute;</trd>
  <trd lng="uk">&c_a;&c_v;&c_t;&#1086;&c_r;&c_s;&#1100;&#1082;&c_i;&c_j;</trd>
</drv>

<drv mrk="auxtor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxtor.0i.JUR">
    <dif>
      Esti <tld/>o de,
      <ref tip="dif" cel="verk.0i">verki</ref>:
      <ekz>
        <ctl>La pupo</ctl> estas unu el la plej gravaj romanoj
        de la pola literaturo, <tld/>ita de Boles&lstroke;aw Prus
        <fnt>
          <aut>P. Fischer-Kotowski</aut>,
          <vrk><url ref="http://www.liberafolio.org/2016/12/28/libroj-de-la-jaro-2016/">,
          26 libroj de la jaro 2016</url></vrk>,
          <lok>libera folio, 2016-12-28</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">sevel<klr tip="ind"> (un oberenn)</klr></trd>
    <trdgrp lng="de">
      <trd>schaffen</trd>,
      <trd>verfassen</trd>
    </trdgrp>
    <trd lng="en"><klr>to </klr>author</trd>
    <trd lng="nl">schrijven<klr tip="ind"> (v.boek)</klr></trd>
    <trdgrp lng="hu">
      <trd>szerez<klr tip="ind"> (m&udblac;vet)</klr></trd>,
      <trd>&iacute;r<klr tip="ind"> (m&udblac;vet)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>scrivere<klr> (creare)</klr></trd>,
      <trd>creare<klr> (comporre)</klr></trd>,
      <trd>comporre<klr> (creare)</klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#32232;&#23531; <pr>bi&amacron;nxi&#283;</pr></trd>,
      <trd>&#32534;&#20889; <pr>bi&amacron;nxi&#283;</pr></trd>,
      <trd>&#25776; <pr>zhu&#224;n</pr></trd>,
      <trd>&#35668; <pr>zhu&#224;n</pr></trd>,
      <trd>&#32232;&#33879; <pr>bi&amacron;nzh&#249;</pr></trd>,
      <trd>&#32534;&#33879; <pr>bi&amacron;nzh&#249;</pr></trd>,
      <trd>&#33879;&#20316; <pr>zh&#249;zu&#242;</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_a;&c_w;&c_t;&c_a;&c_r;&c_a;&c_m;</ind></trd>,
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="he">&#1500;&#1495;&#1489;&#1512;</trd>
  <trdgrp lng="pl">
    <trd>stworzy&cacute;</trd>,
    <trd>by&cacute;  <ind>autorem</ind></trd>
  </trdgrp>
</drv>

<drv mrk="auxtor.0signo">
  <kap><tld/>signo</kap>
  <snc>
    <dif>
      Signo montranta la <tld/>on de iu tekstloko; referenco.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je; <ind>&c_a;&c_w;&c_t;&c_a;&c_r;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Autorangabe</trd>,
    <trd>Verfasserangabe</trd>
  </trdgrp>
  <trd lng="he">&#1488;&#1493;&#1514; &#1488;&#1493;&#1496;&#1493;&#1502;&#1496;&#1497;</trd>
  <trd lng="hu">hivatkoz&aacute;s szerz&odblac;re</trd>
  <trdgrp lng="pl">
    <trd>znak  <ind>autora</ind></trd>,
    <trd>referencja <ind>autorska</ind></trd>
  </trdgrp>
</drv>

<drv mrk="auxtor.kun0o">
  <kap>kun<tld/>o</kap>
  <snc mrk="auxtor.kun0o.JUR">
    <dif>
      Tiu, kiu parte verkis.
    </dif>
    <trdgrp lng="zh">
      <trd>&#21512;&#30528;&#32773; <pr>h&#233;zhu&#243;zh&#283;</pr></trd>,
      <trd>&#21512;&#33879;&#32773; <pr>h&#233;zhu&#243;zh&#283;</pr></trd>,
      <trd>&#20849;&#21516;&#22519;&#31558;&#32773; <pr>g&#242;ngt&#243;ngzh&#237;b&#464;zh&#283;</pr></trd>,
      <trd>&#20849;&#21516;&#25191;&#31508;&#32773; <pr>g&#242;ngt&#243;ngzh&#237;b&#464;zh&#283;</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_a;&c_w;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Co-Autor</trd>,
    <trd>Mitautor</trd>,
    <trd>Mitverfasser</trd>
  </trdgrp>
  <trd lng="br">kenoberour</trd>
  <trd lng="en">co-author</trd>
  <trd lng="es">coautor</trd>
  <trd lng="fr">co-auteur</trd>
  <trd lng="hu">t&aacute;rsszerz&odblac;</trd>
  <trd lng="it">co-autore</trd>
  <trd lng="nl">coauteur</trd>
  <trd lng="pl">wsp&oacute;&lstroke;autor</trd>
  <trd lng="ru">&c_s;&c_o;&c_a;&c_v;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">spoluautor</trd>
  <trd lng="uk">&c_s;&#1087;&c_ib;&c_v;&c_a;&c_v;&c_t;&#1086;&c_r;</trd>
</drv>
</art>
