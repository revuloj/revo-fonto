<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: burs.xml,v 1.12 2022/03/13 20:13:44 revo Exp $">
    <kap>
      <rad>burs</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="burs.0o">
      <kap><tld/>o</kap>
      <uzo tip="stl">RAR</uzo>
      <snc>
        <dif>
          Saka <ref tip="super" cel="mon.0ujo">monujo</ref>, monsaketo:
          <ekz>
            li devus akcepti kiel fakton, ke &ccirc;iuj migraj kavaliroj
            <klr>...</klr> portis <tld/>on bone provizitan
            <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 3a, p. 40a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>burza</trd>,
    <trd>m&#xED;&#x161;ek</trd>,
    <trd>vychl&#xED;penina</trd>,
    <trd>v&#xE1;&#x10D;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Geldbeutel</trd>,
        <trd>Geldb&ouml;rse</trd>,
        <trd>Portemonnaie</trd>
      </trdgrp>
  <trdgrp lng="es">
        <trd>cartera</trd>,
        <trd>monedero</trd>
      </trdgrp>
  <trd lng="fr">bourse<klr> (porte-monnaie)</klr></trd>
  <trdgrp lng="he">
    <trd>&#x5D0;&#x5B7;&#x5E8;&#x5B0;&#x5E0;&#x5B8;&#x5E7;</trd>,
    <trd>&#x5EA;&#x5E4;&#x5D9;&#x5D7;&#x5D4;</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nzes <ind>zacsk&oacute;</ind></trd>
  <trdgrp lng="pl">
        <trd>sakiewka</trd>,
        <trd>portmonetka</trd>
      </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>me&#x161;ec</trd>,
    <trd>tobolka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x43C;&#x430;&#x43D;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x43A;&#x430;&#x43B;&#x438;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x441;&#x443;&#x43C;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
  </art>

