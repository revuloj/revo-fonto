<art mrk="$Id: burs.xml,v 1.15 2025/03/28 16:17:59 revo Exp $">
<kap>
  <rad>burs</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="burs.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Saka <ref tip="super" cel="mon.0ujo">monujo</ref>, monsaketo:
      <ekz>
        li devus akcepti kiel fakton, ke &ccirc;iuj migraj kavaliroj
        <klr>...</klr> portis <tld/>on bone provizitan
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 3a, p. 40a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>monujo</ke>.
  </rim>

  <trd lng="be">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>burza</trd>,
    <trd>m&#237;&scaron;ek</trd>,
    <trd>vychl&#237;penina</trd>,
    <trd>v&#225;&#269;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geldbeutel</trd>,
    <trd>Geldb&ouml;rse</trd>,
    <trd>Portemonnaie</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cartera</trd>,
    <trd>monedero</trd>
  </trdgrp>
  <trd lng="fr">bourse<klr> (porte-monnaie)</klr></trd>
  <trdgrp lng="he">
    <trd>&#1488;&#1463;&#1512;&#1456;&#1504;&#1464;&#1511;</trd>,
    <trd>&#1514;&#1508;&#1497;&#1495;&#1492;</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nzes <ind>zacsk&oacute;</ind></trd>
  <trdgrp lng="pl">
    <trd>sakiewka</trd>,
    <trd>portmonetka</trd>
  </trdgrp>
  <trd lng="ro">portmoneu</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>me&scaron;ec</trd>,
    <trd>tobolka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&#1084;&c_a;&#1085;&c_je;&c_c;&#1100;</trd>,
    <trd>&#1082;&c_a;&#1083;&c_i;&c_t;&#1082;&c_a;</trd>,
    <trd>&c_s;&c_u;&#1084;&#1082;&c_a;</trd>
  </trdgrp>
</drv>
