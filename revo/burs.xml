<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>burs</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="burs.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eta saka po&scirc;<ref tip="super" cel="mon.0ujo">monujo</ref>:
      <ekz>
        li devus akcepti kiel fakton, ke &ccirc;iuj migraj kavaliroj
        <klr>...</klr> portis <tld/>on bone provizitan
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 3a, p.
        40a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  <trd lng="fr">bourse<klr> (porte-monnaie)</klr></trd>
  <trd lng="hu">p&eacute;nzes <ind>zacsk&oacute;</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
</drv>
</art>

