<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plet</rad>/o
</kap>
<drv mrk="plet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Plato el ligno, metalo a&ubreve; ofte el lakita kartono, kiu servas por
      alporti telerojn, pladojn, glasojn, tasojn dum man&gcirc;o:
      <ekz>
        la &ccirc;ambristino eniris kun karafo sur ar&gcirc;enta <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 17a, p. 102a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_n;&c_o;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Pr&auml;sentierteller</trd>,
    <trd>Servierbrett</trd>,
    <trd>Tablett</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>plateau<klr> (pour le service)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&aacute;lca</trd>
  <trdgrp lng="nl">
    <trd>presenteerblad</trd>,
    <trd>dienblad</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_n;&c_o;&c_s;</trd>
</drv>
</art>
