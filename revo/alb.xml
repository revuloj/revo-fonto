<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alb</rad>/o
</kap>
<drv mrk="alb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Pastra blanka tola &gcirc;ispieda mesvesto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>
  <trd lng="en">alb</trd>
  <trd lng="fr">aube</trd>
  <trdgrp lng="hu">
    <trd>alba</trd>,
    <trd>miseing</trd>,
    <trd>karing</trd>
  </trdgrp>
  <trd lng="nl">albe</trd>
  <trd lng="pt">alva</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_h;&c_a;&c_r;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
