<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alb.xml,v 1.20 2023/08/30 18:23:32 revo Exp $">
<kap>
  <rad>alb</rad>/o
</kap>

<drv mrk="alb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Pastra blanka tola &gcirc;ispieda mes<ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o">vesto</ref>:
      <ekz>
        la&ubreve; la meslibro de papo Pio la 5a (16-a jarcento), dum la surmeto de la <tld/>o,
        la vesti&gcirc;anto devis reciti <klr>[&#x2026;]</klr> la pre&gcirc;on &leftquot;sanktigu min, Sinjoro,
        kaj purigu mian koron por ke per la sango de la &scirc;afido senmakule mi meritu la
        eternan &gcirc;ojon&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=405381">Albo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>
  <trd lng="cs">alba</trd>
  <trd lng="de">Albe</trd>
  <trd lng="en">alb</trd>
  <trd lng="fr">aube</trd>
  <trdgrp lng="hu">
    <trd>alba</trd>,
    <trd>miseing</trd>,
    <trd>karing</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>alba<klr tip="amb"> (rel.)</klr></trd>,
    <trd>camice<klr tip="amb"> (rel.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30D0;</trd>
  <trd lng="nl">albe</trd>
  <trd lng="pl">alba</trd>
  <trd lng="pt">alva</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_h;&c_a;&c_r;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">alba</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43B;&#x44C;&#x431;&#x430;</trd>,
    <trd>&#x431;&#x456;&#x43B;&#x438;&#x439; &#x441;&#x442;&#x438;&#x445;&#x430;&#x440;</trd>
  </trdgrp>
</drv>
</art>
