<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvik.xml,v 1.8 2022/08/07 19:25:30 revo Exp $">
<kap>
  <rad>kvik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvik.0i">
  <kap><tld/>i</kap>
  <snc mrk="kvik.0i.ZOO">
    <dif>
      Eligi akutajn krietojn, <ref tip="super" cel="blek.0i.beste">bleki</ref> kiel iuj birdoj,
      ratoj,
      musteledoj:
      <ekz>
        la apikeco de l' deklivo <klr>...</klr> ne timigis &scirc;in, kvankam
        la
        aliaj sinjorinoj <tld/>is kaj fermis la okulojn
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cvrlikat</trd>,
    <trd>kvi&ccaron;et</trd>
  </trdgrp>
  <trd lng="de">quieken</trd>
  <trd lng="fr">couiner</trd>
  <trd lng="nl">piepen</trd>
  <trd lng="pl">kwicze&cacute;</trd> 
  <trd lng="pt">piar</trd>
  <trd lng="ro">sc&acirc;nci</trd>
  <trd lng="sk">kvi&ccaron;a&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_v;&c_ib;&c_r;&c_ib;&#x43D;&#x44C;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_i;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>

