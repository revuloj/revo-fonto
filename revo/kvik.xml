<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvik.xml,v 1.7 2022/03/22 15:44:14 revo Exp $">
<kap>
  <rad>kvik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvik.0i">
  <kap><tld/>i</kap>
  <snc mrk="kvik.0i.ZOO">
    <dif>
      Eligi akutajn krietojn, <ref tip="super" cel="blek.0i.beste">bleki</ref> kiel iuj birdoj, ratoj,
      musteledoj:
      <ekz>
        la apikeco de l' deklivo <klr>...</klr> ne timigis &scirc;in, kvankam
        la
        aliaj sinjorinoj <tld/>is kaj fermis la okulojn
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cvrlikat</trd>,
    <trd>kvi&ccaron;et</trd>
  </trdgrp>
  <trd lng="fr">couiner</trd>
  <trd lng="nl">piepen</trd>
  <trd lng="pl">kwicze&cacute;</trd> 
  <trd lng="pt">piar</trd>
  <trd lng="ro">sc&acirc;nci</trd>
  <trd lng="sk">kvi&ccaron;a&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x446;&#x432;&#x456;&#x440;&#x456;&#x43D;&#x44C;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x438;&#x449;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x449;&#x435;&#x431;&#x435;&#x442;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>

