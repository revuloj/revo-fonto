<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvik.0i">
  <kap><tld/>i</kap>
  <snc mrk="kvik.0i.ZOO">
    <dif>
      Eligi akutajn krietojn, <ref tip="super"
      cel="blek.0i.beste">bleki</ref> kiel iuj birdoj, ratoj,
      musteledoj:
      <ekz>
        la apikeco de l' deklivo <klr>...</klr> ne timigis &scirc;in, kvankam
        la
        aliaj sinjorinoj <tld/>is kaj fermis la okulojn
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">couiner</trd>
  <trd lng="nl">piepen</trd>
  <trd lng="pt">piar</trd>
  <trd lng="sk">kvičať</trd>
</drv>
</art>

