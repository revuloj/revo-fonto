<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ragu.xml,v 1.16 2022/08/01 22:33:23 revo Exp $">
<kap>
  <rad>ragu</rad>/o
</kap>
<drv mrk="ragu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Vianda, legoma a&ubreve; fi&scirc;a
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo.homa">man&gcirc;a&jcirc;o</ref>,
      kuirita en spicita sa&ubreve;co:
      <ekz>
        gula&scirc;o estas la hungara nacia man&gcirc;a&jcirc;o kaj estas supo, la tre densigita varianto
        estas <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215662">Gula&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_u;</trd>
  <trd lng="cs">rag&uacute;</trd>
  <trd lng="de">Ragout</trd>
  <trd lng="es">rag&uacute;</trd>
  <trd lng="fr">rago&ucirc;t</trd>
  <trd lng="hu">ragu</trd>
  <trd lng="nl">ragout</trd>
  <trd lng="pl">rago&ucirc;t</trd>
  <trdgrp lng="pt">
    <trd>ragu</trd>,
    <trd>ensopado</trd>
  </trdgrp>
  <trd lng="ro">tocan&abreve;</trd>
  <trd lng="ru">&c_r;&c_a;&c_g;&c_u;</trd>
  <trd lng="sk">rag&uacute;</trd>
  <trd lng="uk">&c_r;&c_a;&c_gu;&c_u;</trd>
</drv>
</art>
