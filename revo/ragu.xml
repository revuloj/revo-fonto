<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ragu.xml,v 1.20 2023/09/03 07:53:09 revo Exp $">
<kap>
  <rad>ragu</rad>/o
</kap>
<drv mrk="ragu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Vianda, legoma a&ubreve; fi&scirc;a
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo.homa">man&gcirc;a&jcirc;o</ref>,
      kuirita en spicita sa&ubreve;co:
      <ekz>
        la junulino legas la menuon de la mendita lun&ccirc;o: kaviaro, &ccirc;ampano, lepora&jcirc;a
        <tld/>o, burgonja vino, glacia&jcirc;o
        <fnt>
          <bib>KrB</bib>
          <lok>Kolekto aprobita</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="gulasx.0o">gula&scirc;o</ref> estas la hungara nacia man&gcirc;a&jcirc;o kaj estas supo, la tre
        densigita varianto
        estas <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215662">Gula&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_u;</trd>
  <trd lng="cs">rag&uacute;</trd>
  <trd lng="de">Ragout</trd>
  <trd lng="en">ragout</trd>
  <trd lng="es">rag&uacute;</trd>
  <trd lng="fr">rago&ucirc;t</trd>
  <trd lng="hu">ragu</trd>
  <trd lng="ja">&#x30E9;&#x30B0;&#x30FC;</trd>
  <trd lng="nl">ragout</trd>
  <trd lng="pl">rago&ucirc;t</trd>
  <trdgrp lng="pt">
    <trd>ragu</trd>,
    <trd>ensopado</trd>
  </trdgrp>
  <trd lng="ro">tocan&abreve;</trd>
  <trd lng="ru">&c_r;&c_a;&c_g;&c_u;</trd>
  <trd lng="sk">rag&uacute;</trd>
  <trd lng="uk">&c_r;&c_a;&c_gu;&c_u;</trd>
</drv>
</art>
