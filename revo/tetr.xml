<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tetr.xml,v 1.21 2023/09/04 18:15:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tetr</rad>/o
</kap>
<drv mrk="tetr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tetr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      el familio <ref tip="malprt" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Lyrurus tetrix</trd> a&ubreve; <trd lng="la">Tetrao tetrix</trd>):
      <ekz>
        la parko havas 107 montopintojn, iuj el kiuj altas pli ol 3000&nbsp;m 
        super la marnivelo, kelkaj el la montaj specioj similas al tiuj,
        kiuj troveblas en la plej norda parto de E&ubreve;ropo: urogalo, <tld/>o kun liroforma vosto, bonazio
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008395.php">Vizito
          al la nacia parko Vanoise</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        malpli grandaj &ccirc;asa&jcirc;oj estas leporoj, galinoformaj birdoj (lagopo, perdriko, <tld/>o, bonazio)
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2013/011065.php">&Ccirc;asado</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
    <bld lok="&WCU;/thumb/5/58/Lyrurus_tetrix_lekking_Sweden.jpg/639px-Lyrurus_tetrix_lekking_Sweden.jpg" prm="CC BY 2.0"><tld/>oj (Lyrurus tetrix), Svedujo
        <fnt>
          <aut>Billy Lindblom el Hallsberg, Svedujo</aut>
          <lok><url ref="&WCW;/File:Lyrurus_tetrix_lekking_Sweden.jpg">&WCW;/File:Lyrurus_tetrix_lekking_Sweden.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_c;&c_ja;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">gall de cua forcada</trd>
  <trdgrp lng="cs">
    <trd>tet&rcaron;ivek obecn&yacute;</trd>,
    <trd>tet&rcaron;&iacute;vek obecn&yacute;</trd>
  </trdgrp>
  <trd lng="de">Birkhuhn</trd>
  <trd lng="en">black grouse</trd>
  <trdgrp lng="fr">
    <trd>petit coq de bruy&egrave;re</trd>,
    <trd>t&eacute;tras lyre</trd>
  </trdgrp>
  <trd lng="hu">ny&iacute;rfajd</trd>
  <trd lng="ja">&#x30AF;&#x30ED;&#x30E9;&#x30A4;&#x30C1;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">korhoen</trd>
  <trd lng="pl">cietrzew</trd>
  <trdgrp lng="pt">
    <trd>galinha-brava</trd>,
    <trd>tetraz-pequeno-das-serras</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v;-&c_k;&c_o;&c_s;&c_a;&c_ch;</trd>,
    <trd>&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v; &c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">tetrov ho&lcaron;niak</trd>
  <trd lng="uk">&#x442;&#x435;&#x442;&#x435;&#x440;&#x443;&#x43A;</trd>
</drv>
</art>
