<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tjurk.xml,v 1.14 2023/09/04 19:01:52 revo Exp $">
<kap>
    <rad>tjurk</rad>/o<fnt>PIV</fnt>
</kap>

<drv mrk="tjurk.0o">
    <kap><tld/>o</kap>
    <snc>
        <dif>
            Ano de <ref cel="tjurk.0alingvo"><tld/>lingva</ref> etno:
            <ekz>
                vana diskuto, &ccirc;u la uzbekoj estas <tld/>igitaj
                ta&gcirc;ikoj a&ubreve; ta&gcirc;ikigitaj <tld/>oj.
            </ekz>
        </dif>
    </snc>
    <rim>
        La rilato inter la nocioj <tld/>o kaj
        <ref tip="sub" cel="turk.0o">turko</ref> estas do simila al
        tiu inter &gcirc;ermano kaj germano.
    </rim>
    <trdgrp lng="be">
      <trd>&c_c;&c_ju;&c_r;&c_a;&c_k;</trd>, 
      <trd>&c_c;&c_ju;&c_r;&c_k;</trd>
    </trdgrp>
    <trd lng="bg">&c_t;&c_ju;&c_r;&c_k;</trd>
    <trd lng="hu">t&uuml;rk</trd>
    <trd lng="ja">&#x30C1;&#x30E5;&#x30EB;&#x30AF;</trd>
  <trd lng="nl">persoon behorend tot de <ind>Turks</ind>e
    taalgroep</trd>
    <trd lng="pl">Turek</trd>
    <trd lng="pt">turcomano</trd>
    <trdgrp lng="ru">
        <trd>&c_t;&c_ju;&c_r;&c_o;&c_k;</trd>,
        <trd>&c_t;&c_ju;&c_r;&c_k;</trd></trdgrp>
    <trd lng="tr">T&uuml;rk</trd>
  <trd lng="sk">Turek</trd>
  <trd lng="cs">Turek</trd>
  <trd lng="uk">&#x442;&#x44E;&#x440;&#x43A;</trd>
</drv>

<drv mrk="tjurk.0a">
    <kap><tld/>a</kap>
    <snc>
        <dif>
            Rilata al la <tld/>oj:
            <ekz>
                la <tld/>aj lingvoj;
            </ekz><ekz>
                la <tld/>a mitologio, kalendaro;
            </ekz><ekz>
                por en&scirc;tatigi kristanismon en Bulgarujo,
                &hcirc;ano Boriso
                devis ekzekutigi grandan parton de la <tld/>a nobelaro;
            </ekz><ekz>
                la <ind><tld/>aj popoloj</ind>.
                <trd lng="be">&c_c;&c_ju;&c_r;&c_s;&c_k;&c_ib;&c_ja;
                &c_n;&c_a;&c_r;&c_o;&c_d;&c_y;</trd>
                <trd lng="de">T&uuml;rkv&ouml;lker</trd>
                <trd lng="hu">t&uuml;rk n&eacute;pek</trd>
                <trd lng="nl">de <ind>Turks</ind>e
                volkeren</trd>
                <trd lng="pl">ludy <ind>tureckie</ind></trd>
                <trd lng="ru">&c_t;&c_ju;&c_r;&c_k;&c_s;&c_k;&c_i;&c_je;
                    &c_n;&c_a;&c_r;&c_o;&c_d;&c_y;</trd>
                <trdgrp lng="tr">
                    <trd>T&uuml;rk milletleri</trd>,
                    <trd>T&uuml;rk&ccedil;e konu&scedil;an
                    milletler</trd></trdgrp>
            </ekz>
        </dif>
    </snc>
    <trd lng="be">&c_c;&c_ju;&c_r;&c_s;&c_k;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>Turk-</trd>
    </trdgrp>
    <trd lng="en">Turkic</trd>
    <trd lng="hu">t&uuml;rk</trd>
    <trd lng="nl">behorend tot de <ind>Turks</ind>e taalgroep</trd>
    <trd lng="pl">turecki</trd>
    <trd lng="ru">&c_t;&c_ju;&c_r;&c_k;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="tjurk.0alingvo">
    <kap><tld/>a lingvo</kap>
    <snc>
        <dif>
            Lingvo el la <tld/>a subfamilio de la
            <ref tip="malprt" cel="altaj.0a_lingvofamilio">Altaja
                lingvofamilio</ref>:
            <ekz>
                iam en la filmo, Lo kantas al si en ia <tld/>a lingvo,
                kaj, &ccirc;ar evidente &ccirc;io okazas en la dezerto
                de
                &Scirc;in&gcirc;iang,
                mi supozas lin ujguro.
                <fnt>
                    <aut>Don Harlow</aut>:
                    <vrk>Ka&ubreve;ranta tigro, ka&scirc;ita drako</vrk>
                    <lok><url ref="http://www.best.com/~donh/Esperanto/Literaturo/Recenzoj/tiger_dragonu.html#rfn01">(recenzo)</url></lok></fnt>.
            </ekz>
        </dif>
    </snc>
    <trd lng="be">&c_c;&c_ju;&c_r;&c_s;&c_k;&c_a;&c_ja;
    &c_m;&c_o;&c_v;&c_a;</trd>
    <trd lng="cs">turkotatarsk&yacute; jazyk</trd>
    <trdgrp lng="de">
      <trd>Turksprache</trd>,
      <trd>T&uuml;rksprache</trd>
    </trdgrp>
    <trd lng="en">Turkic language</trd>
    <trd lng="hu">t&uuml;rk nyelv</trd>
    <trd lng="nl">taal behorend tot de <ind>Turks</ind>e taalgroep</trd>
    <trd lng="pl">j&eogonek;zyk <ind>turecki</ind></trd>
    <trd lng="ru">&c_t;&c_ju;&c_r;&c_k;&c_s;&c_k;&c_i;&c_j;
    &c_ja;&c_z;&c_y;&c_k;</trd>
</drv>
</art>

