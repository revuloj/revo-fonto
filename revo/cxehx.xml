<vortaro>
<art mrk="$Id: cxehx.xml,v 1.32 2024/10/23 07:29:35 revo Exp $">
<kap><ofc>10</ofc>
  <rad>&ccirc;e&hcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="cxehx.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <snc mrk="cxehx.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="cxehx.CX0ujo"><tld lit="&Ccirc;"/>ujo</ref>:
      <ekz>
        mi havas sufi&ccirc;e da kontaktoj kun sovetianoj, hungaroj, <nom>GDR</nom>-anoj kaj <tld/>oj
        <fnt>
          <bib>FSI</bib>
          <lok>III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la svisoj, la <tld/>oj kaj poloj tion tute same faras
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la rusoj invadis Pragon, la rezistantaj <tld/>oj demonstris sian
        humursenton &scirc;ercante pri <nom>Winnetou</nom>, konsiderata kiel la lasta bona ru&gcirc;a
        kamarado
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9310-sarajevo.txt">La Fino De La
          Mondo Komencigxas En Sarajevo</url></vrk>
          <lok>93/10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="cxehx.0o.naciano">
    <dif>
      Homo kun <tld/>a <ref tip="lst" cel="naci.0eco" lst="voko:popolanoj">nacieco</ref>:
      <ekz>
        novelojn kaj rakontojn publikigis la <tld lit="&Ccirc;"/>o <nom>Vladimir Vana</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ilia]</klr> gepatra lingvo konas nur tri gradojn de vokala vasteco
        (<tld/>oj, kroatoj, grekoj, japanoj k.m.a.)
        <fnt>
          <bib>LAE</bib>
          <lok>2. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj (precipe maljunaj) citavanoj konsideras sin &gcirc;is nun moravianoj, dum la
        plimulto sentas sin bohemianoj (a&ubreve; la junaj prifajfas tion, konsiderante sin
        <tld/>oj)
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Chrdle</aut>
          <vrk><url ref="https://www.monato.be/2009/009780.php">Nova Esperanto-muzeo (2)</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_h;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_h;</trd>
  <trd lng="cs">&Ccaron;ech</trd>
  <trd lng="de">Tscheche</trd>
  <trd lng="en">Czech</trd>
  <trd lng="fr">Tch&egrave;que<klr> (subst.)</klr></trd>
  <trd lng="he">&he;&resh;&pe;&vav;&bet;&lamed;&yod;&qof;&he; &he;&tsadi;'&kaf;&yod;&tav;</trd>
  <trd lng="ja">&#12481;&#12455;&#12467;&#20154;  <pr>&#12481;&#12455;&#12467;&#12376;&#12435;</pr></trd>
  <trd lng="nl">Tsjech</trd>
  <trd lng="no">tsjekker</trd>
  <trd lng="it">ceco<klr> (sost.)</klr></trd>
  <trd lng="id">Cek</trd>
  <trdgrp lng="pl">
    <trd>Czech</trd>,
    <trd>pepik<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_je;&c_h;</trd>
  <trd lng="zh">&#25463;&#20811;&#20154; <pr>ji&eacute;k&egrave;r&eacute;n</pr></trd>
</drv>

<drv mrk="cxehx.0a">
  <kap><ofc>10</ofc><tld/>a</kap>
  <snc mrk="cxehx.0a.KOMUNE">
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        mi redaktis tiun peton en la <tld/>a
        <ref tip="lst" cel="lingv.0o.homa" lst="voko:lingvoj">lingvo</ref>
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sovetia biero? espereble ili tie havos <tld/>an
        <fnt>
          <bib>CKv</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a poeto &Scirc;tefo <nom>Urban</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_sh;&c_k;&c_i;</trd>
  <trd lng="cs">&ccaron;esk&yacute;</trd>
  <trd lng="de">tschechisch</trd>
  <trd lng="en">Czech</trd>
  <trd lng="fr">tch&egrave;que<klr> (adj.)</klr></trd>
  <trd lng="he">&tsadi;'&kaf;&yod;</trd>
  <trd lng="it">ceco<klr> (agg.)</klr></trd>
  <trd lng="ja">&#12481;&#12455;&#12467;&#12398;  <pr>&#12385;&#12359;&#12371;&#12398;</pr></trd>
  <trd lng="nl">Tsjechisch</trd>
  <trd lng="no">tsjekkisk</trd>
  <trd lng="pl">czeski</trd>
  <trd lng="uk">&c_ch;&c_je;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trd lng="zh">&#25463;&#20811;&#35486; <pr>ji&eacute;k&egrave;y&#468;</pr></trd>
</drv>

<drv mrk="cxehx.CX0ujo">
  <kap><ofc>10</ofc><tld lit="&Ccirc;"/>ujo</kap>
  <snc mrk="cxehx.CX0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      situanta en Mez<ref tip="malprt" cel="euxrop1.E0o">e&ubreve;ropo</ref>
      kun &ccirc;efurbo <ref tip="prt" cel="prag.0o">Prago</ref>.

    </dif>
    <ref tip="sin" cel="cxehxi.CX0o">&Ccirc;e&hcirc;io</ref>
  </snc>
  <trd lng="be">&c_Ch;&c_e;&c_h;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_Ch;&c_je;&c_h;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>&Ccaron;esko</trd>,
    <trd>&Ccaron;esk&aacute; republika</trd>
  </trdgrp>
  <trd lng="de">Tschechien</trd>
  <trdgrp lng="en">
    <trd>Czechia</trd>,
    <trd>Czech Republic</trd>
  </trdgrp>
  <trd lng="es">Rep&uacute;blica Checa</trd>
  <trdgrp lng="fr">
    <trd>Tch&eacute;quie</trd>,
    <trd>R&eacute;publique tch&egrave;que</trd>
  </trdgrp>
  <trd lng="id">Republik <ind>Ceko</ind></trd>
  <trdgrp lng="it">
   <trd>Cechia</trd>,
   <trd><klr>Repubblica </klr>Ceca</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Tsjechi&euml;</trd>,
    <trd><ind>Tsjechische</ind> Republiek</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>Tsjekkia</trd>,
    <trd><ind>Tsjekkisk</ind> Republikk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Czechy</trd>,
    <trd>Republika <ind>Czeska</ind></trd>
  </trdgrp>
  <trd lng="pt">Rep&uacute;blica Checa</trd>
  <trdgrp lng="zh">
    <trd>&#25463;&#20811; <pr>ji&eacute;k&egrave;</pr></trd>,
    <trd>&#25463;&#20811;&#20849;&#21644;&#22269; <pr>ji&eacute;k&egrave;g&ograve;ngh&eacute;gu&oacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cxehx.0slovaka">
  <kap>
    <ofc>10</ofc><tld/>oslovaka
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>oslovakoj, al <tld lit="&Ccirc;"/>oslovakujo:
      <ekz>
        la <tld/>oslovaka skolo montri&gcirc;is ankora&ubreve; vivanta, kvankam &gcirc;i povis &gcirc;is
        anta&ubreve;nelonge nur tre malmulton eldoni enlande
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1935 aperis cetere <tld/>oslovaka Antologio
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Pitter</nom> estis ekzilita de la komunista <tld/>oslovaka registaro kaj lia
        entrepreno devis da&ubreve;ri sen li
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008886.php"
              >Prisilentita heroo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_h;&c_a;&c_s;&c_l;&c_a;&c_v;&c_a;&c_c;&c_k;&c_ib;</trd>
  <trd lng="de">tschechoslovakisch</trd>
</drv>

<drv mrk="cxehx.0slovako">
  <kap>
    <ofc>10</ofc><tld/>oslovako
  </kap>
  <snc>
    <dif>
      Civitano de <tld lit="&Ccirc;"/>oslovakujo:
      <ekz>
        fine, anka&ubreve; la Bulgaroj kaj <tld lit="&Ccirc;"/>oslovakoj devis cedi al la oficiala
        klarigo, ke necesas nun dedi&ccirc;i sin al pli ur&gcirc;aj taskoj ol flegado de
        internacia lingvo, ke dum konstruado de socialismo simple ne estas loko por
        Esperanto
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_h;&c_a;&c_s;&c_l;&c_a;&c_v;&c_a;&c_k;</trd>
  <trd lng="de">Tschechoslovake</trd>
</drv>

<drv mrk="cxehx.CX0oslovakujo">
  <kap><ofc>10</ofc><tld lit="&Ccirc;"/>oslovakujo</kap>
  <snc mrk="cxehx.CX0oslovakujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Eksa
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref>
      (1918-1992) situanta en Mez<ref tip="malprt" cel="euxrop1.E0o">e&ubreve;ropo</ref>,
      en la teritorio de nuntempaj <ref tip="prt" cel="cxehx.CX0ujo.GEOG">&Ccirc;e&hcirc;ujo</ref>
      kaj <ref tip="prt" cel="slovak.S0ujo.GEOG">Slovakujo</ref>;
      <ref tip="sin" cel="cxehxoslovaki.0o.GEOG">&Ccirc;e&hcirc;oslovakio</ref>:
      <ekz>
        Esperanto en <tld lit="&Ccirc;"/>oslovakujo dum la germana okupado
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        hejme sin sentis nek la slovakoj en la tiel nomita Hungarujo, lando de hungaroj,
        nek la hungaroj en <tld lit="&Ccirc;"/>oslovakujo, lando de &ccirc;e&hcirc;oj kaj slovakoj
        <fnt>
          <bib>FSp</bib>
          <lok>Dua Parto: Svisa Sperto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ch;&c_e;&c_h;&c_a;&c_s;&c_l;&c_a;&c_v;&c_a;&c_k;&c_ib;&c_ja;</trd>
  <trd lng="cs">&Ccaron;eskoslovensko</trd>
  <trd lng="en">Czechoslovakia</trd>
  <trd lng="es">Checoslovaquia</trd>
  <trd lng="fr">Tch&eacute;coslovaquie</trd>
  <trd lng="nl">Tsjechoslowakije</trd>
  <trd lng="no">Tsjekkoslovakia</trd>
  <trd lng="pl">Czechos&lstroke;owacja</trd>
</drv>
</art>

