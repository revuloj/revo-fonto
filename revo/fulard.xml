<art mrk="$Id: fulard.xml,v 1.16 2023/11/10 06:08:01 revo Exp $">
<kap>
  <rad>fulard</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fulard.0o">
  <kap><tld/>o</kap>
  <snc mrk="fulard.0o.TEKS_sxtofo">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Malpeza silka teksa&jcirc;o por vestoj, kravatoj ktp. 
    </dif>
    <trd lng="de">Foulard</trd>
    <trd lng="hu">selyemkelme</trd>
    <trd lng="nl">foulard</trd>
    <trd lng="pt">fular</trd>
    <trd lng="zh">&#x8ECB;&#x8ECA; <pr>y&#xE0;ch&#x113;</pr></trd>
</snc>
  <snc mrk="fulard.0o.TEKS_vesto">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Delikata <ref tip="super" cel="tuk.kol0o.TEKS">koltuko</ref>,
      el silko a&ubreve; alia malpeza &scirc;tofo:  
      <ekz>
        ne zorgu pri mi! Vi ne portas <tld/>on, uzu do la mian
        <fnt>
          <aut>M. Kuroda</aut>,
          <vrk>Ambur&gcirc;ono</vrk>,
          <lok>Animo drivas, 1990</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Seidenhalstuch</trd>
    <trdgrp lng="hu">
      <trd>selyemkend&odblac;</trd>,
      <trd>selyems&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">halsdoekje</trd>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>fulard</trd>,
    <trd>ful&#xE1;r</trd>
  </trdgrp>
  <trd lng="fr">foulard</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30FC;&#x30E9;&#x30FC;&#x30C9;</trd>,
    <trd>&#x30B9;&#x30AB;&#x30FC;&#x30D5;</trd>,
    <trd>&#x30CD;&#x30C3;&#x30AB;&#x30C1;&#x30FC;&#x30D5;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>foulard</trd>,
    <trd>echarpe</trd>,
    <trd>cachen&ecirc;</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>jemn&#xE1; hodv&#xE1;bna tkanina</trd>,
    <trd>&#x161;atka na krk</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x443;&#x43B;&#x44F;&#x440; (&#x442;&#x43A;&#x430;&#x43D;&#x438;&#x43D;&#x430;)</trd>,
    <trd>&#x43D;&#x430;&#x448;&#x438;&#x439;&#x43D;&#x430; &#x445;&#x443;&#x441;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x433;&#x430;&#x43B;&#x441;&#x442;&#x443;&#x43A; (&#x43F;&#x456;&#x43E;&#x43D;&#x435;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439;)</trd>
  </trdgrp>
</drv>
</art>
