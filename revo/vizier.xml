<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vizier</rad>/o
</kap>
<drv mrk="vizier.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Movebla viza&gcirc;&scirc;irmilo de kasko.
    </dif>
    <trd lng="nl">vizier</trd>
    <trd lng="ru">&c_z;&c_a;&c_b;&c_r;&c_a;&c_l;&c_o;</trd>
  </snc>
  <snc>
    <dif>
      Anta&ubreve;rando de kapvesto
    </dif>
    <refgrp tip="sin">
      <ref cel="bek.0o">beko</ref>,
      <ref cel="kasked.0o">kaskedo</ref>.
    </refgrp>
    <trd lng="nl">klep<klr tip="ind"> (v.pet)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>
  </snc>
  <trd lng="es">visera</trd>
</drv>
</art>
