<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>eduk</rad>/i
</kap>
<drv mrk="eduk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Direkti la elvolvi&gcirc;on de fizikaj, moralaj kaj intelektaj kapabloj
      de infano a&ubreve; junulo:
      <ekz>
        oni <tld/>is lin kiel idealiston;
      </ekz>
      <ekz>
        sin dedi&ccirc;i al <ind><tld/>ado</ind> de junuloj.
        <trd lng="fr">&eacute;ducation<klr tip="ind"> (des jeunes)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="zorg.0i">zorgi</ref>,
      <ref cel="vart.0i">varti</ref>,
      <ref cel="kultur.0i">kulturi</ref>,
      <ref cel="discip">disciplini</ref>,
      <ref cel="guvern.0i">guverni</ref>,
      <ref cel="instru.0i">instrui</ref>.
    </refgrp>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Dresi beston:
      <ekz>
        <tld/>i tigron<fnt>Z</fnt>,
      </ekz>
      <ekz>
        hundon, papagon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sovagx">malsova&gcirc;igi</ref>,
      <ref cel="instru.0i">instrui</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_r;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Instrui iun pri la manieroj kaj kutimoj de &gcirc;entileco kaj dececo.
    </dif>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Flegi vegeta&jcirc;on:
      <ekz>
        <tld/>i hederon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vegeta&jcirc;oj <tld/>itaj en oran&gcirc;erioj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vychov&aacute;vat</trd>
  <trd lng="en">educate</trd>
  <trdgrp lng="fr">
    <trd>dresser<klr tip="ind"> (un animal)</klr></trd>,
    <trd>&eacute;duquer<klr tip="ind"> (un enfant)</klr></trd>,
    <trd>&eacute;lever<klr tip="ind"> (un enfant)</klr></trd>,
    <trd>faire <ind>pousser</ind><klr tip="ind"> (une plante)</klr></trd></trdgrp> 
</drv>
<drv mrk="eduk.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu profesie <tld/>as infanojn a&ubreve; junulojn a&ubreve; bestojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="instru.0isto">instruisto</ref>,
      <ref cel="pedago.0o">pedagogo</ref>,
      <ref cel="mentor.0o">mentoro</ref>,
      <ref cel="dres">dresisto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;ducateur</trd>,
    <trd>dresseur</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
<drv mrk="eduk.bon0iteco">
  <kap>bon<tld/>iteco</kap>
  <snc>
    <dif>
      Kvalito de tiu, kiu obeas la kutimojn de &gcirc;entileco kaj dececo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;ducation<klr tip="ind"> (bonne &eacute;ducation)</klr></trd></trdgrp> 
  <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
