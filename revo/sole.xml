<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sole</rad>/o
</kap>
<drv mrk="sole.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="pleuxr3.0oj">ple&ubreve;ronektedoj</ref>
      (<trd lng="la">Solea</trd>), plata mara bongusta fi&scirc;o,
      kun okuloj sur unu flanko kaj kun nesimetria bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Seezunge</trd>
  <trd lng="fr">sole</trd>
  <trd lng="nl">tong<klr tip="ind"> (vis)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_ja;&c_z;&c_y;&c_k;<klr tip="ind"> (&c_r;&c_y;&c_b;&c_a;)</klr></trd>,
    <trd>&c_s;&c_o;&c_l;&c_mol; <klr tip="amb">(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
  </trdgrp>
</drv>
</art>
