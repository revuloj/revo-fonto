<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sole.xml,v 1.21 2022/03/27 17:06:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sole</rad>/o
</kap>
<drv mrk="sole.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Plata mara bongusta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>, kun iom langoforma korpo,
      kun okuloj sur unu flanko bruna makulita kaj kun nesimetria bu&scirc;o;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="soleed.0oj">soleedoj</ref>
      (<trd lng="la">Solea</trd>):
      <ekz>
        <klr>[en]</klr> litova kuirarto <klr>[&#x2026;]</klr> fuma&jcirc;itaj fi&scirc;oj (kaj maraj kaj dol&ccirc;akvaj) estas populaj pladoj, inter ili <tld/>oj kaj angiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=309749">Litova kuirarto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cd/Solea_solea_1.jpg/640px-Solea_solea_1.jpg" prm="CC BY-SA 4.0"><tld/>o (Solea solea), Dover, Belga bordakvo
        <fnt>
          <aut>&#xA9; Hans Hillewaert, propra verko</aut>
          <lok><url ref="&WCW;/File:Solea_solea_1.jpg">&WCW;/File:Solea_solea_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">llenguado</trd>
  <trd lng="cs">l&yacute;tkov&yacute; sval</trd>
  <trd lng="de">Seezunge</trd>
  <trd lng="en">Solea <klr>(includes common sole)</klr></trd>
  <trd lng="fr">sole</trd>
  <trd lng="hu">nyelvhal</trd>
  <trd lng="nl">tong<klr tip="ind"> (vis)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_ja;&c_z;&c_y;&c_k;<klr tip="ind"> (&c_r;&c_y;&c_b;&c_a;)</klr></trd>,
    <trd>&c_s;&c_o;&c_l;&c_mol; <klr tip="amb">(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="sk">l&yacute;tkov&yacute; sval</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x44F;&#x437;&#x438;&#x43A;</trd>
</drv>
</art>
