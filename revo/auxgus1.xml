<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>A&ubreve;gust</rad>/o
    </kap>

<drv mrk="auxgus1.0o">
  <kap><ofc>*</ofc><tld lit="A"/>o</kap>
  <snc mrk="auxgus1.A0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld lit="A"/>o estas mia plej amata filo
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>;
      </ekz>
      <ekz>
        Lia Regnestra Mo&scirc;to la Re&gcirc;o Frederiko <tld/>o de Saksujo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj tagoj eliris dekreto de Cezaro <tld/>o, ke la tuta mondo estu registrita
        <fnt>
          <bib>NT</bib>
          <lok>Luko 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a kohorto
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">August</trd>
  <trd lng="de">August<klr tip="amb"> (Vorname)</klr></trd>
  <trd lng="fr">Auguste<klr tip="amb"> (pr&eacute;nom)</klr></trd>
  <trd lng="it">Augusto</trd>
  <trd lng="ja">&#12450;&#12454;&#12464;&#12473;&#12488;&#12453;&#12473; [<ind>&#12354;&#12358;&#12368;&#12377;&#12392;&#12357;&#12377;</ind>]</trd>
  <trd lng="lat">Augustus</trd>
  <trd lng="oc">August<klr tip="amb"> (prenom)</klr></trd>
  <trd lng="pl">August</trd>
</drv>

  </art>

