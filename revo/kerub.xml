<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kerub</rad>/o
</kap>
<drv mrk="kerub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Supernatura esta&jcirc;o, en formo de flugilhava virbovo kun homa
kapo. 
    </dif>
    <trd lng="en">cherub</trd>
    <trdgrp lng="de">
      <trd>Cherub</trd>,
      <trd>Kerub</trd>
    </trdgrp>
    <trd lng="hu">kerub</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">KAT</uzo>
    <dif>
      An&gcirc;elo de la dua &ccirc;iela &hcirc;oro, kiun oni prezentas en
pentra&jcirc;oj
      a&ubreve; skulpta&jcirc;oj per flugilhava kapo de infano. 
    </dif>
    <refgrp tip="sin">
      <ref cel="seraf.0o">serafo</ref>.
    </refgrp>
    <trd lng="en">cherub</trd>
    <trd lng="de">Cherubim</trd>
    <trd lng="hu">kerub</trd>
    <trd lng="ru">&c_h;&c_je;&c_r;&c_u;&c_v;&c_i;&c_m;</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;arma infano.  
    </dif>
    <trd lng="en">cherub</trd>
    <trd lng="hu">kisangyal</trd>
    <trd lng="ru">&c_h;&c_je;&c_r;&c_u;&c_v;&c_i;&c_m;</trd>
  </snc>
<trd lng="nl">cherubijn</trd>
</drv>
</art>
