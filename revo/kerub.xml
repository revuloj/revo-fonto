<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kerub</rad>/o
</kap>
<drv mrk="kerub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Supernatura esta&jcirc;o, en formo de flugilhava virbovo kun homa
      kapo.
    </dif>
    <trd lng="en">cherub</trd>
    <trdgrp lng="de">
      <trd>Cherub</trd>,
      <trd>Kerub</trd>
    </trdgrp>
    <trd lng="fr">ch&eacute;rubin<klr tip="ind"> (ange zoomorphe)</klr></trd>
    <trd lng="hu">kerub</trd>
  </snc>
  <snc>
    <uzo tip="fak">KAT</uzo>
    <dif>
      An&gcirc;elo de la dua &ccirc;iela &hcirc;oro, kiun oni prezentas en
      pentra&jcirc;oj a&ubreve; skulpta&jcirc;oj per flugilhava kapo de infano.
    </dif>
    <refgrp tip="vid">
      <ref cel="seraf.0o">serafo</ref>.
    </refgrp>
    <trd lng="en">cherub</trd>
    <trd lng="de">Cherubim</trd>
    <trd lng="fr">ch&eacute;rubin<klr tip="ind"> (ange du deuxi&egrave;me ch&oelig;ur)</klr></trd>
    <trd lng="hu">kerub</trd>
    <trd lng="ru">&c_h;&c_je;&c_r;&c_u;&c_v;&c_i;&c_m;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;arma infano.
    </dif>                     
    <trd lng="br">kaezhig Doue</trd>
    <trd lng="en">cherub</trd>
    <trdgrp lng="fr">
      <trd>ch&eacute;rubin<klr tip="ind"> (enfant charmant)</klr></trd>,
      <trd>ange<klr tip="ind"> (enfant charmant)</klr></trd>
    </trdgrp>
    <trd lng="hu">kisangyal</trd>
    <trd lng="ru">&c_h;&c_je;&c_r;&c_u;&c_v;&c_i;&c_m;</trd>
  </snc>
  <trd lng="be">&c_h;&c_je;&c_r;&c_u;&c_v;&c_ib;&c_m;</trd>  
  <trd lng="br">kerubin</trd>
  <trd lng="nl">cherubijn</trd>
</drv>
</art>
