<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>episkop</rad>/o
    </kap>
    <drv mrk="episko.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="eklezi.0ulo">Ekleziulo</ref>
          konsekrita en la plejsupran gradon de pastrado,
          kiu en la plimulto de <tld/>aj eklezioj estas rigardata kiel
          seninterrompa transmeto de
          <ref tip="vid" cel="aposto.0o.KRI">apostola</ref> dono.
          Kutime <tld/>o estas religia estro de
          <ref tip="vid" cel="episko.0ujo0lando"><tld/>ujo</ref>.
        </dif>
      </snc>
      <trd lng="el">&epsilon_psili;&pi;&jota_acute;&sigma;&kappa;&omikron;&pi;&omikron;&sigma_fina;</trd>
      <trd lng="ru">&c_je;&c_p;&c_i;&c_s;&c_k;&c_o;&c_p;</trd>
    </drv>
    <drv mrk="episko.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Rezidejo de <tld/>o kaj la <tld/>uja administrejo.
        </dif>
      </snc>
      <trd lng="ru">&c_je;&c_p;&c_i;&c_s;&c_k;&c_o;&c_p;&c_i;&c_ja;</trd>
    </drv>
    <drv mrk="episko.0ujo0lando">
      <kap><tld/>ujo, <tld/>lando</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Eklezia distrikto estrata de <tld/>o. 
        </dif>
      </snc>
      <trd lng="el">&epsilon_psili;&pi;&alfa;&rho;&chi;&jota_acute;&alfa;</trd>
      <trdgrp lng="en"><trd>bishopric</trd>, <trd>diocese</trd></trdgrp>
      <trd lng="ru">&c_je;&c_p;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
    </drv>
    <drv mrk="episko.cxef0o">
      <kap>&ccirc;ef<tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Altranga <tld/>o.
        </dif>
      </snc>
      <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_je;&c_p;&c_i;&c_s;&c_k;&c_o;&c_p;</trd>
    </drv>
  </art>
<!--
$Log$
-->
</vortaro>
