<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Babiloni</rad>/o
</kap>

<drv mrk="babiloni.0o">
  <kap><tld/>o <fnt>PIV</fnt></kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Antikva regno en
      <ref tip="malprt" cel="mezopotami.0o">Mezopotamio</ref>,
      kaj kies &ccirc;efurbo estis
      <ref tip="prt" cel="babilon.0o">Babilono</ref>:
      <ekz>
        la tuta <ref cel="sumer.0ujo">Sumerujo</ref> i&gcirc;is
        parto de la suda <tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="babel.0o">Babelo</ref>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_b;&c_ib;&c_l;&c_jo;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="de">Babylonien</trd>
  <trd lng="en">Babylonia</trd>
  <trd lng="fr">Babylonie</trd>
  <trd lng="hu">Babil&oacute;nia</trd>
  <trd lng="nl">Babyloni&euml;</trd>
  <trd lng="ru">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_i;&c_ja;</trd>
</drv>

</art>

