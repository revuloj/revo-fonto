<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: silen1.xml,v 1.3 2023/12/26 07:23:45 afido Exp $">
    <kap>
      <rad>Silen</rad>/o
    </kap>

<drv mrk="silen1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc mrk="silen1.0o.dio">
    <dif>
      <klr>(en la greka mitologio)</klr> <ref tip="super" cel="di.0o.FIL">Dio</ref>,
      edukanto de <ref cel="dioniz.0o.dio">Dionizo</ref>:
      <ekz>
        <tld/>o, kiu bonvenigis la gastojn en la majesta atrio, kutime sekvis Dionizon,
        la grekan dion de la vino
        <fnt>
          <bib>CRI&#8239;</bib>
          <aut>Alessandra Madella</aut>
          <vrk><url ref="https://esperanto.cri.cn/audio/program2/4752/20220304/740108.html"
              >Dioniza tigro de la vina Silka Vojo</url></vrk>
          <lok>2022-03-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Midas</nom> tuj ekkonis, ke la oldulo estas <tld/>o
        <fnt>
          <aut>Eduard Peti&scirc;ka, trad. Cezar </aut>
          <vrk><url ref="https://unpoised2.rssing.com/chan-3152057/all_p15.html"
              >Midaso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="satiru.0o.MIT">satiruso</ref>
    <trd lng="be">&c_S;&c_y;&c_l;&c_je;&c_n; <klr>(&c_g;&c_r;&c_e;&c_c;&c_k;&c_ib; &c_b;&c_o;&c_g;)</klr></trd>
    <trd lng="de">Silenos</trd>
    <trd lng="en">Silenus</trd>
    <trd lng="fr">Sil&egrave;ne</trd>
    <trdgrp lng="grc">
      <trd>&Sigma;&jota;&lambda;&eta;&ny;&omikron_ton;&sigma_fina;</trd>,
      <trd>&Sigma;&epsilon;&jota;&lambda;&eta;&ny;&omikron_ton;&sigma_fina;</trd>
    </trdgrp>
    <trdgrp lng="lat">
      <trd>Silenus</trd>,
      <trd>Selenus</trd>
    </trdgrp>
    <trd lng="pl">Sylen</trd>
    <trd lng="ru">&c_S;&c_i;&c_l;&c_je;&c_n;</trd>
  </snc>
  <ref tip="hom" cel="silen.0o">sileno (planto)</ref>
</drv>
  </art>

