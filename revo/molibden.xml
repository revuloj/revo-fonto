<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>molibden</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="molibden.0o">
  <kap><tld/>o</kap>
  <mlg>Mo</mlg>
  <snc mrk="molibden.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 42, atompezo 95,94, simbolo <ctl>Mo</ctl>.
      Griza substanco.
    </dif>
  </snc>
  <adm>
    SPV (suplemento de Plena Vortaro) markas la vorton kiel zamenhofan.
    <aut>&jcirc;v</aut>
  </adm>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ib;&c_b;&c_d;&c_e;&c_n;</trd>    
  <trd lng="br">molibden</trd>
  <trd lng="ca">molibd&egrave;</trd>
  <trd lng="de">Molybd&auml;n</trd>
  <trd lng="en">molybdenum</trd>
  <trd lng="es">molibdeno</trd>
  <trd lng="fr">molybd&egrave;ne</trd>
  <trd lng="hu">molibd&eacute;n</trd>
  <trd lng="nl">molybdeen</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_i;&c_b;&c_d;&c_je;&c_n;</trd>
</drv>
</art>
