<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nit.xml,v 1.25 2024/01/29 15:59:10 revo Exp $">
<kap>
  <rad>nit</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nit.0o">
   <kap><tld/>o</kap>
   <snc mrk="nit.0o.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Metala stangeto, ofte kun kapeto simila al la najla &ccirc;e unu
       ekstremo, uzata por fari
       <ref tip="malprt" cel="nit.0junto">nitjunton</ref>
       traigante la kunigatajn
       pecojn per la <tld/>o kaj bate displatigante ties duan
       ekstremon por formi la duan kapeton fiksantan. Ekzistas
       anka&ubreve; glataj <tld/>oj, kies kapetojn oni formas samtempe
       dum
       la fiksado:
       <ekz>
         al iliaj oreloj venis la malla&ubreve;ta klako de la fortiritaj
         <tld/>egoj
         <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
         <lok>1982</lok></fnt>.
       </ekz>
     </dif>
     <ref tip="sin" cel="vinkt.0o.nito">vinkto</ref>
   </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>nit <klr>(nyt)</klr></trd>,
    <trd>nt</trd>
  </trdgrp>
  <trd lng="de">Niet(e)</trd>
  <trd lng="el">&pi;&rho;&jota;&tau;&sigma;&jota_ton;&ny;&jota;</trd>
  <trd lng="en">rivet</trd>
  <trd lng="es">remache</trd>
  <trd lng="fa">&f_p;&a_r;&f_ch;</trd>
  <trd lng="fr">rivet</trd>
  <trd lng="hu">szegecs</trd>
  <trd lng="ja">&#x30EA;&#x30D9;&#x30C3;&#x30C8;</trd>
  <trd lng="nl">klinknagel</trd>
  <trd lng="pl">nit</trd>
  <trd lng="pt">rebite</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>nit</trd>,
    <trd>nt</trd>
  </trdgrp>
  <trd lng="uk">&#x437;&#x430;&#x43A;&#x43B;&#x435;&#x43F;&#x43A;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x925A;&#x91D8; <pr>m&#x1CE;od&#x12B;ng</pr></trd>,
    <trd>&#x94C6;&#x9489; <pr>m&#x1CE;od&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nit.0i">
   <kap><tld/>i</kap>
   <snc mrk="nit.0i.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Kunfiksi per <tld/>o(j).
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_a;&c_k;&c_l;&c_ja;&c_p;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trd lng="cs">n&yacute;tovat</trd>
  <trd lng="de">nieten</trd>
  <trd lng="el">&pi;&rho;&jota;&tau;&sigma;&ny;&alfa_ton;&rho;&omega;</trd>
  <trd lng="en">to <ind>rivet</ind></trd>
  <trd lng="es">remachar</trd>
  <trd lng="fa">&f_p;&a_r;&f_ch; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
     <trd>riveter</trd>,
     <trd>river<klr tip="ind"> (deux plaques)</klr></trd>
   </trdgrp>
  <trd lng="ja">&#x30EA;&#x30D9;&#x30C3;&#x30C8;&#x3067;&#x7559;&#x3081;&#x308B;  <pr>&#x30EA;&#x30D9;&#x30C3;&#x30C8;&#x3067;&#x3068;&#x3069;&#x3081;&#x308B;</pr></trd>
  <trd lng="nl">vastklinken</trd>
  <trd lng="pl">nitowa&cacute;</trd>
  <trd lng="pt">rebitar</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">nitova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x435;&#x43F;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x43A;&#x43B;&#x435;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x2019;&#x454;&#x434;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438; &#x437;&#x430;&#x43A;&#x43B;&#x435;&#x43F;&#x43A;&#x430;&#x43C;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x925A; <pr>m&#x1CE;o</pr></trd>,
    <trd>&#x94C6; <pr>m&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nit.0najlo">
   <kap><tld/>najlo</kap>
   <snc mrk="nit.0najlo.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       <tld lit="N"/>o kies unu kapeto estas anta&ubreve;fabrikita.
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_a;&c_ch;&c_n;&c_y; &c_c;&c_mol;&c_v;&c_ib;&c_k;</trd>
   <trdgrp lng="de">
       <trd>Blindniete</trd>,
       <trd>Nietnagel</trd>
   </trdgrp>
   <trd lng="es">robl&oacute;n</trd>
   <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_o;&c_ch;&c_n;&c_y;&c_j;
   &c_g;&c_v;&c_o;&c_z;&c_d;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x76F2;&#x925A;&#x91D8; <pr>m&#xE1;ngm&#x1CE;od&#x12B;ng</pr></trd>,
    <trd>&#x76F2;&#x94C6;&#x9489; <pr>m&#xE1;ngm&#x1CE;od&#x12B;ng</pr></trd>,
    <trd>&#x925A;&#x91D8; <pr>m&#x1CE;od&#x12B;ng</pr></trd>,
    <trd>&#x94C6;&#x9489; <pr>m&#x1CE;od&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nit.0junto">
   <kap><tld/>junto</kap>
   <snc mrk="nit.0junto.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Nedisigebla kunigo de pecoj per <tld/>oj.
     </dif>
     <ref tip="super" cel="junt.0o.kunigo">junto</ref>
   </snc>
   <trd lng="be">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_a;&c_ch;&c_n;&c_a;&c_je; &c_z;&c_l;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
   <trd lng="de">Nietverbindung</trd>
   <trd lng="en">rivet(ed) joint</trd>
   <trd lng="pl">po&lstroke;&aogonek;czenie <ind>nitowane</ind></trd>
   <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_o;&c_ch;&c_n;&c_o;&c_je;
   &c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#x925A;&#x63A5; <pr>m&#x1CE;oji&#x113;</pr></trd>,
    <trd>&#x94C6;&#x63A5; <pr>m&#x1CE;oji&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
