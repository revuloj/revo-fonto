<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nit</rad>/o
</kap>
<drv mrk="nit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala stangeto, ofte kun kapeto simila al la najla &ccirc;e unu
      ekstremo, uzata por fari <ref tip="malprt"
      cel="nit.0junto">nitjunton</ref> traigante la kungatajn
      pecojn per la <tld/>o kaj bate displatigante ties duan
      ekstremon por formi la duan kapeton fiksantan. Ekzistas
      anka&ubreve; glataj <tld/>oj, kies kapetojn oni formas samtempe dum
      la fiksado.
    </dif>
    <ref tip="sin" cel="vinkt.0o.nito">vinkto</ref>
  </snc>
  <trd lng="de">Niet(e)</trd>
  <trd lng="el">&pi;&rho;&jota;&tau;&sigma;&jota_ton;&ny;&jota;</trd>
  <trd lng="en">rivet</trd>
  <trd lng="hu">szegecs</trd>
  <trd lng="nl">klinknagel</trd>
  <trd lng="pt">rebite</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_k;&c_a;</trd>
</drv>

<drv mrk="nit.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Kunfiksi per <tld/>o(j).
    </dif>
  </snc>
  <trd lng="de">nieten</trd>
  <trd lng="el">&pi;&rho;&jota;&tau;&sigma;&ny;&alfa_ton;&rho;&omega;</trd>
  <trd lng="en">to <ind>rivet</ind></trd>
  <trd lng="pt">rebitar</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="nit.0najlo">
  <kap><tld/>najlo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="N"/>o kies unu kapeto estas anta&ubreve;fabrikita.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Blindniete</trd>,
      <trd>Nietnagel</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_g;&c_v;&c_o;&c_z;&c_d;&c_mol;</trd>
</drv>

<drv mrk="nit.0junto">
  <kap><tld/>junto</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Nedisigebla kunigo de pecoj per <tld/>oj.
    </dif>
    <ref tip="super" cel="junt.0o.kunigo">junto</ref>
  </snc>
  <trd lng="de">Nietverbindung</trd>
  <trd lng="en">rivet(ed) joint</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_jo;&c_p;&c_o;&c_ch;&c_n;&c_o;&c_je; &c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
