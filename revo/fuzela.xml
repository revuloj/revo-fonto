<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>fuzela&gcirc;</rad>/o</kap>
<drv mrk="fuzela.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      &Ccirc;efa struktura korpo de <ref tip="malprt" cel="aviad.0ilo">aviadilo</ref>,
      al kiu adapti&gcirc;as la flugiloj, <ref tip="vid" cel="empen.0o">empenoj</ref>
      k subekipa&jcirc;o.
    </dif>
    <ref tip="sin" cel="hul.0o">hulo</ref>
  </snc>
  <trdgrp lng="en">
    <trd>fuselage</trd>,
    <trd>hull</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_ju;&c_z;&c_je;&c_l;&c_ja;&c_zh;</trd>
</drv>
</art>

