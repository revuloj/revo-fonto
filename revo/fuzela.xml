<art mrk="$Id: fuzela.xml,v 1.20 2025/03/29 18:49:18 revo Exp $">
<kap>
  <rad>fuzela&gcirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="fuzela.0o">
  <kap><tld/>o</kap>
  <snc mrk="fuzela.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      &Ccirc;efa struktura korpo de <ref tip="malprt" cel="aviad.0ilo">aviadilo</ref>,
      al kiu adapti&gcirc;as la flugiloj, <ref tip="vid" cel="empen.0o">empenoj</ref>
      kaj subekipa&jcirc;o:
      <ekz>
        en Usono fini&gcirc;is testoj kun <tld/>oj gluitaj
        <fnt>
          <aut>R. Partecke</aut>,
          <vrk><url ref="&STEB;ingxenierado/ceteraj/aviado/trafikaj-aeroplanoj.htm">
          La trafikaj aeroplanoj de la estonto</url></vrk>,
          <lok>AEST '81</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="hul.0o">hulo</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>korpo</ke>(de aviadilo).
  </rim>

  <trd lng="be">&c_f;&c_ju;&c_z;&c_e;&c_l;&c_ja;&c_zh;</trd>
  <trd lng="de">Rumpf <klr>(Flugzeug)</klr></trd>
  <trdgrp lng="en">
    <trd>fuselage</trd>,
    <trd>hull</trd>
  </trdgrp>
  <trd lng="es">fuselaje</trd>
  <trd lng="fr">fuselage</trd>
  <trd lng="hu">rep&uuml;l&odblac;g&eacute;pt&ouml;rzs</trd>
  <trd lng="it">fusoliera</trd>
  <trdgrp lng="ja">
    <trd>&#33012;&#20307;  <pr>&#12393;&#12358;&#12383;&#12356;</pr></trd>,
    <trd>&#27231;&#20307;  <pr>&#12365;&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">romp<klr tip="ind"> (van vliegtuig)</klr></trd>
  <trd lng="pt">fuselagem</trd>
  <trd lng="ru">&c_f;&c_ju;&c_z;&c_je;&c_l;&c_ja;&c_zh;</trd>
  <trd lng="uk">&c_f;&#1102;&c_z;&c_je;&#1083;&#1103;&c_zh;</trd>
  <trd lng="zh">&#26426;&#36523; <pr>j&#299;sh&#275;n</pr></trd>
</drv>
