<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fuzela.xml,v 1.13 2011/03/03 09:10:11 revo Exp $">
<kap>
  <rad>fuzela&gcirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="fuzela.0o">
  <kap><tld/>o</kap>
  <snc mrk="fuzela.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      &Ccirc;efa struktura korpo de <ref tip="malprt"
      cel="aviad.0ilo">aviadilo</ref>,
      al kiu adapti&gcirc;as la flugiloj, <ref tip="vid"
      cel="empen.0o">empenoj</ref>
      kaj subekipa&jcirc;o:
      <ekz>
        en Usono fini&gcirc;is testoj kun <tld/>oj gluitaj
        <fnt>
          <aut>R. Partecke</aut>,
          <vrk><url
          ref="http://www.eventoj.hu/steb/ingxenierado/ceteraj/aviado/trafikaj-aeroplanoj.htm">
          La trafikaj aeroplanoj de la estonto</url></vrk>,
          <lok>AEST '81</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="hul.0o">hulo</ref>
  </snc>
  <trd lng="be">&c_f;&c_ju;&c_z;&c_e;&c_l;&c_ja;&c_zh;</trd>
  <trd lng="de">Rumpf</trd>
  <trdgrp lng="en">
    <trd>fuselage</trd>,
    <trd>hull</trd>
  </trdgrp>
  <trd lng="es">fuselaje</trd>
  <trd lng="fr">fuselage</trd>
  <trd lng="hu">rep&uuml;l&odblac;g&eacute;pt&ouml;rzs</trd>
  <trd lng="it">fusoliera</trd>
  <trd lng="nl">romp<klr tip="ind"> (van vliegtuig)</klr></trd>
  <trd lng="pt">fuselagem</trd>
  <trd lng="ru">&c_f;&c_ju;&c_z;&c_je;&c_l;&c_ja;&c_zh;</trd>
</drv>
</art>

