<vortaro>
<art mrk="$Id$">
<kap><rad>fuzela&gcirc;</rad>/o</kap>
<drv mrk="fuzela.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      &Ccirc;efa struktura korpo de aviadilo, al kiu adapti&gcirc;as la flugiloj,
      empenoj k subekipa&jcirc;o.
    </dif>
    <ref tip="sin" cel="hul.0o">hulo</ref>
    <trdgrp lng="en">
      <trd>fuzelage</trd>,
      <trd>hull</trd>
    </trdgrp>
    <trd lng="ru">&c_f;&c_ju;&c_z;&c_je;&c_l;&c_ja;&c_zh;</trd>
  </snc>
</drv>
</art>

