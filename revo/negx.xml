    <trd>&a_b;&a_r;&a_f;&#x200C;&a_d;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_b;&a_r;&a_f;&#x200C;&a_r;&f_y;&a_z;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">flocon de neige</trd>
  <trd lng="he">&pe;&tav;&yod;&tav; &shin;&lamed;&#x2137;</trd>
  <trd lng="hu">h&oacute;pehely</trd>
