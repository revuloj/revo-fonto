<drv mrk="negx.0blovo">
  <kap><tld/>blovo,
    <var><kap><tld/>blovado</kap></var>
  </kap>
  <snc mrk="negx.0blovo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="vent.0ego.MET">Ventego</ref> kun <tld/>o:
      <ekz>
        senfina vintro kun &gcirc;iaj <tld/>blovoj, frostoj kaj glacioj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>III</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni staras en la marbrizo, kiu efikas al ni, kvaza&ubreve; <tld/>oblovado 
        en <nom>Saskatchewan</nom>
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 43</lok>
        </fnt>;
      </ekz>
      <ekz>
        Tri Noveloj: Pikre&gcirc;ino, <tld lit="N"/>blovado, Pafo
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_je;&c_ja;</trd>,
