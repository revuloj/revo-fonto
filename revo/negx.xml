    <trd>&a_t;&a_w;&a_f;&a_A;&a_n; &a_b;&a_r;&a_f;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blizzard</trd>,
    <trd>temp&ecirc;te de neige</trd>
  </trdgrp>
  <trd lng="he">&samekh;&vav;&pe;&tav;
  &shin;&lamed;&#8503;&yod;&fmem;</trd>
  <trd lng="hu">h&oacute;f&uacute;v&aacute;s</trd>
