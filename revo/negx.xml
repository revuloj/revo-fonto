<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ne&gcirc;</rad>/o
</kap>
<drv mrk="negx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kristali&gcirc;inta akva vaporo, kiu defalas de la atmosfero en formo de
      blankaj malpezaj floketoj:
      <ekz>
        la <tld/>o falis &ccirc;iam pli dense<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li zorgas pri &gcirc;i kiel pri <tld/>o pasintjara<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sn&iacute;h</trd>
  <trd lng="de">Schnee</trd>
  <trd lng="en">snow</trd>
  <trd lng="es">nieve</trd>
  <trd lng="fr">neige</trd>
  <trd lng="it">neve</trd>
</drv>
<drv mrk="negx.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      Aparta floko de <tld/>o.
    </dif>
  </snc>
</drv>
<drv mrk="negx.0blovado">
  <kap><tld/>blovado</kap>
  <snc>
    <dif>
      Ventego kun <tld/>o.
    </dif>
  </snc>
</drv>
<drv mrk="negx.0ofloro">
  <kap><tld/>ofloro</kap>
  <snc>
    <ref tip="dif" cel="galant.0o">galanto</ref>
  </snc>
</drv>
</art>
