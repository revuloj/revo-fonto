<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ne&gcirc;</rad>/o
</kap>
<drv mrk="negx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Kristali&gcirc;inta akva vaporo, kiu defalas de la atmosfero en formo de
      blankaj malpezaj floketoj:
      <ekz>
        la <tld/>o falis &ccirc;iam pli dense<fnt>Z</fnt>;
      </ekz>
      <ekz>li zorgas pri &gcirc;i kiel pri <tld/>o pasintjara
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sn&iacute;h</trd>
  <trd lng="de">Schnee</trd>
  <trd lng="el">&chi;&jota;&omikron_ton;&ny;&jota;</trd>
  <trd lng="en">snow</trd>
  <trd lng="es">nieve</trd>
  <trd lng="fr">neige</trd>
  <trd lng="he">&shin;&lamed;&gimel;</trd>
  <trd lng="hu">h&oacute;</trd>
  <trd lng="it">neve</trd>
  <trd lng="nl">sneeuw</trd>
  <trd lng="pt">neve</trd>
  <trd lng="ru">&c_s;&c_n;&c_je;&c_g;</trd>
</drv>

<drv mrk="negx.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Aparta floko de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Schneeflocke</trd>
  <trd lng="es">copo de nieve</trd>
  <trd lng="he">&pe;&tav;&yod;&tav; &shin;&lamed;&gimel;</trd>
  <trd lng="hu">h&oacute;pehely</trd>
  <trd lng="nl">sneeuwvlok</trd>
  <trd lng="pt">floco de neve</trd>
  <trd lng="ru">&c_s;&c_n;&c_je;&c_zh;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="negx.0ulino">
  <kap><tld/>ulino <fnt>B</fnt></kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Knabino, blanka kiel <tld/>o, el fabelo de la fratoj Grimm:
      <ekz>
        Plej bela vi estas &ccirc;e ni, ho re&gcirc;ino,
        Sed miloble pli bela estas <tld lit="N"/>ulino
        <fnt><aut>Fratoj Grimm</aut>:
        <vrk>Ne&gcirc;ulino</vrk>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schneewittchen</trd>
  <trd lng="en">Snowhite</trd>
  <trd lng="es">Blanca Nieves</trd>
  <trd lng="fr">Blanche Neige</trd>
  <trd lng="he">&shin;&lamed;&gimel;&yod;&he;</trd>
  <trd lng="hu">H&oacute;feh&eacute;rke</trd>
  <trd lng="nl">Sneeuwwitje</trd>
  <trd lng="pt">Branca de Neve</trd>
  <trd lng="ru">&c_B;&c_je;&c_l;&c_o;&c_s;&c_n;&c_je;&c_zh;&c_k;&c_a;</trd>
</drv>

<drv mrk="negx.0blovado">
  <kap><tld/>blovado</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Ventego kun <tld/>o.
    </dif>
  </snc>
  <trd lng="he">&samekh;&vav;&pe;&tav; &shin;&lamed;&gimel;&yod;&fmem;</trd>
  <trd lng="hu">h&oacute;f&uacute;v&aacute;s</trd>
  <trd lng="nl">sneeuwstorm</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="negx.0ofloro">
  <kap><tld/>ofloro</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="edelve.0o">edelvejso</ref>
      (<trd lng="la">Leontopodium alpinum</trd>).
    </dif>
  </snc>
  <trd lng="de">Edelwei&szlig;</trd>
  <trdgrp lng="he">
    <trd>&lamed;&bet;&nun;&bet;&fnun;<klr tip="ind"> (&alef;&dalet;&lamed;&vav;&vav;&yod;&samekh;)</klr></trd>,
    <trd>&alef;&dalet;&lamed;&vav;&vav;&yod;&samekh;</trd>
  </trdgrp>
  <trd lng="hu">gyop&aacute;r</trd>
  <trd lng="nl">sneeuwklokje</trd>
  <trd lng="ru">&c_e;&c_d;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_j;&c_s;</trd>
</drv>

<drv mrk="negx.0tabulo">
  <kap><tld/>tabulo</kap>
  <snc>
    <dif>
      Speco de plata kaj ligna glitilo, pli lar&gcirc;a kaj malpli longa ol
      <ref tip="vid" cel="ski.0o">skio</ref>, per kiu oni glitas sur ne&gcirc;o,
      kaj kiun oni irigas anta&ubreve;enpu&scirc;ante &gcirc;in per unu piedo.
    </dif>
  </snc>
  <ref tip="vid" cel="skut.0ilo">skutilo</ref>
  <trd lng="en">snow board</trd>
  <trd lng="hu">h&oacute;deszka</trd>
  <trd lng="nl">snowboard</trd>
  <trd lng="ru">&c_s;&c_n;&c_o;&c_u;&c_b;&c_o;&c_r;&c_d;</trd>
</drv>
</art>
