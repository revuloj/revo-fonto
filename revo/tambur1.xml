<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tamburin</rad>/o
</kap>
<drv mrk="tambur1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>. Speco
      de malgranda tamburo.
    </dif>
    <trd lng="be">&c_t;&c_a;&c_m;&c_b;&c_u;&c_r;&c_y;&c_n;</trd>
    <trd lng="hu">tamburin</trd>
  <trdgrp lng="pt">
   <trd>pandeiro</trd>
  </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_m;&c_b;&c_u;&c_r;&c_i;&c_n;</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>.
      Malgranda tamburo garnita per tintiloj kaj havanta nur unu ledan
      lamenon, kiun oni frapas per la mano.
    </dif>
    <trd lng="be">&c_b;&c_u;&c_b;&c_je;&c_n;</trd>
    <trd lng="hu">cs&ouml;rg&odblac;dob</trd>
    <trd lng="ru">&c_b;&c_u;&c_b;&c_je;&c_n;</trd>
  </snc>
  <trd lng="de">Tamburin</trd>
  <trd lng="fr">tambourin</trd>
  <trd lng="nl">tamboerijn</trd>
  <trd lng="sv">tamburin</trd>
</drv>
</art>
