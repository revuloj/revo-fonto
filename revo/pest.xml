<kap>
  <ofc>*</ofc>
  <rad>pest</rad>/o
</kap>
<drv mrk="pest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Epidemia <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">malsano</ref> 
      devena de Oriento kaj preska&ubreve; &ccirc;iam mortiga:
      <ekz>
        dek junaj virinoj kaj viroj, fu&gcirc;intaj kamparen 
        pro la <tld/>o, kiu paralizis Florencon en
        la jaro 1348.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="hxoler.0o">&hcirc;olero</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_u;&c_m;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_u;&c_m;&c_a;</trd>
  <trd lng="br">bosenn<klr tip="ind"> (kle&ntilde;ved)</klr></trd>
  <trdgrp lng="cs">
    <trd>mor</trd>,
    <trd>pestis</trd>
  </trdgrp>
  <trd lng="de">Pest</trd>
  <trdgrp lng="en">
    <trd>pest</trd>,
    <trd>pestilence</trd>,
    <trd>plague</trd>
  </trdgrp>
  <trd lng="fr">peste</trd>
  <trd lng="hu">pestis</trd>
  <trd lng="id">pes</trd>
  <trdgrp lng="ja">
    <trd>&#x30DA;&#x30B9;&#x30C8;  <pr>&#x307A;&#x3059;&#x3068;</pr></trd>,
    <trd>&#x75AB;&#x75C5;  <pr>&#x3048;&#x304D;&#x3073;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x60AA;&#x75AB;  <pr>&#x3042;&#x304F;&#x3048;&#x304D;</pr></trd>,
    <trd>&#x6709;&#x5BB3;&#x306A;&#x4EBA;&#x7269;  <pr>&#x3086;&#x3046;&#x304C;&#x3044;&#x306A;&#x3058;&#x3093;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x5BB3;&#x866B;  <pr>&#x304C;&#x3044;&#x3061;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">pest</trd>
  <trdgrp lng="pl">
    <trd>d&zdot;uma</trd>,
    <trd>czarna &sacute;mier&cacute;</trd>,
    <trd>m&oacute;r</trd>,
    <trd>zaraza morowa</trd>
  </trdgrp>
  <trd lng="pt">peste <klr tip="ind">(doen&ccedil;a)</klr></trd>
  <trd lng="ru">&c_ch;&c_u;&c_m;&c_a;</trd>
  <trd lng="sk">mor</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x443;&#x43C;&#x430;</trd>,
    <trd>&#x43C;&#x43E;&#x440;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x75A0; <pr>l&#xEC;</pr></trd>,
    <trd>&#x7658; <pr>l&#xEC;</pr></trd>,
    <trd>&#x817A;&#x9F20;&#x75AB; <pr>xi&#xE0;nsh&#x1D4;y&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
