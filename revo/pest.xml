<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pest.xml,v 1.21 2022/03/25 12:53:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pest</rad>/o
</kap>
<drv mrk="pest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Epidemia <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">malsano</ref> 
      devena de Oriento kaj preska&ubreve; &ccirc;iam mortiga:
      <ekz>
        dek junaj virinoj kaj viroj, fu&gcirc;intaj kamparen 
        pro la <tld/>o, kiu paralizis Florencon en
        la jaro 1348.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="hxoler.0o">&hcirc;olero</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_u;&c_m;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_u;&c_m;&c_a;</trd>
  <trd lng="br">bosenn<klr tip="ind"> (kle&ntilde;ved)</klr></trd>
  <trdgrp lng="cs">
    <trd>mor</trd>,
    <trd>pestis</trd>
  </trdgrp>
  <trd lng="de">Pest</trd>
  <trdgrp lng="en">
    <trd>pest</trd>,
    <trd>pestilence</trd>,
    <trd>plague</trd>
  </trdgrp>
  <trd lng="fr">peste</trd>
  <trd lng="hu">pestis</trd>
  <trd lng="id">pes</trd>
  <trd lng="nl">pest</trd>
  <trdgrp lng="pl">
    <trd>d&zdot;uma</trd>,
    <trd>czarna &sacute;mier&cacute;</trd>,
    <trd>m&oacute;r</trd>,
    <trd>zaraza morowa</trd>
  </trdgrp>
  <trd lng="pt">peste <klr tip="ind">(doen&ccedil;a)</klr></trd>
  <trd lng="ru">&c_ch;&c_u;&c_m;&c_a;</trd>
  <trd lng="sk">mor</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x443;&#x43C;&#x430;</trd>,
    <trd>&#x43C;&#x43E;&#x440;</trd>
  </trdgrp>
</drv>
</art>
