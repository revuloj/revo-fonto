<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: silic.xml,v 1.6 2022/05/05 05:09:56 afido Exp $">
    <kap>
      <rad>silic</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="silic.0o">
      <kap><tld/>o</kap>
      <snc mrk="silic.0o.KEM">
        <uzo tip="fak">KEM</uzo>
        <dif>
          <ref cel="oksid.du0o">Duoksido</ref> de
          <ref cel="silici.0o">silicio</ref>, <frm>SiO<sub>2</sub></frm>:
          <ekz>
            siliko konsistas el <tld/>o<fnt><bib>PIV2</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Siliciumdioxid</trd>,
          <trd>Kiesels&auml;ure</trd>
        </trdgrp>
      </snc>
      <snc mrk="silic.0o.GEOL">
        <uzo tip="fak">GEOL</uzo>
        <dif>
          &Ccirc;ia mineralo a&ubreve;
          <ref cel="rok.0ajxo.GEOL">roka&jcirc;o<sncref/></ref>
          konsistanta plejparte el ia formo de
          <tld/>o<sncref ref="silic.0o.KEM"/>, precipe
          <refgrp tip="sub">
            <ref cel="kvarc.0o">kvarco</ref>,
            <ref cel="sabl.0o">sablo</ref>,
            <ref cel="silik.0o.silikpeco">siliko<sncref/></ref>,
            <ref cel="agat.0o">agato</ref>,
            <ref cel="kalcedon.0o">kalcedono</ref>,
            <ref cel="opal.0o">opalo</ref>
          </refgrp>:
          <ekz>
            grundoj <klr>&#x2026;</klr> malri&ccirc;aj je <tld/>o kaj sterkaj
            elementoj <klr>&#x2026;</klr>, do malta&ubreve;gaj por la agrikulturo
            <fnt><bib>Viki</bib>,
              <lok><url ref="&Viki;/Laterito">Laterito</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_r;&c_e;&c_m;&c_n;&c_ja;&c_z;&c_jo;&c_m;</trd>
      <trd lng="en">silica</trd>
      <trd lng="fr">silice</trd>
      <trd lng="ru">&c_k;&c_r;&c_je;&c_m;&c_n;&c_je;&c_z;&c_jo;&c_m;</trd>
      <trd lng="uk">&#x43A;&#x440;&#x435;&#x43C;&#x43D;&#x435;&#x437;&#x435;&#x43C;</trd>
</drv>
  </art>

