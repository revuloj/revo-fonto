<drv mrk="intele.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="intele.0ulo.KOMUNE">
    <dif>
      <ref tip="vid" cel="kler.0ulo.PED">Klerulo</ref>,
      homo kiu sin okupas kaj interesi&gcirc;as pri <tld/>aj aferoj, kiu
      precipe laboras per <tld/>o:
      <ekz>
        multleginta, multvoja&gcirc;inta <tld/>ulo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        societo de multaj <tld/>uloj &ndash; artistoj kaj literaturistoj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        stari&gcirc;is la pola filozofo <nom>Lutoslavski</nom> tondrante
        kontra&ubreve; &ccirc;iu subteno al monstra lingvo
        <klr>[&#8230;]</klr> la&ubreve; li abomena al &ccirc;iuj
        <tld/>uloj
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 13a, p.
        39a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_l;&c_je;&c_k;&c_t;&c_u;&c_a;&c_l;</trd>
  <trd lng="cs">intelektu&aacute;l</trd>
