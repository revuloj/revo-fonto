<art mrk="$Id: parued.xml,v 1.13 2022/03/25 10:18:30 revo Exp $">
<kap>
  <rad>parued</rad>/oj
</kap>
<drv mrk="parued.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Paridae</trd>):
      <ekz>
        <tld/>oj<klr>[n&#x2026;]</klr> oni dividas en du subfamiliojn: <nom>Remizinae</nom>,
        kies nesto pendas sur la arbobran&ccirc;oj kaj <nom>Parinae</nom>, kies nesto trovi&gcirc;as en
        trunkokavoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16219">Paruedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_ib;&c_c;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>sy&scaron;koroviti&aacute;</trd>,
    <trd>s&yacute;korovit&iacute;</trd>
  </trdgrp>
  <trd lng="de">Meisen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">cinegef&eacute;l&eacute;k</trd>
  <trd lng="nl">mezen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">sikory  <klr>(ptaki)</klr></trd>
  <trd lng="pt">par&iacute;deos</trd>
  <trd lng="ro">pi&#x21B;igoi</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_i;&c_c;&c_y;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_i;&c_n;&c_i;&c_c;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">s&yacute;korkovit&eacute;</trd>
  <trd lng="uk">&#x441;&#x438;&#x43D;&#x438;&#x446;&#x435;&#x432;&#x456;</trd>
</drv>
</art>
