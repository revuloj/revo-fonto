<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: entomo.xml,v 1.28 2023/11/07 20:34:10 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>entomologi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="entomo.0o">
  <kap><tld/>o</kap>
  <snc mrk="entomo.0o.SCI">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri la insektoj:
      <ekz>
        <tld/>o enradiki&gcirc;as en preska&ubreve; &ccirc;iu kulturo &ccirc;iutempaj ekde prahistoriaj tempoj,
        &ccirc;efe je la apero de la agrikulturo (plagoj, bredado de abeloj), sed la
        scienca studo komencis &jcirc;use en la 16-a jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29546">Entomologio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_t;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_n;&c_t;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">entomologia</trd>
  <trd lng="cs">entomologie</trd>
  <trdgrp lng="de">
    <trd>Insektenkunde</trd>,
    <trd>Insektologie<klr> [ark]</klr></trd>,
    <trd>Entomologie<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="en">entomology</trd>
  <trd lng="es">entomolog&iacute;a</trd>
  <trd lng="fr">entomologie</trd>
  <trd lng="he">&alef;&nun;&tet;&vav;&mem;&vav;&lamed;&vav;&gimel;&yod;&he;</trd>
  <trdgrp lng="hu">
    <trd>rovartan</trd>,
    <trd>entomol&oacute;gia</trd>
  </trdgrp>
  <trd lng="ja">&#x6606;&#x866B;&#x5B66;  <pr>&#x3053;&#x3093;&#x3061;&#x3085;&#x3046;&#x304C;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>insectenkunde</trd>,
    <trd>entomologie</trd>
  </trdgrp>
  <trd lng="pl">entomologia</trd>
  <trd lng="pt">entomologia</trd>
  <trd lng="ru">&c_e;&c_n;&c_t;&c_o;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">n&aacute;uka o&nbsp;hmyze</trd>
  <trd lng="uk">&#x435;&#x43D;&#x442;&#x43E;&#x43C;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x6606;&#x866B;&#x5B66; <pr>k&#x16B;nch&#xF3;ngxu&#xE9;</pr></trd>
</drv>

<drv mrk="entomo.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="entomo.0isto.SCI">
    <dif>
      Specialisto pri
      <ref tip="vid" cel="entomo.0o.SCI"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_t;&c_a;&c_m;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">entom&ograve;leg</trd>
  <trd lng="en">entomologist</trd>
  <trd lng="fr">entomologiste</trd>
  <trd lng="pl">entomolog</trd>
</drv>

</art>
