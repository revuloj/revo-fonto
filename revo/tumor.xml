<kap>
  <ofc>9</ofc><rad>tumor</rad>/o
  <fnt>B</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tumor.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="tumor.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Malsaneca &ccirc;elaro kreskanta en la organismo:
      <ekz>
        frua ekkono de malgrandaj <tld/>oj ebligos anka&ubreve; la
        kancerkuracadon sen kirurgia apliko<fnt><aut>Evgeni
        Georgiev</aut>, <vrk>Novaj kuracmetodoj kaj prevento</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 4a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="kancer.0o">kancero</ref>
    <refgrp tip="vid">
      <ref cel="veruk.0o">veruko</ref>,
      <ref cel="sxankr.0o">&scirc;ankro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_h;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_t;&c_u;&c_m;&c_o;&c_r;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xFB2;&#xF53;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>tumor</trd>,
    <trd>eixidura</trd>
  </trdgrp>
  <trd lng="de">Tumor</trd>
  <trd lng="en">tumor</trd>
  <trdgrp lng="fa">
    <trd>&a_gh;&a_d;&a_h;</trd>,
    <trd>&a_t;&a_w;&a_m;&a_w;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tumeur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>daganat</trd>,
    <trd>tumor</trd>
  </trdgrp>
  <trd lng="ja">&#x816B;&#x760D;  [<ind>&#x3057;&#x3085;&#x3088;&#x3046;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>tumor</trd>,
    <trd>gezwel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tumor</trd>,
    <trd>guz<klr tip="amb"> nowotworowy</klr></trd>,
    <trd>nowotw&oacute;r</trd>
  </trdgrp>
  <trd lng="pt">tumor</trd>
  <trd lng="ru">&c_o;&c_p;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x443;&#x445;&#x43B;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x43D;&#x43E;&#x432;&#x43E;&#x442;&#x432;&#x456;&#x440;</trd>
  </trdgrp>
