<art mrk="$Id: tumor.xml,v 1.25 2025/10/31 12:58:49 revo Exp $">
<kap>
  <ofc>9</ofc><rad>tumor</rad>/o
  <fnt><bib>VdE</bib></fnt>
</kap>
<drv mrk="tumor.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="tumor.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Malsaneca <ref tip="super" cel="cxel.0aro.BIO">&ccirc;elaro</ref> kreskanta en la organismo:
      <ekz>
        frua ekkono de malgrandaj <tld/>oj ebligos anka&ubreve; la
        kancerkuracadon sen kirurgia apliko
        <fnt>
          <aut>Evgeni Georgiev</aut>
           <vrk><url ref="&Monato;/2001/007233.html">Novaj kuracmetodoj kaj prevento</url></vrk>
          <bib>Monato</bib>
          <lok>2001/04, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        maligna <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009090.php"
              >Kion signifas la nocio la&ubreve;nature?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        al aidoso-rilataj malsanoj oni rilatigas iujn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php"
              >La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        beda&ubreve;rinde trovi&gcirc;as kelkaj <tld/>oj de maltoleremo, kiuj ka&ubreve;zas misfarton
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="kancer.0o">kancero</ref>
    <refgrp tip="vid">
      <ref cel="veruk.0o">veruko</ref>,
      <ref cel="sxankr.0o">&scirc;ankro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_h;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_t;&c_u;&c_m;&c_o;&c_r;</trd>
  <trd lng="bo">&t_sa;&t_kata;&t_rata;&t_na;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>tumor</trd>,
    <trd>eixidura</trd>
  </trdgrp>
  <trd lng="de">Tumor</trd>
  <trd lng="en">tumor</trd>
  <trdgrp lng="fa">
    <trd>&a_gh;&a_d;&a_h;</trd>,
    <trd>&a_t;&a_w;&a_m;&a_w;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tumeur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>daganat</trd>,
    <trd>tumor</trd>
  </trdgrp>
  <trd lng="ja">&#x816B;&#x760D;  <pr>&#x3057;&#x3085;&#x3088;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>tumor</trd>,
    <trd>gezwel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tumor</trd>,
    <trd>guz<klr tip="amb"> nowotworowy</klr></trd>,
    <trd>nowotw&oacute;r</trd>
  </trdgrp>
  <trd lng="pt">tumor</trd>
  <trd lng="ru">&c_o;&c_p;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_u;&c_h;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_o;&c_t;&c_v;&c_ib;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x80BF;&#x7624; <pr>zh&#x1D2;ngli&uacute;</pr></trd>,
    <trd>&#x816B;&#x7624; <pr>zh&#x1D2;ngli&uacute;</pr></trd>,
    <trd>&#x80BF;&#x5757; <pr>zh&#x1D2;ngku&agrave;i</pr></trd>,
    <trd>&#x816B;&#x584A; <pr>zh&#x1D2;ngku&agrave;i</pr></trd>,
    <trd>&#x7624; <pr>li&uacute;</pr></trd>,
    <trd>&#x7624;&#x5B50; <pr>li&uacute;zi</pr></trd>,
    <trd>&#x7624; <pr>li&uacute;</pr></trd>,
    <trd>&#x7645; <pr>li&uacute;</pr></trd>
  </trdgrp>
</drv>
