  <rad>solid</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="solid.0a">
  <kap><tld/>a</kap>
  <snc mrk="solid.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Mal<ref tip="ant" cel="fluid.0a.FIZ">fluida</ref>;
      kvalifikas substancon, kies molekuloj estas
      fikse kunigitaj unu kun la alia, tio estas &ccirc;iun substancon, kiu
      estas nek <ref cel="flu.0ajxa">flueca</ref>
      nek <ref cel="gas.0a">gasa</ref>:
      <ekz>
        vi fari&gcirc;is bezonantoj de lakto kaj ne de <tld/>a nutra&jcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu rubo <tld/>a estu enmaren &jcirc;etita
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>
    <trd lng="de">fest</trd>
    <trd lng="hu">szil&aacute;rd</trd>
    <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="solid.0a.firma">
    <dif>
      <ref tip="dif" cel="firm.0a.fiksita">Firma<sncref/></ref>,
      malfacile detruebla:
      <ekz>
        <tld/>aj botoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vinkto
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a horlo&gcirc;o, kiu iris bone la&ubreve; la minuto
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_y;&c_v;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_u;&c_zh;&c_y;</trd>
    </trdgrp>
    <trd lng="de">fest</trd>
    <trdgrp lng="hu">
      <trd>er&odblac;s</trd>,
      <trd>tart&oacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="solid.0a.serioza">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="serioz.0a">Serioza</ref>, <ref cel="impon.0a">impona</ref>,
      ne&scirc;erca, mal<ref tip="ant" cel="frivol.0a.iu">frivola</ref>:
      <ekz>
        <ind><tld/>a argumento</ind>;
        <trd lng="hu">komoly &eacute;rv</trd>
      </ekz>
      <ekz>
        li faris al li <tld/>an moralinstruon, klarigis al li <klr>[&#8230;]</klr> kiel forte
        li meritas punon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi faros al vi <tld/>an batadon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li volonte havigus al la bienmastro <klr>[&#8230;]</klr> <tld/>an vipadon
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio <klr>[&#8230;]</klr> estas alporto de <tld/>a utilo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virino <klr>[&#8230;]</klr> <tld/>e purigadis <klr>[la lanternon]</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas <tld/>ajn sciojn kaj spiriton
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas <tld/>a laboro por survojigi ur&gcirc;ajn reformojn
        <fnt>
       <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2000/006835.php">Pekoj de patriarko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="efekti.0a">efektiva</ref>,
       <ref cel="ver.0a">vera</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ernsthaft</trd>,
      <trd>seri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>komoly</trd>,
      <trd>j&oacute;zan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_l;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="solid.0a.fidinda">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="fid.0inda">Fidinda</ref>:
      <ekz>
        seriozaj kaj <tld/>aj homoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        needzigita viro de <tld/>a <klr>(<ref cel="matur.0a">matura</ref>)</klr> a&gcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_n;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vertrauensw&uuml;rdig</trd>,
      <trd>seri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szolid</trd>,
      <trd>megb&iacute;zhat&oacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_l;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_jo;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&#xF58;&#xF41;&#xFB2;&#xF7A;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;ryhodn&yacute;</trd>,
    <trd>masivn&iacute;</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>solidn&iacute;</trd>,
    <trd>spolehliv&yacute;</trd>,
    <trd>tuh&yacute;</trd>,
    <trd>t&ecaron;lesov&yacute;</trd>
  </trdgrp>
  <trd lng="es">s&oacute;lido</trd>
  <trd lng="fr">solide<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x56FA;&#x4F53;&#x306E;  <pr>&#12371;&#x305F;&#12356;&#x306E;</pr></trd>,
    <trd>&#x56FA;&#12356;  <pr>&#x304B;&#x305F;&#12356;</pr></trd>,
    <trd>&#38929;&#x4E08;&#x306A;  <pr>&#x304C;&#12435;&#12376;&#12423;&#12358;&#x306A;</pr></trd>,
    <trd>&#x78BA;&#x56FA;&#12392;&#12375;&#x305F;  <pr>&#x304B;&#12387;&#12371;&#12392;&#12375;&#x305F;</pr></trd>,
    <trd>&#x78BA;&#x5B9F;&#x306A;  <pr>&#x304B;&#x304F;&#12376;&#12388;&#x306A;</pr></trd>,
    <trd>&#22533;&#x5B9F;&#x306A;  <pr>&#12369;&#12435;&#12376;&#12388;&#x306A;</pr></trd>,
    <trd>&#12375;&#12387;&#x304B;&#x308A;&#12375;&#x305F;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vast</trd>,
    <trd>stevig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>solidny</trd>,
    <trd>bry&lstroke;owaty</trd>,
    <trd>trwa&lstroke;y</trd>,
    <trd>zbity</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pevn&yacute;</trd>,
    <trd>sol&iacute;dny</trd>,
    <trd>spo&lcaron;ahliv&yacute;</trd>,
    <trd>tuh&yacute;</trd>
  </trdgrp>
  <trd lng="tok">kiwen</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_j;</trd>,
    <trd>&#x43C;&c_ib;&c_c;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29282;&#x5B9E; <pr>l&#xE1;oshi</pr></trd>,
    <trd>&#29282;&#x5BE6; <pr>l&#xE1;oshi</pr></trd>,
    <trd>&#x539A;&#x5B9E; <pr>h&#xF2;ushi</pr></trd>,
    <trd>&#x539A;&#x5BE6; <pr>h&#xF2;ushi</pr></trd>,
    <trd>&#x56FA; <pr>g&#xF9;</pr></trd>,
    <trd>&#x575A;&#x56FA; <pr>ji&amacron;ng&#xF9;</pr></trd>,
    <trd>&#22533;&#x56FA; <pr>ji&amacron;ng&#xF9;</pr></trd>,
    <trd>&#x5F37;&#20581; <pr>qi&#xE1;ngji&#xE0;n</pr></trd>,
    <trd>&#x5F3A;&#20581; <pr>qi&#xE1;ngji&#xE0;n</pr></trd>,
    <trd>&#x624E;&#x624E;&#x5B9E;&#x5B9E; <pr>zh&amacron;zhash&#xED;sh&#xED;</pr></trd>,
    <trd>&#x624E;&#x624E;&#x5BE6;&#x5BE6; <pr>zh&amacron;zhash&#xED;sh&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="solid.0o">
  <kap><tld/>o</kap>
  <snc mrk="solid.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Korpo en <tld/>a stato:
      <ekz>
        hidrargo estas la nura metalo, kiu
        je ordinara temperaturo ne estas <tld/>o;
      </ekz>
      <ekz>
        benzoata acido <klr>[&#8230;]</klr> estas senkolora kristala <tld/>o
        <fnt>
          <vrk><url ref="&Viki;/Benzoata_acido">Benzoata acido -
          Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Feststoff</trd>,
      <trd>Festk&ouml;rper</trd>
    </trdgrp>
    <trd lng="fr">solide<klr tip="ind"> (corps solide)</klr></trd>
    <trd lng="hu">szil&aacute;rdtest</trd>
    <trd lng="pl">cia&lstroke;o sta&lstroke;e</trd>
    <trdgrp lng="zh">
    <trd>&#x56FA;&#x4F53; <pr>g&#xF9;t&#x1D0;</pr></trd>,
    <trd>&#x56FA;&#x9AD4; <pr>g&#xF9;t&#x1D0;</pr></trd>,
    <trd>&#x56FA;&#24577;&#29289;&#x8D28; <pr>g&#xF9;t&#xE0;iw&#xF9;zh&#xED;</pr></trd>,
    <trd>&#x56FA;&#x614B;&#29289;&#x8CEA; <pr>g&#xF9;t&#xE0;iw&#xF9;zh&#xED;</pr></trd>,
    <trd>&#x56FA;&#x4F53;&#26448;&#26009; <pr>g&#xF9;t&#x1D0;c&#xE1;ili&#xE0;o</pr></trd>,
    <trd>&#x56FA;&#x9AD4;&#26448;&#26009; <pr>g&#xF9;t&#x1D0;c&#xE1;ili&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="solid.0o.kinematiko">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(kinematiko)</uzo>
    <dif>
      Tia objekto en la tridimensia spaco, ke la distanco inter
      du ajnaj &gcirc;iaj punktoj ne &scirc;an&gcirc;i&gcirc;as dum
      movo.
    </dif>
    <trd lng="en">rigid body</trd>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>Festk&ouml;rper</trd>
    </trdgrp>
    <trd lng="fr">solide ind&eacute;formable<klr tip="ind">
    (cin&eacute;matique)</klr></trd>
    <trd lng="hu">merev test</trd>
    <trd lng="pl">cia&lstroke;o sta&lstroke;e</trd>
    <trd lng="es">s&oacute;lido r&iacute;gido</trd>
  </snc>
  <snc mrk="solid.0o.geometrio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Tridimensia
      <ref tip="super" cel="figur.geometria0o">geometria figuro</ref>:
      <ekz>
        la <tld/>oj de <nom>Johnson</nom> povas esti konstruita surbaze de kelkaj piramidoj,
        kupoloj, kaj rotondoj, platonaj <tld/>oj, ar&hcirc;imedaj <tld/>oj, prismoj, kaj
        kontra&ubreve;prismoj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Solido_de_Johnson">Solido de
          Johnson</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      Malnovaj ekvivalentoj:
      <ref cel="korp.0o.solido">korpo</ref>,
      <ref cel="volume.0ajxo">volumena&jcirc;o</ref>;
    </refgrp>
    <refgrp tip="sub">
      Specifaj solidoj:
      <ref cel="angul.solida0o"><tld/>a angulo</ref>,
      <ref cel="edr.du0o">duedro</ref>,
      <ref cel="edr.tri0o">triedro</ref>,
      <ref cel="edr.plur0o">pluredro</ref>,
      <ref cel="sfer.0o.solido">sfero</ref>,
      <ref cel="cilind.0o.solido">cilindro</ref>,
      <ref cel="konus.0o.solido">konuso</ref>,
      <ref cel="elipso.0o">elipsoido</ref>,
      <ref cel="sektor.0o.de_sfero">sektoro</ref>,
      <ref cel="segmen.0o.sfero">segmento</ref>,
      <ref cel="tor.0o.MAT">toro</ref>;
    </refgrp>
    <refgrp tip="vid">
      Rimarkindaj ecoj de solido:
      <ref cel="bar.0ita.metrikaspaco">barita</ref>,
      <ref cel="bar.ne0ita.MAT">nebarita</ref>,
      <ref cel="konvek.0a.subaro">konveksa</ref>,
      <ref cel="konkav.0a.subaro">konkava</ref>,
      <ref cel="rivolu.0a.figuro">rivolua</ref>;
      atributoj de iuj solidoj :
      <ref cel="are.0o.mezuro">areo</ref>,
      <ref cel="rand.0o.figuro">rando</ref>,
      <ref cel="volume.0o.MAT">volumeno</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_c;&c_je;&c_l;&c_a;</trd>,
      <trd>&c_g;&c_je;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>geometrischer <ind>K&ouml;rper</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>geometric <ind>body</ind></trd>,
      <trd>solid</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>solide<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>volume<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>corps g&eacute;om&eacute;trique</trd>
    </trdgrp>
    <trd lng="hu">(m&eacute;rtani) <ind>test</ind></trd>
    <trd lng="pl">bry&lstroke;a</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_o;</trd>,
      <trd>&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
  <trd lng="cs">t&ecaron;leso</trd>
  <trd lng="es">s&oacute;lido</trd>
  <trdgrp lng="ja">
    <trd>&#x56FA;&#x4F53;  <pr>&#12371;&#x305F;&#12356;</pr></trd>,
    <trd/>,
    <trd>&#x7ACB;&#x4F53;  <pr>&#x308A;&#12387;&#x305F;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
  <trd lng="sk">teleso</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&#x43E;&#x43C;&c_je;&c_t;&c_r;&c_i;&c_ch;&#x43D;&c_je; &c_t;&c_ib;&#x43B;&#x43E;</trd>,
    <trd>&c_t;&c_v;&c_je;&c_r;&c_d;&c_je; &c_t;&c_ib;&#x43B;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="solid.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="solid.0eco.kvalito">
    <dif>
      Kvalito de io densa, eltenema, malfacile
      rompebla, fidinda:
      <ekz>
        &ccirc;io en li montris <tld/>econ, la&ubreve; sia ekstera&jcirc;o li &scirc;ajnis konvena homo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        revenas la malnovaj, tradiciaj virtoj, kiaj la tute banalaj pacienco,
        respondeco, bona kvalito, <tld/>eco
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007123.html">Defio a&ubreve; devio de politiko</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 6</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>solidnost</trd>,
    <trd>tuhost</trd>
  </trdgrp>
  <trd lng="de">Solidit&auml;t</trd>
  <trd lng="fr">solidit&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x56FA;&#12373;  <pr>&#x304B;&#x305F;&#12373;</pr></trd>,
    <trd>&#x5F37;&#x56FA;&#12373;  <pr>&#x304D;&#12423;&#12358;&#12371;&#12373;</pr></trd>,
    <trd>&#22533;&#x5B9F;&#12373;  <pr>&#12369;&#12435;&#12376;&#12388;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>solidno&sacute;&cacute;</trd>,
    <trd>stateczno&sacute;&cacute;</trd>,
    <trd>stan sta&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="sk">
