<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: solid.xml,v 1.35 2021/05/29 14:33:16 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>solid</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="solid.0a">
  <kap><tld/>a</kap>
  <snc mrk="solid.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Mal<ref tip="ant" cel="fluid.0a.FIZ">fluida</ref>; 
      kvalifikas substancon, kies molekuloj estas
      fikse kunigitaj unu kun la alia, tio estas &ccirc;iun substancon, kiu
      estas nek <ref cel="flu.0ajxa">flueca</ref> 
      nek <ref cel="gas.0a">gasa</ref>:
      <ekz>
        vi fari&gcirc;is bezonantoj de lakto kaj ne de <tld/>a nutra&jcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu rubo <tld/>a estu enmaren &jcirc;etita
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="https://www.monato.be/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>
    <trd lng="de">fest</trd>
    <trd lng="hu">szil&aacute;rd</trd>
    <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="solid.0a.firma">
    <dif>
      <ref tip="dif" cel="firm.0a.fiksita">Firma<sncref/></ref>,
      malfacile detruebla:
      <ekz>
        <tld/>aj botoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vinkto
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a horlo&gcirc;o, kiu iris bone la&ubreve; la minuto
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_y;&c_v;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_u;&c_zh;&c_y;</trd>
    </trdgrp>
    <trd lng="de">fest</trd>
    <trdgrp lng="hu">
      <trd>er&odblac;s</trd>,
      <trd>tart&oacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="solid.0a.serioza">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="serioz.0a">Serioza</ref>, <ref cel="impon.0a">impona</ref>, 
      ne&scirc;erca, mal<ref tip="ant" cel="frivol.0a.iu">frivola</ref>:
      <ekz>
        <ind><tld/>a argumento</ind>;
        <trd lng="hu">komoly &eacute;rv</trd>
      </ekz>
      <ekz>
        li faris al li <tld/>an moralinstruon, klarigis al li <klr>[&#x2026;]</klr> kiel forte
        li meritas punon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi faros al vi <tld/>an batadon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li volonte havigus al la bienmastro <klr>[&#x2026;]</klr> <tld/>an vipadon
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio <klr>[&#x2026;]</klr> estas alporto de <tld/>a utilo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virino <klr>[&#x2026;]</klr> <tld/>e purigadis <klr>[la lanternon]</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas <tld/>ajn sciojn kaj spiriton
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas <tld/>a laboro por survojigi ur&gcirc;ajn reformojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006835.html">Germanio: Pekoj de patriarko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="efekti.0a">efektiva</ref>,
       <ref cel="ver.0a">vera</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ernsthaft</trd>,
      <trd>seri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>komoly</trd>,
      <trd>j&oacute;zan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_l;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="solid.0a.fidinda">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="fid.0inda">Fidinda</ref>:
      <ekz>
        seriozaj kaj <tld/>aj homoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        needzigita viro de <tld/>a <klr>(<ref cel="matur.0a">matura</ref>)</klr> a&gcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_n;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vertrauensw&uuml;rdig</trd>,
      <trd>seri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szolid</trd>,
      <trd>megb&iacute;zhat&oacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_l;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_jo;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&#xF58;&#xF41;&#xFB2;&#xF7A;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;ryhodn&yacute;</trd>,
    <trd>masivn&iacute;</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>solidn&iacute;</trd>,
    <trd>spolehliv&yacute;</trd>,
    <trd>tuh&yacute;</trd>,
    <trd>t&ecaron;lesov&yacute;</trd>
  </trdgrp>
  <trd lng="es">s&oacute;lido</trd>
  <trd lng="fr">solide<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="nl">
    <trd>vast</trd>,
    <trd>stevig</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pevn&yacute;</trd>,
    <trd>sol&iacute;dny</trd>,
    <trd>spo&lcaron;ahliv&yacute;</trd>,
    <trd>tuh&yacute;</trd>
  </trdgrp>
  <trd lng="tp">kiwen</trd>
</drv>

<drv mrk="solid.0o">
  <kap><tld/>o</kap>
  <snc mrk="solid.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Korpo en <tld/>a stato:
      <ekz>
        hidrargo estas la nura metalo, kiu
        je ordinara temperaturo ne estas <tld/>o;
      </ekz>
      <ekz>
        benzoata acido <klr>[&#x2026;]</klr> estas senkolora kristala <tld/>o
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Benzoata_acido">Benzoata acido - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Feststoff</trd>,
      <trd>Festk&ouml;rper</trd>
    </trdgrp>
    <trd lng="fr">solide<klr tip="ind"> (corps solide)</klr></trd>
    <trd lng="hu">szil&aacute;rdtest</trd>
  </snc>
  <snc mrk="solid.0o.kinematiko">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(kinematiko)</uzo>
    <dif>
      Tia objekto en la tridimensia spaco, ke la distanco inter
      du ajnaj &gcirc;iaj punktoj ne &scirc;an&gcirc;i&gcirc;as dum
      movo.
    </dif>
    <trd lng="en">rigid body</trd>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>Festk&ouml;rper</trd>
    </trdgrp>
    <trd lng="fr">solide ind&eacute;formable<klr tip="ind">
    (cin&eacute;matique)</klr></trd>
    <trd lng="hu">merev test</trd>
    <trd lng="pl">cia&lstroke;o sta&lstroke;e</trd>
    <trd lng="es">s&oacute;lido r&iacute;gido</trd>
  </snc>
  <snc mrk="solid.0o.geometrio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Tridimensia
      <ref tip="super" cel="figur.geometria0o">geometria figuro</ref>:
      <ekz>
        la <tld/>oj de <nom>Johnson</nom> povas esti konstruita surbaze de kelkaj piramidoj,
        kupoloj, kaj rotondoj, platonaj <tld/>oj, ar&hcirc;imedaj <tld/>oj, prismoj, kaj
        kontra&ubreve;prismoj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Solido_de_Johnson">Solido de Johnson</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      Malnovaj ekvivalentoj:
      <ref cel="korp.0o.solido">korpo</ref>,
      <ref cel="volume.0ajxo">volumena&jcirc;o</ref>;
    </refgrp>
    <refgrp tip="sub">
      Specifaj solidoj:
      <ref cel="angul.solida0o"><tld/>a angulo</ref>,
      <ref cel="edr.du0o">duedro</ref>,
      <ref cel="edr.tri0o">triedro</ref>,
      <ref cel="edr.plur0o">pluredro</ref>,
      <ref cel="sfer.0o.solido">sfero</ref>,
      <ref cel="cilind.0o.solido">cilindro</ref>,
      <ref cel="konus.0o.solido">konuso</ref>,
      <ref cel="elipso.0o">elipsoido</ref>,
      <ref cel="sektor.0o.de_sfero">sektoro</ref>,
      <ref cel="segmen.0o.sfero">segmento</ref>,
      <ref cel="tor.0o.MAT">toro</ref>;
    </refgrp>
    <refgrp tip="vid">
      Rimarkindaj ecoj de solido:
      <ref cel="bar.0ita.metrikaspaco">barita</ref>,
      <ref cel="bar.ne0ita.MAT">nebarita</ref>,
      <ref cel="konvek.0a.subaro">konveksa</ref>,
      <ref cel="konkav.0a.subaro">konkava</ref>,
      <ref cel="rivolu.0a.figuro">rivolua</ref>;
      atributoj de iuj solidoj :
      <ref cel="are.0o.mezuro">areo</ref>,
      <ref cel="rand.0o.figuro">rando</ref>,
      <ref cel="volume.0o.MAT">volumeno</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_c;&c_je;&c_l;&c_a;</trd>,
      <trd>&c_g;&c_je;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>geometrischer <ind>K&ouml;rper</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>geometric <ind>body</ind></trd>,
      <trd>solid</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>solide<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>volume<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>corps g&eacute;om&eacute;trique</trd>
    </trdgrp>
    <trd lng="hu">(m&eacute;rtani) <ind>test</ind></trd>
    <trd lng="pl">bry&lstroke;a</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_o;</trd>,
      <trd>&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
  <trd lng="cs">t&ecaron;leso</trd>
  <trd lng="es">s&oacute;lido</trd>
  <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
  <trd lng="sk">teleso</trd>
</drv>

<drv mrk="solid.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="solid.0eco.kvalito">
    <dif>
      Kvalito de io densa, eltenema, malfacile
      rompebla, fidinda:
      <ekz>
        &ccirc;io en li montris <tld/>econ, la&ubreve; sia ekstera&jcirc;o li &scirc;ajnis konvena homo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        revenas la malnovaj, tradiciaj virtoj, kiaj la tute banalaj pacienco,
        respondeco, bona kvalito, <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007123.html">Defio a&ubreve; devio de politiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>solidnost</trd>,
    <trd>tuhost</trd>
  </trdgrp>
  <trd lng="de">Solidit&auml;t</trd>
  <trd lng="fr">solidit&eacute;</trd>
  <trdgrp lng="sk">
    <trd>d&ocirc;kladnos&tcaron;</trd>,
    <trd>slu&scaron;nos&tcaron;</trd>,
    <trd>sol&iacute;dnos&tcaron;</trd>,
    <trd>spo&lcaron;ahlivos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="solid.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="solid.0igi.firmigi">
    <dif>
      Igi <tld/>a, kunteni&gcirc;a, ne facile diserigebla
      a&ubreve; rompebla, firma, fidinda:
      <ekz>
        guar-gumo <klr>[&#x2026;]</klr> estas uzata anka&ubreve; por iom <tld/>igi man&gcirc;a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="https://www.monato.be/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        li provis <tld/>igi sian financan kaj profesian vivon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2010/010141.php">Homo universala, homo moderna: Johann Joachim Becher naski&gcirc;is anta&ubreve; 375 jaroj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        dormo plej gravas por <tld/>igi la memoron kaj la lernoprocesojn
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2012/010683.php">Dormu bone, vivu bone</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> kontribuis al la <tld/>igo <klr>[&#x2026;]</klr> de la kulturo de
        milito
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007175.html">Muroj dumiljara solidas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">konsolidieren</trd>
  <trdgrp lng="fr">
    <trd>consolider</trd>,
    <trd>solidifier</trd>
  </trdgrp>
</drv>

<drv mrk="solid.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="solid.0igxi.firmigxi">
    <dif>
      I&gcirc;i <tld/>a, rezista, malmola,
      nerompi&gcirc;ema, nefleksebla, serioza:
      <ekz>
        liaj filoj <klr>[&#x2026;]</klr> estas iom malbonkondutaj <klr>[sed]</klr> kun la tempo
        ili jam <tld/>i&gcirc;os
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fakuloj atendas nun la duan fazon de banka <tld/>i&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2003/008044.html">Internaciaj bankoj stabiligas siajn poziciojn</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>festigen</ind></trd>,
    <trd>zur <ind>Vernunft</ind> kommen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>consolider</ind></trd>,
    <trd>se <ind>solidifier</ind></trd>
  </trdgrp>
</drv>

<drv mrk="solid.platona0o">
  <kap>platona <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib><lok>regula</lok></fnt>
  <snc mrk="solid.platona0o.MAT">
    <dif>
      <ref cel="regul.0a.pluredro">Regula</ref>
      <ref tip="super" cel="edr.plur0o">pluredro</ref>:
      <ekz>
        platona <tld/>o komponita el 12 regulaj kvinlateraj edroj
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Dekduedro">Dekduedro - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="platon.0o">Platono</ref>
    <bld lok="../bld/pluredr.png">
      la kvin Platonaj solidoj
    </bld>
    <trd lng="be">&c_P;&c_l;&c_ja;&c_t;&c_o;&c_n;&c_a; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
    <trd lng="de">Platonischer <ind>K&ouml;rper</ind></trd>
    <trd lng="en">Platonic <ind>body</ind></trd>
    <trd lng="fr">solide platonicien</trd>
    <trdgrp lng="hu">
      <trd>szab&aacute;lyos <ind>test</ind></trd>,
      <trd>platoni <ind>test</ind></trd>
