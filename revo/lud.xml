<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lud</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lud.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="lud.0i.SPO">
    <gra><vspec>x</vspec></gra>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Sin amuzi, sin distri, agante, ne por ia utilo, sed por nura
      plezuro
      de agado:
      <ekz>
        la infanoj <tld/>as sur la herbejo;
      </ekz>
      <ekz>
        <tld/>i per pupo, pilko, kartoj;
      </ekz>
      <ekz>
        <tld/>i pilkon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i kartojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i keglojn per kranioj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i kun iu <tld/>on de pugnoj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i militon<fnt>Z</fnt>
        <klr>(imitante militon)</klr>;
      </ekz>
      <ekz>
        la &jcirc;onglisto <tld/>is per ponardoj;
      </ekz>
      <ekz>
        fari sian taskon kvaza&ubreve; <tld/>ante
        <klr>(tute facile kaj senpene)</klr>;
      </ekz>
      <ekz>
        <tld/>i per vortoj, fari vort<tld/>ojn
        <klr>(kombini similsonajn sed malsamsignifajn vortojn por
        &scirc;erco
        kaj amuzo)</klr>;
      </ekz>
      <ekz>
        <tld/>i per okuloj
        <klr>(= okulumi)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la suno <tld/>is <klr>(treme brilis)</klr> tra la bran&ccirc;aro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vento <tld/>is <klr>(agrable blovetis)</klr> tra la foliaro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vento <tld/>is kun &scirc;iaj haroj
        <klr>(agrable flirtigis &scirc;iajn harojn)</klr>.
      </ekz>
    </dif>
    <trd lng="bg">&c_i;&c_g;&c_r;&c_a;&c_ja;</trd>
    <trd lng="es">jugar</trd>
    <trdgrp lng="nl">
      <trd>zich <ind>vermaken</ind></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bawi&cacute; si&eogonek;</trd>,
      <trd>gra&cacute;</trd>
    </trdgrp>
    <trd lng="pt">brincar</trd>
    <trd lng="sk">hra&tcaron; sa</trd>
  </snc>
  <snc mrk="lud.0i.veti">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Vet<tld/>i, riski la &scirc;ancojn pri gajno a&ubreve; perdo:
      <ekz>
        <tld/>i monon;
      </ekz>
      <ekz>
        <ind>mon<tld/>i</ind>;
        <trd lng="hu">p&eacute;nzben <ind>j&aacute;tszik</ind></trd>
      </ekz>
      <ekz>
        <tld/>i en la borso, en vetkuroj.
      </ekz>
    </dif>
    <trd lng="bg">&c_i;&c_g;&c_r;&c_a;&c_ja;</trd>
    <trdgrp lng="es">
      <trd>jugar</trd>,
      <trd>apostar</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>speculeren</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gra&cacute;</trd>,
      <trd>obstawia&cacute;<klr> (np. zak&lstroke;ady)</klr></trd>,
      <trd>spekulowa&cacute;<klr> (np. na gie&lstroke;dzie)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>apostar</trd>,
      <trd>especular</trd>
    </trdgrp>
  </snc>
  <snc mrk="lud.0i.BELA">
    <dif>
      <ref tip="vid" cel="prezen1.0i">Prezenti</ref> spekta&jcirc;on, vivan
      arta&jcirc;on.
    </dif>
    <subsnc mrk="lud.0i.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <gra><vspec>x</vspec></gra>
      <dif>
        <ref tip="super" cel="muzik.0i">Muziki</ref> per instrumento por
        sin
        distri a&ubreve; distri aliajn:
        <ekz>
          Petro <klr>...</klr> penis ludi sur la gitaro <klr>...</klr>
          valson
          <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 22a, p.
          143a</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>i violonon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          gloru la Eternulon <klr>...</klr>, per dekkorda psaltero
          <tld/>u
          al Li
          <fnt><bib>MT</bib>, <lok>&Psa; 33:2</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="bg">&c_s;&c_v;&c_i;&c_r;&c_i;</trd>
      <trd lng="br">seni&ntilde;</trd>
      <trd lng="es">tocar <klr>(un instrumento)</klr></trd>
      <trdgrp lng="nl">
        <trd>bespelen</trd>
      </trdgrp>
      <trd lng="pl">gra&cacute; <klr> (na instrumencie muzycznym)</klr></trd>
      <trd lng="pt">tocar <klr>(instrumento musical)</klr></trd>
      <trd lng="tp">kalama</trd>
    </subsnc>
    <subsnc mrk="lud.0i.TEA">
      <uzo tip="fak">TEA</uzo>
      <gra><vspec>tr</vspec></gra>
      <dif>
        Distri la aliajn, prezentante teatra&jcirc;on,
        <ref tip="vid" cel="aktor.0i">aktori</ref>,
        <ref tip="vid" cel="rol.0i">roli</ref>:
        <ekz>
          <tld/>i komedion, rolon;
        </ekz>
        <ekz>
          &leftquot;vi do <tld/>u ion novan,&rightquot; &scirc;i diris al siaj
          aktorinoj
          <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 54a, volumo 2a, p.
          211a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="de">auff&uuml;hren</trd>
      <trd lng="es">actuar</trd>
      <trdgrp lng="nl">
        <trd>opvoeren</trd>
      </trdgrp>
      <trd lng="pl">gra&cacute;</trd>
      <trd lng="pt">atuar</trd>
    </subsnc>
  </snc>
  <snc mrk="lud.0i.sxajnigi">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <ref tip="vid" cel="sxajn.0igi">&Scirc;ajnigi</ref>,
      <ref tip="vid" cel="hipokr.0i">hipokriti</ref>:
      <ekz>
        <tld/>i funebron, virton;
      </ekz>
      <ekz>
        <tld/>i piulon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>vorspielen</trd>,
      <trd>vormachen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>zich <ind>voordoen</ind> als</trd>
    </trdgrp>
    <trd lng="pl">gra&cacute;</trd>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="br">c'hoari<klr tip="ind"> (v.)</klr></trd>
  <trd lng="cs">hr&aacute;t (si)</trd>
  <trd lng="de">spielen</trd>
  <trd lng="en">play</trd>
  <trd lng="fr">jouer</trd>
  <trd lng="hu">j&aacute;tszik</trd>
  <trdgrp lng="nl">
    <trd>spelen</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hra&tcaron;</trd>
</drv>

<drv mrk="lud.0o">
  <kap><tld/>o</kap>
  <snc mrk="lud.0o.ago">
    <tezrad fak="SPO"/>
    <dif>
      Ago <tld/>i:
      <ekz>
        dan&gcirc;era <tld/>o;
      </ekz>
      <ekz>
        la&ubreve; filozofia vidpunkto la belartoj estas speco de <tld/>o;
      </ekz>
      <ekz>
        mon<tld/>o;
      </ekz>
      <ekz>
        sportaj <tld/>oj;
      </ekz>
      <ekz>
        meti (riski) en la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        floraj <tld/>oj
        <klr>(literaturaj konkursoj)</klr>;
      </ekz>
      <ekz>
        <tld/>o aparte kaj aferoj aparte<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o (&scirc;an&gcirc;aspektoj) de la lumo tra la foliaro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ser&ccirc;i belecon en la <tld/>ado de linioj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la imago.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lud.0o.maniero">
    <dif>
      Maniero la&ubreve; kiu oni <tld/>as:
      <ekz>
        bela <tld/>o de la orgeno, de la orkestro;
      </ekz>
      <ekz>
        malbona <tld/>o de aktoro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rol.0o">rolo</ref>
    </refgrp>
  </snc>
  <snc mrk="lud.0o.sxancoj">
    <dif>
      Tuto da &scirc;ancoj a&ubreve; atutoj, pri kiuj oni disponas:
      <ekz>
        <ind>fari mienon fieran al <tld/>o mizera</ind><fnt>Z</fnt>.
        <trd lng="fr">faire contre mauvaise fortune bon c&oelig;ur</trd>
        <trd lng="hu">j&oacute; k&eacute;pet v&aacute;g a rossz
        j&aacute;t&eacute;khoz</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="br">c'hoari<klr tip="ind"> (ak.)</klr></trd>
  <trd lng="de">Spiel</trd>
  <trd lng="en">game</trd>
  <trd lng="es">juego</trd>
  <trd lng="fr">jeu</trd>
  <trd lng="hu">j&aacute;t&eacute;k</trd>
  <trd lng="nl">spel</trd>
  <trd lng="pl">gra</trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;</trd>
  <trd lng="sk">hra</trd>
</drv>

<drv mrk="lud.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="lud.0ilo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Objekto, per kiu infanoj <tld/>as:
      <ekz>
        la &ccirc;ambro de la infanoj estis plena de <tld/>iloj
        (t.e. pupo, infana kale&scirc;eto, balanc&ccirc;evalo,
        rajdvipo k.a.)<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        al la virino ili diras: estu <tld/>ilo, servu kiel amuzilo
        al la viroj!<fnt>Z</fnt>
      </ekz>
    </dif>
    <rim>
      La vorto apliki&gcirc;as metafore anka&ubreve; al objektoj destinitaj
      por distri plenkreskulojn.
    </rim>
  </snc>
  <trd lng="be">&c_c;&c_a;&c_c;&c_k;&c_a;</trd>
  <trd lng="br">c'hoariell</trd>
  <trd lng="de">Spielzeug</trd>
  <trd lng="en">toy</trd>
  <trd lng="fr">jouet</trd>
  <trd lng="hu">j&aacute;t&eacute;kszer</trd>
  <trd lng="nl">stuk <ind>speelgoed</ind></trd>
  <trd lng="pl">zabawka</trd>
  <trd lng="pt">brinquedo</trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">hra&ccaron;ka</trd>
</drv>

<drv mrk="lud.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <tld lit="L"/>i anta&ubreve; publiko:
      <ekz>
        la tridek-kvaran fojon estis anta&ubreve;<tld/>ita al ili
        la sama peco, sed ili &ccirc;iam ankora&ubreve; ne konis
        &gcirc;in perfekte
        <fnt><bib>Fab2</bib>, <lok>La najtingalo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">c'hoari<klr tip="ind"> (dirak arvesterien)</klr></trd>
  <trd lng="fr">jouer<klr tip="ind"> (devant un public)</klr></trd>
  <trd lng="de">vorspielen</trd>
  <trd lng="hu">elj&aacute;tszik<klr tip="ind"> (k&ouml;z&ouml;ns&eacute;g
  el&odblac;tt)</klr></trd>
  <trd lng="pl">odgrywa&cacute;</trd>
</drv>

<drv mrk="lud.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Io, kio anta&ubreve;as <tld/>on:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vo&ccirc;oj de dioj, nevideblaj manoj, homo martirata per
        bolanta pe&ccirc;o, tio estis nur anta&ubreve;<tld/>oj!
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 7</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="prelud.0o">preludo</ref>
  </snc>
  <trd lng="br">rakc'hoari</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;lude</trd>,
    <trd>entr&eacute;e en mati&egrave;re</trd>,
    <trd>hors d'&oelig;uvre<klr tip="ind"> (entr&eacute;e en
    mati&egrave;re)</klr></trd>
  </trdgrp>
  <trd lng="de">Vorspiel</trd>
  <trd lng="hu">el&odblac;j&aacute;t&eacute;k</trd>
</drv>

<drv mrk="lud.baskul0o">
  <kap>baskul<tld/>o</kap>
  <snc mrk="lud.baskul0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Infana <tld/>o per <ref cel="baskul.0o.TEK">baskulo</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>seesaw</trd>,
      <trd>teeter-totter</trd>
    </trdgrp>
    <trd lng="hu">libik&oacute;ka</trd>
  </snc>
  <snc mrk="lud.baskul0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika manovro, kiu konsistas en la alterna favorado de jen unu,
      jen alia el la du inter si malamikaj partioj, landoj ktp.
    </dif>
    <trdgrp lng="en">
      <trd>waffling</trd>
    </trdgrp>
    <trd lng="hu">hintapolitika</trd>
  </snc>
  <trd lng="fr">jeu de <ind>bascule</ind></trd>
</drv>

<drv mrk="lud.flug0ilo">
  <kap>flug<tld/>ilo</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <ref tip="dif" cel="kajt.0o">kajto</ref>
  </snc>
  <trd lng="br">sarpant-nij</trd>
  <trd lng="de">Drachen <klr tip="ind">(Spielzeug)</klr></trd>
  <trd lng="en">kite</trd>
  <trd lng="fr">cerf-volant<klr tip="ind"> (jouet,
  instrument)</klr></trd>
  <trd lng="hu">s&aacute;rk&aacute;ny<klr tip="ind">
  (j&aacute;t&eacute;k)</klr></trd>
  <trd lng="pl">latawiec</trd>
  <trd lng="sk">&scaron;arkan <klr>(papierov&yacute;)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;
  &c_z;&c_m;&c_je;&c_j;</trd>
</drv>

<drv mrk="lud.hazard0o">
  <kap>hazard<tld/>o, <var><kap>hazarda <tld/>o</kap></var></kap>
  <snc mrk="lud.hazard0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.0o.ago"><tld lit="L"/>o</ref>, en kiu 
      gajno a&ubreve; malgajno dependas de neanta&ubreve;videblaj, hazardaj
      okaza&jcirc;oj:
      <ekz>
        enspezo el <tld/>ludo konsistigas 30-50% de la financa enspezo
        de Makao
        <fnt>
          <aut>Zhang Meizhi</aut>,
          <vrk><url ref="">Vizito al Makao (4)</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1999-11</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lud.vet0o.SPO">vetludo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">jeu de hasard</trd>
</drv>

<drv mrk="lud.kart0i">
  <kap>kart<tld/>i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="lud.0i.SPO">Ludi</ref> per ludkartoj:
      <ekz>
        Anna Sergeevna majstre kart<tld/>is
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 16a, p.
        89a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>c'hoari karto&ugrave;</trd>,
    <trd>c'hoari ar c'harto&ugrave;</trd>
  </trdgrp>
  <trd lng="de">Karten <ind>spielen</ind></trd>
  <trd lng="fr">jouer aux cartes</trd>
  <trd lng="hu">k&aacute;rty&aacute;zik</trd>
  <trd lng="pl">gra&cacute; w karty</trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_v;
  &c_k;&c_a;&c_r;&c_t;&c_y;</trd>
</drv>

<drv mrk="lud.prov0i">
  <kap>prov<tld/>i</kap>
  <uzo tip="fak">TEA</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <tld lit="L"/>i teatra&jcirc;on a&ubreve; muzikpecon sen publiko,
      por sperti&gcirc;i kaj plibonigi la <tld/>adon:
      <ekz>
        <klr>[ni]</klr> estis vokitaj hiera&ubreve; posttagmeze,
        post la <ind>prov<tld/>o</ind>, en la oficejon de la direktoro,
        sinjoro Hadda
        <fnt><bib>UrdHadda</bib>, <lok>p. 6</lok></fnt>;
        <trd lng="br">pleustradenn<klr tip="ind"> (c'hoariva,
        sonerezh)</klr></trd>
        <trd lng="fr">r&eacute;p&eacute;tition<klr tip="ind"> (d'une
        repr&eacute;sentation)</klr></trd>
      </ekz>
      <ekz>
        li <klr>...</klr> kune kun mi iris al la pupteatra trupo
        por rigardi ilian lernadon kaj prov<tld/>on
        <fnt>
        <vrk>El Popola &Ccirc;inio</vrk>
        <lok> <url
        ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/2000-05/bd2000-5-15.htm">
        http://www.china.org..cn/EL/EL/ElPopolaCxinio/2000-05/bd2000-5-15.htm</url>
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">pleustri&ntilde;<klr tip="ind"> (c'hoariva,
  sonerezh)</klr></trd>
  <trd lng="de">proben</trd>
  <trd lng="fr">r&eacute;p&eacute;ter<klr tip="ind"> (une
  repr&eacute;sentation)</klr></trd>
  <trd lng="hu">pr&oacute;b&aacute;l<klr tip="ind">
  (el&odblac;ad&aacute;st)</klr></trd>
  <trd lng="pl">gra&cacute;<klr> pr&oacute;b&eogonek;</klr></trd>
  <trd
  lng="ru">&c_r;&c_je;&c_p;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
  </trd>
</drv>

<drv mrk="lud.vet0o">
  <kap>vet<tld/>o</kap>
  <snc mrk="lud.vet0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.0o.ago"><tld lit="L"/>o</ref> pri kies 
      rezulto oni vetas, precipe vetas monon:
      <ekz>
        estas notitaj la rezultoj de preska&ubreve; &ccirc;iuj piedpilkaj
