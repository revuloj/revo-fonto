<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bodisatv</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bodisatv.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BUD</uzo>
    <dif>
      Kvaza&ubreve;-budao, rezignanta pri propra savo por
      unue helpi al aliaj vivantoj savi&gcirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_d;&c_h;&c_ib;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>
  <trd lng="fr">bodhisattva</trd>
  <trd lng="hu">bodhiszattva</trd>
  <trd lng="sk">bodhisatva</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_d;&c_h;&c_i;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_d;&c_i;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>

