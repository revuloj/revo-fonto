<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bodisatv</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="bodisatv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BUD</uzo>
  <snc>
    <dif>
      Rezignanto pri atingebla propra savo
      votanta, ke &scirc;i a&ubreve; li unue helpos
      al aliaj vivantoj savi&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bodhisattva</trd>
  </trdgrp>
  <trd lng="hu">bodhiszattva</trd>
  <trd lng="sk">bodhisatva</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_d;&c_h;&c_i;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_d;&c_i;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>

