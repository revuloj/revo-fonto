  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Malalta, senprivilegia klaso el popolo:
      <ekz>
        en antikva Romo la <tld/>anoj havis neniujn civitanajn rajtojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_b;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>plebs</trd>,
    <trd>&rcaron;&iacute;msk&aacute; chudina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>armes <ind>Volk</ind></trd>,
    <trd>Plebs</trd>,
    <trd>P&ouml;bel</trd>,
    <trd>Plebejer<klr tip="amb"> (einfaches Volk)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>plebs</trd>,
    <trd>common people</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pl&egrave;be</trd>
  </trdgrp>
  <trd lng="hu">plebs</trd>
  <trdgrp lng="ja">
    <trd>&#x5E73;&#x6C11;&#x968E;&#x7D1A;  <pr>&#x3078;&#x3044;&#x307F;&#x3093;&#x304B;&#x3044;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x5EB6;&#x6C11;  <pr>&#x3057;&#x3087;&#x307F;&#x3093;</pr></trd>,
    <trd>&#x4E0B;&#x5C64;&#x6C11;  <pr>&#x304B;&#x305D;&#x3046;&#x305F;&#x307F;</pr></trd>
  </trdgrp>
  <trd lng="nl">plebs</trd>
  <trd lng="pl">plebs</trd>
  <trd lng="pt">plebe</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_n;&c_a;&c_r;&c_o;&c_d;&c_mol;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_b;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">ple&ccaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_je;&c_b;&c_s;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_t;&#x43E;&#x43B;&#x44E;&c_d;</trd>,
    <trd>&c_ch;&c_je;&c_r;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8CA9;&#x592B;&#x8D70;&#x5352; <pr>f&#xE0;nf&#x16B;z&#x1D2;uz&#xFA;</pr></trd>,
    <trd>&#x8D29;&#x592B;&#x8D70;&#x5352; <pr>f&#xE0;nf&#x16B;z&#x1D2;uz&#xFA;</pr></trd>,
    <trd>&#x9ECE;&#x6C11; <pr>l&#xED;m&#xED;n</pr></trd>,
    <trd>&#x4E0B;&#x5C42;&#x6C11;&#x4F17; <pr>xi&#xE0;c&#xE9;ngm&#xED;nzh&#xF2;ng</pr></trd>,
    <trd>&#x4E0B;&#x5C64;&#x6C11;&#x773E; <pr>xi&#xE0;c&#xE9;ngm&#xED;nzh&#xF2;ng</pr></trd>,
    <trd>&#x66B4;&#x6C11; <pr>b&#xE0;om&#xED;n</pr></trd>,
    <trd>&#x7FA4;&#x4F17; <pr>q&#xFA;nzh&#xF2;ng</pr></trd>,
    <trd>&#x7FA4;&#x773E; <pr>q&#xFA;nzh&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pleb.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Ordinarulo havanta nul privilegion:
      <ekz>
        mi ja estas <tld/>ano, <klr>...</klr> mi ne havas blazonon
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p.
        126a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_b;&c_e;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>plebejec</trd>,
    <trd>p&rcaron;&iacute;slu&scaron;n&iacute;k &rcaron;&iacute;m. chudiny</trd>
  </trdgrp>
  <trd lng="de">Plebejer<klr tip="amb"> (Person)</klr></trd>
  <trdgrp lng="en">
    <trd>plebian</trd>,
    <trd>commoner</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pl&eacute;b&eacute;ien<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x5E73;&#x6C11;  <pr>&#x3078;&#x3044;&#x307F;&#x3093;</pr></trd>
  <trd lng="nl">plebejer</trd>
  <trd lng="pl">plebejczyk</trd>
  <trd lng="pt">plebeu</trd>
  <trd lng="sk">plebejec</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_je;&c_b;&c_je;&c_j;</trd>,
    <trd>&c_shch;&c_je; &c_s;&#x43C;&c_je;&c_r;&c_d;</trd>
  </trdgrp>
</drv>
