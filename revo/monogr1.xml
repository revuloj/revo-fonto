    <trd>monogramme</trd>
  </trdgrp>
  <trd lng="hu">monogramm</trd>
  <trdgrp lng="ja">
    <trd>&#12514;&#12494;&#12464;&#12521;&#12512;</trd>,
    <trd>&#32068;&#21512;&#12379;&#25991;&#23383;  <pr>&#12367;&#12415;&#12354;&#12431;&#12379;&#12418;&#12376;</pr></trd>
  </trdgrp>
  <trd lng="nl">monogram</trd>
