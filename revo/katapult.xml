<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>katapult</rad>/o
</kap>
<drv mrk="katapult.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Malnova militma&scirc;ino, funkcianta per stre&ccirc;ado kaj subita malstre&ccirc;o
      de &scirc;nuroj, kaj uzata por pafi &scirc;tonojn, &jcirc;etarmilojn ktp.
    </dif>
    <ref tip="sin" cel="balist.0o">balisto</ref>
    <trdgrp lng="de">
      <trd>Katapult</trd>,
      <trd>Wurfmaschine</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Knaba ludilo, konsistanta el ligna a&ubreve; fera forketo kun ka&ubreve;&ccirc;uka
      rubando, por pafi &scirc;tonetojn ktp.
    </dif>
    <trdgrp lng="de">
      <trd>Schleuder</trd>,
      <trd>Zwille</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_o;&c_g;&c_a;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc num="3">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Mekanika aparato, por anta&ubreve;en&jcirc;eti aviadilon for de la ferdeko de
      &scirc;ipo, a&ubreve; velglisilon for de la grundo.
    </dif>
    <trdgrp lng="de">
      <trd>Flugzeugkatapult</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
  </snc>
  <snc num="4">
    <uzo tip="fak">AVI</uzo>
    <dif>
      A&ubreve;tomata sava aparato por el&jcirc;eti piloton el flugma&scirc;ino.
    </dif>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
  </snc>
  <trdgrp lng="de">
      <trd>Schleudersitz</trd>
   </trdgrp>
  <trd lng="en">catapult</trd>
  <trd lng="nl">katapult</trd>
</drv>
<drv mrk="katapult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anta&ubreve;enpeli per <tld/>o.
    </dif>
  </snc>
  <trd lng="nl">katapulteren</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>

