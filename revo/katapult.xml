<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katapult.xml,v 1.27 2023/12/03 08:00:25 revo Exp $">
<kap>
  <ofc>9</ofc><rad>katapult</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="katapult.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="katapult.0o.TEK">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Me&hcirc;anika &jcirc;etilo:
      <ekz>
        iu kiprano, prave a&ubreve; malprave ekkonscii&gcirc;inte, ke li bezonas pli
        spacohavan
        lo&gcirc;ejon, iris iun belan tagon al vendejo de anta&ubreve;pretigitaj
        konstrua&jcirc;oj,
        akiris unu el ili kaj &jcirc;etegis &gcirc;in sur la ebenan tegmenton de sia
        domo,
        bonvolu ne demandi min kiel: mi supozas, ke per helikoptero a&ubreve;
        levitacio &ndash;
        malpli probable per <tld/>o, kiun oni ne ofte vidas sur la
        insulo
        <fnt>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010183.php">Terasa
          apartamento</url></vrk>
          <bib>Monato</bib>
          <lok>2011/01, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="katapult.0o.ARM">
      <uzo tip="fak">MIL</uzo>
      <uzo tip="fak">HIS</uzo>
      <dif>
        Malnova militma&scirc;ino, funkcianta per diversaj
        sistemoj, uzata por pafi &scirc;tonojn, &jcirc;etarmilojn ktp:
        <ekz>
          jen ili denove kun la <tld/>oj
          <fnt>
            <aut>Alberto Garc&iacute;a Fumero</aut>
            <vrk><url ref="&Monato;/2003/007991.html">Espero</url></vrk>
            <bib>Monato</bib>
            <lok>2003/03, p. 22</lok>
          </fnt>;
        </ekz>
        <ekz>
          oni e&ccirc; konstruis imita&jcirc;ojn de <nac>tormentum</nac>
          (sie&gcirc;<tld/>o)
          <fnt>
            <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2004/008467.html">Kampoj de Soria Antonio Machado, universala
                poeto</url></vrk>
                <bib>Monato</bib>
                <lok>2004/12, p. 23</lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="sub" cel="balist.0o">balisto</ref>
      <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Katapult</trd>,
        <trd>Wurfmaschine</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>haj&iacute;t&oacute;g&eacute;p</trd>,
        <trd>katapulta</trd>
      </trdgrp>
      <trd lng="it">catapulta<klr> (macchina bellica)</klr></trd>
      <trd lng="nl">katapult</trd>
      <trd lng="pl">katapulta</trd>
      <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
    </subsnc>
    <subsnc mrk="katapult.0o.SPO">
      <dif>
        Knaba <ref tip="lst" cel="lud.0ilo" lst="voko:ludiloj">ludilo</ref>, konsistanta el ligna a&ubreve;
        fera
        forketo kun elasta rubando, por pafi &scirc;tonetojn ktp:
      </dif>
      <trd lng="be">&c_r;&c_a;&c_g;&c_a;&c_t;&c_k;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Schleuder</trd>,
        <trd>Zwille</trd>
      </trdgrp>
      <trd lng="fr">fronde<klr> (catapulte)</klr></trd>
      <trd lng="hu">cs&uacute;zli</trd>
      <trd lng="it">fionda</trd>
      <trd lng="nl">katapult</trd>
      <trd lng="pl">proca</trd>
      <trd lng="ru">&c_r;&c_o;&c_g;&c_a;&c_t;&c_k;&c_a;</trd>
    </subsnc>
    <subsnc mrk="katapult.0o.AVI_startilo">
      <uzo tip="fak">AVI</uzo>
      <dif>
        Me&hcirc;anika aparato, por anta&ubreve;en&jcirc;eti aviadilon for de
        la ferdeko de &scirc;ipo, a&ubreve; velglisilon for de la grundo.
      </dif>
      <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Flugzeugkatapult</trd>
      </trdgrp>
      <trd lng="hu">katapult</trd>
      <trd lng="it">catapulta<klr> (per aerei)</klr></trd>
      <trd lng="nl">lanceerinrichting</trd>
      <trdgrp lng="pl">
        <trd>katapulta</trd>,
        <trd>wyrzutnia</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
    </subsnc>
    <subsnc mrk="katapult.0o.AVI_savsistemo">
      <uzo tip="fak">AVI</uzo>
      <dif>
        A&ubreve;tomata sava aparato por el&jcirc;eti piloton el flugma&scirc;ino.
      </dif>
      <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Schleudersitz</trd>
      </trdgrp>
      <trd lng="fr">si&egrave;ge &eacute;jectable</trd>
      <trd lng="it"><ind>sedile</ind> eiettabile</trd>
      <trd lng="nl">schietstoel</trd>
      <trd lng="pl">katapulta</trd>
      <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
    </subsnc>
  </snc>
  <trd lng="ca">catapulta</trd>
  <trd lng="en">catapult</trd>
  <trd lng="es">catapulta</trd>
  <trd lng="fr">catapulte</trd>
  <trd lng="hu">katapult</trd>
  <trdgrp lng="ja">
    <trd>&#x5F29;  <pr>&#x3069;</pr></trd>,
    <trd>&#x6295;&#x77F3;&#x6A5F;  <pr>&#x3068;&#x3046;&#x305B;&#x304D;&#x304D;</pr></trd>,
    <trd>&#x3071;&#x3061;&#x3093;&#x3053;</trd>,
    <trd>&#x30AB;&#x30BF;&#x30D1;&#x30EB;&#x30C8;</trd>
  </trdgrp>
  <trd lng="pl">katapulta</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_t;&c_a;&#x43F;&c_u;&#x43B;&#x44C;&c_t;&c_a;</trd>,
    <trd>&#x43C;&c_je;&c_t;&c_a;&#x43B;&#x44C;&#x43D;&c_a; &#x43C;&c_a;&c_sh;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_r;&#x43E;&c_g;&c_a;&c_t;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F29;&#x70AE; <pr>n&#x1D4;p&#xE0;o</pr></trd>,
    <trd>&#x5F39;&#x5F13; <pr>d&#xE0;ng&#x14D;ng</pr></trd>,
    <trd>&#x77F3;&#x5F29; <pr>sh&#xED;n&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="katapult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anta&ubreve;enpeli per <tld/>o:
      <ekz>
        se la vidvo bojas, perfide pri bankroto &gcirc;ojas <klr>[&#x2026;]</klr>,
        <tld/>as
        <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">catapultar</trd>
  <trd lng="cs">katapultovat</trd>
  <trdgrp lng="de">
      <trd>katapultieren</trd>,
      <trd>abschie&szlig;en</trd>,
      <trd>wegschleudern</trd>
  </trdgrp>
  <trd lng="es">catapultar</trd>
  <trd lng="fr">catapulter</trd>
  <trd lng="hu">katapult&aacute;l</trd>
  <trd lng="it">catapultare</trd>
  <trd lng="ja">&#x30AB;&#x30BF;&#x30D1;&#x30EB;&#x30C8;&#x3067;&#x767A;&#x5C04;&#x3059;&#x308B;  <pr>&#x30AB;&#x30BF;&#x30D1;&#x30EB;&#x30C8;&#x3067;&#x306F;&#x3063;&#x3057;&#x3083;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">wegslingeren</trd>
  <trdgrp lng="pl">
    <trd>katapultowa&cacute;</trd>,
    <trd>strzela&cacute; z <ind>katapulty</ind></trd>,
    <trd>strzela&cacute; z <ind>procy</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">katapultova&tcaron;</trd>
  <trd lng="uk">&#x43A;&c_a;&c_t;&c_a;&#x43F;&c_u;&#x43B;&#x44C;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x6295;&#x77F3; <pr>t&#xF3;ush&#xED;</pr></trd>,
    <trd>&#x629B;&#x77F3; <pr>p&#x101;osh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>

