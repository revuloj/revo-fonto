<art mrk="$Id: lampir.xml,v 1.27 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lampir</rad>/o
</kap>
<drv mrk="lampir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="lampir1.0oj"><tld/>edoj</ref> (<trd lng="la">Lampyris</trd>),
      kies ino havas &ccirc;e abdomeno du organojn per kiuj &gcirc;i povas
      produkti
      gla&ubreve;kan lumeton:
      <ekz>
        <ctl>studi &ccirc;e la ne&gcirc;a helo a&ubreve; <tld/>a brilo</ctl>
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, p. 592a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_ja;&c_k;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cuca de llum</trd>,
    <trd>lluerna</trd>,
    <trd>llumeneta</trd>
  </trdgrp>
  <trd lng="cs">sv&ecaron;tlu&scaron;ka</trd>
  <trdgrp lng="de">
      <trd>Leuchtk&auml;fer</trd>,
      <trd>Gl&uuml;hw&uuml;rmchen</trd>
    </trdgrp>
  <trd lng="el">&pi;&ypsilon;&gamma;&omikron;&lambda;&alfa;&my;&pi;&jota_ton;&delta;&alfa;</trd>
  <trdgrp lng="en">
    <trd>glow-worm</trd>,
    <trd>firefly</trd>
  </trdgrp>
  <trd lng="es">luci&eacute;rnaga</trd>
  <trd lng="fr">vers luisant</trd>
  <trd lng="hu">szentj&aacute;nosbog&aacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#x30C4;&#x30C1;&#x30DC;&#x30BF;&#x30EB;</trd>,
    <trd>&#x30DB;&#x30BF;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">glimworm</trd>
  <trd lng="pl">&sacute;wietlik &sacute;wi&eogonek;toja&nacute;ski</trd>
  <trdgrp lng="pt">
    <trd>vaga-lume</trd>,
    <trd>pirilampo</trd>
  </trdgrp>
  <trd lng="ro">licurici </trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">sv&auml;toj&aacute;nska mu&scaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_ib;&c_t;&#x43B;&#x44F;&#x43A;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&#x43B;&#x44F;&c_ch;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8424; <pr>y&#xED;ng</pr></trd>,
    <trd>&#x87A2; <pr>y&#xED;ng</pr></trd>,
    <trd>&#x735C; <pr>l&#xED;n</pr></trd>,
    <trd>&#x8688; <pr>qi&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
