<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lampir</rad>/o
</kap>
<drv mrk="lampir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="lampir.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lampyris</trd>),
      havanta &ccirc;e abdomeno du organojn per kiuj &gcirc;i
      povas produkti lumeton;.
    </dif>
  </snc>
  <trd lng="de">Gl&uuml;hw&uuml;rmchen</trd>
</drv>
<drv mrk="lampir.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lampyridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Leuchtk&auml;fer</trd>
</drv>
</art>
