<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lampir</rad>/o
</kap>
<drv mrk="lampir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="lampir1.0oj"><tld/>edoj</ref> (<trd lng="la">Lampyris</trd>),
      kies ino havas ĉe abdomeno du organojn per kiuj ĝi povas produkti
      glaŭkan lumeton:
      <ekz>
        <ctl>studi ĉe la neĝa helo aŭ <tld/>a brilo</ctl>
        <fnt><bib>RugxDom</bib>, <lok>ĉapitro 75a, p. 592a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>сьвятляк</trd>,
    <trd>сьветлячок</trd>
  </trdgrp>
  <trd lng="de">Glühwürmchen</trd>
  <trd lng="el">πυγολαμπίδα</trd>
  <trdgrp lng="en">
    <trd>glow-worm</trd>,
    <trd>firefly</trd>
  </trdgrp>
  <trd lng="fr">vers luisant</trd>
  <trd lng="hu">szentjánosbogár</trd>
  <trd lng="nl">glimworm</trd>
  <trdgrp lng="pt">
    <trd>vaga-lume</trd>,
    <trd>pirilampo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>светляк</trd>,
    <trd>светлячок</trd>
  </trdgrp>
</drv>
</art>
