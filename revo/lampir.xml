  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="lampir1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lampyris</trd>),
      havanta &ccirc;e abdomeno du organojn per kiuj &gcirc;i
      povas produkti lumeton.
    </dif>
  </snc>
  <trd lng="de">Gl&uuml;hw&uuml;rmchen</trd>
  <trd lng="nl">glimworm</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
