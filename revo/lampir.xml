<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lampir</rad>/o
</kap>
<drv mrk="lampir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="lampir1.0oj"><tld/>edoj</ref> (<trd lng="la">Lampyris</trd>),
      kies ino havas &ccirc;e abdomeno du organojn per kiuj &gcirc;i povas produkti
      gla&ubreve;kan lumeton:
      <ekz>
        <ctl>studi &ccirc;e la ne&gcirc;a helo a&ubreve; <tld/>a brilo</ctl>
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, p. 592a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gl&uuml;hw&uuml;rmchen</trd>
  <trd lng="el">&pi;&ypsilon;&gamma;&omikron;&lambda;&alfa;&my;&pi;&jota_ton;&delta;&alfa;</trd>
  <trd lng="fr">vers luisant</trd>
  <trd lng="hu">szentj&aacute;nosbog&aacute;r</trd>
  <trd lng="nl">glimworm</trd>
  <trdgrp lng="pt">
    <trd>vaga-lume</trd>,
    <trd>pirilampo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
