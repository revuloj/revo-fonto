<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: paladi.xml,v 1.18 2018/11/29 16:10:17 revo Exp $">
<kap><rad>paladi</rad>/o<fnt>Z</fnt></kap>
<drv mrk="paladi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Pd</mlg>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="46">elemento</ref> kun
      atomnumero 46, atompezo 106,4; multekosta 
      griza transir<ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> kemie similas al plat<klr>[e]</klr>no, kaj
        <klr>[&#8230;]</klr> &ccirc;efe uzata en kataliziloj kaj juveloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22794">Paladio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_p;&c_a;&c_l;&c_a;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">palladiom</trd>
  <trdgrp lng="cs">
    <trd>Palladion</trd>,
    <trd>palladium</trd>
  </trdgrp>
  <trd lng="de">Palladium</trd>
  <trd lng="en">palladium</trd>
  <trd lng="es">paladio</trd>
  <trd lng="fr">palladium</trd>
  <trd lng="hu">pall&aacute;dium</trd>
  <trd lng="id">paladium</trd>
  <trd lng="it">palladio</trd>
  <trd lng="la">Palladium</trd>
  <trd lng="nl">palladium</trd>
  <trd lng="pl">pallad</trd>
  <trd lng="pt">pal&aacute;dio</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_l;&c_a;&c_d;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>pal&aacute;dium</trd>,
    <trd>zn. Pd</trd>
  </trdgrp>
  <trd lng="sv">palladium</trd>
</drv>
</art>
