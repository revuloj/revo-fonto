<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>paladi</rad>/o<fnt>Z</fnt></kap>
<drv mrk="paladi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 46, atompezo 106,4, simbolo Pd. Multekosta griza <ref
      tip="super" cel="metal.0o">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_d;&c_y;&c_j;</trd>       
  <trd lng="br">palladiom</trd>
  <trd lng="de">Palladium</trd>  
  <trd lng="be">palladium</trd>
  <trd lng="hu">pall&aacute;dium</trd>
  <trd lng="nl">palladium</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_l;&c_a;&c_d;&c_i;&c_j;</trd>
</drv>
</art>
