<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adekva.xml,v 1.34 2023/09/27 06:31:21 revo Exp $">
<kap>
  <rad>adekvat</rad>/a
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="adekva.0a">
  <kap><tld/>a</kap>
  <snc mrk="adekva.0a.KOMUNE">
    <dif>
      <refgrp tip="dif">
        <ref cel="konfor.cel0a">Celkonforma</ref>,
        <ref cel="konfor.0a.celkonforma">konforma <sncref/></ref>
      </refgrp>:
      <ekz>
        parto de la nuntempe laboranta lektoraro ne havas <tld/>ajn
        kvalifikojn
        <fnt><aut>Stanis&lstroke;aw &Sacute;migielski</aut>: <vrk>Instruisto
        de la angla ser&ccirc;ata</vrk>, <bib>Monato</bib><lok>2001/02, p. 19</lok></fnt>;
      </ekz>
      <ekz>
        tia antologio devas <klr>[&#8230;]</klr> enteni kiom eble plej
        <tld/>ajn tradukojn de elektitaj pecoj
        <fnt><aut>Nikolao Gudskov</aut>:
        <vrk>Provo ektu&scirc;i serban poezion (Srdj Arandjelovi&cacute;, Antologio
        de  la serba poezio (1200-2000))</vrk>, <bib>Monato</bib><lok>
        2002/02, p. 32</lok></fnt>.
      </ekz>
      <ekz>
        parto de la nuntempe laboranta lektoraro ne havas <tld/>ajn kvalifikojn
        <fnt>
          <aut>Stanis&lstroke;aw &Sacute;migielski</aut>
          <vrk><url ref="&Monato;/2000/007193.html">Instruisto de la angla ser&ccirc;ata</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        la grandaj potencoj <klr>[&#8230;]</klr> ne <tld/>e informas pri sia agado
        <fnt>
          <aut>Maritza Guti&eacute;rrez</aut>
          <vrk><url ref="&Monato;/1993/A931202.php">A&ubreve;di&gcirc;as komuna vo&ccirc;o</url></vrk>
           <bib>Monato</bib>
          <lok>1993/12, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dec.0a.merita">deca<sncref/></ref>,
      <ref cel="gxust.0a.celo">&gcirc;usta<sncref/></ref>,
      <ref cel="konven.0a">konvena</ref>,
      <ref cel="cel.laux0a">la&ubreve;cela</ref>,
      <ref cel="oportu.0a">oportuna</ref>,
      <ref cel="suficx.0a">sufi&ccirc;a</ref>,
      <ref cel="tauxg.0a">ta&ubreve;ga</ref>
    </refgrp>
  </snc>
  <snc mrk="adekva.0a.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="prt" cel="komple2.0a.KOMUNE">Kompleta</ref>
      kaj <ref tip="prt" cel="gxust.0a.celo">&gcirc;usta</ref>:
      <ekz>
        la&ubreve; <nom>Carnap</nom> ekspliko estas <tld/>a, kiam &gcirc;i estas simila al la eksplikata nocio,
        ekzakta, science utila kaj simpla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_e;&c_k;&c_v;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_d;&c_je;&c_k;&c_v;&c_a;&c_t;&c_je;&c_n;</trd>
  <trd lng="ca">adequat</trd>
  <trdgrp lng="cs">
    <trd>adekv&aacute;tn&iacute;</trd>,
    <trd>odpov&iacute;daj&iacute;c&iacute;</trd>,
    <trd>pat&rcaron;i&ccaron;n&yacute;</trd>,
    <trd>p&rcaron;im&ecaron;&rcaron;en&yacute;</trd>,
    <trd>vhodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ad&auml;quat</trd>,
    <trd>entsprechend</trd>,
    <trd>angemessen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>adequate</trd>,
    <trd>appropriate</trd>,
    <trd>sufficient</trd>
  </trdgrp>
  <trd lng="es">adecuado</trd>
  <trdgrp lng="fr">
    <trd>ad&eacute;quat</trd>,
    <trd>adapt&eacute;</trd>,
    <trd>appropri&eacute;</trd>,
    <trd>juste<klr tip="ind"> (appropri&eacute;)</klr></trd>,
    <trd>propre<klr tip="ind"> (appropri&eacute;)</klr></trd>,
    <trd>opportun<klr tip="ind"> (appropri&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E0;&#x5D0;&#x5D5;&#x5EA;</trd>
  <trd lng="it">adeguato</trd>
  <trdgrp lng="ja">
    <trd>&#x76EE;&#30340;&#x306B;&#x304B;&#x306A;&#12387;&#x305F; 
    [<ind>&#12418;&#x304F;&#12390;&#x304D;&#x306B;&#x304B;&#x306A;&#12387;&#x305F;</ind>]</trd>,
    <trd>&#21313;&#20998;&#x306A;  [<ind>&#12376;&#12421;&#12358;&#12406;&#12435;&#x306A;</ind>]</trd>,
    <trd>&#x59A5;&#x5F53;&#x306A;  [<ind>&#12384;&#12392;&#12358;&#x306A;</ind>]</trd>,
    <trd>&#21313;&#20840;&#x306A;  [<ind>&#12376;&#12421;&#12358;&#x305C;&#12435;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geschikt</trd>,
    <trd>adequaat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>adekwatny</trd>,
    <trd>odpowiedni</trd>
 </trdgrp>
  <trdgrp lng="pt">
    <trd>adequado</trd>,
    <trd>apropriado</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>adecvat</trd>,
    <trd>potrivit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_je;&c_k;&c_v;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>primeran&yacute;</trd>,
    <trd>zodpovedaj&uacute;ci</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>adekvat</trd>,
    <trd>tillfredst&auml;llande</trd>,
    <trd>passande</trd>,
    <trd>l&auml;mplig</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_d;&c_je;&#x43A;&c_v;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_v;&c_ib;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_t;&#x43E;&c_t;&#x43E;&c_zh;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
