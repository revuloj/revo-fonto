<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adekvat</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="adekva.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Konforma, egalvalora, ta&ubreve;ge esprimanta.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ad&auml;quat</trd>,
    <trd>entsprechend</trd>,
    <trd>angemessen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>adequate</trd>,
    <trd>appropriate</trd>,
    <trd>sufficient</trd>
  </trdgrp>
  <trd lng="es">adecuado</trd>
  <trd lng="fr">ad&eacute;quat</trd>
  <trd lng="it">adeguato</trd>
  <trdgrp lng="nl">
    <trd>geschikt</trd>,
    <trd>adequaat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adequado</trd>,
    <trd>apropriado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_je;&c_k;&c_v;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
