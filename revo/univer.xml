<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>univers</rad>/o
</kap>
<drv mrk="univer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad/>
  <tezrad fak="AST"/>
  <snc mrk="univer.0o.FIZ">
    <dif>
      La tuto de &ccirc;io, kio ekzistas:
      <ekz>
        post tempo la antimaterio malaperis el la <tld/>o
        <fnt>
        <aut>OH</aut>,
        <vrk>&Ccirc;u antimaterio en la universo?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;</trd>
  <trd lng="ca">univers</trd>
  <trd lng="br">hollved</trd>
  <trd lng="de">Universum</trd>
  <trd lng="el">&sigma;&ypsilon_ton;&my;&pi;&alfa;&ny;</trd>
  <trd lng="en">universe</trd>
  <trd lng="es">universo</trd>
  <trd lng="fr">univers</trd>
  <trdgrp lng="hu">
    <trd>vil&aacute;gegyetem</trd>,
    <trd>univerzum</trd>
  </trdgrp>
  <trd lng="it">universo</trd>
  <trd lng="nl">heelal</trd>
  <trd lng="pt">universo</trd>
  <trd lng="ru">&c_v;&c_s;&c_je;&c_l;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
<trdgrp lng="tp">
<trd>ale</trd>,
<trd>ali</trd>
</trdgrp>
  <trd lng="sk">vesm&iacute;r</trd>
</drv>

<drv mrk="univer.0a">
  <kap><tld/>a</kap>
  <snc mrk="univer.0a.FIZ">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld/>a &hcirc;emio, fiziko
        <klr>(studantaj la fenomenoj je la skalo de l' <tld/>o, kaj ne de
        la tero)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y;</trd>
  <trd lng="br">hollvedel</trd>
  <trd lng="ca">universal</trd>
  <trd lng="en">universal</trd>
  <trd lng="es">universal</trd>
  <trdgrp lng="fr">
    <trd>cosmique</trd>,
    <trd>universel<klr> (cosmique)</klr></trd>
  </trdgrp>
  <trd lng="hu">&udblac;r-</trd>
  <trd lng="pt">universal</trd>
  <trdgrp lng="sk">
    <trd>kozmick&yacute;</trd>,
    <trd>vesm&iacute;rny</trd>
  </trdgrp>
<trdgrp lng="tp">
<trd>ale</trd>,
<trd>ali</trd>
</trdgrp>
</drv>
</art>
