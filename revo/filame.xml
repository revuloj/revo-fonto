<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filament</rad>/o
</kap>
<drv mrk="filame.0o">
  <kap><tld/>o</kap>
  <snc mrk="filame.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fleksebla, tre maldika cilindra objekto, multe pli longa ol dika.
    </dif>
  </snc>
  <snc mrk="filame.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tiuforma anatomia elemento.
    </dif>
  </snc>
  <snc mrk="filame.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      La malsupra parto de <ref tip="malprt" cel="stamen.0o">stameno</ref>.
    </dif>
    <trd lng="de">Staubfaden</trd>
  </snc>
  <snc mrk="filame.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Tiuforma metala objekto, kiu, varmigita de elektro, produktas lumon en
      lampo.
    </dif>
  </snc>
</drv>
<drv mrk="filame.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta karakteron de <tld/>o.
    </dif>
  </snc>
  <trd lng="en">filament</trd>
</drv>
</art>
