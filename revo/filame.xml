<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filament</rad>/o
</kap>
<drv mrk="filame.0o">
   <kap><tld/>o</kap>
   <snc mrk="filame.0o.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Fleksebla, tre maldika cilindra objekto, multe pli longa ol dika.
     </dif>
     <trd lng="ru">&c_zh;&c_g;&c_u;&c_t;</trd>
   </snc>
   <snc mrk="filame.0o.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Tiuforma anatomia elemento.
     </dif>
     <trd lng="ru">&c_zh;&c_g;&c_u;&c_t;&c_i;&c_k;</trd>
   </snc>
   <snc mrk="filame.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       La malsupra parto de <ref tip="malprt" cel="stamen.0o">stameno</ref>.
     </dif>
     <adm>
     Koherigi kun la difino de stameno, kiu parolas pri "fadeno". <aut>MB</aut>
     </adm>
     <trd lng="de">Staubfaden</trd>
     <trd lng="ru">&c_zh;&c_g;&c_u;&c_t;&c_i;&c_k;</trd>
   </snc>
   <snc mrk="filame.0o.ELE">
     <uzo tip="fak">ELE</uzo>
     <dif>
       Tiuforma metala objekto, kiu, varmigate de elektro, produktas lumon en
       lampo.
     </dif>
     <trd lng="ru">&c_n;&c_i;&c_t;&c_mol; &c_n;&c_a;&c_k;&c_a;&c_l;&c_a;</trd>
   </snc>
   <trdgrp lng="fr">
     <trd>filament</trd>,
     <trd>filandre</trd>,
     <trd>filet<klr tip="ind"> (bot.)</klr></trd></trdgrp>
</drv>
<drv mrk="filame.0eca">
   <kap><tld/>eca</kap>
   <snc>
     <dif>
       Havanta karakteron de <tld/>o.
     </dif>
   </snc>
   <trd lng="en">filament</trd>
   <trd lng="fr">filamenteux</trd>
</drv>
</art>
