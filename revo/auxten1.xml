<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;tentik</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="auxten1.0a">
  <kap><tld/>a</kap>
  <snc mrk="auxten1.0a.propra">
    <dif>
      Nedubeble certa, vera, originala, ne falsita,
      <ref tip="sin" cel="auxten.0a.KOMUNE">a&ubreve;tenta</ref>:
      <ekz>
        <tld/>a dokumento;
      </ekz>
      <ekz>
        mezepokaj verkoj prezentataj per <tld/>aj
        muzikiloj<fnt><aut>-</aut>: <vrk>Anna kaj Mati Pentus</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 6a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_w;&c_t;&c_e;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_e;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_a;&c_v;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_n;</trd>,
      <trd>&c_i;&c_s;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;</trd>
    </trdgrp>
    <trd lng="it">autentico</trd>
    <trdgrp lng="pl">
      <trd>autentyczny</trd>,
      <trd>prawdziwy</trd>,
      <trd>rzeczywisty</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_a;&c_u;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxten1.0a.auxtentigita">
    <uzo tip="stl">EVI</uzo>
    <dif>
      A&ubreve;tentigita.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_je;&c_n;</trd>
    <trd lng="it">autentico<klr> (autentificato)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="ant">
    <ref cel="fals.0a.malauxtenta">falsa</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>authentisch</trd>,
    <trd>echt</trd>
  </trdgrp>
  <trd lng="en">authentic</trd>
  <trd lng="es">aut&eacute;ntico</trd>
  <trd lng="fr">authentique</trd>
  <trdgrp lng="hu">
    <trd>hiteles</trd>,
    <trd>val&oacute;di</trd>,
    <trd>autentikus</trd>
  </trdgrp>
  <trd lng="nl">authentiek</trd>
  <trd lng="sk">autentick&yacute;</trd>
</drv>

<drv mrk="auxten1.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="auxten1.0eco.KOMUNE">
    <dif>
      Eco de io <tld/>a,
      <ref tip="sin" cel="auxten.0eco.KOMUNE">a&ubreve;tenteco</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_w;&c_t;&c_e;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_e;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_a;&c_v;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Authentizit&auml;t</trd>,
    <trd>Echtheit</trd>
  </trdgrp>
  <trd lng="en">authenticity</trd>
  <trd lng="es">autenticidad</trd>
  <trd lng="fr">authenticit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>hiteless&eacute;g</trd>,
    <trd>val&oacute;dis&aacute;g</trd>
  </trdgrp>
  <trd lng="it">autenticit&agrave;</trd>
  <trdgrp lng="nl">
    <trd>authenticiteit</trd>,
    <trd>echtheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>autentyczno&sacute;&cacute;</trd>,
    <trd>prawdziwo&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_u;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="auxten1.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxten1.0igi.KOMUNE">
    <dif>
      Oficiale certigi la <tld/>econ,
      <ref tip="sin" cel="auxten.0igi.KOMUNE">a&ubreve;tentigi</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_v;&c_a;&c_m; <ind>&c_a;&c_v;&c_t;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;&c_t;&c_o;</ind> &c_n;&c_a;</trd>,
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_v;&c_a;&c_m; <ind>&c_i;&c_s;&c_t;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;&c_t;&c_a;</ind> &c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>authentifizieren</trd>,
      <trd>Echtheit best&auml;tigen</trd>
    </trdgrp>
    <trd lng="en">authenticate</trd>
    <trd lng="es">autentificar</trd>
    <trd lng="fr">authentifier</trd>
    <trd lng="hu">hiteles&iacute;tr</trd>
    <trd lng="it">autentificare</trd>
    <trdgrp lng="nl">
      <trd>waarmerken</trd>,
      <trd>authentificeren</trd>
    </trdgrp>
    <trd lng="pl">potwierdza&cacute; <ind>autentyczno&sacute;&cacute;</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="auxten1.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="auxten1.mal0a.KOMUNE">
    <dif>
      Ne vera, ne origina, falsita;
      <ref tip="sin" cel="auxten.mal0a.KOMUNE">mala&ubreve;tenta</ref>:
      <ekz>
        mal<tld/>a, malsincera, sirope elgutanta &ccirc;armo
        <fnt>
          <aut>J. Wain, trad. M. Boulton</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr73/rikardo.html">
          Sinjoreto Rikardo</url></vrk>,
          <lok>Nica Literatura Revuo, 1962-01 &gcirc;is 02, (7/3), p. 
          93a-106a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">inauthentic</trd>
  <trd lng="fr">inauthentique</trd>
  <trdgrp lng="pl">
    <trd>nieautentyczny</trd>,
    <trd>nieprawdziwy</trd>,
    <trd>fa&lstroke;szywy</trd>
  </trdgrp>
</drv>
</art>
