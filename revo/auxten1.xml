<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;tentik</rad>/a
</kap>
<drv mrk="auxten1.0a">
  <kap><tld/>a</kap>
  <snc mrk="auxten1.0a.propra">
    <dif>
      Nedubeble certa, vera, originala, ne falsita:
      <ekz>
        <tld/>a dokumento;
      </ekz>
      <ekz>
        mezepokaj verkoj prezentataj per <tld/>aj
        muzikiloj<fnt><aut>-</aut>: <vrk>Anna kaj Mati Pentus</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 6a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_a;&c_u;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxten1.0a.auxtentigita">
    <uzo tip="stl">EVI</uzo>
    <dif>
      A&ubreve;tentigita.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="ant">
    <ref cel="fals.0a.malauxtenta">falsa</ref>
  </refgrp>
  <ref tip="sin" cel="auxten.0a">a&ubreve;tenta</ref>
  <trdgrp lng="de">
    <trd>authentisch</trd>,
    <trd>echt</trd>
  </trdgrp>
  <trd lng="es">aut&eacute;ntico</trd>
  <trd lng="fr">authentique</trd>
  <trdgrp lng="hu">
    <trd>hiteles</trd>,
    <trd>val&oacute;di</trd>,
    <trd>autentikus</trd>
  </trdgrp>
  <trd lng="sk">autentick&yacute;</trd>
</drv>

<drv mrk="auxten1.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de io <tld/>a.
    </dif>
    <refgrp tip="sin">
      <ref cel="auxten.0eco">a&ubreve;tenteco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Authentizit&auml;t</trd>,
    <trd>Echtheit</trd>
  </trdgrp>
  <trd lng="es">autenticidad</trd>
  <trd lng="fr">authenticit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>hiteless&eacute;g</trd>,
    <trd>val&oacute;dis&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_u;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="auxten1.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Oficiale certigi la <tld/>econ.
    </dif>
    <refgrp tip="sin">
      <ref cel="auxten.0igi">a&ubreve;tentigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>authentifizieren</trd>,
      <trd>Echtheit best&auml;tigen</trd>
    </trdgrp>
    <trd lng="es">autentificar</trd>
    <trd lng="fr">authentifier</trd>
    <trd lng="hu">hiteles&iacute;tr</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
