<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id$">
<kap><rad>fabac</rad>/oj</kap>
<drv mrk="fabac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Fabaceae</trd>),
      kun nesimetriaj papilisimilaj floroj kaj gu&scirc;oj.
    </dif>
    <ref tip="sin" cel="legume1.0oj">legumenacoj</ref>
    <refgrp tip="sub">
      <ref cel="vici.0o">vicio</ref>,
      <ref cel="fazeol.0o">fazeolo</ref>,
      <ref cel="piz.0o">pizo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schmetterlingsbl&uuml;tengew&auml;chse</trd>
  <trd lng="fr">fabac&eacute;es</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_o;&c_v;&c_y;&c_je;</trd>
</drv>
</art>

