<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fabac.xml,v 1.21 2022/03/18 13:01:59 revo Exp $">
<kap>
  <rad>fabac</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fabac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="fabac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref>
      de lignoplantoj, duonarbustoj, sta&ubreve;doj a&ubreve; herboj unujaraj
      el la ordo de faboparencaj
      <ref tip="lst" lst="voko:faboparencoj" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Fabaceae</trd>) kun:
      folioj alternaj a&ubreve; kruce alternaj, kun kromfolioj;
      floroj dulipaj, papiliformaj;
      5 sepaloj ofte kunkreskintaj;
      5 petaloj estantaj la supra la flago, la du flankaj la aloj kaj la du malsupraj la &scirc;ipeto;
      10 stamenoj, kunkreskintaj &dash; a&ubreve; &ccirc;iuj je fermita a&ubreve; nur na&ubreve; je supre malfermita &dash; tubo &ccirc;irka&ubreve;anta  la ovolujon;
      1 pistilo;
      gu&scirc;oj klape malfermi&gcirc;antaj, disfalantaj en partojn unusemajn a&ubreve; helice turni&gcirc;antaj:
      <ekz>
        &ccirc;iro estas maldika spirale volvi&gcirc;anta kro&ccirc;ilo de grimpa planto, ekz-e de vinberujo a&ubreve; &ccirc;e kelkaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20025">&Ccirc;iro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la esprimo <ref tip="prt" cel="akaci.0o">akacio</ref> estas ofte uzata por 
        <ref tip="prt" cel="robini.0o">robinio</ref>, kio estas tute erara, amba&ubreve; genroj 
        <klr>[...]</klr> apartenas nur al la sama familio, al la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21300">Akacio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="legume1.0oj">legumenacoj</ref>
    <refgrp tip="prt">
      <ref cel="fab.0o">fabo</ref>,
      <ref cel="fazeol.0o">fazeolo</ref>,
      <ref cel="piz.0o">pizo</ref>,
      <ref cel="vici.0o">vicio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_o;&c_v;&c_y;&c_ja;
  <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">fab&agrave;cies</trd>
  <trd lng="cs">mot&yacute;lokv&ecaron;t&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Schmetterlingsbl&uuml;tengew&auml;chse</trd>
  <trd lng="es">leguminosas</trd>
  <trd lng="fr">fabac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;velyesek<klr tip="ind"> (csal&aacute;d)</klr></trd>,
    <trd>pillang&oacute;svir&aacute;g&uacute;ak</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fabacee</trd>,
    <trd>leguminose</trd>
  </trdgrp>
  <trd lng="nl">vlinderbloemigen</trd>
  <trd lng="pl">motylkowate</trd>
  <trd lng="pt">fab&aacute;ceas</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_o;&c_v;&c_y;&c_je;</trd>
  <trd lng="sk">mot&yacute;&lcaron;okvet&eacute; <klr>(&ccaron;e&lcaron;a&dcaron;)</klr></trd>
  <trd lng="sv">&auml;rtv&auml;xter</trd>
  <trd lng="uk">&#x431;&#x43E;&#x431;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
