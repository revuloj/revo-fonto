<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>nikel</rad>/o
</kap>
<drv mrk="nikel.0o">
  <kap><tld/>o</kap>
  <mlg>Ni</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 28, atompezo 58,71, simbolo <frm>Ni</frm>; ar&gcirc;ente
      brila tre malmola
      transir<ref tip="super" cel="metal.0o">metalo</ref> diverse uzata:
      <ekz>
        Kosovo havas konsiderindajn rezervojn de
        <tld/>o-<ref tip="vid" cel="kobalt.0o">kobalto</ref>
        <fnt><aut>Bardhyl Mezini</aut>: <vrk>La vera 
        ka&ubreve;zo de la milito</vrk>, <bib>Monato</bib><lok>jaro 1999a,
        numero 5a,
        p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_k;&c_je;&c_l;&c_mol;</trd>     
  <trd lng="br">nikel</trd>
  <trd lng="de">Nickel</trd>
  <trd lng="el">&ny;&jota;&kappa;&epsilon_ton;&lambda;&lambda;&jota;&omikron;</trd>
  <trd lng="en">nickel</trd>
  <trd lng="es">n&iacute;quel</trd>
  <trd lng="fr">nickel</trd>
  <trd lng="hu">nikkel</trd>
  <trd lng="it">nichel</trd>
  <trd lng="la">Niccolum</trd>
  <trd lng="nl">nikkel</trd>
  <trd lng="pl">n&iacute;kiel</trd>
  <trd lng="pt">n&iacute;quel</trd>
  <trd lng="ru">&c_n;&c_i;&c_k;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sv">nickel</trd>
</drv>

<drv mrk="nikel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tegi per tre maldika tavolo el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_k;&c_je;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">nikela&ntilde;</trd>
  <trd lng="de">vernickeln</trd>
  <trd lng="el">(&epsilon;&pi;&jota;)&ny;&jota;&kappa;&epsilon;&lambda;&lambda;&omega_ton;&ny;&omega;</trd>
  <trd lng="en"><ind>nickel</ind> plate</trd>
  <trd lng="es">niqueler</trd>
  <trd lng="fr">nickeler</trd>
  <trd lng="hu">nikkelez</trd>
  <trd lng="nl">vernikkelen</trd>
  <trd lng="pl">n&iacute;klowa&cacute;</trd>
  <trd lng="pt">niquelar</trd>
  <trd lng="ru">&c_n;&c_i;&c_k;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rnickla</trd>
</drv>
</art>
