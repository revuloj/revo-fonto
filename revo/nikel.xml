<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>nikel</rad>/o
</kap>
<drv mrk="nikel.0o">
<kap><tld/>o</kap>
<snc>
<dif>
&Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun atomnumero 28, atompezo 58,71, simbolo Ni. Ar&gcirc;entebrila tre malmola <ref tip="super" cel="metal.0o">metalo</ref> uzata por diversaj uzoj. 
</dif>
</snc>
<trd lng="de">Nickel</trd>
<trd lng="en">nickel</trd>
<trd lng="fr">nickel</trd>
<trd lng="nl">nikkel</trd>
<trd lng="sv">nickel</trd>
 </drv>
<drv mrk="nikel.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Tegi per tre maldika tavolo de <tld/>o.  
</dif>
</snc>
<trd lng="de">vernickeln</trd>
<trd lng="nl">vernikkelen</trd>
 </drv>
</art>
