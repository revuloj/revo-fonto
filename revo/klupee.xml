<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klupee.xml,v 1.15 2024/08/09 06:36:38 revo Exp $">
<kap>
  <rad>klupeed</rad>/oj
</kap>
<drv mrk="klupee.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="klupe.0oformaj">klupeoformaj
      fi&scirc;oj</ref> (<trd lng="la">Clupeidae</trd>):
      <ekz>
        sproto a&ubreve; sprato estas fi&scirc;o el la familio de la <tld/>oj, al kiu
        anka&ubreve; apartenas
        la haringo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=268832">Sproto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_l;&c_ja;&c_d;&c_c;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="de">Heringsfische</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_w;&f_k;&#8204; &a_m;&a_A;&a_h;&f_y;&a_A;&a_n;</trd>,
    <trd>&a_s;&a_A;&a_r;&a_d;&f_y;&a_n; &a_m;&a_A;&a_h;&f_y;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">clup&eacute;id&eacute;s</trd>
  <trd lng="hu">heringf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30CB;&#x30B7;&#x30F3;&#x79D1;  <pr>&#x30CB;&#x30B7;&#x30F3;&#x304B;</pr></trd>
  <trd lng="nl">haringen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">&sacute;ledziowate</trd>
  <trd lng="ro">clupeide</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_d;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43E;&c_s;&c_je;&#x43B;&c_je;&c_d;&c_c;&c_je;&c_v;&c_ib;</trd>
</drv>
</art>
