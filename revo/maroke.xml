<art mrk="$Id: maroke.xml,v 1.20 2023/10/19 15:13:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>maroken</rad>/o
</kap>
<drv mrk="maroke.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Grajneca kolorita kapro<ref tip="super" cel="led.0o">ledo</ref>:
      <ekz>
        ni admiris <klr>[&#x2026;]</klr> kajerojn binditajn el <tld/>o por la &ccirc;iutagaj menuoj
        <fnt>
          <aut>Georges Simenon, trad. Daniel Luez</aut>
          <vrk><url ref="&LOdE;Libroj/Simenon.pdf">Maigret hezitas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxagri.0o">&scirc;agrino</ref>,
      <ref cel="sxam.0o">&scirc;amo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_a;&c_k;&c_je;&c_n;</trd>,
    <trd>&c_s;&c_a;&c_f;'&c_ja;&c_n;</trd>
  </trdgrp>        
  <trd lng="br">gavrgen</trd>
  <trd lng="ca">tafilet</trd>
  <trdgrp lng="de">
    <trd>Maroquin</trd>,
    <trd>Saffianleder</trd>
  </trdgrp>
  <trd lng="en">Moroccan <ind>leather</ind></trd>  
  <trdgrp lng="es">
    <trd>tafilete</trd>,
    <trd>cuero marroqu&iacute;</trd>
  </trdgrp>
  <trd lng="fr">maroquin</trd>
  <trdgrp lng="hu">
    <trd>marokk&oacute;b&odblac;r</trd>,
    <trd>szatty&aacute;nb&odblac;r</trd>
    <trd>maroken</trd>
  </trdgrp>  
  <trd lng="ja">&#x30E2;&#x30ED;&#x30C3;&#x30B3;&#x9769;  <pr>&#x30E2;&#x30ED;&#x30C3;&#x30B3;&#x304C;&#x308F;</pr></trd>
  <trd lng="nl">marokijnleer</trd>
  <trd lng="pl">safian</trd>
  <trd lng="pt">marroquim</trd>
  <trd lng="ro">maroc</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_f;&c_mol;&c_ja;&c_n;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_o;&c_k;&c_i;&c_n;</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x430;&#x43F;&#x2019;&#x44F;&#x43D;</trd>
</drv>
</art>
