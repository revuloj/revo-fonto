<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kadavr</rad>/o
</kap>
<drv mrk="kadavr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Korpo de mortinta homo a&ubreve; besto:
      <ekz><tld/>a palo kovris lian viza&gcirc;on.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skelet.0o">skeleto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_p;</trd>
  <trdgrp lng="de">
     <trd>Leiche</trd>,
     <trd>Leichnam</trd>,
     <trd>Kadaver</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cadaver</trd>,
    <trd>corpse</trd>,
    <trd>(dead) <ind>body</ind></trd>
  </trdgrp>
  <trd lng="es">cad&aacute;ver</trd>
  <trdgrp lng="fr">
    <trd>cadavre</trd>,
    <trd>corps<klr tip="ind"> (d&eacute;pouille mortelle)</klr></trd>,
    <trd>d&eacute;pouille<klr tip="ind"> (mortelle)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&gimel;&vav;&pe;&he;</trd>,
    <trd>&gimel;&vav;&vav;&yod;&yod;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hulla</trd>,
    <trd>holttest</trd>,
    <trd>d&ouml;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lijk</trd>,
    <trd>kadaver</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_p;</trd>
  <trdgrp lng="sv">
    <trd>lik</trd>,
    <trd>kadaver</trd>
  </trdgrp>
</drv>
</art>
