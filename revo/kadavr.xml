<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kadavr.xml,v 1.32 2022/03/21 17:51:56 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>kadavr</rad>/o
</kap>
<drv mrk="kadavr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="korp.0o.ANA">Korpo</ref> de mortinta homo a&ubreve; besto:
      <ekz>
        rabobirdoj mallevi&gcirc;is sur la <tld/>ojn, sed <nom>Abram</nom> ilin forpelis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la bovo de iu homo kornobatos bovon de lia proksimulo tiel, ke &gcirc;i mortos,
        tiam <klr>[&#x2026;]</klr> la kadavron ili dividu
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj <tld/>oj konsumi&gcirc;os en la dezerto
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 14:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> ku&scirc;is sur la placo dis&jcirc;etitaj lancoj kaj hakiloj, kaj
        <klr>[&#x2026;]</klr> amasoj da vunditoj kaj <tld/>oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        enmetu viajn glavojn, &ccirc;i tie li ku&scirc;as kiel <tld/>i&gcirc;inta kato
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas pala kiel <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tie]</klr> skeleto de mizero plej la&ubreve;te sonas per siaj nudaj ostoj, plej
        terure rigardas per siaj <tld/>aj okuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li preferus vidi sian filinon <tld/>o, ol ies konkubino
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris al la fajro, varmigis sian manon kaj metis &gcirc;in sur la viza&gcirc;on de la
        mortinto, sed la <tld/>o restis malvarma
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a tuko</ind> <klr>(por entombigo)</klr>
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
        <trd lng="de">Leichentuch</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skelet.0o">skeleto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_p;</trd>
  <trd lng="bg">&c_t;&c_r;&c_u;&c_p;</trd>
  <trd lng="bo">&t_ra;&t_naro;&t_tsheg;</trd>
  <trd lng="ca">cad&agrave;ver</trd>
  <trdgrp lng="cs">
    <trd>kad&aacute;ver</trd>,
    <trd>mrtvola</trd>,
    <trd>mrtv&eacute; t&ecaron;lo</trd>,
    <trd>mr&scaron;ina</trd>,
    <trd>t&ecaron;lo zem&rcaron;el&eacute;ho</trd>,
    <trd>uhynul&eacute; zv&iacute;&rcaron;e</trd>,
    <trd>umrlec</trd>,
    <trd>zdechlina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Leiche</trd>,
    <trd>Leichnam</trd>,
    <trd>Kadaver</trd>
  </trdgrp>
  <trdgrp lng="en">
   <trd>cadaver</trd>,
   <trd>corpse</trd>,
   <trd>(dead) <ind>body</ind></trd>
  </trdgrp>
  <trd lng="es">cad&aacute;ver</trd>
  <trdgrp lng="fa">
     <trd>&a_j;&a_s;&a_d;</trd>,
     <trd>&a_j;&a_n;&a_A;&a_z;&a_h;</trd>,
     <trd>&a_n;&a_ayn;&a_sh;</trd>
   </trdgrp>
  <trdgrp lng="fi">
   <trd><klr tip="amb">(kuollut) </klr> ruumis</trd>,
   <trd>raato</trd>
  </trdgrp>
  <trdgrp lng="fr">
   <trd>cadavre</trd>,
   <trd>corps<klr tip="ind"> (d&eacute;pouille mortelle)</klr></trd>,
   <trd>d&eacute;pouille<klr tip="ind"> (mortelle)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
   <trd>&gimel;&vav;&pe;&he;</trd>,
   <trd>&gimel;&vav;&vav;&yod;&yod;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>hulla</trd>,
   <trd>holttest</trd>,
   <trd>d&ouml;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>lijk</trd>,
   <trd>kadaver</trd>
  </trdgrp>
  <trd lng="pl">trup</trd>
  <trd lng="pt">cad&aacute;ver</trd>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_p;</trd>
  <trd lng="sk">m&racute;tvola</trd>
  <trdgrp lng="sv">
   <trd>lik</trd>,
   <trd>kadaver</trd>
  </trdgrp>
  <trdgrp lng="tr">
     <trd>ceset</trd>,
     <trd>kadavra</trd>
   </trdgrp>
  <trd lng="uk">&#x442;&#x440;&#x443;&#x43F;</trd>
</drv>

<drv mrk="kadavr.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Io mortinta, resta&jcirc;o de <tld/>o:
      <ekz>
        la sebon de <tld/>a&jcirc;o <klr>[&#x2026;]</klr> oni povas uzi por &ccirc;ia laboro, sed man&gcirc;i &gcirc;in
        vi ne devas
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 7:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;on kaj ion, kion dis&scirc;iris bestoj, li ne man&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 22:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun nigran mortosimilan kapturni&gcirc;on <klr>[&#x2026;]</klr> donas la odoro de fendi&gcirc;anta
        <tld/>a&jcirc;o kaj la vido de <tld/>oplena loko de batalo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="mort.0intajxo">mortinta&jcirc;o</ref>,
       <ref cel="putr.0ajxo.KOMUNE">putra&jcirc;o<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Aas</trd>,
    <trd>Kadaver</trd>
  </trdgrp>
  <trd lng="en">carrion</trd>
  <trd lng="fr"> charogne</trd>
  <trd lng="ru">&c_p;&c_a;&#x301;&c_d;&c_a;&c_l;&c_mol;</trd>
</drv>


</art>
