<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>diferenc</rad>/i</kap>
<drv mrk="difere.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti malsama:
      <ekz>
        <tld/>i de, je iu, per, pri, pro io;
      </ekz>
      <ekz>
        ili forte <tld/>as inter si per la sa&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj tiuj projektoj abisme <tld/>as de Esperanto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.de0igxi">deklini&gcirc;i</ref>,
      <ref cel="vari.0i">varii</ref>,
      <ref cel="kontra8">kontrasti</ref>,
      <ref cel="distin.0igxi">distingi&gcirc;i</ref>,
      <ref cel="devi.0i">devii</ref>,
      <ref cel="diverg.0i">diver&gcirc;i</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="mar.0o">maro</ref>,
      <ref cel="fend.0o">fendo</ref>,
      <ref cel="abism.0o">abismo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">differ</trd>
  <trdgrp lng="fr">
    <trd>diff&eacute;rer</trd>,
    <trd>&ecirc;tre <ind>diff&eacute;rent</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="difere.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsameco:
      <ekz>
        oni devas fari <tld/>on inter amba&ubreve; okazoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malgrava estas la <tld/>o inter profitemo kaj profitemeco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distan.0o">distanco</ref>,
      <ref cel="nuanc.0o">nuanco</ref>,
      <ref cel="konsen">malkonsento</ref>.
    </refgrp>
    <trd lng="cs">rozd&iacute;l</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_i;&c_je;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_n;&c_i;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="difere.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Rezulto de <ref cel="subtra.0o">subtraho</ref>:
      <ekz>
        la <tld/>o inter 10 kaj 6 estas 4.
      </ekz>
    </dif>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="fr">diff&eacute;rence</trd>
</drv>

<drv mrk="difere.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malsama:
      <ekz>
        li estas tro <tld/>a je (de, ol) sia frato.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="divers.0a">diversa</ref>,
      <ref cel="ali.0a">alia</ref>,
      <ref cel="proksi">malproksima</ref>,
      <ref cel="fremd.0a">fremda</ref>,
      <ref cel="antipo">antipoda</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">diff&eacute;rent</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="difere.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi malsamaj.
    </dif>
    <trd lng="fr">rendre <ind>diff&eacute;rents</ind></trd>
  </snc>
  <snc>
    <dif>
      Distingi, malkonfuzi; evidentigi <tld/>on:
      <ekz>
        <tld/>igi la verbojn la&ubreve; transitiveco kaj netransitiveco;
      </ekz>
      <ekz>
        <tld/>igi la juston de la maljusto.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>diff&eacute;rencier<klr tip="ind"> (distinguer)</klr></trd>,
      <trd>distinguer</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
