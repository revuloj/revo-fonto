<drv mrk="difere.0a">
  <kap><tld/>a</kap>
  <snc mrk="difere.0a.KLA">
    <dif>
      <ref tip="dif" cel="sam.mal0a">Malsama</ref>:
      <ekz>
        ni ne devas tiel severe ju&gcirc;i homojn tiel <tld/>ajn de ni, tute <tld/>ajn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fra&ubreve;lino dismetis sur la montrotablo dekon da <tld/>aj ekzempleroj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Rika</nom> tamen a&ccirc;etas tri <klr>[fajfilojn]</klr> de <tld/>a longeco
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>a &scirc;i estas de mi
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>!
      </ekz>
      <ekz>
        io <tld/>a <klr>(<ref cel="apart.0a.speciala">aparta</ref>)</klr>
        kaj stranga estas en ilia morto
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli-malpli <tld/>aj opinioj pri negravaj detaloj, sed preska&ubreve; la sama fundamenta
        koncepto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        diversaj juraj sistemoj preskribas <tld/>ajn regulojn de interpretado
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la evoluo de la homaro <klr>[&#8230;]</klr> en <tld/>ajn kaj poste e&ccirc;
        antagonismajn klasojn, rompi&gcirc;is anka&ubreve; la lingvo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="egal.0a">egala</ref>
    <refgrp tip="vid">
      <ref cel="ali.0a">alia</ref>,
      <ref cel="divers.0a">diversa</ref>,
      <ref cel="fremd.0a">fremda</ref>,
      <ref cel="proksi">malproksima</ref>,
      <ref cel="specia.0a">speciala</ref>
    </refgrp>
  </snc>
  <trd lng="bo">&t_kha;&t_ga;&t_tsheg;&t_kha;&t_ga;&t_tsheg;</trd>
  <trd lng="en">different</trd>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_o;&c_z;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odli&scaron;n&yacute;</trd>,
    <trd>rozd&iacute;ln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterschiedlich</trd>,
    <trd>abweichend</trd>,
    <trd>differierend</trd>
  </trdgrp>
  <trd lng="es">diferente</trd>
  <trd lng="fr">diff&eacute;rent</trd>
  <trd lng="he">&shin;&vav;&nun;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#36949;&#12387;&#x305F;  <pr>&#12385;&#x304C;&#12387;&#x305F;</pr></trd>,
    <trd>&#30064;&#x306A;&#12387;&#x305F;  <pr>&#12371;&#12392;&#x306A;&#12387;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>r&oacute;&zdot;ny</trd>,
    <trd>odmienny</trd>,
    <trd>inny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
