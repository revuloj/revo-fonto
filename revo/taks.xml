<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>taks</rad>/i
    </kap>

    <drv mrk="taks.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="taks.0i.KOMUNE">
        <dif>
          Difini la valoron de iu a&ubreve; io:
          <ekz>
            kiel oni vin <tld/>as, tiel oni vin regalas
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <tld/>i iun ne la&ubreve; lia sukceso, sed la&ubreve; lia
            merito<fnt>Z</fnt>;
          </ekz><ekz>
            ne <tld/>u vian kontra&ubreve;ulon tro malforta<fnt>Z</fnt>;
          </ekz><ekz>
            li <tld/>is, kiel grandajn servojn la pastro povas fari al
            li<fnt>B</fnt>;
          </ekz><ekz>
            belecon <tld/>as ne okulo, sed koro
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            malpla&ccirc;as nenio, se <tld/>as pasio
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            mi estas certa, ke vi alte <tld/>os la gravecon de tiu
            fakto<fnt>Z</fnt>;
          </ekz><ekz>
            oni <tld/>as je (&gcirc;is) cent la nombron de la pereintoj;
          </ekz><ekz>
            mi <tld/>as tiujn kondi&ccirc;ojn neakcepteblaj;
          </ekz><ekz>
            ni <tld/>as la tempon ne ta&ubreve;ga por tiu propono;
          </ekz><ekz>
            li faris ne<tld/>eblajn servojn;
          </ekz><ekz>
            ne<tld/>ebla kolekto da bela&jcirc;oj;
          </ekz><ekz>
            oni devas nek sub<tld/>i (tro malalte <tld/>i) nek tro<tld/>i la
            rolon de l' sentoj.
          </ekz>
        </dif>
        <ref tip="vid" cel="mezur.0i">mezuri</ref>
        <trdgrp lng="be">
          <trd>&c_a;&c_c;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>, 
          <trd>&c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol; 
            <ind>&c_c;&c_a;&c_n;&c_u;</ind></trd>
        </trdgrp>  
        <trd lng="hu">&eacute;rt&eacute;kel</trd>
        <trdgrp lng="sv">
          <trd>taxera</trd>,
          <trd>uppskatta</trd>,
          <trd>v&auml;rdera</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>absch&auml;tzen</trd>,
          <trd>bewerten</trd>,
          <trd>taxieren</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_c;&c_je;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_o;&c_c;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;
            <ind>&c_o;&c_c;&c_je;&c_n;&c_k;&c_u;</ind></trd>
        </trdgrp>
      </snc>

      <snc mrk="taks.0i.ARK">
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="aprez.0i">aprezi</ref>
        <ekz>
          <tld/>i la sanon ni lernas en malsano
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
        <trdgrp lng="be">
          <trd>&c_c;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
          <trd>&c_p;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_sh;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>hochsch&auml;tzen</trd>,
          <trd>hoch einsch&auml;tzen</trd>,
          <trd><klr>zu </klr>sch&auml;tzen <ind>wissen</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>&eacute;rt&eacute;kel</trd>,
          <trd>nagyra tart</trd>
        </trdgrp>
        <trd lng="ru">&c_c;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
        <trd lng="sv">uppskatta</trd>
      </snc>

      <refgrp tip="vid">
        <ref cel="estim.0i">estimi</ref>,
        <ref cel="konsid.0i">konsideri</ref>,
        <ref cel="rigard.0i">rigardi</ref>,
        <ref cel="jugx.0i.taksi">ju&gcirc;i<sncref/></ref>,
        <ref cel="konjek.0i">konjekti</ref>.
      </refgrp>

      <trd lng="de">sch&auml;tzen</trd>
      <trd lng="en">estimate</trd>
      <trd lng="fr">estimer</trd>
      <trdgrp lng="nl">
        <trd>taxeren</trd>,
        <trd>schatten</trd>,
        <trd>waarderen</trd>
      </trdgrp>
      <trd lng="vo">stim&ouml;n</trd>
    </drv>
  </art>
