<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tigr</rad>/o
</kap>

<drv mrk="tigr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="panter.0o.genro">pantero</ref>
      (<trd lng="la">Panther tigris</trd>), kun la&ubreve;lar&gcirc;e
      nigre striata felo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_g;&c_r;</trd>
  <trd lng="bg">&c_t;&c_i;&c_g;&c_malmol;&c_r;</trd>
  <trd lng="ca">tigre</trd>
  <trd lng="en">tiger</trd>
  <trd lng="de">Tiger</trd>
  <trd lng="fr">tigre</trd>
  <trd lng="hu">tigris</trd>
  <trd lng="nl">tijger</trd>
  <trd lng="pl">tygrys</trd>
  <trdgrp lng="pt">
    <trd>tigre</trd>
  </trdgrp>
  <trd lng="sv">tiger</trd>
  <trd lng="ru">&c_t;&c_i;&c_g;&c_r;</trd>
  <trd lng="tr">kaplan</trd>
  <trd lng="sk">tiger</trd>
</drv>
<drv mrk="tigr.0ino">
  <kap>
  <tld/>ino
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Inseksa <ref tip="super" cel="tigr.0o">tigro</ref>.
    </dif>
    <trd lng="ca">tigressa</trd>
    <trd lng="pl">tygrysica</trd>
  </snc>
</drv>
</art>
