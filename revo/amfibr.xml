<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>amfibra&hcirc;</rad>/o
</kap>
<drv mrk="amfibr.0o">
  <kap><tld/>o</kap>
  <snc mrk="amfibr.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref>, konsistanta
      el unu longa a&ubreve; akcenta silabo inter du mallongaj a&ubreve; senakcentaj;
      simbole &ypsilon;&dash;&ypsilon;:
      <ekz>
        <tld/>a verso;
      </ekz>
      <ekz>
        &leftquot;doloro&rightquot; konsistigas <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_f;&c_ib;&c_b;&c_r;&c_a;&c_h;&c_ib;&c_j;</trd>
  <trd lng="en">amphibrach</trd>
  <trd lng="fr">amphibraque</trd>
  <trd lng="hu">amphibrachisz</trd>
  <trd lng="nl">amfibrachys</trd>
  <trd lng="ru">&c_a;&c_m;&c_f;&c_i;&c_b;&c_r;&c_a;&c_h;&c_i;&c_j;</trd>
</drv>

<drv mrk="amfibr.n0o">
  <kap>n-<tld/>o</kap>
  <snc mrk="amfibr.n0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe konsistanta el
      n <tld/>oj; simbole /&ypsilon;&dash;&ypsilon;/<sup>n</sup>:
      <ekz>
        <ind>tri<tld/>o</ind>.
        <trd lng="fr">trim&egrave;tre amphibrachique</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ru"><klr tip="amb">n-</klr>&c_s;&c_t;&c_o;&c_p;&c_n;&c_y;&c_j; &c_a;&c_m;&c_f;&c_i;&c_b;&c_r;&c_a;&c_h;&c_i;&c_j;</trd>
</drv>
</art>
