<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amfibr.xml,v 1.27 2024/01/22 21:20:22 revo Exp $">
<kap>
  <rad>amfibra&hcirc;</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="k">amfibrak</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>

<drv mrk="amfibr.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="amfibr.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref>,
      konsistanta
      el unu longa a&ubreve; akcenta silabo inter du mallongaj a&ubreve;
      respektive senakcentaj; simbole &ypsilon;&#8208;&ypsilon;:
      <ekz>
        <tld/>a verso;
      </ekz><ekz>
        la vorto <ctl>doloro</ctl> konsistigas <tld/>on;
      </ekz><ekz>
        s-ro <nom>Brendon Clark</nom> <klr>[&#8230;]</klr> rekomendis la tro&hcirc;eon,
        kaj d-ro <nom>Cecil Bean</nom> <klr>[&#8230;]</klr> la <tld/>on, kiel
        la plej <ctl>naturan</ctl> kaj sekve plej vaste uzindan
        metrikon en Esperanto
        <fnt>
          <aut>William AULD</aut>:
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr24/ritmou.html">Unu ritmo
          en &leftquot;La infana raso&rightquot;</url></vrk>.
          <lok>La nica literatura revuo, 2:4</lok>
        </fnt>;
      </ekz><ekz>
        kurioze estas, kiel niaj <tld/>oj aliformi&gcirc;as angle en
        anapestojn, donante tiel al la poemo multe pli viglan
        movon
        <fnt>
          <aut>G. Waringhien</aut>:
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr61/zamenhof.html">Recenzo:
          &leftquot;Zamenhof Creator of Esperanto&rightquot; de Marjorie
              Boulton</url></vrk>.
          <lok>La nica literatura revuo, 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu verso estas dekunu-silaba <klr>[&#8230;]</klr> kvar jamboj
        <klr>plus</klr> unu <tld var="k"/>o
        <fnt>
          <aut>M. M. Campagna</aut>,
          <vrk>Esperantigo mia...</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          1996-08-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">amf&iacute;brac</trd>
  <trd lng="be">&c_a;&c_m;&c_f;&c_ib;&c_b;&c_r;&c_a;&c_h;&c_ib;&c_j;</trd>
  <trd lng="de">Amphibrachys</trd>
  <trd lng="en">amphibrach</trd>
  <trd lng="fr">amphibraque</trd>
  <trd lng="he">&alef;&#1463;&mem;&pe;&#1460;&yod;&bet;&#1468;&#1460;&yod;</trd>
  <trd lng="hu">amphibrachisz</trd>
  <trdgrp lng="ja">
    <trd>&#25233;&#25562;&#25233;&#26684;  <pr>&#12424;&#12367;&#12424;&#12358;&#12424;&#12367;&#12363;&#12367;</pr></trd>,
    <trd>&#30701;&#38263;&#30701;&#26684;  <pr>&#12383;&#12435;&#12385;&#12423;&#12358;&#12383;&#12435;&#12363;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">amfibrachys</trd>
  <trd lng="pl">amfibrach</trd>
  <trd lng="ro">amfibrah</trd>
  <trd lng="ru">&c_a;&c_m;&c_f;&c_i;&c_b;&c_r;&c_a;&c_h;&c_i;&c_j;</trd>
  <trd lng="uk">&c_a;&c_m;&c_f;&c_ib;&c_b;&c_r;&c_a;&c_h;&c_ib;&c_j;</trd>
</drv>

<drv mrk="amfibr.n0o">
  <kap>n-<tld/>o</kap>
  <snc mrk="amfibr.n0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe
      konsistanta
      el <frm>n</frm> <tld/>oj; simbole <frm>/&ypsilon;&#8208;&ypsilon;/<sup>n</sup></frm>:
      <ekz>
        <ind>tri<tld/>o</ind>.
        <trd lng="fr">trim&egrave;tre amphibrachique</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ru"><klr tip="amb">n-</klr>&c_s;&c_t;&c_o;&c_p;&c_n;&c_y;&c_j; <ind>&c_a;&c_m;&c_f;&c_i;&c_b;&c_r;&c_a;&c_h;&c_i;&c_j;</ind></trd>
  <trd lng="pl"><klr tip="amb">n-ty</klr> <ind>amfibrach</ind></trd>
</drv>
</art>
