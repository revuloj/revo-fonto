<art mrk="$Id: ataksi.xml,v 1.17 2022/04/25 15:11:00 revo Exp $">
<kap>
  <rad>ataksi</rad>/o
</kap>
<drv mrk="ataksi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="patolo.0o.procezo">Patologia</ref>
      senordeco de korpaj movoj:
      <ekz>
        multaj lo&gcirc;antoj de Minamata kaj najbaraj urboj, kiuj man&gcirc;is tiujn fi&scirc;ojn,
        malsani&gcirc;is, severe suferante de paralizo de nervoj, tremoj de membroj,
        <tld/>o, parola malfacilo
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/011021.php">Minamata, alia tragedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_a;&c_k;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="cs">ataxie</trd>
  <trd lng="de">Ataxie</trd>
  <trd lng="en">ataxia</trd>
  <trd lng="fa">&a_A_madda;&a_t;&a_A;&f_k;&a_s;&f_y;</trd>
  <trd lng="fr">ataxie</trd>
  <trd lng="hu">ataxia</trd>
  <trd lng="it">atassia</trd>
  <trd lng="ru">&c_a;&c_t;&c_a;&c_k;&c_s;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ataxia</trd>,
    <trd>poru&scaron;enie koordinovanosti telesn&yacute;ch pohybov</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x442;&#x430;&#x43A;&#x441;&#x456;&#x44F;</trd>
</drv>
</art>
