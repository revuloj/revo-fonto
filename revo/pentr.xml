<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pentr</rad>/i
</kap>
<drv mrk="pentr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pentr.0i.reprezenti">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Prezenti ion per strekoj kaj koloroj:
      <ekz>
        <tld/>i viron, arbon, pejza&gcirc;on per oleo, per akvarelo;
      </ekz>
      <ekz>
        li <tld/>is la diinon nuda;
      </ekz>
      <ekz>ne <tld/>u diablon sur la muro
        <klr>(se oni aludas pri dan&gcirc;ero, &gcirc;i okazas.
        <ref tip="vid"
cel="lup.0o">lupo</ref>)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
      <uzo tip="stl">FIG</uzo>
        la suno <tld/>is brilan strion sur la vasta
        akvaro;
      </ekz>
      <ekz>
        la luno <tld/>is sur la &gcirc;ardenoj ombrojn de l'
        piramidoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        sur lia viza&gcirc;o estis <tld/>ita la kutimo diri
        la veron<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="desegn.0i">desegni</ref>,
      <ref cel="skiz.0i">skizi</ref>.
    </refgrp>
    <trd lng="de">malen</trd>
    <trd lng="hu">fest</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr
tip="amb">(&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;&c_m;&c_i;)</klr></trd>,
      <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;
&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_mol;&c_ju;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pentr.0i.priskribi">
  <uzo tip="fak">BELE</uzo>
  <uzo tip="stl">FIG</uzo>
    <dif>
      Priskribi karakterojn a&ubreve; scenojn per vortoj:
      <ekz>
        li majstre <tld/>is la &gcirc;ojojn kaj mal&gcirc;ojojn de la
humiluloj;
      </ekz>
      <ekz>
        <tld/>i la homojn tiaj, kiaj ili devus esti.
      </ekz>
    </dif>
    <trd lng="hu">lefest<klr tip="amb"> (&aacute;tv.)</klr></trd>
    <trd
lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="pentr.0i.imagi">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Imagi drastan kaj vivan bildon de io:
      <ekz>
        ofte li <tld/>adis al si tiun &ccirc;i tempon sub &ccirc;iaj
        cirkonstancoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>fest</trd>,
      <trd>ecsetel</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_i;&c_v;&c_o; &c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">paint</trd>
  <trdgrp lng="fr">
    <trd>peindre</trd>
  </trdgrp>
  <trd lng="nl">schilderen</trd>
  <trd lng="sv">m&aring;la</trd>
</drv>

<drv mrk="pentr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <tld lit="P"/>overko.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_ju;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_c;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Gem&auml;lde</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr> (&oelig;uvre picturale)</klr></trd>
  </trdgrp>
  <trd lng="hu">festm&eacute;ny</trd>
  <trd lng="nl">schilderij</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">m&aring;lning</trd>
</drv>

<drv mrk="pentr.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <ref tip="dif" cel="pitore.0a">pitoreska</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;</trd>
  <trd lng="hu">fest&odblac;i</trd>
  <trd lng="nl">schilderachtig</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
