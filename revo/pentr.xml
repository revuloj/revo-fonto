    <trd>&#32117;&#12395;&#12394;&#12427;  <pr>&#12360;&#12395;&#12394;&#12427;</pr></trd>,
    <trd>&#32118;&#26223;&#12398;  <pr>&#12380;&#12387;&#12369;&#12356;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">schilderachtig</trd>
  <trdgrp lng="pl">
