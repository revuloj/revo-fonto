<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>lap</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lap.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lap.0o.planto">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Arctium</trd>).
    </dif>
    <trd lng="hu">bojtorj&aacute;n<klr tip="amb">
      (n&ouml;v&eacute;ny)</klr></trd>
    <trd lng="nl">kleefkruid</trd>
    <trdgrp lng="pt">
      <trd>bardana</trd>,
      <trd>bardana-maior</trd>,    
      <trd>erva-dos-pergamassos</trd>,    
      <trd>erva-dos-tinhosos</trd>,
      <trd>pergamassa</trd>,
      <trd>pergamasso</trd>,    
      <trd>pergamasso-maior</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_o;&c_p;&c_u;&c_h;</trd>
  </snc>
  <snc mrk="lap.0o.fruktaro">
    <dif>
      La alkro&ccirc;i&gcirc;ema fruktaro de la planto.
    </dif>
    <trd lng="hu">bojtorj&aacute;n<klr tip="amb">
      (term&eacute;s)</klr></trd>
    <trd lng="nl">klit</trd>
    <trd lng="ru">&c_r;&c_je;&c_p;&c_je;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_o;&c_p;&c_u;&c_h;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>llapassa</trd>,
    <trd>rapalassa</trd>
  </trdgrp>
  <trd lng="de">Klette</trd>
  <trd lng="en">burdock</trd>
  <trd lng="sv">kardborre</trd>
</drv>
</art>
