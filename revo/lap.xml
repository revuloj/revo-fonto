<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lap.xml,v 1.24 2022/05/01 12:04:34 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>lap</rad>/o
</kap>
<drv mrk="lap.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lap.0o.planto">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro<sncref/></ref>
      el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Arctium</trd>), kun alkro&ccirc;i&gcirc;emaj globaj fruktoj:
      <ekz>
        sub granda folio de <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno varmege lumis sur &ccirc;iuj verdaj <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam <tld/>o kreskas sola, ne, kie unu kreskas, tie kreskas
        anka&ubreve; aliaj, 
        &gcirc;i estas io belega, kaj la tuta belega&jcirc;o estas manga&jcirc;o por
        limakoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Feli&ccirc;a familio</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/ca/ArctiumLappa1.jpg/680px-ArctiumLappa1.jpg" prm="CC BY-SA 3.0"><tld/>o (Arctium lappa), fotita en Malsupra Saksio, Germanujo
        <fnt>
          <aut>Christian Fischer, propra verko</aut>
          <lok><url ref="&WCW;/File:ArctiumLappa1.jpg">&WCW;/File:ArctiumLappa1.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="en">burdock</trd>
    <trd lng="fr">bardane</trd>
    <trd lng="hu">bojtorj&aacute;n<klr tip="amb">
      (n&ouml;v&eacute;ny)</klr></trd>
    <trd lng="pl">&lstroke;opian</trd>
    <trdgrp lng="pt">
      <trd>bardana</trd>,
      <trd>bardana-maior</trd>,    
      <trd>erva-dos-pergamassos</trd>,    
      <trd>erva-dos-tinhosos</trd>,
      <trd>pergamassa</trd>,
      <trd>pergamasso</trd>,    
      <trd>pergamasso-maior</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_o;&c_p;&c_u;&c_h;</trd>
  </snc>
  <snc mrk="lap.0o.frukto">
    <dif>
      La alkro&ccirc;i&gcirc;ema frukto de tiu planto:
      <ekz>
        mi jam alkro&ccirc;i&gcirc;is al mia kandidato kiel <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>akto dua, sceno tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        li komencis kuri kaj provis liberi&gcirc;i de la teda griza simio, sed
        tiu sidis firme kiel <tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">burdock <ind>bur</ind></trd>
    <trd lng="hu">bojtorj&aacute;n<klr tip="amb">
      (term&eacute;s)</klr></trd>
    <trd lng="pl">rzep</trd>
    <trd lng="ru">&c_r;&c_je;&c_p;&c_je;&c_j;</trd>
  </snc>
  <snc mrk="lap.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <fnt><bib>HejmVort</bib></fnt>
    <ref tip="dif" cel="ferm.lap0ilo">lapfermilo </ref>
    <trd lng="de">Klettverschluss</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_o;&c_p;&c_u;&c_h;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>llapassa</trd>,
    <trd>rapalassa</trd>
  </trdgrp>
  <trd lng="de">Klette</trd>
  <trd lng="nl">klit</trd>
  <trd lng="sv">kardborre</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&#x43F;&#x443;&#x445;</trd>,
    <trd>&#x440;&#x435;&#x43F;&#x2019;&#x44F;&#x445;</trd>
  </trdgrp>
</drv>
</art>
