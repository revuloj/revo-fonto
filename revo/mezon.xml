<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mezon</rad>/o
</kap>
<drv mrk="mezon.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Klaso de nestabilaj <ref tip="vid" cel="partik2.0o">partikloj</ref>
      kun nula a&ubreve; entjera spino, apartenantaj al klaso de
      <ref tip="super" cel="hadron.0o">hadronoj</ref> kaj ne havantaj
      barjonan &scirc;argon.
    </dif>
    <ref tip="prt" cel="kvark.0o">kvarko</ref>
  </snc>
  <trd lng="de">Meson</trd>
  <trdgrp lng="en">
    <trd>meson</trd>,
    <trd>mesotron</trd>
  </trdgrp>
  <trd lng="nl">meson</trd>
  <trd lng="ru">&c_m;&c_je;&c_z;&c_o;&c_n;</trd>
</drv>
</art>
