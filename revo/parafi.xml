<vortaro>
<art mrk="$Id: parafi.xml,v 1.20 2023/09/03 12:45:04 revo Exp $">
<kap><ofc>1</ofc> <rad>parafin</rad>/o</kap>
<drv mrk="parafi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Vaksosimila blanka kristaleca 
      <ref tip="super" cel="substa.0o.FIZ">substanco</ref>, 
      ekstraktata el mineralaj
      oleoj kaj uzata por lumigado, medicino kc.:
      <ekz>
        <nom>Karl von Reichenbach</nom> (1788-1869) eltrovinto de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=192942">Reichenbach</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la kandel' el <tld/>o, flamekhel' ... kaj para fino
        <fnt>
          <aut>Sabine Trenner (Trine, kantata de Kajto)</aut>
          <vrk><url ref="https://www.youtube.com/watch?v=j6yWRXPeGDw">Para-fino</url></vrk>
          <lok>Youtube</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="kandel.0o">kandelo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_f;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_a;&c_f;&c_i;&c_n;</trd>
  <trd lng="ca">parafina</trd>
  <trd lng="cs">parafin</trd>
  <trd lng="de">Paraffin</trd>
  <trd lng="en">paraffin</trd>
  <trd lng="es">parafina</trd>
  <trd lng="fr">paraffine</trd>
  <trd lng="hu">paraffin</trd>
  <trd lng="ja">&#x30D1;&#x30E9;&#x30D5;&#x30A3;&#x30F3;</trd>
  <trd lng="nl">paraffine</trd>
  <trd lng="pl">parafina</trd>
  <trd lng="pt">parafina</trd>
  <trd lng="ro">parafin&abreve;</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_f;&c_i;&c_n;</trd>
  <trd lng="sk">paraf&iacute;n</trd>
  <trd lng="uk">&#x43F;&#x430;&#x440;&#x430;&#x444;&#x456;&#x43D;</trd>
</drv>
</art>
