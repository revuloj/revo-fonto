<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jodl.xml,v 1.5 2023/12/03 07:09:18 afido Exp $">
<kap>
  <rad>jodl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="jodl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="jodl.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kant.0i.homo">Kanti</ref> per rapida alterno inter 
      brusta vo&ccirc;o kaj <ref tip="prt" cel="falset.0o">falseto</ref>, 
      la&ubreve; maniero en E&ubreve;ropo tradicii&gcirc;inta en la Alpoj post 1800,
      en Afriko trovebla &ccirc;e pigmeoj:
      <ekz>
        la Alpa korno sonis profunde melankolie, kaj la pa&scirc;tisto gaje <tld/>is en la valo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ilinca</nom> estas rumania kantisto kaj <tld/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=585792">Ilinca B&abreve;cil&abreve;</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jubil.0i">jubili</ref>,
      <ref cel="ulul.0i">ululi</ref>
    </refgrp>
    <trd lng="de">jodeln</trd>
    <trd lng="en">yodel</trd>
    <trd lng="fr">iodler</trd>
    <trd lng="zh">&#x5CB3;&#x5F97;&#x5C14;&#x6B4C; <pr>yu&#xE8;d&#xE9;&#x11B;rg&#x113;</pr></trd>
</snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol; <ind>&c_jo;&c_d;&c_l;&c_je;&c_m;</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_ib;&c_v;&c_a;&c_t;&c_i; &c_z; &#x43F;&c_je;&c_r;&c_je;&#x43B;&c_i;&c_v;&c_a;&#x43C;&c_i;
    (&#x43F;&#x43E;-&c_t;&c_ib;&c_r;&#x43E;&#x43B;&#x44C;&c_s;&#x43A;&c_i;</trd>,
    <trd>&#x43D;&c_a; &c_t;&c_ib;&c_r;&#x43E;&#x43B;&#x44C;&c_s;&#x43A;&c_i;&c_j; &#x43B;&c_a;&c_d;)</trd>
  </trdgrp>
</drv>
</art>

