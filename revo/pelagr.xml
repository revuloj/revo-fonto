<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pelagr.xml,v 1.12 2024/02/17 06:43:05 revo Exp $">
<kap>
  <rad>pelagr</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="pelagr.0o">
  <kap><tld/>o</kap>
  <snc mrk="pelagr.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ha&ubreve;t<ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>, kiun
      ka&ubreve;zas PP-vitaminmanko:
      <ekz>
        en Ta&scirc;kento li ekmalsanis, komence je di<klr>s</klr>enterio,
        poste venis <tld/>o
        <fnt>
          <aut>B. Tornado</aut>,
          <vrk><url ref="http://pri-historio.boom.ru/xoxlov.htm">
          Nikolao &Hcirc;o&hcirc;lov</url></vrk>,
          <lok>Progreso, 1990, p. 143a-150a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li postvivis <tld/>on en Persio
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_l;&c_ja;&c_g;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>drsn&aacute; k&uring;&zcaron;e</trd>,
    <trd>pelagra</trd>
  </trdgrp>
  <trd lng="de">Pellagra</trd>
  <trd lng="en">pellagra</trd>
  <trd lng="fr">pellagre</trd>
  <trd lng="id">pelagra</trd>
  <trd lng="it">pellagra</trd>
  <trd lng="nl">pellagra</trd>
  <trdgrp lng="pl">
    <trd>pelagra</trd>,
    <trd>rumie&nacute;</trd>
  </trdgrp>
  <trd lng="ro">pelagr&abreve;</trd>
  <trd lng="sk">pelagra</trd>
  <trd lng="uk">&c_p;&c_je;&c_l;&c_a;&c_g;&c_r;&c_a; (&c_a;&c_v;&c_ib;&c_t;&c_a;&c_m;&c_ib;&c_n;&c_o;&c_z;)</trd>
  <trd lng="zh">&#x7CD9;&#x76AE;&#x75C5; <pr>c&#x101;op&#xED;b&#xEC;ng</pr></trd>
</drv>
</art>

