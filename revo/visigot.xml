<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>visigot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="visigot.0o">
  <kap><tld/>o</kap>
  <snc mrk="visigot.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de tiu parto de la <ref tip="malprt" cel="got.0o.ETN">gota</ref>
      popolo, kiu post diversaj lokoj de sudokcidenta E&ubreve;ropo
      regis Hispanion de la 5a &gcirc;is la komenco de la 8a jarcento:
      <ekz>
        li <klr>...</klr> venkobatis Alarikon la 2an, re&gcirc;on de la <tld/>oj
        <fnt><bib>Viki</bib>, <lok>&leftquot;historio de Francio&rightquot;,
        2005-09-28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Visigoth<klr tip="amb"> (subst.)</klr></trd>,
    <trd>Wisigoth<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="visigot.0a">
  <kap><tld/>a</kap>
  <snc mrk="visigot.0a.ETN">
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>visigoth<klr tip="amb"> (adj.)</klr></trd>,
    <trd>wisigoth<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>