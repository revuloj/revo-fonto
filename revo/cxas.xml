<drv mrk="cxas.post0i">
  <kap>post<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxas.post0i.CXAS">
    <dif>
      Sekvi la spurojn de <tld/>ata besto:
      <ekz>
        miaj post<tld/>antoj perdis mian spuron
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la&ubreve; informoj, kiujn mi post<tld/>is, <klr>[la
        TEJO-estraro transdonis la aferon]</klr> al la estraro de UEA
        <fnt>
          <aut>Leon R.</aut>
          <vrk>Subita haltigo de seminario</vrk>
          <lok><url ref="http://www.forumo.hpg.ig.com.br/119/evelyn.htm">Forumo 119
          (2001:4)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiele <nom>Mnemosina</nom> &ccirc;e soklo alabastra, en son&gcirc;o
        vi post<tld/>as al ia rev' fantasta
        <fnt>
          <aut>T. Gautier, trad. L. Tarkony</aut>
          <vrk>La apatia</vrk>
          <lok><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr35/gautier/apatiau.html">Nica
          Literatura Revuo 3/5</url></lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
