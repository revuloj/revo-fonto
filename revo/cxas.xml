<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;as</rad>/i
</kap>
<drv mrk="cxas.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxas.0i.bestojn">
    <dif>
      Persekuti bestojn por ilin kapti a&ubreve; mortigi:
      <ekz>
        <tld/>i
        kuniklojn, perdrikojn, aprojn, fi&scirc;ojn...
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="en">hunt</trd>
    <trd lng="fr">chasser</trd>
    <trd lng="nl">jagen</trd>
    <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="cxas.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konstante fervore peni por akiri ion:
      <ekz>
        <tld/>i profiton<fnt>Z</fnt>, monon, honorojn, famon;
      </ekz>
      <ekz>
        ser&ccirc;u pacon kaj <tld/>u &gcirc;in
        <fnt><bib>MT</bib>, <lok>&Psa; 34:14</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
     
<trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_g;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>chase after</trd>,
      <trd>hunt</trd>,
      <trd>seek after</trd>,
      <trd>pursue</trd>
    </trdgrp>
    <trd lng="fr">courir (apr&egrave;s)</trd>
    <trd lng="nl">najagen</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_o;&c_h;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     
<trd>&c_p;&c_r;&c_je;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">jagen</trd>
  <trd lng="es">cazar</trd>
  <trd lng="hu">vad&aacute;szik</trd>
  <trd lng="pt">ca&ccedil;ar</trd>
  <trd lng="sk">po&lcaron;ova&tcaron;</trd>
  <trd lng="sv">jaga</trd>
  <trd lng="tr">avlamak</trd>
</drv>

<drv mrk="cxas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Persekutado de bestoj:
      <ekz>
        la filo de la re&gcirc;o, kiu revenis de <tld/>o, &scirc;in
renkontis
        <fnt><bib>F</bib>, <lok>&FE; 21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">chasse<klr> (activit&eacute;)</klr></trd>
</drv>

<drv mrk="cxas.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>e persekutata besto, bestospeco.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_ch;&c_y;&c_n;&c_a; <klr
tip="amb">(&c_n;&c_a; &c_ja;&c_k;&c_u;&c_ju;
&c_p;&c_a;&c_l;&c_ju;&c_ju;&c_c;&c_mol;)</klr></trd>
  <trd lng="fr">gibier</trd>
</drv>

<drv mrk="cxas.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu profesie a&ubreve; sporte <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;</trd>
  <trd lng="de">J&auml;ger</trd>
  <trd lng="en">hunter</trd>
  <trd lng="es">cazador</trd>
  <trd lng="fr">chasseur</trd>
  <trd lng="hu">vad&aacute;sz</trd>
  <trd lng="nl">jager</trd>
  <trd lng="pt">ca&ccedil;ador</trd>
  <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">po&lcaron;ovn&iacute;k</trd>
  <trd lng="sv">j&auml;gare</trd>
  <trd lng="tr">avc&inodot;</trd>
</drv>

<drv mrk="cxas.post0i">
  <kap>post<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sekvi la spurojn de <tld/>ata besto:
      <ekz>
        la&ubreve; informoj, kiujn mi post<tld/>is, <klr>[la
TEJO-estraro
        transdonis la aferon]</klr> al la estraro de UEA
        <fnt>
        <aut>Leon R.</aut>
        <vrk>Subita haltigo de seminario</vrk>
        <lok><url
ref="http://www.forumo.hpg.ig.com.br/119/evelyn.htm">Forumo 119
(4/2001)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiele Mnemosina &ccirc;e soklo alabastra, en son&gcirc;o
        vi post<tld/>as al ia rev' fantasta
        <fnt>
        <aut>Th. Gautier trad. L. Tarkony</aut>
        <vrk>La apatia</vrk>
        <lok><url
ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr35/gautier/apatiau.html">Nica
Literatura Revuo 3/5</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj post<tld/>antoj perdis mian spuron
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>pister</trd>,
    <trd>poursuivre</trd>
  </trdgrp>
</drv>

<drv mrk="cxas.0hundo">
  <kap><tld/>hundo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="hund.0o.dombesto">Hundo</ref> dresita por
      <tld/>ado.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;
&c_s;&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
  <trd lng="de">Jagdhund</trd>
  <trdgrp lng="en">
    <trd>hunting dog</trd>,
    <trd>hunting hound</trd>
  </trdgrp>
  <trd lng="fr">chien de chasse</trd>
  <trd lng="hu">vad&aacute;szkutya</trd>
  <trd lng="nl">jachthond</trd>
  <trd lng="pt">c&atilde;o de ca&ccedil;a</trd>
  <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_mol;&c_ja;
&c_s;&c_o;&c_b;&c_a;&c_k;&c_a;</trd>
  <trd lng="sv">jakthund</trd>
  <trd lng="tr">av k&ouml;pe&gbreve;&inodot;</trd>
</drv>

<drv mrk="cxas.kur0o">
  <kap>kur<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o, en kiu oni persekutigas beston
      per rapide kurantaj <tld/>hundoj.
    </dif>
  </snc>
  <trd lng="fr">chasse &agrave; courre</trd>
  <trd lng="hu">hajt&oacute;vad&aacute;szat</trd>
  <trd lng="nl">jacht</trd>
  <trd lng="ru">&c_p;&c_o;&c_g;&c_o;&c_n;&c_ja;</trd>
</drv>

<drv mrk="cxas.sxtel0i">
  <kap>&scirc;tel<tld/>i</kap>
  <snc>
    <dif>
      Ka&scirc;e, senrajte <ref tip="super"
cel="cxas.0i.bestojn"><tld/>i</ref>.
    </dif>
  </snc>
  <trd
lng="be">&c_b;&c_r;&c_a;&c_k;&c_a;&c_n;&c_mol;&c_je;&c_r;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">wildern</trd>
  <trd lng="en">poach</trd>
  <trd lng="fr">braconner</trd>
  <trd lng="hu">orvvad&aacute;szik</trd>
  <trd lng="nl">stropen</trd>
  <trd
lng="ru">&c_b;&c_r;&c_a;&c_k;&c_o;&c_n;&c_mol;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">pytlia&ccaron;i&tcaron;</trd>
</drv>
</art>
