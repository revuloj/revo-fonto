<drv mrk="cxas.0o">
  <kap><ofc>*</ofc><tld/>o,
    <var><kap><tld/>ado</kap></var>
  </kap>
  <snc mrk="cxas.0o.CXAS">
    <dif>
      Persekutado de bestoj:
      <ekz>
        la filo de la re&gcirc;o, kiu revenis de <tld/>o, &scirc;in
        renkontis
        <fnt><bib>F</bib>, <lok>&FE; 21</lok></fnt>.
      </ekz>
      <ekz>
        en&scirc;ovi&gcirc;is la kato <klr>[&#x2026;]</klr> kaj komencis <tld/>adon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen oni komencis la <tld/>adon, la harpunon oni enpu&scirc;is al la rosmaro en la
        bruston
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        dormus leporo, se &gcirc;i <tld/>on ne timus
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        hundo bonrasa estas bona por <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        festeno kaj <tld/>o kaj da &scirc;uldoj amaso
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="hund.cxas0o">&ccirc;ashundo</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2006/009020.php">Bavario funebras pro Beppo Bruno</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        matene li man&gcirc;os <tld/>akiron
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 49:27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_o;&c_v;</trd>
  <trdgrp lng="ca">
    <trd>ca&ccedil;a <klr tip="amb">(acci&oacute;, activitat)</klr></trd>,
    <trd>pesca <klr tip="amb">(acci&oacute;, activitat)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jagen</trd>,
    <trd>Jagd</trd>
  </trdgrp>
  <trd lng="fr">chasse<klr> (activit&eacute;)</klr></trd>
  <trd lng="he">&tsadi;&yod;&dalet;</trd>
  <trd lng="hu">vad&aacute;szat</trd>
  <trdgrp lng="id">
    <trd><baz>buru</baz>perburuan</trd>,
    <trd><baz>buru</baz>pemburuan</trd>,
    <trd><baz>kejar</baz>pengejaran</trd>,
    <trd><baz>uber</baz>penguberan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x72E9;&#x731F;  <pr>&#x3057;&#x3085;&#x308A;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x72E9;&#x308A;  <pr>&#x304B;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="pl">polowanie</trd>
  <trd lng="ro">v&acirc;n&abreve;toare</trd>
  <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x730E; <pr>li&egrave;</pr></trd>
</drv>

<drv mrk="cxas.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o
  <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-4986">&ccirc;as'a&jcirc;'</url></lok></fnt></kap>
  <snc mrk="cxas.0ajxo.CXAS">
    <dif>
      Tio, kion oni <tld/>as a&ubreve; jam kaptis <tld/>e:
      <ekz>
        alportu al mi <tld/>a&jcirc;on kaj faru por mi bongustan man&gcirc;a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is <tld/>a&jcirc;on kaj alportis al mi, kaj mi man&gcirc;is de &ccirc;io
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        lupo sova&gcirc;a ne &gcirc;enas siajn samulojn, nek dis&scirc;iras sian <tld/>a&jcirc;on tiel kruele
        kaj pasie, kiel homo homon sub la nomo de Dio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2004/008268.html">Poemoj de Morteza Mirbaghian</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_ch;&c_y;&c_n;&c_a; <klr tip="amb">(&c_n;&c_a;
  &c_ja;&c_k;&c_u;&c_ju; &c_p;&c_a;&c_l;&c_ju;&c_ju;&c_c;&c_mol;)</klr></trd>
  <trdgrp lng="ca">
    <trd>ca&ccedil;a <klr tip="amb">(all&ograve; ca&ccedil;at)</klr></trd>,
    <trd>pesca <klr tip="amb">(all&ograve; pescat)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jagdbeute</trd>,
    <trd>Beute</trd>
  </trdgrp>
  <trd lng="fr">gibier</trd>
  <trd lng="he">&shin;&lamed;&lamed; &tsadi;&yod;&dalet;</trd>
  <trdgrp lng="hu">
    <trd>vad</trd>,
    <trd>vad&aacute;szzs&aacute;km&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>buru</baz>buruan</trd>,
    <trd><baz>buru</baz>perburuan</trd>,
    <trd>binatang buruan</trd>,
    <trd>daging buruan</trd>,
    <trd>hewan buruan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7372;&#x7269;  <pr>&#x3048;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x990C;&#x98DF;  <pr>&#x3048;&#x3058;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pl">dziczyzna</trd>
  <trd lng="ro">v&acirc;nat</trd>
  <trd lng="ru">&c_d;&c_i;&c_ch;&c_mol;<klr> (&c_z;&c_a;
  &c_k;&c_o;&c_t;&c_o;&c_r;&c_o;&c_j; &c_o;&c_h;&c_o;&c_t;&c_ja;&c_t;&c_s;&c_ja;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_d;&c_o;&c_b;&c_i;&c_ch; (&c_m;&c_i;&c_s;&c_l;&c_i;&c_v;&c_s;&c_mol;&c_k;&c_a;)</trd>
  </trdgrp>
  <trd lng="zh">&#x63A0; <pr>l&uuml;&egrave;</pr></trd>
