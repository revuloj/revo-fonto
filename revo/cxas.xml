    <trd>vedado de caza</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;serve de chasse</trd>,
    <trd>terrain de chasse</trd>
  </trdgrp>
  <trd lng="he">&alef;&zayin;&vav;&resh; &tsadi;&yod;&yod;&dalet;</trd>
  <trdgrp lng="id">
    <trd>cagar perburuan</trd>,
    <trd>suaka perburuan</trd>
  </trdgrp>
  <trd lng="ja">&#29471;&#22580;  <pr>&#12426;&#12423;&#12358;&#12400;</pr></trd>
  <trdgrp lng="pl">
    <trd>&lstroke;owisko</trd>,
