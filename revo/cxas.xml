<drv mrk="cxas.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="cxas.0ejo.CXAS">
    <dif>
      Natura areo uzata por <tld/>ado:
      <ekz>
        la domo havas vastajn arbarojn, mar&ccirc;ojn kaj <tld/>ejojn
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
      <ekz>
        la posedanto de la <tld/>ejo denuncis la pafanton pri tio, ke li en lia
        <tld/>distrikto <tld/>as ursojn
        <fnt>
          <vrk><url ref="http://esperanto.wz.cz/Legolibreto_II.pdf">Legolibreto II.</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Buntia Insularo estis <klr>[&#x2026;]</klr> populara <tld/>ejo de foko<tld/>istoj
        <fnt>
          <vrk><url ref="&Viki;/Ba%C5%ADnti-Insularo">Ba&ubreve;nti-Insularo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;
    <klr>(&c_p;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_a;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vedat <klr tip="amb">(de ca&ccedil;a)</klr></trd>,
    <trd>&agrave;rea privada de ca&ccedil;a</trd>,
    <trd>reserva de ca&ccedil;a</trd>
  </trdgrp>
  <trd lng="de">Jagdgrund</trd>
  <trdgrp lng="es">
    <trd>coto de caza</trd>,
    <trd>vedado de caza</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;serve de chasse</trd>,
    <trd>terrain de chasse</trd>
  </trdgrp>
