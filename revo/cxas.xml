<drv mrk="cxas.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxas.0i.bestojn">
    <dif>
      Persekuti bestojn por ilin kapti a&ubreve; mortigi:
      <ekz>
        <tld/>i
        kuniklojn, perdrikojn, aprojn, fi&scirc;ojn...
      </ekz>
      <ekz>
        <tld/>a&jcirc;o <tld/>anton ne atendas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as du leporojn, kaptas neniun
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>as kapta&jcirc;on por la leonino
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 38:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        novnazioj nokte persekutis du vjetnamojn, <tld/>is ilin tra la urbeto, batis kaj
        tretegis
        <fnt><aut>Stefan Maul</aut>,
        <vrk><url ref="&Monato;/2000/006929.php">Brutalaj dekstruloj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/04, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>ca&ccedil;ar</trd>,
      <trd>pescar</trd>
    </trdgrp>
    <trd lng="en">hunt</trd>
    <trd lng="fr">chasser</trd>
    <trd lng="nl">jagen</trd>
    <trd lng="pl">polowa&cacute;</trd>
    <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="cxas.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konstante fervore peni por akiri ion:
      <ekz>
        ili &ccirc;iuj amas donacojn kaj <tld/>as profiton
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        ser&ccirc;u pacon kaj <tld/>u &gcirc;in
        <fnt><bib>MT</bib>, <lok>&Psa; 34:14</lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;iuj eniris <klr>[&#8230;]</klr> pelante anta&ubreve; si gregon da esperantistoj, <tld/>itaj
        for de siaj matenman&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Helga Rapley</aut>
          <vrk><url ref="&Monato;/2004/008356.html">Blanka dramo en Bureso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hitler</nom> komencis <tld/>i kaj malliberigi kontra&ubreve;ulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Maria Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2003/007936.html">Jubileo hontinda</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_g;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>perseguir</trd>,
      <trd>perca&ccedil;ar</trd>
    </trdgrp>
    <trdgrp lng="en">
