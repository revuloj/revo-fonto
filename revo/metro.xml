<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>metro</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="metro.0o">
  <kap><tld/>o</kap>
  <snc mrk="metro.0o.FER">
    <uzo tip="fak">TRA</uzo>
    <uzo tip="fak">FER</uzo>
    <dif>
      Urba eksterstrata fervojo por amasa rapida transportado de
      pasa&gcirc;eroj.
      <tld lit="M"/>o povas esti subtera, surtera a&ubreve; supertera.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_t;&c_r;&c_o;</trd>,
    <trd>&c_m;&c_e;&c_t;&c_r;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_t;&c_e;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_je;&c_t;&c_r;&c_o;</trd>
  <trdgrp lng="ca">
    <trd>metro</trd>,
    <trd>metropolit&agrave;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>U-Bahn</trd>,
    <trd>Untergrundbahn</trd>,
    <trd>Metro</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>subway</trd>,
    <trd>underground</trd>,
    <trd>metro</trd>,
    <trd>rapid transit</trd>,
    <trd>L</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_q;&a_T;&a_A;&a_r; &a_sh;&a_h;&a_r;&f_y;</trd>,
    <trd>&a_m;&a_t;&a_r;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;tro</trd>
  <trd lng="hu">metr&oacute;</trd>
  <trd lng="nl">metro</trd>
  <trd lng="pl">metro</trd>
  <trdgrp lng="pt">
   <trd>metr&ocirc;</trd>,
   <trd>metropolitano <klr>(subst., transporte</klr></trd>,
   <trd>metro <klr tip="ind">(PT, metropolitano)</klr></trd>
  </trdgrp>
  <trd lng="ro">metrou</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_r;&c_o;</trd>
  <trd lng="sk">metro</trd>
</drv>
</art>
