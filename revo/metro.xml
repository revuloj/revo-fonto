<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>metro</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="metro.0o">
  <kap><tld/>o</kap>
  <snc mrk="metro.0o.FER">
    <uzo tip="fak">TRA</uzo>
    <uzo tip="fak">FER</uzo>
    <dif>
      Urba eksterstrata fervojo por amasa rapida transportado de
      pasa&gcirc;eroj.
      <tld lit="M"/>o povas esti subtera, surtera a&ubreve; supertera.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>U-Bahn</trd>,
    <trd>Untergrundbahn</trd>,
    <trd>Metro</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_t;&c_r;&c_o;</trd>,
    <trd>&c_m;&c_e;&c_t;&c_r;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_t;&c_e;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rapid transit</trd>,
    <trd>subway</trd>,
    <trd>metro</trd>,
    <trd>L</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;tro</trd>
  <trd lng="hu">metr&oacute;</trd>
  <trd lng="nl">metro</trd>
  <trd lng="pl">metro</trd>
  <trd lng="ro">metrou</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_r;&c_o;</trd>
</drv>
</art>
