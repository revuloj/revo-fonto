<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>anser</rad>/o
</kap>
<drv mrk="anser.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genroj el la familio
      <ref tip="super" cel="anas.0edoj">anasedoj</ref>
      (<trd lng="la">Anser</trd>, <trd lng="la">Branta</trd> k.a.):
      <ekz>celante <tld/>on li trafis aeron<fnt>Z</fnt>. </ekz>  
    </dif>
    <trd lng="cs">husa</trd>
    <trd lng="de">Gans</trd>
    <trd lng="fr">oie</trd>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Malsa&gcirc;a homo.  
    </dif>
  </snc>
</drv>
<drv mrk="anser.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Anseriformes</trd>).
    </dif>
  </snc>
  <trd lng="de">G&auml;nsev&ouml;gel</trd>
</drv>
</art>
