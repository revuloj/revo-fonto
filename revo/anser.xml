<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anser.xml,v 1.40 2022/03/29 06:34:17 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>anser</rad>/o
</kap>
<drv mrk="anser.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anser.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genroj</ref> el la familio
      <ref tip="malprt" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Anser</trd>, <trd lng="la">Branta</trd> k.a.):
      <ekz>
        celi <tld/>on, trafi aeron
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i tu&scirc;as lin, kiel akvo <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pif, paf! eksonis subite krakoj, kaj amba&ubreve; sova&gcirc;aj <tld/>oj falis 
        senvive en la kanaron, kaj la akvo fari&gcirc;is ru&gcirc;a de sango
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/e/e4/Canada_goose_flight_cropped_and_NR.jpg/640px-Canada_goose_flight_cropped_and_NR.jpg"
    prm="CC BY-SA 2.5">Kanada ansero, &ccirc;e Burnaby Lago, Brita Kolumbio
         <fnt>
           <aut>fotita de Alan D. Wilson kaj prilaborita de Diliff</aut>
           <lok><url
           ref="&WCW;/File:Canada_goose_flight_cropped_and_NR.jpg">&WCW;/File:Canada_goose_flight_cropped_and_NR.jpg</url></lok>
         </fnt>
    </bld>
    <trd lng="be">&c_g;&c_u;&c_s;&c_mol;</trd>
    <trd lng="br">gwaz<klr tip="ind"> (evn)</klr></trd>
    <trd lng="cs">husa</trd>
    <trd lng="de">Gans</trd>
    <trd lng="en">goose</trd>
    <trd lng="fr">oie</trd>
    <trd lng="he">&alef;&vav;&vav;&zayin;</trd>
    <trd lng="hu">l&uacute;d</trd>
    <trd lng="id">angsa</trd>
    <trd lng="nl">gans</trd>
    <trd lng="no">g&aring;s</trd>
    <trd lng="pl">g&eogonek;&sacute;</trd>
    <trd lng="pt">ganso</trd>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_mol;</trd>
  </snc>
  <snc mrk="anser.0o.hejmbesto">
    <uzo tip="fak">AGR</uzo>
    <dif>
       <ref tip="lst" cel="bird.korto0o" lst="voko:birdoj">Kortobirdo</ref>,
        deveninta de la specio <trd lng="la">Anser anser</trd>:
      <ekz>
        fordoni <tld/>on, por ricevi paseron
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej bonguste vaporis rostita <tld/>o, far&ccirc;ita per prunoj kaj pomoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faris la inter&scirc;an&gcirc;on: la &scirc;osea impostisto ricevis la <tld/>on, kaj la
        vila&gcirc;ano la kokinon
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/39/Domestic_Goose.jpg/454px-Domestic_Goose.jpg" prm="CC BY-SA 3.0">Korta
    ansero, Tasmanio, A&ubreve;stralio
         <fnt>
           <aut>Noodle snacks, propra verko</aut>
           <lok><url ref="&WCW;/File:Domestic_Goose.jpg">&WCW;/File:Domestic_Goose.jpg</url></lok>
         </fnt>
    </bld>
     <trd lng="be">&c_g;&c_u;&c_s;&c_mol;</trd>
    <trd lng="de">Gans</trd>
    <trd lng="en">goose</trd>
    <trd lng="he">&alef;&vav;&vav;&zayin; &alef;&pe;&vav;&resh;</trd>
    <trd lng="hu">liba</trd>
    <trd lng="id">angsa</trd>
    <trd lng="pl">g&eogonek;&sacute;</trd>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_mol;</trd>
  </snc>
  <snc mrk="anser.0o.malsagxulo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malsa&gcirc;a homo:
      <ekz>
        malsa&gcirc;a <tld/>o, diris la krimulino, la truo estas granda, mi mem povus eniri, kaj
        &scirc;i metis la kapon en la fornon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur gardu vian langon, vi impertinenta <tld/>o
        <fnt>
          <aut>Hans Weinhengst</aut>,
          <vrk>Tur-Strato 4</vrk>,
          <lok>
          <url ref="http://www.fadedpage.com/showbook.php?pid=20131216">
        Fadedpage</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_a;&c_k;</trd>
  </snc>
  <trd lng="de">Gans (dumme)</trd>
  <trd lng="es">ganso</trd>
  <trd lng="hu">liba</trd>
  <trd lng="it">oca</trd>
  <trd lng="sk">hus</trd>
  <trd lng="uk">&c_g;&c_u;&c_s;&#x43A;&c_a;</trd>
</drv>

<drv mrk="anser.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Anseriformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_s;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="de">G&auml;nsev&ouml;gel</trd>
  <trd lng="fr">ans&eacute;riformes</trd>
  <trd lng="he">&#x5D1;&#x5E6;&#x5D5;&#x5E8;&#x5EA; &#x5D0;&#x5D5;&#x5D5;&#x5D6;</trd>
  <trd lng="hu">l&uacute;dalak&uacute;ak</trd>
  <trd lng="it">anseriformi</trd>
  <trd lng="ru">&c_g;&c_u;&c_s;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_s;&c_je;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>,
    <trd>&#x43F;&#x43B;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_ch;&c_a;&c_s;&c_t;&#x43E;&c_d;&c_z;&#x44C;&#x43E;&c_b;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
