<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bazili1.xml,v 1.21 2023/08/31 08:48:50 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bazilisk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bazili1.0o">
  <kap><tld/>o</kap>
  <snc mrk="bazili1.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Fabela <ref tip="super" cel="serpen.0o">serpento</ref>, kiu mortigis per nura rigardo:
      <ekz>
        &ccirc;u vi scias, kio ku&scirc;as en tiu ovo? en &gcirc;i ku&scirc;as <tld/>o! &gcirc;ian vidon neniu &ccirc;i tie
        povas elteni
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Basilisk</trd> 
    <trd lng="en">basilisk</trd> 
    <trd lng="hu">baziliszkusz</trd>
  </snc>
  <snc mrk="bazili1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda sendan&gcirc;era <ref tip="super" cel="lacert.0o">lacerto</ref>
      vivanta precipe en Centrameriko;
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio koritofanedoj (<trd lng="la">Basiliscus</trd>).
    </dif>
    <bld lok="&WCU;/thumb/a/ad/Basiliscus_plumifrons_(Ca%C3%B1o_Negro).jpg/640px-Basiliscus_plumifrons_(Ca%C3%B1o_Negro).jpg" prm="CC BY-SA 4.0">Pluma <tld/>o (Basiliscus plumifrons) &ccirc;e Ca&ntilde;o Negro, Kostariko
        <fnt>
          <aut>&#xA9; Hans Hillewaert, propra verko</aut>
          <lok><url ref="&WCW;/File:Basiliscus_plumifrons_(Ca%C3%B1o_Negro).jpg">&WCW;/File:Basiliscus_plumifrons_(Ca&ntilde;o_Negro).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Basilisk <klr>(Gattung)</klr></trd>
    <trd lng="hu">panamai baziliszkusz</trd>
    <trd lng="en">American basilisk</trd> 
  </snc>
  <trd lng="be">&c_v;&c_a;&c_s;&c_ib;&c_l;&c_ib;&c_s;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>Bazilisa</trd>,
    <trd>bazili&scaron;ek</trd>
  </trdgrp>
  <trd lng="fr">basilic<klr tip="ind"> (myth., zool.)</klr></trd>
  <trd lng="he">&#x5D1;&#x5D6;&#x5D9;&#x5DC;&#x5D9;&#x5E1;&#x5E7;</trd>
  <trd lng="it">basilisco</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30B8;&#x30EA;&#x30B9;&#x30AF;</trd>,
    <trd>&#x30D0;&#x30B7;&#x30EA;&#x30B9;&#x30AF;</trd>,
    <trd>&#x30BB;&#x30D3;&#x30EC;&#x30C8;&#x30AB;&#x30B2;</trd>
  </trdgrp>
  <trd lng="pl">bazyliszek</trd>
  <trd lng="ru">&c_v;&c_a;&c_s;&c_i;&c_l;&c_i;&c_s;&c_k;</trd>
  <trd lng="sk">bazili&scaron;ok</trd>
  <trd lng="uk">&#x432;&#x430;&#x441;&#x438;&#x43B;&#x456;&#x441;&#x43A;</trd>
</drv>
</art>
