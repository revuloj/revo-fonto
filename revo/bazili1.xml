<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bazilisk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bazili1.0o">
  <kap><tld/>o</kap>
  <snc mrk="bazili1.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Fabela serpento, kiu mortigis per nura rigardo.
    </dif>
    <trd lng="de">Basilisk</trd> 
    <trd lng="en">basilisk</trd> 
    <trd lng="hu">baziliszkusz</trd>
  </snc>
  <snc mrk="bazili1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda sendan&gcirc;era lacerto el specoj
      vivantaj precipe en Centrameriko;
      genro el familio koritofanedoj
      (<trd lng="la">basiliscus</trd>).
    </dif>
    <trd lng="de">Helmbasilik</trd>
    <trd lng="hu">panamai baziliszkusz</trd>
    <trd lng="en">American basilisk</trd> 
  </snc>
  <trd lng="be">&c_v;&c_a;&c_s;&c_ib;&c_l;&c_ib;&c_s;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>Bazilisa</trd>,
    <trd>bazilišek</trd>
  </trdgrp>
  <trd lng="fr">basilic<klr tip="ind"> (myth., zool.)</klr></trd>
  <trd lng="it">basilisco</trd>
  <trd lng="pl">bazyliszek</trd>
  <trd lng="ru">&c_v;&c_a;&c_s;&c_i;&c_l;&c_i;&c_s;&c_k;</trd>
  <trd lng="sk">bazilišok</trd>
</drv>
</art>
