<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tars1.xml,v 1.8 2024/01/12 01:37:45 revo Exp $">
    <kap>
      <rad>Tars</rad>/o
      <fnt><bib>NT</bib></fnt>
    </kap>

<drv mrk="tars1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref>
      en <ref tip="malprt" cel="turk.T0ujo">Turkujo</ref>,
      naski&gcirc;loko de Sankta Pa&ubreve;lo (34&deg;56&#8217;E, 36&deg;55&#8217;N):
      <ekz>
        ser&ccirc;u en la domo de Judas viron, nomatan Sa&ubreve;lo, el <tld/>o, &ccirc;ar jen li pre&gcirc;as
        <fnt>
          <bib>NT</bib>
          <lok><url ref="&Steloj;biblio">La agoj 9:11</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo diris: Mi estas Judo el <tld/>o en Kilikio, bur&gcirc;o de urbo ne malnobla
        <fnt>
          <bib>NT</bib>
          <lok><url ref="&Steloj;biblio">La agoj 21:39</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        post el&scirc;ipi&gcirc;o &ccirc;e <nom>&Ccirc;esme</nom> en Turkio, sekvis trairo tra interna Anatolio,
        la&ubreve; Efezo, <tld/>o kaj Antio&hcirc;io, la urbo, kie unuafoje la sekvantoj de
        Jesuo ricevis la nomon kristanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009125.php">Pedalante al Jerusalemo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tarsus</trd>
  </snc>
  <ref tip="hom" cel="tars.0o">tarso (anatomio)</ref>
   <trdgrp lng="be">
    <trd>&c_T;&c_a;&c_r;&c_s;&c_u;&c_s;</trd>,
    <trd>&c_T;&c_a;&c_r;&c_s;</trd>
  </trdgrp>
  <trd lng="ja">&#x30BF;&#x30EB;&#x30B9;&#x30B9;</trd>
  <trd lng="pl">Tarsus</trd>
  <trdgrp lng="uk">
    <trd>&c_T;&c_a;&c_r;&c_s;&c_u;&c_s;</trd>,
    <trd>&c_T;&c_a;&c_r;&c_s;</trd>
  </trdgrp>
</drv>
  </art>

