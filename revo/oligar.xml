<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>oligar&hcirc;i</rad>/o, oligarkio
</kap>
<drv mrk="oligar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>,
      en kiu regas malmultaj homoj a&ubreve; familioj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_g;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Oligarchie</trd>,
     <trd>Gruppenherrschaft</trd>
  </trdgrp>
  <trd lng="el">&omikron;&lambda;&jota;&gamma;&alfa;&rho;&chi;&jota_ton;&alfa;</trd>
  <trdgrp lng="fr">
    <trd>oligarchie</trd>
  </trdgrp>
  <trd lng="nl">oligarchie</trd>
  <trd lng="pt">oligarquia</trd>
  <trd lng="ru">&c_o;&c_l;&c_i;&c_g;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
</drv>
</art>
