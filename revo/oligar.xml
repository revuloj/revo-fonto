</kap>
<drv mrk="oligar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>,
