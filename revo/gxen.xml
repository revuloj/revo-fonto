    <trd lng="bg">&c_b;&c_je;&c_z;&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_je;&c_n;</trd>
    <trd lng="en">relaxed</trd>
  </snc>
  <snc mrk="gxen.sen0a.senhonta">
    <dif>
      Ne sufi&ccirc;e <tld/>anta sin kontra&ubreve; aliaj; tro
      senceremonia;
      malkonvena; senhonta; sen<tld/>a&ccirc;a; tro sen<tld/>a:
      <ekz>
        sen<tld/>a konduto, vesto;
      </ekz>
      <ekz>
        sen<tld/>e paroli kontra&ubreve; iu.
