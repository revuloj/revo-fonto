<drv mrk="gxen.0ateco">
  <kap><tld/>ateco, <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="gxen.0ateco.sento">
    <dif>
      Sento de tiu, kiu sin <tld/>as:
      <ekz>
        lia <tld/>ateco faris lin iom ridinda, kaj mallerta;
      </ekz>
      <ekz>
        <tld/>eca mieno, rideto;
      </ekz>
      <ekz>
        <nom>Marta</nom> rimarkis lian <tld/>atecon, sed &scirc;i pensis, ke la ka&ubreve;zo de tio estas la
        &ccirc;eestado de flanka persono
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_jo;&c_m;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
