<drv mrk="tabi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Japandevena &gcirc;ismaleola <ref tip="super" cel="sxtrum.0o">&scirc;trumpo</ref>,
      kies du pintoj tenas aparte la dikan kaj la ceterajn
      pied-fingrojn:
      <ekz>
        oni portas <tld/>ojn, kiam oni portas getaojn a&ubreve; zoriojn kun tradicia kimono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=206178">Tabioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/93/Kusari_tabi_1.JPG/640px-Kusari_tabi_1.JPG" prm="CC BY-SA 3.0">ledaj
    <tld/>oj
        <fnt>
          <aut>Samuraiantiqueworld, propra verko</aut>
          <lok><url ref="&WCW;/File:Kusari_tabi_1.JPG">&WCW;/File:Kusari_tabi_1.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_b;&c_ib; <klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib;&c_ja; &c_sh;&c_k;&c_a;&c_r;&c_p;&c_e;&c_t;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="ca">
    <trd>tabi</trd>,
    <trd>mitjons japonesos</trd>
  </trdgrp>
  <trd lng="cs">tabi</trd>
  <trd lng="de">Tabi</trd>
  <trd lng="ja">&#x8DB3;&#x888B;  <pr>&#x305F;&#12403;</pr></trd>
  <trd lng="pl">tabi  <klr>(japo&nacute;skie skarpety)</klr></trd>
  <trdgrp lng="pt">
    <trd>tabi</trd>
  </trdgrp>
  <trd lng="sk">tabi</trd>
  <trd lng="uk">&c_t;&c_a;&c_b;&c_ib;</trd>
</drv>
