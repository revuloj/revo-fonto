<art mrk="$Id: pacifi1.xml,v 1.24 2025/04/04 18:41:26 revo Exp $">
<kap><ofc>3</ofc><rad>pacifist</rad>/o<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="pacifi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="adept.0o.FIL">Adepto</ref> de
      <ref tip="vid" cel="pacifi.0o">pacifismo</ref>:
      <ekz>
        milito, vorto mita kaj magia, glora por la patriotoj, abomena por la <tld/>oj,
        sed neadekvata en la koncernaj cirkonstancoj
       <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        malpacema <tld/>o
        <fnt>
          <aut>Josef Fliegner</aut>
            <vrk><url ref="&Monato;/2001/007291.php">Propono al malpacema pacifisto</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 4</lok>
          </fnt>;
      </ekz>
      <ekz>
        li dizertis, ricevis pro tio mortokondamnon (kiun, feli&ccirc;e, oni ne sukcesis
        plenumi), kaj dum la tuta cetero de sia vivo li estis fervora <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008886.php">Prisilentita heroo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>pacisto</ke>, <ke>pacamanto</ke>.
  </rim>

  <trd lng="be">&c_p;&c_a;&c_c;&c_y;&c_f;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">pacifista</trd>
  <trd lng="de">Pazifist</trd>
  <trd lng="en">pacifist</trd>
  <trd lng="es">pacifista</trd>
  <trd lng="hu">pacifista</trd>
  <trd lng="ja">&#24179;&#21644;&#20027;&#32681;&#32773;  <pr>&#12408;&#12356;&#12431;&#12375;&#12421;&#12366;&#12375;&#12419;</pr></trd>
  <trd lng="nl">pacifist</trd>
  <trd lng="pl">pacyfista</trd>
  <trd lng="pt">pacifista</trd>
  <trd lng="ro">pacifist</trd>
  <trd lng="ru">&c_p;&c_a;&c_c;&c_i;&c_f;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">pacifist</trd>
  <trd lng="uk">&#1087;&c_a;&c_c;&c_i;&c_f;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#21453;&#25112;&#35770;&#32773; <pr>f&#462;nzh&#224;nl&#249;nzh&#283;</pr></trd>,
    <trd>&#21453;&#25136;&#35542;&#32773; <pr>f&#462;nzh&#224;nl&#249;nzh&#283;</pr></trd>,
    <trd>&#21644;&#24179;&#20027;&#20041;&#32773; <pr>h&#233;p&#237;ngzh&#468;y&#236;zh&#283;</pr></trd>,
    <trd>&#21644;&#24179;&#20027;&#32681;&#32773; <pr>h&#233;p&#237;ngzh&#468;y&#236;zh&#283;</pr></trd>,
    <trd>&#19981;&#25269;&#25239;&#20027;&#20041;&#32773; <pr>b&#249;d&#464;k&#224;ngzh&#468;y&#236;zh&#283;</pr></trd>,
    <trd>&#19981;&#25269;&#25239;&#20027;&#32681;&#32773; <pr>b&#249;d&#464;k&#224;ngzh&#468;y&#236;zh&#283;</pr></trd>
  </trdgrp>
</drv>
