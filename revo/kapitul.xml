<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapitul.xml,v 1.11 2022/07/03 12:46:44 revo Exp $">
<kap>
  <rad>kapitul</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kapitul.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="kapitul.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref> kun
      diskforme vasti&gcirc;inta akso, sur kiu sentige sidas
      la floroj kvaza&ubreve; en korbeto:
      <ekz>
        <tld/>o de leontodo, asteracoj;
      </ekz>
      <ekz>
        la <tld/>o <klr>[de sunfloro]</klr> movi&gcirc;as dum la tago direktante sin al suno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16605"
              >Sunfloro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">cap&iacute;tol <klr>(bot.)</klr></trd>
    <trd lng="de">K&ouml;rbchen</trd>
    <trd lng="en">capitulum</trd>
    <trd lng="fr">capitule</trd>
    <trd lng="hu">f&eacute;szekvir&aacute;gzat</trd>
    <trd lng="pl">koszyczek <klr>(kwiatostan)</klr> </trd>     
    <trd lng="ro">co&#537;</trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
    <trdgrp lng="uk">
      <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_ib;&c_t;&c_u;&c_l;</trd>
    </trdgrp>
  </snc>
  <ref tip="hom" cel="kapitul1.0o">kapitulo (gvidantaro)</ref>
</drv>
</art>
