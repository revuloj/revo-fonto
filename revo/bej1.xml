<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bej1.xml,v 1.2 2021/03/26 21:55:04 revo Exp $">
<kap>
  <rad>bej</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bej1.0a">
  <kap><tld/>a</kap>
  <snc mrk="bej1.0a.KOL">
    <uzo tip="klr">(precipe pri besta felo)</uzo>
    <dif>
      de ru&gcirc;e-bruna
      har<ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>
      ofte malheli&gcirc;anta &ccirc;e la ekstremo de membroj:
      <ekz>
        la vandojn kovris la afi&scirc;oj de &scirc;ia juna&gcirc;o:
        filmaj korifeoj jam delonge forgesitaj,
        <tld/>aj poneoj kun zorge plektita kolhararo
        <fnt>
          <aut>J. Jacobsen</aut>
          <vrk><url
          ref="http://esperanto.net/literaturo/bk/verk/puncbovl.html">
          La Pun&ccirc;bovlo de la diablo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blond.0a.hararo">blonda</ref>,
      <ref cel="brun.0a">bruna</ref>,
      <ref cel="pig.0a.KOL">piga</ref>,
      <ref cel="ruan.0a.KOL">ruana</ref>,
      <ref cel="ruf.0a.KOL">rufa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Braun <klr>(Pferd)</klr></trd>,
    <trd>Fuchs <klr>(Pferd)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>alezan</trd>,
    <trd>bai</trd>
  </trdgrp>
</drv>
</art>

