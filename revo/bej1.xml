<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bej1.xml,v 1.6 2024/10/03 01:55:40 revo Exp $">
<kap>
  <rad>bej</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bej1.0a">
  <kap><tld/>a</kap>
  <snc mrk="bej1.0a.KOL">
    <uzo tip="klr">(precipe pri besta felo)</uzo>
    <dif>
      De ru&gcirc;e-bruna
      har<ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>
      ofte malheli&gcirc;anta &ccirc;e la ekstremo de membroj:
      <ekz>
        la vandojn kovris la afi&scirc;oj de &scirc;ia juna&gcirc;o:
        filmaj korifeoj jam delonge forgesitaj,
        <tld/>aj poneoj kun zorge plektita kolhararo
        <fnt>
          <aut>J. Jacobsen</aut>
          <vrk><url ref="http://esperanto.net/literaturo/bk/verk/puncbovl.html">
          La Pun&ccirc;bovlo de la diablo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blond.0a.hararo">blonda</ref>,
      <ref cel="brun.0a">bruna</ref>,
      <ref cel="pig.0a.KOL">piga</ref>,
      <ref cel="ruan.0a.KOL">ruana</ref>,
      <ref cel="ruf.0a.KOL">rufa</ref>
    </refgrp>
    <trd lng="zh">&#x7D05;&#x68D5;&#33394; <pr>h&#xF3;ngz&#x14D;ngs&#xE8;</pr></trd>
</snc>  <trd lng="as">alaz&aacute;n</trd>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_ja;&c_d;&c_y;</trd>,
    <trd>&c_g;&c_n;&c_je;&c_d;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rotbraun <klr>(Pferd)</klr></trd>,
    <trd>fuchsbraun <klr>(Pferd)</klr></trd>
  </trdgrp>
  <trd lng="el">&kappa;&alfa;&sigma;&tau;&alfa;&ny;&omikron;&kappa;&jota_ton;&tau;&rho;&jota;&ny;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>brownish-orange</trd>,
    <trd>sorrel</trd>
  </trdgrp>
  <trd lng="es">alaz&aacute;n</trd>
  <trdgrp lng="fr">
    <trd>alezan</trd>,
    <trd>bai</trd>
  </trdgrp>
  <trd lng="pl">rudy</trd>
  <trd lng="pt">baio</trd>
  <trd lng="sk">hnedo&ccaron;erven&yacute;</trd>
  <trd lng="uk">&c_g;&c_n;&c_ib;&c_d;&c_i;&c_j;</trd>
</drv>
</art>

