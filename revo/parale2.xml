<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>paralelogram</rad>/o
</kap>
<drv mrk="parale2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="later.kvar0o">Kvarlatero</ref>, kies
      <ref cel="kontraux.0a.kvarlatero">kontra&ubreve;aj</ref>
      lateroj estas duope paralelaj:
      <ekz>
        la paralelaj lateroj de <tld/>o estas anka&ubreve;
        <ref cel="egal.0a.izometria">egalaj</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_l;&c_je;&c_l;&c_a;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">Parallelogramm</trd>
  <trd lng="en">parallelogram</trd>
  <trd lng="fr">parall&eacute;logramme</trd>
  <trd lng="hu">paralelogramma</trd>
  <trd lng="nl">parallellogram</trd>
  <trd lng="pl">r&oacute;wnoleg&lstroke;obok</trd>
  <trd lng="pt">paralelogramo</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_l;&c_l;&c_je;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
</drv>
</art>
