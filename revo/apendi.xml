<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apendic</rad>/o
</kap>
<drv mrk="apendi.0o">
  <kap><tld/>o</kap>
  <snc mrk="apendi.0o.kromajxo">
    <dif>
      Akcesora parto.
    </dif>
    <trdgrp lng="de">
      <trd>Anhang</trd>,
      <trd>Appendix [en sciencaj libroj]</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="apendi.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tubforma organo, trovi&gcirc;anta sur la intesto, de la cekumo:
      <ekz>
        pro mortorisko de neoperaciebla apendicito nur homoj sen <tld/>o
        povas iri lo&gcirc;i tiel izole<fnt><aut>Wim M. A. De Smet</aut>:
        <vrk>Kiu murdis Dian Fossey?</vrk>, <bib>Monato</bib><lok>jaro
        1995a, numero 12a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Wurmfortsatz (des Blinddarms)</trd>,
      <trd>Blinddarm</trd>,
      <trd>Appendix</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_s;</trd>
    <trd lng="sv">blindtarm</trd>
  </snc>
  <trd lng="en">appendix</trd>
  <trd lng="es">ap&eacute;ndice</trd>
  <trd lng="fr">appendice</trd>
</drv>
</art>
