<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: apendi.xml,v 1.32 2023/10/28 08:33:07 revo Exp $">
    <kap>
      <rad>apendic</rad>/o
    </kap>

    <drv mrk="apendi.0o">
      <kap><tld/>o</kap>
      <snc mrk="apendi.0o.ANA">
	<uzo tip="fak">ANA</uzo>
	<dif>
	  Vermforma elkreska&jcirc;o &ccirc;e la homa <ref cel="cekum.0o">cekumo</ref>:
	  <ekz>
	    pro mortorisko de neoperaciebla
	    <ref cel="apendi1.0o">apendicito</ref> nur homoj sen <tld/>o
	    povas iri lo&gcirc;i tiel izole
	    <fnt><aut>Wim M. A. De Smet</aut>:
	      <vrk>Kiu murdis Dian Fossey?</vrk>, <bib>Monato</bib><lok>jaro
		1995a, numero 12a, p. 23a</lok></fnt>.
	  </ekz>
	</dif>
	<trd lng="be">&c_a;&c_p;&c_e;&c_n;&c_d;&c_y;&c_k;&c_s;</trd>
	<trdgrp lng="de">
	  <trd>Wurmfortsatz (des Blinddarms)</trd>,
	  <trd>Blinddarm<klr> [ev.]</klr></trd>,
	  <trd>Appendix</trd>
	</trdgrp>
        <trd lng="he">&tav;&vav;&samekh;&pe;&tav;&fnun;</trd>
        <trd lng="hu">f&eacute;regny&uacute;lv&aacute;ny</trd>
        <trd lng="pl">wyrostek robaczkowy</trd>
	<trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_s;</trd>
	<trd lng="sk">slep&eacute; &ccaron;revo</trd>
	<trd lng="sv">blindtarm</trd>
        <trdgrp lng="zh">
    <trd>&#x95CC;&#x5C3E; <pr>l&#xE1;nw&#x11B;i</pr></trd>,
    <trd>&#x9611;&#x5C3E; <pr>l&#xE1;nw&#x11B;i</pr></trd>,
    <trd>&#x76F2;&#x80A0; <pr>m&#xE1;ngch&#xE1;ng</pr></trd>,
    <trd>&#x76F2;&#x8178; <pr>m&#xE1;ngch&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
      <snc mrk="apendi.0o.kromajxo">
	<uzo tip="fak">TIP</uzo>
	<dif>
	  = <ref tip="dif" cel="don.al0o.apendico">Aldono<sncref/></ref>.
	</dif>
	<trd lng="be">&c_d;&c_a;&c_d;&c_a;&c_t;&c_a;&c_k;</trd>
	<trdgrp lng="de">
	  <trd>Anh&auml;ngsel</trd>,
	  <trd>Anhang</trd>,
	  <trd>Beigabe</trd>,
	  <trd>Appendix<klr> [en sciencaj libroj]</klr></trd>
	</trdgrp>
    <trd lng="he">&nun;&samekh;&pe;&het;</trd>
    <trdgrp lng="hu">
      <trd>f&uuml;ggel&eacute;k</trd>,
      <trd>mell&eacute;klet</trd>
    </trdgrp>
	<trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
	<trd lng="sk">dodatok</trd>
        <trdgrp lng="zh">
    <trd>&#x5269;&#x4E0B;&#x7684;&#x4E00;&#x4EBA; <pr>sh&#xE8;ngxiadey&#x12B;r&#xE9;n</pr></trd>,
    <trd>&#x526F;&#x9801; <pr>f&#xF9;y&#xE8;</pr></trd>,
    <trd>&#x526F;&#x9875; <pr>f&#xF9;y&#xE8;</pr></trd>
  </trdgrp>
</snc>
      <rim>
	La senco anatomia <tld/>o<sncref ref="apendi.0o.ANA"/> estas
	konsiderinde pli internacia ol la senco <tld/>o<sncref ref="apendi.0o.kromajxo"/>
	kiu povas aspekti kiel malagrabla kaj nelogika stranga&jcirc;o.
	<aut>Sergio Pokrovskij</aut>
      </rim>
      <trd lng="ca">ap&egrave;ndix</trd>
      <trd lng="en">appendix</trd>
      <trd lng="es">ap&eacute;ndice</trd>
      <trd lng="fr">appendice</trd>
      <trd lng="it">appendice</trd>
      <trdgrp lng="ja">
    <trd>&#x4ED8;&#x9332;  <pr>&#x3075;&#x308D;&#x304F;</pr></trd>,
    <trd>&#x866B;&#x5782;  <pr>&#x3061;&#x3085;&#x3046;&#x3059;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="tr">apandis</trd>
      <trd lng="sk">dodatok, pr&iacute;loha</trd>
      <trd lng="ms">lampiran</trd>
  <trd lng="sk">dodatok, pr&iacute;loha</trd>
  <trdgrp lng="cs">
    <trd>apendix</trd>,
    <trd>p&rcaron;&iacute;v&ecaron;sek slep&eacute;ho st&rcaron;eva</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x434;&#x430;&#x442;&#x43E;&#x43A;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x440;&#x43E;&#x441;&#x442;&#x43E;&#x43A;; &#x430;&#x43F;&#x435;&#x43D;&#x434;&#x438;&#x43A;&#x441;</trd>
  </trdgrp>
</drv>
