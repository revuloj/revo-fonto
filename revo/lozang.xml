<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>lozan&gcirc;</rad>/o
</kap>
<drv mrk="lozang.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lozang.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref cel="regul.0a.plurlatero">Regula</ref>
      <ref tip="super" cel="later.n0o">kvarlatero</ref>:
      <ekz>
        la diagonaloj de <tld/>o estas ortaj.
      </ekz>
    </dif>
    <ref tip="sin" cel="romb.0o.figuro">rombo.</ref>
  </snc>
  <snc>
    <dif>
      <tld lit="L"/>forma figuro uzata en heraldiko.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rhombus</trd>,
    <trd>Raute</trd>
  </trdgrp>
  <trd lng="es">rombo</trd>
  <trd lng="nl">ruit</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_b;</trd>
</drv>
</art>
