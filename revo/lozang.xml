<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lozang.xml,v 1.21 2016/11/27 21:10:16 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>lozan&gcirc;</rad>/o
</kap>
<drv mrk="lozang.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lozang.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      <ref tip="dif" cel="romb.0o.figuro">Rombo</ref>:
      <ekz>
        la ludpleto estas <tld/>o formita el sesangulaj &ccirc;eloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=489810'>Heksludo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[suledoj]</klr> havas <klr>[...]</klr> <tld/>o-forman voston kies 
        eksteraj plumoj estas pli mallongaj ol la centraj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=54362'>Suledoj</url></lok>
        </fnt>.
      </ekz>
    </dif>      
    <ref tip="lst" lst="voko:formoj" cel="later.kvar0o">kvarlatero</ref>
    <trd lng="ca">rombe</trd>
    <trd lng="el">&rho;&omikron_ton;&my;&beta;&omikron;&sigma_fina;</trd>
    <trd lng="en">rhombus</trd>
    <trd lng="fa">&a_l;&a_w;&a_z;&f_y;</trd>
    <trdgrp lng="fr">
      <trd>losange</trd>,
      <trd>rhombe<klr tip="ind"> (losange)</klr></trd></trdgrp>
    <trd lng="hu">rombusz</trd>
    <trd lng="it">rombo</trd>
    <trd lng="pl">romb</trd>
    <trdgrp lng="pt">
      <trd>losango</trd>,
      <trd>rombo</trd>
    </trdgrp>
  </snc>
  <snc mrk="lozang.0o.HER">
    <uzo tip="fak">HER</uzo>
    <dif>
      <tld lit="L"/>forma figuro uzata en heraldiko.
    </dif>
    <trd lng="ca">losange</trd>
    <trd lng="en">lozenge</trd>
    <trd lng="hu">ruta<klr tip="ind"> (c&iacute;merben)</klr></trd>
    <trd lng="it">losanga</trd>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_m;&c_b;</trd>
  <trdgrp lng="de">
    <trd>Raute</trd>,
    <trd>Rhombus</trd>
  </trdgrp>
  <trd lng="es">rombo</trd>
  <trd lng="nl">ruit</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_b;</trd>
</drv>
</art>
