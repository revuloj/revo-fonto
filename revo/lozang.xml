<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>lozan&gcirc;</rad>/o
</kap>
<drv mrk="lozang.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lozang.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <ref tip="dif" cel="romb.0o.figuro">rombo.</ref>
    <trdgrp lng="fr">
      <trd>losange</trd>,
      <trd>rhombe<klr tip="ind"> (losange)</klr></trd></trdgrp> 
  </snc>
  <snc mrk="lozang.0o.HER">
    <uzo tip="fak">HER</uzo>
    <dif>
      <tld lit="L"/>forma figuro uzata en heraldiko.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rhombus</trd>,
    <trd>Raute</trd>
  </trdgrp>
  <trd lng="es">rombo</trd>
  <trd lng="nl">ruit</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_b;</trd>
</drv>
</art>
