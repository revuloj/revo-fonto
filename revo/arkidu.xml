<art mrk="$Id: arkidu.xml,v 1.23 2022/04/25 15:11:00 revo Exp $">
<kap>
  <rad>arkiduk</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="&hcirc;">ar&hcirc;iduk</rad>/o <fnt><bib>Viki</bib></fnt></kap></var>
</kap>
<drv mrk="arkidu.0o">
  <kap><tld/>o,
    <var><kap><tld var="&hcirc;"/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="duk.cxef0o.familiano">&Ccirc;efduko</ref>:
      <ekz>
        kiam la a&ubreve;stra <tld/>o, samtempe imperiestro de la germana
        regno, Maksimiliano,
        ser&ccirc;is edukonton por sia nepo Karolo, li elektis Adrianon: en
        1507 li do
        fari&gcirc;is la pedagogo de la estonta imperiestro Karolo la 5a
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2009/009781.php">La sola nederlanddevena papo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Maksimiliano naski&gcirc;is &ccirc;e la palaco <nom>Sch&ouml;nbrunn</nom> de
        Vieno, filo de la <tld var="&hcirc;"/>o
        Francisko <nom>Karl</nom> el A&ubreve;strio kaj de <nom>Sofia</nom> el
        Bavario
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17247">Maksimiliano (Meksiko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_c;&c_g;&c_je;&c_r;&c_c;&c_a;&c_g;</trd>
  <trd lng="bg">&c_je;&c_r;&c_c;&c_h;&c_je;&c_r;&c_c;&c_o;&c_g;</trd>
  <trdgrp lng="cs">
    <trd>arcikn&iacute;&zcaron;e</trd>,
    <trd>arciv&eacute;voda</trd>,
    <trd>velkov&eacute;voda</trd>
  </trdgrp>
  <trd lng="de">Erzherzog</trd>
  <trd lng="en">archduke</trd>
  <trd lng="es">archiduque</trd>
  <trd lng="fr">archiduc</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5DB;&#x5D9;&#x5D3;&#x5D5;&#x5DB;&#x5E1;</trd>
  <trd lng="hu">f&odblac;herceg</trd>
  <trd lng="it">arciduca</trd>
  <trd lng="nl">aartshertog</trd>
  <trd lng="pl">arcyksi&aogonek;&zdot;&eogonek;</trd>     
  <trd lng="ro">arhiduce</trd>
  <trd lng="sk">arcivojvoda</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x440;&#x446;&#x433;&#x435;&#x440;&#x446;&#x43E;&#x491;</trd>,
    <trd>&#x430;&#x440;&#x445;&#x456;&#x43A;&#x43D;&#x44F;&#x437;&#x44C;</trd>,
    <trd>&#x432;&#x435;&#x43B;&#x438;&#x43A;&#x438;&#x439; &#x43A;&#x43D;&#x44F;&#x437;&#x44C;</trd>
  </trdgrp>
</drv>
