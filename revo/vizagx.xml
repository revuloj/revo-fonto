<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>viza&gcirc;</rad>/o
</kap>
<drv mrk="vizagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="vizagx.0o.ANA">
    <dif>
      Anta&ubreve;a parto de l' homa <ref tip="malprt"
      cel="kap.0o.ANA">kapo</ref>:
      <ekz>
        nur pano kun froma&gcirc;o, sed afabla <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bela <tld/>o estas duono da doto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ridanta, larma, senhonta <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la heroo rigardas al la morto en la <tld/>on
        (<ref tip="vid" cel="vid.0al0e">vidalvide</ref>);
      </ekz>
      <ekz>
        diru tion al mi rekte en la <tld/>o;
      </ekz>
      <ekz>
        mokadi iun &gcirc;uste en la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>a flanko de la domo estis turnita
        orienten<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o <klr>(supra&jcirc;o)</klr> de la tero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malgranda drinkejo montris sian afablan <tld/>on al
        la voja&gcirc;anto<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frunt.0o.ANA">frunto <sncref/></ref>,
      <ref cel="mien.0o">mieno</ref>
    </refgrp>
    <trd lng="be">&c_t;&c_v;&c_a;&c_r;</trd>
    <trd lng="cs">obli&ccaron;ej</trd>
    <trd lng="el">&pi;&rho;&omega_ton;&sigma;&omikron;&pi;&omikron;</trd>
    <trd lng="en">face</trd>
    <trdgrp lng="es">
      <trd>cara</trd>,
      <trd>rostro</trd>
    </trdgrp>
    <trd lng="fr">visage</trd>
    <trd lng="he">&pe;&nun;&yod;&fmem;</trd>
    <trd lng="hr">lice</trd>
    <trdgrp lng="hu">
      <trd>arc</trd>,
      <trd>arculat</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gezicht</trd>,
      <trd>aangezicht</trd>
    </trdgrp>
    <trd lng="pl">twarz</trd>
    <trdgrp lng="pt">
      <trd>rosto</trd>,
      <trd>face</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_i;&c_c;&c_o;</trd>
  </snc>
  <snc mrk="vizagx.0o.indo">
    <dif>
      Publika senripro&ccirc;eco, pro kiu oni povas nehonte <tld/>i al homoj:
      <ekz>
        Matrono Xing publike perdigis al &scirc;i la <tld/>on
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, volumo 2a, p.
        511a</lok></fnt>;
      </ekz>
      <ekz>
        se neniu scius tion &ccirc;i, ni &ccirc;iuj povus <ind>savi la <tld/>on</ind>
        de via bopatrino
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        545a</lok></fnt>;
        <trd lng="fr">sauver la <ind>face</ind></trd>
        <trd lng="hu">megmenti a <ind>becs&uuml;let&eacute;t</ind></trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol; <ind>&c_l;&c_i;&c_c;&c_o;</ind></trd>,
          <trd>&c_s;&c_p;&c_a;&c_s;&c_t;&c_i; <ind>&c_ch;&c_je;&c_s;&c_t;&c_mol;</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>konservi la <tld/>on</ind>;
        <trd lng="fr">garder la <ind>face</ind></trd>
        <trd lng="hu">meg&odblac;rzi a <ind>becs&uuml;let&eacute;t</ind></trd>
        <trd lng="ru">&c_s;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol; <ind>&c_l;&c_i;&c_c;&c_o;</ind></trd>
      </ekz>
      <ekz>
        <ind>perdi la <tld/>on</ind>.
        <trd lng="fr">perdre la <ind>face</ind></trd>
        <trd lng="hu">elveszti a <ind>becs&uuml;let&eacute;t</ind></trd>
        <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol; <ind>&c_l;&c_i;&c_c;&c_o;</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplomb.0o">aplombo</ref>,
      <ref cel="honor.0o">honoro</ref>,
      <ref cel="fid.mem0o">memfido</ref>
    </refgrp>
    <trd lng="fr">face<klr> (visage)</klr></trd>
    <trd lng="hu">becs&uuml;let</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_c;&c_o;<klr tip="ind"> (&c_ch;&c_je;&c_s;&c_t;&c_mol;)</klr></trd>,
      <trd>&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Gesicht</trd>
</drv>

<drv mrk="vizagx.0i">
  <kap><tld/>i</kap>
  <snc mrk="vizagx.0i.KOMUNE">
    <dif>
      Teni la <tld/>on, <ref tip="vid"
      cel="rigard.0i.direkte">rigardi</ref> al iu flanko:
      <ekz>
        &ccirc;ar &scirc;i <tld/>is al alia direkto, li ne povis rekoni &scirc;in
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 10a, p. 101a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>tourn&eacute;</ind> vers</trd>,
    <trd>faire <ind>face</ind> &agrave;<klr tip="amb"> (&ecirc;tre tourn&eacute;
    vers)</klr></trd>
  </trdgrp>
</drv>
</art>
