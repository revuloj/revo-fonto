<drv mrk="vizagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vizagx.0i.KOMUNE">
    <dif>
      Teni la <tld/>on, <ref tip="vid" cel="rigard.0i.direkte">rigardi</ref> al iu flanko:
      <ekz>
        &ccirc;ar &scirc;i <tld/>is al alia direkto, li ne povis rekoni
        &scirc;in
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 10a, p.
        101a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>hinwenden</ind></trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_a;&c_r;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a; <ind>&c_t;&c_v;&c_a;&c_r;&c_a;&c_m;</ind></trd>
  <trd lng="en">face</trd>
  <trd lng="fa">&a_r;&a_w; &f_k;&a_r;&a_d;&a_n;<klr tip="amb">
  (&a_b;&a_h; &a_s;&a_w;&f_y; &f_k;&a_s;&f_y; &f_y;&a_A;
  &f_ch;&f_y;&a_z;&f_y;)</klr></trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>tourn&eacute;</ind> vers</trd>,
    <trd>faire <ind>face</ind> &agrave;<klr tip="amb"> (&ecirc;tre
    tourn&eacute;
    vers)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>meng<ind>hadap</ind></trd>,
    <trd>mengarahkan <ind>wajah</ind></trd>
  </trdgrp>
  <trd lng="it">volgersi<klr> (volgere il viso verso)</klr></trd>
  <trd lng="pl">patrze&cacute; twarz&aogonek; w  <ind>twarz</ind></trd>
</drv>

<drv mrk="vizagx.du0a">
  <kap>du<tld/>a</kap>
  <snc mrk="vizagx.du0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kiu havas du <ref tip="vid" cel="vizagx.0o.MIEN">viza&gcirc;ojn<sncref/></ref>,
      du malsamajn aspektojn, respektive vidatan kaj ka&scirc;atan:
      <ekz>
        mi mem anka&ubreve; &mdash; mi ne amas homojn du<tld/>ajn; al mi tre pla&ccirc;as via senka&scirc;eco
        <fnt><bib>Revizoro</bib></fnt>
      </ekz>.
    </dif>
    <refgrp tip="vid">
      <ref cel="ambigu.0a">ambigua</ref>,
      <ref cel="facet.0o.FIG">faceto<sncref/></ref>,
      <ref cel="sincer.mal0a">malsincera</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>duplice</trd>,
      <trd>hypocrite</trd>
    </trdgrp>
    <trd lng="pl">dwulicowy</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_d;&c_v;&c_u;&c_l;&c_ib;&c_c;&c_y;</trd>,
  <trd>&c_d;&c_v;&c_u;&c_h;&c_a;&c_b;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
</trdgrp>
</drv>

<drv mrk="vizagx.0altere">
  <kap>
    <tld/>altere
  </kap>
  <snc>
    <dif>
      Kun la <tld/>o turnita malsupren; alteren:
      <ekz>
        <nom>Abraham</nom> &jcirc;etis sin <tld/>altere, kaj ekridis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 17:17</lok>
        </fnt>;
