<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vizagx.xml,v 1.116 2023/10/23 06:31:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>viza&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vizagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vizagx.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Anta&ubreve;a parto de l' homa <ref tip="malprt" cel="kap.0o.ANA">kapo</ref>:
      <ekz>
        &scirc;i kovris sian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 38:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        li lavis sian <tld/>on kaj eliris
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 43:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne aperu plu anta&ubreve; mia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 44:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &scirc;vito de via <tld/>o vi man&gcirc;ados panon, &gcirc;is vi revenos en la teron, el kiu
        vi estas prenita
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 3:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;arma milda <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun subite ru&gcirc;i&gcirc;inta <tld/>o kaj flamantaj okuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        virojn mi ser&ccirc;as, kiuj rigardas al la morto en la <tld/>on kaj lasas la dan&gcirc;eron
        ludi &ccirc;irka&ubreve; ili
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        trafi per la parolo rekte en la <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        bela <tld/>o estas duono da doto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frunt.0o.ANA">frunto<sncref/></ref>,
      <ref cel="mien.0o">mieno</ref>
    </refgrp>
    <trd lng="be">&c_t;&c_v;&c_a;&c_r;</trd>
    <trd lng="cs">obli&ccaron;ej</trd>
    <trd lng="el">&pi;&rho;&omega_ton;&sigma;&omikron;&pi;&omikron;</trd>
    <trd lng="en">face</trd>
    <trdgrp lng="es">
      <trd>cara</trd>,
      <trd>rostro</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_h;&a_r;&a_h;</trd>,
      <trd>&a_S;&a_w;&a_r;&a_t;</trd>,
      <trd>&a_r;&a_w;</trd>,
      <trd>&a_r;&a_kh;</trd>,
      <trd>&a_r;&a_kh;&a_s;&a_A;&a_r;</trd>,
    </trdgrp>
    <trd lng="fr">visage</trd>
    <trd lng="he">&pe;&nun;&yod;&fmem;</trd>
    <trd lng="hr">lice</trd>
    <trdgrp lng="hu">
      <trd>arc</trd>,
      <trd>arculat</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>muka</trd>,
      <trd>wajah</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>volto</trd>,
      <trd>viso</trd>,
      <trd>faccia</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gezicht</trd>,
      <trd>aangezicht</trd>
    </trdgrp>
    <trd lng="pl">twarz</trd>
    <trdgrp lng="pt">
      <trd>rosto</trd>,
      <trd>face</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_i;&c_c;&c_o;</trd>
  </snc>
    <snc mrk="vizagx.0o.MIEN">
    <dif>
      La <tld/>o rigardata kiel signo de sentoj, de emocioj, de karaktero;
      <ref tip="vid" cel="mien.0o.ANA">mieno<sncref/></ref>,
      <ref tip="vid" cel="fizion.0o.MIEN">fizionomio<sncref/></ref>:
      <ekz>
        nur pano kun froma&gcirc;o, sed afabla <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> kisis lin kun senhonta <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 7:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        hazarda vorto subite &scirc;an&gcirc;is lian <tld/>on
        <fnt>
          <aut>M. Proust, trad. J.-L. Tortel</aut>
          <vrk>Unu amo de Swann</vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_v;&c_a;&c_r;</trd>,
      <trd>&c_m;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vizagx.0o.indo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Publika senripro&ccirc;eco,
      <ref tip="vid" cel="honor.0o">honoro</ref>:
      <ekz>
        matrono <nom>Xing</nom> publike perdigis al &scirc;i la <tld/>on
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, volumo 2a, p.
        511a</lok></fnt>;
      </ekz>
      <ekz>
        se neniu scius tion &ccirc;i, ni &ccirc;iuj povus <ind>savi la
        <tld/>on</ind>
        de via bopatrino
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        545a</lok></fnt>.
        <trd lng="fr">sauver la <ind>face</ind></trd>
        <trd lng="hu">megmenti a <ind>becs&uuml;let&eacute;t</ind></trd>
        <trd lng="it">salvare la <ind>faccia</ind></trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;
          <ind>&c_l;&c_i;&c_c;&c_o;</ind></trd>,
          <trd>&c_s;&c_p;&c_a;&c_s;&c_t;&c_i;
          <ind>&c_ch;&c_je;&c_s;&c_t;&c_mol;</ind></trd>
        </trdgrp>
      </ekz>
<!-- mankas cita&jcirc;oj:
      <ekz>
        <ind>konservi la <tld/>on</ind>;
        <trd lng="fr">garder la <ind>face</ind></trd>
        <trd lng="hu">meg&odblac;rzi a
        <ind>becs&uuml;let&eacute;t</ind></trd>
        <trd lng="it">salvare la <ind>faccia</ind></trd>
        <trd lng="ru">&c_s;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;
        <ind>&c_l;&c_i;&c_c;&c_o;</ind></trd>
      </ekz>
      <ekz>
        <ind>perdi la <tld/>on</ind>.
        <trdgrp lng="fa">
          <trd>&a_m;&a_f;&a_t;&a_D;&a_H; &a_sh;&a_d;&a_n;</trd>,
          <trd>&a_b;&f_y;&#8204;&a_H;&f_y;&a_th;&f_y;&a_t;
          &a_sh;&a_d;&a_n;</trd>,
          <trd>&a_b;&f_y;&#8204;&a_A_madda;&a_b;&a_r;&a_w;
          &a_sh;&a_d;&a_n;</trd>
        </trdgrp>
        <trd lng="fr">perdre la <ind>face</ind></trd>
        <trd lng="hu">elveszti a <ind>becs&uuml;let&eacute;t</ind></trd>
        <trd lng="it">perdere la <ind>faccia</ind></trd>
        <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol;
        <ind>&c_l;&c_i;&c_c;&c_o;</ind></trd>
      </ekz>
-->
    </dif>
    <refgrp tip="vid">
      <ref cel="aplomb.0o">aplombo</ref>,
      <ref cel="fid.mem0o">memfido</ref>
    </refgrp>
    <trd lng="be">&c_t;&c_v;&c_a;&c_r;</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_b;&a_r;&a_w;</trd>,
      <trd>&a_H;&f_y;&a_th;&f_y;&a_t;</trd>
    </trdgrp>
    <trd lng="fr">face<klr> (visage)</klr></trd>
    <trd lng="hu">becs&uuml;let</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_c;&c_o;<klr tip="ind">
      (&c_ch;&c_je;&c_s;&c_t;&c_mol;)</klr></trd>,
      <trd>&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vizagx.0o.de_objekto">
    <uzo tip="stl">RAR</uzo>
    <dif>
      La videbla, anta&ubreve;a parto de objekto,
      &gcirc;ia <ref tip="vid" cel="front.0o.antauxo">fronto<sncref/></ref>,
      &gcirc;ia <ref tip="vid" cel="fasad.0o.ARKI">fasado<sncref/></ref>:
      <ekz>
        la <tld/>a flanko de la domo estis turnita orienten
        <fnt>
          <bib>MT</bib>
          <lok>&Jeh; 47:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o <klr>(supra&jcirc;o)</klr> de la tero
        <fnt>
          <bib>MT</bib>
          <lok>&Nom; 22:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda drinkejo <klr>[&#8230;]</klr> montris sian afablan <tld/>on al
        la voja&gcirc;anto
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_v;&c_a;&c_r;</trd>,
      <trd>&c_f;&c_a;&c_s;&c_a;&c_d;</trd>,
      <trd>&c_p;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
