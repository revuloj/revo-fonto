<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>falang</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="falang.0o">
<kap><tld/>o</kap>
  <snc mrk="falang.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la artikigitaj partoj, el kiuj konsistas <ref tip="malprt"
      cel="fingr.0o">fingro</ref>.   
    </dif>
    <trdgrp lng="de">
     <trd>Fingerglied</trd>,
     <trd>Zehenglied</trd></trdgrp>
    <trd lng="en">phalanx</trd>
    <trd lng="es">falange</trd>
    <trd lng="hu">ujjperc</trd>
    <trd lng="nl">vingerkootje</trd>
  </snc>
  <snc mrk="falang.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Anaro organizita por batalo.
    </dif>
    <refgrp tip="vid">
      <ref cel="legi.0o">legio</ref>,
      <ref cel="regime.0o">regimento</ref>
    </refgrp>
    <trd lng="de">Schlachtordnung</trd>
    <trd lng="en">phalanx</trd>
    <trd lng="hu">falanx</trd>
    <trd lng="pl">falanga</trd>
    <trd lng="pt">falange</trd>
  </snc>
  <snc mrk="falang.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Macedona batalaran&gcirc;o, konsistanta el ta&ccirc;mento da soldatoj
      armitaj per longaj pikiloj kaj ordigitaj la&ubreve; profundaj vicoj. 
    </dif>
    <trd lng="de">Phalanx</trd>
    <trd lng="en">phalanx</trd>
    <trd lng="hu">falanx</trd>
    <trd lng="nl">falanx</trd>
    <trd lng="pl">falanga</trd>
    <trd lng="pt">falange</trd>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_a;&c_n;&c_g;&c_a;</trd>
  <trd lng="ca">falange</trd>
  <trd lng="fr">phalange</trd>
  <trd lng="it">falange</trd>
  <trd lng="pt">falange</trd>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_a;&c_n;&c_g;&c_a;</trd>
</drv>
</art>

