<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sigel</rad>/i
</kap>

<drv mrk="sigel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sigel.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Signo enpremita en moligitan substancon.
    </dif>
    <subsnc mrk="sigel.0o.TIP_auxtentigi">
      <dif>
        Por certe konfirmi kaj
        a&ubreve;tentigi dokumenton a&ubreve; por sankcii kaj definitivigi
        kontrakton:
        <ekz>
          <uzo tip="stl">FIG</uzo>
          la <tld/>o ne estas ankora&ubreve; metita<fnt>Z</fnt>
          <klr>(la afero ne estas finita)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sigel.0o.TIP_fermi">
      <dif>
        Por certigi la fermitecon de
        koverto a&ubreve; por distingige indiki la a&ubreve;toron.
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="stamp.0o.presajxo">stampo<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sigel.0o.FIG_konfirmilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certigilo, konfirmilo:
      <ekz>
        tio metis la <tld/>on al lia gloro.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sigel.0o.FIG_fermilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certa fermilo:
      <ekz>
        rakonti ion sub la <tld/>o de la sekreto<fnt>B</fnt>;
      </ekz>
      <ekz>
        havi <tld/>on sur la lipoj<fnt>B</fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="sigel.0o.FIG_signo">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Distingilo:
      <ekz>
        sur &ccirc;io ku&scirc;is la <tld/>o de malri&ccirc;eco<fnt>K</fnt>;
      </ekz>
      <ekz>
        tiuj detaloj portas la <tld/>on de la vero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pe&ccaron;etn&iacute; prsten</trd>,
    <trd>sekret</trd>,
    <trd>sekr&eacute;t</trd>
  </trdgrp>
  <trd lng="de">Siegel</trd>
  <trd lng="en">seal</trd>
  <trd lng="es"><ind>sello</ind> (como el de las cartas
  medievales)</trd>
  <trdgrp lng="fr">
    <trd>scell&eacute;</trd>,
    <trd>sceau</trd>
  </trdgrp>
  <trd lng="hu">pecs&eacute;t</trd>
  <trd lng="nl">zegel</trd>
  <trd lng="pl">piecz&eogonek;&cacute;</trd>
  <trdgrp lng="pt">
    <trd>selo</trd>,
    <trd>sinete</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">pe&ccaron;atn&yacute; prste&ncaron;</trd>
</drv>

<drv mrk="sigel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sigel.0i.TIP">
    <dif>
      <ref tip="super" cel="mark.0i.KOMUNE">Marki</ref>
      per almeto a&ubreve; surmeto de <tld/>o.
    </dif>
    <trdgrp lng="en">
      <trd>to seal</trd>,
      <trd>to set one's seal to</trd>, <trd>to stamp</trd>, <trd>to
      mark</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</ind></trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol; <ind>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;&c_ju;</ind></trd>,
      <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sigel.0i.FIG_konfirmi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certe konfirmi kaj definitivigi kvaza&ubreve; per <tld/>o:
      <ekz>
        tiu kontrakto <tld/>is la interligon, la amikecon;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is lia sorto.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to seal</trd>,
      <trd>to stamp</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol; <ind>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;&c_ju;</ind></trd>
  </snc>
  <snc mrk="sigel.0i.FIG_fermi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certe fermi kvaza&ubreve; per <tld/>o:
      <ekz>
        mia bu&scirc;o estis <tld/>ita per mia promeso.
      </ekz>
    </dif>
    <trd lng="en">to seal</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>opat&rcaron;it pe&ccaron;et&iacute;</trd>,
    <trd>pe&ccaron;etit</trd>,
    <trd>zape&ccaron;etit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>versiegeln</trd>,
    <trd>besiegeln</trd>
  </trdgrp>
  <trd lng="es"><ind>sellar</ind> (sobre cera)</trd>
  <trdgrp lng="fr">
    <trd>cacheter</trd>,
    <trd>sceller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lepecs&eacute;tel</trd>,
    <trd>megpecs&eacute;tel</trd>
  </trdgrp>
  <trd lng="nl">verzegelen</trd>
  <trdgrp lng="pl">
  <trd>piecz&eogonek;towa&cacute;</trd>,
  <trd>opatrzy&cacute; <ind>piecz&eogonek;ci&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">selar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;<klr> (&c_k;&c_a;&c_k;&c_i;&c_m;-&c_l;..
    &c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k;&c_o;&c_m;)</klr></trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_je;&c_j;&c_m;&c_i;&c_t;&c_mol;<klr> (&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">zape&ccaron;ati&tcaron;</trd>
</drv>
</art>
