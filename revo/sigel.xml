<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sigel.xml,v 1.33 2024/12/10 12:54:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sigel</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="sigel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sigel.0i.TIP">
    <dif>
      <ref tip="super" cel="mark.0i.iu">Marki</ref>
      per almeto a&ubreve; surmeto de <tld/>o:
      <ekz>
        &scirc;i skribis leterojn en la nomo de <nom>A&hcirc;ab</nom> kaj <tld/>is per lia <tld/>ilo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 21:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estis skribita kaj <tld/>ita per la ringo de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ester 3:12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="plomb.0i.sigeli">plombi<sncref/></ref>
    <trdgrp lng="de">
      <trd>besiegeln</trd>,
      <trd>Siegel aufdr&uuml;cken</trd>,
      <trd>mit einem Siegel versehen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to seal</trd>,
      <trd>to set one's seal to</trd>,
      <trd>to stamp</trd>,
      <trd>to mark</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</ind></trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol; <ind>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;&c_ju;</ind></trd>,
      <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sigel.0i.FIG_konfirmi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certe konfirmi kaj <ref tip="super" cel="defini.0igi">definitivigi</ref> kvaza&ubreve; per <tld/>o:
      <ekz>
        tiu kontrakto <tld/>is la interligon, la amikecon;
      </ekz>
      <ekz>
        estas mia morto tio, kio <tld/>us ilian feli&ccirc;econ
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is lia sorto.
      </ekz>
    </dif>
    <trd lng="de">besiegeln</trd>
    <trdgrp lng="en">
      <trd>to seal</trd>,
      <trd>to stamp</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol; <ind>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;&c_ju;</ind></trd>
  </snc>
  <snc mrk="sigel.0i.FIG_fermi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certe fermi kvaza&ubreve; per <tld/>o:
      <ekz>
        fonto &scirc;losita, puto <tld/>ita
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 4:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia bu&scirc;o estis <tld/>ita per mia promeso.
      </ekz>
    </dif>
    <trd lng="de">versiegeln</trd>
    <trd lng="en">to seal</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_ja;&c_ch;&c_a;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_ch;&c_a;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_ja;&c_ch;&c_a;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>opat&rcaron;it pe&ccaron;et&iacute;</trd>,
    <trd>pe&ccaron;etit</trd>,
    <trd>zape&ccaron;etit</trd>
  </trdgrp>
  <trd lng="es"><ind>sellar</ind> (sobre cera)</trd>
  <trdgrp lng="fr">
    <trd>cacheter</trd>,
    <trd>sceller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lepecs&eacute;tel</trd>,
    <trd>megpecs&eacute;tel</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21360;&#12434;&#25276;&#12377;  <pr>&#12356;&#12435;&#12434;&#12362;&#12377;</pr></trd>,
    <trd>&#25466;&#21360;&#12377;&#12427;  <pr>&#12394;&#12388;&#12356;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#35519;&#21360;&#12377;&#12427;  <pr>&#12385;&#12423;&#12358;&#12356;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#23553;&#12434;&#12377;&#12427;  <pr>&#12405;&#12358;&#12434;&#12377;&#12427;</pr></trd>,
    <trd>&#23553;&#21360;&#12377;&#12427;  <pr>&#12405;&#12358;&#12356;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#20445;&#35388;&#12377;&#12427;  <pr>&#12411;&#12375;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#30906;&#23450;&#12377;&#12427;  <pr>&#12363;&#12367;&#12390;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#30906;&#23455;&#12395;&#12377;&#12427;  <pr>&#12363;&#12367;&#12376;&#12388;&#12395;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">verzegelen</trd>
  <trdgrp lng="pl">
    <trd>piecz&eogonek;towa&cacute;</trd>,
    <trd>opatrzy&cacute; <ind>piecz&eogonek;ci&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">selar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;<klr> (&c_k;&c_a;&c_k;&c_i;&c_m;-&c_l;..&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k;&c_o;&c_m;)</klr></trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_je;&c_j;&c_m;&c_i;&c_t;&c_mol;<klr> (&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">zape&ccaron;ati&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_u;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_ib;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_t;&c_a;&c_v;&c_r;&c_o;</trd>,
    <trd>&c_t;&c_a;&c_v;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23553;&#21475; <pr>f&emacron;ngk&#466;u</pr></trd>,
    <trd>&#32216; <pr>ji&amacron;n</pr></trd>,
    <trd>&#32516; <pr>ji&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sigel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sigel.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Signo enpremita en moligitan substancon.
    </dif>
    <subsnc mrk="sigel.0o.TIP_auxtentigi">
      <dif>
        Por certe konfirmi kaj a&ubreve;tentigi dokumenton a&ubreve; por sankcii kaj definitivigi
        kontrakton:
        <ekz>
          la vortoj de libro <tld/>ita
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 29:11</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          la <tld/>o ne estas ankora&ubreve; metita
          <fnt>
            <bib>PrV</bib>
          </fnt>
          <klr>(la afero ne estas finita)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sigel.0o.TIP_fermi">
      <dif>
        Por certigi la fermitecon de
        koverto a&ubreve; por distingige indiki la a&ubreve;toron:
        <ekz>
          mi metos en la nomo de mia sinjoro <tld/>on sur la traktato
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XI</lok>
          </fnt>;
        </ekz>
        <ekz>
          venis al Konstanco nova petletero kun <tld/>oj de 250 &ccirc;e&hcirc;aj nobeloj, ke <nom>Hus</nom>
          estu
          denove liberigita
          <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="plomb.0o.sigelo">plombo<sncref/></ref>,
      <ref cel="stamp.0o.presajxo">stampo<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sigel.0o.FIG_konfirmilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certigilo, konfirmilo:
      <ekz>
        tio metis la <tld/>on al lia gloro.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sigel.0o.FIG_fermilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certa fermilo:
      <ekz>
        <klr>[li]</klr> klarigis al <klr>[li tion]</klr>, sub <tld/>o de la plej granda
        sekreto kaj kun pruvoj en la mano
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas grandan <tld/>on sur la lipoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sigel.0o.FIG_signo">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Distingilo:
      <ekz>
        konservu min kiel <tld/>on en via koro
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 8:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de perfekteco, plena de sa&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 28:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ricevis la signon de cirkumcido, kiel <tld/>on de la virto
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 4:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &ccirc;io ku&scirc;is la <tld/>o de malri&ccirc;eco
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pe&ccaron;etn&iacute; prsten</trd>,
    <trd>sekret</trd>,
    <trd>sekr&eacute;t</trd>
  </trdgrp>
  <trd lng="de">Siegel</trd>
  <trd lng="en">seal</trd>
  <trd lng="es"><ind>sello</ind> (como el de las cartas
  medievales)</trd>
  <trdgrp lng="fr">
    <trd>scell&eacute;</trd>,
    <trd>sceau</trd>
  </trdgrp>
  <trd lng="hu">pecs&eacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#21360;  <pr>&#12375;&#12427;&#12375;</pr></trd>,
    <trd>&#21360;&#24433;  <pr>&#12356;&#12435;&#12360;&#12356;</pr></trd>,
    <trd>&#23553;&#21360;  <pr>&#12405;&#12358;&#12356;&#12435;</pr></trd>,
    <trd>&#12375;&#12427;&#12375;</trd>,
    <trd>&#30906;&#35388;  <pr>&#12363;&#12367;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#21051;&#21360;  <pr>&#12371;&#12367;&#12356;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">zegel</trd>
  <trd lng="pl">piecz&eogonek;&cacute;</trd>
  <trdgrp lng="pt">
    <trd>selo</trd>,
    <trd>sinete</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">pe&ccaron;atn&yacute; prste&ncaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>,
    <trd>&c_t;&c_a;&c_v;&c_r;&c_o; (&c_z;&c_n;&c_a;&c_k;)</trd>
  </trdgrp>
  <trd lng="zh">&#23553;&#21360; <pr>f&emacron;ngy&igrave;n</pr></trd>
</drv>

<drv mrk="sigel.0ilo">
  <kap>
    <tld/>ilo, <var><kap><tld/>ringo</kap></var>
  </kap>
  <snc>
    <dif>
      Gravurita peco, per kiu oni premas sian signon en <tld/>on:
      <ekz>
        rekonu, al kiu apartenas la <tld/>ilo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 38:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ringo sur mia dekstra mano
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 22:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        gravuritaj <tld/>ile, &ccirc;iu kun sia nomo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:21</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="stamp.0ilo">stampilo</ref>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_s;&c_mol;&c_c;&c_jo;&c_n;&c_a;&c_k; &c_z;&c_mol; &c_p;&c_ja;&c_ch;&c_a;&c_t;&c_k;&c_a;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Siegelstempel</trd>,
    <trd>Siegelring</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21360;&#31456;  <pr>&#12356;&#12435;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#21360;&#21028;  <pr>&#12356;&#12435;&#12400;&#12435;</pr></trd>,
    <trd>&#12399;&#12435;&#12371;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>piecz&aogonek;tka</trd>,
    <trd>sygnet</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_s;&c_t;&c_je;&c_n;&c_mol; &c_z; &c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21360;&#20449; <pr>y&igrave;nx&igrave;n</pr></trd>,
    <trd>&#20844;&#31456; <pr>g&omacron;ngzh&amacron;ng</pr></trd>,
    <trd>&#37628;&#21360; <pr>g&amacron;ngy&igrave;n</pr></trd>,
    <trd>&#38050;&#21360; <pr>g&amacron;ngy&igrave;n</pr></trd>
  </trdgrp>
</drv>
</art>
