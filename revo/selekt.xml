<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>selekt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="selekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="selekt.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="elekt.0i.KOMUNE">Elekti</ref>
      kaj apartigi bonajn erojn, for&jcirc;etante aliajn.
    </dif>
    <subsnc mrk="selekt.0i.bredo">
      <uzo tip="fak">BOT</uzo>
      <uzo tip="fak">ZOO</uzo>
      <dif>
        Elekti kun la celo plibonigi specon, rason ks.
      </dif>
    </subsnc>
    <rim>
      Oni ne konfuzu kun <ref cel="elekt.0i">elekti</ref> en pli
      &gcirc;enerala senco.
    </rim>
  </snc>
  <trd lng="ca">seleccionar</trd>
  <trd lng="de">selektieren</trd>
  <trd lng="fr">s&eacute;lectionner</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;laszt</trd>,
    <trd>szelekt&aacute;l</trd>
  </trdgrp>
  <trd lng="it">selezionare<klr> (agr, allev, nat.)</klr></trd>
  <trdgrp lng="nl">
    <trd>selecteren</trd>,
    <trd>kweken</trd>,
    <trd>fokken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="selekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; rezulto de tiu ago:
      <ekz>
        <ind>natura <tld/>o</ind>.
        <trd lng="hu">term&eacute;szetes <ind>kiv&aacute;laszt&oacute;d&aacute;s</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_l;&c_je;&c_k;&c_c;&c_y;&c_ja;</trd>, 
    <trd>&c_a;&c_d;&c_b;&c_o;&c_r;</trd> 
  </trdgrp>
  <trd lng="ca">selecci&oacute;</trd>
  <trd lng="de">Selektion</trd>
  <trd lng="fr">s&eacute;lection</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;laszt&oacute;d&aacute;s</trd>,
    <trd>szelekci&oacute;</trd>
  </trdgrp>
  <trd lng="it">selezione<klr> (agr, allev, nat.)</klr></trd>
  <trd lng="nl">selectie</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
