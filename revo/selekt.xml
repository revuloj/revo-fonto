<art mrk="$Id: selekt.xml,v 1.28 2025/04/05 15:16:34 revo Exp $">
<kap>
  <rad>selekt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="selekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="selekt.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="elekt.0i.KOMUNE">Elekti</ref>
      kaj apartigi bonajn erojn, for&jcirc;etante aliajn.
    </dif>
    <subsnc mrk="selekt.0i.bredo">
      <uzo tip="fak">BOT</uzo>
      <uzo tip="fak">ZOO</uzo>
      <dif>
        Elekti kun la celo plibonigi specon, rason ks.
      </dif>
    </subsnc>
    <rim>
      Oni ne konfuzu kun <ref cel="elekt.0i">elekti</ref> en pli
      &gcirc;enerala senco.
    </rim>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>elekti</ke>, <ke>apartigi</ke>; (pri bestobredado) prefere diru <ke>bredelekti</ke>, <ke>elektobredi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_e;&c_l;&c_je;&c_k;&c_c;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_e;&c_l;&c_je;&c_k;&c_c;&c_y;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="ca">seleccionar</trd>
  <trdgrp lng="cs">
    <trd>prov&#225;d&#283;t selekci</trd>,
    <trd>prov&#225;d&#283;t v&#253;b&#283;r</trd>,
    <trd>vyb&#237;rat</trd>
  </trdgrp>
  <trd lng="de">selektieren</trd>
  <trd lng="en">select</trd>
  <trd lng="es">seleccionar</trd>
  <trd lng="fr">s&eacute;lectionner</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;laszt</trd>,
    <trd>szelekt&aacute;l</trd>
  </trdgrp>
  <trd lng="it">selezionare<klr> (agr, allev, nat.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#36984;&#25246;&#12377;&#12427;  <pr>&#12379;&#12435;&#12383;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#28120;&#27760;&#12377;&#12427;  <pr>&#12392;&#12358;&#12383;&#12377;&#12427;</pr></trd>,
    <trd>&#20998;&#38626;&#12377;&#12427;  <pr>&#12406;&#12435;&#12426;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>selecteren</trd>,
    <trd>kweken</trd>,
    <trd>fokken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dobiera&cacute;</trd>,
    <trd>selekcjonowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odladi&tcaron; <klr>(r&#225;dio, tel.)</klr></trd>,
    <trd>selektova&tcaron;</trd>,
    <trd>vybra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&#1086;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_je;&#1083;&c_je;&#1082;&c_c;&c_ib;&#1086;&#1085;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36873;&#25321; <pr>xu&#462;nz&#233;</pr></trd>,
    <trd>&#36984;&#25799; <pr>xu&#462;nz&#233;</pr></trd>,
    <trd>&#21047; <pr>shu&#224;</pr></trd>,
    <trd>&#25361;&#36873; <pr>ti&#257;oxu&#462;n</pr></trd>,
    <trd>&#25361;&#36984; <pr>ti&#257;oxu&#462;n</pr></trd>,
    <trd>&#25249; <pr>l&#250;n</pr></trd>,
    <trd>&#25476; <pr>l&#250;n</pr></trd>,
    <trd>&#29956;&#36873; <pr>zh&#275;nxu&#462;n</pr></trd>,
    <trd>&#29956;&#36984; <pr>zh&#275;nxu&#462;n</pr></trd>,
    <trd>&#36873;&#25300; <pr>xu&#462;nb&#225;</pr></trd>,
    <trd>&#36984;&#25300; <pr>xu&#462;nb&#225;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="selekt.0o">
  <kap><tld/>o</kap>
  <snc mrk="selekt.0o.KOMUNE">
    <dif>
      Ago <tld/>i a&ubreve; rezulto de tiu ago.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_l;&c_je;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_d;&c_b;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">selecci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>selekce</trd>,
    <trd>v&#253;b&#283;r</trd>
  </trdgrp>
  <trd lng="de">Selektion</trd>
  <trd lng="en">selection</trd>
  <trd lng="es">selecci&oacute;n</trd>
  <trd lng="fr">s&eacute;lection</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;laszt&oacute;d&aacute;s</trd>,
    <trd>szelekci&oacute;</trd>
  </trdgrp>
  <trd lng="it">selezione<klr> (agr, allev, nat.)</klr></trd>
  <trd lng="nl">selectie</trd>
  <trdgrp lng="pl">
    <trd>dob&oacute;r</trd>,
    <trd>selekcja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>selekcia</trd>,
    <trd>v&#253;ber</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&#1083;&c_je;&#1082;&c_c;&c_ib;&#1103;</trd>,
    <trd>&c_d;&#1086;&c_b;&c_ib;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="selekt.natura0o">
  <kap>natura <tld/>o,
  <var><kap>natura <tld/>ado</kap></var></kap>
  <snc mrk="selekt.natura0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Rezulto de konkurado inter individuoj kaj grupoj por
      pluvivado, pro kiu konservi&gcirc;as kaj naskas idojn
      plej bone adaptitaj vivuloj:
      <ekz>
        konservon de favoraj varioj kaj forigon de
        malfavoraj varioj, mi nomas Natura <tld lit="S"/>ado
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 4a</lok></fnt>;
      </ekz>
      <ekz>
         ekzistas multaj aliaj faktoroj, kiuj influas la evolucion
         apud la natura <tld/>o a&ubreve; kunlabore kun &gcirc;i
         <fnt>
           <bib>Viki</bib>,
           <lok><url ref="&Viki;/Jaroslav_Flegr">
           Jaroslav Flegr</url>, 2012-01-22</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_a;&c_d;&c_b;&c_o;&c_r;</ind></trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;irozen&#253; v&#253;b&#283;r</trd>,
    <trd>p&rcaron;&#237;rodn&#237; v&#253;b&#283;r</trd>
  </trdgrp>
  <trd lng="en">natural selection</trd>
  <trd lng="es">selecci&oacute;n natural</trd>
  <trd lng="fr">s&eacute;lection naturelle</trd>
  <trd lng="hu">term&eacute;szetes <ind>kiv&aacute;laszt&oacute;d&aacute;s</ind></trd>
  <trdgrp lng="ja">
    <trd>&#33258;&#28982;&#28120;&#27760;  <pr>&#12375;&#12380;&#12435;&#12392;&#12358;&#12383;</pr></trd>,
    <trd>&#33258;&#28982;&#36984;&#25246;  <pr>&#12375;&#12380;&#12435;&#12379;&#12435;&#12383;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="pl">dob&oacute;r naturalny</trd>
  <trd lng="sk">prirodzen&#253; v&#253;ber</trd>
  <trd lng="uk">&#1087;&c_r;&c_i;&c_r;&#1086;&c_d;&#1085;&c_i;&c_j; &c_d;&#1086;&c_b;&c_ib;&c_r;</trd>
</drv>
