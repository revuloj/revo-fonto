<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>selekt</rad>/i
</kap>
<drv mrk="selekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Elekti kun la celo plibonigi specon, rason ks.
    </dif>
  </snc>
  <trd lng="de">selektieren</trd>
  <trdgrp lng="nl">
    <trd>selecteren</trd>,
    <trd>kweken</trd>,
    <trd>fokken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="selekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; rezulto de tiu ago:
      <ekz>
        natura <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Selektion</trd>
  <trd lng="nl">selectie</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
