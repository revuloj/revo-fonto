<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bibliotek</rad>/o
</kap>
<drv mrk="biblio1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kolekto da libroj:
      <ekz>
        li havas ri&ccirc;an bibliotekon<fnt>PIV1</fnt> <klr>(t.e. multajn
        valorajn librojn)</klr>;
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sinonimo de <ref tip="sin" cel="seri.0o.aroTIP">serio</ref> en
      kelkaj seriotitoloj:
      <ekz>
        <tld/>o Tutmonda;
      </ekz>
      <ekz>
        <tld/>o Nova Kulturo;
      </ekz>
      <ekz>
        Esperanta Biblioteko Internacia <klr>(eldonita de Ellersiek &amp;
        Borel)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      &Scirc;ranko a&ubreve; bretaro por libroj.
    </dif>
  <trd lng="en">book case</trd>
  </snc>
  <snc>
    <dif>
      Ejo, &ccirc;ambro, domo, en kiu oni konservas librojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bibliothek</trd>,
    <trd>B&uuml;cherei</trd>
  </trdgrp>
  <trd lng="en">library</trd>
  <trd lng="es">biblioteca</trd>
  <trd lng="fr">biblioth&egrave;que</trd>
  <trd lng="hu">k&ouml;nyvt&aacute;r</trd>
  <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
  <trd lng="sk">kni&zcaron;nica</trd>
  <trd lng="sv">bibliotek</trd>
</drv>
</art>
