<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: biblio1.xml,v 1.38 2023/10/29 19:11:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bibliotek</rad>/o
</kap>
<drv mrk="biblio1.0o">
  <kap><tld/>o</kap>
  <snc mrk="biblio1.0o.ejo">
    <dif>
      Publika <ref cel="serv.0o.EKON">servo</ref> kaj ties ejo (&ccirc;ambro, domo), kie oni
      konservas librojn, gazetojn kaj eventuale anka&ubreve; aliajn
      registritajn datumojn (sondiskojn, filmojn ktp) por ebligi
      al la homoj legi, uzi a&ubreve; pruntepreni ilin:
      <ekz>
        &ccirc;iu el la regimentoj, kiuj staras en Parizo, posedas propran <tld/>on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi konati&gcirc;adis kun la libroj el la <tld/>o de la metiolernantoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lo&gcirc;antaj en urboj pruntas la necesajn librojn el <tld/>oj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vizitis la &ccirc;efajn e&ubreve;ropajn <tld/>ojn kaj konsultis la plej eminentajn
        historiistojn
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toroj, interalie, abunde utiligis la materialojn, kiuj trovi&gcirc;as en la
        arkivo de <nom>CED</nom>, en la <tld lit="B"/>o <nom>Hodler</nom> de UEA kaj en la Internacia
        Esperanto-Muzeo
        <fnt>
          <bib>EeP</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &scirc;atas <tld/>ojn por <tld/>i, ne por fari laboron, kiu postulas egan, egan
        <klr>[&#x2026;]</klr> stre&ccirc;on de la menso
        <fnt>
          <bib>CKv</bib>
          <lok>8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Longino</nom> estis priskribata kiel &leftquot;vivanta <tld/>o kaj iranta muzeo&rightquot; &ndash; tiel
        granda estis lia klereco
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="libr.0ejo.biblioteko">librejo<sncref/></ref>
    <refgrp tip="prt">
      <ref cel="salon.lego0o">legosalono</ref>,
      <ref cel="libr.0otenejo.konservejo">librotenejo<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="arhxiv.0ejo">arkivejo</ref>,
      <ref cel="muze.0o.ejo">muzeo<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_ja;&c_t;&c_e;&c_k;&c_a;</trd>
    <trd lng="de">B&uuml;cherei</trd>
    <trd lng="pl">biblioteka</trd>
    <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#x56FE;&#x4E66;&#x9986; <pr>t&#xFA;sh&#x16B;gu&#x1CE;n</pr></trd>,
    <trd>&#x4E66;&#x5E93; <pr>sh&#x16B;k&#xF9;</pr></trd>,
    <trd>&#x56FE;&#x4E66;&#x5BA4; <pr>t&#xFA;sh&#x16B;sh&#xEC;</pr></trd>,
    <trd>&#x6587;&#x5E93; <pr>w&#xE9;nk&#xF9;</pr></trd>,
    <trd>&#x516C;&#x5171;&#x56FE;&#x4E66;&#x9986; <pr>g&#x14D;ngg&#xF2;ngt&#xFA;sh&#x16B;gu&#x1CE;n</pr></trd>,
    <trd>&#x85CF;&#x4E66; <pr>c&#xE1;ngsh&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="biblio1.0o.kolekto">
    <dif>
      <ref tip="super" cel="kolekt.0o.art">Kolekto</ref>
      da libroj, privata a&ubreve; publika:
      <ekz>
        mi havas nek edzinon, nek infanojn, nek <tld/>on, sed &dash; kiel mi diris &dash; mi
        abonas la Gazeton de Anoncoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia edzino lasis al la filinoj malgrandan <tld/>on, kiu enhavas anka&ubreve; verkojn de
        poezio
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        voja&gcirc;olibroj <klr>[&#x2026;]</klr> plenigis la bibliotekon de Fernando
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu unua volumeto Rumanaj legendoj kaj rakontoj aperas en Esperanto, kaj
        promesas plezurigi nin kaj pliri&ccirc;igi nian esperantan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Adriana Pavani</aut>
          <vrk><url ref="&Monato;/2003/008147.html">Rumanaj legendoj el Brazilo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="kabine.0o.ARKI">kabineto<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_ja;&c_t;&c_e;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_n;&c_ib;&c_g;&c_a;&c_z;&c_b;&c_o;&c_r;</trd>
    </trdgrp>
    <trd lng="de">private <ind>B&uuml;chersammlung</ind></trd>
    <trdgrp lng="pl">
      <trd>biblioteka</trd>,
      <trd>ksi&eogonek;gozbi&oacute;r</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
    <trd lng="zh">&#x56FE;&#x4E66; <pr>t&#xFA;sh&#x16B;</pr></trd>
</snc>
  <snc mrk="biblio1.0o.serio">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sinonimo de <ref tip="sin" cel="seri.0o.aroTIP">serio</ref>
      en kelkaj seriotitoloj:
      <ekz>
        &leftquot;<tld lit="B"/>o Tutmonda&rightquot;
        <fnt>
          <bib>Viki&#x202F;</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Biblioteko_Tutmonda">Biblioteko Tutmonda</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;Esperanta <tld lit="B"/>o Internacia&rightquot;
        <klr>(eldonita de <nom>Ellersiek</nom> kaj <nom>Borel</nom>)</klr>
        <fnt>
          <bib>Viki&#x202F;</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Esperanta_Biblioteko_Internacia">Esperanta Biblioteko Internacia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_ja;&c_t;&c_e;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Buchreihe</trd>,
      <trd>Schriftenreihe</trd>,
      <trd>Gesammelte <ind>Werk</ind>e</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
    <trd lng="zh">&#x4E1B;&#x4E66; <pr>c&#xF3;ngsh&#x16B;</pr></trd>
</snc>
  <snc mrk="biblio1.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Speciale aran&gcirc;ita
      <ref tip="super" cel="dosier.0o.KOMP">dosiero</ref>,
      entenata programajn partojn
      (subprogramojn: procedurojn, makroojn, konstantojn ktp),
      atingeblajn per ilia nomo dum bindado kun programo:
      <ekz>
        dinamike bindebla <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de matematikaj, grafikaj funkcioj.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_ja;&c_t;&c_e;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Lib <klr>(Programmbibliothek)</klr></trd>,
      <trd>Programmbibliothek</trd>
    </trdgrp>
    <trd lng="pl">biblioteka</trd>
    <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="biblio1.0o.sxranko">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="sxrank.libro0o">Libro&scirc;ranko</ref>
      a&ubreve; <ref cel="bret.libro0aro">librobretaro</ref>:
      <ekz>
        apud la kameno pendis malgranda <tld/>o, kiu &ccirc;e la unua rigardo <klr>[&#x2026;]</klr>
        enhavas la plej elektitajn trezorojn de la angla literaturo
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la metiisto,
        (de nov-jorka kuracisto, rerakontita de E.
        Weilsh&auml;user)</lok></fnt>;
      </ekz>
      <ekz>
        dekstre de la pordo dormetis iu meblo <tld/>-simila <klr>[&#x2026;]</klr>
        sufi&ccirc;e alta kaj &gcirc;ia supra parto havis vitrajn pordetojn kun
        malhelru&gcirc;aj kurtenoj
        <fnt>
        <aut>Raymond Schwarz</aut>, <vrk>Kiel akvo de l' rivero</vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      PIV2 retenas tiun sencon, sed por mi &gcirc;i estas surpriza kaj
      fremda. Neniam mi nomus meblon <tld/>o.
      Kiel atestas diskuto en <nom>soc.culture.esperanto</nom>, por plimulto
      da esperantistoj la esperanta vorto <tld/>o ne havas &ccirc;i tiun
      sencon. Tiu senco malestas en pluraj gravaj tradukvortaroj.
      Eksterdube la vortoj <ctl>libro&scirc;ranko</ctl> kaj
      <ctl>librobretaro</ctl> estas bone atestitaj en la
      literaturo kaj de &ccirc;iuj bone komprenataj; ekzemploj pri mebla
      <tld/>o maloftas kaj ne estas imitindaj.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trd lng="be">&c_k;&c_n;&c_ib;&c_zh;&c_n;&c_a;&c_ja;
    &c_sh;&c_a;&c_f;&c_a;</trd>
    <trd lng="en">book case</trd>
    <trd lng="pl">biblioteczka</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_y;&c_j;
      &c_sh;&c_k;&c_a;&c_f;</trd>,
      <trd>&c_s;&c_t;&c_je;&c_l;&c_l;&c_a;&c_zh; &c_d;&c_l;&c_ja;
      &c_k;&c_n;&c_i;&c_g;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_da;&t_pa;&t_drengbu;&t_tsheg;&t_ma;&t_dza;&t_naro;&t_da;&t_tsheg;&t_kha;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>bibliot&eacute;ka</trd>,
    <trd>knihovna <klr>(sk&rcaron;&iacute;&ncaron;, m&iacute;stnost, soubor knih)</klr></trd>
  </trdgrp>
  <trd lng="de">Bibliothek</trd>
  <trd lng="en">library</trd>
  <trd lng="es">biblioteca</trd>
  <trd lng="fr">biblioth&egrave;que</trd>
  <trd lng="he">&samekh;&#x5B4;&pe;&#x5B0;&resh;&#x5B4;&yod;&#x5B8;&he;</trd>
  <trd lng="hu">k&ouml;nyvt&aacute;r</trd>
  <trd lng="id">per<ind>pustaka</ind>an</trd>
  <trdgrp lng="ja">
    <trd>&#x56F3;&#x66F8;&#x9928;  <pr>&#x3068;&#x3057;&#x3087;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x56F3;&#x66F8;&#x5BA4;  <pr>&#x3068;&#x3057;&#x3087;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x8535;&#x66F8;  <pr>&#x305E;&#x3046;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x30E9;&#x30A4;&#x30D6;&#x30E9;&#x30EA;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bibliotheek</trd>,
    <trd>boekerij</trd>
  </trdgrp>
  <trd lng="ro">bibliotec&abreve;</trd>
  <trdgrp lng="sk">
    <trd>kni&zcaron;nica</trd>,
    <trd>kni&zcaron;nica s&uacute;borov</trd>
  </trdgrp>
  <trd lng="sv">bibliotek</trd>
  <trd lng="uk">&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
</drv>
</art>
