    <kap>
      <rad>Oseti</rad>/o
    </kap>

<drv mrk="oseti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Regiono etendi&gcirc;anta trans la kartvela-rusa limo,
      en kiu lo&gcirc;as <ref tip="prt" cel="oset.0o">oseto</ref>j:
      <ekz>
        en la jaro 1774 <tld/>o libervole ali&gcirc;is al la Rusia Imperio
        <fnt>
          <bib>Monato</bib>
          <aut>Vja&ccirc;eslav (Slavik) Ivanov</aut>
          <vrk><url ref="https://www.monato.be/2003/007865.html"
              >La oseta lingvo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Sovetio ili havis du a&ubreve;tonomiajn regionojn:
        Nord-<tld/>o ene de Rusio kaj
        Sud-<tld/>o kadre de Kartvelio
        <fnt>
          <bib>Monato</bib>
          <aut>Vja&ccirc;eslav (Slavik) Ivanov</aut>
          <vrk><url ref="https://www.monato.be/2003/007865.html"
              >La oseta lingvo</url></vrk>
          <lok>2003</lok>
        </fnt>,
      </ekz>
    </dif>
    <trd lng="de">Ossetien</trd>
    <trd lng="en">Ossetia</trd>
    <trd lng="fr">Oss&eacute;tie</trd>
  <trd lng="ru">&c_O;&c_s;&c_je;&c_t;&c_i;&c_ja;</trd>
  </snc>
</drv>

  </art>

