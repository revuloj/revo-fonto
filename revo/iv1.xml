<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iv1.xml,v 1.9 2023/08/09 07:24:52 revo Exp $">
<kap>
  <rad>Iv</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="iv1.0o">
  <kap><tld/>o</kap>
  <snc mrk="iv1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o Lapenna, kredeble la plej granda oratoro esperanta
        <fnt><bib>Plumamikoj</bib>, <lok>prozo nefikcia</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Ib;&c_v;&c_a; <klr>(&c_ib;&c_m;&c_ja;)</klr></trd>,
    <trd>&c_Ib;&c_w;</trd>
  </trdgrp>
  <trd lng="br">Erwan</trd>
  <trd lng="fr">Yves</trd>
  <trd lng="hu">Iv&oacute;</trd>
  <trd lng="it">Ivo</trd>
  <trd lng="ru">&c_I;&c_v;</trd>
  <trd lng="pt">Ivo<klr tip="amb"> (nome)</klr></trd>
  <trd lng="uk">&c_Ib;&c_v;&#x43E;</trd>
</drv>
</art>

