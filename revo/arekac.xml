<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arekac.xml,v 1.4 2023/10/28 08:33:08 revo Exp $">
<kap>
    <rad>arekac</rad>/oj <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="arekac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="arekac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="palm.0oj">Palmoj</ref>;
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">familio</ref>
      de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Arecaceae</trd>):
      <ekz>
        <tld/>oj a&ubreve; palmoj estas planta familio de angiospermoj <klr>[&#x2026;]</klr>,
        kiu jam estis disvasti&gcirc;anta anta&ubreve; &ccirc;. 70 milionojn da jaroj en la kretotempo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36161">Arekacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Palmengew&auml;chse</trd>
    <trdgrp lng="zh">
    <trd>&#x69DF;&#x6994;&#x79D1; <pr>b&#x12B;nl&#xE1;ngk&#x113;</pr></trd>,
    <trd>&#x6AB3;&#x6994;&#x79D1; <pr>b&#x12B;nl&#xE1;ngk&#x113;</pr></trd>,
    <trd>&#x68D5;&#x6988;&#x79D1; <pr>z&#x14D;ngl&#xFC;k&#x113;</pr></trd>,
    <trd>&#x68D5;&#x6ADA;&#x79D1; <pr>z&#x14D;ngl&#xFC;k&#x113;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>,
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_m;&c_y; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>,
    <trd>&c_a;&c_r;&c_e;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ar&eacute;cac&eacute;es</trd>,
    <trd>palmac&eacute;es</trd>
  </trdgrp>
</drv>
</art>

