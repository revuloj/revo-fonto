<vortaro>
  <art mrk="$Id: hort.xml,v 1.1 2023/01/29 14:24:10 afido Exp $">
    <kap>
      <rad>hort</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="hort.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HOR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="gxarde.frukto0o">Frukto&gcirc;ardeno</ref>:
      <ekz>
        maturaj or-citronoj en sunbanata <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2004/008467.html"
              >Kampoj de Soria Antonio Machado, universala poeto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi gvatis &ccirc;ieltendon, sunorajn vojojn, <tld/>ojn,kaj jen la maron
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Obstgarten</trd>
    <trd lng="en">orchard</trd>
    <trd lng="fr">verger</trd>
    <trd lng="ru">&c_f;&c_r;&c_u;&c_k;&c_t;&c_o;&c_v;&c_y;&c_j; &c_s;&c_a;&c_d;</trd>
  </snc>
</drv>

  </art>

