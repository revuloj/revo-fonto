<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hort.xml,v 1.5 2023/11/15 17:08:02 afido Exp $">
    <kap>
      <rad>hort</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="hort.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HOR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="gxarde.frukto0o">Frukto&gcirc;ardeno</ref>:
      <ekz>
        maturaj or-citronoj en sunbanata <tld/>o
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008467.html">Kampoj de Soria Antonio Machado, universala poeto</url></vrk>
              <bib>Monato</bib>
          <lok>2004/12, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi gvatis &ccirc;ieltendon, sunorajn vojojn, <tld/>ojn,kaj jen la maron
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_d; <klr>(&c_f;&c_r;&c_u;&c_k;&c_t;&c_o;&c_v;&c_y;)</klr></trd>
    <trd lng="de">Obstgarten</trd>
    <trd lng="en">orchard</trd>
    <trd lng="fr">verger</trd>
    <trd lng="nl">fruittuin</trd>
    <trd lng="ru">&c_f;&c_r;&c_u;&c_k;&c_t;&c_o;&c_v;&c_y;&c_j; &c_s;&c_a;&c_d;</trd>
  </snc>
  <trd lng="zh">&#x679C;&#x56ED; <pr>gu&#x1D2;yu&#xE1;n</pr></trd>
</drv>

  </art>

