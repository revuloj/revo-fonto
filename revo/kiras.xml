<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kiras</rad>/o
</kap>
<drv mrk="kiras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Metala, iafoje leda arma&jcirc;o, &scirc;irmanta la bruston kaj dorson:
      <ekz><tld/>o de kavaleriano.</ekz>
    </dif>
    <trd lng="de">Harnisch</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>,
      <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_k;&c_i;&c_r;&c_a;&c_s;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Aro de la &scirc;talaj platoj fiksitaj sur milit&scirc;ipo a&ubreve; alia milita
      veturilo, por &scirc;irmi &gcirc;in kontra&ubreve; pafa&jcirc;oj:
      <ekz>
        <ind><tld/>a&ubreve;to</ind>,
        <trdgrp lng="ru">
          <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_v;&c_i;&c_k;</trd>,
          <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>&scirc;ipo</ind>.
        <trd lng="de">Panzerschiff</trd>
        <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_je;&c_c; <klr>(&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_mol;)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="blend">blenda&jcirc;o</ref>.
    </refgrp>
    <trd lng="de">Sch&uuml;rze</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
  </snc>
  <snc num="3">
    <dif>
      Osta, korneca a&ubreve; kalka tega&jcirc;o, kiu &scirc;irmas la korpon de testudoj,
      krustuloj kc. 
    </dif>
    <refgrp tip="sin">
      <ref cel="karapa.0o">karapaco</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>
  </snc>
</drv>
<drv mrk="kiras.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>o:
      <ekz>menso <tld/>ita de (obstina en) anta&ubreve;ju&gcirc;oj kaj fiereco.</ekz>
    </dif>
    <trd lng="de">Panzer</trd>
  </snc>
</drv>
</art>

