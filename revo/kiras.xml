<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kiras</rad>/o
</kap>
<drv mrk="kiras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kiras.0o.KOMUNE">
    <dif>
      Tre malmola, fortika <ref tip="super"
      cel="sxel.0o.KOMUNE">&scirc;elo</ref>.
    </dif>
    <subsnc mrk="kiras.0o.portebla">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Metala, iafoje leda arma&jcirc;o, &scirc;irmanta la bruston kaj
        dorson:
        <ekz>
          <tld/>o de kavaleriano.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>,
        <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
        <trd>&c_k;&c_ib;&c_r;&c_a;&c_s;&c_a;</trd>
      </trdgrp>
      <trd lng="ca">cuirassa</trd>
      <trd lng="de">Harnisch</trd>
      <trd lng="en">breastplate</trd>
      <trd lng="es">coraza</trd>
      <trd lng="fr">cuirasse</trd>
      <trdgrp lng="hu">
        <trd>p&aacute;nc&eacute;l</trd>,
        <trd>v&eacute;rt</trd>
      </trdgrp>
      <trd lng="it">corazza</trd>
      <trd lng="nl">harnas</trd>
      <trdgrp lng="pl">
        <trd>zbroja</trd>,
        <trd>pancerz</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>,
        <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
        <trd>&c_k;&c_i;&c_r;&c_a;&c_s;&c_a;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kiras.0o.de_veturilo">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Aro de &scirc;talaj platoj, fiksitaj sur milit&scirc;ipo
        a&ubreve; alia milita
        veturilo, por &scirc;irmi &gcirc;in kontra&ubreve;
        pafa&jcirc;oj:
        <ekz>
          <ind><tld/>a&ubreve;to</ind>,
          <trdgrp lng="be">
            <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_ib;&c_k;</trd>,
            <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;</trd>
          </trdgrp>
          <trd lng="de">Panzerwagen</trd>
          <trd lng="en">armored car</trd>
          <trd lng="fr">voiture blind&eacute;e</trd>
          <trd lng="hu">p&aacute;nc&eacute;laut&oacute;</trd>
          <trd lng="it">blindato<klr> (sost.)</klr></trd>
          <trdgrp lng="nl">
            <trd>pantserauto</trd>,
            <trd>pantservoertuig</trd>
          </trdgrp>
          <trdgrp lng="pl">
          	<trd>pojazd <ind>opancerzony</ind></trd>,
            <trd>samoch&oacute;d <ind>pancerny</ind></trd>
          </trdgrp>
          <trdgrp lng="ru">
            <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_v;&c_i;&c_k;</trd>,
            <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;</trd>
          </trdgrp>
        </ekz>
        <ekz>
          &ccirc;u la avino a&ubreve;dis, ke Germanio estas konstruonta
          novan <ind><tld/>&scirc;ipo</ind>n
          <trd
          lng="be">&c_b;&c_r;&c_a;&c_n;&c_ja;&c_n;&c_o;&c_s;&c_je;&c_c;
          <klr
          tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
          <trd lng="de">Panzerschiff</trd>
          <trd lng="fr">cuirass&eacute;<klr tip="ind">
          (navire)</klr></trd>
          <trd lng="hu">p&aacute;nc&eacute;lhaj&oacute;</trd>
          <trd lng="it">corazzata<klr> (sost. mar.)</klr></trd>
          <trd lng="nl">pantserschip</trd>
          <trd lng="pl">pancernik</trd>
          <trd
          lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_je;&c_c;
          <klr
          tip="amb">(&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_mol;)</klr></trd>
          <fnt><bib>Metrop</bib></fnt>?
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="blend1.0o">blendo</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;</trd>,
        <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
      </trdgrp>
      <trd lng="de">Sch&uuml;rze</trd>
      <trdgrp lng="en">
        <trd>armor</trd>,
        <trd>armor plate</trd>
      </trdgrp>
      <trd lng="fr">blindage</trd>
      <trdgrp lng="hu">
        <trd>p&aacute;nc&eacute;l</trd>,
        <trd>p&aacute;nc&eacute;lzat</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>corazza</trd>,
        <trd>blindatura</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>pantser</trd>,
        <trd>pantsering</trd>,
        <trd>blindering</trd>
      </trdgrp>
      <trd lng="pl">pancerz</trd>
      <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
    </subsnc>
    <subsnc mrk="kiras.0o.ZOO">
      <uzo tip="fak">ZOO</uzo>
      <ref tip="dif" cel="karapa.0o">karapaco</ref>
      <adm>
        PIV2 diras, ke "kiraso" estas la &gcirc;usta termino. "Karapaco"
        koncernus nur la dorsan flankon de kiraso.
        <aut>MB</aut>
      </adm>
      <trd lng="be">&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
      <trd lng="fr">carapace</trd>
      <trd lng="hu">p&aacute;nc&eacute;l</trd>
      <trd lng="it">corazza</trd>
      <trd lng="nl">pantser</trd>
      <trdgrp lng="pl">
        <trd>skorupa</trd>,
        <trd>pancerz</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>
    </subsnc>
  </snc>
</drv>

<drv mrk="kiras.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kiras.0i.KOMUNE">
    <dif>
      Provizi per <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        menso <tld/>ita de <klr>(obstina en)</klr>
        anta&ubreve;ju&gcirc;oj kaj fiereco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">panzern</trd>
  <trdgrp lng="es">
    <trd>acorazar</trd>,
    <trd>blindar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blinder</trd>,
    <trd>cuirasser</trd>,
    <trd>endurcir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;nc&eacute;loz</trd>,
    <trd>v&eacute;rtez</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>corazzare</trd>,
    <trd>blindare</trd>
  </trdgrp>
  <trd lng="nl">pantseren</trd>
  <trdgrp lng="pl">
    <trd>pancerzy&cacute;</trd>,
    <trd>opancerza&cacute;</trd>,
    <trd>zbroi&cacute;<klr> (okrywa&cacute; zbroj&aogonek;)</klr></trd>
  </trdgrp>
  <trd
  lng="ru">&c_b;&c_r;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kiras.masx0o">
  <kap>ma&scirc;<tld/>o</kap>
  <snc mrk="kiras.masx0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Militista &scirc;irma tuniko el feraj interplektitaj ringetoj,
      uzita
      interalie en e&ubreve;ropa mezepoko:
      <ekz>
        vi devas plekti dek unu ma&scirc;<tld/>ojn
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_ch;&c_u;&c_g;&c_a;</trd>
  <trd lng="br">sae mailhet</trd>
  <trdgrp lng="de">
    <trd>Kettenr&uuml;stung</trd>,
    <trd>Kettenpanzer</trd>
  </trdgrp>
  <trd lng="en">chainmail</trd>
  <trd lng="fr">cotte de maille</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;nc&eacute;ling</trd>,
    <trd>ingv&eacute;rt</trd>
  </trdgrp>
  <trd lng="it"><ind>cotta</ind> di maglia</trd>
  <trd lng="nl">mali&euml;nkolder</trd>
  <trdgrp lng="pl">
    <trd>kolczuga</trd>,
    <trd>misiurka</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_u;&c_g;&c_a;</trd>
</drv>
</art>
