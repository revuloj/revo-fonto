<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kiras.xml,v 1.25 2020/02/17 18:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kiras</rad>/o
</kap>
<drv mrk="kiras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kiras.0o.KOMUNE">
    <dif>
      Tre malmola, fortika <ref tip="super"
      cel="sxel.0o.KOMUNE">&scirc;elo</ref>:
      <ekz>
        nukleaj centraloj la&ubreve;dire estas <klr>[&#8230;]</klr> fortikaj kontra&ubreve;
        mezaj aviadiloj,
        kaj neniu beton<tld/>o &scirc;irmas kontra&ubreve; kra&scirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="https://www.monato.be/2011/010418.php">Adia&ubreve;o
          al nukleo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        glaci<tld/>aj altkrestoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Maljunulo de la Migranta Monto</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kiras.0o.portebla">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Metala, iafoje leda arma&jcirc;o, &scirc;irmanta la bruston kaj
        dorson:
        <ekz>
          <tld/>o de kavaleriano;
        </ekz>
        <ekz>
          kupra kasko estis sur lia kapo, kaj per skvamita <tld/>o li
          estis vestita
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 17:5</lok>
          </fnt>;
        </ekz>
        <ekz>
          eniris en du vicoj deko da gvardiistoj en oritaj kaskoj kaj
          <tld/>oj, kun nudaj
          glavoj
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro VI</lok>
          </fnt>;
        </ekz>
        <ekz>
          iliaj persistemaj &ccirc;evaloj kun longaj haregoj havis sur la
          kapoj kaj brustoj
          kuprajn <tld/>ojn, similajn al fi&scirc;a skvamo
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro X</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          surmetinte la <tld/>on de justeco
          <fnt>
            <bib>NT</bib>
            <lok>Efesanoj 6:14</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="arm.0ajxo.armilo">arma&jcirc;o<sncref/></ref>,
        <ref tip="vid" cel="harnis.0o">harniso</ref>,
        <ref cel="sxild.0o.MIL">&scirc;ildo<sncref/></ref>,
        <ref cel="arm.sxirm0o">&scirc;irmarmo</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>,
        <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
        <trd>&c_k;&c_ib;&c_r;&c_a;&c_s;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Harnisch</trd>,
        <trd>R&uuml;stung</trd>
      </trdgrp>
      <trd lng="en">breastplate</trd>
      <trd lng="es">coraza</trd>
      <trd lng="fr">cuirasse</trd>
      <trdgrp lng="hu">
        <trd>p&aacute;nc&eacute;l</trd>,
        <trd>v&eacute;rt</trd>
      </trdgrp>
      <trd lng="it">corazza</trd>
      <trd lng="nl">harnas</trd>
      <trdgrp lng="pl">
        <trd>zbroja</trd>,
        <trd>pancerz</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>,
        <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
        <trd>&c_k;&c_i;&c_r;&c_a;&c_s;&c_a;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kiras.0o.de_veturilo">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Aro de &scirc;talaj platoj, fiksitaj sur milit&scirc;ipo
        a&ubreve; alia milita
        veturilo, por &scirc;irmi &gcirc;in kontra&ubreve;
        pafa&jcirc;oj:
        <ekz>
          <ind><tld/>a&ubreve;to</ind>;
          <trdgrp lng="be">
            <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_ib;&c_k;</trd>,
            <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;</trd>
          </trdgrp>
          <trd lng="ca">cotxe blindat</trd>
          <trd lng="de">Panzerwagen</trd>
          <trd lng="en">armored car</trd>
          <trd lng="fr">voiture blind&eacute;e</trd>
          <trd lng="hu">p&aacute;nc&eacute;laut&oacute;</trd>
          <trd lng="it">blindato<klr> (sost.)</klr></trd>
          <trdgrp lng="nl">
            <trd>pantserauto</trd>,
            <trd>pantservoertuig</trd>
          </trdgrp>
          <trdgrp lng="pl">
          	<trd>pojazd <ind>opancerzony</ind></trd>,
            <trd>samoch&oacute;d <ind>pancerny</ind></trd>
          </trdgrp>
          <trdgrp lng="ru">
            <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_v;&c_i;&c_k;</trd>,
            <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;</trd>
          </trdgrp>
        </ekz>
        <ekz>
          &ccirc;u la avino a&ubreve;dis, ke Germanio estas konstruonta
          novan <ind><tld/>&scirc;ipo</ind>n
          <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_ja;&c_n;&c_o;&c_s;&c_je;&c_c;
          <klr tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
          <trd lng="ca">cuirassat<klr> (n&agrave;ut.)</klr></trd>
          <trd lng="de">Panzerschiff</trd>
          <trd lng="fr">cuirass&eacute;<klr tip="ind">
          (navire)</klr></trd>
          <trd lng="hu">p&aacute;nc&eacute;lhaj&oacute;</trd>
          <trd lng="it">corazzata<klr> (sost. mar.)</klr></trd>
          <trd lng="nl">pantserschip</trd>
          <trd lng="pl">pancernik</trd>
          <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_je;&c_c;
          <klr tip="amb">(&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_mol;)</klr></trd>
          <fnt><bib>Metrop</bib></fnt>?
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="blend1.0o">blendo</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;</trd>,
        <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
      </trdgrp>
      <trd lng="de">Sch&uuml;rze</trd>
      <trdgrp lng="en">
        <trd>armor</trd>,
        <trd>armor plate</trd>
      </trdgrp>
      <trd lng="fr">blindage</trd>
      <trdgrp lng="hu">
        <trd>p&aacute;nc&eacute;l</trd>,
        <trd>p&aacute;nc&eacute;lzat</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>corazza</trd>,
        <trd>blindatura</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>pantser</trd>,
        <trd>pantsering</trd>,
        <trd>blindering</trd>
      </trdgrp>
      <trd lng="pl">pancerz</trd>
      <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
    </subsnc>
    <subsnc mrk="kiras.0o.ZOO">
      <uzo tip="fak">ZOO</uzo>
      <dif>
        <ref tip="dif" cel="karapa.0o">Karapaco</ref>:
        <ekz>
          <uzo tip="stl">FIG</uzo>
          kvin gigantoj en la nigraj insekt<tld/>oj staris rekte kaj
          senmove
          <fnt>
            <aut>Michael Ende, trad. Wolfram Diestel</aut>
            <vrk>La Sen&ccirc;esa Rakonto</vrk>
            <lok>La Batalo je la Ebura Turo</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
      <trd lng="de">Panzer</trd>
      <trd lng="en">shell</trd>
      <trd lng="fr">carapace</trd>
      <trd lng="hu">p&aacute;nc&eacute;l</trd>
      <trd lng="it">corazza</trd>
      <trd lng="nl">pantser</trd>
      <trdgrp lng="pl">
        <trd>skorupa</trd>,
        <trd>pancerz</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>
      <adm>
        PIV2 diras, ke "kiraso" estas la &gcirc;usta termino. "Karapaco"
        koncernus nur la dorsan flankon de kiraso.
        <aut>MB</aut>
      </adm>
    </subsnc>
  </snc>
  <trd lng="ca">cuirassa</trd>
  <trdgrp lng="cs">
    <trd>krun&yacute;&rcaron;</trd>,
    <trd>kyrys</trd>,
    <trd>panc&iacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>brnenie</trd>,
    <trd>krunier</trd>,
    <trd>kyras</trd>,
    <trd>kyrys</trd>,
    <trd>pancier</trd>
  </trdgrp>
</drv>

<drv mrk="kiras.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kiras.0i.KOMUNE">
    <dif>
      Provizi per <tld/>o:
      <ekz>
        la Kongresan Centron &ccirc;irka&ubreve;is vicoj da <tld/>itaj policanoj, kaj
        kontra&ubreve; ili
        staris grupoj da manifestaciantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007258.html"
              >Mondaj Financoj: Post la kunsido de financistoj en
              Prago</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la princo de&jcirc;oros en nur le&gcirc;ere <tld/>ita skoltad-veturilo sur
        iliaj stratoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2007/009240.php">Sango
          kaj rango</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        sep gla&ccirc;eroj <tld/>as la ne-aktivan vulkanon
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url
          ref="https://www.monato.be/2003/007824.html">Tondrulo montego
          de la Andoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        menso <tld/>ita de <klr>(obstina en)</klr>
        anta&ubreve;ju&gcirc;oj kaj fiereco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">cuirassar</trd>
  <trdgrp lng="cs">
    <trd>obrnit</trd>,
    <trd>opanc&eacute;&rcaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>r&uuml;sten</trd>,
    <trd>panzern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to armor <klr>(usone)</klr></trd>,
    <trd>armour <klr>(brite)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>acorazar</trd>,
    <trd>blindar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blinder</trd>,
    <trd>cuirasser</trd>,
    <trd>endurcir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;nc&eacute;loz</trd>,
    <trd>v&eacute;rtez</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>corazzare</trd>,
    <trd>blindare</trd>
  </trdgrp>
  <trd lng="nl">pantseren</trd>
  <trdgrp lng="pl">
    <trd>pancerzy&cacute;</trd>,
    <trd>opancerza&cacute;</trd>,
    <trd>zbroi&cacute;<klr> (okrywa&cacute; zbroj&aogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>obrni&tcaron;</trd>,
    <trd>opancierova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kiras.masx0o">
  <kap>ma&scirc;<tld/>o</kap>
  <snc mrk="kiras.masx0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Militista &scirc;irma tuniko el feraj interplektitaj ringetoj,
      uzita
      interalie en e&ubreve;ropa mezepoko:
      <ekz>
        vi devas plekti dek unu ma&scirc;<tld/>ojn
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        Sa&ubreve;lo, vestita en ma&scirc;<tld/>o, falinta sub sia &ccirc;evalo, survoje al
        Damasko
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="https://www.monato.be/2008/009516.php">&Ccirc;u
          &scirc;tormo en tetaso a&ubreve; en ambono?</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_ch;&c_u;&c_g;&c_a;</trd>
  <trd lng="br">sae mailhet</trd>
  <trdgrp lng="ca">
    <trd>ausberg</trd>,
    <trd>cota de malla</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kettenr&uuml;stung</trd>,
    <trd>Kettenpanzer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mail<klr tip="ind"> (armor)</klr></trd>,
    <trd>chain mail</trd>,
    <trd>chainmail</trd>
  </trdgrp>
  <trd lng="fr">cotte de maille</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;nc&eacute;ling</trd>,
    <trd>ingv&eacute;rt</trd>
  </trdgrp>
  <trd lng="it"><ind>cotta</ind> di maglia</trd>
  <trd lng="nl">mali&euml;nkolder</trd>
  <trdgrp lng="pl">
    <trd>kolczuga</trd>,
    <trd>misiurka</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_u;&c_g;&c_a;</trd>
</drv>
</art>
