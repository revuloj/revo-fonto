<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kiras</rad>/o
</kap>
<drv mrk="kiras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kiras.0o.portebla">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Metala, iafoje leda arma&jcirc;o, &scirc;irmanta la bruston kaj dorson:
      <ekz>
        <tld/>o de kavaleriano.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>,
      <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_k;&c_ib;&c_r;&c_a;&c_s;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Harnisch</trd>
    <trd lng="en">breastplate</trd>
    <trd lng="fr">cuirasse</trd>
    <trdgrp lng="hu">
      <trd>p&aacute;nc&eacute;l</trd>,
      <trd>v&eacute;rt</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>,
      <trd>&c_l;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_k;&c_i;&c_r;&c_a;&c_s;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kiras.0o.de_veturilo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Aro de &scirc;talaj platoj, fiksitaj sur milit&scirc;ipo a&ubreve; alia milita
      veturilo, por &scirc;irmi &gcirc;in kontra&ubreve; pafa&jcirc;oj:
      <ekz>
        <ind><tld/>a&ubreve;to</ind>,
        <trdgrp lng="be">
          <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_ib;&c_k;</trd>,
          <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;</trd>
        </trdgrp>
        <trd lng="en">armored car</trd>
        <trd lng="fr">voiture blind&eacute;e</trd>
        <trd lng="hu">p&aacute;nc&eacute;laut&oacute;</trd>
        <trdgrp lng="ru">
          <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_v;&c_i;&c_k;</trd>,
          <trd>&c_b;&c_r;&c_o;&c_n;&c_je;&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>&scirc;ipo</ind>.
        <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_ja;&c_n;&c_o;&c_s;&c_je;&c_c; <klr tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
        <trd lng="de">Panzerschiff</trd>
        <trd lng="fr">cuirass&eacute;<klr tip="ind"> (navire)</klr></trd>
        <trd lng="hu">p&aacute;nc&eacute;lhaj&oacute;</trd>
        <trd lng="nl">pantserschip</trd>
        <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_je;&c_c; <klr>(&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_mol;)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="blend1.0o">blendo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
    </trdgrp>
    <trd lng="de">Sch&uuml;rze</trd>
    <trdgrp lng="en">
      <trd>armor</trd>,
      <trd>armor plate</trd>
    </trdgrp>
    <trd lng="fr">blindage</trd>
    <trdgrp lng="hu">
      <trd>p&aacute;nc&eacute;l</trd>,
      <trd>p&aacute;nc&eacute;lzat</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>
  </snc>
  <snc mrk="kiras.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="karapa.0o">karapaco</ref>
    <adm>
      PIV2 diras, ke "kiraso" estas la gxusta termino. "Karapaco"
      koncernus nur la dorsan flankon de kiraso.
      <aut>MB</aut>
    </adm>
    <trd lng="be">&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
    <trd lng="fr">carapace</trd>
    <trd lng="hu">p&aacute;nc&eacute;l</trd>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>
  </snc>
  <trd lng="nl">pantser</trd>
</drv>

<drv mrk="kiras.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        menso <tld/>ita de <klr>(obstina en)</klr>
        anta&ubreve;ju&gcirc;oj kaj fiereco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">panzern</trd>
  <trdgrp lng="fr">
    <trd>blinder</trd>,
    <trd>cuirasser</trd>,
    <trd>endurcir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;nc&eacute;loz</trd>,
    <trd>v&eacute;rtez</trd>
  </trdgrp>
  <trd lng="nl">panseren</trd>
</drv>
</art>
