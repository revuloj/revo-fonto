<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dakot.xml,v 1.2 2023/02/21 08:34:41 revo Exp $">
<kap>
  <rad>Dakot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dakot.norda0o">
  <kap>Norda <tld/>o,
  <var><kap>Nord<tld
  lit="d"/>o<fnt><bib>PIV1</bib></fnt></kap></var></kap>
  <snc mrk="dakot.norda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la norda parto de 
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        la&ubreve; la raportoj Norda <tld/>o estas tre sendan&gcirc;era 
        &scirc;tato
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Aprilo_2007.html">
          taglibro</url></vrk>,
          <lok>2007-04-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_D;&c_a;&c_k;&c_o;&c_t;&c_a;</trd>
  <trd lng="en">North Dakota</trd>
  <trd lng="fr">Dakota du Nord</trd>
</drv>

<drv mrk="dakot.suda0o">
  <kap>Suda <tld/>o,
  <var><kap>Sud<tld
  lit="d"/>o<fnt><bib>PIV1</bib></fnt></kap></var></kap>
  <snc mrk="dakot.suda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la centra parto de 
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        en siaj voja&gcirc;oj, Chris foje trapasas simile 
        allogajn vila&gcirc;ojn <klr>...</klr> en Suda <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://bulteno.esperanto-usa.org/2014/3/90-senintenca_esperantajxo.html">
          &Ccirc;u vila&gcirc;o efektive vizitinda?</url></vrk>,
          <lok>Usona Esperantisto 2014:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_D;&c_a;&c_k;&c_o;&c_t;&c_a;</trd>
  <trd lng="en">South Dakota</trd>
  <trd lng="fr">Dakota du Sud</trd>
</drv>
</art>

