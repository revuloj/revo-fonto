<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lumb</rad>/o
</kap>
<drv mrk="lumb.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Malsupra parto de la dorso, amba&ubreve;flanke de la spino, inter tiu kaj
      la koksoj:
      <ekz>
        zonu vian <tld/>on per glavo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Lende</trd>
  <trdgrp lng="en">
    <trd>loin</trd>,
    <trd>haunch</trd>,
    <trd>small of the back</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>zona lumbar</trd>,
    <trd>lomo</trd>
  </trdgrp>
  <trd lng="hu">&aacute;gy&eacute;k</trd>
  <trd lng="nl">lende</trd>
  <trd lng="pl">l&eogonek;d&zacute;wie</trd>
  <trd lng="pt">lombo</trd>
  <trd lng="ru">&c_p;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="lumb.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Sama parto de la bu&ccirc;bestoj destinita por man&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Lende</trd>,
     <trd>Lendenfilet</trd>
  </trdgrp>
  <trd lng="en">loin</trd>
  <trd lng="es">lomo</trd>
  <trdgrp lng="hu">
     <trd>vesepecsenye</trd>,
     <trd>b&eacute;lsz&iacute;n</trd>,
     <trd>h&aacute;tsz&iacute;n</trd>
  </trdgrp>
  <trd lng="nl">lendestuk</trd>
  <trd lng="pt">lombo</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
