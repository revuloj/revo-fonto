<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oligark.xml,v 1.11 2020/09/08 06:10:58 revo Exp $">
<kap>
  <ofc>7</ofc><rad>oligark</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><ofc>7</ofc><rad var="&hcirc;">oligar&hcirc;</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="oligark.0o">
  <kap><ofc>7</ofc><tld/>o,
    <var><kap><ofc>7</ofc><tld var="&hcirc;"/>o</kap></var>
  </kap>
  <snc mrk="oligark.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano de reganta grupo a&ubreve; familio en
      <ref tip="vid" cel="oligar.0o.POL">oligar&hcirc;io</ref>:
      <ekz>
        la rusiaj ekonomiaj <tld var="&hcirc;"/>oj ekestintaj post la pereo de Sovetunio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=542074">Aleksandr Lebedev</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        repo kontra&ubreve; <tld var="&hcirc;"/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=209017">Repo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        efektiva potenco estas en la manoj de la prezidento,
        la provincestroj kaj la <tld/>oj
        <fnt>
          <aut>S. Kalb</aut>,
          <vrk><url ref="http://www.oocities.org/kalblando/enc/r/ru.htm">
          Rusio</url></vrk>,
          <lok>en: Enciklopedio Kalblanda, 1996</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Oligarch</trd>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_g;&c_a;&c_r;&c_h;</trd>
  <trdgrp lng="cs">
    <trd>oligarch</trd>,
    <trd>oligarcha</trd>
  </trdgrp>
  <trd lng="en">oligarch</trd>
  <trd lng="fr">oligarque</trd>
  <trd lng="pl">oligarcha</trd>
  <trd lng="sk">oligarcha</trd>
</drv>
</art>

