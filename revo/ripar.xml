<drv mrk="ripar.0o">
  <kap><tld/>o</kap>
  <snc mrk="ripar.0o.ago_rezulto">
    <dif>
      La ago <tld/>i,
      <ref tip="dif" cel="ripar.0ado.KOMUNE">riparado</ref>:
      <ekz>
        la tempiojn &#x2010; sen <tld/>o &#x2010; anserpiede stampas faldoj
        <fnt>
          <aut>R. Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
          <lok>Anta&ubreve;signoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ministra kabineto malpermesis renovigon, <tld/>on kaj
        rekonstruadon de
        administrejoj
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006806.php">&Ccirc;iea &scirc;parado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la jaro 2000 estis investitaj en bazaj <tld/>laboroj kaj
        modernigoj
        <klr>[&#x2026;]</klr> milionoj da usonaj dolaroj
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2001/007307.html">Pliigita aktivado de LUKoil</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 10</lok>
          </fnt>;
      </ekz>
      <ekz>
        la kompanio <klr>[&#x2026;]</klr> prokrastis la ur&gcirc;e bezonatan
        <tld/>-programon
        <fnt>
	<aut>Paul Gubbins</aut>
	<vrk><url ref="&Monato;/2001/007260.php">Politiko elreligita</url></vrk>
	<bib>Monato</bib>
	<lok>2001/02, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_a;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
    <trd lng="ca">reparaci&oacute;</trd>
    <trd lng="en">repair</trd>
    <trd lng="es">reparaci&oacute;n</trd>
    <trd lng="hu">jav&iacute;t&aacute;s</trd>
    <trd lng="pl">naprawa</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;</trd>,
      <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ripar.0o.riparajxo">
    <dif>
      <ref tip="dif" cel="ripar.0ajxo.KOMUNE">Ripara&jcirc;o</ref>, la rezulto
      post <tld/>ado:
      <ekz>
        dum la tri pasintaj monatoj la vesto pli eluzi&gcirc;is,
        en diversaj lokoj &gcirc;i vidigis <tld/>ojn kaj flikojn
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; &c_d;&c_e;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; &c_r;&c_e;&c_ch;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Flicken</trd>,
      <trd>repariertes Objekt</trd>,
      <trd>ausgebessertes Objekt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>patch</trd>,
      <trd>fix</trd>,
      <trd>repaired item</trd>
    </trdgrp>
    <trd lng="hu">jav&iacute;t&aacute;s<klr tip="amb"> (helye is)</klr></trd>
    <trd lng="pl">reperacja</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>n&aacute;prava</trd>,
    <trd>od&ccaron;in&ecaron;n&iacute;</trd>,
    <trd>oprava</trd>,
    <trd>spr&aacute;vka</trd>
  </trdgrp>
  <trd lng="de">Reparatur</trd>
  <trd lng="fr">r&eacute;paration</trd>
  <trdgrp lng="ja">
    <trd>&#x4FEE;&#x7406;  <pr>&#x3057;&#x3085;&#x3046;&#x308A;</pr></trd>,
    <trd>&#x4FEE;&#x7E55;  <pr>&#x3057;&#x3085;&#x3046;&#x305C;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napravenie</trd>,
    <trd>n&aacute;hrada</trd>,
    <trd>repar&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_g;&c_o;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DAD;&#x4FEE; <pr>w&#xE9;ixi&#x16B;</pr></trd>,
    <trd>&#x7EF4;&#x4FEE; <pr>w&#xE9;ixi&#x16B;</pr></trd>
  </trdgrp>
</drv>

