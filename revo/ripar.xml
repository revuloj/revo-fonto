<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ripar</rad>/i
</kap>
<drv mrk="ripar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Remeti <ref tip="ant" cel="difekt.0i">difektitan</ref> aferon
      en bonan, uzeblan staton:
      <ekz>
        oni dungis &scirc;tonhakistojn kaj &ccirc;arpentistojn
        por renovigi la domon de la Eternulo, anka&ubreve; fera&jcirc;istojn
        kaj kupra&jcirc;istojn por <tld/>i la domon de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Kr2; 24:12</lok></fnt>;
      </ekz>
      <ekz>
        kiam Sanbalat kaj Tobija <klr>...</klr> a&ubreve;dis,
        ke la muregoj de Jerusalem estas <tld/>ataj kaj ke la
        bre&ccirc;oj komencis esti fermataj ili tre ekkoleris
        <fnt><bib>MT</bib>, <lok>&Neh; 4:7</lok></fnt>;
      </ekz>
      <ekz>
        li vidis aliajn du fratojn <klr>...</klr>
        en la &scirc;ipeto kun ilia patro Zebedeo <tld/>antajn
        siajn retojn
        <fnt><bib>NT</bib>, <lok>&Mat; 4:21</lok></fnt>;
      </ekz>
      <ekz>
        mi iris por <tld/>igi mian brakhorlo&gcirc;on
        <fnt><aut>Marjorie Boulton</aut>, <vrk>Dinomanio</vrk>,
        <bib>Monato</bib>, <lok>jaro 1994a, numero 2a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="flik.0i">fliki</ref>,
      <ref cel="san.0igi">sanigi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="korekt.0i">korekti</ref>,
      <ref cel="bon.re0igi">rebonigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">spravit</trd>
  <trdgrp lng="de">
    <trd>reparieren</trd>,
    <trd>wiederherstellen</trd>
  </trdgrp>
  <trd lng="en">repair</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;parer</trd>,
    <trd>restaurer<klr tip="amb"> (r&eacute;parer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jav&iacute;t</trd>,
    <trd>helyre&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>repareren</trd>,
    <trd>herstellen</trd>,
    <trd>maken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">reparera</trd>
</drv>

<drv mrk="ripar.0o">
  <kap><tld/>o</kap>
  <snc mrk="ripar.0o.riparado">
    <dif>
      <ref tip="dif" cel="ripar.0ado">riparado</ref>.
    </dif>
    <trd lng="en">repair</trd>
    <trd lng="fr">r&eacute;paration</trd>
    <trd lng="hu">jav&iacute;t&aacute;s</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;</trd>,
      <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ripar.0o.riparajxo">
    <dif>
      <ref tip="dif" cel="ripar.0ajxo">ripara&jcirc;o</ref>:
      <ekz>
        dum la tri pasintaj monatoj la vesto pli eluzi&gcirc;is,
        en diversaj lokoj &gcirc;i vidigis <tld/>ojn kaj flikojn
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">r&eacute;paration<klr tip="amb"> (endroit r&eacute;par&eacute;)</klr></trd>
    <trd lng="hu">jav&iacute;t&aacute;s<klr tip="amb"> (helye is)</klr></trd>
  </snc>
</drv>

<drv mrk="ripar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Parto <tld/>ita, remetita en ta&ubreve;gan staton.
    </dif>
  </snc>
  <trd lng="hu">jav&iacute;tott (alkat)r&eacute;sz</trd>  
</drv>

<drv mrk="ripar.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago ripari.
    </dif>
  </snc>
  <trd lng="en">repair</trd>
  <trd lng="fr">r&eacute;paration</trd>
  <trd lng="hu">jav&iacute;t&aacute;s</trd>  
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

</art>
