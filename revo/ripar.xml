<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ripar</rad>/i
</kap>
<drv mrk="ripar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Rebonigi:
      <ekz>
        <tld/>i &scirc;iritan veston
        (<ref tip="sin" cel="flik.0i">fliki</ref>);
      </ekz>
      <ekz>
        <tld/>i horlo&gcirc;on, ma&scirc;inon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i mankon, forgeson.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">spravit</trd>
  <trd lng="de">reparieren</trd>
  <trd lng="en">repair</trd>
  <trdgrp lng="nl">
    <trd>repareren</trd>,
    <trd>herstellen</trd>,
    <trd>maken</trd>
  </trdgrp><trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_je;&c_m;&c_o;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">reparera</trd>
</drv>
</art>
