<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adenin.xml,v 1.14 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>adenin</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="adenin.0o">
  <kap><tld/>o</kap>
  <snc mrk="adenin.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <frm>C<sub>5</sub>H<sub>5</sub>N<sub>5</sub></frm>, unu el la kvin
      nukleo-bazoj
      de DNA kaj RNA:
      <ekz>
        en DNA, <tld/>o ligi&gcirc;as al <ref cel="timin.0o">timino</ref>, kaj 
        <ref cel="guanin.0o">guanino</ref> ligi&gcirc;as al citosino
        <fnt>
          <aut>Ste&ccirc;jo Norvel</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas purina nitrogen-bazo, kiu povas aperi en 
        <ref tip="malprt" cel="nukleotid.0o.GNT">nukleotido</ref>j de DNA kaj RNA
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Adenino">
          adenino</url>, 2010-07-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x817A;&#x560C;&#x5464; <pr>xi&#xE0;nbi&#x101;ol&#xED;ng</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_d;&c_e;&c_n;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_a;&c_d;&c_je;&c_n;&c_i;&c_n;</trd>
  <trd lng="ca">adenina</trd>
  <trd lng="da">adenin</trd>
  <trd lng="de">Adenin</trd>
  <trd lng="en">adenine</trd>
  <trd lng="es">adenina</trd>
  <trd lng="fr">ad&eacute;nine</trd>
  <trd lng="hu">adenin</trd>
  <trd lng="it">adenina</trd>
  <trd lng="ja">&#x30A2;&#x30C7;&#x30CB;&#x30F3;</trd>
  <trd lng="nl">adenine</trd>
  <trd lng="pl">adenina</trd>
  <trd lng="ro">adenin&abreve;</trd>
  <trd lng="sv">adenin</trd>
  <trd lng="uk">&c_a;&c_d;&c_je;&#x43D;&c_ib;&#x43D;</trd>
</drv>
</art>

