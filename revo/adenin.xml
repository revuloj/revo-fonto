<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adenin</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="adenin.0o">
  <kap><tld/>o</kap>
  <snc mrk="adenin.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Iu el la
      <ref tip="super" cel="nukleotid.0o.GNT">nukleotido</ref>specoj
      de ADN a&ubreve; ARN;
      simbole <frm>C<sub>5</sub>H<sub>5</sub>N<sub>5</sub></frm>:
      <ekz>
        <tld/>o estas purina nitrogen-bazo, kiu povas aperi en 
        nukleotidoj de DNA kaj RNA
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Adenino">
          adenino</url>, 2010-07-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ad&eacute;nine</trd>
  <trd lng="sv">adenin</trd>
</drv>
</art>

