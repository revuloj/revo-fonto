<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: acet.xml,v 1.25 2021/02/28 09:26:17 revo Exp $">
<kap>
  <rad>acet</rad>/o
</kap>

<drv mrk="acet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="vinagr.0o">Vinagro</ref>:
      <ekz>
        Ja&ccirc;jo &scirc;telis hejme botelon da <tld/>o, ni &scirc;utis en &gcirc;in pistitan kreton, la
        <tld/>o eksiblis kiel &scirc;a&ubreve;makvo, kaj sur la fundon falis griza pulvoro &mdash;
        kalcia acetato
        <fnt>
          <aut>Genri&hcirc; Altov, trad. Jurij Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/TRIZ/angulo.html">Angulo de atako</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_c;&c_a;&c_t;</trd>
  <trd lng="bg">&c_o;&c_c;&c_je;&c_t;</trd>
  <trd lng="ca">vinagre</trd>
  <trdgrp lng="cs">
    <trd>kyselina octov&aacute;</trd>,
    <trd>ocet</trd>
  </trdgrp>
  <trd lng="de">Essig</trd>
  <trd lng="en">vinegar</trd>
  <trd lng="es">&aacute;cido <ind>acetico</ind></trd>
  <trd lng="fa">&a_s;&a_r;&f_k;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>vinaigre</trd>,
    <trd>acide ac&eacute;tique</trd>
  </trdgrp>
  <trd lng="he">&#x5D7;&#x5D5;&#x5DE;&#x5E5;</trd>
  <trd lng="hu">ecetsav</trd>
  <trd lng="it">aceto</trd>
  <trd lng="nl">azijn</trd>
  <trd lng="pl">ocet</trd>
  <trdgrp lng="pt">
    <trd>aceto</trd>,
    <trd>vinagre</trd>
  </trdgrp>
  <trd lng="ro">o&tcedil;et</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_s;&c_u;&c_s;</trd>,
    <trd>&c_u;&c_k;&c_s;&c_u;&c_s;&c_n;&c_a;&c_ja; &c_e;&c_s;&c_s;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">ocot</trd>
  <trd lng="sv">vin&auml;ger</trd>
</drv>
</art>
