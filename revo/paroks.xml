<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>paroksism</rad>/o
</kap>
<drv mrk="paroks.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plej alta grado de intenseco de iu sento, pasio a&ubreve; malsano:
      <ekz>
        en <tld/>o de kolero, doloro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="eg.0eco">egeco</ref>,
      <ref cel="kulmin.0o">kulmino</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Paroxysmus</trd>
  <trd lng="nl">paroxisme</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_a;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_o;&c_k;&c_s;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
</drv>
</art>
