<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc>
     <rad>Venezuel</rad>/o <fnt><bib>PIV1</bib></fnt></kap>

<drv mrk="venezuel.V0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Venezuelo">Venezuelo</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>,
      limanta al 
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>,
      <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="gujan.G0o.sxtato">Gujano</ref> kaj 
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>. 
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_e;&c_n;&c_e;&c_s;&c_u;&c_e;&c_l;&c_a;</trd>
  <trd lng="bg">&c_V;&c_je;&c_n;&c_je;&c_c;&c_u;&c_je;&c_l;&c_a;</trd>
  <trd lng="ca">Vene&ccedil;uela</trd>
  <trd lng="cs">Venezuela</trd>
  <trd lng="de">Venezuela</trd>
  <trd lng="en">Venezuela</trd>
  <trdgrp lng="fr">
  <trd>Venezuela</trd>,
  <trd>V&eacute;n&eacute;zu&eacute;la</trd>
  </trdgrp>
  <trd lng="hu">Venezuela</trd>
  <trd lng="id">Venezuela</trd>
  <trd lng="nl">Venezuela</trd>
  <trd lng="no">Venezuela</trd>
  <trd lng="pl">Wenezuela</trd>
  <trd lng="pt">Venezuela</trd>
  <trd lng="sk">Venezuela</trd>
</drv>

<drv mrk="venezuel.0ano">
  <kap><tld lit="v"/>ano <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Blankaj venezuelanoj">Blankaj
        venezuelanoj</url></lok></fnt>,
  <var><kap><tld lit="v"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Venezuelo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>V&eacute;n&eacute;zu&eacute;lien</trd>
    <trd>V&eacute;n&eacute;zu&eacute;lienne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="venezuel.0a">
  <kap><tld lit="v"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Venezuelo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>v&eacute;n&eacute;zu&eacute;lien</trd>
    <trd>v&eacute;n&eacute;zu&eacute;lienne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>


