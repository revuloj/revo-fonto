<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Venezuel</rad>/o</kap>
<drv mrk="venezuel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>, limanta
      al 
      <ref tip="vid" cel="kolombi.0o">Kolombio</ref>,
      <ref tip="vid" cel="brazil.0o">Brazilo</ref>,
      <ref tip="vid" cel="gvajan.0o">Gvajano</ref> kaj 
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>. 
    </dif>
  </snc>
  <trd lng="bg">&c_V;&c_je;&c_n;&c_je;&c_c;&c_u;&c_je;&c_l;&c_a;</trd>
  <trd lng="de">Venezuela</trd>
  <trd lng="hu">Venezuela</trd>
  <trd lng="pt">Venezuela</trd>
</drv>
</art>


