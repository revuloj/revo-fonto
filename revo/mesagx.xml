<drv mrk="mesagx.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc mrk="mesagx.0isto.transportisto">
    <dif>
      Transportisto de <tld/>oj:
      <ekz>
        la Morto estas cetere la plej certa <tld/>isto, malgra&ubreve; siaj diversaj okupoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        la usona filmo <tld lit="M"/>isto
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2005/008414.php">Rado-brigado</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">BIO</uzo>
        por &ccirc;iuj tri nukleotidoj en la <ind><tld/>ista RNA</ind> estos unu la&ubreve;kode specifita
        aminoacido
        <fnt>
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>.
        <trd lng="de">Boten-RNA</trd>
        <trd lng="en">messenger RNA</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="kurier.0o.POSX">kuriero</ref>
    <ref tip="vid" cel="send.0ito">sendito</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_r;&#x2019;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_a;&c_n;&c_je;&c_c;</trd>,
