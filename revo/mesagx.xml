<drv mrk="mesagx.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mesagx.0i.TEL">
    <dif>
      Sendi <tld/>on ien:
      <ekz>
        politikistoj interrompis siajn feriojn por <tld/>i unu al la alia pri la krizo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010478.php">Malstulta sezono</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efministro <tld/>is al la mondo, ke enkondukata estas nova erao en la japana
        politiko
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009924.php">Hatojama Jukio: nova
          &ccirc;efministro</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>is al la familianoj &ccirc;e kiu tombejo ku&scirc;as
        nekonata korpo
        <fnt><aut>S. &Scirc;timec</aut>, <vrk>Morto en Melburna
        tramo</vrk>, <bib>Monato</bib>, <lok>jaro 1997a, numero 8a, p.
        24a</lok></fnt>;
      </ekz>
      <ekz>
        la viro <tld/>is al retpa&gcirc;o pri sinmortigo, anoncante, ke li ser&ccirc;as homojn por
        kunmorti kun li
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008113.html">Forpaso-forumo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="inform.0i">informi</ref>,
       <ref cel="sci.0igi">sciigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">
  &c_d;&c_a;&c_s;&c_l;&c_a;&c_c;&c_mol;
  <ind>
  &c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
  </ind>
  </trd>
  <trdgrp lng="de">
    <trd>benachrichtigen</trd>,
    <trd>mitteilen</trd>
  </trdgrp>
  <trd lng="en">send a message</trd>
  <trd lng="es">enviar un <ind>mensaje</ind></trd>
  <trd lng="fr">envoyer un <ind>message</ind></trd>
  <trd lng="hu">&uuml;zen</trd>
  <trdgrp lng="ja">
    <trd>&#x4F1D;&#x8A00;&#x3059;&#x308B;  <pr>&#x3067;&#x3093;&#x3054;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8A00;&#x3065;&#x3051;&#x308B;  <pr>&#x3053;&#x3068;&#x3065;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">een <ind>bericht</ind> zenden</trd>
  <trdgrp lng="pl">
    <trd>wysy&lstroke;a&cacute; <ind>wiadomo&sacute;&cacute;</ind>
    </trd>,
    <trd>komunikowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">
  &c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol;
  <ind>&c_s;&c_o;&c_o;&c_b;&c_shch;&c_je;&c_n;&c_i;&c_je;</ind>
  </trd>
  <trdgrp lng="zh">
    <trd>&#x53D1;&#x77ED;&#x4FE1; <pr>f&#x101;du&#x1CE;nx&#xEC;n</pr></trd>,
    <trd>&#x767C;&#x77ED;&#x4FE1; <pr>f&#x101;du&#x1CE;nx&#xEC;n</pr></trd>,
