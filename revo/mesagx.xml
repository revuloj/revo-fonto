    <trd>&#x3053;&#x3068;&#x3065;&#x3066;</trd>,
    <trd>&#x30E1;&#x30C3;&#x30BB;&#x30FC;&#x30B8;  <pr>&#x3081;&#x3063;&#x305B;&#x30FC;&#x3058;</pr></trd>,
    <trd>&#x304A;&#x544A;&#x3052;  <pr>&#x304A;&#x3064;&#x3052;</pr></trd>,
    <trd>&#x8A17;&#x5BA3;  <pr>&#x305F;&#x304F;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x6559;&#x66F8;  <pr>&#x304D;&#x3087;&#x3046;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="nl">bericht</trd>
  <trdgrp lng="pl">
