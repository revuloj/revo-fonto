<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <ofc>7</ofc>
  <rad>Klar</rad>/o <fnt><bib/></fnt>
</kap>
<drv mrk="klar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klar1.0o.nomo">
   <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        brilis la okuloj de la filino, la &ccirc;arma fra&ubreve;lino <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fra&ubreve;lino <tld/>o kaj fra&ubreve;lino Kristino ne povas plenumi la tutan kudradon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Klara</trd>
  </snc>
  <trd lng="de"/>
  <trdgrp lng="en">
   <trd>Claire</trd>
   <trd>Clair</trd>
   <trd>Clare</trd>
  </trdgrp>  
  <trdgrp lng="fr">
   <trd>Claire</trd>
   <trd>Clara</trd>
  </trdgrp>  
   <trd lng="es">Clara</trd>
  <trdgrp lng="it">
   <trd>Chiara</trd>
   <trd>Clara</trd>
  </trdgrp>  
   <trd lng="pt">Clara</trd>
</drv>
<drv mrk="klar1.0njo">
  <kap><ofc>*</ofc> Klanjo</kap>
  <snc>
   <dif>
    Karesformo de la nomo <tld/>o:
      <ekz>
        Johanon, ... <tld/>on, ... iliaj gepatroj nomas Johan&ccirc;jo
        (a&ubreve; Jo&ccirc;jo), ... Klanjo, ...
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">Kl&auml;rchen</trd>
  <trdgrp lng="fr">
    <trd>Clairette</trd>
    <trd>"Clarinette"</trd>
  </trdgrp>
</drv></art>


