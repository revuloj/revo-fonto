<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <ofc>7</ofc>
  <rad>Klar</rad>/o <fnt><bib></bib></fnt>
</kap>
<drv mrk="klar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klar1.0o.SNC">
   <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
      <ekz>
        <fnt><lok>Akademio de Esperanto, 7OA</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
   <trd>Claire</trd>
   <trd>Clair</trd>
   <trd>Clare</trd>
  </trdgrp>  
  <trdgrp lng="fr">
   <trd>Claire</trd>
   <trd>Clara</trd>
  </trdgrp>  
   <trd lng="es">Clara</trd>
  <trdgrp lng="it">
   <trd>Chiara</trd>
   <trd>Clara</trd>
  </trdgrp>  
   <trd lng="pt">Clara</trd>
</drv>
<drv mrk="klar1.0njo">
  <kap><ofc>*</ofc> Klanjo</kap>
  <snc>
   <dif>
    Karesformo de la nomo <tld/>o:
      <ekz>
        Johanon, ... <tld/>on, ... iliaj gepatroj nomas Johan&ccirc;jo
        (a&ubreve; Jo&ccirc;jo), ... Klanjo, ...
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Clairette</trd>
    <trd>"Clarinette"</trd>
  </trdgrp>
</drv></art>


