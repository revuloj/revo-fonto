<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klar1.xml,v 1.6 2019/02/28 17:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <ofc>7</ofc>
  <rad>Klar</rad>/o <fnt><bib/></fnt>
</kap>
<drv mrk="klar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klar1.0o.nomo">
   <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        brilis la okuloj de la filino, la &ccirc;arma fra&ubreve;lino <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o tre dezirus adia&ubreve;i la amikinon, sed ne trovas pretekston
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        fra&ubreve;lino <tld/>o kaj fra&ubreve;lino Kristino ne povas plenumi la tutan
        kudradon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Klara</trd>
  </snc>
  <ref tip="hom" cel="klar.0a">klara</ref>
  <trdgrp lng="en">
   <trd>Claire</trd>
   <trd>Clair</trd>
   <trd>Clare</trd>
  </trdgrp>  
  <trdgrp lng="fr">
   <trd>Claire</trd>
   <trd>Clara</trd>
  </trdgrp>  
   <trd lng="es">Clara</trd>
  <trdgrp lng="it">
   <trd>Chiara</trd>
   <trd>Clara</trd>
  </trdgrp>  
  <trd lng="pl">Klara</trd>
  <trd lng="pt">Clara</trd>
</drv>
<drv mrk="klar1.0njo">
  <kap><ofc>*</ofc> Klanjo</kap>
  <snc>
   <dif>
    Karesformo de la nomo <tld/>o:
      <ekz>
        Johanon, <klr>[&#8230;]</klr> <tld/>on, <klr>[&#8230;]</klr> iliaj gepatroj nomas Johan&ccirc;jo
        (a&ubreve; Jo&ccirc;jo), <klr>[&#8230;]</klr> Klanjo
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">Kl&auml;rchen</trd>
  <trdgrp lng="fr">
    <trd>Clairette</trd>
    <trd>"Clarinette"</trd>
  </trdgrp>
  <trd lng="pl">Klarcia</trd>
 </drv>
 <drv mrk="klar1.0anino">
  <kap><tld/>anino</kap>
  <snc mrk="klar1.0anino.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="dif" cel="monahx.0ino">Mona&hcirc;ino</ref> el la 
      <ref tip="dif" cel="orden.0o">ordeno</ref> de
      sankta <tld/>o:
      <ekz>
        kunlaborantino de Sankta Francisko el Asizo kaj fondintino de la mon&hcirc;inoj
        <tld/>aninoj, &scirc;i estis proklamita sankta de papo Aleksandro la 4-a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=422297">Klara el Asizo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Mona&hcirc;ejo de la Re&gcirc;aj Sen&scirc;uistinoj <klr>[&#8230;]</klr> estas mona&hcirc;ejo de
        <klr>[&#8230;]</klr> klaraninoj, fondita en 1559 de Johanino de A&ubreve;strio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=434206">Mona&hcirc;ejo de la Re&gcirc;aj Sen&scirc;uistinoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Klarisse</trd>
   <trd lng="fr">"Clarisse"</trd>
  <trd lng="pl">klaryska</trd>
</drv>
</art>


