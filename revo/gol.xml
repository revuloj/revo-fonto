<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gol</rad>/o
</kap>
<drv mrk="gol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Rezulto de sporta ludo atingata per &jcirc;eto de pilko en la celon. 
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_l;</trd>
  <trd lng="de">Tor</trd>
  <trd lng="es">gol</trd>
  <trdgrp lng="fr">
    <trd>but<klr tip="ind"> (point marqu&eacute;)</klr></trd>,
    <trd>goal<klr tip="ind"> (point marqu&eacute;)</klr></trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>g&oacute;l</trd>,
    <trd>tal&aacute;lat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doel<klr tip="ind"> (doelpunt)</klr></trd>,
    <trd>doelpunt</trd>
  </trdgrp>
  <trd lng="pt">gol<klr tip="amb"> (ponto, tento)</klr></trd>
  <trd lng="ru">&c_g;&c_o;&c_l;</trd>
</drv>

<drv mrk="gol.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Kadro tra kiu devas esti &jcirc;etata la pilko por atingi <tld/>on. 
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_a; <klr>(&c_u; &c_g;&c_u;&c_l;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trd lng="de">Tor</trd>
  <trd lng="es">porter&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>but<klr tip="ind"> (cage de but)</klr></trd>,
    <trd>cage de but</trd></trdgrp> 
  <trd lng="hu">kapu<klr tip="ind"> (sport)</klr></trd>
  <trdgrp lng="nl">
    <trd>doel</trd>,
    <trd>goal</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>balizas<klr tip="amb"> (futebol)</klr></trd>, 
   <trd>traves<klr tip="amb"> (futebol)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_t;&c_a;</trd>
</drv>

<drv mrk="gol.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu el la trupo, kiu gardas la <tld/>ejon, por eviti la en&jcirc;eton de
      la pilko.  
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k; <klr>(&c_u; &c_g;&c_u;&c_l;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trdgrp lng="de">
     <trd>Torwart</trd>,
     <trd>Torh&uuml;ter</trd></trdgrp>
  <trdgrp lng="es">
     <trd>portero</trd>,
     <trd>guardameta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gardien de but</trd>,
    <trd>goal<klr tip="ind"> (gardien de but)</klr></trd></trdgrp> 
  <trd lng="hu">kapus<klr tip="ind"> (sport)</klr></trd>
  <trdgrp lng="nl">
    <trd>doelman</trd>,
    <trd>doelverdediger</trd>,
    <trd>keeper</trd>,
    <trd>goalkeeper</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>goleiro</trd>, 
   <trd>arqueiro</trd>, 
   <trd>guarda-redes</trd> 
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_a;&c_t;&c_a;&c_r;&c_mol;</trd>
</drv>
</art>
