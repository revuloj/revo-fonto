<kap>
  <ofc>*</ofc>
  <rad>meti</rad>/o
</kap>
<drv mrk="meti.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="labor.man0o">Manlabor</ref>a <ref tip="super" cel="profes.0o">profesio</ref>:
      <ekz>
        ili estis la&ubreve; <tld/>o tendofaristoj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 18:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tria filo fari&gcirc;is lernanto de tornisto kaj &ccirc;ar la <tld/>o estas malfacila, li
        lernis pli longe ol la fratoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        al posedanto de <tld/>o mankas nenio
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>ne rezonu pri tio, kio estas ekster via <tld/>o</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de"><ind>Schuster</ind>, bleib bei deinem Leisten</trd>
      </ekz>
      <ekz>
        &ccirc;iu klopodu nur en sia <tld/>o, tiam al la urbo mankos nenio
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="okup.0o">okupo</ref>,
      <ref cel="ofic.0o">ofico</ref>,
      <ref cel="specia">specialo</ref>,
      <ref cel="fak.0o">fako</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_ja;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="br">micher dorn</trd>
  <trd lng="cs">&rcaron;emeslo</trd>
  <trdgrp lng="de">
    <trd>Handwerk</trd>,
    <trd>Gewerbe</trd>
  </trdgrp>
  <trd lng="el">&chi;&epsilon;&jota;&rho;&omega;&ny;&alfa;&kappa;&tau;&jota;&kappa;&omikron_ton;
  &epsilon;&pi;&alfa_ton;&gamma;&gamma;&epsilon;&lambda;&my;&alfa;</trd>
  <trdgrp lng="en">
    <trd>blue-collar <ind>profession</ind></trd>,
    <trd>trade</trd>,
    <trd>handicraft</trd>
  </trdgrp>
  <trd lng="es">oficio manual</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;tier</trd>,
    <trd><ind>profession</ind> manuelle</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mesters&eacute;g</trd>,
    <trd>k&eacute;zm&udblac;ves szakma</trd>,
    <trd>k&eacute;zm&udblac;ipar</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>lavoro da <ind>operaio</ind></trd>,
    <trd>lavoro manuale</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x624B;&#x8077;  <pr>&#x3066;&#x3057;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x624B;&#x4ED5;&#x4E8B;  <pr>&#x3066;&#x3057;&#x3054;&#x3068;</pr></trd>,
    <trd>&#x8077;&#x696D;  <pr>&#x3057;&#x3087;&#x304F;&#x304E;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">ambacht</trd>
  <trd lng="pl">rzemios&lstroke;o</trd>
  <trdgrp lng="pt">
   <trd>of&iacute;cio <klr>(profiss&atilde;o)</klr></trd>,
   <trd>mister <klr>(profiss&atilde;o)</klr></trd>,
   <trd>arte <klr>(profiss&atilde;o)</klr></trd>,
   <trd>ind&uacute;stria <klr>(profiss&atilde;o)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">remeslo</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x435;&#x43C;&#x435;&#x441;&#x43B;&#x43E;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x43C;&#x438;&#x441;&#x435;&#x43B;</trd>,
    <trd>&#x43C;&#x430;&#x439;&#x441;&#x442;&#x435;&#x440;&#x441;&#x442;&#x432;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x624B;&#x827A; <pr>sh&#x1D2;uy&#xEC;</pr></trd>,
    <trd>&#x624B;&#x85DD; <pr>sh&#x1D2;uy&#xEC;</pr></trd>,
    <trd>&#x624B;&#x5DE5;&#x4E1A; <pr>sh&#x1D2;ug&#x14D;ngy&#xE8;</pr></trd>,
    <trd>&#x624B;&#x5DE5;&#x696D; <pr>sh&#x1D2;ug&#x14D;ngy&#xE8;</pr></trd>,
    <trd>&#x5DE5; <pr>g&#x14D;ng</pr></trd>,
    <trd>&#x624B;&#x5DE5;&#x827A; <pr>sh&#x1D2;ug&#x14D;ngy&#xEC;</pr></trd>,
    <trd>&#x624B;&#x5DE5;&#x85DD; <pr>sh&#x1D2;ug&#x14D;ngy&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="meti.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="lok.0o.spaca">Loko</ref>, &ccirc;ambro destinita por manlaboro:
      <ekz>
        <tld/>ejo de gantisto
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> montris al mi kabineton de meza grandeco, en kiu mi de la unua
        rigardo ekkonis <tld/>ejon de botisto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la aliaj submajstroj de la <tld/>ejo demandis lin, kial li tiel senripoze
        <klr>[&#x2026;]</klr> meditas
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="atelier.0o">ateliero</ref>,
       <ref cel="garagx.0o">gara&gcirc;o</ref>,
       <ref cel="manufa.0o">manufakturo</ref>,
       <ref cel="ripar.0ejo">riparejo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_n;&c_ja;</trd>
  <trd lng="br">labouradeg</trd>
  <trd lng="de">Werkstatt</trd>
  <trdgrp lng="en">
    <trd>workplace</trd>,
    <trd>jobsite</trd>
  </trdgrp>
  <trd lng="es">lugar de trabajo manual</trd>
  <trd lng="fr">atelier</trd>
  <trd lng="hu">m&udblac;hely</trd>
  <trd lng="it">luogo di lavoro</trd>
  <trdgrp lng="ja">
    <trd>&#x4ED5;&#x4E8B;&#x5834;  <pr>&#x3057;&#x3054;&#x3068;&#x3070;</pr></trd>,
    <trd>&#x5DE5;&#x623F;  <pr>&#x3053;&#x3046;&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">werkplaats</trd>
  <trd lng="pl">warsztat rzemie&sacute;lniczy</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  <trd lng="sk">diel&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x439;&#x441;&#x442;&#x435;&#x440;&#x43D;&#x44F;</trd>,
    <trd>&#x430;&#x442;&#x435;&#x43B;&#x44C;&#x454;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5DE5;&#x573A; <pr>g&#x14D;ngch&#x1CE;ng</pr></trd>,
    <trd>&#x5DE5;&#x5834; <pr>g&#x14D;ngch&#x1CE;ng</pr></trd>,
    <trd>&#x8ECA;&#x9593; <pr>ch&#x113;ji&#x101;n</pr></trd>,
    <trd>&#x8F66;&#x95F4; <pr>ch&#x113;ji&#x101;n</pr></trd>,
    <trd>&#x4F5C;&#x574A; <pr>zu&#x14D;fang</pr></trd>,
    <trd>&#x574A; <pr>f&#xE1;ng</pr></trd>,
    <trd>&#x5DE5;&#x574A; <pr>g&#x14D;ngf&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="meti.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="hom.0o">Homo</ref> profesie manlaboranta:
      <ekz>
        Demetrio, ar&gcirc;enta&jcirc;isto, <klr>[&#x2026;]</klr> liveris al la <tld/>istoj multe da okupo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis sam<tld/>isto
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 18:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> egipta popolo, dividi&gcirc;is en du klasojn: terkulturistoj kaj
        <tld/>istoj
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        brava homo, <tld/>isto, svati&gcirc;as al vi, la gantisto <nom>Erik</nom>
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;dis marteladon kaj frapadon, &ccirc;ar &ccirc;iu <tld/>isto havis sian 
        <ref cel="stabl.0o.metitablo">stablo</ref>n en la
        pordo a&ubreve; sur la trotuaro
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj aliaj, komercistoj, <tld/>istoj kaj pa&scirc;tistoj levi&gcirc;is, dankis
        <klr>[&#x2026;]</klr> kaj foriris hejmen
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="gild.0o">gildo</ref>
    <ref tip="sub" cel="majstr.0o.lertulo">majstro<sncref/></ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_je;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="br">
    <trd>artizan</trd>,
    <trd>dornwezhour</trd>
  </trdgrp> 
  <trd lng="de">Handwerker</trd>
  <trdgrp lng="en">
    <trd>blue-collar <ind>worker</ind></trd>,
    <trd>tradesman</trd>
  </trdgrp>
  <trd lng="es">operario</trd>
  <trd lng="fr">artisan</trd>
  <trdgrp lng="hu">
    <trd>mesterember</trd>,
    <trd>k&eacute;zm&udblac;ves</trd>,
    <trd>iparos</trd>
  </trdgrp>
  <trd lng="it">operaio</trd>
  <trdgrp lng="ja">
    <trd>&#x8077;&#x4EBA;  <pr>&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x719F;&#x7DF4;&#x5DE5;  <pr>&#x3058;&#x3085;&#x304F;&#x308C;&#x3093;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">ambachtsman</trd>
  <trd lng="pl">rzemie&sacute;lnik</trd>
  <trd lng="ru">&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">remeseln&iacute;k</trd>
  <trd lng="uk">&#x440;&#x435;&#x43C;&#x456;&#x441;&#x43D;&#x438;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x5320; <pr>ji&#xE0;ng</pr></trd>,
    <trd>&#x5DE5;&#x5320; <pr>g&#x14D;ngji&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

