<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>meti</rad>/o
</kap>
<drv mrk="meti.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Manlabora profesio:
      <ekz>&ccirc;iu klopodu nur en sia <tld/>o, tiam al la urbo mankos
        nenio
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bala.0i">balai</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="okup.0o">okupo</ref>,
      <ref cel="ofic.0o">ofico</ref>,
      <ref cel="specia">specialo</ref>,
      <ref cel="fak.0o">fako</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Handwerk</trd>,
    <trd>Gewerbe</trd>
  </trdgrp>
  <trd lng="be">&c_r;&c_a;&c_m;&c_ja;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="cs">&rcaron;emeslo</trd>
  <trd lng="en">blue-collar <ind>profession</ind></trd>
  <trd lng="nl">ambacht</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">remeslo</trd>
</drv>

<drv mrk="meti.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, &ccirc;ambro destinita por manlaboro.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_n;&c_ja;</trd>
  <trd lng="de">Werkstatt</trd>
  <trdgrp lng="en">
    <trd>workplace</trd>,
    <trd>jobsite</trd>
  </trdgrp>
  <trd lng="nl">werkplaats</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  <trd lng="sk">diel&ncaron;a</trd>
</drv>

<drv mrk="meti.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Homo profesie manlaboranta.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_je;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Handwerker</trd>
  <trd lng="en">blue-collar <ind>worker</ind></trd>
  <trd lng="nl">ambachtsman</trd>
  <trd lng="ru">&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">remeseln&iacute;k</trd>
</drv>

<drv mrk="meti.ne0ulo">
  <kap>
    ne<tld/>ulo
  </kap>
  <snc>
    <dif>
      <ref tip="sin" cel="amator.0o">Amatoro</ref> 2.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_f;&c_e;&c_s;&c_ib;&c_ja;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_y;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Laie</trd>
  <trd lng="en">layman</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
