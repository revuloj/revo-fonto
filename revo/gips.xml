<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gips.xml,v 1.22 2015/07/04 08:37:35 revo Exp $">
<kap><ofc>*</ofc>
<rad>gips</rad>/o</kap>
<drv mrk="gips.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pulvorigita kalka &scirc;tono, uzata en konstruarto por &scirc;miri a&ubreve; tegi
      murojn.
    </dif>
  </snc>
  <adm>
    La &leftquot;kalka sxtono&rightquot; aperanta en la difino fakte ne estas ia
    ajn, sed gxi estas gipsosxtono (enkondukota). <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>guix</trd>,
    <trd>algeps</trd>
  </trdgrp>
  <trd lng="de">Gips</trd>
  <trdgrp lng="en">
    <trd>gypsum</trd>,
    <trd>plaster of Paris</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&f_ch;</trd>
  <trd lng="fr">pl&acirc;tre</trd>
  <trd lng="hu">gipsz</trd>
  <trdgrp lng="nl">
    <trd>gips</trd>,
    <trd>plaaster</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gesso</trd>,
   <trd>gipso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_p;&c_s;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>s&inodot;va</trd>,
    <trd>al&ccedil;&inodot;</trd>
  </trdgrp>
  <trd lng="sk">sadra</trd>
  <trdgrp lng="cs">
    <trd>gyps</trd>,
    <trd>s치dra</trd>
  </trdgrp>
</drv>

<drv mrk="gips.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri, tegi per <tld/>o:
      <ekz>
        <tld/>i plafonon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stuk.0i">stuki</ref>.
    </refgrp>
    <trd lng="be">&c_g;&c_ib;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">gipsen</trd>
    <trd lng="en">plaster</trd>
    <trd lng="fa">&f_g;&f_ch; &f_g;&a_r;&a_f;&a_t;&a_n;</trd>
    <trd lng="fr">pl&acirc;trer</trd>
    <trd lng="hu">begipszel</trd>
    <trdgrp lng="nl">
      <trd>pleisteren</trd>,
      <trd>in het <ind>gips</ind> zetten</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>gessar</trd>,
     <trd>estucar com gesso</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_i;&c_p;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="tr">s&inodot;valamak</trd>
  </snc>
  <snc>
    <dif>
      Miksi <tld/>on al io, sur&scirc;uti per <tld/>o:
      <ekz>
        <tld/>i vinon por &gcirc;in klarigi, kampon por &gcirc;in plibonigi.
      </ekz>
    </dif>
    <trd lng="de">kalken</trd>
    <trd lng="fr">chauler</trd>
    <trd lng="hu">meszez</trd>
    <trd lng="nl">gips toevoegen</trd>
    <trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>    
  </snc>
  <trd lng="ca">enguixar</trd>
  <trdgrp lng="cs">
    <trd>s치drovat</trd>,
    <trd>zas치drovat</trd>
  </trdgrp>
  <trd lng="sk">sadrova콘</trd>
</drv>
</art>
