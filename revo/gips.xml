<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gips</rad>/o</kap>
<drv mrk="gips.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pulvorigita kalka &scirc;tono, uzata en konstruarto por &scirc;miri a&ubreve; tegi
      murojn.
    </dif>
  </snc>
  <adm>
    La &leftquot;kalka sxtono&rightquot; aperanta en la difino fakte ne estas ia
    ajn, sed gxi estas gipsosxtono (enkondukota). <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_s;</trd>
  <trd lng="de">Gips</trd>
  <trd lng="fr">pl&acirc;tre</trd>
  <trd lng="hu">gipsz</trd>
  <trd lng="nl">gips</trd>
  <trdgrp lng="pt">
   <trd>gesso</trd>, 
   <trd>gipso</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_s;</trd>
</drv>

<drv mrk="gips.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri, tegi per <tld/>o:
      <ekz>
        <tld/>i plafonon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stuk.0i">stuki</ref>.
    </refgrp>
    <trd lng="be">&c_g;&c_ib;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">gipsen</trd>
    <trd lng="fr">pl&acirc;trer</trd>
    <trd lng="hu">begipszel</trd>
    <trd lng="nl">gipsen</trd>
    <trdgrp lng="pt">
     <trd>gessar</trd>, 
     <trd>estucar com gesso</trd> 
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Miksi <tld/>on al io, sur&scirc;uti per <tld/>o:
      <ekz>
        <tld/>i vinon por &gcirc;in klarigi, kampon por &gcirc;in plibonigi.
      </ekz>
    </dif>
    <trd lng="de">kalken</trd>
    <trd lng="hu">meszez</trd>
  </snc>
</drv>
</art>
