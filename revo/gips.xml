<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gips.xml,v 1.30 2023/11/11 11:36:35 revo Exp $">
<kap><ofc>*</ofc>
<rad>gips</rad>/o</kap>
<drv mrk="gips.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gips.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="pulvor.0o">Pulvoro</ref>, produktita el <tld/>&scirc;tono, mineralo   
      <frm>Ca[SO<sub>4</sub>]&middot;2H<sub>2</sub>O</frm>, uzata en konstruarto kaj modlado:
      <ekz>
        <klr>[tie]</klr> staris blankaj homoj, <klr>[&#x2026;]</klr> parte el marmoro, parte el
        <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        per batoj de martelo li faligis kaj pulvorigis <klr>[&#x2026;]</klr> la vitrajn
        rompa&jcirc;ojn enmetitajn en la <tld/>on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        masko el <tld/>o
        <fnt>
          <aut>M. Proust, trad. J.-L. Tortel</aut>
          <vrk>Unu amo de Swann</vrk>
          <lok>2019</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>figuroj sur la komodo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        modelante el <tld/>o frison la&ubreve; antikva modelo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <adm>
    La &leftquot;kalka &scirc;tono&rightquot; aperanta en la difino fakte ne estas ia
    ajn, sed &gcirc;i estas gipso&scirc;tono (enkondukota). <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>guix</trd>,
    <trd>algeps</trd>
  </trdgrp>
  <trd lng="de">Gips</trd>
  <trdgrp lng="en">
    <trd>gypsum</trd>,
    <trd>plaster of Paris</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&f_ch;</trd>
  <trd lng="fr">pl&acirc;tre</trd>
  <trd lng="hu">gipsz</trd>
  <trdgrp lng="ja">
    <trd>&#x713C;&#x77F3;&#x818F;  <pr>&#x3084;&#x305B;&#x3063;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x30AE;&#x30D7;&#x30B9;</trd>,
    <trd>&#x77F3;&#x818F;  <pr>&#x305B;&#x3063;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gips</trd>,
    <trd>plaaster</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gesso</trd>,
   <trd>gipso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_p;&c_s;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>s&inodot;va</trd>,
    <trd>al&ccedil;&inodot;</trd>
  </trdgrp>
  <trd lng="sk">sadra</trd>
  <trdgrp lng="cs">
    <trd>gyps</trd>,
    <trd>s&aacute;dra</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_ib;&c_p;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x77F3;&#x818F; <pr>sh&#xED;g&#x101;o</pr></trd>,
    <trd>&#x786B;&#x9178;&#x9223; <pr>li&#xFA;su&#x101;ng&#xE0;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gips.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <tld/>aj figuroj
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kopioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jano faris <tld/>an mulda&jcirc;on de la spuroj
        <fnt>
          <bib>ChV</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        montrante sur la &scirc;ranko malgrandan <tld/>an kapon, dividitan en numeritajn
        kvarangulojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_s;&c_a;&c_v;&c_y;</trd>
</drv>

<drv mrk="gips.0i">
  <kap><tld/>i, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gips.0i.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      &Scirc;miri, tegi, ripari per <tld/>o:
      <ekz>
        <tld/>i plafonon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj lirikoj restas <tld/>umitaj: kelkaj vere bonaj, pli multaj &scirc;ablonaj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo MINNAJA</aut>
          <vrk><url ref="&Monato;/2003/008065.html">Finaj agordoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stuk.0i">stuki</ref>
    </refgrp>
    <trd lng="be">&c_g;&c_ib;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">gipsen</trd>
    <trd lng="en">plaster</trd>
    <trd lng="fa">&f_g;&f_ch; &f_g;&a_r;&a_f;&a_t;&a_n;</trd>
    <trd lng="fr">pl&acirc;trer</trd>
    <trd lng="hu">begipszel</trd>
    <trdgrp lng="nl">
      <trd>pleisteren</trd>,
      <trd>in het <ind>gips</ind> zetten</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>gessar</trd>,
     <trd>estucar com gesso</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_i;&c_p;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="tr">s&inodot;valamak</trd>
    <trdgrp lng="zh">
    <trd>&#x5857;&#x4EE5;&#x818F;&#x6CE5; <pr>t&#xFA;y&#x1D0;g&#x101;on&#xED;</pr></trd>,
    <trd>&#x6577;&#x818F;&#x836F; <pr>f&#x16B;g&#x101;oy&#xE0;o</pr></trd>,
    <trd>&#x6577;&#x4EE5;&#x77F3;&#x818F; <pr>f&#x16B;y&#x1D0;sh&#xED;g&#x101;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="gips.0i.enmeti">
    <dif>
      Miksi <tld/>on al io, sur&scirc;uti per <tld/>o:
      <ekz>
        <tld/>i vinon por &gcirc;in klarigi;
      </ekz>
      <ekz>
        <tld/>i kampon por &gcirc;in plibonigi.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_m;&c_je;&c_sh;&c_v;&c_a;&c_c;&c_mol; &c_z; <ind>&c_g;&c_ib;&c_p;&c_s;&c_a;&c_m;</ind></trd>
    <trd lng="de">kalken</trd>
    <trd lng="fr">chauler</trd>
    <trd lng="hu">meszez</trd>
    <trd lng="nl">gips toevoegen</trd>
    <trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>    
  </snc>
  <trd lng="ca">enguixar</trd>
  <trdgrp lng="cs">
    <trd>s&aacute;drovat</trd>,
    <trd>zas&aacute;drovat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x818F;&#x3092;&#x5857;&#x308B;  <pr>&#x305B;&#x3063;&#x3053;&#x3046;&#x3092;&#x306C;&#x308B;</pr></trd>,
    <trd>&#x6F06;&#x55B0;&#x3092;&#x5857;&#x308B;  <pr>&#x3057;&#x3063;&#x304F;&#x3044;&#x3092;&#x306C;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">sadrova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i; &c_g;&c_ib;&c_p;&c_s;&c_o;&c_m;</trd>,
    <trd>&c_g;&c_ib;&c_p;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="gips.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Miksa&jcirc;o el <tld/>o kaj aliaj aldona&jcirc;oj uzata por modlado, 
      plenigo de murtruoj, gluado ktp:
      <ekz>
        mi miksas mem la <tld/>a&jcirc;on
        <fnt>
          <bib>KrM</bib>
          <lok>6. Kiu Ne Riskas, Tiu Ne Gajnas.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_s;&c_a;&c_v;&c_a;&c_ja; &c_m;&c_a;&c_s;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Gipsmasse</trd>,
    <trd>D&uuml;belmasse</trd>
  </trdgrp>
  <trd lng="ja">&#x77F3;&#x818F;&#x7D30;&#x5DE5;  <pr>&#x305B;&#x3063;&#x3053;&#x3046;&#x3055;&#x3044;&#x304F;</pr></trd>
</drv>








