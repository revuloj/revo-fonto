<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gips</rad>/o
</kap>
<drv mrk="gips.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pulvorigita kalka &scirc;tono, uzata en konstruarto por &scirc;miri a&ubreve; tegi murojn.
    </dif>
  </snc>
  <trd lng="de">Gips</trd>
  <trd lng="fr">pl&acirc;tre</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_s;</trd>
</drv>
<drv mrk="gips.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      &Scirc;miri, tegi per <tld/>o: <ekz><tld/>i plafonon.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stuk.0i">stuki</ref>.
    </refgrp>
    <trd lng="de">gipsen</trd>
    <trd lng="fr">pl&acirc;trer</trd>
  </snc>
  <snc num="2">
    <dif>
      Miksi <tld/>on al io, sur&scirc;uti per <tld/>o:
      <ekz><tld/>i vinon por &gcirc;in klarigi, kampon por &gcirc;in plibonigi.</ekz>
    </dif>
    <trd lng="de">kalken</trd>
  </snc>
</drv>
</art>
