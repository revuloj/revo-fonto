<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kartav</rad>/i
</kap>
<drv mrk="kartav.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <uzo tip="fak">LIN</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Prononci la literon "r" per la gor&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_k;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="en">speak with a <ind>burr</ind>
    <klr> (with exaggerated rolling of the letter "r")</klr></trd>
  <trd lng="fr">grasseyer</trd>
  <trd lng="hu">raccsol</trd>
  <trd lng="nl">brouwen<klr tip="ind"> (de keel-r uitspreken)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
