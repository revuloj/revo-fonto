<art mrk="$Id: skabi.xml,v 1.21 2024/03/25 06:40:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skabi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skabi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skabi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Infekta <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
      de la ha&ubreve;to ka&ubreve;zata de malgrandaj akaroj
      kaj karakterizata de forta jukado:
      <ekz>
        dromedaro periode suferas <tld/>on,
        kiun oni flegas per vegeta&jcirc;a gudro
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_s;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>pra&scaron;ivina</trd>,
    <trd>skabies</trd>,
    <trd>svrab</trd>
  </trdgrp>
  <trd lng="de">Kr&auml;tze</trd>
  <trdgrp lng="en">
    <trd>scab<klr tip="amb"> (disease)</klr></trd>,
    <trd>mange</trd>,
    <trd>scabies</trd>,
    <trd>infection</trd>,
    <trd>itch<klr tip="amb"> (disease)</klr></trd>
  </trdgrp>
  <trd lng="es">sarna</trd>
  <trd lng="fr">gale</trd>
  <trd lng="hu">r&uuml;h</trd>
  <trdgrp lng="id">
    <trd>kudis</trd>,
    <trd>skabies</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3044;&#x305B;&#x3093;</trd>,
    <trd>&#x3072;&#x305C;&#x3093;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schurft</trd>,
    <trd>scabi&euml;s</trd>
  </trdgrp>
  <trd lng="pl">&sacute;wierzb</trd>
  <trd lng="ru">&c_ch;&c_je;&c_s;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">svrab</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_r;&#x43E;&c_s;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x75A5; <pr>ji&#xE8;</pr></trd>,
    <trd>&#x760C; <pr>l&#xE0;</pr></trd>,
    <trd>&#x765E; <pr>l&#xE0;i</pr></trd>,
    <trd>&#x7669; <pr>l&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>
