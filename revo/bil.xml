<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>bil</rad>/o
    <fnt>Z</fnt>
  </kap>
  
  <drv mrk="bil.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">EKON</uzo>
    <snc>
      <dif>
        Rajto pri kapitalo, kutime negocebla en la borso; valorpapero.  
      </dif>
    </snc>
    <refgrp tip="vid"><ref cel="akci.0o">akcio</ref>,
    <ref cel="obliga.0o">obligacio</ref></refgrp>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_ja;
    &c_p;&c_a;&c_p;&c_je;&c_r;&c_a;</trd>
    <trdgrp lng="de">
       <trd>Effekt</trd>,
       <trd>begebbares Wertpapier</trd>,
       <trd>vertretungsf&auml;higes Wertpapier</trd></trdgrp>
    <trdgrp lng="en">
       <trd>negotiable security</trd>,
       <trd>negotiable document of value</trd>,
       <trd>negotiable instrument</trd></trdgrp>
    <trd lng="hu">&eacute;rt&eacute;kpap&iacute;r</trd>
    <trd lng="nl">effect</trd>
    <trd lng="pl">papier warto&sacute;ciowy</trd>
    <trd lng="ru">&c_c;&c_je;&c_n;&c_n;&c_a;&c_ja;
    &c_b;&c_u;&c_m;&c_a;&c_g;&c_a;</trd>
  </drv>
  
  <drv mrk="bil.0borso">
    <kap><tld/>oborso</kap>
    <uzo tip="fak">EKON</uzo>
    <snc>
      <dif>
        Loko, kie oni negocas <tld/>ojn.  
      </dif>
    </snc>
    <trd lng="be">&c_f;&c_o;&c_n;&c_d;&c_a;&c_v;&c_a;&c_ja;
    &c_b;&c_ib;&c_r;&c_zh;&c_a;</trd>
    <trd lng="de">Effektenb&ouml;rse</trd>
    <trd lng="hu">&eacute;rt&eacute;kt&odblac;zsde</trd>
    <trd lng="nl">effectenbeurs</trd>
  </drv>
</art>
