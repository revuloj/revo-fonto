<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lagr.xml,v 1.18 2019/10/06 22:10:18 revo Exp $">
<kap>
  <rad>lagr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lagr.0o">
  <kap><tld/>o</kap>
  <snc mrk="lagr.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="malprt" cel="masxin.0o">Ma&scirc;ina</ref> peco, 
      kiu portas la <ref tip="vid" cel="aks.0o">akson</ref> a&ubreve; 
      <ref tip="vid" cel="sxaft.0o">&scirc;afton</ref> de 
      <ref tip="malprt" cel="rad.0o">rado</ref> a&ubreve; de
      transmisia me&hcirc;anismo:
      <ekz>
        la&ubreve; direkto de la forto, kiun glitaj kaj rulaj <tld/>oj retenas,
        oni 
        distingas <tld/>ojn radialajn kaj aksajn
        <fnt>
          <bib>Viki</bib>,
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/Lagro">Lagro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ef<tld/>o;
      </ekz>
      <ekz>
        e&gcirc;o<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kran.0o">krano</ref>,
      <ref cel="krank.0o">kranko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_sh;&c_y;&c_p;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">coixinet<klr> (tecnol.)</klr></trd>
  <trd lng="cs">lo&zcaron;isko</trd>
  <trdgrp lng="de">
     <trd>Lager <klr tip="ind">(Technik)</klr></trd>,
     <trd>W&auml;lzlager</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bearing</trd>,
    <trd>bushing</trd>
  </trdgrp>
  <trd lng="fa">&f_y;&a_A;&a_t;&a_A;&a_q;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>palier lisse</trd>,
    <trd>roulement m&eacute;canique</trd>
  </trdgrp>
  <trd lng="hu">csap&aacute;gy</trd>
  <trd lng="nl">lager<klr tip="ind"> (in machine)</klr></trd>
  <trd lng="pl">&lstroke;o&zdot;ysko</trd>
  <trdgrp lng="pt">
    <trd>suporte de mancal</trd>,
    <trd>mancal</trd>,
    <trd>coxim</trd>,
    <trd>chumaceira</trd>    
  </trdgrp>
  <trd lng="ro">cuzinet</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_sh;&c_i;&c_p;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">lo&zcaron;isko</trd>
  <trd lng="sv">lager<klr tip="ind"> (i maskin)</klr></trd>
</drv>
</art>
