<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lagr</rad>/o
</kap>
<drv mrk="lagr.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ma&scirc;ina peco, kiu portas la akson a&ubreve; &scirc;afton de rado a&ubreve; de transmisia
      mekanismo:
      <ekz>
        <tld/>o de akso;
      </ekz>
      <ekz>
        &ccirc;ef<tld/>o, e&gcirc;o<tld/>o,
      </ekz>
      ks.
    </dif>
    <refgrp tip="vid">
      <ref cel="kran.0o">krano</ref>,
      <ref cel="krank.0o">kranko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_sh;&c_y;&c_p;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
     <trd>Lager</trd>,
     <trd>W&auml;lzlager</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bearing</trd>,
    <trd>bushing</trd>
  </trdgrp>
  <trd lng="hu">csap&aacute;gy</trd>
  <trd lng="nl">lager<klr tip="ind"> (in machine)</klr></trd>
  <trdgrp lng="pt">
    <trd>suporte de mancal</trd>,
    <trd>mancal</trd>,
    <trd>coxim</trd>,
    <trd>chumaceira</trd>    
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_sh;&c_i;&c_p;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
