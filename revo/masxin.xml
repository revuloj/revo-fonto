<drv mrk="masxin.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="masxin.0a.rilata">
    <dif>
      Rilata al <tld/>o, de <tld/>o:
      <ekz>
        vapor<tld/>a kaldrono
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a muzeo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007097.html">Konkurso: Per malnovaj a&ubreve;toj &ccirc;irka&ubreve; Balta Maro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iaj <tld/>aj kaj kemiaj rimedoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2011/010422.php">Kampara fabrikado</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="masxin.0a.per">
    <dif>
      Per <tld/>o:
      <ekz>
        <tld/>a tradukado
        <fnt>
          <bib>Monato</bib>
          <aut>Marc vanden Bempt</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/006846.html">http://steloj.de/esperanto/monato2/006846.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">maschinell</trd>
    <trdgrp lng="pl">
      <trd>maszynowy</trd>,
      <trd>bezwiedny</trd>,
      <trd>machinalny</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>ma&#x219;in&abreve;</trd>,
      <trd>incon&scedil;tient&abreve;</trd>,
      <trd>mecanic&abreve;</trd>,
      <trd>involuntare</trd>
     </trdgrp> 
  </snc>
</drv>

<drv mrk="masxin.0e">
  <kap><tld/>e</kap>
  <snc mrk="masxin.0e.TEK">
    <dif>
      Per <tld/>o, ne per mano:
      <ekz>
        puntoj, tapi&scirc;oj fabrikataj <tld/>e
        <klr>(me&hcirc;anike)</klr>;
      </ekz>
      <ekz>
        anta&ubreve;e oni faligis arbojn ne <tld/>e kaj grandkvante kiel nun
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2010/009971.php">Nestokestoj</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z; &c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_g;&c_a;&c_j;
      <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;</ind></trd>,
      <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_n;&c_y;&c_m;
      &c_s;&c_p;&c_o;&c_s;&c_a;&c_b;&c_a;&c_m;</trd>,
      <trd>&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="br">ent-<ind>mekanikel</ind></trd>
    <trd lng="de">maschinell</trd>
    <trd lng="fr">m&eacute;caniquement</trd>
    <trd lng="hu">g&eacute;ppel</trd>
    <trd lng="it">a <ind>macchina</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_s; &c_p;&c_o;&c_m;&c_o;&c_shch;&c_mol;&c_ju;
      &c_m;&c_a;&c_sh;&c_i;&c_n;</trd>,
      <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_n;&c_y;&c_m;
      &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_o;&c_m;</trd>
    </trdgrp>
  </snc>
  <snc mrk="masxin.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sen konscia volo, kvaza&ubreve; per <tld/>o, a&ubreve;tomate:
      <ekz>
        agi nur <tld/>e;
      </ekz>
      <ekz>
        li mensogas <tld/>e
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;</trd>
    <trd lng="br">evel ur <ind>mekanik</ind></trd>
    <trd lng="fr">machinalement</trd>
    <trd lng="de">maschinell</trd>
    <trd lng="hu">g&eacute;piesen</trd>
    <trd lng="it">meccanicamente<klr> (automaticamente)</klr></trd>
   <trd lng="nl">machinaal</trd> 
   <trdgrp lng="pl">
     <trd>maszynowo</trd>
     <trd>machinalnie</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>mecanic</trd>
     <trd>automat</trd>
   </trdgrp>
   <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;</trd>
  </snc>
</drv>

<drv mrk="masxin.0ejo">
  <kap><tld/>ejo <fnt><bib>EBV</bib></fnt></kap>
  <snc mrk="masxin.0ejo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Konstrua&jcirc;o a&ubreve; &ccirc;ambro por funkciantaj <tld/>oj:
      <ekz>
        en la po&ubreve;pa <tld/>ejo
        <fnt>
          <aut>Efremov Ivan</aut>
          <vrk><url ref="https://www.litmir.me/br/?b=87611&amp;p=16">La nebulozo de Andromedo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        lia kajuto estis en proksimo de <tld/>ejo kaj bruego de <tld/>oj penetris en la
        kajuton
        <fnt>
          <vrk><url ref="https://insentence.com/sanceligi-in-a-sentence">&scirc;anceli&gcirc;i</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Maschinenraum</trd>,
    <trd>Maschinenhalle</trd>
  </trdgrp>
  <trd lng="br">sal an ardivinko&ugrave;</trd>
  <trd lng="fr">salle des machines</trd>
  <trd lng="it"><ind>sala</ind> macchine</trd>
  <trd lng="pl">maszynownia</trd>
  <trdgrp lng="ro">
    <trd>camera motoarelor</trd>
    <trd>&icirc;nc&abreve;perile de ma&#x219;ini</trd>
  </trdgrp>
</drv>

<drv mrk="masxin.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="masxin.0ismo.TEK">
    <dif>
      <tld lit="M"/>epoko, industria stato, en kiu manlaboro estas
      pliparte anstata&ubreve;ata per <tld/>oj:
      <ekz>
        <tld/>ismo venigis la industrian revolucion, kiu perfektigis homekspluatadon kaj
        pravigis la aktualan kapitalismon
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006916.html">Bugra bug-insekto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Maschinisierung</trd>,
    <trd>Mechanisierung</trd>
  </trdgrp>
  <trd lng="fr">machinisme</trd>
  <trd lng="hu">g&eacute;pes&iacute;t&eacute;s</trd>
  <trd lng="nl">mechanisatie</trd>
  <trd lng="pl">mechanizacja</trd>
  <trd lng="ro">mecanizare</trd>
  <trd lng="ru">&c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="masxin.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="masxin.0isto.TEK">
    <dif>
      Homo, kiu profesie direktas <tld/>on:
      <ekz>
        <klr>[li]</klr> malbenadis la neelporteble malrapidan <klr>[&#x2026;]</klr> iradon de
        la vagonaro kaj la <tld/>iston kaj la konduktoron
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li varbis pilotojn kaj <tld/>istojn el Usono, promesante altan salajron
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Maschinist</trd>
    <trd lng="fr">m&eacute;canicien</trd>
    <trd lng="hu">g&eacute;p&eacute;sz</trd>
    <trd lng="nl">machinist</trd>
  </snc>
  <snc mrk="masxin.0isto.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Homo, kiu en teatrejo starigas la dekoracion sur &gcirc;usta loko:
      <ekz>
        kiam li vespere sidis en la teatro, por &gcirc;ui plenanime, li perdadis &ccirc;ian spiritan
        ekvilibron, se la <tld/>isto faris e&ccirc; la plej malgrandan eraron, se li faris
        tro helan lumon malanta&ubreve; la disko de la luno, a&ubreve; se sur insulo en la Sundo
        montri&gcirc;is palmo, a&ubreve; kakto en Tirolo
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">machiniste</trd>
    <trd lng="hu">d&iacute;szletez&odblac; munk&aacute;s</trd>
    <trd lng="nl">toneelmeester</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="br">
    <trd>ardivinker</trd>,
    <trd>mekaniker</trd>
  </trdgrp>
  <trd lng="de">Maschinist</trd>
  <trd lng="it">macchinista</trd>
  <trdgrp lng="pl">
    <trd>maszynista</trd>
    <trd>&sacute;lusarz</trd>
  </trdgrp>
  <trd lng="ro">ma&#x219;inist utilaje</trd>
  <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="masxin.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="masxin.flug0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aparat.flug0o.AVI">Flugaparato</ref>,
      veturilo
      kapabla iri tra la aero:
      <ekz>
        en la da&ubreve;ro de sep jaroj li konstruigis artoplenajn
        flug<tld/>ojn
        <fnt><bib>Fab4</bib>, <lok>malbona princo, legendo</lok></fnt>;
      </ekz>
      <ekz>
        venos sendube la tempo, kiam <klr>[&#x2026;]</klr> povos kongresanoj
        veturi per flug<tld/>oj tre rapide k<klr>aj</klr> malmultkoste
        <fnt><bib>Lanti</bib>, <lok>La signifo de niaj
        Kongresoj</lok></fnt>;
      </ekz>
      <ekz>
        monpunon tamen riskis la aviadisto pro tio, ke sur la flug<tld/>o estis
        reklamataj produkta&jcirc;oj de tabakfirmao
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/007181.html">http://steloj.de/esperanto/monato2/007181.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Flugmaschine</trd>
  <trdgrp lng="fr">
    <trd>machine volante</trd>,
    <trd>vaisseau a&eacute;rien</trd>
  </trdgrp>
  <trd lng="it">aeromobile</trd>
  <trdgrp lng="pl">
    <trd>aerodyna</trd>
    <trd>statek powietrzny</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aerodina</trd>
    <trd>vehicul aerian</trd>
  </trdgrp>
</drv>

<drv mrk="masxin.il0o">
  <kap>il<tld/>o <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="masxin.il0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="M"/>o uzata por produkti industriajn ilojn.
    </dif>
  </snc>
  <trd lng="br">mekanik-benveg</trd>
  <trd lng="de">Werkzeugmaschine</trd>
  <trd lng="fr">machine-outil</trd>
  <trd lng="hu">szersz&aacute;mg&eacute;p</trd>
  <trd lng="it"><ind>macchina</ind> utensile</trd>
  <trd lng="pl">obrabiarka</trd>
  <trd lng="ro">ma&#x219;in&abreve; unealt&abreve;</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="masxin.infera0o">
  <kap>infera <tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="masxin.infera0o.ARM">
    <dif>
      Eksploda aparato, bombo:
      <ekz>
        esti &jcirc;etita en malliberejon por kelkaj tagoj estas io pli 
        simpla kaj pli senriska, ol meti inferan <tld/>on
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">machine infernale</trd>
  <trd lng="pl">piekielna machina</trd>
  <trd lng="ro">ma&#x219;in&abreve; infernal&abreve;</trd>
</drv>

<drv mrk="masxin.konstru0o">
  <kap>konstru<tld/>o</kap>
  <snc mrk="masxin.konstru0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Ajna <tld/>o uzata en konstruejo por foso, levo, premo kaj 
      similaj pezaj taskoj:
      <ekz>
        en la jaro 1938 diversaj konstru<tld/>oj jam aperis en la mondo, tamen estis
        neniu buldozo dum la konstruado de la &scirc;oseo inter la provinco Junano,
        sud-okcidenta &Ccirc;inio, kaj Birmo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde la fondo de <nom>Volvo</nom> apartenas al la produktaro diversaj 
        omnibusoj <klr>[&#x2026;]</klr>, &scirc;ar&gcirc;a&ubreve;toj
        <klr>[&#x2026;]</klr> kaj konstru<tld/>oj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Volvo&rightquot;, 
        2007-10-19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Baumaschine</trd>
  <trd lng="fr">machine de chantier</trd>
  <trd lng="it"><ind>macchinario</ind> edile</trd>
  <trd lng="pl">maszyna budowlana</trd>
  <trd lng="ro">ma&#x219;in&abreve; de construc&#x21B;ie</trd>
</drv>

<drv mrk="masxin.kudro0o">
  <kap>kudro<tld/>o</kap>
  <snc mrk="masxin.kudro0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="M"/>o, kiu rapide kudras sub&scirc;ovatajn
      &scirc;tofojn:
      <ekz>
        oni a&ubreve;dis bruon de du kudro<tld/>oj, super kiuj estis
        klinitaj anka&ubreve; du virinaj figuroj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        nur la kudro<tld/>oj monotone krakis
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007140.html">de Julian Modest</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja;
  &c_m;&c_a;&c_sh;&c_y;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwrierez</trd>,
    <trd>mekanik gwriat</trd>
  </trdgrp>
  <trd lng="de">N&auml;hmaschine</trd>
  <trd lng="fr">machine &agrave; coudre</trd>
  <trd lng="it"><ind>macchina</ind> per cucire</trd>
  <trd lng="pl">maszyna do szycia</trd>
  <trd lng="ro">ma&#x219;in&abreve; de cusut</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja;
  &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="masxin.kuir0o">
  <kap>kuir<tld/>o</kap>
  <snc mrk="masxin.kuir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="M"/>o por varmigi kuirpotojn kaj kuiri
      man&gcirc;a&jcirc;ojn:
      <ekz>
        la vitroceramikaj kuir<tld/>oj estas sistemoj de kuirado en kiuj estas vitro
        inter la fonto de varmo kaj la ujo varmigota
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=601055">Vitroceramika kuirma&scirc;ino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">fornigell</trd>
  <trd lng="de">Kochger&auml;t</trd>
  <trd lng="fr">cuisini&egrave;re</trd>
  <trdgrp lng="pl">
    <trd>piec kuchenny</trd>
    <trd>kuchenka</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>aragaz</trd>
    <trd>sob&abreve;</trd>
  </trdgrp>
</drv>

<drv mrk="masxin.lav0o">
  <kap>
    lav<tld/>o
  </kap>
  <snc mrk="masxin.lav0o.aparato">
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref>, kiu memstare lavas 
      vesta&jcirc;ojn, man&gcirc;ilaron ks:
      <ekz>
        &scirc;i instruis al mi manipuli lav<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Lin Sheng</aut>
          <vrk><url ref="https://www.monato.be/2006/008670.php">Forpasis mia karulino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj cirkonstancoj kompreneble ne estas bezono je kolortelevidilo,
        lav<tld/>o, fridujo
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007168.html">Elektraj aparatoj mankas en la kamparo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        niaj lav<tld/>o kaj man&gcirc;ilarlavilo proponas dekojn da programoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2010/009945.php">Kiom da elektroniko?</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  &c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_p;&c_je;&c_r;&c_a;&c_l;&c_n;&c_ja;</trd>
  <trd lng="br">kannerez<klr tip="ind"> (ardivink)</klr></trd>
  <trd lng="cs">pra&ccaron;ka</trd>
  <trd lng="de">Waschmaschine</trd>
  <trd lng="en">washing machine</trd>
  <trd lng="es">lavadora</trd> 
  <trdgrp lng="fr">
    <trd>machine &agrave; laver</trd>,
    <trd>lave-linge</trd>
  </trdgrp>
  <trd lng="he">&mem;&kaf;&vav;&nun;&tav;
  &kaf;&bet;&yod;&samekh;&he;</trd>
  <trd lng="hu">mos&oacute;g&eacute;p</trd>
  <trd lng="it">lavatrice</trd>
  <trd lng="nl">wasmachine</trd>
  <trd lng="pl">pralka</trd>
  <trd lng="ro">ma&#x219;in&abreve; de sp&abreve;lat rufe</trd>
  <trd lng="sk">pr&aacute;&ccaron;ka</trd>
  <trd lng="sv">tv&auml;ttmaskin</trd>
</drv>

<drv mrk="masxin.skrib0o">
  <kap>skrib<tld/>o</kap>
  <snc mrk="masxin.skrib0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="tajp.0ilo">Tajpilo</ref>:
      <ekz>
        tute ellaborita skrib<tld/>o en 1891
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la ligna skribotablo apud la fenestro staris skrib<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2010/010053.php">Ekslibriso</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi verkis, netigis per mia skrib<tld/>o iliajn manuskriptojn
          <fnt>
            <aut>Hasegawa T.</aut>,
            <vrk><url ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
            En &Ccirc;inio Batalanta</url></vrk>, 
            <lok>1954</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schreibmaschine</trd>
  <trd lng="en">typewriter</trd>
  <trd lng="es">m&aacute;quina de <ind>escribir</ind></trd>
  <trd lng="fr">machine &agrave; &eacute;crire</trd>
  <trd lng="hu">&iacute;r&oacute;g&eacute;p</trd>
  <trd lng="it">macchina per <ind>scrivere</ind></trd>
  <trd lng="nl">schrijfmachine</trd>
  <trd lng="pl">maszyna do pisania</trd>
  <trd lng="ro">ma&#x219;in&abreve; de scris</trd>
  <trd lng="ru">&c_p;&c_i;&c_sh;&c_u;&c_shch;&c_a;&c_ja;
  &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sv">skrivmaskin</trd>
</drv>

<drv mrk="masxin.te0o">
  <kap>
    te<tld/>o
  </kap>
  <snc mrk="masxin.te0o.aparato">
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref>
      por boligi akvon por teo kaj teni &gcirc;in varme; la rusa, okcidentazia
      modelo nomata <ref tip="sub" cel="samova.0o">samovaro</ref> posedas duan 
      malgrandan ujon por tekoncentra&jcirc;o:
      <ekz>
        &scirc;i enportigis la te<tld/>on, por kuiri por li bonan tason da sambuka teo
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;dis la obtuzan kantadon de la te<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Franjo okupi&gcirc;is pri la te<tld/>o, kaj mi iom legis
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Teemaschine</trd>
  <trd lng="pl">samowar</trd>
  <trd lng="ro">samovar</trd>
</drv>

<drv mrk="masxin.vapor0o">
  <kap>vapor<tld/>o</kap>
  <snc mrk="masxin.vapor0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="masxin.0o.TEK"><tld lit="M"/>o</ref>, kiu
      transformas la ekspansian forton de akvovaporo al me&hcirc;anika movado:
      <ekz>
        tiam ni ne havis vapor<tld/>ojn
        <fnt>
          <bib>Fab4</bib>,
          <lok>Peco da perlovico, 2.</lok>
        </fnt>;
      </ekz>
      <ekz>
        vapor<tld/>oj, fervojoj, radioaparatoj estis inventitaj ene de ducento da jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hans B. Welling</aut>
          <vrk><url ref="https://www.monato.be/2014/011331.php">Nesuspektindaj flutistoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_v;&c_a;&c_ja;
  <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</ind></trd>
  <trd lng="br">ijinenn dre vurezh</trd>
  <trd lng="de">Dampfmaschine</trd>
  <trd lng="fr">machine &agrave; vapeur</trd>
  <trd lng="hu">g&odblac;zg&eacute;p</trd>
  <trd lng="it"><ind>macchina</ind> a vapore</trd>
  <trd lng="nl">stoommachine</trd>
  <trd lng="pl">silnik parowy</trd>
  <trd lng="ro">motor cu abur</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_v;&c_a;&c_ja;
  s<ind>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</ind></trd>
</drv>

<drv mrk="masxin.vendo0o">
  <kap>
    vendo<tld/>o
  </kap>
  <snc mrk="masxin.vendo0o.aparato">
    <dif>
      Aparato, el kiu oni povas ricevi varojn post a&ubreve;tomata akcepto de pago:
      <ekz>
        <klr>[ili]</klr> a&ccirc;etas tabak-varojn el a&ubreve;tomataj vendo<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2008/009623.php">Tabako ankora&ubreve; ne tabua</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vendo<tld/>o ne redonis la reston de la mono
        <fnt>
          <vrk><url ref="https://lv.vocapp.com/dictionary/en/eo/money">Fiszkoteka</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Verkaufsautomat</trd>
  <trd lng="en">vending machine</trd>
  <trd lng="pl">automat sprzedaj&aogonek;cy</trd>
  <trd lng="ro">automat de v&acirc;nzare</trd>
</drv>


