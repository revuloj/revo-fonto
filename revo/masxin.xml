  <trd lng="sv">tv&auml;ttmaskin</trd>
  <trd lng="uk">&c_p;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
