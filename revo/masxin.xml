  <trd lng="be">&c_b;&c_u;&c_d;&c_a;&c_w;&c_n;&c_ib;&c_ch;&c_a;&c_ja; <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</ind></trd>
