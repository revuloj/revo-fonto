<drv mrk="masxin.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="masxin.0a.rilata">
    <dif>
      Rilata al <tld/>o, de <tld/>o:
      <ekz>
        vapor<tld/>a kaldrono
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a muzeo
        <fnt><aut>Laimius Stra&zcaron;nickas</aut>:
          <vrk><url ref="&Monato;/2000/007097.php">Per malnovaj a&ubreve;toj &ccirc;irka&ubreve; Balta Maro</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/11, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iaj <tld/>aj kaj kemiaj rimedoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2011/010422.php">Kampara fabrikado</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="masxin.0a.per">
    <dif>
      Per <tld/>o:
      <ekz>
        <tld/>a tradukado
        <fnt>
       <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2000/006846.php">Perkomputila tradukado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">maschinell</trd>
    <trdgrp lng="pl">
      <trd>maszynowy</trd>,
      <trd>bezwiedny</trd>,
      <trd>machinalny</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>ma&#x219;in&abreve;</trd>,
      <trd>incon&#x219;tient&abreve;</trd>,
      <trd>mecanic&abreve;</trd>,
      <trd>involuntare</trd>
     </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7528;&#x673A;&#x5668; <pr>y&#xF2;ngj&#x12B;q&#xEC;</pr></trd>,
    <trd>&#x7528;&#x6A5F;&#x5668; <pr>y&#xF2;ngj&#x12B;q&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6A5F;&#x68B0;&#x306E;  <pr>&#x304D;&#x304B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x6A5F;&#x68B0;&#x7684;&#x306A;  <pr>&#x304D;&#x304B;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_je;&c_h;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="masxin.0e">
  <kap><tld/>e</kap>
  <snc mrk="masxin.0e.TEK">
    <dif>
      Per <tld/>o, ne per mano:
      <ekz>
        puntoj, tapi&scirc;oj fabrikataj <tld/>e
        <klr>(me&hcirc;anike)</klr>;
      </ekz>
      <ekz>
        anta&ubreve;e oni faligis arbojn ne <tld/>e kaj grandkvante kiel nun
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2010/009971.php">Nestokestoj</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z; &c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_g;&c_a;&c_j;
      <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;</ind></trd>,
      <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_n;&c_y;&c_m;
      &c_s;&c_p;&c_o;&c_s;&c_a;&c_b;&c_a;&c_m;</trd>,
      <trd>&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="br">ent-<ind>mekanikel</ind></trd>
    <trd lng="de">maschinell</trd>
    <trd lng="fr">m&eacute;caniquement</trd>
    <trd lng="hu">g&eacute;ppel</trd>
    <trd lng="it">a <ind>macchina</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_s; &c_p;&c_o;&c_m;&c_o;&c_shch;&c_mol;&c_ju;
      &c_m;&c_a;&c_sh;&c_i;&c_n;</trd>,
      <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_n;&c_y;&c_m;
      &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_o;&c_m;</trd>
    </trdgrp>
  </snc>
  <snc mrk="masxin.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sen konscia volo, kvaza&ubreve; per <tld/>o, a&ubreve;tomate:
      <ekz>
        agi nur <tld/>e;
      </ekz>
      <ekz>
        li mensogas <tld/>e
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;</trd>
    <trd lng="br">evel ur <ind>mekanik</ind></trd>
    <trd lng="fr">machinalement</trd>
    <trd lng="de">maschinell</trd>
    <trd lng="hu">g&eacute;piesen</trd>
    <trd lng="it">meccanicamente<klr> (automaticamente)</klr></trd>
   <trd lng="nl">machinaal</trd>
   <trdgrp lng="pl">
     <trd>maszynowo</trd>
     <trd>machinalnie</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>mecanic</trd>
     <trd>automat</trd>
   </trdgrp>
   <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x6A5F;&#x68B0;&#x3067;  <pr>&#x304D;&#x304B;&#x3044;&#x3067;</pr></trd>,
    <trd>&#x6A5F;&#x68B0;&#x7684;&#x306B;  <pr>&#x304D;&#x304B;&#x3044;&#x3066;&#x304D;&#x306B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_n;&c_i;&c_m; &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_o;&c_m;</trd>,
    <trd>&c_m;&c_je;&c_h;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_o;</trd>
  </trdgrp>
