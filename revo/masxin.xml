<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ma&scirc;in</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="masxin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="masxin.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref>, kiu produktas,
      transsendas a&ubreve; utiligas forton kaj anstata&ubreve;as a&ubreve; plifaciligas
      la
      homan laboron:
      <ekz>
        dra&scirc;<tld/>o, skrib<tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Maschine</trd>
  </snc>
  <snc mrk="masxin.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malsimpla organizo kombinita por atingi difinitan celon:
      <ekz>
        la homa <tld/>o;
      </ekz>
      <ekz>
        la tera <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        administracia <tld/>o estas &ccirc;iam malrapida.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>ardivink</trd>,
    <trd>ijinenn</trd>,
    <trd>mekanik</trd>
  </trdgrp>
  <trd lng="cs">stroj</trd>
  <trd lng="de">Maschine</trd>
  <trd lng="en">machine</trd>
  <trd lng="fr">machine</trd>
  <trd lng="hu">g&eacute;p(ezet)</trd>
  <trd lng="it">macchina</trd>
  <trd lng="nl">machine</trd>
  <trd lng="pt">m&aacute;quina</trd>
  <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
  <trd lng="tp">ilo</trd>
</drv>

<drv mrk="masxin.0e">
  <kap><tld/>e</kap>
  <snc mrk="masxin.0e.TEK">
    <dif>
      Per <tld/>o, ne per mano:
      <ekz>
        puntoj, tapi&scirc;oj fabrikataj <tld/>e
        <klr>(me&hcirc;anike)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z; &c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_g;&c_a;&c_j;
      <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;</ind></trd>,
      <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_n;&c_y;&c_m; &c_s;&c_p;&c_o;&c_s;&c_a;&c_b;&c_a;&c_m;</trd>,
      <trd>&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="br">ent-<ind>mekanikel</ind></trd>
    <trd lng="de">maschinell</trd>
    <trd lng="fr">m&eacute;caniquement</trd>
    <trd lng="hu">g&eacute;ppel</trd>
    <trd lng="it">a <ind>macchina</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_s; &c_p;&c_o;&c_m;&c_o;&c_shch;&c_mol;&c_ju; &c_m;&c_a;&c_sh;&c_i;&c_n;</trd>,
      <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_n;&c_y;&c_m; &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_o;&c_m;</trd>
    </trdgrp>
  </snc>
  <snc mrk="masxin.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sen konscia volo, kvaza&ubreve; per <tld/>o, a&ubreve;tomate:
      <ekz>
        agi nur <tld/>e;
      </ekz>
      <ekz>
        li mensogas <tld/>e
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;</trd>
    <trd lng="br">evel ur <ind>mekanik</ind></trd>
    <trd lng="fr">machinalement</trd>
    <trd lng="de">maschinell</trd>
    <trd lng="hu">g&eacute;piesen</trd>
    <trd lng="it">meccanicamente<klr> (automaticamente)</klr></trd>
    <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;</trd>
  </snc>
  <trd lng="nl">machinaal</trd>
</drv>

<drv mrk="masxin.0ejo">
  <kap><tld/>ejo</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="masxin.0ejo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Konstrua&jcirc;o a&ubreve; cambro por funkciantaj <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Maschinenraum</trd>,
    <trd>Maschinenhalle</trd>
  </trdgrp>
  <trd lng="br">sal an ardivinko&ugrave;</trd>
  <trd lng="fr">salle des machines</trd>
  <trd lng="it"><ind>sala</ind> macchine</trd>
</drv>

<drv mrk="masxin.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="masxin.0ismo.TEK">
    <dif>
      <tld lit="M"/>epoko, industria stato, en kiu manlaboro estas
      pliparte anstata&ubreve;ata per <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Maschinisierung</trd>,
    <trd>Mechanisierung</trd>
  </trdgrp>
  <trd lng="fr">machinisme</trd>
  <trd lng="hu">g&eacute;pes&iacute;t&eacute;s</trd>
  <trd lng="nl">mechanisatie</trd>
  <trd lng="ru">&c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="masxin.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="masxin.0isto.TEK">
    <dif>
      Homo, kiu profesie direktas <tld/>on.
    </dif>
    <trd lng="de">Maschinist</trd>
    <trd lng="fr">m&eacute;canicien</trd>
    <trd lng="hu">g&eacute;p&eacute;sz</trd>
    <trd lng="nl">machinist</trd>
  </snc>
  <snc mrk="masxin.0isto.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Homo, kiu en teatrejo starigas la dekoracion sur &gcirc;usta loko.
    </dif>
    <trd lng="fr">machiniste</trd>
    <trd lng="hu">d&iacute;szletez&odblac; munk&aacute;s</trd>
    <trd lng="nl">toneelmeester</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="br">
    <trd>ardivinker</trd>,
    <trd>mekaniker</trd>
  </trdgrp>
  <trd lng="de">Maschinist</trd>
  <trd lng="it">macchinista</trd>
  <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="masxin.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="masxin.flug0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aparat.flug0o.AVI">Flugaparato</ref>,
      veturilo
      kapabla iri tra la aero:
      <ekz>
        en la da&ubreve;ro de sep jaroj li konstruigis artoplenajn
        flug<tld/>ojn
        <fnt><bib>Fab4</bib>, <lok>malbona princo, legendo</lok></fnt>;
      </ekz>
      <ekz>
        venos sendube la tempo, kiam <klr>...</klr> povos kongresanoj
        veturi per flug<tld/>oj tre rapide k<klr>aj</klr> malmultkoste
        <fnt><bib>Lanti</bib>, <lok>La signifo de niaj
        Kongresoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Flugmaschine</trd>
  <trdgrp lng="fr">
    <trd>machine volante</trd>,
    <trd>vaisseau a&eacute;rien</trd>
  </trdgrp>
  <trd lng="it">aeromobile</trd>
</drv>

<drv mrk="masxin.il0o">
  <kap>il<tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="masxin.il0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="M"/>o uzata por produkti industriajn ilojn.
    </dif>
  </snc>
  <trd lng="br">mekanik-benveg</trd>
  <trd lng="de">Werkzeugmaschine</trd>
  <trd lng="fr">machine-outil</trd>
  <trd lng="hu">szersz&aacute;mg&eacute;p</trd>
  <trd lng="it"><ind>macchina</ind> utensile</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="masxin.konstru0o">
  <kap>konstru<tld/>o</kap>
  <snc mrk="masxin.konstru0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Ajna <tld/>o uzata en konstruejo por foso, levo, premo kaj 
      similaj pezaj taskoj:
      <ekz>
        ekde la fondo de Volvo apartenas al la produktaro diversaj 
        omnibusoj <klr>...</klr>, &scirc;ar&gcirc;a&ubreve;toj <klr>...</klr> kaj 
        konstru<tld/>oj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Volvo&rightquot;, 
        2007-10-19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Baumaschine</trd>
  <trd lng="fr">machine de chantier</trd>
  <trd lng="it"><ind>macchinario</ind> edile</trd>
</drv>

<drv mrk="masxin.kudro0o">
  <kap>kudro<tld/>o</kap>
  <snc mrk="masxin.kudro0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="M"/>o, kiu rapide kudras sub&scirc;ovatajn &scirc;tofojn:
      <ekz>
        oni a&ubreve;dis bruon de du kudro<tld/>oj, super kiuj estis
        klinitaj anka&ubreve; du virinaj figuroj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_y;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwrierez</trd>,
    <trd>mekanik gwriat</trd>
  </trdgrp>
  <trd lng="de">N&auml;hmaschine</trd>
  <trd lng="fr">machine &agrave; coudre</trd>
  <trd lng="it"><ind>macchina</ind> per cucire</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="masxin.kuir0o">
  <kap>kuir<tld/>o</kap>
  <snc mrk="masxin.kuir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ma&scirc;ino por varmigi kuirpotojn kaj kuiri man&gcirc;a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="br">fornigell</trd>
  <trd lng="de">Kochger&auml;t</trd>
  <trd lng="fr">cuisini&egrave;re</trd>
</drv>

<drv mrk="masxin.skrib0o">
  <kap><tld/>ma&scirc;ino</kap>
  <snc mrk="masxin.skrib0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <ref tip="dif" cel="tajp.0ilo">tajpilo</ref>
    <ekz>
      mi verkis, netigis per mia skrib<tld/>o iliajn manuskriptojn
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url
          ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>, 
          <lok>1954</lok>
        </fnt>.
    </ekz>
  </snc>
  <trd lng="de">Schreibmaschine</trd>
  <trd lng="en">typewriter</trd>
  <trd lng="es">m&aacute;quina de <ind>escribir</ind></trd>
  <trd lng="fr">machine &agrave; &eacute;crire</trd>
  <trd lng="hu">&iacute;r&oacute;g&eacute;p</trd>
  <trd lng="it">macchina per <ind>scrivere</ind></trd>
  <trd lng="nl">schrijfmachine</trd>
  <trd lng="ru">&c_p;&c_i;&c_sh;&c_u;&c_shch;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sv">skrivmaskin</trd>
</drv>

<drv mrk="masxin.vapor0o">
  <kap>vapor<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="masxin.vapor0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="masxin.0o.TEK"><tld lit="M"/>o</ref>, en kiu
      efikas la ekspansia forto de akvovaporo:
      <ekz>
        tiam ni ne havis vapor<tld/>ojn
        <fnt><bib>Fab4</bib>, <lok>peco da perlovico, 2.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_v;&c_a;&c_ja; <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</ind></trd>
  <trd lng="br">ijinenn dre vurezh</trd>
  <trd lng="de">Dampfmaschine</trd>
  <trd lng="fr">machine &agrave; vapeur</trd>
  <trd lng="hu">g&odblac;zg&eacute;p</trd>
  <trd lng="it"><ind>macchina</ind> a vapore</trd>
  <trd lng="nl">stoommachine</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_v;&c_a;&c_ja; s<ind>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</ind></trd>
</drv>
</art>
