<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>arbed</rad>/o
</kap>
<drv mrk="arbed.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ligna <ref tip="super" cel="plant.dauxra0o">da&ubreve;ra
      planto</ref> kun malalta trunketo, kiu disbran&ccirc;i&gcirc;as
      tuj de la bazo:
      <ekz>
        lilako, ribo, kratago estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="arbust.0o">arbusto</ref>
  <trdgrp lng="de">
    <trd>Strauch</trd>,
    <trd>Busch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bush</trd>,
    <trd>shrub</trd>
  </trdgrp>
  <trd lng="fr">arbrisseau</trd>
  <trd lng="ru">&c_k;&c_u;&c_s;&c_t;</trd>
</drv>
</art>
