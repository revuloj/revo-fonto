<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>arbed</rad>/o
</kap>
<drv mrk="arbed.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ligna <ref tip="super" cel="plant.dauxra0o">da&ubreve;ra
      planto</ref> kun malalta trunketo, kiu
      disbran&ccirc;i&gcirc;as tuj de la bazo:
      <ekz>
        lilako, ribujo, kratago estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Strauch</trd>,
    <trd>Busch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bush</trd>,
    <trd>shrub</trd>
  </trdgrp>
  <trd lng="fr">arbrisseau</trd>
  <trd lng="ru">&#1082;&#1091;&#1089;&#1090;</trd>
</drv>
</art>
