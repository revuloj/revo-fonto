<art mrk="$Id$">
<kap>
<rad>koni</rad>/o
</kap>
<drv mrk="koni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Conium</trd>), tre venena vegeta&jcirc;o kun
      blueta, ofte ru&gcirc;e makulita tigo kaj museca malbonodoro,
      simila al <ref tip="vid" cel="cikut.0o">cikuto</ref>.
    </dif>
  </snc>
  <trd lng="de">Schierling</trd>
  <trd lng="en">hemlock</trd>
  <trd lng="nl">scheerling</trd>
</drv>
</art>
