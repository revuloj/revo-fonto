<vortaro>
  <art mrk="$Id: darkemon.xml,v 1.2 2023/08/31 10:43:57 afido Exp $">
    <kap>
      <rad>darkemon</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="darkemon.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ormonero eldonita de Dario la I-a post 515 a.K.,
      60-ono de babilona <ref tip="malprt" cel="min1.0o.MEZ_MON">mino</ref>:
      <ekz>
        ili donis por la bezonoj de la domo de Dio kvin mil kikarojn da oro kaj dek mil
        <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 29:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o portas bildon de militisto a&ubreve; re&gcirc;o kun pafarko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=713161">Darkemono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Dareikos</trd>
    <trd lng="en">Persian daric</trd>
    <trd lng="fr">Darique</trd>
    <trd lng="ru">&c_D;&c_a;&c_r;&c_i;&c_k;</trd>
  </snc>
  <trd lng="ja">&#x30C0;&#x30EA;&#x30C3;&#x30AF;</trd>
</drv>

  </art>

