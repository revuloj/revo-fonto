<drv mrk="sal.0a">
  <kap><tld/>a</kap>
  <snc mrk="sal.0a.KUI">
    <dif>
      <tld lit="S"/><ref tip="lst" cel="gust.0o.eco" lst="voko:gustoj">gusta</ref>:
      <ekz>
        gustumi <tld/>an akvon
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidi&gcirc;is man&gcirc;eti sur la bordo de ia rivero, kaj lavetante <tld/>an fi&scirc;on en la
        akvo de la rivero, li eksentis tre agrablan odoron
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Li &scirc;an&gcirc;as <klr>[&#8230;]</klr> fontojn de akvo en seka&jcirc;on, fruktoportan teron en
        <tld/>an dezerton
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 107:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj &ccirc;i tiuj kunvenis en la valo <nom>Sidim</nom>, kiu nun estas la <tld lit="S"/>a Maro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 14:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a aero estas fre&scirc;a kaj agrabla
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        Larmoj la plej <tld/>aj, // Bruligu al mi tute la okulojn
        <fnt>
          <bib>Hamlet</bib>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>
  <trd lng="bo">&t_tsha;&t_tsheg;&t_kha;&t_shabchu;&t_tsheg;</trd>
  <trd lng="ca">salat</trd>
  <trdgrp lng="cs">
    <trd>osolen&yacute;</trd>,
    <trd>slan&yacute;</trd>
  </trdgrp>
  <trd lng="de">salzig</trd>
  <trd lng="en">salty</trd>
  <trd lng="es">salado</trd>
  <trd lng="fr">sal&eacute;</trd>
  <trd lng="he">&mem;&lamed;&vav;&het;</trd>
  <trd lng="hu">s&oacute;s</trd>
  <trd lng="ia">salate</trd>
  <trd lng="id">asin</trd>
  <trd lng="is">saltur</trd>
  <trd lng="it">salato</trd>
  <trd lng="km">&#6036;&#6098;&#6042;&#6083;</trd>
  <trd lng="nl">zout<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">s&lstroke;ony</trd>
  <trd lng="pt">salgado</trd>
  <trd lng="ro">s&abreve;rat</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
