<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sal</rad>/o
</kap>
<drv mrk="sal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sal.0o.natriaKlorido">
    <dif>
      Vulgara nomo de la natria klorido, konstante uzata por gustigi la
      man&gcirc;a&jcirc;ojn.
    </dif>
  </snc>
  <snc mrk="sal.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kombina&jcirc;o de acido kun bazo.
    </dif>
  </snc>
  <trd lng="de">Salz</trd>
  <trd lng="en">salt</trd>
  <trd lng="es">sal</trd>
  <trd lng="nl">zout</trd>
  <trd lng="pl">s&oacute;l</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_mol;</trd>
</drv>

<drv mrk="sal.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="S"/>gusta.
    </dif>
  </snc>
  <trd lng="de">salzig</trd>
  <trd lng="en">salt</trd>
  <trd lng="nl">zout</trd>
  <trd lng="pl">s&lstroke;ony</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>on en:
      <ekz>
        pli bone ne <tld/>i ol <tld/>i tro multe
        <klr>(ne tro babilu)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">salzen</trd>
  <trd lng="nl">zouten</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sal.ter0o0sxtono">
  <kap>ter<tld/>o, <tld/>&scirc;tono</kap>
  <snc>
    <dif>
      <tld lit="S"/>o formanta solidajn tavolojn en la tero.
    </dif>
  </snc>
</drv>

<drv mrk="sal.mar0o">
  <kap>mar<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="S"/>o ekstraktata el mara akvo per elvapori&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Meersalz</trd>
  <trd lng="nl">zeezout</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_o;&c_l;&c_mol;</trd>
</drv>
</art>
