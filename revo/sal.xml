    <drv mrk="sal.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="sal.0o.natriaKlorido">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Ordinara nomo de la natria klorido, uzata
          interalie kiel <ref tip="super" cel="spic.0o">spico</ref>
          por gustigi man&gcirc;a&jcirc;ojn.
        </dif>
        <trd lng="hi">&#x928;&#x92E;&#x915; [<ind>namak</ind>]</trd>
      </snc>
      <snc mrk="sal.0o.KEM">
        <uzo tip="fak">KEM</uzo>
        <dif>
          Kombina&jcirc;o de acido kun bazo.
        </dif>
      </snc>
  <trd lng="af">sout</trd>
  <trd lng="am">&#x1328;&#x12CD;</trd>
  <trd lng="ar">&#x645;&#x644;&#x62D;</trd>
