<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fantasmagori</rad>/o
</kap>
<drv mrk="fantas.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Arto por aperigi fantomojn per optikaj iluzioj en malluma &ccirc;ambro.
    </dif>
    <trdgrp lng="de">
     <trd>Phantasmorgie</trd>,
     <trd>Vorspiegelung von <ind>&uuml;bernat&uuml;rlichen</ind> Dingen</trd></trdgrp>
    <trdgrp lng="hu">
     <trd>r&eacute;mk&eacute;p</trd>,
     <trd>r&eacute;ml&aacute;tom&aacute;s</trd></trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Efektoj produktataj per iluziigaj rimedoj en beletro kaj belartoj.
    </dif>
    <trdgrp lng="de">
     <trd>Phantasmorgie</trd>,
     <trd>Truggebilde</trd>,
     <trd>Blendwerk</trd></trdgrp>
    <trdgrp lng="hu">
     <trd>szemf&eacute;nyveszt&eacute;s</trd>,
     <trd>fantazmag&oacute;ria</trd></trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_m;&c_a;&c_g;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="fr">fantasmagorie</trd>
  <trd lng="nl">fantasmagorie</trd>
  <trd lng="ru">&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_m;&c_a;&c_g;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
