<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sepal.xml,v 1.18 2022/05/17 08:38:34 revo Exp $">
<kap>
  <rad>sepal</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sepal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kalik.0afolio">Kalika folio</ref>:
      <ekz>
        Akva spinaco: <klr>[&#8230;]</klr> la kvin <tld/>oj estas senharaj kaj
        preska&ubreve; samformaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=560288">Akva
          spinaco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aro kaj petalaro estas, kune, la
        <ref tip="super" cel="foli.0o.BOT">florfoli</ref>aro, 
        nefekunda parto, 
        kies rolo estas protekti la fekundajn partojn kaj allogi
        polenantajn bestojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16882">Floro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="petal.0o">petalo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_v;&c_ja;&c_n;&c_o;&c_ch;&c_k;&c_a;&c_v;&c_y; &c_l;&c_ib;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_ch;&c_a;&c_sh;&c_a;&c_l;&c_ib;&c_s;&c_mol;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">s&egrave;pal</trd>
  <trdgrp lng="cs">
    <trd>kali&scaron;n&iacute; l&iacute;stek</trd>,
    <trd>sepalum</trd>
  </trdgrp>
  <trd lng="de">Kelchblatt</trd>
  <trd lng="en">sepal</trd>
  <trd lng="hu">cs&eacute;szelev&eacute;l</trd>
  <trd lng="it">sepalo</trd>
  <trd lng="nl">kelkblad</trd>
  <trd lng="pl">kielich</trd>     
  <trd lng="ro">sepal&abreve;</trd>
  <trd lng="ru">&c_ch;&c_a;&c_sh;&c_je;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">kali&scaron;n&yacute; l&iacute;stok</trd>
  <trd lng="uk">&c_ch;&c_a;&c_sh;&#x43E;&#x43B;&c_i;&c_s;&c_t;&#x43E;&#x43A;</trd>
</drv>
</art>
