<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>4</ofc> <rad>orbit</rad>/o</kap>
<drv mrk="orbit.0o">
  <kap><tld/>o</kap>
  <snc mrk="orbit.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Okulkavo.
    </dif>
    <trd lng="de">Augenh&ouml;hle</trd>
    <trd lng="el">&kappa;&omikron_ton;&gamma;&chi;&eta;<klr> (&omikron;&phi;&theta;&alfa;&lambda;&my;&omikron;&ypsilon_ton;)</klr></trd>
    <trd lng="en">eye socket</trd>
    <trd lng="fr">orbite<klr tip="ind"> (de l'oeil)</klr></trd>
    <trd lng="hu">szemg&ouml;d&ouml;r</trd>
    <trd lng="nl">oogkas</trd>
    <trd lng="pl">oczod&oacute;&lstroke;</trd>
  </snc>
  <snc mrk="orbit.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Kurba linio, sekvata de astroj (planedo, kometo) dum ilia irado
      &ccirc;irka&ubreve; alia astro:
      <ekz>
        kalkuli la <tld/>on de la luno
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ebeno de la <tld/>oj de la elektronoj
        ne estas konstanta.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Bahn</trd>,
      <trd>Umlaufbahn</trd>
    </trdgrp>
    <trd lng="cs">dr&aacute;ha</trd>
    <trd lng="el">&tau;&rho;&omikron;&chi;&jota;&alfa_ton;</trd>
    <trd lng="en">orbit</trd>
    <trd lng="es">&oacute;rbita</trd>
    <trd lng="fr">orbite</trd>
    <trdgrp lng="hu">
      <trd>p&aacute;lya</trd>,
      <trd>kering&eacute;si p&aacute;lya</trd>
    </trdgrp>
    <trd lng="it">orbita</trd>
    <trd lng="nl">baan<klr tip="ind"> (astronomie)</klr></trd>
    <trd lng="pl">orbita</trd>
    <trd lng="ru">&c_o;&c_r;&c_b;&c_i;&c_t;&c_a;</trd>
    <trd lng="tr">y&ouml;r&uuml;nge</trd>
  </snc>
  <snc mrk="orbit.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Regiono regata de iu scienco a&ubreve; influata de iu homo:
      <ekz>
        tiu speco de fenomeno entreni&gcirc;as iom post iom en la
        <tld/>on de la &hcirc;emiaj sciencoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kamp.0o">kampo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Machtbereich</trd>,
      <trd>Einflusssph&auml;re</trd>
    </trdgrp>
    <trd lng="el">&omikron_ton;&rho;&jota;&omikron;</trd>
    <trd lng="hu">hat&oacute;k&ouml;r</trd>
    <trd lng="nl">invloedssfeer</trd>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_b;&c_ib;&c_t;&c_a;</trd>
  <trd lng="en">field</trd>
  <trd lng="pt">&oacute;rbita</trd>
  <trd lng="pl">orbita</trd>
  <trd lng="ru">&c_o;&c_r;&c_b;&c_i;&c_t;&c_a;</trd>
</drv>
</art>
