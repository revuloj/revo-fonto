  <trd lng="nl">Karische Zee</trd>
  <trd lng="pl">Morze <ind>Karskie</ind></trd>
  <trd lng="ru">&c_K;&c_a;&c_r;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
</drv>

<drv mrk="mar.Kariba0o">
  <kap>
    Kariba <tld lit="M"/>o
  </kap>
  <snc mrk="mar.Kariba0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="atlant.A0o">Atlantiko</ref> norde de Sudameriko 
      kaj limigita per vasta insularo, i.a. Antiloj:
      <ekz>
        <ref cel="kub1.0o">Kubo</ref>n varmigas trankvilaj marondoj de la Kariba <tld lit="M"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Yoandra Isabel Rodr&iacute;guez Labrada</aut>
          <vrk><url ref="https://www.monato.be/2009/009753.php">Energiaj ma&scirc;inoj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum tiu periodo la nombro de urbanoj <klr>[kreskos&#x2026;]</klr> en Latin-Ameriko kaj
        la regiono de la Kariba <tld lit="M"/>o de 394 &gcirc;is 609 milionoj
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="https://www.monato.be/2007/009311.php">Urbi&gcirc;o kaj malri&ccirc;i&gcirc;o</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Karibisches Meet</trd>

</drv>

<drv mrk="mar.LaptevaM0o">
  <kap>Lapteva <tld lit="M"/>o</kap>
  <snc mrk="mar.LaptevaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld lit="M"/>o</ref>
      &ccirc;e la <ref tip="malprt" cel="siberi.S0o">Siberia</ref> bordo
      de la <ref tip="malprt" cel="ocean.ArktaO0o">Arkta
      Oceano</ref>,
      inter la <ref cel="mar.KaraaM0o">Karaa <tld lit="M"/>o</ref>
      kaj la <ref cel="mar.OrientsiberiaM0o">Orientsiberia <tld lit="M"/>o</ref>:
      <ekz>
        permafrosto de insulo situanta en la insularo <nom>Lja&ccirc;ov</nom>, en Lapteva
        <tld lit="M"/>o, norde de Siberio
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011214.php">2013:
          dek donacoj al la posteuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Olenjok</nom> <klr>[unu el]</klr> alfluantoj de la Lapteva maro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=113657">Olenjok</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="laptev.L0o">Laptevo</ref>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_r;&c_a; &c_L;&c_a;&c_p;&c_c;&c_je;&c_v;&c_y;&c_h;</trd>
  <trd lng="de">Laptev See</trd>
  <trd lng="en">Laptev Sea</trd>
  <trd lng="es">Mar de Laptev</trd>
  <trd lng="hu">Laptyev-tenger</trd>
  <trd lng="id"><baz>laut</baz> Laut Laptev</trd>
  <trd lng="nl">Laptevzee</trd>
  <trd lng="pl">Morze <ind>&Lstroke;aptiew&oacute;w</ind></trd>
  <trd lng="pt">mar de <ind>Laptev</ind></trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_je; &c_L;&c_a;&c_p;&c_t;&c_je;&c_v;&c_y;&c_h;</trd>
</drv>

<drv mrk="mar.MalvivaM0o">
  <kap>Malviva <tld lit="M"/>o <fnt><bib>FK</bib></fnt></kap>
  <snc mrk="mar.MalvivaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sala lago inter Israelo kaj Jordanio, nutrata de la rivero
      Jordano:
      <ekz>
        malgra&ubreve; tiaj malfavoraj kondi&ccirc;oj en la sal-broma akvo de la
        Malviva <tld lit="M"/>o ekzistas vivaj kreita&jcirc;oj
  	<fnt>
          <bib>FK</bib> <lok>5. El la vivo kaj sciencoj</lok>.
  	</fnt>;
      </ekz><ekz>
        unufoje <klr>[mi]</klr> min banis en la Malviva <tld lit="M"/>o
        kaj pasigis unu sabaton en Jerusalemo
        <fnt>
         <aut>S. Pokrovskij</aut>:
         <vrk>Pilgrimo en la Sanktan Landon</vrk>.
         <bib>LOdE</bib>, <lok>n-ro 61, 1999:11</lok>.
        </fnt>.
      </ekz>
    </dif>
   </snc>
   <trd lng="de">Totes Meer</trd>
   <trd lng="en">Dead Sea</trd>
   <trd lng="fr">Mer Morte</trd>
   <trd lng="pl">Morze Martwe</trd>
   <trd lng="ru">&c_M;&c_jo;&c_r;&c_t;&c_v;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
</drv>

<drv mrk="mar.MezM0o">
  <kap>Mez<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="mar.MezM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="mediterane.M0o.GEOG">Mediteraneo</ref>:
      <ekz>
        <nom>Nolan</nom> iam renkontis <nom>Burr</nom>-on en unu el niaj &scirc;ipoj, kiam grupo
        da usonanoj vizitis &gcirc;in sur la
        Mez<ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
        <fnt>
          <aut>Edward Everett Hale, trad. E. L. Robinson, trad. Don
            Harlow</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/La_Viro_Senpatria.html">La viro senpatria</url></vrk>,
          <lok>1907 kaj 1994</lok>
       </fnt>;
     </ekz><ekz>
        boato de Mez<tld/>a speco
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>aldonoj, malgranda marista
            terminaro</lok></fnt>.
     </ekz>
