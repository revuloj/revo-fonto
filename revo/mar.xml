  <trd lng="fr">Mer <ind>M&eacute;diterran&eacute;e</ind></trd>
  <trd lng="hu">F&ouml;ldk&ouml;zi-tenger</trd>
  <trdgrp lng="id">
    <trd><baz>laut</baz> Laut Mediterania</trd>,
    <trd><baz>laut</baz> Laut Tengah</trd>
 </trdgrp>
  <trd lng="nl">Middellandse Zee</trd>
  <trd lng="pl">Morze <ind>&Sacute;r&oacute;dziemne</ind></trd>
  <trd lng="ru">&c_S;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">Stredozemn&eacute; more</trd>
 </drv>

<drv mrk="mar.NigraM0o">
  <kap>Nigra <tld lit="M"/>o</kap>
  <snc mrk="mar.NigraM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Interna <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
      <ref tip="malprt" cel="atlant.A0o">Atlantika</ref>
      inter E&ubreve;ropo kaj Azio, &ccirc;e la bordoj de
      <ref cel="rus.R0ujo">Rusujo</ref>, Ukrainujo, Rumanujo,
      <ref cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref cel="turk.T0ujo">Turkujo</ref> kaj
      <ref cel="kartvel.K0ujo">Kartvelujo</ref>:
      <ekz>
        la juna okulisto denove ser&ccirc;is praktikadon en alia loko, 
        en <nom>&Hcirc;erson</nom>, apud Nigra <tld lit="M"/>o, li klopodis sensukcese
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; el Rusio &gcirc;is Ankaro estas funkcianta tubo, sed oni 
        elektis novan vojon sub la Nigra <tld lit="M"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2008/009465.php">Rivalaj
          gastuboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; 600 jaroj la Litova Granda Duklando etendi&gcirc;is &gcirc;is la 
        Nigra <tld lit="M"/>o kaj estis la plej granda &scirc;tato en E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/010165.php">Celen
          sursele</url></vrk>
        </fnt>.
      </ekz>
   </dif>
 </snc>
  <adm>
    Mi dubas, &ccirc;u vere "atlantika".
    <aut>MB</aut>
 </adm>
  <trd lng="be">&c_Ch;&c_o;&c_r;&c_n;&c_a;&c_je;
    &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="br">Mor Du</trd>
  <trd lng="bg">&c_Ch;&c_je;&c_r;&c_n;&c_o; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="de">Schwarzes Meer</trd>
  <trd lng="en">Black Sea</trd>
  <trd lng="fr">Mer Noire</trd>
  <trd lng="id"><baz>laut</baz> Laut Hitam</trd>
  <trd lng="ja">&#x9ED2;&#x6D77;
    [<ind>&#x3053;&#x3063;&#x304B;&#x3044;</ind>]</trd>
  <trd lng="nl">Zwarte Zee</trd>
  <trd lng="pl">Morze <ind>Czarne</ind></trd>
  <trd lng="pt">Mar Negro</trd>
  <trd lng="ru">&c_Ch;&c_jo;&c_r;&c_n;&c_o;&c_je;
    &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="tr">Karadeniz</trd>
 </drv>

<drv mrk="mar.NordaM0o">
  <kap>Norda <tld lit="M"/>o</kap>
  <snc mrk="mar.NordaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="atlant.A0o">Atlantiko</ref>
      limanta al nordokcidenta
      <ref tip="vid" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj orienta bordo de
      <ref tip="vid" cel="brit.B0ujo">Britujo</ref>:
      <ekz>
        la vera nigra oro de Britio ne estas la petrolo de la Norda <tld lit="M"/>o, sed la angla lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MACAOIDH</aut>
          <vrk><url ref="https://www.monato.be/2004/008394.html">La angla,
          &ccirc;u troja &ccirc;evalo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ia akvo fluas al la rejna baseno kaj plue al la Norda <tld lit="M"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2015/011396.php">&Ccirc;arnira
          urbo</url></vrk>
        </fnt>.
      </ekz>
   </dif>
 </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="br">Mor an Hanternoz</trd>
  <trd lng="de">Nordsee</trd>
  <trd lng="fr">Mer du Nord</trd>
  <trd lng="hu">&Eacute;szaki-tenger</trd>
  <trd lng="id"><baz>laut</baz> Laut Utara</trd>
  <trd lng="nl">Noordzee</trd>
  <trd lng="pl">Morze <ind>P&oacute;&lstroke;nocne</ind></trd>
  <trd lng="ru">&c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
 </drv>

<drv mrk="mar.OhxockaM0o">
  <kap>O&hcirc;ocka <tld lit="M"/>o <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="mar.OhxockaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld lit="M"/>o</ref>
      en la norda <ref tip="malprt" cel="pacifik.P0o">Pacifiko</ref>,
      okcidente de Kam&ccirc;atko kaj Kuriloj:
      <ekz>
        Sa&hcirc;aleno estas azia insulo inter la Japana Maro kaj la O&hcirc;ocka
        <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3818">Sa&hcirc;aleno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        okcidente de Hokajdo estas Japana Maro, sud-oriente estas
        Pacifiko, nord-oriente estas O&hcirc;ocka <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21333">Hokajdo</url></lok>
        </fnt>.
      </ekz>
   </dif>
   <ref tip="vid" cel="ohxock.O0o">O&hcirc;ocko</ref>
 </snc>
  <trd lng="be">&c_A;&c_h;&c_o;&c_c;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Ochotskisches Meer</trd>
  <trd lng="en">Sea of <ind>Okhotsk</ind></trd>
  <trd lng="fr">Mer d'Okhotsk</trd>
  <trd lng="hu">Ohotszki-tenger</trd>
  <trd lng="id"><baz>laut</baz> Laut Okhotsk</trd>
  <trd lng="nl">Zee van <ind>Ochotsk</ind></trd>
  <trd lng="pl">Morze <ind>Ochockie</ind></trd>
  <trd lng="pt">mar de <ind>Okhotsk</ind></trd>
  <trd lng="ru">&c_O;&c_h;&c_o;&c_t;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
 </drv>

<drv mrk="mar.OrientaCXinaM0o">
  <kap>Orienta &Ccirc;ina <tld lit="M"/>o,
    <var><kap>Orient&ccirc;ina <tld lit="M"/>o
        <fnt><bib>9OA</bib></fnt></kap></var>
  </kap>
  <snc mrk="mar.OrientaCXinaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="malprt" cel="pacifik.P0o">Pacifika</ref>
      <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
      oriente de la kontinenta <ref cel="cxin.CX0ujo">&Ccirc;inujo</ref>:
      <ekz>
        Japanion &ccirc;irka&ubreve;as <tld/>oj, Pacifika oceano en la oriento kaj
        Japana 
        <tld lit="M"/>o en la okcidento, norde aldoni&gcirc;as O&hcirc;ocka <tld lit="M"/>o kaj sude Orient&ccirc;ina <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1913">Japanio</url></lok>
        </fnt>.
      </ekz>
   </dif>
 </snc>
  <trd lng="be">&c_U;&c_s;&c_h;&c_o;&c_d;&c_n;&c_je;-&c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Ostchinesisches Meer</trd>
  <trd lng="en">East China Sea</trd>
  <trd lng="fr">Mer de Chine orientale</trd>
  <trd lng="nl">Oost-Chinese zee</trd>
  <trd lng="hu">Kelet-k&iacute;nai-tenger</trd>
  <trd lng="id"><baz>laut</baz> Laut Cina Timur</trd>
  <trd lng="pl">Morze <ind>Wschodniochi&nacute;skie</ind></trd>
  <trd lng="ru">&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_o;-&c_K;&c_i;&c_t;&c_a;&c_j;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
 </drv>

<drv mrk="mar.OrientsiberiaM0o">
  <kap>Orientsiberia <tld lit="M"/>o
  <fnt><bib>BoER</bib></fnt></kap>
  <snc mrk="mar.OrientsiberiaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="mar.0o" lst="voko:maroj">
      <tld lit="M"/>o</ref> &ccirc;e la <ref cel="siberi.S0o">Siberia</ref> bordo
      de la <ref tip="malprt" cel="ocean.ArktaO0o">Arkta
      Oceano</ref>, inter la <ref cel="mar.LaptevaM0o">Lapteva <tld lit="M"/>o</ref> kaj la
      <ref cel="mar.CXukcxaM0o">&Ccirc;uk&ccirc;a <tld lit="M"/>o</ref>:
      <ekz>
        la Arkta Oceano havas preska&ubreve;cirklan formon, kaj 
        &gcirc;ia areo <klr>[&#x2026;]</klr> inkluzivas plurajn <tld/>ojn kaj
        golfojn: nome la Bafina Golfo, la Barenca <tld lit="M"/>o, la Boforta
        <tld lit="M"/>o, la &Ccirc;uk&ccirc;a <tld lit="M"/>o, la Orientsiberia <tld lit="M"/>o, la
        Gronlanda <tld lit="M"/>o, la Hudsona Golfo, la Hudsona <tld lit="M"/>kolo, la Karska <tld lit="M"/>o, la Lapteva <tld lit="M"/>o, la Blanka <tld lit="M"/>o kaj aliajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27307">Arkta
          Oceano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jana</nom> (rivero) <klr>[&#x2026;]</klr> enfluas la orientsiberian <tld/>on,        
        parton de la glacia nordpolusa <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=109817">Jana
          (rivero)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ostsibirische See</trd>
  <trd lng="en">East Siberian Sea</trd>
  <trd lng="fr">Mer de Sib&eacute;rie Orientale</trd>
  <trd lng="hu">Kelet-szib&eacute;riai-tenger</trd>
  <trd lng="id"><baz>laut</baz> Laut Siberia Timur</trd>
  <trd lng="nl">Oost-Siberische Zee</trd>
  <trd lng="pl">Morze <ind>Wschodniosyberyjskie</ind></trd>
  <trd lng="ru">&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_o;-&c_S;&c_i;&c_b;&c_i;&c_r;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
 </drv>

<drv mrk="mar.RugxaM0o">
  <kap>Ru&gcirc;a <tld lit="M"/>o</kap>
  <snc mrk="mar.RugxaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld lit="M"/>o</ref>
      en
      <ref tip="malprt" cel="ocean.Hinda0o">Hinda Oceano</ref>
      inter nord-orienta <ref tip="vid" cel="afrik.A0o">Afriko</ref>
      kaj <ref tip="vid" cel="arab.A0aDuoninsulo">Araba
      Duoninsulo</ref>:
      <ekz>
        Dio kondukis la popolon &ccirc;irka&ubreve;ire, per la vojo tra la dezerto,
        al la Ru&gcirc;a <tld lit="M"/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 13:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili elmovi&gcirc;is de la monto <nom>Hor</nom> la&ubreve; la vojo al la Ru&gcirc;a <tld lit="M"/>o, por &ccirc;irka&ubreve;iri la landon de <nom>Edom</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 21:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        Eritreo estas &scirc;tato etendi&gcirc;anta en nordorienta Afriko sur
        <tld/>bordo de Ru&gcirc;a <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=990">Eritreo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="bg">&c_Ch;&c_je;&c_r;&c_v;&c_je;&c_n;&c_o; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="de">Rotes Meer</trd>
  <trd lng="en">Red Sea</trd>
  <trd lng="fr">Mer Rouge</trd>
  <trd lng="he">&yod;&fmem; &samekh;&vav;&fpe;</trd>
  <trd lng="hu">V&ouml;r&ouml;s-tenger</trd>
  <trd lng="id"><baz>laut</baz> Laut Merah</trd>
  <trd lng="ja">&#x7D05;&#x6D77;
    [<ind>&#x3053;&#x3046;&#x304B;&#x3044;</ind>]</trd>
  <trd lng="nl">Rode Zee</trd>
  <trd lng="pl">Morze <ind>Czerwone</ind></trd>
  <trd lng="ru">&c_K;&c_r;&c_a;&c_s;&c_n;&c_o;&c_je; &c_M;&c_o;&c_r;&c_je;</trd>
</drv>

