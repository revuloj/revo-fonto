<drv mrk="mar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mar.0o.parto">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Iel apartigita parto de la
      <ref tip="malprt" cel="ocean.0o">oceano</ref>:
      <ekz>
        la&ubreve;du la <tld/>on, sed restu sur tero
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li jam estas trans montoj kaj <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sablo sur la bordo de la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj elektitaj militestroj dronis en la Ru&gcirc;a <tld lit="M"/>o  
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        en profundega&jcirc;oj de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>o estas iom malkvieta kaj la fi&scirc;kaptistoj 
        direktas sin kun siaj &scirc;ipoj hejmen, rigardu, kiel ili tiam
        trans&scirc;ipi&gcirc;as trans la rifojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
   </dif>
    <lstref lst="voko:maroj">maroj</lstref>
    <trd lng="id">laut</trd>
  </snc>
  <snc mrk="mar.0o.oceano">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="ocean.0o">Oceano</ref>:
      <ekz>
        <tld/>o &ccirc;iujn riverojn ricevas kaj tamen ne krevas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&a_b;&a_fatha;&a_H;&a_sukun;&a_r;&a_dammatan;  &a_j;
      &a_b;&a_kasra;&a_H;&a_fatha_A;&a_r;&a_dammatan;</trd>
  </snc>
  <snc mrk="mar.0o.lago">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Iom granda sala lago (precipe en kelkaj propraj nomoj):
      <ekz>
        Kaspia <tld/>o,
     </ekz><ekz>
        Malviva <tld/>o;
     </ekz>
      <ekz>
        &ccirc;iuj &ccirc;i tiuj kunvenis en la valo <nom>Sidim</nom>, kiu nun estas la Sala
        <tld lit="M"/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 14:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mar.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Grandega kvanto:
      <ekz>
        <ind><tld/>o da sango</ind><fnt><bib>PV</bib></fnt>;
        <trd lng="hu">tengernyi v&eacute;r</trd>
        <trd lng="ru">&c_m;&c_o;&c_r;&c_je; &c_k;&c_r;&c_o;&c_v;&c_i;</trd>
      </ekz><ekz>
        <tld/>o da mizeroj
        <fnt>
          <bib>PrV</bib>
        </fnt>
        <fnt><aut>Zamenhof</aut>: <vrk>Hamleto</vrk></fnt>;
      </ekz><ekz>
        la urbo disvolvis anta&ubreve; li ondi&gcirc;antan <tld/>on da
        tegmentoj;
      </ekz>
      <ekz>
        la suno lumadis, por aran&gcirc;i <klr>[&#x2026;]</klr> <tld/>on da lumo
        <fnt>
          <bib>Fab2</bib>
          <lok>Feli&ccirc;a familio</lok>
        </fnt>;
      </ekz>
      <ekz>
        li englutis <tld/>on da teo<fnt><bib>PV</bib></fnt>;
      </ekz><ekz>
        inter diro kaj faro estas vasta <tld/>o
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk><url ref="http://miresperanto.com/frazeologio/proverbaro-3.htm">Zamenhofa
             Proverbaro 3</url></vrk>
        </fnt>
        <klr>(granda diferenco
        <refgrp tip="vid">vd. 
          <ref cel="abism.0o">abismo</ref>,
          <ref cel="mont.0o">monto</ref>
       </refgrp>)</klr>.
      </ekz>
    </dif>
    <trd lng="id"><ind>laut</ind>an</trd>
  </snc>
  <trd lng="af">see</trd>
