<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mar</rad>/o
</kap>
<drv mrk="mar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vasta etenda&jcirc;o da sala akvo,
      kiu kovras plej grandan parton de la terglobo.
    </dif>
    <trd lng="ar">&ba;&fatha;&Ha;&sukun;&ra;&dammatan; &jim; &ba;&kasra;&Ha;&fatha_alif;&ra;&dammatan;</trd>
  </snc>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Difinita parto de tiu etenda&jcirc;o:
      <ekz>
        Norda, Mediteranea <tld/>o;
      </ekz>
      <ekz>
        en landoj trans<tld/>aj estas oraj arbaroj
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mensog.0i">mensogi</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Grandega kvanto:
      <ekz>
        <ind><tld/>o da sango</ind>, da sablo, da polvo, da mizeroj<fnt>Z</fnt>;
        <trd lng="hu">tengernyi v&eacute;r</trd>
      </ekz>
      <ekz>
        la urbo disvolvis anta&ubreve; li ondi&gcirc;antan <tld/>on da tegmentoj;
      </ekz>
      <ekz>
        li englutis <tld/>on da teo<fnt>B</fnt>;
      </ekz>
      <ekz>
        inter diro kaj faro estas <tld/>o<fnt>Z</fnt>
        <klr>(granda diferenco)</klr>
        <refgrp tip="sin">
          <ref cel="abism.0o">abismo</ref>,
          <ref cel="mont.0o">monto</ref>
        </refgrp>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Meer</trd>
  <trd lng="en">sea</trd>
  <trd lng="es">mar</trd>
  <trd lng="fr">mer</trd>
  <trd lng="hu">tenger</trd>
  <trd lng="it">mare</trd>
  <trd lng="nl">zee</trd>
  <trd lng="pl">morze</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">more</trd>
  <trd lng="sv">hav</trd>
  <trd lng="tr">deniz</trd>
</drv>

<drv mrk="mar.0kolo">
  <kap><tld/>kolo</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malvasta&jcirc;o de maro.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>strait</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Meerenge</trd>,
    <trd>Meeresstra&szlig;e</trd>,
    <trd>Sund</trd>
  </trdgrp>
</drv>
<trd lng="hu">tengerszoros</trd>
</art>
