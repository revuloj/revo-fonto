<drv mrk="mar.Kariba0o">
  <kap>
    Kariba <tld lit="M"/>o
  </kap>
  <snc mrk="mar.Kariba0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="atlant.A0o">Atlantiko</ref> norde de Sudameriko
      kaj limigita per vasta insularo, i.a. Antiloj:
      <ekz>
        <ref cel="kub1.0o">Kubo</ref>n varmigas trankvilaj marondoj de la Kariba <tld lit="M"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Yoandra Isabel Rodr&iacute;guez Labrada</aut>
          <vrk><url ref="&Monato;/2009/009753.php">Energiaj ma&scirc;inoj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum tiu periodo la nombro de urbanoj <klr>[kreskos&#x2026;]</klr> en Latin-Ameriko kaj
        la regiono de la Kariba <tld lit="M"/>o de 394 &gcirc;is 609 milionoj
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2007/009311.php">Urbi&gcirc;o kaj malri&ccirc;i&gcirc;o</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_r;&c_y;&c_b;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Karibisches Meer</trd>
  <trdgrp lng="de">
      <trd>Karibisches Meer</trd>,
      <trd>Karibik</trd>
  </trdgrp>
  <trd lng="id"><ind>Laut</ind> Karibia</trd>
  <trd lng="zh">&#x52A0;&#x52D2;&#x6BD4;&#x6D77; <pr>ji&#x101;l&#xE8;b&#x1D0;h&#x1CE;i</pr></trd>
</drv>

<drv mrk="mar.LaptevaM0o">
  <kap>Lapteva <tld lit="M"/>o</kap>
  <snc mrk="mar.LaptevaM0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld lit="M"/>o</ref>
