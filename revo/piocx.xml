<drv mrk="piocx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Hakfosilo, konsistanta el ligna tenilo perpendikulara al fera
      stango, kies unu ekstremo estas pinta kaj alia plata:
      <ekz>
        montoj <klr>[&#8230;]</klr> oni prilaboradis per <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 7:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devis alporti al li hakilon kaj <tld/>on por batfaligi la kolombejon, sed
        neniu estis en &gcirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la unua vico staris la fosistoj kun pintohakiloj, en la dua kun <tld/>oj, en
        la tria kun fosiloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj <tld/>oj kaj &scirc;oveliloj estas bronzaj, sekve fortikaj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plugado rekte okazas mane per plug<tld/>o, por turni la teron, rompi la
        terbulojn
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d3/Pickaxe.jpg/307px-Pickaxe.jpg" prm="PD"><tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Pickaxe.jpg">&WCW;/File:Pickaxe.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_d;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">pic</trd>
  <trdgrp lng="de">
    <trd>zweizinkige <ind>Hacke</ind></trd>,
    <trd>Kreuzhacke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pick</trd>,
    <trd>pickaxe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pico</trd>,
    <trd>piqueta</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_l;&a_n;&f_g;</trd>
  <trd lng="fr">pioche</trd>
  <trd lng="ja">&#12388;&#12427;&#12399;&#12375;</trd>
  <trd lng="nl">houweel</trd>
  <trd lng="hu">cs&aacute;k&aacute;ny</trd>
  <trd lng="it">piccone</trd>
  <trdgrp lng="pl">
    <trd>oskard</trd>,
    <trd>kilof</trd>
  </trdgrp>
   <trdgrp lng="pt">
    <trd>alvi&atilde;o</trd>,
    <trd>picareta</trd>
  </trdgrp>
  <trd lng="ro">t&acirc;rn&abreve;cop</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_j;&c_l;&c_o;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21313;&#23383;&#37804; <pr>sh&iacute;z&igrave;g&#462;o</pr></trd>,
    <trd>&#21313;&#23383;&#38224; <pr>sh&iacute;z&igrave;g&#462;o</pr></trd>,
    <trd>&#27915;&#37804; <pr>y&aacute;ngg&#462;o</pr></trd>,
    <trd>&#27915;&#38224; <pr>y&aacute;ngg&#462;o</pr></trd>,
    <trd>&#37804;&#38957; <pr>g&#462;otou</pr></trd>,
    <trd>&#38224;&#22836; <pr>g&#462;otou</pr></trd>
  </trdgrp>
</drv>

<drv mrk="piocx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Labori per <tld/>o:
      <ekz>
        la homoj plu perlaboras la striktajn vivrimedojn <tld/>ante la teron kaj
        produktante fruktojn, olivojn kaj mielon, ne malsame ol en la pasintaj
        jarcentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010686.php">Mizeriga petrolo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol; <klr>(&c_k;&c_ib;&c_r;&c_k;&c_a;&c_j;, &c_m;&c_a;&c_t;&c_y;&c_k;&c_a;&c_j;)</klr></trd>,
    <trd>&c_a;&c_b;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">hacken</trd>
  <trd lng="es">picar</trd>
  <trd lng="ca">picar</trd>
  <trd lng="en">pickaxe</trd>
  <trd lng="fa">&f_k;&a_l;&a_n;&f_g; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">piocher</trd>
  <trd lng="hu">cs&aacute;k&aacute;nyoz</trd>
  <trd lng="it">picconare</trd>
  <trd lng="ja">&#12388;&#12427;&#12399;&#12375;&#12391;&#25496;&#12427;  <pr>&#12388;&#12427;&#12399;&#12375;&#12391;&#12411;&#12427;</pr></trd>
  <trd lng="pl">kopa&cacute;  <ind>oskardem</ind></trd>
  <trd lng="ro">lovi cu un  <ind>t&acirc;rn&abreve;cop</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_o;&c_b;&c_l;&c_ja;&c_t;&c_i; &c_k;&c_i;&c_r;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_i;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37604;&#22320; <pr>ch&uacute;d&igrave;</pr></trd>,
    <trd>&#38148;&#22320; <pr>ch&uacute;d&igrave;</pr></trd>,
    <trd>&#37917; <pr>ju&eacute;</pr></trd>,
    <trd>&#38242; <pr>ju&eacute;</pr></trd>
  </trdgrp>
</drv>
