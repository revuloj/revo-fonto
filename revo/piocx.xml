<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pio&ccirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="piocx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Hakfosilo, konsistanta el ligna tenilo perpendikulara al fera
      stango, kies unu ekstremo estas pinta kaj alia plata.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zweizinkige <ind>Hacke</ind></trd>,
    <trd>Kreuzhacke</trd>
  </trdgrp>
  <trd lng="nl">houweel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="piocx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Labori per <tld/>o.
    </dif>
  </snc>
</drv>
</art>
