<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pio&ccirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="piocx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Hakfosilo, konsistanta el ligna tenilo perpendikulara al fera
      stango, kies unu ekstremo estas pinta kaj alia plata.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_d;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zweizinkige <ind>Hacke</ind></trd>,
    <trd>Kreuzhacke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pick</trd>,
    <trd>pickaxe</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_l;&a_n;&f_g;</trd>
  <trd lng="nl">houweel</trd>
  <trd lng="hu">cs&aacute;k&aacute;ny</trd>
   <trdgrp lng="pt">
    <trd>alvi&atilde;o</trd>,
    <trd>picareta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="piocx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Labori per <tld/>o.
    </dif>
  </snc>
  <trd lng="fa">&f_k;&a_l;&a_n;&f_g; &a_z;&a_d;&a_n;</trd>
  <trd lng="hu">cs&aacute;k&aacute;nyoz</trd>
</drv>
</art>
