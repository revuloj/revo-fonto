<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>iredent</rad>/a
 <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ireden.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Celanta unui&gcirc;on kun la &ccirc;efa teritorio de la nacio.
    </dif>
  </snc>
  <trd lng="de">irredentistisch</trd>
  <trd lng="nl">onbevrijd</trd>
</drv>
<drv mrk="ireden.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Politika doktrino de tiuj, kiuj celas unui&gcirc;on kun
      la &ccirc;efa nacia teritorio.
    </dif>
  </snc>
  <trd lng="de">Irredentismus</trd>
  <trd lng="nl">irredentisme</trd>
  <trd lng="ru">&c_i;&c_r;&c_r;&c_je;&c_d;&c_je;&c_n;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
