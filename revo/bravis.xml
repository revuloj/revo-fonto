<kap>
  <rad>bravisim</rad>/e <fnt>Z</fnt> <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="bravis.0e">
  <kap><tld/>e</kap>
  <snc mrk="bravis.0e.ekkrio">
    <dif>
      <ref tip="dif" cel="brav.0e.bone">Brav</ref>ege:
      <ekz>
        <tld/>e knaboj &ndash; apla&ubreve;das la artamanta spektanto
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>  
    </dif>    
    <trd lng="be">&c_b;&c_r;&c_a;&c_v;&c_ib;&c_s;&c_ib;&c_m;&c_a;!</trd>
    <trdgrp lng="ca">
      <trd>extraordinari!</trd>,
      <trd>superb!</trd>
    </trdgrp>
    <trd lng="fr">bravissimo!</trd>
    <trd lng="hu">bravisszim&oacute;!</trd>
    <trdgrp lng="pl">
      <trd>&sacute;wietnie!</trd>,
      <trd>wspaniale!</trd>,
      <trd>fantastycznie!</trd>,
      <trd>cudownie!</trd>,
      <trd>bravissimo!</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_v;&c_i;&c_s;&c_s;&c_i;&c_m;&c_o;!</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x6700;&#x9AD8;&#xFF01;  <pr>&#x3055;&#x3044;&#x3053;&#x3046;&#xFF01;</pr></trd>,
    <trd>&#x4E16;&#x754C;&#x4E00;&#xFF01;  <pr>&#x305B;&#x304B;&#x3044;&#x3072;&#x3068;&#xFF01;</pr></trd>
  </trdgrp>
</drv>
</art>
