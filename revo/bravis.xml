<vortaro>
<art mrk="$Id$">
  <kap>
  <rad>bravisim</rad>/e
  <fnt>Z</fnt>
  </kap>
  <drv mrk="bravis.0e">
    <kap><tld/>e</kap>
    <snc>
      <dif>
        Tute <ref tip="dif" cel="brav.0e.bone">brave 2</ref>.  
      </dif>
      
      <trd
      
      lng="be">&c_b;&c_r;&c_a;&c_v;&c_ib;&c_s;&c_ib;&c_m;&c_a;!</trd>
      <trdgrp lng="ca">
        <trd>extraordinari!</trd>,
        <trd>superb!</trd>
      </trdgrp>
      <trd lng="fr">bravissimo!</trd>
      <trd lng="hu">bravisszim&oacute;!</trd>
      <trdgrp lng="pl">
        <trd>&sacute;wietnie!</trd>,
        <trd>wspaniale!</trd>,
        <trd>fantastycznie!</trd>,
        <trd>cudownie!</trd>,
        <trd>bravissimo!</trd>
      </trdgrp>
      <trd
      lng="ru">&c_b;&c_r;&c_a;&c_v;&c_i;&c_s;&c_s;&c_i;&c_m;&c_o;!</trd>
    </snc>
  </drv>
</art>
