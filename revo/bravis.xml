<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bravis.xml,v 1.14 2024/05/26 11:20:41 revo Exp $">
<kap>
  <rad>bravisim</rad>/e <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="bravis.0e">
  <kap><tld/>e</kap>
  <snc mrk="bravis.0e.ekkrio">
    <dif>
      <ref tip="dif" cel="brav.0e.bone">Brav</ref>ege:
      <ekz>
        Brave! <tld lit="B"/>e! Vi venigas min rekte al la &gcirc;usta &ccirc;apitro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Brave! Bravege! <tld lit="B"/>e!!! Sendube vi he&ubreve;ristikas
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e knaboj &ndash; apla&ubreve;das la artamanta spektanto
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_a;&c_v;&c_ib;&c_s;&c_ib;&c_m;&c_a;!</trd>
    <trdgrp lng="ca">
      <trd>extraordinari!</trd>,
      <trd>superb!</trd>
    </trdgrp>
    <trd lng="fr">bravissimo!</trd>
    <trd lng="hu">bravisszim&oacute;!</trd>
    <trdgrp lng="pl">
      <trd>&sacute;wietnie!</trd>,
      <trd>wspaniale!</trd>,
      <trd>fantastycznie!</trd>,
      <trd>cudownie!</trd>,
      <trd>bravissimo!</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_v;&c_i;&c_s;&c_s;&c_i;&c_m;&c_o;!</trd>
  </snc>
  <trd lng="de">bravissimo!</trd>
  <trdgrp lng="ja">
    <trd>&#26368;&#39640;&#65281;  <pr>&#12373;&#12356;&#12371;&#12358;&#65281;</pr></trd>,
    <trd>&#19990;&#30028;&#19968;&#65281;  <pr>&#12379;&#12363;&#12356;&#12402;&#12392;&#65281;</pr></trd>
  </trdgrp>
</drv>
</art>
