<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dist</rad>/a
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="dist.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="proksi.mal0a">malproksima</ref>
    <ekz>
      la &scirc;ildisto, vidante sin &ccirc;iufoje pli <tld/>a, devis
      krii al sia
      mastro<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 10a, p.
      77a</lok>.</fnt>
    </ekz>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_ib;</trd>
  <trd lng="de">fern</trd>
  <trd lng="fr">distant</trd>
  <trdgrp lng="pl">
    <trd>daleki</trd>,
    <trd>odleg&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>distante</trd>,
    <trd>long&iacute;nquo</trd>,
    <trd>longe</trd>
  </trdgrp>
</drv>

<drv mrk="dist.0ede">
  <kap><tld/>e de</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="proksi.mal0ede">malproksime de</ref>
    <ekz>
      ne tre <tld/>e de ni staras grupo de <klr>...</klr> altaj
      fagoj<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 12a, p.
      89a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_a; &c_a;&c_d;</trd>
  <trd lng="de">weit entfernt von</trd>
  <trd lng="fr">loin de</trd>
  <trdgrp lng="pl">
    <trd>daleko</trd>,
    <trd>z <ind>dala</ind><klr> od</klr></trd>,
    <trd>w <klr>pewnej</klr><ind>odleg&lstroke;o&sacute;ci</ind><klr>
    od</klr></trd>
  </trdgrp>
  <trd lng="pt">longe de</trd>
</drv>
</art>

