<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dist.xml,v 1.16 2023/11/04 14:29:03 revo Exp $">
<kap>
  <rad>dist</rad>/a
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="dist.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dist.0a.malproksima">
    <dif>
      <ref tip="dif" cel="proksi.mal0a">Malproksima</ref>:
      <ekz>
        la tuta konstruo estas e&ccirc; pli granda ol ili anta&ubreve;e supozis
        estante ja pli <tld/>a
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>,
        <lok>tk-stafeto, 1982</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i, tute sola, en&scirc;ipi&gcirc;is por veturi de Halifakso en Kanado cele al iu haveno en
        la <tld/>a E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;guris brila tag' feriadon en land' malproksima: &ccirc;iuj infanoj de la familio tre
        nervozis: longe kovi&gcirc;is l'&#x202F;unika plano je flugo tiel <tld/>en
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2014/011397.php">Kiel senkulpe trafi mezen de fratbu&ccirc;a
          milito</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distan.0o.SPA">distanco</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>dalek&yacute;</trd>,
    <trd>vzd&aacute;len&yacute;</trd>
  </trdgrp>
  <trd lng="de">fern</trd>
  <trd lng="en">distant, far away</trd>
  <trd lng="fr">distant</trd>
  <trd lng="nl">ver</trd>
  <trdgrp lng="pl">
    <trd>daleki</trd>,
    <trd>odleg&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>distante</trd>,
    <trd>long&iacute;nquo</trd>,
    <trd>longe</trd>
  </trdgrp>
  <trd lng="sk">vzdialen&yacute;</trd>
  <trd lng="uk">&c_m;&c_a;&c_l;&c_o;&c_v;&c_zh;&c_i;&c_v;. &c_v;&c_ib;&c_d;&c_d;&c_a;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x85C4; <pr>q&#xED;</pr></trd>,
    <trd>&#x8FDC;&#x8DDD; <pr>yu&#x1CE;nj&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dist.0ede">
  <kap><tld/>e de</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dist.0ede.malproksime">
    <dif>
      <ref tip="dif" cel="proksi.mal0ede">Malproksime de</ref>:
      <ekz>
        ne tre <tld/>e de ni staras grupo de <klr>[&#x2026;]</klr> altaj
        fagoj<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 12a, p.
        89a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_a; &c_a;&c_d;</trd>
  <trd lng="de">weit entfernt von</trd>
  <trd lng="en">far from</trd>
  <trd lng="fr">loin de</trd>
  <trd lng="nl">verwijderd van</trd>
  <trdgrp lng="pl">
    <trd>daleko</trd>,
    <trd>z <ind>dala</ind><klr> od</klr></trd>,
    <trd>w <klr>pewnej</klr> <ind>odleg&lstroke;o&sacute;ci</ind><klr>
    od</klr></trd>
  </trdgrp>
  <trd lng="pt">longe de</trd>
</drv>
</art>

