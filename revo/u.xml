<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>u</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="u.0.vokalo">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      Unu el la kvin <ref tip="dif" cel="vokal.0o">vokaloj</ref> de
      Esperanto, la malvasta malanta&ubreve;a, kaj
      nomo de tiu <ref tip="dif" cel="fonem.0o">fonemo</ref> [<tld/>].
    </dif>
    <trd lng="fr">u<klr> (voyelle)</klr></trd>
  </snc>
  <snc>
     <dif>
      La dudekkvina <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta
      alfabeto, uzata i.a. kiel:
     </dif>
     <trd lng="fr">u<klr> (lettre)</klr></trd>
     <subsnc>
     <dif>
     (maj.) signo de formo: <ekz><tld lit="U"/>-fero</ekz>;
     </dif>
     <trd lng="fr">U<klr> (forme de)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">KEM</uzo>
     <dif>
     (maj.) simbolo de uranio:<ekz> <tld lit="U"/></ekz>;
     </dif>
     <trd lng="fr">uranium</trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">GEOG</uzo>
     <dif>
     (maj.) simbolo de okcidento (uesto):<ekz> <tld lit="U"/></ekz>;
     </dif>
     <trd lng="fr">ouest</trd>
     </subsnc>
  </snc>
  <snc>
  <ref tip="vid" cel="voliti.0o">-<tld/> (volitivo)</ref>
  <trd lng="fr">-u<klr> (volitif)</klr></trd>
  </snc>
</drv>
</art>

