<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: u.xml,v 1.9 2021/05/10 18:13:54 revo Exp $">
<kap>
    <ofc>*</ofc><ofc>7</ofc><rad>u</rad>
</kap>
<drv mrk="u.0">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc mrk="u.0.vokalo">
     <dif>
      La dudekkvina <ref tip="super" cel="liter.0o.propra">litero</ref>
      de la Esperanta alfabeto
      <ekz>
        nomoj de la literoj: a, bo, co, <klr>[...]</klr>, to, u, &ubreve;o, vo,
        zo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 1</lok>
        </fnt>;
      </ekz>
      <ekz>
         Go-Po-<tld lit="U"/>! &ndash; aldonis jam tute la&ubreve;te la kasisto
         <fnt><bib>Metrop</bib></fnt>;
      </ekz>      
     uzata i.a. kiel:
     </dif>
     <subsnc>
       <dif>
       (maj.) signo de formo: <ekz><tld lit="U"/>-fero;</ekz>
       </dif>
       <trd lng="fr">U<klr> (forme de)</klr></trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">KEM</uzo>
       <dif>
       (maj.) simbolo de uranio:<ekz> <tld lit="U"/>;</ekz>
       </dif>
       <trd lng="fr">uranium</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">GEOG</uzo>
       <dif>
       (maj.) simbolo de okcidento (uesto):<ekz> <tld lit="U"/>.</ekz>
       </dif>
       <trd lng="fr">ouest</trd>
     </subsnc>
     <trd lng="fr">u<klr> (lettre)</klr></trd>
  </snc>
  <snc>
    <ref tip="vid" cel="u1.0">-<tld/></ref>  <ref tip="vid"
    cel="voliti.0o">(volitivo)</ref>
    <trd lng="fr">-u<klr> (volitif)</klr></trd>
  </snc>
</drv>
</art>

