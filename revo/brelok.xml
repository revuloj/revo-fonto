<vortaro>
<art mrk="$Id$">
<kap>
  <rad>brelok</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="brelok.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="galant2.0o">Galanterio</ref> konsistanta el
      eta penda&jcirc;o
      fiksita al vesto, juvelo, po&scirc;horlo&gcirc;a &ccirc;eno...:
      <ekz>
        soldata kostumo milkolore bunta, kovrita per mil vitraj <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 51a, p.
        378a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_r;&c_u;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="de">Berlocke</trd>
  <trd lng="fr">breloque</trd>
  <trdgrp lng="hu">
    <trd>fityeg&odblac;</trd>,
    <trd>m&uuml;ty&uuml;rke</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>brelok</trd>,
    <trd>breloczek</trd>,
    <trd>wisior</trd>,
    <trd>wisiorek</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_je;&c_l;&c_o;&c_k;</trd>
</drv>
</art>
