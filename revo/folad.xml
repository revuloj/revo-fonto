<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: folad.xml,v 1.25 2023/11/09 20:48:33 revo Exp $">
<kap>
  <rad>folad</rad>/o
</kap>
<drv mrk="folad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="folade.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pholas</trd>),
      mara <ref tip="malprt" cel="valv.du0uloj">duvalvulo</ref>,
      kiu boras en rokoj:
      <ekz>
        la Granda <tld/>o boras precipe en roka sedimento, sed anka&ubreve; en ligno kaj vivas profunde en
        tiuj bortruoj,
        &gcirc;i trovi&gcirc;as en la tajda zono kaj &gcirc;is profundeco de dek metroj.
      </ekz>
    </dif>
    <ref tip="sin" cel="konk.bor0o">borkonko</ref>
    <bld lok="&WCU;/thumb/e/ed/FMIB_50217_Pholades_dactylus%2C_having_hollowed_out_a_shelter_in_a_blo_k_of_gneiss.jpeg/494px-FMIB_50217_Pholades_dactylus%2C_having_hollowed_out_a_shelter_in_a_blo_k_of_gneiss.jpeg" prm="PD">desegno de <tld/>oj (Pholades dactylus)
        <fnt>
          <aut>Charles Bevalet, Fibuier, Louis (1868): &leftquot;Ocean World&rightquot;&nbsp;</aut>
          <lok><url ref="&WCW;/File:FMIB_50217_Pholades_dactylus,_having_hollowed_out_a_shelter_in_a_blo_k_of_gneiss.jpeg">&WCW;/File:FMIB_50217_Pholades_dactylus,_having_hollowed_out_a_shelter_in_a_blo_k_of_gneiss.jpeg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_a;&c_t;&c_o;&c_ch;</trd>
  <trd lng="ca">f&ograve;lada</trd>
  <trd lng="cs">&scaron;a&scaron;kov&aacute;n&iacute;</trd>
  <trd lng="de">Bohrmuschel</trd>
  <trd lng="fr">pholade</trd>
  <trd lng="hu">f&uacute;r&oacute;kagyl&oacute;</trd>
  <trd lng="ja">&#x30CB;&#x30AA;&#x30AC;&#x30A4;</trd>
  <trd lng="nl">boormossel</trd>
  <trd lng="pl">ska&lstroke;otocz</trd>
  <trd lng="pt">f&oacute;lade</trd>
  <trd lng="ro">sondele </trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_l;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_n;&c_je;&c_t;&c_o;&c_ch;&c_je;&c_c; <klr tip="amb">(&c_m;&c_o;&c_l;&c_l;&c_ju;&c_s;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="sk">&scaron;a&scaron;kovanie</trd>
  <trd lng="uk">&c_s;&c_v;&c_je;&c_r;&c_d;&#x43B;&c_u;&#x43D;</trd>
  <trd lng="zh">&#x947D;&#x5E8A; <pr>zu&#xE0;nchu&#xE1;ng</pr></trd>
</drv>
</art>
