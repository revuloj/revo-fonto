<art mrk="$Id$">
<kap>
  <rad>folad</rad>/o
</kap>
<drv mrk="folad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="folade.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pholas</trd>), mara konko, kiu boras en rokoj.
    </dif>
    <ref tip="sin" cel="konk.bor0o">borkonko</ref>
  </snc>
  <trd lng="de">Bohrmuschel</trd>
  <trd lng="fr">pholade</trd>
  <trd lng="nl">boormossel</trd>
  <trd lng="hu">f&uacute;r&oacute;kagyl&oacute;</trd>
  <trd lng="pt">f&#243;lade</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_l;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_n;&c_je;&c_t;&c_o;&c_ch;&c_je;&c_c; <klr tip="amb">(&c_m;&c_o;&c_l;&c_l;&c_ju;&c_s;&c_k;)</klr></trd>
  </trdgrp>
</drv>
</art>
