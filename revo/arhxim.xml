<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arhxim.xml,v 1.22 2023/10/28 08:33:08 revo Exp $">
<kap>
  <rad>Ar&hcirc;imed</rad>/o, <var><kap><rad var="k">Arkimed</rad>/o</kap></var>
</kap>
<drv mrk="arhxim.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV1</bib><lok>&leftquot;Arkimedo&rightquot;</lok></fnt>
  <snc mrk="arhxim.0o.greko">
    <dif>
      Greklingve:
      <trd lng="grc">&Alfa;&rho;&chi;&jota;&my;&eta_acute;&delta;&eta;&sigma_fina;</trd> (287-212 a.K.),
      greka <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>
      kaj fizikisto el <ref tip="malprt" cel="sirakus.0o.sicilia_urbo">Sirakuzo</ref>:
      <ekz>
        la historio pri <tld/>o ne estis precize tia, kiel oni &gcirc;in
        skribas: estas ja
        vere, ke li estis mortigita, kiam Romianoj konkeris Sirakuzon,
        sed ne estas
        &gcirc;uste, ke <klr>[&#x2026;]</klr> li &ccirc;agrene ekmurmuris: &leftquot;Ne difektu
        miajn cirklojn!&rightquot;
        <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2018/08/morto-de-arhimedo.html#!">Morto de
          Ar&hcirc;imedo | Esperanta Retradio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li venigis grekan klerulon, kies nomo estis <tld/>o, kaj
        rakontis al li sian
        timon pri la falsita oro
        <fnt>
          <vrk><url ref="https://de.m.wikibooks.org/wiki/Esperanto:_Kapitel_46">Esperanto: Kapitel
          46 - Wikibooks, Sammlung freier Lehr
              ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="k"/>o estis anka&ubreve; elstara in&gcirc;eniero, formulanta principon de
        el&jcirc;et-forto kaj
        koncernan le&gcirc;on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=151">Arkimedo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x963F;&#x57FA;&#x7C73;&#x5FB7; <pr>&#x101;j&#x12B;m&#x1D0;d&#xE9;</pr></trd>
</snc>                         
  <trd lng="be">&c_A;&c_r;&c_h;&c_ib;&c_m;&c_e;&c_d;</trd>
  <trd lng="br">Arc'himedes</trd>
  <trd lng="de">Archimedes</trd>
  <trd lng="en">Archimedes</trd>
  <trd lng="es">Arquimedes</trd>
  <trd lng="fr">Archim&egrave;de</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5DB;&#x5D9;&#x5DE;&#x5D3;&#x5E1;</trd>
  <trd lng="hu">Arkhim&eacute;d&eacute;sz</trd>
  <trd lng="it">Archimede</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30AD;&#x30E1;&#x30C7;&#x30B9;</trd>
  <trd lng="nl">Archimedes</trd>
  <trd lng="no">Arkimedes</trd>
  <trd lng="pl">Archimedes</trd>
  <trd lng="ru">&c_A;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;</trd>
  <trd lng="sk">Archimedes</trd>
  <trd lng="uk">&#x410;&#x440;&#x445;&#x456;&#x43C;&#x435;&#x434;</trd>
</drv>

<drv mrk="arhxim.a0a">
  <kap><tld lit="a"/>a</kap>
  <snc mrk="arhxim.a0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="arhxim.a0a.grupo">
      <dif>
        <klr>(p.p. adicia <ref tip="super" cel="grup.0o.MAT">grupo</ref>,
        provizita per <ref cel="tut.0eca.rilato">tuteca</ref>
        <ref cel="rilat.ordo0o">ordo-rilato</ref>)</klr>
        Tia, ke por &ccirc;iuj du strikte
        <ref cel="poziti.0a.nombro">pozitivaj</ref> elementoj
        <frm am="x">x</frm> kaj <frm am="y">y</frm> en
        &gcirc;i ekzistas tia entjero <frm am="n">n</frm>, ke
        <frm am="nx ge y">nx &#x2265; y</frm>:
        <ekz>
          la reeloj konsistigas <tld lit="a"/>an grupon.
        </ekz>
      </dif>
      <adm>
        Fonto trovenda. <aut>MB</aut>
      </adm>
      <trd lng="de">archimedisch<klr tip="amb"> (geordnete
      Gruppe)</klr></trd>
      <trd lng="en">Archimedean<klr tip="ind"> (group)</klr></trd>
      <trd lng="fr"><klr tip="ind">(groupe) </klr>archim&eacute;dien</trd>
      <trd lng="hu">arkhim&eacute;d&eacute;szi</trd>
      <trd lng="nl">Archimedische<klr tip="amb"> (groep)</klr></trd>
      <trd lng="no">arkimediske<klr tip="amb"> (gruppe)</klr></trd>
      <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_o;&c_v;&c_a;<klr tip="amb">
      (&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="arhxim.a0a.diversaj">
      <refgrp tip="vid">
        <ref cel="spiral.arhximeda0o"><tld lit="a"/>a spiralo</ref>.
      </refgrp>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_r;&c_h;&c_ib;&c_m;&c_e;&c_d;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_ib;&c_m;&c_e;&c_d;&c_a;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>des <ind>Archimedes</ind></trd>,
    <trd>Archimedisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Archimedes[']</trd>,
    <trd>Archimedean</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'<ind>Archim&egrave;de</ind></trd>,
    <trd>archim&eacute;dien</trd>
  </trdgrp>                       
  <trd lng="br">-<ind>Arc'himedes</ind></trd>
  <trd lng="hu">archimedesi</trd>
  <trdgrp lng="it">
    <trd>di Archimede</trd>,
    <trd>archimedeo</trd>
  </trdgrp>
  <trd lng="pl">Archimedesa</trd>
  <trdgrp lng="ru">
    <trd>&c_A;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
