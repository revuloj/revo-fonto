<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Ar&hcirc;imed</rad>/o
</kap>
<drv mrk="arhxim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV1</bib><lok>artikolo: Arkimedo</lok></fnt>
  <snc>
    <dif>
      Greklingve:
      <trd lng="grc">&Alfa;&rho;&chi;&jota;&my;&eta_acute;&delta;&eta;&sigma_fina;</trd>.
      Greka matematikisto kaj fizikisto el Sirakuzo, 287-212 a.K.
    </dif>
  </snc>
  <trd lng="de">Archimedes</trd>
  <trd lng="en">Archimedes</trd>
  <trd lng="es">Arquimedes</trd>
  <trd lng="fr">Archim&egrave;de</trd>
  <trd lng="ru">&c_A;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;</trd>
</drv>

<drv mrk="arhxim.a0a">
  <kap><tld lit="a"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="grup.arhximeda0o"><tld lit="a"/>a grupo</ref>,
      <ref cel="spiral.arhximeda0o"><tld lit="a"/>a spiralo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>des <ind>Archimedes</ind></trd>,
    <trd>Archimedisch</trd></trdgrp> 
  <trdgrp lng="en">
    <trd>Archimedes[']</trd>,
    <trd>Archimedean</trd></trdgrp> 
  <trdgrp lng="fr">
    <trd>d'<ind>Archim&egrave;de</ind></trd>,
    <trd>archim&eacute;dien</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_A;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_o;&c_v;</trd></trdgrp> 
</drv>
</art>

