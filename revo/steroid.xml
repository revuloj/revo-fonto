  <art mrk="$Id: steroid.xml,v 1.4 2024/03/25 07:28:36 afido Exp $">
    <kap>
      <rad>steroid</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="steroid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo><uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Iu el grupo de <ref tip="super" cel="lipid.0o">lipido</ref>j kun tri
      seskarbonaj kaj unu kvinkarbona ringoj,
      al kiu apartenas multaj hormonoj, alkaloidoj kaj vitaminoj:
      <ekz>
        estrogenoj <klr>[&#x2026;]</klr> estas <tld/>aj seksaj hormonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=517574">Estrogeno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> glata endoplasma retikulo <klr>[&#x2026;]</klr> havas plurajn funkciojn
        pri metabolismo kiel ekzemple, sintezo de <klr>[&#x2026;]</klr> <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=304133">Endoplasma retikulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/df/Trimethyl_steroid-nomenclature.png/640px-Trimethyl_steroid-nomenclature.png" prm="PD">strukturo de <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Trimethyl_steroid-nomenclature.png">https://commons.wikimedia.org/wiki/File:Trimethyl_steroid-nomenclature.png</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Steroid</trd>
    <trd lng="en">steroid</trd>
    <trd lng="fr">st&eacute;ro&iuml;de</trd>
    <trdgrp lng="pl">
      <trd>steryd</trd>,
      <trd>steroid</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_o;&c_i;&c_d;</trd>
  </snc>
<trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_o;&c_ib;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x7C7B;&#x56FA;&#x9187; <pr>l&#xE8;ig&#xF9;ch&#xFA;n</pr></trd>,
    <trd>&#x985E;&#x56FA;&#x9187; <pr>l&#xE8;ig&#xF9;ch&#xFA;n</pr></trd>,
    <trd>&#x753E;&#x916E; <pr>z&#x101;it&#xF3;ng</pr></trd>,
    <trd>&#x753E;&#x4F53;&#x6FC0;&#x7D20; <pr>z&#x101;it&#x1D0;j&#x12B;s&#xF9;</pr></trd>,
    <trd>&#x753E;&#x9AD4;&#x6FC0;&#x7D20; <pr>z&#x101;it&#x1D0;j&#x12B;s&#xF9;</pr></trd>
  </trdgrp>
</drv>
