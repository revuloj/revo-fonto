<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: deka.xml,v 1.5 2023/08/31 10:43:58 revo Exp $">
<kap>
    <rad>deka</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="deka.0">
  <kap><tld/>-</kap>
  <mlg>da</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="deka.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Dek&rightquot;, 10 en la Internacia Sistemo de unuoj (SI):
      <ekz>
        1 aro = 100 kvadrataj metroj (m<sup>2</sup>)
        = 1 kvadrata <tld/>metro (dam<sup>2</sup>)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36855">Aro (mezurunuo)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        nutra&jcirc;bezono estas minimuma, se kalkuli specialajn grajna&jcirc;ojn por &ccirc;in&ccirc;iloj, nur
        2&#x202F;-&#x202F;3 <tld/>gramoj por unu tago
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17760">&Ccirc;in&ccirc;ilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;-</trd>
  <trd lng="bg">&c_d;&c_je;&c_k;&c_a;-</trd>
  <trd lng="ca">deca-</trd>
  <trd lng="de">Deka-</trd>
  <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron;&sigma;&tau;&omikron_ton;-</trd>
  <trd lng="en">deca-</trd>
  <trd lng="es">deca-</trd>
  <trd lng="fa">&a_d;&f_k;&a_A;</trd>
  <trd lng="fr">d&eacute;ca-</trd>
  <trd lng="hu">deka-</trd>
  <trd lng="it">deca-</trd>
  <trd lng="ja">&#x30C7;&#x30AB;&#xFF5E;</trd>
  <trd lng="nl">deka-</trd>
  <trd lng="no">deka-</trd>
  <trd lng="pl">deka-</trd>
  <trd lng="pt">deca-</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;-</trd>
  <trd lng="sk">deka-</trd>
  <trd lng="sv">deka-</trd>
</drv>
</art>

