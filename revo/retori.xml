<art mrk="$Id: retori.xml,v 1.17 2021/04/11 15:58:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>retorik</rad>/o
</kap>
<drv mrk="retori.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="lst" lst="voko:artoj" cel="art.0o.belarto">Arto</ref>
      trafe kaj elokvente <ref cel="parol.0i.diri">paroli</ref>:
      <ekz>
        en la jaro 1886 la universitato posedis jam <klr>[&#8230;]</klr> kurson de
        mikroskopio, kurson teologian, kurson de <tld/>o kaj de &hcirc;emio
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_t;&c_o;&c_r;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>r&eacute;torika</trd>,
    <trd>&rcaron;e&ccaron;nictv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rhetorik</trd>,
    <trd>Redekunst</trd>
  </trdgrp>
  <trd lng="en">rhetoric</trd>
  <trd lng="es">ret&oacute;rica</trd>
  <trd lng="fr">rh&eacute;torique</trd>
  <trdgrp lng="hu">
    <trd>&eacute;kessz&oacute;l&aacute;s</trd>,
    <trd>sz&oacute;noklattan</trd>,
    <trd>retorika</trd>
  </trdgrp>
  <trd lng="nl">retoriek</trd>
  <trd lng="pt">ret&oacute;rica</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_t;&c_o;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o; <ind>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="sk">re&ccaron;n&iacute;ctvo</trd>
</drv>
