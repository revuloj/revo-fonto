<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>ceremoni</rad>/o
    </kap>
    <drv mrk="ceremo.0o">
      <kap><tld/>o</kap>
      <snc mrk="ceremo.0o.solena">
        <dif>
          Eksteraj formoj de religia a&ubreve; civila solena&jcirc;o:
          <ekz>
            edzi&gcirc;a <tld/>o,
          </ekz><ekz>
            <tld/>o de akcepto de alilanda &scirc;tatestro.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="etiket.0o">etiketo</ref>,
          <ref cel="formal">formala&jcirc;oj</ref>,
          <ref cel="rit.0o">rito</ref>.
        </refgrp>
      </snc>
      <snc mrk="ceremo.0o.gxentilajxo">
        <dif>
          Eksteraj formoj, iomete &gcirc;enaj, de &gcirc;entileco
          en neintimaj interrilatoj:
          <ekz>
            ne &gcirc;enu vin, do lasu &ccirc;iajn <tld/>ojn;
          </ekz><ekz>
            <tld/>a vesto, <tld/>ema homo.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="afekt">afekto</ref>.
        </refgrp>
      </snc>
      <trd lng="es">ceremonia</trd>
      <trd lng="fr">c&eacute;r&eacute;monie</trd>
      <trd lng="pt">cerim&ocirc;nia</trd>
      <trd lng="ru">&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
      <trd lng="sv">ceremoni</trd>
    </drv>
    <drv mrk="ceremo.sen0a">
      <kap>sen<tld/>a</kap>
      <snc>
        <dif>
          Senetiketa, sen&gcirc;ena, familiara:
          <ekz>
            sen<tld/>a vizito;
          </ekz><ekz>
            tiuj vortoj estas simple sen<tld/>a mensogado<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>simple <klr tip="ind">(sans-fa&ccedil;on)</klr></trd>,
        <trd>sans-fa&ccedil;on</trd></trdgrp>
      <trd lng="pt">sem-cerim&ocirc;nia</trd>
      <trd lng="ru">&c_b;&c_je;&c_s;&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </drv>
  </art>
