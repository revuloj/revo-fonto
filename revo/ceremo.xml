<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ceremo.xml,v 1.35 2022/04/26 16:27:52 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ceremoni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ceremo.0o">
  <kap><tld/>o</kap>
  <snc mrk="ceremo.0o.solena">
    <dif>
      Eksteraj formoj de religia a&ubreve; civila
      <ref tip="vid" cel="solen.0o.MOR">solena&jcirc;o</ref>:
      <ekz>
        funebra <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 24:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> plenumis la interedzigan <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kortega <tld/>o postulas maloportunecon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>!
      </ekz>
      <ekz>
        lia sankteco estis okupita per multenombraj religiaj <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        enterigitaj la&ubreve; la egipta <tld/>aro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidi la fatalan preparadon kaj la ekzekutajn <tld/>ojn, kaj senti, kiel
        <klr>[&#x2026;]</klr> venas pli proksime, terure pli proksime la malbenita ma&scirc;ino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etiket.0o">etiketo</ref>,
      <ref cel="pomp.0o">pompo</ref>,
      <ref cel="proces1.0o">procesio</ref>,
      <ref cel="rit.0o">rito</ref>
    </refgrp>
    <trd lng="br">lid</trd>
    <trdgrp lng="pl">
      <trd>ceremonia</trd>,
      <trd>obrz&eogonek;d</trd>
    </trdgrp>
  </snc>
  <snc mrk="ceremo.0o.gxentilajxo">
    <dif>
      Eksteraj formoj, iomete &gcirc;enaj, de &gcirc;entileco
      en neintimaj interrilatoj:
      <ekz>
        ne &gcirc;enu vin, do lasu &ccirc;iajn <tld/>ojn;
      </ekz>
      <ekz>
        <klr>[post tiu longa peto]</klr> ne farante pli <tld/>ojn, li sidi&gcirc;is kaj
        rigardis sur la kampon
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili devas eniri en la sakon, ni ne faras <tld/>ojn <klr>[&#x2026;]</klr> kaj mi ne
        ellasas ilin <klr>[&#x2026;]</klr> pli frue, ol kiam mi trovas tion bona
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faris <tld/>ojn kun la mamo de sia patrino anta&ubreve; ol su&ccirc;i &gcirc;in
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        estante re&gcirc;idino, &scirc;i ne bezonis fari grandan <tld/>on kaj tial &scirc;i diris
        <klr>[rekte&#x2026;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt">afekto</ref>,
      <ref cel="gxenti.0umi">&gcirc;entilumi</ref>
    </refgrp>
    <trd lng="de">Umst&auml;nde <klr>(H&ouml;flichkeiten)</klr></trd>
    <trd lng="pl">ceremonia</trd>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_y;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="bo">&t_ma;&t_cha;&t_naro;&t_da;&t_tsheg;&t_pa;&t_tsheg;&t_-a;&t_ba;&t_shabchu;&t_la;&t_tsheg;</trd>
  <trd lng="ca">cerim&ograve;nia</trd>
  <trdgrp lng="cs">
    <trd>ceremonie</trd>,
    <trd>ob&rcaron;ad</trd>
  </trdgrp>
  <trd lng="de">Zeremonie</trd>
  <trd lng="en">ceremony</trd>
  <trd lng="es">ceremonia</trd>
  <trdgrp lng="fr">
    <trd>c&eacute;r&eacute;monie</trd>,
    <trd>protocole</trd>
  </trdgrp>
  <trd lng="he">&tet;&#x5B6;&qof;&#x5B6;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>szertart&aacute;s</trd>,
    <trd>cerem&oacute;nia</trd>
  </trdgrp>
  <trd lng="it">cerimonia</trd>
  <trd lng="nl">ceremonie</trd>
  <trd lng="pt">cerim&ocirc;nia</trd>
  <trd lng="ru">&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>cerem&oacute;nia</trd>,
    <trd>obrad</trd>
  </trdgrp>
  <trd lng="sv">ceremoni</trd>
  <trd lng="uk">&#x446;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x43D;&#x456;&#x44F;</trd>
</drv>

<drv mrk="ceremo.0a">
  <kap><tld/>a</kap>
  <snc mrk="ceremo.0a.MOR">
    <dif>
      Karakteriza de <tld/>o, konforma al <tld/>o:
      <ekz>
        <tld/>a vesto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        antikva <tld/>a &ccirc;arego
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mankis al li ta&ubreve;gaj vestoj por <tld/>a vizito
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> &scirc;ajnas al ni ridinde <tld/>a
        <fnt>
          <bib>LR</bib>
          <lok>8. Personaj pronomoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="formal.0a">formala</ref>,
      <ref cel="solen.0a">solena</ref>
    </refgrp>
    <trd lng="de">zeremoniell</trd>
  </snc>
  <trd lng="bg">&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_a;&c_l;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>ceremoni&aacute;ln&iacute;</trd>,
    <trd>form&aacute;ln&iacute;</trd>,
    <trd>ob&rcaron;adn&yacute;</trd>
  </trdgrp>
  <trd lng="en">ceremonial</trd>
  <trd lng="fr">c&eacute;r&eacute;moniel</trd>
  <trd lng="he">&tet;&#x5B6;&qof;&#x5B6;&samekh;&yod;</trd>
  <trd lng="nl">ceremonieel</trd>
  <trd lng="pt">cerimonial</trd>
  <trdgrp lng="sk">
    <trd>form&aacute;lny</trd>,
    <trd>obradn&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x446;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x43D;&#x456;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="ceremo.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="ceremo.0ema.MOR">
    <dif>
      Zorge obeanta, eventuale tro atentanta
      la <tld/>an ordon:
      <ekz>
        unu adjutanto, unu <tld/>ema gvardiano kaj iu sinjorino el nove alvenintaj...
        <fnt>
          <aut>Lermontov M.</aut>
          <vrk><url ref="&LOdE;Mary-01.htm">Princidino Mary</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cerimoni&oacute;s</trd>
  <trdgrp lng="fr">
    <trd>c&eacute;r&eacute;monieux</trd>,
    <trd>compass&eacute;</trd>
  </trdgrp>
  <trd lng="he">&tet;&#x5B4;&qof;&samekh;&#x5B4;&yod;</trd>
</drv>

<drv mrk="ceremo.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="ceremo.0estro.ajna">
    <dif>
      Gvidanto de <tld/>o:
      <ekz>
        oni reportis la &ccirc;erkon sur la &ccirc;aron, dum iu
        de&jcirc;oranta <tld/>estro invitis nin supreniri sur
        a&ubreve;tobussimilan veturilon intertempe alvenintan
        <fnt>
        <aut>R. Schwartz</aut>,
        <vrk>&Ccirc;e l' entombigo de l' povra Aleksandro</vrk>,
        <lok>La Nica Literatura Revuo, 1:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>szertart&aacute;smester</trd>,
      <trd>protokollf&odblac;n&ouml;k</trd>
    </trdgrp>
    <trd lng="it">cerimoniere</trd>
    <trd lng="pl">mistrz <ind>ceremonii</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_v;&c_je;&c_d;&c_u;&c_shch;&c_i;&c_j;<klr tip="ind">
      (&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="ceremo.0estro.kortega">
    <dif>
      Altranga kortegano kies funkcio estas kontroli kaj gvidi
      ta&ubreve;gan plenumon de kortegaj <tld/>oj:
      <ekz>
        <ctl>sur la strato oni staras kun la baldakeno, kiun oni
        portos super Via Re&gcirc;a Mo&scirc;to en la parada
        irado!</ctl>
        raportis la &ccirc;efa <tld/>estro
        <fnt><bib>Fab1</bib>, <lok>La novaj vestoj de la
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zeremonienmeister</trd>
    <trdgrp lng="hu">
      <trd>udvarmester</trd>,
      <trd>hoppmester</trd>,
      <trd>cerem&oacute;niamester</trd>
    </trdgrp>
    <trd lng="ru">&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_j;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>
  </snc>
  <trd lng="bg">&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_a;&c_l;-&c_m;&c_a;&c_j;&c_s;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>cerimonier</trd>,
    <trd>mestre de cerim&ograve;nies</trd>
  </trdgrp>
  <trd lng="en">master of <ind>ceremonies</ind></trd>
  <trdgrp lng="fr">
    <trd>chef du protocole</trd>,
    <trd>maitre de c&eacute;r&eacute;monie</trd>
  </trdgrp>
    <trd lng="he">&mem;&nun;&het;&he;</trd>
  <trdgrp lng="pt">
    <trd>cerimonialista</trd>,
    <trd>mestre de cerim&ocirc;nias</trd>
  </trdgrp>
  <trd lng="nl">ceremoniemeester</trd>
  <trd lng="uk">&#x446;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x43D;&#x456;&#x439;&#x43C;&#x435;&#x439;&#x441;&#x442;&#x435;&#x440;</trd>
</drv>

<drv mrk="ceremo.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="ceremo.sen0a.MOR">
    <dif>
      Senetiketa, sen&gcirc;ena, familiara:
      <ekz>
        sen<tld/>a vizito;
      </ekz>
      <ekz>
        &scirc;ia honesta amiko estas tiel sen<tld/>a rilate &scirc;in, <klr>[pro kio]</klr> &scirc;i 
        mem solene anoncis al &ccirc;iuj <klr>[&#x2026;]</klr>, ke &scirc;i jam estas fian&ccirc;ino
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj viaj vortoj estas simple sen<tld/>a <klr>(<ref cel="kasx.mal0a">malka&scirc;a</ref>)</klr> 
        mensogado
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="rekt.0a.senpera">rekta<sncref/></ref>,
       <ref cel="per.sen0a">senpera</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_c;&c_y;&c_r;&c_y;&c_m;&c_o;&c_n;&c_n;&c_y;</trd>
  <trd lng="bg">&c_n;&c_je;&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_l;&c_n;&c_o;</trd>
  <trd lng="br">diardo&ugrave;</trd>
  <trdgrp lng="ca">
    <trd>informal</trd>,
    <trd>descarat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neob&rcaron;adn&yacute;</trd>,
    <trd>neost&yacute;chav&yacute;</trd>,
    <trd>prost&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ungezwungen</trd>,
    <trd>ohne F&ouml;rmlichkeiten</trd>
  </trdgrp>
  <trd lng="en">informal</trd>
  <trdgrp lng="fr">
    <trd>simple <klr tip="ind">(sans-fa&ccedil;on)</klr></trd>,
    <trd>sans-fa&ccedil;on</trd>
  </trdgrp>
  <trd lng="hu">cerem&oacute;niamentes</trd>
  <trdgrp lng="it">
  <trd>informale</trd>,
  <trd>discorsivo<klr> (informale)</klr></trd>,
  <trd>famigliare<klr> (informale)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>informeel</trd>,
    <trd>ongedwongen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezceremonialnie</trd>,
    <trd>rubasznie</trd>
  </trdgrp>
  <trd lng="pt">sem-cerim&ocirc;nia</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bez obradov</trd>,
    <trd>jednoduch&yacute;</trd>
  </trdgrp>
</drv>
</art>
