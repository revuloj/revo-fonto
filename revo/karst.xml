<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karst.xml,v 1.14 2025/12/30 18:48:13 revo Exp $">
<kap>
  <rad>karst</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="karst.0o">
  <kap><tld/>o</kap>
  <snc mrk="karst.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Malebena reliefo el precipe kalkaj &scirc;tonoj, kiun trafosis akvoj.
    </dif>
  <trd lng="en">karst</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_s;&c_t;</trd>
  <trd lng="ca">carst</trd>
  <trd lng="cs">kras</trd>
  <trd lng="de">Karst</trd>
  <trd lng="fa">&f_k;&a_A;&a_r;&a_s;&a_t;</trd>
  <trd lng="fr">karst</trd>
  <trd lng="nl">karst</trd>
  <trd lng="pl">kras</trd>
  <trd lng="ro">carst</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_s;&c_t;</trd>
  <trd lng="sk">kras</trd>
  <trd lng="uk">&c_k;&c_a;&c_r;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5580;&#x65AF;&#x7279; <pr>k&amacron;s&imacron;t&egrave;</pr></trd>,
    <trd>&#x5580;&#x65AF;&#x7279;&#x5730;&#x5F62; <pr>k&amacron;s&imacron;t&egrave;d&igrave;x&iacute;ng</pr></trd>,
    <trd>&#x5CA9;&#x6EB6; <pr>y&aacute;nr&oacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="karst.0a">
  <kap><tld/>a</kap>
  <snc mrk="karst.0a.GEO">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="klr">(pri natura areo)</uzo>
    <dif>
      Kun aspekto de <tld/>o, precipe kalka kaj tre malregula, malebena:
      <ekz>
        &ccirc;i tio estas <tld/>a lando kun dika tavolo de kalk&scirc;tonoj
        <fnt><aut>&Ccirc;ina Radio Internacia</aut>, <vrk>Pitoreska urbo
        Guilin</vrk>, <lok>2004-06-29</lok></fnt>.
      </ekz>
    </dif>
  <trd lng="en">karstic</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_s;&c_t;&c_a;&c_v;&c_y;</trd>
  <trd lng="ca">c&agrave;rstic</trd>
  <trd lng="cs">krasov&yacute;</trd>
  <trd lng="de">karstig</trd>
  <trd lng="fr">karstique</trd>
  <trd lng="nl">karst-</trd>
  <trd lng="pl">krasowy</trd>
  <trd lng="ro">carstic</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_s;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">krasov&yacute;</trd>
</drv>
</art>

