<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dekret.xml,v 1.27 2024/05/26 15:27:21 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>dekret</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="dekret.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Oficiala <ref tip="super" cel="ordon.0o.volo">ordono</ref> de rajtigita a&ubreve;toritato
      (kontraste al <ref cel="legx.0o.JUR">le&gcirc;o</ref>):
      <ekz>
        eldoni <tld/>on;
      </ekz>
      <ekz>
        en &ccirc;iu urbo <klr>[&#8230;]</klr> kien atingis la ordono kaj <tld/>o de la re&gcirc;o, estis
        &ccirc;e la Judoj &gcirc;ojo kaj gajeco
        <fnt>
          <bib>MT</bib>
          <lok>Ester 8:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katolikaj pastroj akiris Papan <tld/>on, kiu malpermesis al la hebreoj lo&gcirc;i
        en tia proksimeco de la &ccirc;efa pre&gcirc;ejo
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sentis sin kvaza&ubreve; en ka&gcirc;o de le&gcirc;oj kaj <tld/>oj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;iniciatinto&rightquot; de Esperanto <klr>[&#8230;]</klr> fidis la popolon de l' uzantoj,
        kontra&ubreve;e <nom>de Beaufront</nom> volis limigi la progreson per <tld/>oj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la baltaj respublikoj de USSR Estonio, Litovio, Latvio estis publikigitaj
        <tld/>oj pri la enkonduko de Esperanto-instruado en lernejojn sub difinitaj
        kondi&ccirc;oj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        virina peto estas <tld/>o
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estis akceptata per kakofonia hurlado de protestoj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bule.0o">buleo</ref>,
      <ref cel="edikt.0o">edikto</ref>,
      <ref cel="ukaz.0o">ukazo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_r;&c_e;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>decret</trd>,
    <trd>ordre</trd>,
    <trd>edicte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dekret</trd>,
    <trd>na&rcaron;&iacute;zen&iacute;</trd>,
    <trd>pr&aacute;vn&iacute; na&rcaron;&iacute;zen&iacute;</trd>,
    <trd>vyhl&aacute;&scaron;ka</trd>,
    <trd>vynos</trd>,
    <trd>v&yacute;m&ecaron;r</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erlass</trd>,
    <trd>Verordnung</trd>,
    <trd>Verf&uuml;gung</trd>,
    <trd>Dekret [arkaika]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decree</trd>,
    <trd>edict</trd>,
    <trd>order</trd>
  </trdgrp>
  <trd lng="es">decreto</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&f_k;&a_m;</trd>,
    <trd>&a_A;&a_m;&a_r;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cret</trd>
  <trd lng="he">&tsadi;&vav;</trd>
  <trdgrp lng="hu">
    <trd>korm&aacute;nyrendelet</trd>,
    <trd>dekr&eacute;tum</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x547D;&#x4EE4;  <pr>&#12417;&#12356;&#x308C;&#12356;</pr></trd>,
    <trd>&#x653F;&#x4EE4;  <pr>&#x305B;&#12356;&#x308C;&#12356;</pr></trd>,
    <trd>&#x52C5;&#x4EE4;  <pr>&#12385;&#12423;&#x304F;&#x308C;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">dekret</trd>
  <trd lng="pt">decreto</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_k;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_u;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>listina</trd>,
    <trd>nariadenie</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_je;&#x43A;&c_r;&c_je;&c_t;</trd>
  <trd lng="zh">&#x6CD5;&#x4EE4; <pr>f&#x1CE;l&#xEC;ng</pr></trd>
</drv>
<drv mrk="dekret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ordon.0i">Ordoni</ref> per <tld/>o:
      <ekz>
        tio efektive estas <tld/>ita, kiel neforigebla le&gcirc;o de Medujo kaj Persujo
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvar monatojn poste, <nom>Himmler</nom> <tld/>is la dissolvi&gcirc;on de la Esperanto-Movado en
        Germanio
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        duko Vilhelmo la 4a <klr>[&#8230;]</klr> <tld/>is, ke por biero oni uzu nur hordeon,
        lupolon kaj akvon
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2008/009232.php">Simpatia vulgariga fakliteraturo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjoro petas, kvaza&ubreve; <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_k;&c_r;&c_e;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol; <ind>&c_d;&c_e;&c_k;&c_r;&c_e;&c_t;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>decretar</trd>,
    <trd>preceptuar</trd>,
    <trd>ordenar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;id&ecaron;lit</trd>,
    <trd>ustanovit</trd>,
    <trd>vydat dekret</trd>,
    <trd>vydat &uacute;&rcaron;edn&iacute; rozhodnut&iacute;</trd>,
    <trd>vym&ecaron;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erlassen</trd>,
    <trd>verordnen</trd>,
    <trd>verf&uuml;gen</trd>,
    <trd>dekretieren [arkaika]</trd>
  </trdgrp>
  <trd lng="en">decree</trd>
  <trd lng="es">decretar</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&f_k;&a_m; &a_S;&a_A;&a_d;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_m;&a_r;&f_y;&a_h; &a_S;&a_A;&a_d;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cr&eacute;ter</trd>
  <trd lng="he">&lamed;&tsadi;&vav;&vav;&tav;</trd>
  <trd lng="hu">elrendel</trd>
  <trdgrp lng="ja">
    <trd>&#x767A;&#x4EE4;&#12377;&#x308B;  <pr>&#x306F;&#12388;&#x308C;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5E03;&#x544A;&#12377;&#x308B;  <pr>&#12405;&#12371;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">dekretowa&cacute;</trd>
  <trd lng="ru">
  &c_d;&c_je;&c_k;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
  </trd>
  <trd lng="sk">vyda&tcaron; dekr&eacute;t</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_d;&c_je;&#x43A;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&#x43D;&#x43E;&c_v;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i; &c_d;&c_je;&#x43A;&c_r;&c_je;&c_t;&#x43E;&#x43C;</trd>,
    <trd>&c_d;&c_je;&#x43A;&c_r;&c_je;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#38930;&#x884C; <pr>b&amacron;nx&#xED;ng</pr></trd>
</drv>
</art>
