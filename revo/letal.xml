<kap>
  <rad>letal</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="letal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="letal.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="mort.0iga.KOMUNE">Mortiga</ref>:
