<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fenic</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fenic.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de popolo vivinta ekde la 3a jarmilo a. K. en <ref tip="vid"
      cel="fenic.0ujo"><tld lit="F"/>ujo</ref>, aktiva en &scirc;ipa komercado
      kaj koloniado &ccirc;irka&ubreve; Mediteraneo,
      ekde la 9a jarcento a. K. dependa je diversaj imperioj:
      <ekz>
        la <tld/>oj parolis &scirc;emidan lingvon
        <fnt>
        <aut>Sergio Pokrovskij</aut>,
        <vrk>Slavonaj skriboj</vrk>,
        <bib>LOdE</bib>,
        <lok>numero 52a, 1999:2a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_je;&c_c;</trd>
  <trd lng="fr">Ph&eacute;nicien<klr> (subst.)</klr></trd>
</drv>

<drv mrk="fenic.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="fr">ph&eacute;nicien<klr> (adj.)</klr></trd>
</drv>

<drv mrk="fenic.0ujo">
  <kap><tld lit="F"/>ujo</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      La devena lando de <tld/>oj, inter Lebanona montaro kaj
      la orienta bordo de Mediteranea maro, proksimume nuna Libano.
    </dif>
    <ref tip="sin" cel="fenici.0o">Fenicio</ref>
  </snc>
  <trd lng="be">&c_F;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_ja;</trd>
  <trd lng="fr">Ph&eacute;nicie</trd>
</drv>
</art>

