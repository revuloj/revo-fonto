<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anemon.xml,v 1.35 2024/01/05 01:35:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>anemon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="anemon.0o">
  <kap><tld/>o</kap>
  <snc mrk="anemon.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Vegeta&jcirc;o kun belaj floroj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref>,
      (<trd lng="la">Anemone</trd>):
      <ekz>
        palru&gcirc;aj <tld/>oj en la verda&jcirc;o belege aspektis
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur kun malfacileco oni povis iri anta&ubreve;en, asperuloj kaj
        <tld/>oj kreskis tro abunde
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la violoj staris en plena florado, la <tld/>oj kaj leontodoj
        disvolvi&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj haroj estis ornamitaj per <tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        tutsola iros mi jen inter <tld/>o
        kaj la sova&gcirc;o de &gcirc;ardenoj sen ord'
        <fnt>
          <aut>J. Yvart</aut>,
          <vrk>Homo kaj Maro</vrk>,
          <lok>Invito por voja&gcirc;o, 2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/b/b7/Anemone_stolonifera_1.JPG/640px-Anemone_stolonifera_1.JPG" prm="CC
  BY-SA 3.0">anemono (Anemone stolonifera), Fukushima, Japanujo
       <fnt>
         <aut>Qwert1234</aut>
         <lok><url
         ref="&WCW;/File:Anemone_stolonifera_1.JPG">&WCW;/File:Anemone_stolonifera_1.JPG</url></lok>
       </fnt>
  </bld>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_e;&c_m;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_p;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_n;&c_je;&c_m;&c_o;&c_n;&c_a;</trd>
  <trd lng="ca">anemone</trd>
  <trdgrp lng="de">
      <trd>Windr&ouml;schen</trd>,
      <trd>Anemone</trd>
  </trdgrp>
  <trd lng="en">anemone</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_q;&a_A;&f_y;&a_q; &a_n;&a_ayn;&a_m;&a_A;&a_n;&f_y;</trd>,
    <trd>&a_A_madda;&a_n;&a_m;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">an&eacute;mone</trd>
  <trd lng="he">&#x5DB;&#x5BC;&#x5B7;&#x5DC;&#x5B8;&#x5E0;&#x5B4;&#x5D9;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>kakukkvir&aacute;g</trd>,
    <trd>szell&odblac;r&oacute;zsa</trd>,
    <trd>p&aacute;pics</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30CD;&#x30E2;&#x30CD;</trd>
  <trd lng="nl">anemoon</trd>
  <trd lng="pl">zawilec</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_m;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_i;&c_c;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>anemon</trd>,
    <trd>sippa</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(sasanka)</klr>*</trd>,
    <trd>anemonka</trd>,
    <trd>veternica</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>anem&oacute;na</trd>,
    <trd>anem&oacute;nka</trd>,
    <trd>sasanka</trd>
  </trdgrp>
  <trd lng="id">anemon</trd>
  <trd lng="uk">&c_a;&#x43D;&c_je;&#x43C;&#x43E;&#x43D;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#37504;&#x84EE;&#x82B1; <pr>y&#xED;nli&#xE1;nhu&amacron;</pr></trd>,
    <trd>&#x94F6;&#x83B2;&#x82B1; <pr>y&#xED;nli&#xE1;nhu&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="anemon.mar0oj">
  <kap>mar<tld/>oj</kap>
  <snc mrk="anemon.mar0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref>
      de florosimilaj koralaj
      <ref tip="super" cel="polip.0o">polipoj</ref>
      el <ref tip="malprt" cel="knid.0uloj">kniduloj</ref>
      (<trd lng="la">Actiniaria</trd>):
      <ekz>
        multaj specioj <klr>[de salikokoj]</klr> vivas
        simbioze kun maranemonoj
        <fnt>
          <aut>trad. P. Desmet'</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/petro_desmet/400854">
          Salikokoj</url></vrk>,
          <lok>2012-07-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6D77;&#33909; <pr>h&#x1CE;iku&#xED;</pr></trd>,
    <trd>&#x6D77;&#33909;&#x76EE; <pr>h&#x1CE;iku&#xED;m&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <bld
  lok="&WCU;/thumb/5/56/Sea_Anemone_%2810062178943%29.jpg/640px-Sea_Anemone_%2810062178943%29.jpg"
  prm="CC0">maranemono
       <fnt>
         <aut>Bernard Spragg. el Christchurch, Nov-Zelando</aut>
         <lok><url
         ref="&WCW;/File:Sea_Anemone_(10062178943).jpg">&WCW;/File:Sea_Anemone_(10062178943).jpg</url></lok>
       </fnt>
  </bld>
  <trdgrp lng="bg">
    <trd>&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a; &c_r;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">mo&rcaron;sk&eacute; sasanky</trd>
  <trd lng="de">Seeanemone</trd>
  <trdgrp lng="en">
    <trd>actinia</trd>,
    <trd>sea anemone</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&a_q;&a_A;&f_y;&a_q; &a_d;&a_r;&f_y;&a_A;&f_y;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>actinie</trd>,
    <trd>an&eacute;mone de mer</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>anemon</ind> laut</trd>,
    <trd>Actiniaria</trd>
  </trdgrp>
  <trd lng="pl">ukwia&lstroke;y</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j;
    &c_a;&c_n;&c_je;&c_m;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">morsk&eacute; sasanky</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43A;&c_t;&c_i;&#x43D;&c_ib;&c_ji;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_ib; &c_a;&#x43D;&c_je;&#x43C;&#x43E;&#x43D;&c_i;</trd>
  </trdgrp>
</drv>

</art>
