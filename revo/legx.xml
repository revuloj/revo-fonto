<drv mrk="legx.laux0igi">
  <kap>la&ubreve;<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="legx.laux0igi.JUR">
    <dif>
      Igi konforma al <tld/>o;
      <ref tip="sin" cel="legx.0igi.lauxlegxigi">le&gcirc;igi<sncref/></ref>:
      <ekz>
        por la&ubreve;<tld/>igi &scirc;in kiel familianon, mia patrino
        organizis malgrandan tefeston
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        litovoj, kiuj patologie inklinas al vetludado, libervole
        prezentis
        siajn datumojn kaj fotojn al &scirc;tata komisiono pri vetado,
        petante,
        ke ludsalonoj ilin identigu kaj ne enlasu, tiel 71 homoj celas
        sin
        protekti dum tiel nomataj malfortaj momentoj, &ccirc;i tiun
        eksperimenton
        oni esperas nun formale la&ubreve;<tld/>igi
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008895.php">Veto-peto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_u; &c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_z; <ind>&c_z;&c_a;&c_k;&c_o;&c_n;&c_a;&c_m;</ind></trd>
