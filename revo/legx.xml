<drv mrk="legx.natur0o">
  <kap>natur<tld/>o,
  <var><kap>natura <tld/>o</kap></var></kap>
  <snc mrk="legx.natur0o.SCI">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Konstanta universala regulo,      
      kiun oni rigardas nepre okazanta
      en fenomenoj de la naturo:
      <ekz>
        la natur<tld/>o de gravito, kiel konate, estis malkovrita 
        <klr>[&#x2026;]</klr> de Ne&ubreve;tono
        <fnt>
          <aut>J. S.Larsen</aut>,
          <vrk><url ref="http://www.liberafolio.org/2009/multlingva-brosuro-proponas-malkovri-esperanton#1248900902">
          Neniu havas neniun teorion</url></vrk>, 
          <lok>libera folio, 2009-07-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;ina lingvo troveblas multaj proverboj pri agrikulturo 
        kaj meteologio <klr>[&#x2026;]</klr>, parto 
        el ili ja konformas al la natura <tld/>o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://esperanto.cri.cn/1/2004/10/11/1@15854.htm">
          Proverboj en la &ccirc;ina lingvo</url></vrk>,
          <lok>2004-10-11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_k;&c_o;&c_n; &c_p;&c_r;&c_y;&c_r;&c_o;&c_d;&c_y;</trd>
    <trd lng="fr">loi de la nature</trd>
    <trd lng="id"><ind>hukum</ind> alam</trd>
    <trd lng="pl">prawo natury</trd>     
    <trd lng="ro">drept natural</trd>
  </snc>
</drv>
