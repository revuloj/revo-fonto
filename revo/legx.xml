      </ekz>
      <ekz>
        fonetikaj <tld/>oj
      </ekz>
      <ekz>
        instrui la <tld/>ojn de la fiziko
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011597.php">&Scirc;ton&jcirc;eto sen
          cenzuro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        somero kaj vintro <klr>[&#8230;]</klr> en &ccirc;iu pejza&gcirc;o obeas al aliaj
        le&gcirc;oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Flugo al la Ebura Turo</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="princi.0o">principo</ref>
    </refgrp>
    <trd lng="en">principle</trd>
    <trdgrp lng="id">
