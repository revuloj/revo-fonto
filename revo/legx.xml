<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>le&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="legx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="legx.0o.JUR">
    <tezrad fak="JUR"/>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Deviga decido, proklamita de plej supera &scirc;tata a&ubreve;toritato,
      ordonanta a&ubreve; malpermesanta ion:
      <ekz>
        la civilaj, politikaj, kriminalaj, konstituciaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>o pri minejoj;
      </ekz>
      <ekz>
        doni, fari, vo&ccirc;doni, nuligi <tld/>on;
      </ekz>
      <ekz>
        <tld/>o mallertulon ligas, lertulon fortigas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>ofaranto, <tld/>odonanto;
      </ekz>
      <ekz>
        <tld/>o pasinta&jcirc;on ne tu&scirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        popola kutimo havas valoron de <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        nesciado de <tld/>o neniun pravigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="legx.0o.komune">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Deviga regulo:
      <ekz>
        la <tld/>oj de la dececo, de la mondumaj rilatoj, de la moralo,
        de la &gcirc;entileco;
      </ekz>
      <ekz>
        la <tld/>o de la pli forta;
      </ekz>
      <ekz>
        deziro <tld/>ojn ne konas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la&ubreve; &ccirc;iuj <tld/>oj de la arto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="en">rule</trd>
  </snc>
  <snc mrk="legx.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <ref tip="dif" cel="legx.natur0o.SCI">natur<tld/>o</ref>
    <ekz>
      la <tld/>oj de la naturo, de la fiziko;
    </ekz>
    <ekz>
      fonetikaj <tld/>oj
    </ekz>
    <refgrp tip="vid">
      <ref cel="princi.0o">principo</ref>
    </refgrp>
    <trd lng="en">principle</trd>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_o;&c_m;&c_je;&c_r;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_o;&c_n;</trd>
  <trd lng="bg">&c_z;&c_a;&c_k;&c_o;&c_n;</trd>
  <trd lng="br">lezenn</trd>
  <trd lng="cs">z&aacute;kon</trd>
  <trd lng="de">Gesetz</trd>
  <trd lng="el">&ny;&omikron_ton;&my;&omikron;&sigma_fina;</trd>
  <trd lng="en">law</trd>
  <trd lng="es">ley</trd>
  <trd lng="ca">llei</trd>
  <trdgrp lng="fr">
    <trd>loi</trd>
  </trdgrp>
  <trd lng="hu">t&ouml;rv&eacute;ny</trd>
  <trd lng="nl">wet</trd>
  <trd lng="pt">lei</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;</trd>
</drv>

<drv mrk="legx.0a">
  <kap><tld/>a</kap>
  <snc mrk="legx.0a.prilegxa">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld/>a forto, povo.
      </ekz>
    </dif>
    <trd lng="de">gesetzlich</trd>
  </snc>
  <snc mrk="legx.0a.lauxlegxa">
    <ref tip="dif" cel="legx.laux0a.JUR">la&ubreve;<tld/>a</ref>
    <ekz>
      <tld/>a decido;
    </ekz>
    <ekz>
      sur <tld/>a bazo
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      <tld/>aj elektoj<fnt>Z</fnt>;
    </ekz>
    <ekz>
      lia <tld/>a edzino<fnt>Z</fnt>;
    </ekz>
    <ekz>
      <tld/>aj, ne<tld/>aj infanoj;
    </ekz>
    <ekz>
      <tld/>e kaj rajte
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="en">lawful</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>
  <trd lng="br">lezennel</trd>
  <trd lng="de">legal</trd>
  <trd lng="en">legal</trd>
  <trd lng="el">&ny;&omikron_ton;&my;&jota;&my;&omikron;&sigma_fina;</trd>
  <trd lng="es">legal</trd>
  <trd lng="ca">legal</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gal</trd>,
    <trd>licite</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wettelijk</trd>,
    <trd>wettig</trd>
  </trdgrp>
  <trd lng="pt">lei</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="legx.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="legx.0aro.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Kolekto a&ubreve; sistemo de <tld/>oj, referencaj por iu homgrupo:
      <ekz>
        <tld/>aro pli justa inter homoj
        <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 7a, p. 76a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_a;&c_n;&c_a;&c_d;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="br">lezennaoueg</trd>
  <trdgrp lng="en">
    <trd>laws</trd>,
    <trd>statutes</trd>
  </trdgrp>
  <trd lng="es">legislaci&oacute;n</trd>
  <trd lng="ca">legislaci&oacute;</trd>
  <trd lng="fr">l&eacute;gislation</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_o;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="legx.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="legx.0isto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Profesiulo sperta pri la <tld/>oj kaj iliaj aplikoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_ju;&c_r;&c_y;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>gwiraour</trd>,
    <trd>lezennour</trd>
  </trdgrp>
  <trd lng="en">lawyer</trd>
  <trdgrp lng="fr">
    <trd>juriste</trd>,
    <trd>l&eacute;giste</trd>
  </trdgrp>
  <trd lng="hu">jogtud&oacute;s</trd>
  <trd lng="nl">rechtsgeleerde</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_ju;&c_r;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="legx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="legx.0igi.legxofari">
    <dif>
      Doni <tld/>ojn al:
      <ekz>
        <tld/>igi popolon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi la fundamentojn de la tero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_c;&c_mol; <ind>&c_z;&c_a;&c_k;&c_o;&c_n;</ind></trd>
    <trd lng="br">rei&ntilde; <ind>lezenno&ugrave;</ind> da</trd>
    <trd lng="en">give laws to</trd>
    <trd lng="fr">donner des <ind>lois</ind></trd>
    <trd lng="nl">wetten geven aan</trd>
    <trd lng="ru">&c_d;&c_a;&c_t;&c_mol; <ind>&c_z;&c_a;&c_k;&c_o;&c_n;</ind></trd>
  </snc>
  <snc mrk="legx.0igi.lauxlegxigi">
    <ref tip="dif" cel="legx.laux0igi.JUR">la&ubreve;le&gcirc;igi</ref>
    <ekz>
      <tld/>igi situacion, siajn rajtojn, pretendojn,
      eksteredzecan infanon.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_a;&c_k;&c_o;&c_n;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_je;&c_g;&c_ib;&c_t;&c_y;&c_m;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">lezennekaat</trd>
    <trd lng="en">legalize</trd>
    <trdgrp lng="fr">
      <trd>l&eacute;galiser</trd>,
      <trd>l&eacute;gitimer</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;rv&eacute;nyes&iacute;t</trd>
    <trd lng="nl">wettigen</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_z;&c_a;&c_k;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_l;&c_je;&c_g;&c_i;&c_t;&c_i;&c_m;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="el">&ny;&omikron;&my;&jota;&my;&omikron;&pi;&omikron;&jota;&omega_ton;</trd>
  <trd lng="pt">legalizar</trd>
  <trd lng="es">legalizar</trd>
  <trd lng="ca">legalitzar</trd>
</drv>

<drv mrk="legx.kontraux0a">
  <kap>kontra&ubreve;<tld/>a</kap>
  <snc mrk="legx.kontraux0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ne<tld/>a, ne konforma al <tld/>o:
      <ekz>
        kontra&ubreve;<tld/>a decido, entrepreno.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">enep al <ind>lezenn</ind></trd>
  <trd lng="de">illegal</trd>
  <trd lng="en">illegal</trd>
  <trd lng="ca">il&middot;legal</trd>
  <trdgrp lng="fr">
    <trd>ill&eacute;gal</trd>,
    <trd>illicite</trd>
  </trdgrp>
  <trd lng="es">ilegal</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;rv&eacute;nyellenes</trd>,
    <trd>illeg&aacute;lis</trd>
  </trdgrp>
  <trd lng="nl">onwettig</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="legx.laux0a">
  <kap>la&ubreve;<tld/>a</kap>
  <snc mrk="legx.laux0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Konforma al <tld/>oj, permesita de la <tld/>oj;
      <ref tip="sin" cel="legx.0a.lauxlegxa"><tld/>a<sncref/></ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">hervez al <ind>lezenn</ind></trd>
  <trd lng="ca">legal</trd>
  <trd lng="de">legal</trd>
  <trd lng="en">legal</trd>
  <trd lng="es">legal</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gal</trd>,
    <trd>licite</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;rv&eacute;nyes</trd>,
    <trd>t&ouml;rv&eacute;nyszer&udblac;</trd>
  </trdgrp>
  <trd lng="nl">wettig</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="legx.laux0igi">
  <kap>la&ubreve;<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="legx.laux0igi.JUR">
    <dif>
      Igi konforma al <tld/>o;
      <ref tip="sin"
      cel="legx.0igi.lauxlegxigi">la&ubreve;le&gcirc;igi<sncref/></ref>:
      <ekz>
        por la&ubreve;<tld/>igi &scirc;in kiel familianon, mia patrino 
        organizis malgrandan tefeston
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">l&eacute;galiser</trd>
</drv>

<drv mrk="legx.0oforta">
  <kap><tld/>oforta</kap>
  <snc>
    <dif>
      Same deviga kiel <tld/>o:
      <ekz>
        <tld/>oforta dekreto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_ib; &c_m;&c_a;&c_je; &c_s;&c_ib;&c_l;&c_u; <ind>&c_z;&c_a;&c_k;&c_o;&c_n;&c_u;</ind></trd>
  <trd lng="en">by force of law</trd>
  <trd lng="fr">ayant force de <ind>loi</ind></trd>
  <trd lng="hu">t&ouml;rv&eacute;nyerej&udblac;</trd>
  <trd lng="nl">wetskracht hebbend</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_s;&c_i;&c_l;&c_u; <ind>&c_z;&c_a;&c_k;&c_o;&c_n;&c_a;</ind></trd>
</drv>

<drv mrk="legx.0oscienco">
  <kap><tld/>oscienco</kap>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Scienco pri <tld/>oj kaj iliaj aplikoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="jur.0o">juro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_a;&c_n;&c_a;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="br">gwir<klr tip="ind"> (skiant)</klr></trd>
  <trdgrp lng="de">
    <trd>Rechtswissenschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>legal science</trd>,
    <trd>jurisprudence</trd>,
    <trd>law</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>droit<klr> (sci.)</klr></trd>
  </trdgrp>
  <trd lng="hu">jogtudom&aacute;ny</trd>
  <trd lng="nl">rechtsgeleerdheid</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;<klr tip="amb"> (&c_n;&c_a;&c_u;&c_k;&c_a;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="legx.kepleraj0oj">
  <kap>kepleraj <tld/>oj</kap>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">MAT</uzo>
    <refgrp tip="vid">
      <ref cel="kepler.0o">Keplero</ref>;
      <ref cel="elips.0o.linio">elipso</ref>,
      <ref cel="vektor.radius0o">radiusvektoro</ref>,
      <ref cel="aks.grandaduon0o">granda duonakso</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_o;&c_n;&c_y; &c_K;&c_je;&c_p;&c_l;&c_je;&c_r;&c_a;</trd>
  <trd lng="br">lezenno&ugrave; Kepler</trd>
  <trd lng="de">Keplersche <ind>Gesetze</ind></trd>
  <trd lng="en">Kepler's <ind>laws</ind></trd>
  <trd lng="fr">lois de Kepler</trd>
  <trd lng="hu">Kepler-t&ouml;rv&eacute;nyek</trd>
  <trd lng="pl">prawa Keplera</trd>
  <trd lng="pt">Leis de Kepler</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_y; &c_K;&c_je;&c_p;&c_l;&c_je;&c_r;&c_a;</trd>
</drv>

<drv mrk="legx.natur0o">
  <kap>natur<tld/>o,
  <var><kap>natura <tld/>o</kap></var></kap>
  <snc mrk="legx.natur0o.SCI">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Konstanta universala regulo,      
      kiun oni rigardas nepre okazanta
      en fenomenoj de la naturo:
      <ekz>
        La natur<tld/>o de gravito, kiel konate, estis malkovrita 
        <klr>...</klr> de Ne&ubreve;tono
        <fnt>
          <aut>J. S.Larsen</aut>,
          <vrk><url
          ref="http://www.liberafolio.org/2009/multlingva-brosuro-proponas-malkovri-esperanton#1248900902">
          Neniu havas neniun teorion</url></vrk>, 
          <lok>libera folio, 2009-07-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;ina lingvo troveblas multaj proverboj pri agrikulturo 
        kaj meteologio <klr>...</klr>, parto 
        el ili ja konformas al la natura <tld/>o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/1/2004/10/11/1@15854.htm">
          Proverboj en la &ccirc;ina lingvo</url></vrk>,
          <lok>2004-10-11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">loi de la nature</trd>
  </snc>
</drv>
</art>
