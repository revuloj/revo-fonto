    <trd>gesetzgebend</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26377;&#x7ACB;&#x6CD5; <pr>y&#x1D2;ul&#xEC;f&#x1CE;</pr></trd>,
    <trd>&#x7ACB;&#x6CD5;&#30340; <pr>l&#xEC;f&#x1CE;de</pr></trd>,
    <trd>&#x7ACB;&#x6CD5; <pr>l&#xEC;f&#x1CE;</pr></trd>
  </trdgrp>
</drv>

