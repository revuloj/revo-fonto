<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>le&gcirc;</rad>/o
</kap>
<drv mrk="legx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="legx.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Deviga decido, proklamita de plej supera &scirc;tata a&ubreve;toritato,
      ordonanta a&ubreve; malpermesanta ion:
      <ekz>
        la civilaj, politikaj, kriminalaj, konstituciaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>o pri minejoj;
      </ekz>
      <ekz>
        doni, fari, vo&ccirc;doni, nuligi <tld/>on;
      </ekz>
      <ekz>
        <tld/>o mallertulon ligas, lertulon fortigas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>ofaranto, <tld/>odonanto;
      </ekz>
      <ekz>
        <tld/>o pasinta&jcirc;on ne tu&scirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        popola kutimo havas valoron de <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        nesciado de <tld/>o neniun pravigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="legx.0o.komune">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Deviga regulo:
      <ekz>
        la <tld/>oj de la dececo, de la mondumaj rilatoj, de la moralo,
        de la &gcirc;entileco;
      </ekz>
      <ekz>
        la <tld/>o de la pli forta;
      </ekz>
      <ekz>
        deziro <tld/>ojn ne konas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la&ubreve; &ccirc;iuj <tld/>oj de la arto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="legx.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Konstanta universala regulo, kiu regas la fenomenojn de la naturo:
      <ekz>
        la <tld/>oj de la naturo, de la fiziko;
      </ekz>
      <ekz>
        fonetikaj <tld/>oj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="princi.0o">principo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">z&aacute;kon</trd>
  <trd lng="el">&ny;&omikron_ton;&my;&omikron;&sigma_fina;</trd>
  <trd lng="en">law</trd>
  <trd lng="es">ley</trd>
  <trdgrp lng="fr">
    <trd>loi</trd>
  </trdgrp>
  <trd lng="hu">t&ouml;rv&eacute;ny</trd>
  <trd lng="nl">wet</trd>
  <trd lng="pt">lei</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;</trd>
</drv>

<drv mrk="legx.0a">
  <kap><tld/>a</kap>
  <snc mrk="legx.0a.prilegxa">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld/>a forto, povo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="legx.0a.lauxlegxa">
    <ref tip="dif" cel="legx.laux0a">la&ubreve;<tld/>a</ref>
    <ekz>
      <tld/>a decido;
    </ekz>
    <ekz>
      sur <tld/>a bazo
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      <tld/>aj elektoj<fnt>Z</fnt>;
    </ekz>
    <ekz>
      lia <tld/>a edzino<fnt>Z</fnt>;
    </ekz>
    <ekz>
      <tld/>aj, ne<tld/>aj infanoj;
    </ekz>
    <ekz>
      <tld/>e kaj rajte
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="el">&ny;&omikron_ton;&my;&jota;&my;&omikron;&sigma_fina;</trd>
  <trd lng="es">legal</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gal</trd>,
    <trd>licite</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wettelijk</trd>,
    <trd>wettig</trd>
  </trdgrp>
  <trd lng="pt">lei</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="legx.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Kolekto a&ubreve; sistemo de le&gcirc;oj, referencaj por iu homgrupo:
      <ekz><tld/>aro pli justa inter homoj
      <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 7a, p. 76a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>l&eacute;gislation</trd>
  </trdgrp>
</drv>

<drv mrk="legx.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Profesiulo sperta pri la <tld/>oj kaj iliaj aplikoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>juriste</trd>,
    <trd>l&eacute;giste</trd>
  </trdgrp>
  <trd lng="hu">jogtud&oacute;s</trd>
  <trd lng="nl">rechtsgeleerde</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="legx.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi la&ubreve;<tld/>a:
      <ekz>
        (la&ubreve;)<tld/>igi situacion, siajn rajtojn, pretendojn, eksteredzecan
        infanon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>l&eacute;galiser</trd>,
      <trd>l&eacute;gitimer</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;rv&eacute;nyes&iacute;t</trd>
    <trd lng="nl">wettigen</trd>
    <trd lng="ru">&c_u;&c_z;&c_a;&c_k;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Doni <tld/>ojn al:
      <ekz>
        <tld/>igi popolon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi la fundamentojn de la tero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>donner des <ind>lois</ind></trd>
    </trdgrp>
    <trd lng="nl">wetten geven aan</trd>
  </snc>
  <trd lng="el">&ny;&omikron;&my;&jota;&my;&omikron;&pi;&omikron;&jota;&omega_ton;</trd>
  <trd lng="pt">legalizar</trd>
</drv>

<drv mrk="legx.kontraux0a">
  <kap>kontra&ubreve;<tld/>a</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Ne<tld/>a, ne konforma al <tld/>o:
      <ekz>
        kontra&ubreve;<tld/>a decido, entrepreno.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ill&eacute;gal</trd>,
    <trd>illicite</trd>
  </trdgrp>
  <trd lng="es">ilegal</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;rv&eacute;nyellenes</trd>,
    <trd>illeg&aacute;lis</trd>
  </trdgrp>
  <trd lng="nl">onwettig</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="legx.laux0a">
  <kap>la&ubreve;<tld/>a</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Konforma al <tld/>oj, permesita de la <tld/>oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="legx.0a.lauxlegxa"><tld/>a <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>l&eacute;gal</trd>,
    <trd>licite</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;rv&eacute;nyes</trd>,
    <trd>t&ouml;rv&eacute;nyszer&udblac;</trd>
  </trdgrp>
  <trd lng="nl">wettig</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="legx.0oforta">
  <kap><tld/>oforta</kap>
  <snc>
    <dif>
      Same deviga kiel <tld/>o:
      <ekz>
        <tld/>oforta dekreto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ayant force de <ind>loi</ind></trd>
  </trdgrp>
  <trd lng="hu">t&ouml;rv&eacute;nyerej&udblac;</trd>
  <trd lng="nl">wetskracht hebbend</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_s;&c_i;&c_l;&c_u; &c_z;&c_a;&c_k;&c_o;&c_n;&c_a;</trd>
</drv>

<drv mrk="legx.0oscienco">
  <kap><tld/>oscienco</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Scienco pri <tld/>oj kaj iliaj aplikoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="jur.0o">juro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>droit<klr> (sci.)</klr></trd>
  </trdgrp>
  <trd lng="hu">jogtudom&aacute;ny</trd>
  <trd lng="nl">rechtsgeleerdheid</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="legx.kepleraj0oj">
  <kap>kepleraj <tld/>oj</kap>
  <uzo tip="fak">AST</uzo>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <refgrp tip="vid">
      <ref cel="kepler.0o">Keplero</ref>;
      <ref cel="elips.0o.linio">elipso</ref>,
      <ref cel="vektor.radius0o">radiusvektoro</ref>,
      <ref cel="aks.grandaduon0o">granda duonakso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Keplerschen <ind>Gesetze</ind></trd>
  <trd lng="en">Kepler['s] <ind>laws</ind></trd>
  <trd lng="fr">lois de Kepler</trd>
  <trd lng="hu">Kepler-t&ouml;rv&eacute;nyek</trd>
  <trd lng="pl">prawa Keplera</trd>
  <trd lng="pt">Leis de Kepler</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_o;&c_n;&c_y; &c_K;&c_je;&c_p;&c_l;&c_je;&c_r;&c_a;</trd>
</drv>
</art>
