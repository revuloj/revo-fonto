<vortaro>
<art mrk="$Id$">
<kap>
  <rad>la&ubreve;dan</rad>/o
</kap>
<drv mrk="lauxda.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Likva medikamento, kies bazo estas opia alkohola&jcirc;o kaj kiu estas
      uzata por kvietigi nervojn.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_k;&c_a; &c_o;&c_p;&c_ib;&c_ju;</trd>
  <trdgrp lng="ca">
     <trd>l&agrave;udanum</trd>,
     <trd>l&agrave;udan</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Opiumtinktur</trd>,
     <trd>Laudanum</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&ypsilon_ton;&delta;&alfa;&ny;&omikron;</trd>
  <trd lng="en">laudanum</trd>
  <trd lng="es">l&aacute;udano</trd>
  <trd lng="hu">laudanum</trd>
  <trd lng="nl">laudanum</trd>
  <trd lng="pt">l&aacute;udano</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_k;&c_a;
&c_o;&c_p;&c_i;&c_ja;</trd>
</drv>
</art>
