<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tabak</rad>/o
</kap>
<drv mrk="tabak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="tabak.0o.BOT">
    <dif>
      <ref tip="dif" cel="nikoti.0o">Nikotiano</ref>.
    </dif>
  </snc>
  <snc mrk="tabak.0o.fumado">
    <dif>
      Sekigitaj, diversmaniere preparitaj folioj de tiu veg., uzataj por
      fumado, ma&ccirc;ado a&ubreve; ennazigo:
      <ekz>
        fum<tld/>o;
      </ekz>
      <ekz>
        flar<tld/>o, naz<tld/>o;
      </ekz>
      <ekz>
        ma&ccirc;<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pip.0o">pipo</ref>,
      <ref cel="cigar.0o">cigaro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_t;&c_u;&c_n;&c_mol;</trd>
  <trd lng="de">Tabak</trd>
  <trd lng="fr">tabac</trd>
  <trd lng="nl">tabak</trd>
  <trd lng="sv">tobak</trd>
  <trd lng="ru">&c_t;&c_a;&c_b;&c_a;&c_k;</trd>
  <trd lng="tr">t&uuml;t&uuml;n</trd>
</drv>
</art>
