    <trd>&#x43F;&#x43E;&#x441;&#x43B;&#x430;&#x431;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6416;&#x52D5; <pr>y&#xE1;od&#xF2;ng</pr></trd>,
    <trd>&#x6447;&#x52A8; <pr>y&#xE1;od&#xF2;ng</pr></trd>,
    <trd>&#x52A8;&#x6447; <pr>d&#xF2;ngy&#xE1;o</pr></trd>,
    <trd>&#x52D5;&#x6416; <pr>d&#xF2;ngy&#xE1;o</pr></trd>,
    <trd>&#x63CC; <pr>s&#x101;i</pr></trd>,
    <trd>&#x6541; <pr>di&#x101;n</pr></trd>,
    <trd>&#x9707;&#x52A8; <pr>zh&#xE8;nd&#xF2;ng</pr></trd>,
    <trd>&#x9707;&#x52D5; <pr>zh&#xE8;nd&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxance.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxance.0igxi.MOV">
    <dif>
      Esti forte skuata kaj fari&gcirc;i facile falonta:
      <ekz>
        <klr>[li]</klr> tiel <tld/>i&gcirc;is sur siaj piedoj, ke la sinjoro permesis al li
        sidi&gcirc;i
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj piedoj <tld/>i&gcirc;is, li sentis kapturnon
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;as la tero kiel ebriulo kaj balanci&gcirc;as kiel
        hamako
        <fnt><bib>MT</bib>, <lok>&Jes; 24:20</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i ne iras rekte la&ubreve; la vojo, &scirc;iaj
        pa&scirc;oj <tld/>i&gcirc;as
        <fnt><bib>MT</bib>, <lok>&Sen; 5:6</lok></fnt>;
      </ekz>
      <ekz>
        ne <tld/>i&gcirc;u dekstren nek maldekstren
        <fnt><bib>MT</bib>, <lok>&Sen; 4:27</lok></fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo glitis facile kaj sen forta <tld/>i&gcirc;ado sur 
        la glata maro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wanken</trd>,
      <trd>schwanken</trd>,
      <trd>wackeln</trd>,
      <trd>ersch&uuml;ttert werden</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>barcolare</trd>,
      <trd>vacillare</trd>,
      <trd>dondolare</trd>, 
      <trd>traballare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zachwia&cacute; si&eogonek;</trd>,
      <trd>chwia&cacute; si&eogonek;</trd>,
      <trd>chybota&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0igxi.state">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti malfortigita, malfirmigita, endan&gcirc;erigita:
      <ekz>
        lia sano ek<tld/>i&gcirc;is
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la enspezoj de la &scirc;tato <klr>[&#x2026;]</klr> ek<tld/>i&gcirc;os, se la popola&ccirc;o komencos
        festi &ccirc;iun sepan tagon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wanken</trd>,
      <trd>wackeln</trd>,
      <trd>ersch&uuml;ttert werden</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>vacillare</trd>,
      <trd>traballare</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0igxi.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="hezit.0i">Heziti</ref>: 
      <ekz>
        vi nun komencas dubeme <tld/>i&gcirc;i
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        Marta videble <tld/>i&gcirc;is momenton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="dub.0i">dubi</ref>
    <trd lng="bg">&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_ja; &c_s;&c_je;</trd>
    <trdgrp lng="it">
      <trd>tentennare</trd>,
      <trd>vacillare</trd>, 
      <trd>oscillare<klr> (fig.)</klr></trd>,
      <trd>titubare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wahn&aogonek;&cacute; si&eogonek;</trd>,
      <trd>waha&cacute; si&eogonek;</trd>,
      <trd>zawaha&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_k;&c_l;&c_a;&c_shch;&c_a;&c_m; &c_s;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_t;&c_malmol;&c_r;&c_s;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="de">schwanken</trd>
  <trdgrp lng="fr">
    <trd>chanceler</trd>,
    <trd>h&eacute;siter</trd>,
    <trd>tituber</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>inog</trd>,
    <trd>ingadozik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x63FA;&#x308C;&#x52D5;&#x304F;  <pr>&#x3086;&#x308C;&#x3046;&#x3054;&#x304F;</pr></trd>,
    <trd>&#x3050;&#x3089;&#x3050;&#x3089;&#x3059;&#x308B;</trd>,
    <trd>&#x52D5;&#x63FA;&#x3059;&#x308B;  <pr>&#x3069;&#x3046;&#x3088;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3061;&#x3085;&#x3046;&#x3061;&#x3087;&#x3059;&#x308B;</trd>,
    <trd>&#x3050;&#x3089;&#x3064;&#x304F;</trd>,
    <trd>&#x5371;&#x6A5F;&#x306B;&#x3072;&#x3093;&#x3059;&#x308B;  <pr>&#x304D;&#x304D;&#x306B;&#x3072;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">wankelen</trd>
  <trdgrp lng="es">
     <trd>sacudirse</trd>,
     <trd>batirse</trd>,
     <trd>estremecerse</trd>,
     <trd>oscilar (r&aacute;pidamente)</trd>,
     <trd>vacilar</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sacudir-se</trd>,
    <trd>balan&ccedil;ar-se</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43B;&#x438;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x442;&#x440;&#x44F;&#x441;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x445;&#x438;&#x442;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43E;&#x441;&#x43B;&#x430;&#x431;&#x43D;&#x443;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x430;&#x433;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8E09;&#x8DC4; <pr>li&#xE0;ngqi&#xE0;ng</pr></trd>,
    <trd>&#x8E09;&#x8E4C; <pr>li&#xE0;ngqi&#xE0;ng</pr></trd>,
    <trd>&#x8D91; <pr>z&#x12B;</pr></trd>,
    <trd>&#x8E0C; <pr>ch&#xF3;u</pr></trd>,
    <trd>&#x8E8A; <pr>ch&#xF3;u</pr></trd>,
    <trd>&#x72B9;&#x7591; <pr>y&#xF3;uy&#xED;</pr></trd>,
    <trd>&#x7336;&#x7591; <pr>y&#xF3;uy&#xED;</pr></trd>,
    <trd>&#x8E09;&#x8E09;&#x8DC4;&#x8DC4; <pr>li&#xE0;ngli&#xE0;ngqi&#xE0;ngqi&#xE0;ng</pr></trd>,
    <trd>&#x8E09;&#x8E09;&#x8E4C;&#x8E4C; <pr>li&#xE0;ngli&#xE0;ngqi&#xE0;ngqi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxance.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc>
    <dif>
      Tiom <ref tip="super" cel="firm.0a.fiksita">firma</ref>,
      konstanta, ke &gcirc;in oni ne povas <tld/>i:
      <ekz>
        &scirc;ia volo estis tiel same ne<tld/>ebla, kiel &scirc;ia fido
        je la Eternulo
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Zminska</nom> &ccirc;i tiun fojon estis ne<tld/>ebla, malvarma kaj oficiala
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel ajn timema &scirc;ia agado estis, &gcirc;i tamen montris firman kaj ne<tld/>eblan
        decidon, al kiu <nom>Clemency</nom> ne povis kontra&ubreve;stari
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_a;&c_h;&c_ib;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_z;&c_l;&c_o;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_o;&c_k;&c_l;&c_a;&c_t;&c_i;&c_m;</trd>
  <trd lng="de">unersch&uuml;tterlich</trd>
  <trd lng="fr">in&eacute;branlable</trd>
  <trd lng="hu">megingathatatlan</trd>
  <trdgrp lng="it">
    <trd>fermo<klr> (irremovibile)</klr></trd>, 
    <trd>incrollabile<klr> (fermo, stabile)</klr></trd>, 
    <trd>saldo<klr> (fermo, stabile)</klr></trd>, 
    <trd>stabile<klr> (inmovibile)</klr></trd>,
    <trd>solido<klr> (fermo, stabile)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
