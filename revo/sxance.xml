<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ancel</rad>/i
</kap>
<drv mrk="sxance.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxance.0i.MOV">
    <dif>
      Forte pu&scirc;i starantan objekton tien kaj reen tiamaniere, ke 
&gcirc;i
      povas
      renversi&gcirc;i a&ubreve; fali, a&ubreve; ke &gcirc;i
      almena&ubreve; ne 
plu firme staras:
      <ekz>
        la rivero sin &jcirc;etis kontra&ubreve; tiun domon, kaj ne
        povis 
&gcirc;in
        <tld/>i;
        &ccirc;ar &gcirc;i estis bone konstruita
        <fnt><bib>NT</bib>, <lok>&Luk; 6:48</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stabil.mal0igi">malstabiligi</ref>,
      <ref cel="sku.0i">skui</ref>,
      <ref cel="trem">tremigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zum Wanken bringen</trd>,
      <trd>zum Schwanken bringen</trd>,
      <trd>ersch&uuml;ttern</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>far <ind>vacillare</ind></trd>,
      <trd>scuotere</trd>,
      <trd>crollare<klr> (far cadere, no distruggere)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wstrz&aogonek;sn&aogonek;&cacute;</trd>,
      <trd>wstrz&aogonek;sa&cacute;</trd>,
      <trd>chybota&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0i.state">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfortikigi, malfirmigi, endan&gcirc;erigi ion:
      <ekz>
        la kresko de nia samideanaro iom post iom <tld/>os tiun baron;
      </ekz>
      <ekz>
        la plena de sa&gcirc;o pastra partio ek<tld/>is la
        a&ubreve;toritaton 
        de l&#8217; faraono
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
      </ekz>
      <ekz>
        la lasta <klr>[mondmilito]</klr> <tld/>is la fundamentojn de la
        kapitalismo en E&ubreve;ropo
        <fnt><bib>Lanti</bib>, <lok>de la deziro al la efektivigo
        ...</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zum Wanken bringen</trd>,
      <trd>ersch&uuml;ttern</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>far <ind>vacillare</ind></trd>,
      <trd>minare<klr> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wstrz&aogonek;sn&aogonek;&cacute;</trd>,
      <trd>wstrz&aogonek;sa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0i.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Hezitigi iun:
      <ekz>
        tiu akcidento <tld/>is la kura&gcirc;ojn.
      </ekz>
    </dif>
    <trd lng="de">ersch&uuml;ttern</trd>
    <trdgrp lng="it">
      <trd>far <ind>vacillare</ind></trd>,
      <trd>indebolire<klr> (fig.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_k;&c_l;&c_a;&c_shch;&c_a;&c_m;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_t;&c_malmol;&c_r;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="en">shake</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;stabiliser</trd>,
    <trd>&eacute;branler</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>sacudir</trd>,
     <trd>batir</trd>,
     <trd>estremecer</trd>,
     <trd>hacer oscilar (fuertemente)</trd>,
     <trd>hacer vacilar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ingat</trd>,
    <trd>megingat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schudden</trd>,
    <trd>doen <ind>wankelen</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zachwia&cacute;</trd>,
    <trd>chwia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sacudir</trd>,
    <trd>balan&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxance.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxance.0igxi.MOV">
    <dif>
      Esti forte skuata kaj fari&gcirc;i facile falonta:
      <ekz>
        li <tld/>i&gcirc;is kaj falis;
      </ekz>
      <ekz>
        liaj piedoj <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <tld/>i&gcirc;as la tero kiel ebriulo kaj balanci&gcirc;as kiel
        hamako
        <fnt><bib>MT</bib>, <lok>&Jes; 24:20</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i ne iras rekte la&ubreve; la vojo, &scirc;iaj
        pa&scirc;oj 
<tld/>i&gcirc;as
        <fnt><bib>MT</bib>, <lok>&Sen; 5:6</lok></fnt>;
      </ekz>
      <ekz>
        ne <tld/>i&gcirc;u dekstren nek maldekstren
        <fnt><bib>MT</bib>, <lok>&Sen; 4:27</lok></fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo glitis facile kaj sen forta <tld/>i&gcirc;ado sur 
        la glata maro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wanken</trd>,
      <trd>schwanken</trd>,
      <trd>wackeln</trd>,
      <trd>ersch&uuml;ttert werden</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>barcolare</trd>,
      <trd>vacillare</trd>,
      <trd>dondolare</trd>, 
      <trd>traballare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zachwia&cacute; si&eogonek;</trd>,
      <trd>chwia&cacute; si&eogonek;</trd>,
      <trd>chybota&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0igxi.state">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti malfortigita, malfirmigita, endan&gcirc;erigita:
      <ekz>
        lia sano ek<tld/>i&gcirc;is
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la enspezoj de la &scirc;tato ek<tld/>i&gcirc;as<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wanken</trd>,
      <trd>wackeln</trd>,
      <trd>ersch&uuml;ttert werden</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>vacillare</trd>,
      <trd>traballare</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0igxi.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Heziti:
      <ekz>
        vi nun komencas dubeme <tld/>i&gcirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Marta videble <tld/>i&gcirc;is momenton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_ja; &c_s;&c_je;</trd>
    <trdgrp lng="it">
      <trd>tentennare</trd>,
      <trd>vacillare</trd>, 
      <trd>oscillare<klr> (fig.)</klr></trd>,
      <trd>titubare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wahn&aogonek;&cacute; si&eogonek;</trd>,
      <trd>waha&cacute; si&eogonek;</trd>,
      <trd>zawaha&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_k;&c_l;&c_a;&c_shch;&c_a;&c_m; &c_s;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_t;&c_malmol;&c_r;&c_s;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="de">schwanken</trd>
  <trdgrp lng="fr">
    <trd>chanceler</trd>,
    <trd>h&eacute;siter</trd>,
    <trd>tituber</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>inog</trd>,
    <trd>ingadozik</trd>
  </trdgrp>
  <trd lng="nl">wankelen</trd>
  <trdgrp lng="es">
     <trd>sacudirse</trd>,
     <trd>batirse</trd>,
     <trd>estremecerse</trd>,
     <trd>oscilar (r&aacute;pidamente)</trd>,
     <trd>vacilar</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sacudir-se</trd>,
    <trd>balan&ccedil;ar-se</trd>
  </trdgrp>
  <trd
  lng="ru">&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="sxance.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc>
    <dif>
      Tiom <ref tip="super" cel="firm.0a.fiksita">firma</ref>,
      konstanta, ke &gcirc;in oni ne povas <tld/>i:
      <ekz>
        &scirc;ia volo estis tiel same ne<tld/>ebla, kiel &scirc;ia fido
        je la
        Eternulo
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_a;&c_h;&c_ib;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_z;&c_l;&c_o;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_o;&c_k;&c_l;&c_a;&c_t;&c_i;&c_m;</trd>
  <trd lng="de">unersch&uuml;tterlich</trd>
  <trd lng="fr">in&eacute;branlable</trd>
  <trd lng="hu">megingathatatlan</trd>
  <trdgrp lng="it">
    <trd>fermo<klr> (irremovibile)</klr></trd>, 
    <trd>incrollabile<klr> (fermo, stabile)</klr></trd>, 
    <trd>saldo<klr> (fermo, stabile)</klr></trd>, 
    <trd>stabile<klr> (inmovibile)</klr></trd>,
    <trd>solido<klr> (fermo, stabile)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onwankelbaar</trd>,
    <trd>onwrikbaar</trd>,
    <trd>onverstoorbaar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niez&lstroke;omny</trd>,
    <trd>niewzruszony</trd>,
    <trd>nieugi&eogonek;ty</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>imbatible</trd>,
    <trd>inamovible</trd>
  </trdgrp>
  <trd lng="pt">inabal&aacute;vel</trd>
</drv>

<drv mrk="sxance.sen0igxa">
  <kap>sen<tld/>i&gcirc;a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="firm.0a.morale">Firma</ref> kaj senhezita en
      sia ago:
      <ekz>
        sen<tld/>i&gcirc;a estas lia koro
        <fnt><bib>MT</bib>, <lok>&Psa; 112:8</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rezolut.0a">rezoluta</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_o;&c_k;&c_o;&c_l;&c_je;&c_b;&c_i;&c_m;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;termin&eacute;<klr tip="amb"> (tenace)</klr></trd>,
    <trd>tenace</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szil&aacute;rd</trd>,
    <trd>hat&aacute;rozott</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fermo<klr> (irremovibile)</klr></trd>, 
    <trd>incrollabile<klr> (irremovibile)</klr></trd>, 
    <trd>saldo<klr> (solido, fermo)</klr></trd>, 
    <trd>solido<klr> (risoluto)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vastberaden</trd>,
    <trd>beslist<klr tip="ind"> (vastberaden)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niewzruszony</trd>,
    <trd>nieugi&eogonek;ty</trd>
  </trdgrp>
  <trd lng="pt">inabal&aacute;vel</trd>
  <trdgrp lng="es">
    <trd>determinado (con intenci&oacute;n de)</trd>,
    <trd>resuelto</trd>
  </trdgrp>
</drv>
</art>
