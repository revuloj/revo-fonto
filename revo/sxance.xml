  <rad>&scirc;ancel</rad>/i
</kap>
<drv mrk="sxance.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxance.0i.MOV">
    <dif>
      Forte <ref tip="super" cel="pusx.0i.delokigi">pu&scirc;i</ref>
      starantan objekton tien kaj reen tiamaniere, ke 
      &gcirc;i povas <ref tip="super" cel="renver.0i.faligi">renvers</ref>i&gcirc;i
      a&ubreve; fali, a&ubreve; ke &gcirc;i almena&ubreve; ne plu firme staras:
      <ekz>
        la rivero sin &jcirc;etis kontra&ubreve; tiun domon, kaj ne
        povis &gcirc;in <tld/>i;
        &ccirc;ar &gcirc;i estis bone konstruita
        <fnt><bib>NT</bib>, <lok>&Luk; 6:48</lok></fnt>;
      </ekz>
      <ekz>
        la vento ek<tld/>is la kristalan akvon kaj enkovris la belegan lilion per blanka
        &scirc;a&ubreve;mo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        inter du pensoj <tld/>is la koro en brusto
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balanc1.0i">balanci</ref>,
      <ref cel="stabil.mal0igi">malstabiligi</ref>,
      <ref cel="sku.0i">skui</ref>,
      <ref cel="sving.0i.skui">svingi<sncref/></ref>,
      <ref cel="trem">tremigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zum Wanken bringen</trd>,
      <trd>zum Schwanken bringen</trd>,
    </trdgrp>
    <trdgrp lng="it">
      <trd>far <ind>vacillare</ind></trd>,
      <trd>scuotere</trd>,
      <trd>crollare<klr> (far cadere, no distruggere)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wstrz&aogonek;sn&aogonek;&cacute;</trd>,
      <trd>wstrz&aogonek;sa&cacute;</trd>,
      <trd>chybota&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0i.state">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfortikigi, malfirmigi, endan&gcirc;erigi ion:
      <ekz>
        neniu estu <tld/>ita de tiuj afliktoj
        <fnt>
          <bib>NT</bib>
          <lok>I. Tesalonikanoj 3:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite &scirc;i <klr>[&#x2026;]</klr> <tld/>is la kapon, faris geston kvaza&ubreve; &scirc;i volas salti
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plena de sa&gcirc;o pastra partio ek<tld/>is la
        a&ubreve;toritaton de l&#x2019; faraono
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
      </ekz>
      <ekz>
        Al dol&ccirc;a lud' de l' amo / Mi timas kor<tld/>ojn
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Tentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la moralaj eksciti&gcirc;oj <klr>[&#x2026;]</klr> ek<tld/>is &scirc;ian fizikan forton
        <fnt>
          <bib>Marta</bib>
        </fnt>?
      </ekz>
      <ekz>
        la lasta <klr>[mondmilito]</klr> <tld/>is la fundamentojn de la
        kapitalismo en E&ubreve;ropo
        <fnt><bib>Lanti</bib>, <lok>de la deziro al la efektivigo
        ...</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zum Wanken bringen</trd>,
    </trdgrp>
    <trdgrp lng="it">
      <trd>far <ind>vacillare</ind></trd>,
      <trd>minare<klr> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wstrz&aogonek;sn&aogonek;&cacute;</trd>,
      <trd>wstrz&aogonek;sa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxance.0i.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="hezit.0i">Heziti</ref>gi, <ref tip="dif" cel="dub.0igi">dubigi</ref> iun:
      <ekz>
        tiu akcidento <tld/>is la kura&gcirc;ojn;
      </ekz>
      <ekz>
        ili ne vortobatalu <klr>[&#x2026;]</klr> kun risko de <tld/>o por la a&ubreve;dantoj
        <fnt>
          <bib>NT</bib>
          <lok>Ii. timoteo 2:14</lok>
        </fnt>.
      </ekz>
    </dif>    
    <trdgrp lng="it">
      <trd>far <ind>vacillare</ind></trd>,
      <trd>indebolire<klr> (fig.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_k;&c_l;&c_a;&c_shch;&c_a;&c_m;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_t;&c_malmol;&c_r;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ersch&uuml;ttern</trd>,
    <trd><ofc>*</ofc>bewegen</trd>,
    <trd><ofc>*</ofc><ind>wankend</ind> machen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shake</trd>,
    <trd><ofc>*</ofc>totter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;stabiliser</trd>,
    <trd>&eacute;branler</trd>,
    <trd><ofc>*</ofc>chanceler</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>sacudir</trd>,
     <trd>batir</trd>,
     <trd>estremecer</trd>,
     <trd>hacer oscilar (fuertemente)</trd>,
     <trd>hacer vacilar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ingat</trd>,
    <trd>megingat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x63FA;&#x3055;&#x3076;&#x308B;  <pr>&#x3086;&#x3055;&#x3076;&#x308B;</pr></trd>,
    <trd>&#x3050;&#x3089;&#x3064;&#x304B;&#x305B;&#x308B;</trd>,
    <trd>&#x52D5;&#x63FA;&#x3055;&#x305B;&#x308B;  <pr>&#x3069;&#x3046;&#x3088;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x3061;&#x3085;&#x3046;&#x3061;&#x3087;&#x3055;&#x305B;&#x308B;</trd>,
    <trd>&#x63FA;&#x308B;&#x304C;&#x3059;  <pr>&#x3086;&#x308B;&#x304C;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schudden</trd>,
    <trd>doen <ind>wankelen</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zachwia&cacute;</trd>,
    <trd><ofc>*</ofc>chwia&cacute;</trd>,
    <trd><ofc>*</ofc>powiewa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sacudir</trd>,
    <trd>balan&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd><ofc>*</ofc>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43B;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x440;&#x44F;&#x441;&#x442;&#x438;</trd>,
