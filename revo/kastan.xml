<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kastanjet</rad>/o
</kap>
<drv mrk="kastan.0o">
  <kap>
    <ofc>4</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ligna a&ubreve; ebura
      <ref tip="super" cel="instru2.frap0o">frapinstrumento</ref>
      el du &scirc;eloj, kiujn oni alligas al fingroj kaj sonigas per
      rapida interfrapado.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_je;&c_t;&c_a;</trd>
  <trd lng="de">Kastagnetten</trd>
  <trd lng="en">castanet</trd>
  <trd lng="fr">castagnettes</trd>
  <trd lng="nl">castagnet</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_je;&c_t;&c_a;</trd>
</drv>
</art>
