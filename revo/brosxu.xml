<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brosxu.xml,v 1.26 2023/10/29 19:11:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bro&scirc;ur</rad>/o <fnt><bib>LR</bib></fnt>
</kap>
<drv mrk="brosxu.0o">
  <kap><tld/>o</kap>
  <snc mrk="brosxu.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="pres.0ajxo">Presa&jcirc;o</ref> el kelkaj kunigitaj folioj
      sen binda&jcirc;o, eventuale kun kartona ekstero:
      <ekz>
        vi povas ricevi de ni la <tld/>on pri homaranismo por la prezo de unu rublo
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>;
      </ekz>
      <ekz>
        miaj Respondoj <klr>[&#x2026;]</klr> estos reeldonitaj kune en formo de aparta <tld/>o, kun aldono
        de la Alfabeta Registro
        <fnt>
          <bib>LR</bib>
          <lok>2. La verkaro de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        interesaj katalogoj, <tld/>oj kaj dokumentoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia unua <tld/>o prezentas fundamenton sufi&ccirc;e fortan
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la neta teksto de la <tld/>o (sen notoj kaj listo de citita literaturo) okupas nur 14 pa&gcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2003/007838.html">Helpilo por kontra&ubreve;stari al
          imperiismo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kajer.0o.de_libro">kajero<sncref/></ref>
    <ref tip="vid" cel="oktav.0o.TIP">oktavo<sncref/></ref>
    <trdgrp lng="zh">
    <trd>&#x8584;&#x672C;&#x4E66; <pr>b&#xF3;b&#x11B;nsh&#x16B;</pr></trd>,
    <trd>&#x5E73;&#x88C5;&#x4E66; <pr>p&#xED;ngzhu&#x101;ngsh&#x16B;</pr></trd>,
    <trd>&#x5C0F;&#x518A; <pr>xi&#x1CE;oc&#xE8;</pr></trd>,
    <trd>&#x5C0F;&#x518A;&#x5B50; <pr>xi&#x1CE;oc&#xE8;zi</pr></trd>,
    <trd>&#x5C0F;&#x4E66; <pr>xi&#x1CE;osh&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_sh;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Brosch&uuml;re</trd>,
    <trd><klr>(gedrucktes)</klr> Heft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>brochure</trd>,
    <trd>leaflet</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>encuadernaci&oacute;n r&uacute;stica</trd>,
    <trd>encuadernaci&oacute;n sin tapa dura</trd>
  </trdgrp>
  <trd lng="fr">brochure</trd>
  <trd lng="he">&het;&vav;&#x5B9;&bet;&#x5B6;&resh;&#x5B6;&tav;</trd>
  <trd lng="hu">bros&uacute;ra</trd>
  <trdgrp lng="ja">
    <trd>&#x4EEE;&#x7DB4;&#x672C;  <pr>&#x304B;&#x308A;&#x3068;&#x3058;&#x307B;&#x3093;</pr></trd>,
    <trd>&#x4E26;&#x88FD;&#x672C;  <pr>&#x306A;&#x307F;&#x305B;&#x3044;&#x307B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">brochure</trd>
  <trdgrp lng="pl">
    <trd>broszura</trd>
    <trd>ksi&aogonek;&zdot;ka w oprawie mi&eogonek;kkiej</trd>
    <trd>ksi&aogonek;&zdot;ka w oprawie klejonej</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_sh;&c_ju;&c_r;&c_a;</trd>
  <trd lng="uk">&c_b;&c_r;&#x43E;&c_sh;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="brosxu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brosxu.0i.TIP">
   <uzo tip="fak">TIP</uzo>
    <dif>
      Kunmeti foliojn de presa&jcirc;o kiel <tld/>o:
      <ekz>
        52 pa&gcirc;oj <tld/>itaj
        <fnt>
	<aut>Jorge Camacho</aut>
	<vrk><url ref="&Monato;/2000/006810.php">Diskutige kaj diskutinde</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas, ke &gcirc;i estas nur glu<tld/>ita
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010411.php">La gramatikiloj de Esperanto la&ubreve; ABC</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        foliumante tra la 87-pa&gcirc;a <tld/>ita verketo en formato A5, oni hazarde trovas rubrikon
        lingvan pri Esperanto
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2012/010410.php">&Hcirc;aose de A &gcirc;is Z</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="bind.0i.TIP">bindi</ref>
    <trd lng="be">&c_b;&c_r;&c_a;&c_sh;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>heften</trd>,
      <trd>broschieren</trd>,
      <trd>klammern <klr>(Brosch&uuml;re)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>encuadernar de forma r&uacute;stica</trd>,
      <trd>encuadernar sin tapa dura</trd>
    </trdgrp>
    <trd lng="fr">brocher <klr tip="ind">(des feuillets)</klr></trd>
    <trd lng="hu">f&udblac;z <klr tip="ind">(k&ouml;nyvet)</klr></trd>
    <trd lng="nl">opnaaien<klr tip="ind"> (in katernen)</klr></trd>
    <trdgrp lng="pl">
      <trd>zszywa&cacute;<klr> broszur&eogonek;</klr></trd>,
      <trd>bindowa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_sh;&c_ju;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="ja">&#x4EEE;&#x7DB4;&#x3059;&#x308B;  <pr>&#x304B;&#x308A;&#x3064;&#x3065;&#x308A;&#x3059;&#x308B;</pr></trd>
  <trd lng="uk">&c_b;&c_r;&#x43E;&c_sh;&c_u;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x7DDA;&#x8A02; <pr>xi&#xE0;nd&#xEC;ng</pr></trd>,
    <trd>&#x7E54;&#x597D; <pr>zh&#x12B;h&#x1CE;o</pr></trd>,
    <trd>&#x7D81;&#x597D; <pr>b&#x1CE;ngh&#x1CE;o</pr></trd>,
    <trd>&#x7E54; <pr>zh&#x12B;</pr></trd>,
    <trd>&#x7CFB;&#x4E0A; <pr>x&#xEC;sh&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
