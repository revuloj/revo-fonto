<art mrk="$Id: lorant1.xml,v 1.10 2022/03/22 18:26:51 revo Exp $">
<kap>
  <rad>lorantac</rad>/oj
</kap>
<drv mrk="lorant1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Loranthaceae</trd>), verdaj duonparazitoj,
      plej ofte kreskantaj en la bran&ccirc;aro de arboj:
      <ekz>
        de la Kanalbeka kukolo <klr>[&#x2026;]</klr> man&gcirc;itaj fruktoj inkludas 
        tiujn de diversaj specioj de figoj, la semojn de la e&ubreve;kaliptoj,
        la fruktojn 
        de arboj de la familio <tld lit="L"/>oj kaj la fruktoj de la
        enmetita moruso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=235635">Kanalbeka
          kukolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mistelgew&auml;chse</trd>
  <trd lng="en">mistletoe</trd>
  <trd lng="fr">Loranthac&eacute;es</trd>
  <trd lng="hu">fagy&ouml;ngyf&eacute;l&eacute;k</trd>
  <trd lng="nl">vogellijmfamilie</trd>
  <trd lng="pl">g&aogonek;zewnikowate</trd>
  <trd lng="pt">lorant&aacute;ceas</trd>
  <trd lng="ro">loranthaceae</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_m;&c_n;&c_je;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_r;&c_je;&c_m;&c_n;&c_je;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_l;&c_o;&c_r;&c_a;&c_n;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x43C;&#x435;&#x43B;&#x43E;&#x432;&#x456;</trd>
</drv>
