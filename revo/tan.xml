<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tan.xml,v 1.26 2023/10/21 13:02:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tan</rad>/i
</kap>
<drv mrk="tan.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transformi per ta&ubreve;gaj procedoj bestan felon en <ref cel="led.0o">ledo</ref>n:
      <ekz>
        <klr>[&ccirc;e]</klr> barilo <klr>[&#x2026;]</klr> pendis feloj kaj ha&ubreve;toj, krudaj kaj
        <tld/>itaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Kora sufero</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna homo, kiu havas viza&gcirc;on de l' koloro de <tld/>ita ha&ubreve;to
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia profesio tiel <tld/>as al li la felon, ke &gcirc;i longan tempon kontra&ubreve;staras al
        akvo
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i al iu la ha&ubreve;ton</ind>
        <klr>(severe ripro&ccirc;i iun)</klr>
        <trd lng="de">jemandem das Fell <ind>gerben</ind></trd>
        <trd lng="fr">passer un <ind>savon</ind>
        &agrave; quelqu'un</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kap.0o">kapo</ref>,
      <ref cel="lav.0i">lavi</ref>,
      <ref cel="predik.0o">prediko</ref>,
      <ref cel="sap.0umi">sapumi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_b;&c_ib;&c_c;&c_mol;
    <klr tip="amb">(&c_s;&c_k;&c_u;&c_r;&c_u;)</klr></trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;
    <klr tip="amb">(&c_s;&c_k;&c_u;&c_r;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adobar</trd>,
    <trd>assaonar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tvrdit</trd>,
    <trd>utvrzovat</trd>,
    <trd>vyd&ecaron;lat k&uring;&zcaron;i</trd>,
    <trd>vyd&ecaron;l&aacute;vat k&uring;&zcaron;i</trd>,
    <trd>vy&ccaron;init k&uring;&zcaron;i</trd>,
    <trd>vy&ccaron;i&ncaron;ovat k&uring;&zcaron;e</trd>,
    <trd>&ccaron;init k&uring;&zcaron;i</trd>
  </trdgrp>
  <trd lng="de">gerben</trd>
  <trd lng="en">tan</trd>
  <trd lng="es">curtir</trd>
  <trd lng="fr">tanner</trd>
  <trd lng="hu">cserez</trd>
  <trdgrp lng="ja">
    <trd>&#x306A;&#x3081;&#x3059;</trd>,
    <trd>&#x6253;&#x3061;&#x3059;&#x3048;&#x308B;  <pr>&#x3046;&#x3061;&#x3059;&#x3048;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">looien</trd>
  <trd lng="pl">garbowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>curtir (pele)</trd>,
   <trd>surrar</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vypracova&tcaron; ko&zcaron;u &ccaron;inidlom</trd>
  <trd lng="sv">garva</trd>
  <trd lng="uk">&c_d;&c_u;&c_b;&c_i;&c_t;&c_i; (&c_sh;&c_k;&c_ib;&c_r;&c_u;)</trd>
</drv>

<drv mrk="tan.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Substanco uzata por <tld/>i felojn.
    </dif>
  </snc>
  <trd lng="be">
  &c_d;&c_u;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_je;
  &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;
  </trd>
  <trdgrp lng="ca">
    <trd>adob</trd>,
    <trd>tan&iacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>t&rcaron;&iacute;slivo</trd>,
    <trd>t&rcaron;&iacute;slovina</trd>,
    <trd>&ccaron;in&iacute;c&iacute; l&aacute;tka</trd>
  </trdgrp>
  <trd lng="de">Gerbstoff</trd>
  <trd lng="en">tannin</trd>
  <trd lng="es">tanino</trd>
  <trd lng="fr">tanin</trd>
  <trd lng="hu">cserz&odblac;anyag</trd>
  <trdgrp lng="ja">
    <trd>&#x76AE;&#x306A;&#x3081;&#x3057;&#x5264;  <pr>&#x304B;&#x308F;&#x306A;&#x3081;&#x3057;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x30BF;&#x30F3;&#x76AE;  <pr>&#x30BF;&#x30F3;&#x304B;&#x308F;</pr></trd>
  </trdgrp>
  <trd lng="nl">run</trd>
  <trd lng="pl">garbnik</trd>
  <trd lng="ru">
  &c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_je;
  &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;
  </trd>
  <trd lng="sk">garbiarske &ccaron;inidlo</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_a; &c_r;&c_je;&c_ch;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_a; &c_k;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tan.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Homo, kies okupo estas <tld/>ado.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>blanquer</trd>,
    <trd>adobador</trd>
  </trdgrp>
  <trd lng="cs">ko&zcaron;eluh</trd>
  <trd lng="de">Gerber</trd>
  <trd lng="en">tanner</trd>
  <trd lng="ja">&#x76AE;&#x306A;&#x3081;&#x3057;&#x8077;&#x4EBA;  <pr>&#x304B;&#x308F;&#x306A;&#x3081;&#x3057;&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>
  <trd lng="pl">garbarz</trd>
  <trd lng="sk">garbiar</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_sh;&c_k;&c_ib;&c_r;&c_ja;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_u;&c_m;&#x2019;&c_ja;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tan.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Loko, kie oni <tld/>as:
      <ekz>
        <klr>[&scirc;i]</klr> venis, kiel &scirc;i diris, por proponi akciojn de sia <tld/>ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kora sufero</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vizitis grandajn <tld/>ejojn kaj stablojn de sandaloj, fandejojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="ca">adoberia</trd>
  <trd lng="cs">ko&zcaron;elu&zcaron;na</trd>
  <trd lng="de">Gerberei</trd>
  <trd lng="en">tannery</trd>
  <trd lng="pl">garbarnia</trd>
  <trd lng="sk">garbiare&ncaron;</trd>
  <trd lng="uk">&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_ja;</trd>
</drv>
</art>
