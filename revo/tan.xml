<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tan.xml,v 1.22 2022/03/28 12:03:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tan</rad>/i
</kap>
<drv mrk="tan.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transformi per ta&ubreve;gaj procedoj bestan felon en ledon:
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i al iu la ha&ubreve;ton</ind>
        <klr>(severe ripro&ccirc;i iun)</klr>
        <trd lng="de"><klr>jemanden </klr>anschw&auml;rzen</trd>
        <trd lng="fr">passer un <ind>savon</ind>
        &agrave; quelqu'un</trd>
        <fnt><bib>PrV</bib></fnt>..
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kap.0o">kapo</ref>,
      <ref cel="lav.0i">lavi</ref>,
      <ref cel="predik.0o">prediko</ref>,
      <ref cel="sap.0umi">sapumi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_b;&c_ib;&c_c;&c_mol;
    <klr tip="amb">(&c_s;&c_k;&c_u;&c_r;&c_u;)</klr></trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;
    <klr tip="amb">(&c_s;&c_k;&c_u;&c_r;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adobar</trd>,
    <trd>assaonar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tvrdit</trd>,
    <trd>utvrzovat</trd>,
    <trd>vyd&#x11B;lat k&#x16F;&#x17E;i</trd>,
    <trd>vyd&#x11B;l&#xE1;vat k&#x16F;&#x17E;i</trd>,
    <trd>vy&#x10D;init k&#x16F;&#x17E;i</trd>,
    <trd>vy&#x10D;i&#x148;ovat k&#x16F;&#x17E;e</trd>,
    <trd>&#x10D;init k&#x16F;&#x17E;i</trd>
  </trdgrp>
  <trd lng="de">gerben</trd>
  <trd lng="en">tan</trd>
  <trd lng="es">curtir</trd>
  <trd lng="fr">tanner</trd>
  <trd lng="hu">cserez</trd>
  <trd lng="nl">looien</trd>
  <trd lng="pl">garbowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>curtir (pele)</trd>,
   <trd>surrar</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vypracova&#x165; ko&#x17E;u &#x10D;inidlom</trd>
  <trd lng="sv">garva</trd>
  <trd lng="uk">&#x434;&#x443;&#x431;&#x438;&#x442;&#x438; (&#x448;&#x43A;&#x456;&#x440;&#x443;)</trd>
</drv>

<drv mrk="tan.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Flua&jcirc;o enhavanta &scirc;elon de kverko a&ubreve; de alia,
      tiucele ta&ubreve;ga arbo, en kiu oni banas tanotajn felojn.
    </dif>
  </snc>
  <trd lng="be">
  &c_d;&c_u;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_je;
  &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;
  </trd>
  <trdgrp lng="ca">
    <trd>adob</trd>,
    <trd>tan&iacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>t&#x159;&#xED;slivo</trd>,
    <trd>t&#x159;&#xED;slovina</trd>,
    <trd>&#x10D;in&#xED;c&#xED; l&#xE1;tka</trd>
  </trdgrp>
  <trd lng="de">Gerbstoff</trd>
  <trd lng="en">tannin</trd>
  <trd lng="es">tanino</trd>
  <trd lng="fr">tanin</trd>
  <trd lng="hu">cserz&odblac;anyag</trd>
  <trd lng="nl">run</trd>
  <trd lng="pl">garbnik</trd>
  <trd lng="ru">
  &c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_je;
  &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;
  </trd>
  <trd lng="sk">garbiarske &#x10D;inidlo</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x443;&#x431;&#x438;&#x43B;&#x44C;&#x43D;&#x430; &#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x434;&#x443;&#x431;&#x438;&#x43B;&#x44C;&#x43D;&#x430; &#x43A;&#x43E;&#x440;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="tan.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Homo, kiu profesie <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>blanquer</trd>,
    <trd>adobador</trd>
  </trdgrp>
  <trd lng="cs">ko&#x17E;eluh</trd>
  <trd lng="en">tanner</trd>
  <trd lng="pl">garbarz</trd>
  <trd lng="sk">garbiar</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x443;&#x431;&#x438;&#x43B;&#x44C;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x448;&#x43A;&#x456;&#x440;&#x44F;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43A;&#x43E;&#x436;&#x443;&#x43C;&#x2019;&#x44F;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="tan.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Loko, kie oni <tld/>as.
    </dif>
  </snc>
  <trd lng="ca">adoberia</trd>
  <trd lng="cs">ko&#x17E;elu&#x17E;na</trd>
  <trd lng="en">tannery</trd>
  <trd lng="pl">garbarnia</trd>
  <trd lng="sk">garbiare&#x148;</trd>
  <trd lng="uk">&#x434;&#x443;&#x431;&#x438;&#x43B;&#x44C;&#x43D;&#x44F;</trd>
</drv>
</art>
