<kap>
  <rad>poliomjelit</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="poliomjelit.0o">
  <kap><tld/>o</kap>
  <snc mrk="poliomjelit.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Spina paralizo de infanoj, konta&gcirc;a virusa <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>, kiu viglis
      precipe en la 20a
      jarcento: 
      <ekz>
        dum tiu tempo li okupis sin krom sia gripo-esploro jam anka&ubreve; 
        pri la problemoj de <tld/>o
        <fnt>
        <aut>C. Riess, trad. O. Ginz</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5705/nokto.html">La
        infansava nokto</url></vrk>,
        <lok>Norda Prismo 1957:5, p. 252-258a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_ib;&c_ja;&c_m;&c_ib;&c_e;&c_l;&c_ib;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>d&#x11B;tsk&#xE1; obrna</trd>,
    <trd>poliomyelitida</trd>,
    <trd>poliomyelitis</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kinderl&auml;hmung</trd>,
    <trd>spinale Kinderl&auml;hmung</trd>,
    <trd>Poliomyelitis</trd>,
    <trd>Polio</trd>
  </trdgrp>
  <trd lng="fr">poliomy&eacute;lite</trd>
  <trdgrp lng="id">
    <trd>polio</trd>,
    <trd>poliomielitis</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DD;&#x30EA;&#x30AA;</trd>,
    <trd>&#x6025;&#x6027;&#x7070;&#x767D;&#x9AC4;&#x708E;  <pr>&#x304D;&#x3085;&#x3046;&#x305B;&#x3044;&#x304B;&#x3044;&#x306F;&#x304F;&#x305A;&#x3044;&#x3048;&#x3093;</pr></trd>,
    <trd>&#x5C0F;&#x5150;&#x9EBB;&#x75FA;  <pr>&#x3057;&#x3087;&#x3046;&#x306B;&#x307E;&#x3072;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kinderverlamming</trd>,
    <trd>polio</trd>
  </trdgrp>
  <trd lng="pl">choroba Heinego-Medina</trd>
  <trdgrp lng="pt">
    <trd>poliomielite</trd>,
    <trd>doen&ccedil;a de <klr tip="amb">Heine-Medin</klr></trd>
  </trdgrp>
  <trd lng="ro">poliomielit&abreve;</trd>
  <trd lng="sk">detsk&#xE1; obrna</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&c_ib;&#x43E;&#x43C;&c_ib;&c_jeu;&#x43B;&c_ib;&c_t;</trd>
</drv>
