    <trd>kereskedelmi <ind>telep</ind></trd>
  </trdgrp>
  <trd lng="nl">factorij</trd>
  <trd lng="pl">faktoria</trd>
  <trd lng="pt">feitoria</trd>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>
