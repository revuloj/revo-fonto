<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>forsiti</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="forsiti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Forsythia</trd>), ornama arbedo
      devenanta el &Ccirc;inujo, kies flavaj floroj aperas
      printempe jam anta&ubreve; la folioj.
    </dif>
  </snc>
  <bld lok="../bld/forsiti.jpg">
    florantaj bran&ccirc;oj de <tld/>o
  </bld>
  <trdgrp lng="de">
    <trd>Forsythia</trd>,
    <trd>Goldflieder</trd>
  </trdgrp>
  <trd lng="es">campanita china</trd>
  <trd lng="nl">forsythia</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_r;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_s;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_s;&c_a;&c_j;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
