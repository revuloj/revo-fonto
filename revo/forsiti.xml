<art mrk="$Id: forsiti.xml,v 1.14 2023/11/10 06:07:59 revo Exp $">
<kap>
  <rad>forsiti</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="forsiti.0o">
  <kap><tld/>o</kap>
  <snc mrk="forsiti.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;indevena ornama arbedo, kies brile flavaj floroj aperas
      printempe jam anta&ubreve; la folioj,
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="oleac.0oj">oleacoj</ref>,
      (<trd lng="la">Forsythia</trd>):
      <ekz>
        &gcirc;oje briladas / la levi&gcirc;anta suno / sur <tld/>o
        <fnt>
          <aut>Serena</aut>,
          <vrk><url ref="http://blog.goo.ne.jp/serena_2005">[blogo]</url></vrk>,
          <lok>2005-09-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/8/81/Forsythia2.JPG/360px-Forsythia2.JPG" prm="CC-BY-SA-3.0">floranta bran&ccirc;o de <tld/>o

       <fnt>
         <aut>Donar Reiskoffer, propra verko</aut>
         <lok><url ref="&WCW;/File:Forsythia2.JPG">&WCW;/File:Forsythia2.JPG</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_f;&c_a;&c_r;&c_z;&c_y;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Forsythia</trd>,
    <trd>Goldflieder</trd>
  </trdgrp>
  <trd lng="es">campanita china</trd>
  <trd lng="fr">forsythia</trd>
  <trdgrp lng="hu">
    <trd>aranyfa</trd>,
    <trd>aranycserje</trd>,
    <trd>forzitia</trd>
  </trdgrp>
  <trd lng="ja">&#x30EC;&#x30F3;&#x30AE;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">forsythia</trd>
  <trd lng="pt">forsythia</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_r;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_s;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_s;&c_a;&c_j;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="uk">&#x444;&#x43E;&#x440;&#x437;&#x438;&#x446;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x8FDE;&#x7FD8; <pr>li&#xE1;nqi&#xE1;o</pr></trd>
</drv>
</art>
