        <tld/>a vida&jcirc;o, monumento, ceremonio, manifestacio,
        rezultato, servo, elpensa&jcirc;o, stilo;
      </ekz>
      <ekz>
        venis Agripo kaj Bernike kun <tld/>a parado
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a diverseco de niaj ideoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        eldoni <klr>[&#8230;]</klr> <tld/>an pazigrafian provon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grandegaj pilonoj de la plej <tld/>a templo en Egipto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        malmultaj nubetoj sur &ccirc;ielo <tld/>a
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Kolonia Ekspozicio en Parizo <klr>[&#8230;]</klr> havis <tld/>an sukceson
        <fnt>
          <bib>LSB</bib>
          <lok>Angkor'a&ubreve;!</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tero estis <klr>[&#8230;]</klr> plena de nekonataj lokoj kaj <tld/>aj ebloj
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>.
      </ekz>
