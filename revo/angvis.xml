<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>angvis</rad>/o
</kap>
<drv mrk="angvis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="angvis.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Anguis</trd>), senpieda rampulo, naskanta
      vivantajn idojn.
    </dif>
  </snc>
  <trd lng="de">Blindschleiche</trd>
  <trdgrp lng="ru">
    <trd>&#1084;&#1077;&#1076;&#1103;&#1085;&#1080;&#1094;&#1072;</trd>,
    <trd>&#1074;&#1077;&#1088;&#1077;&#1090;&#1077;&#1085;&#1085;&#1080;&#1094;&#1072;</trd>
  </trdgrp>
</drv>
<drv mrk="angvis.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Anguidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schleichen</trd>
</drv>
</art>
