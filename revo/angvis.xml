<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: angvis.xml,v 1.24 2023/08/30 18:23:34 revo Exp $">
<kap>
<rad>angvis</rad>/o
</kap>
<drv mrk="angvis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Senpieda <ref tip="lst" cel="reptil.0o" lst="voko:reptilioj">reptilio</ref>
      naskanta vivantajn idojn;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="angvis1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anguis</trd>):
      <ekz>
        <klr>[en]</klr> Krimea montaro <klr>[&#x2026;]</klr> kaj anta&ubreve;montaro multas lacertoj,
        ranoj, bufoj, tritoj, <tld/>oj, kolubroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127782">Krimea montaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        najtingalo kaj <tld/>o, kiuj amba&ubreve; havis nur unu okulon
        <fnt>
          <aut>Fratoj Grimm</aut>
          <vrk><url ref="http://www.ipernity.com/blog/cezar/450159">Pri najtingalo kaj angviso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/5d/Blindschleiche_Anguis_fragilis.jpg/640px-Blindschleiche_Anguis_fragilis.jpg" prm="CC BY 3.0"><tld/>o (Anguis fragilis) &ccirc;e Blaubeuren, Germanujo
        <fnt>
          <aut>H.Krisp, propra verko</aut>
          <lok><url ref="&WCW;/File:Blindschleiche_Anguis_fragilis.jpg">&WCW;/File:Blindschleiche_Anguis_fragilis.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vidriol</trd>,
    <trd>serp de vidre</trd>,
    <trd>noia</trd>
  </trdgrp>
  <trd lng="cs">slep&yacute;&scaron;</trd>
  <trd lng="de">Blindschleiche</trd>
  <trd lng="fr">orvet</trd>
  <trdgrp lng="he">
    <trd>&#x5E7;&#x5DE;&#x5D8;&#x5DF;</trd>,
    <trd>&#x5E0;&#x5D7;&#x5E9; &#x5D6;&#x5DB;&#x5D5;&#x5DB;&#x5D9;&#x5EA;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;r&eacute;keny <ind>gy&iacute;k</ind></trd>,
    <trd>l&aacute;batlan <ind>gy&iacute;k</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30B7;&#x30CA;&#x30B7;&#x30C8;&#x30AB;&#x30B2;</trd>
  <trd lng="nl">hazelworm</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_ja;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_je;&c_t;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_je;&c_t;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">slep&uacute;ch</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x435;&#x440;&#x435;&#x442;&#x456;&#x43D;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x432;&#x435;&#x440;&#x435;&#x442;&#x456;&#x43B;&#x44C;&#x43D;&#x438;&#x446;&#x44F;</trd>
  </trdgrp>
</drv>

</art>
