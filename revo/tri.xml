<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>tri</rad></kap>
    <drv mrk="tri.0">
      <kap><ofc>*</ofc><tld/></kap>
      <snc>
        <dif>
          Du kaj unu. 
        </dif>
      </snc>
      <trd lng="bg">&c_t;&c_r;&c_i;</trd>
      <trd lng="cs">t&rcaron;i</trd>
      <trd lng="es">tres</trd>
      <trd lng="nl">drie</trd>
      <trd lng="fr">trois</trd>
      <trd lng="de">drei</trd>
      <trd lng="en">three</trd>
      <trd lng="pl">trzy</trd>
      <trd lng="ru">&c_t;&c_r;&c_i;</trd>
      <trd lng="tr">&uuml;&ccedil;</trd>

    </drv>
    <drv mrk="tri.0unuo">
      <kap><tld/>unuo</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Kuno de tri <ref cel="person.0o.KRI">personoj</ref>:
          Patro, Filo kaj Sankta Spirito,
          konsistigantaj, la&ubreve; la kristana mistero, unu solan Dion la&ubreve;
          sia <ref cel="esenc.0o.KRI">esenco</ref>.  
        </dif>
      </snc>
      <trd lng="el">&Tau;&rho;&jota;&alfa_ton;&sigma_fina;</trd>
      <trd lng="en">Trinity</trd>
      <trd lng="es">Trinidad</trd>
      <trd lng="ru">&c_T;&c_r;&c_o;&c_i;&c_c;&c_a;</trd>
    </drv>
  </art>
