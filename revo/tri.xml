<drv mrk="tri.0ope">
  <kap>
    <tld/>ope
  </kap>
  <snc>
    <dif>
      Tri aferoj a&ubreve; personoj kune:
      <ekz>
        en la nomo de Sankta <tld lit="T"/>opo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        fadeno <tld/>opigita ne balda&ubreve; dis&scirc;iri&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 4:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu tempo <nom>Izrael</nom> estos <tld/>ope kun Egiptujo kaj
        Asirio
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 19:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj bonaj aferoj kuni&gcirc;as <tld/>ope
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_t;&c_r;&c_o;&c_h;</trd>,
