<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tri</rad>
</kap>
<drv mrk="tri.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Du kaj unu. Matematika simbolo 3:
      <ekz>
        kie iam lo&gcirc;is unu malliberulo, nun lo&gcirc;as du, <tld/>, e&ccirc;
        kvar<fnt><aut>Paul Gubbins</aut>: <vrk>&Scirc;ipo de la pasinteco</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 6a, p. 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_r;&c_i;</trd>
  <trd lng="ca">tres</trd>
  <trd lng="cs">t&rcaron;i</trd>
  <trd lng="de">drei</trd>
  <trd lng="en">three</trd>
  <trd lng="es">tres</trd>
  <trd lng="fr">trois</trd>
  <trd lng="he">&shin;&lamed;&vav;&shin;</trd>
  <trd lng="it">tre</trd>
  <trd lng="nl">drie</trd>
  <trd lng="pl">trzy</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;</trd>
  <trd lng="sv">tre</trd>
  <trd lng="tr">&uuml;&ccedil;</trd>
</drv>

<drv mrk="tri.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      triaranga, venanta post du anta&ubreve;aj:
      <ekz>
        la <tld/>a jarmilo alvenos la 1an de januaro 2001<fnt><aut>Evgeni
        Georgiev</aut>: <vrk>&Ccirc;u kalendar-reformo estos bezonata?</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 2a, p. 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">tercer</trd>
  <trd lng="en">third</trd>
  <trdgrp lng="fr">
    <trd>troisi&egrave;me</trd>
  </trdgrp>
</drv>

<drv mrk="tri.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      triarange, post du anta&ubreve;aj:
      <ekz>
        en 1990 Nov-Zelando havis la plej grandan proporcion de memmortigoj
        por junaj virinoj kaj la <tld/>e plej alta por junaj
        viroj<fnt><aut>Donald Rogers</aut>: <vrk>Ekonomia miraklo a&ubreve;
        malsano?</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 2a, p.
        14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">thirdly</trd>
  <trdgrp lng="fr">
    <trd>troisi&egrave;mement</trd>
  </trdgrp>
</drv>

<drv mrk="tri.0unuo">
  <kap><tld lit="T"/>unuo</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Kuno de tri <ref cel="person.0o.KRI">personoj</ref>:
      Patro, Filo kaj Sankta Spirito,
      konsistigantaj, la&ubreve; la kristana mistero, unu solan Dion la&ubreve;
      sia <ref cel="esenc.0o.KRI">esenco</ref>.
    </dif>
  </snc>
  <trd lng="grc">&Tau;&rho;&jota;&alfa_ton;&sigma_fina;</trd>
  <trdgrp lng="ca">
    <trd>tr&iacute;ade</trd>,
    <trd>trinitat</trd>,
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dreieinigkeit</trd>,
    <trd>Dreifaltigkeit</trd>,
    <trd>Trinit&auml;t</trd>
  </trdgrp>
  <trd lng="en">Trinity</trd>
  <trd lng="es">Trinidad</trd>
  <trdgrp lng="fr">
    <trd>trinit&eacute;</trd>
  </trdgrp>
  <trd lng="he">&he;&shin;&yod;&lamed;&vav;&shin; &he;&qof;&dalet;&vav;&shin;</trd>
  <trd lng="nl">Drie&euml;enheid</trd>
  <trd lng="ru">&c_T;&c_r;&c_o;&c_i;&c_c;&c_a;</trd>
  <trd lng="sv">treenigheten</trd>
</drv>
</art>
