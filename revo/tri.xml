<drv mrk="tri.0">
  <kap> <ofc>*</ofc> <tld/>
  </kap>
  <mlg>3</mlg>
  <snc>
    <dif>
      Du kaj unu:
      <ekz>
        naski&gcirc;is al Noa <tld/> filoj: <nom>&Scirc;em</nom>, <nom>&Hcirc;am</nom> kaj <nom>Jafet</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sekvantaj <tld/> verkoj estas rigardataj kiel fundamento de Esperanto: 1) La
        16-regula gramatiko, 2) la Universala Vortaro, 3) la Ekzercaro
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie iam lo&gcirc;is unu malliberulo, nun lo&gcirc;as du,
        <tld/>, e&ccirc; kvar
        <fnt>
          <aut>Paul Gubbins</aut>:
          <vrk><url ref="&Monato;/1997/005526.html">&Scirc;ipo de la pasinteco</url></vrk>,
          <bib>Monato</bib>
          <lok>1997/06, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[por]</klr> fari&gcirc;i poetino <klr>[&#8230;]</klr> oni bezonas <tld/> aferojn:
        sa&gcirc;on, fantazion kaj senton
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la juna virino aperis <tld/> diversaj sentoj, nome: rememoro, sopiro kaj
        espero
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u li frenezi&gcirc;is? li faris <tld/> partojn, kaj ni estas du
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; <tld/> tagoj mi vizitis vian kuzon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        mil okcent na&ubreve;dek <tld/>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj" val="3">nombro</ref>
