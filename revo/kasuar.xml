<art mrk="$Id$">
<kap>
  <rad>kasuar</rad>/o
</kap>
<drv mrk="kasuar.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <tld/>oformaj birdoj
      (<trd lng="la">Casuarius</trd>), vivanta en A&ubreve;stralio
      simila al struto kun nigra plumaro kaj senplumaj kapo kaj kolo.
    </dif>
  </snc>
  <trd lng="en">cassowary</trd>
  <trd lng="de">Kasuar</trd>
  <trd lng="nl">kasuaris</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_u;&c_a;&c_r;</trd>
</drv>
</art>
