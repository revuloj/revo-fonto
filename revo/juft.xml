<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: juft.xml,v 1.2 2023/08/11 12:42:03 afido Exp $">
    <kap>
      <rad>juft</rad>/o
    </kap>

<drv mrk="juft.0o">
  <kap><tld/>o</kap>
  <snc mrk="juft.0o.ledo">
    <dif>
      Densa fortika <ref tip="super" cel="led.0o">ledo</ref>, interne
      traktata per betulgudro, uzata i.a. por &scirc;uoj:
      <ekz>
          jufto de Tvero
          <fnt><aut>J. German, tr. J. Finkel</aut>:
            <vrk>Rusio juna</vrk>.
            <lok><url
 ref="https://www.esperanto.mv.ru/German/rus-1-02-3.html">&Ccirc;ap. 2.3</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas ledo farita de junaj bestoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=571152"
              >Jufto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ju;&c_h;&c_t;</trd>,
      <trd>&c_ju;&c_h;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Juchtenleder</trd>
    <trd lng="en">Russia leather</trd>
    <trd lng="ru">&c_ju;&c_f;&c_t;&c_mol;</trd>
  </snc>
</drv>

  </art>

