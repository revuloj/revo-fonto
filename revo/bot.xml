<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bot.xml,v 1.38 2023/10/29 19:11:03 revo Exp $">
<kap>
<ofc>*</ofc>
  <rad>bot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bot.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Alta <ref tip="super" cel="sxu.0o.TEKS">&scirc;uo</ref>
      ka&ubreve;&ccirc;uka a&ubreve; leda, kiu atingas &gcirc;is genuo:
      <ekz>
        peltaj <tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj piedoj <klr>[&#x2026;]</klr> &scirc;velis kaj forte doloris al li en la malvastaj
        <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis novajn <tld/>ojn, <klr>[&#x2026;]</klr> ili knaris
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> &scirc;telas en la <tld/>ojn ar&gcirc;entajn kuleretojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn po&scirc;ojn, la tornistron, la &ccirc;akon kaj la <tld/>ojn li plenigis tiel, ke li
        preska&ubreve; ne povis iri
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[lia]</klr> tuta lo&gcirc;eja ilaro konsistis nur el <ind><tld/>otirilo</ind> kaj kombilo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
        <trd lng="de">Stiefelknecht</trd>
      </ekz>
      <ekz>
        <klr>[ili]</klr> portis <klr>[&#x2026;]</klr> mallongajn surtutojn kaj <klr>[&#x2026;]</klr>
        <tld/>ojn kun <ref cel="tibi.0ingo">tibiingo</ref>j
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kristnasko alproksimi&gcirc;as per <ind>sepmejlaj <tld/>oj</ind>
        <klr>(tre rapide)</klr>
        <fnt>
          <vrk><url ref="http://www.ipernity.com/blog/bernardo/292276">ipernity: La mesia libro von Bernardo</url></vrk>
        </fnt>;
        <trd lng="de">Siebenmeilenstiefel</trd>
      </ekz>
      <ekz>
        najbaro ne &gcirc;emas, kiam <tld/>o nin premas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_o;&c_t;</trd>
  <trd lng="bg">&c_b;&c_o;&c_t;&c_u;&c_sh;</trd>
  <trd lng="br">heuz</trd>
  <trd lng="ca">bota</trd>
  <trd lng="de">Stiefel</trd>
  <trd lng="en">boot</trd>
  <trd lng="es">bota</trd>
  <trd lng="fr">botte</trd>
  <trd lng="id">bot<klr tip="amb"> (sepatu)</klr></trd>
  <trdgrp lng="pl">
    <trd>kozak</trd>
  </trdgrp>
  <trd lng="pt">bota</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;&c_o;&c_g;</trd>
  <trd lng="sv">st&ouml;vel</trd>
    <trdgrp lng="zh">
    <trd>&#x9774; <pr>xu&#x113;</pr></trd>,
    <trd>&#x9774;&#x5F62;&#x9152;&#x676F; <pr>xu&#x113;x&#xED;ngji&#x1D4;b&#x113;i</pr></trd>,
    <trd>&#x9774;&#x5B50; <pr>xu&#x113;zi</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&t_la;&t_hata;&t_ma;&t_tsheg;&t_ga;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>holinka</trd>,
    <trd>vysok&aacute; bota</trd>
  </trdgrp>
  <trd lng="he">&mem;&gimel;&fpe;</trd>
  <trdgrp lng="sk">
    <trd>vysok&aacute; top&aacute;nka</trd>,
    <trd>&ccaron;i&zcaron;ma</trd>
  </trdgrp>
  <trd lng="sw">buti</trd>
  <trd lng="uk">&#x447;&#x43E;&#x431;&#x456;&#x442;</trd>
</drv>

<drv mrk="bot.0eto">
  <kap><tld/>eto, <var><kap>duon<tld/>o</kap></var></kap>
  <snc mrk="bot.0eto.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS">&Scirc;uo</ref> meza inter
      <tld/>o kaj ordinara &scirc;uo, iom superanta la maleolon:
      <ekz>
        &scirc;i petis nur pri <klr>[&#x2026;]</klr> paro da <tld/>etoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        juna fra&ubreve;lineto <klr>[&#x2026;]</klr> en malvasta pelto kaj kun mirinde graciaj
        <tld/>etoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li portis duon<tld/>ojn, rigidan kolkovrilon kaj pudritan perukon
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_k;</trd>
    <trd lng="br">botinez</trd>
    <trdgrp lng="ca">
      <trd>bot&iacute;</trd>,
      <trd>botina</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Stiefeletten</trd>,
      <trd>Schn&uuml;rstiefel</trd>,
      <trd>Halbschaftstiefel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>short boot</trd>,
      <trd>hiking boot</trd>,
      <trd>work boot</trd>
    </trdgrp>
    <trd lng="es">bot&iacute;n</trd>
    <trd lng="fr">bottine</trd>
    <trdgrp lng="pl">
      <trd>bot</trd>,
      <trd>boty<klr> {pl}</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_t;&c_i;&c_n;&c_o;&c_k;</trd>
    <trd lng="sv">k&auml;nga</trd>
  </snc>
  <trd lng="cs">bota nad kotn&iacute;ky</trd>
  <trd lng="sk">&ccaron;i&zcaron;mi&ccaron;ka</trd>
  <trd lng="uk">&#x447;&#x435;&#x440;&#x435;&#x432;&#x438;&#x43A;</trd>
  <trd lng="zh">&#x77ED;&#x7D71;&#x9774; <pr>du&#x1CE;nt&#x1D2;ngxu&#x113;</pr></trd>
</drv>

<drv mrk="bot.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc mrk="bot.0isto.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="far.0isto">Faristo</ref> de <tld/>oj 
      kaj aliaj piedvestoj:
      <ekz>
        la <tld/>isto faras <tld/>ojn kaj &scirc;uojn
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastro <klr>[&#x2026;]</klr> montris al mi kabineton de meza grandeco, en kiu mi de
        la unua rigardo ekkonis metiejon de <tld/>isto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>istoj kaj tanistoj alkuris kaj demandis, kiom li postulas pro la feloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili aspektas kiel miaj propraj galo&scirc;oj <klr>[&#x2026;]</klr> e&ccirc; okulo de <tld/>isto ne
        povus ilin distingi
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>!
      </ekz>
      <ekz>
        &ccirc;e <tld/>isto la &scirc;uo estas &ccirc;iam kun truo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_sh;&c_a;&c_v;&c_je;&c_c;</trd>
  <trd lng="br">kere</trd>
  <trd lng="ca">sabater</trd>
  <trd lng="de">Stiefelmacher</trd>
  <trd lng="en">bootmaker</trd>
  <trd lng="es">zapatero<klr> (profesi&oacute;n)</klr></trd>
  <trdgrp lng="fr">
    <trd>bottier</trd>,
    <trd>cordonnier</trd>
  </trdgrp>
  <trd lng="pl">szewc</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>obuvn&iacute;k</trd>,
    <trd>&scaron;vec</trd>
  </trdgrp>
  <trd lng="he">&samekh;&#x5B7;&nun;&dalet;&#x5B0;&lamed;&#x5B8;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#x9774;&#x5C4B;  <pr>&#x304F;&#x3064;&#x3084;</pr></trd>,
    <trd>&#x9774;&#x8077;&#x4EBA;  <pr>&#x304F;&#x3064;&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="sk">&ccaron;i&zcaron;m&aacute;r</trd>
</drv>

<drv mrk="bot.akvo0o">
  <kap>
    akvo<tld/>o, <var><kap>gum<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o el materialo nepenetrebla de akvo por uzi sur malseka grundo:
      <ekz>
        la maljuna korteganino metis sur sin akvo<tld/>ojn kaj postkuris
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu kun <ind>akvo<tld/>oj</ind> iras en mar&ccirc;a akvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
        <trd lng="de">Gummistiefel</trd>
      </ekz>
      <ekz>
        la mano profundi&gcirc;as tre singarde en unu el la du gum<tld/>oj
        <fnt>
          <bib>MD05</bib>
          <vrk>La skandalo de la &leftquot;kvaza&ubreve;politiko&rightquot; kiu makulas la prezidanton Alvaro Uribe</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="galosx.0o">galo&scirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="de">Gummistiefel</trd>
  <trd lng="en">Wellington boot</trd>
  <trdgrp lng="fr">
    <trd>botte en caoutchouc</trd>,
    <trd>botte de pluie</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_i;&#x301;&c_n;&c_o;&c_v;&c_y;&c_je; &c_s;&c_a;&c_p;&c_o;&c_g;&c_i;&#x301;</trd>
</drv>


<drv mrk="bot.rajd0o">
  <kap>rajd<tld/>o</kap>
  <snc mrk="bot.rajd0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="B"/>o alta &gcirc;is sub la genuo kun glata plando, 
      konvenigita por rajdado:
      <ekz>
        <nom>Sara</nom> helpas al li detiri la rajd<tld/>ojn
        <fnt>
          <bib>RBa</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x99AC;&#x9774; <pr>m&#x1CE;xu&#x113;</pr></trd>
</snc>
  <trd lng="de">Reitstiefel</trd>
  <trd lng="es">bota de montar</trd>
  <trd lng="fr">botte cavali&egrave;re</trd>
  <trd lng="pl">oficerki</trd>
</drv>

<drv mrk="bot.lekila0ojn">
  <kap>leki la <tld/>ojn</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="bot.lekila0ojn.KOMUNE">
    <dif>
      Malnoble <ref tip="super" cel="flat.0i">flati</ref>, <ref tip="vid" cel="servut.0o">servutece</ref> 
      konduti:
      <ekz>
        <nom>Ka&ubreve;ser</nom> scias konduti en la &ccirc;eesto de la intendanto pli 
        dece-konvene ol tiuj &scirc;iaj kolegoj, nek provante konkuri, nek 
        <tld/>ante la botojn
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>1982</lok></fnt>;
      </ekz>
      <ekz>
        la landaj esperantistaj organiza&jcirc;oj <klr>[&#x2026;]</klr> servute 
        lekis la <tld/>ojn de la regantoj, asertante, ke la
        esperantistoj ne estas malamikoj de la ekzistantaj registaroj kaj de la 
        &scirc;tataj lingvoj
        <fnt>
          <aut>M. Duc Goninaz</aut>,
          <vrk><url ref="http://www.satesperanto.org/La-lecionoj-de-La-dangxera-lingvo.html">
          La lecionoj de &leftquot;La dan&gcirc;era lingvo&rightquot;</url></vrk>,
          <lok>Sennacieca Revuo, 1993, numero 121a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">besar els peus (d'alg&uacute;)</trd>
  <trd lng="de">die <ind>Stiefel</ind> lecken</trd>
  <trdgrp lng="es">
    <trd>agradar a alguien para conseguir un favor</trd>,
    <trd>lamer las botas</trd>,
    <trd>ser un lameculos</trd>,
    <trd>ser un pelota</trd>
  </trdgrp>
  <trd lng="fr">l&eacute;cher les bottes</trd>
  <trd lng="pl">p&lstroke;aszczy&cacute; si&eogonek; <klr>(przed kim&sacute;)</klr></trd>
</drv>
</art>
