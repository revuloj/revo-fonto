<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: estron.xml,v 1.3 2023/09/27 10:29:36 revo Exp $">
<kap>
    <rad>estron</rad>/o
</kap>
<drv mrk="estron.0o">
  <kap><tld/>o</kap>
  <snc mrk="estron.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="estrogen.0o.FIZL">Estrogeno</ref>, kiu estas
      precipe produktita post la menopa&ubreve;zo:
      <ekz>
        naturaj estrogenoj estas la foliklohormonoj estradiolo, <tld/>o
        kaj estriolo
        <fnt>
          <vrk>Medicinalaj Plantoj kaj Sufero de la Prostato</vrk>,
          <lok><url
          ref="https://scienca-revuo.info/article/download/115/94">Scienca
          Revuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_t;&c_r;&c_o;&c_n;</trd>
  <trd lng="ca">estrona</trd>
  <trd lng="cs">estron</trd>
  <trd lng="de">&Ouml;stron</trd>
  <trdgrp lng="en">
    <trd>estrone</trd>,
    <trd>oestrone</trd>
  </trdgrp>
  <trd lng="es">estrona</trd>
  <trd lng="fr">&oelig;strone</trd>
  <trd lng="gl">estrona</trd>
  <trdgrp lng="nl">
    <trd>oestron</trd>,
    <trd>estron</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_s;&c_t;&c_r;&c_o;&c_n;</trd>
  <trd lng="sv">&ouml;stron</trd>
</drv>
</art>

