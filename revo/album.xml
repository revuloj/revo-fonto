    <trd>&#20889;&#29983;&#24115;  <pr>&#12375;&#12419;&#12379;&#12356;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#12469;&#12452;&#12531;&#24115;  <pr>&#12373;&#12356;&#12435;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#26354;&#38598;  <pr>&#12365;&#12423;&#12367;&#12375;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>album, pam&auml;tn&iacute;k</trd>,
    <trd>pam&auml;tn&iacute;k</trd>
  </trdgrp>
