<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tuberk.xml,v 1.22 2020/02/16 15:10:16 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>tuberkuloz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="tuberk.0o">
  <kap><tld/>o</kap>
  <snc mrk="tuberk.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Bacila <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> karakterizata de formi&gcirc;o de
      <ref tip="prt" cel="tuberk1.0o">tuberkuloj</ref> en iaj
      organoj &ccirc;e homo kaj besto:
      <ekz>
        pulma, osta <tld/>o;
      </ekz>
      <ekz>
        prevente ni &ccirc;iuj povas batali kontra&ubreve; <tld/>o
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_aT1XL3xtqYI/"
              >Radio Havano Kubo Esperanto 16-02-20 </url></vrk>
          <lok>2020-02-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="ftiz.0o">ftizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_h;&c_o;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
  </trdgrp>
  <trd lng="ca">tuberculosi</trd>
  <trd lng="cs">tuberkul&oacute;za</trd>
  <trd lng="de">Tuberkulose</trd>
  <trd lng="en">tuberculosis</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_kasra;&a_l;&a_shadda;</trd>,
    <trd>&a_t;&a_b; &a_l;&a_A;&a_z;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">tuberculose</trd>
  <trdgrp lng="hu">
    <trd>g&uuml;m&odblac;k&oacute;r</trd>,
    <trd>tuberkul&oacute;zis</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>TBC</trd>,
    <trd>tuberkulosis</trd>
  </trdgrp>
  <trd lng="nl">tuberculose</trd>
  <trdgrp lng="pl">
    <trd>tuberkuloza</trd>,
    <trd>gru&zacute;lica</trd>,
    <trd>suchoty<klr> (przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">tuberculose</trd>
  <trd
  lng="ru">&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>suchoty</trd>,
    <trd>tuberkul&oacute;za</trd>
  </trdgrp>
</drv>

<drv mrk="tuberk.koks0o">
  <kap>koks<tld/>o</kap>
  <snc mrk="tuberk.koks0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
    <ref tip="malprt" cel="tuberk.0o">Tuberkulozo</ref> de la koksa
    artiko.
    </dif>
  </snc>
  <trd lng="fr">coxalgie</trd>  
</drv>
</art>
