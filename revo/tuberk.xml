<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>tuberkuloz</rad>/o
</kap>
<drv mrk="tuberk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Bacila malsano karakterizata de formi&gcirc;o de tuberkuloj en iaj organoj
      &ccirc;e homo kaj besto:
      <ekz>
        pulma, osta <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ftiz.0o">ftizo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Tuberkulose</trd>
  <trd lng="nl">tuberculose</trd>
  <trd lng="ru">&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
</drv>
</art>
