<drv mrk="tuberk.0o">
  <kap><tld/>o</kap>
  <snc mrk="tuberk.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Bacila <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref> karakterizata de formi&gcirc;o de
      <ref tip="prt" cel="tuberk1.0o">tuberkuloj</ref> en iaj
      organoj &ccirc;e homo kaj besto:
      <ekz>
        pulma, osta <tld/>o;
      </ekz>
      <ekz>
        prevente ni &ccirc;iuj povas batali kontra&ubreve; <tld/>o
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_aT1XL3xtqYI/">Radio Havano Kubo Esperanto 16-02-20 </url></vrk>
          <lok>2020-02-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="ftiz.0o">ftizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_h;&c_o;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
  </trdgrp>
  <trd lng="ca">tuberculosi</trd>
  <trd lng="cs">tuberkul&oacute;za</trd>
  <trd lng="de">Tuberkulose</trd>
  <trd lng="en">tuberculosis</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_kasra;&a_l;&a_shadda;</trd>,
    <trd>&a_t;&a_b; &a_l;&a_A;&a_z;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">tuberculose</trd>
  <trdgrp lng="hu">
    <trd>g&uuml;m&odblac;k&oacute;r</trd>,
    <trd>tuberkul&oacute;zis</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>TBC</trd>,
    <trd>tuberkulosis</trd>
  </trdgrp>
  <trd lng="ja">&#x7D50;&#x6838;  <pr>&#x3051;&#x3063;&#x304B;&#x304F;</pr></trd>
  <trd lng="nl">tuberculose</trd>
  <trdgrp lng="pl">
    <trd>tuberkuloza</trd>,
    <trd>gru&zacute;lica</trd>,
    <trd>suchoty<klr> (przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">tuberculose</trd>
  <trd lng="ru">&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>suchoty</trd>,
    <trd>tuberkul&oacute;za</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x443;&#x431;&#x435;&#x440;&#x43A;&#x443;&#x43B;&#x44C;&#x43E;&#x437;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D50;&#x6838;&#x75C5; <pr>ji&#xE9;h&#xE9;b&#xEC;ng</pr></trd>,
    <trd>&#x7ED3;&#x6838;&#x75C5; <pr>ji&#xE9;h&#xE9;b&#xEC;ng</pr></trd>,
    <trd>&#x7D50;&#x6838; <pr>ji&#xE9;h&#xE9;</pr></trd>,
    <trd>&#x7ED3;&#x6838; <pr>ji&#xE9;h&#xE9;</pr></trd>,
    <trd>&#x75E8; <pr>l&#xE1;o</pr></trd>,
    <trd>&#x7646; <pr>l&#xE1;o</pr></trd>,
    <trd>&#x80BA;&#x7D50;&#x6838; <pr>f&#xE8;iji&#xE9;h&#xE9;</pr></trd>,
    <trd>&#x80BA;&#x7ED3;&#x6838; <pr>f&#xE8;iji&#xE9;h&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tuberk.koks0o">
  <kap>koks<tld/>o</kap>
  <snc mrk="tuberk.koks0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
    <ref tip="malprt" cel="tuberk.0o">Tuberkulozo</ref> de la koksa
    artiko.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_k;&c_s;&c_a;&c_l;&c_mol;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z; &c_t;&c_a;&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_g;&c_n;&c_je;&c_v;&c_a;&c_g;&c_a; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">coxalgie</trd>  
</drv>
