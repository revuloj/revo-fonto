<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plad</rad>/o
</kap>
<drv mrk="plad.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plad.0o.propr">
    <dif>
      Granda telero, kiu servas por alporti sur la tablon la man&gcirc;a&jcirc;ojn,
      disdonotajn al &ccirc;iu man&gcirc;anto:
      <ekz>
        <tld/>o da gria&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">t&aacute;l</trd>
    <trdgrp lng="nl">
      <trd>schaal</trd>,
      <trd>schotel</trd>
    </trdgrp>
  </snc>
  <snc mrk="plad.0o.mangx">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La man&gcirc;a&jcirc;o entenata en <ref tip="vid"
      cel="plad.0o.propr"><tld/>o</ref>.
    </dif>
    <trd lng="hu">&eacute;tel</trd>
    <trd lng="nl">gerecht</trd>
  </snc>
  <snc mrk="plad.0o.menu">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Ccirc;iu el la eroj, kiuj konsistigas <ref tip="malprt"
      cel="menu.0o.bankeda">menuon</ref>.
    </dif>
    <trd lng="nl">gerecht</trd>
  </snc>
  <trd lng="de">Platte</trd>
  <trd lng="es">plato</trd>
  <trd lng="fr">plat<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">fog&aacute;s<klr tip="ind"> (&eacute;tel)</klr></trd>
  <trd lng="ru">&c_b;&c_l;&c_ju;&c_d;&c_o;</trd>
</drv>

<drv mrk="plad.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="P"/>o, destinita por paciencigi malsatulojn, anta&ubreve;
      la man&gcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>anta&ubreve;<tld/>e al</ind> la tagman&gcirc;o, Kant ricevis
        de Yvonne, la kara edzineto, unue kiseton, kaj poste predikon iom
        severan
        <fnt><bib>UrdHadda</bib><lok>p. 11</lok></fnt>.
        <trdgrp lng="fr">
          <trd>en guise d'<ind>amuse-gueule</ind> &agrave;</trd>,
          <trd>en guise d'<ind>entr&eacute;e en mati&egrave;re</ind> &agrave;</trd>,
          <trd>en <ind>pr&eacute;lude</ind> &agrave;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">amuse-gueule</trd>
  <trd lng="ru">&c_l;&c_jo;&c_g;&c_k;&c_a;&c_ja; <ind>&c_z;&c_a;&c_k;&c_u;&c_s;&c_k;&c_a;</ind></trd>
</drv>
</art>
