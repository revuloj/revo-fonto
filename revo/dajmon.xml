<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>dajmon</rad>/o
    </kap>

    <drv mrk="dajmon.0o">
      <kap>
        <tld/>o <fnt><bib>PIV1</bib></fnt>
      </kap>
      <snc>
        <uzo tip="fak">REL</uzo>
        <dif>
	  Mezranga supernatura esta&jcirc;o, ofte malbona sed iam favora,
	  peranto inter la homoj kaj la dioj, a&ubreve; granda dio aganta
	  inkognite:
	  <ekz>
	    la persona <tld/>o de Sokrato;
	  </ekz><ekz>
	    latineca vorto por la grekdevena &leftquot;<tld/>o&rightquot; estas
	    <ref tip="sub" cel="geni.0o.Roma">genio<sncref/></ref>.
	  </ekz>
        </dif>
      </snc>
      <refgrp tip="sub">
	<ref cel=" angxel.0o">an&gcirc;elo</ref>,
	<ref cel="demon.0o.diablo">demono</ref>,
	<ref cel="elf.0o">elfo</ref>,
	<ref cel="fe.0o">feo</ref>,
	<ref cel="kobold.0o">koboldo</ref>
      </refgrp>
      <trd lng="en">daemon</trd>
      <trd lng="es">demonio</trd>
      <trd lng="grc">&delta;&alfa;&jota_acute;&my;&omega;&ny;</trd>
      <trd lng="ru">&c_d;&c_je;&c_m;&c_o;&c_n;</trd>
    </drv>

  </art>

