<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dajmon.xml,v 1.19 2024/03/23 10:24:48 revo Exp $">
<kap>
  <rad>dajmon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="dajmon.0o">
   <kap><tld/>o</kap>
   <snc mrk="dajmon.0o.REL">
     <tezrad fak="REL"/>
     <uzo tip="fak">REL</uzo>
     <dif>
       Mezranga supernatura esta&jcirc;o, ofte malbona sed iam favora;
       peranto inter la homoj kaj la dioj, a&ubreve; granda dio aganta
       inkognite;
       <ref tip="sin" cel="geni.0o.dajmono">genio<sncref/></ref>,
       <ref tip="sin" cel="gxeni.0o.REL">&gcirc;enio</ref>:
      <ekz>
        adorado de Dioj kaj <tld/>oj kaj herooj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>;
      </ekz>
      <ekz>
         la persona <tld/>o de Sokrato;
      </ekz>
      <ekz>
        neniuj <tld/>oj estos donitaj al vi, vi devos mem elekti proprajn <tld/>ojn
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Dek</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="angxel.0o">an&gcirc;elo</ref>,
       <ref cel="demon.0o.diablo">demono</ref>,
       <ref cel="elf.0o">elfo</ref>,
       <ref cel="fe.0o">feo</ref>,
       <ref cel="kobold.0o">koboldo</ref>
    </refgrp>
  </snc>
  <snc mrk="dajmon.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <klr>(en uniksaj operaciumoj)</klr>
      Fona <ref tip="super" cel="procez.0o.KOMP">procezo</ref> plenumanta
      taskojn de servilo kaj dormanta la ceteran tempon:
      <ekz>
        la tempo<tld/>o <nom>cron</nom>, la presa <tld/>o <nom>lpd</nom>, la TTT-<tld/>o <nom>httpd</nom>;
      </ekz>
      <ekz>
        la <tld/>o <nom>sendmail</nom> <klr>[&#8230;]</klr> povis agi kiel ajna uzanto kies po&scirc;ton &gcirc;i
        devis administri
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=699117"
              >Retpo&scirc;ta plusendado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Daemon <klr>(Prozess)</klr></trd>
    <trd lng="en">daemon</trd>
    <trd lng="fr">daemon (informatique)</trd>
  </snc>
   <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_n;</trd>
   <trd lng="bg">&c_d;&c_je;&c_m;&c_o;&c_n;</trd>
   <trd lng="ca">d&egrave;mon</trd>
   <trd lng="cs">d&eacute;mon</trd>
   <trdgrp lng="de">
     <trd>D&auml;mon</trd>,
     <trd>D&auml;monion</trd>
   </trdgrp>
   <trd lng="en">daemon</trd>
   <trd lng="es">demonio</trd>
   <trdgrp lng="fr">
     <trd>d&eacute;mon</trd>,
     <trd>g&eacute;nie</trd>
   </trdgrp>
   <trd lng="grc">&delta;&alfa;&jota_ton;&my;&omega;&ny;</trd>
   <trdgrp lng="he">
    <trd>&dalet;&mem;&vav;&fnun;</trd>,
    <trd>&shin;&dalet;&vav;&fnun;</trd>
  </trdgrp>
  <trd lng="hu">d&eacute;mon</trd>
   <trd lng="nl">demon</trd>
   <trdgrp lng="pl">
     <trd>demon</trd>,
     <trd>dajmon<klr>{m}</klr></trd>,
     <trd>dajmona<klr>{f}</klr></trd>
   </trdgrp>
   <trd lng="pt">dem&ocirc;nio</trd>
   <trd lng="ru">&c_d;&c_je;&c_m;&c_o;&c_n;</trd>
  <trd lng="uk">(&c_o;&c_s;&c_o;&c_b;&c_i;&c_s;&c_t;&c_je;) &c_b;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#37034;&#28789; <pr>xi&eacute;l&iacute;ng</pr></trd>,
    <trd>&#31934;&#21147;&#36807;&#20154;&#30340;&#20154; <pr>j&imacron;ngl&igrave;gu&ograve;r&eacute;nder&eacute;n</pr></trd>,
    <trd>&#38750;&#20961;&#30340;&#20154; <pr>f&emacron;if&aacute;nder&eacute;n</pr></trd>,
    <trd>&#24694;&#29022; <pr>&egrave;sh&agrave;</pr></trd>,
    <trd>&#26497;&#24694;&#30340;&#20154; <pr>j&iacute;&egrave;der&eacute;n</pr></trd>,
    <trd>&#22934;&#39764; <pr>y&amacron;om&oacute;</pr></trd>,
    <trd>&#39740;&#39749; <pr>gu&#464;m&egrave;i</pr></trd>,
    <trd>&#37034;&#31070; <pr>xi&eacute;sh&eacute;n</pr></trd>,
    <trd>&#20982;&#24694;&#30340;&#21270;&#36523; <pr>xi&omacron;ng&egrave;dehu&agrave;sh&emacron;n</pr></trd>,
    <trd>&#39764; <pr>m&oacute;</pr></trd>,
    <trd>&#39764;&#39740; <pr>m&oacute;gu&#464;</pr></trd>,
    <trd>&#22934; <pr>y&amacron;o</pr></trd>,
    <trd>&#22934;&#24618; <pr>y&amacron;ogu&agrave;i</pr></trd>
  </trdgrp>
</drv>
</art>
