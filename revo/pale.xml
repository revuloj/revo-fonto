<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>pale</rad>/o
</kap>
<drv mrk="pale.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="po1.0acoj">poacoj</ref>,
      &ccirc;iu el la du <ref tip="super" cel="brakte.0o">brakteoj</ref>
      kovrantaj unuopan floron:
      <ekz>
        <ind>interna <tld/>o</ind>;
        <trd lng="de">Vorspelze</trd>
      </ekz>
      <ekz>
        la <ind>ekstera <tld/>o</ind> ofte havas ariston;
        <trd lng="de">Deckspelze</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lodikl.0o">lodiklo</ref>,
      <ref cel="glum.0o">glumo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Spelze <klr tip="ind">(Deck-, Vor-)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
