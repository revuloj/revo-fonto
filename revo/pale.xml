<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pale.xml,v 1.13 2022/03/25 10:18:30 revo Exp $">
<kap>
  <rad>pale</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pale.0o">
  <kap><tld/>o</kap>
  <snc mrk="pale.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="poac.0oj">poacoj</ref>,
      &ccirc;iu el la du <ref tip="super" cel="brakte.0o">brakteoj</ref>
      kovrantaj unuopan floron:
      <ekz>
        <ind>interna <tld/>o</ind>;
        <trd lng="de">Vorspelze</trd>
      </ekz>
      <ekz>
        la <ind>ekstera <tld/>o</ind> ofte havas ariston;
        <trd lng="de">Deckspelze</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lodikl.0o">lodiklo</ref>,
      <ref cel="glum.0o">glumo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>pal&eacute;</trd>,
    <trd>z&aacute;pas</trd>
  </trdgrp>
  <trd lng="de">Spelze<klr tip="ind"> (Deck-, Vor-)</klr></trd>
  <trd lng="fr">glumelle</trd>
  <trd lng="hu">pelyva</trd>
  <trd lng="nl">kroonkafje</trd>
  <trd lng="pl">plewa</trd>
  <trd lng="pt">p&aacute;lea</trd>
  <trdgrp lng="ro">
    <trd>glum&abreve;</trd>,
    <trd>ploaie</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_s;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_ch;&c_je;&c_sh;&c_u;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>pal&eacute;</trd>,
    <trd>z&aacute;pas</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x456;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x432;&#x456;&#x442;&#x43A;&#x43E;&#x432;&#x430; &#x43B;&#x443;&#x441;&#x43A;&#x430; &#x437;&#x43B;&#x430;&#x43A;&#x43E;&#x432;&#x438;&#x445;</trd>
  </trdgrp>
</drv>
</art>
