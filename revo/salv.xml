<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>salv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="salv.0o">
  <kap><tld/>o</kap>
  <snc mrk="salv.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Samtempa <ref tip="super" cel="paf.0ado.ARM">pafado</ref> de 
      multaj eksplodpafiloj a&ubreve; kanonoj:
      <ekz>
        la kroz&scirc;ipo, post pa&ubreve;zo, batis la horizonton per potenca
        saluta 
        <tld/>o
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url
          ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_p;</trd>
  <trd lng="de">Salve</trd>
  <trd lng="en">salvo</trd>
  <trd lng="es">salvo</trd>
  <trd lng="fr">salve</trd>
  <trd lng="hu">sort&udblac;z</trd>
  <trd lng="it">salva</trd>
  <trd lng="nl">salvo</trd>
  <trd lng="pt">salva</trd>
  <trd lng="ru">&c_z;&c_a;&c_l;&c_p;</trd>
</drv>
</art>
