<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: disip.xml,v 1.13 2025/03/29 10:30:17 revo Exp $">
<kap>
  <rad>disip</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="disip.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="disip.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="sxpar.mal0i">Mal&scirc;pari</ref>,
      dis&jcirc;eti, dissemi:
      <ekz>
        &scirc;i murmuras, ke vi <tld/>emas anstata&ubreve; <klr>[&#8230;]</klr>
        praktiki ekonomion
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p.
        352a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="perd.dis0i">disperdi</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>mal&#349;pari</ke>, <ke>forneniigi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;
    &c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_zh;&c_y;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>marnit</trd>,
    <trd>mrhat</trd>,
    <trd>ne&scaron;et&rcaron;it</trd>,
    <trd>pl&yacute;tvat</trd>,
    <trd>promarnit</trd>,
    <trd>promarnit</trd>,
    <trd>promrhat</trd>,
    <trd>rozhazovat</trd>
  </trdgrp>
  <trd lng="de">verschwenden</trd>
  <trd lng="en">waste</trd>
  <trdgrp lng="fr">
    <trd>dilapider</trd>,
    <trd>gaspiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;kozol</trd>,
    <trd>pazarol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verkwisten</trd>,
    <trd>verkwanselen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trwoni&cacute;</trd>,
    <trd>roztrwania&cacute;</trd>,
    <trd>marnowa&cacute;</trd>,
    <trd>marnotrawi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gastar</trd>,
    <trd>esbanjar</trd>,
    <trd>desperdi&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_zh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>premrha&tcaron;</trd>,
    <trd>prem&aacute;rni&tcaron;</trd>,
    <trd>rozh&aacute;dza&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">sl&ouml;sa</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#1086;&c_z;&c_t;&c_r;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#1086;&c_z;&c_t;&c_r;&c_i;&#1085;&#1100;&#1082;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1087;&c_r;&#1086;&c_c;&c_i;&#1085;&c_d;&c_r;&#1102;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28389;&#26045; <pr>l&#224;nsh&#299;</pr></trd>,
    <trd>&#31967;&#36367; <pr>z&amacron;ot&#224;</pr></trd>,
    <trd>&#38753; <pr>m&#237;</pr></trd>,
    <trd>&#31354;&#32791; <pr>k&#333;ngh&#224;o</pr></trd>,
    <trd>&#22053; <pr>s&amacron;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="disip.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="disip.0o.KOMUNE">
    <dif>
      Mal&scirc;paro, dis&jcirc;eto:
      <ekz>
        aktivo senteoria <klr>[&#8230;]</klr> estas nura blinda agado,
        <tld/>o de fortoj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Verschwendung</trd>
  <trd lng="fr">dissipation</trd>
</drv>
</art>

