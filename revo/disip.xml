<vortaro>
<art mrk="$Id$">
<kap>
  <rad>disip</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="disip.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="sxpar.mal0i">mal&scirc;pari</ref>
    <ekz>
      &scirc;i murmuras, ke vi <tld/>emas anstata&ubreve; <klr>...</klr>
      praktiki
      ekonomion
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p.
      352a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;
    &c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_zh;&c_y;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>marnit</trd>,
    <trd>mrhat</trd>,
    <trd>nešetřit</trd>,
    <trd>plýtvat</trd>,
    <trd>promarnit</trd>,
    <trd>promarnit</trd>,
    <trd>promrhat</trd>,
    <trd>rozhazovat</trd>
  </trdgrp>
  <trd lng="de">verschwenden</trd>
  <trd lng="en">waste</trd>
  <trdgrp lng="fr">
    <trd>dilapider</trd>,
    <trd>gaspiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;kozol</trd>,
    <trd>pazarol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verkwisten</trd>,
    <trd>verkwanselen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trwoni&cacute;</trd>,
    <trd>roztrwania&cacute;</trd>,
    <trd>marnowa&cacute;</trd>,
    <trd>marnotrawi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gastar</trd>,
    <trd>esbanjar</trd>,
    <trd>desperdi&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_zh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>premrhať</trd>,
    <trd>premárniť</trd>,
    <trd>rozhádzať</trd>
  </trdgrp>
  <trd lng="sv">sl&ouml;sa</trd>
</drv>
</art>

