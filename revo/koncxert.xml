<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kon&ccirc;ert</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="koncxert.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika &gcirc;enro populara en e&ubreve;ropa klasiko,
      plurparta, en kiu eta ensemblo a&ubreve; unu muzikilo
      dialogas kun plena orkestro.
    </dif>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="vid" cel="koncer1.0o">koncerto</ref>.
  </rim>
  <trdgrp lng="de">
     <trd>Concerto</trd>,
     <trd>Komposition f&uuml;r Solopartien mit Orchesterbegleitung</trd>
  </trdgrp>
  <trd lng="fr">concerto</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t; <klr tip="amb">(&c_m;&c_u;&c_z;. &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
