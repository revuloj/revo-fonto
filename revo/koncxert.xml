<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koncxert.xml,v 1.11 2018/06/28 22:10:18 revo Exp $">
<kap>
  <rad>kon&ccirc;ert</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="koncxert.0o">
  <kap><tld/>o</kap>
  <snc mrk="koncxert.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Plurparta <ref tip="super" cel="kompon.0ajxo">kompona&jcirc;o</ref>
      en kiu eta ensemblo a&ubreve; unu muzikilo
      dialogas kun plena muzikilaro:
      <ekz>
        tria movimento de la <ctl><tld lit="K"/>o por Marimbo kaj
        Orkestro</ctl>
        <fnt>
        <aut>F. Fonseca</aut>,
        <vrk><url
        ref="http://www.musicexpress.com.br/artisto.asp?Artista=71">Ney
        Rosauro</url></vrk>,
        <lok>MusicExpress, [vidita je 2006-06-14]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="vid" cel="koncer1.0o">koncerto</ref>.
  </rim>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t; <klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_t;&c_v;&c_o;&c_r;)</klr></trd>
  <trdgrp lng="de">
    <trd>Concerto</trd>,
    <trd>Komposition f&uuml;r Solopartien mit Orchesterbegleitung</trd>
  </trdgrp>
  <trd lng="ca">concert<klr> (per a un o m&eacute;s solistes i
  orquestra)</klr></trd>
  <trd lng="fr">concerto</trd>
  <trd lng="nl">concerto</trd>
  <trd lng="pl">koncert <klr>(forma muzyczna)</klr></trd>
  <trd lng="pt">concerto <klr tip="amb">(pe&ccedil;a musical)</klr></trd>
  <trd lng="ro">concert <klr>(compozi&#539;ie muzical&abreve;)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t; <klr tip="amb">(&c_m;&c_u;&c_z;. &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
