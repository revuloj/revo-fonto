</drv>

<drv mrk="konsid.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="konsid.0inda.KOMUNE">
    <dif>
      Kiun indas <tld/>i, ne preteratenti:
      <ekz>
        <tld/>indaj demandoj
        <fnt>
          <aut>S. Petrovi&cacute;</aut>,
          <vrk><url ref="http://artista.ikso.net/konsilo/subven.php">
          Konsiloj pri subvenciakirado por artaj / kulturaj
          aran&gcirc;oj</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni anka&ubreve; malfermadis ilin por forveturantoj, kies nombro estis kelkfoje
        <tld/>inde granda
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la de vi dirita estas ekster <tld/>indeco
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="konsid.0inda.kiel">
    <dif>
      Kiun indas <tld/>i kiel ...::
      <ekz>
        la espero, ke oni malkovros aliajn dokumentojn, estis nun <tld/>inda kiel
        &hcirc;imera&jcirc;o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la medio, en kiu ili vivas, estas <tld/>inda kiel identa&jcirc;o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; <ind>&c_r;&c_a;&c_z;&c_g;&c_l;&c_ja;&c_d;&c_u;</ind></trd>
