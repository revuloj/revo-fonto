<drv mrk="konsid.sen0eal">
  <kap>sen<tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="konsid.sen0eal.KOMUNE">
    <dif>
      Ne <tld/>ante,
      <ref tip="vid" cel="indife.0eal.KOMUNE">indiferente al</ref>,
      <ref tip="vid" cel="respek.sen0eal.KOMUNE">senrespekte al</ref>:
      <ekz>
        tute sen<tld/>e al la demando &ccirc;u <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiel <klr>[&#x2026;]</klr> la pronomo <em>li</em>  personigas  la
        homecon  en  opozicio  al  la  nehomeco  sen<tld/>e  al  la
        sekso
        <fnt>
          <aut>C. Gacond</aut>,
          <vrk><url ref="http://www.esperanto-gacond.ch/Pri_la_personaj_pronomoj.pdf">
          Pri la personaj pronomoj</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja; <ind>&c_w;&c_l;&c_ib;&c_ch;&c_v;&c_a;&c_ju;&c_ch;&c_y;</ind>&#x2026;</trd>,
