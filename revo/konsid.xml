<drv mrk="konsid.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="konsid.0ema.PSI">
    <dif>
      Kiu inklinas <tld/>i &ccirc;iujn eventuala&jcirc;ojn:
      <ekz>
        &leftquot;mi timas ke li malvarmumos pro ku&scirc;ado sur la malseka herbo,&rightquot; diris Alico, kiu
        estis tre <tld/>ema knabino
        <fnt>
          <aut>Lewis Carrol, trad. Donald Richard Broadribb</aut>
          <vrk><url ref="konsiderema">Trans la spegulo kaj kion Alico trovis tie</url></vrk>
          <lok>&ccirc;ap. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Majstro <klr>[&#x2026;]</klr> inspiris min senti, pensi, esprimi min, esti
        <tld/>ema, teni altajn idealojn
        <fnt>
          <bib>Kon11</bib>
          <aut>Ralph Dumain</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=kontakto-2011-2019&amp;sekcio=kontakto-471">Voja&gcirc;o al hungara literaturo, tereno nekonata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nur bone informita civitano povas anka&ubreve; esti politike <tld/>ema
        <fnt>
          <lok><url ref="https://www.radio-frei.de/index.php?kartikel_id=5222">Radio F.R.E.I., Turingio Internacia 45</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="anim.facil0a">facilanima</ref>,
      <ref cel="risk.0ema">riskema</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pruden.0a">prudenta</ref>,
      <ref cel="zorg.0ema">zorgema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">uv&aacute;&zcaron;liv&yacute;</trd>
  <trdgrp lng="de">
    <trd>bedacht</trd>,
    <trd>bed&auml;chtig</trd>
    <trd>besonnen</trd>,
    <trd>umsichtig</trd>,
    <trd>&uuml;berlegt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>considerate</trd>,
    <trd>reflective</trd>,
    <trd>thoughtful</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>razonable<klr> (persona)</klr></trd>,
    <trd>reflexivo<klr> (persona)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raisonnable<klr> (personne)</klr></trd>,
    <trd>r&eacute;fl&eacute;chi<klr> (personne)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x601D;&#x616E;&#x6DF1;&#x3044;  <pr>&#x3057;&#x308A;&#x3087;&#x3076;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x3088;&#x304F;&#x8003;&#x3048;&#x308B;  <pr>&#x3088;&#x304F;&#x304B;&#x3093;&#x304C;&#x3048;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bedachtzaam</trd>
  <trd lng="pl">rozwa&zdot;ny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_d;&c_u;&c_m;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">uv&aacute;&zcaron;liv&yacute;</trd>
  <trdgrp lng="zh">
