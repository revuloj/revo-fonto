        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>adis tiun &ccirc;i ringon <klr>[&#8230;]</klr> kiel ringon de fian&ccirc;i&gcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
