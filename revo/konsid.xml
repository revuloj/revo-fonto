  </trdgrp>
  <trdgrp lng="es">
    <trd>digno de tener en consideraci&oacute;n</trd>,
    <trd>considerable</trd>
  </trdgrp>
  <trd lng="fr">consid&eacute;rable</trd>
  <trdgrp lng="ja">
