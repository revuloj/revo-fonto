  <trd lng="jw">nimbang</trd>
  <trd lng="ka">&#x10D2;&#x10D0;&#x10DC;&#x10D8;&#x10EE;&#x10D8;&#x10DA;&#x10DD;&#x10E1;</trd>
  <trd lng="km">&#x1796;&#x17B7;&#x1785;&#x17B6;&#x179A;&#x178E;&#x17B6;</trd>
  <trd lng="ko">&#xACE0;&#xB824;</trd>
  <trd lng="lo">&#xE9E;&#xEB4;&#xE88;&#xEB2;&#xEA5;&#xEB0;&#xE99;&#xEB2;</trd>
  <trd lng="ml">&#xD2A;&#xD30;&#xD3F;&#xD17;&#xD23;&#xD3F;&#xD15;&#xD4D;&#xD15;&#xD41;&#xD15;</trd>
  <trd lng="mr">&#x935;&#x93F;&#x91A;&#x93E;&#x930;</trd>
  <trd lng="my">&#x1005;&#x1009;&#x103A;&#x1038;&#x1005;&#x102C;&#x1038;&#x1015;&#x102B;</trd>
  <trd lng="ne">&#x935;&#x93F;&#x91A;&#x93E;&#x930;</trd>
  <trd lng="ny">tione</trd>
  <trd lng="pa">&#xA24;&#xA47; &#xA35;&#xA3F;&#xA1A;&#xA3E;&#xA30;</trd>
  <trd lng="ps">&#x67E;&#x647; &#x67E;&#x627;&#x645; &#x6A9;&#x6D0;</trd>
  <trdgrp lng="pt">
    <trd>considerar</trd>,
    <trd>refletir</trd>,
    <trd>reputarr</trd>,
    <trd>encarar</trd>,
    <trd>atentar</trd>,
    <trd>ter em <ind>vista</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_mol; &c_v;&c_o;
    &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <klr>(&c_k;&c_a;&c_k;
    &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trd lng="sd">&#x63A;&#x648;&#x631;</trd>
