<drv mrk="konsid.0e_al">
  <kap><tld/>e al,
  <var><kap><tld/>e ...-n</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="konsid.0e_al.KOMUNE">
    <dif>
      Rigarde al,
      <ref tip="vid" cel="vid.0al0e_al">vidalvide al</ref>:
      <ekz>
        <tld/>e la zorgojn, kiuj &scirc;in &ccirc;irka&ubreve;is <klr>[&#x2026;]</klr>
        la kvanto kaj eco de la man&gcirc;o, kiu devis subteni &scirc;ian vivon,
        ne povis okupi grandan lokon en &scirc;iaj pensoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li fakte kondukis min al iu <klr>[&#x2026;]</klr>, kiu, <tld/>e al la persono de
        Zemoki, akceptis min tre afable
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_d;&c_a;&c_ch;&c_y;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_ib;</ind> &c_d;&c_a;&#x2026;</trd>,
    <trd>&c_sh;&c_t;&c_o; &c_d;&c_a;&#x2026;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unter dem <ind>Gesichtspunkt</ind>, dass</trd>,
    <trd>unter <ind>Beachtung</ind> von</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>en consideraci&oacute;n a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en consid&eacute;ration de</trd>,
    <trd>eu &eacute;gard &agrave;</trd>,
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9274;&#x4E8E; <pr>ji&#xE0;ny&#xFA;</pr></trd>,
    <trd>&#x4F86;&#x8B1B; <pr>l&#xE1;iji&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konsid.re0i">
  <kap>
    re<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>i denove:
      <ekz>
        montri&gcirc;is utile re<tld/>i la demandon por konstati, &ccirc;u la nuna koncepto de
        ne&ubreve;traleco de UEA estas adekvata
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la indikitaj rezonoj la prokuratoro petas la &ccirc;efpastron re<tld/>i la decidon
        kaj el la du kondamnitoj liberigi la malpli dan&gcirc;eran
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la juna vidvino re<tld/>is sian sesjaran edzinecon, &scirc;i apena&ubreve; e&ccirc; por si mem
        povis ju&gcirc;i, &ccirc;u &gcirc;i estis feli&ccirc;a a&ubreve; malfeli&ccirc;a
        <fnt>
          <bib>Kiso</bib>
          <lok>&Aring;GE</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rigard.re0i.tempon">rerigardi<sncref/></ref>,
      <ref cel="retros.0o">retrospektivi</ref>,
      <ref cel="revizi.0i">revizii</ref>
    </refgrp>
  </snc>
  <trd lng="de">neu <ind>betrachten</ind></trd>
  <trd lng="en">review</trd>
  <trdgrp lng="zh">
    <trd>&#x8986;&#x6838; <pr>f&#xF9;h&#xE9;</pr></trd>,
    <trd>&#x65B0;&#x4FEE; <pr>x&#x12B;nxi&#x16B;</pr></trd>,
