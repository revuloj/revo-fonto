<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mops.xml,v 1.19 2024/01/26 19:05:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mops</rad>/o
</kap>
<drv mrk="mops.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malgranda mallonghara
      <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      kun platdika nazo, subraso de <ref tip="super" cel="dog.0o">dogo</ref>:
      <ekz>
        kiel modelon li havis blankan mallonge pritonditan <tld/>on, sed la besto, kiu
        ne volis stari trankvile, pro tio estis alligita je la kapo kaj la vosto per
        &scirc;nuretoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i transdonis al ni siajn paperojn kaj prenis la <tld/>on sur la brakon
        <fnt>
          <bib>Fab3</bib>
          <lok>Kora sufero</lok>
        </fnt>;
      </ekz>
      <ekz>
        bufo, kiu bojis kiel astma <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="buldog.0o">buldogo</ref>
  <trd lng="be">&c_m;&c_o;&c_p;&c_s;</trd>
  <trd lng="de">Mops</trd>
  <trdgrp lng="en">
    <trd>pug</trd>,
    <trd>pug-dog</trd>
  </trdgrp>
  <trd lng="it">carlino<klr> (zoo)</klr></trd>
  <trd lng="ja">&#x30D1;&#x30B0;</trd>
  <trd lng="nl">mops</trd>
  <trd lng="pl">mops</trd>
  <trd lng="pt">dogue</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_p;&c_s;</trd>,
    <trd>&c_m;&c_o;&c_s;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mopsl&iacute;k</trd>
  <trdgrp lng="cs">
    <trd>mops</trd>,
    <trd>mopsl</trd>,
    <trd>mopsl&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x43E;&#x43F;&#x441; (&#x441;&#x43E;&#x431;&#x430;&#x43A;)</trd>
  <trdgrp lng="zh">
    <trd>&#x516B;&#x54E5;&#x72D7; <pr>b&#x101;g&#x113;g&#x1D2;u</pr></trd>,
    <trd>&#x5DF4;&#x54E5; <pr>b&#x101;g&#x113;</pr></trd>,
    <trd>&#x5DF4;&#x54E5;&#x72AC; <pr>b&#x101;g&#x113;qu&#x1CE;n</pr></trd>,
    <trd>&#x54C8;&#x5DF4;&#x72D7; <pr>h&#x1CE;bag&#x1D2;u</pr></trd>
  </trdgrp>
</drv>
</art>
