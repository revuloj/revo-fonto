<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mops</rad>/o
</kap>
<drv mrk="mops.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malgranda mallonghara
      <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      kun platdika nazo, subraso de
      <ref tip="super" cel="dog.0o">dogo</ref>.
    </dif>
  </snc>
  <ref tip="vid" cel="buldog.0o">buldogo</ref>
  <trd lng="be">&c_m;&c_o;&c_p;&c_s;</trd>
  <trd lng="de">Mops</trd>
  <trdgrp lng="en">
    <trd>pug</trd>,
    <trd>pug-dog</trd>
  </trdgrp>
  <trd lng="nl">mops</trd>
  <trd lng="pt">dogue</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_p;&c_s;</trd>,
    <trd>&c_m;&c_o;&c_s;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
