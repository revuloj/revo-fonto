<vortaro>
<art mrk="$Id: ortoped.xml,v 1.5 2022/08/30 09:32:33 revo Exp $">
<kap>
    <rad>ortoped</rad>/o
</kap>
<drv mrk="ortoped.0o">
  <kap><tld/>o</kap>
  <snc mrk="ortoped.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="ortope.0isto.MED">Ortopediisto</ref>:
      <ekz>
        la <tld/>o plusendis min al specialisto &ccirc;e la 
        granda malsanulejo de <nom>Samsung</nom>
        <fnt>
          <aut>B. Wennergren</aut>,
          <vrk><url ref="http://de.bertilow.com/blogo/2007/10/lambastonoj/">
          Lambastonoj</url></vrk>,
          <lok>BBBlogo, 2007-10-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia <tld/>o jam diris al mi anta&ubreve; pluraj jaroj:
        &leftquot;kun tio vi devas vivi&rightquot;
        <fnt>
          <aut>M. Kamp</aut>,
          <vrk><url ref="http://www.bruno-groening.org/esperanto/mwf/erfahrungsbericht.htm">
          Spertoraporto de kuracisto</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_a;&c_p;&c_e;&c_d;</trd>
  <trd lng="de">Orthop&auml;de</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n;&zwnj;&f_p;&a_z;&a_sh;&f_k;</trd>,
    <trd>&a_A;&a_damma;&a_r;&a_t;&a_w;&f_p;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">orthop&eacute;diste</trd>
  <trd lng="it">ortopedico<klr> (sost.)</klr></trd>
  <trdgrp lng="nl">
    <trd>orthopedist</trd>,
    <trd>orthopeed</trd>
  </trdgrp>
  <trd lng="pl">ortopeda</trd>
  <trd lng="ro">ortoped</trd>
</drv>
</art>

