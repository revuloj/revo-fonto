<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&hcirc;alif</rad>/o
</kap>
<drv mrk="hxalif.0o">
  <kap><tld/>o</kap>
  <snc mrk="hxalif.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <ref tip="dif" cel="kalif.0o.ISL">kalifo</ref>
    <ekz>
      <tld/>o mamuno 11 sendis al imperiestro teofilo <klr>...</klr>
      leteron
      <fnt><aut>Sergio Pokrovskij</aut>, <vrk>slavonaj skriboj</vrk>,
      <bib>LOdE</bib>, <lok>numero 51a, 1999:1a</lok></fnt>.
    </ekz>
  </snc>
</drv>

<drv mrk="hxalif.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="hxalif.0ujo.ISL">
    <uzo tip="fak">ISL</uzo>
    <ref tip="dif" cel="kalif.0ujo.ISL">kalifujo</ref>
  </snc>
</drv>
</art>

