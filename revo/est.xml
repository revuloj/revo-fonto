  <trd lng="fr">pass&eacute;<klr tip="ind"> (temps)</klr></trd>
  <trd lng="hu">m&uacute;lt<klr tip="ind"> (id&odblac;)</klr></trd>
  <trd lng="it">passato, il tempo che e' stato</trd>
  <trd lng="nl">verleden <ind>tijd</ind></trd>
  <trd lng="pl">przesz&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ro">trecut</trd>
