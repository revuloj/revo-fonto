  <trd lng="ca">futur</trd>
  <trd lng="de">Zukunft</trd>
  <trd lng="es">futuro</trd>
  <trd lng="fr">futur<klr tip="ind"> (temps)</klr></trd>
  <trd lng="hu">j;ouml;v&odblac;<klr tip="ind"> (id&odblac;)</klr></trd>
  <trd lng="it">futuro, il tempo che sara'</trd>
  <trd lng="nl">toekomende <ind>tijd</ind></trd>
  <trd lng="pl">przysz&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ro">viitor</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_u;&c_shch;&c_je;&c_je;
  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>

<drv mrk="est.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi, formi, produkti.
    </dif>
    <refgrp tip="vid">
      <ref cel="nask.0i">naski</ref>,
      <ref cel="port.al0i">alporti</ref>,
      <ref cel="vok.al0i">alvoki</ref>,
      <ref cel="vek.0i">veki</ref>,
      <ref cel="kre.0i">krei</ref>,
      <ref cel="ven">venigi</ref>,
      <ref cel="okaz.0igi">okazigi</ref>,
      <ref cel="kauxz.0i">ka&ubreve;zi</ref>,
      <ref cel="produk.0i">produkti</ref>,
      <ref cel="star.0igi">starigi</ref>,
      <ref cel="teks.0i">teksi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;
<klr>(&c_d;&c_a;
&c_ib;&c_s;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>causar</trd>,
    <trd>ocasionar</trd>,
    <trd>provocar</trd>,
    <trd>suscitar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hervorrufen</trd>,
    <trd>herstellen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>causer<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>cr&eacute;er<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>faire</trd>,
    <trd>faire na&icirc;tre</trd>,
    <trd>g&eacute;n&eacute;rer</trd>,
    <trd>occasionner</trd>,
    <trd>susciter<klr tip="ind"> (occasionner)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&eacute;tes&iacute;t</trd>,
    <trd>l&eacute;trehoz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>causare<klr tip="ind">provocare, cagionare</klr></trd>,
    <trd>creare<klr tip="ind">suscitare</klr></trd>
    <trd>generare<klr tip="ind">produrre</klr></trd>
  </trdgrp>
  <trd lng="nl">veroorzaken</trd>
  <trd lng="pl">powodowa&cacute; <ind>powstanie</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; &c_k;
&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="est.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i ekzistanta:
      <ekz>
        mi e&ccirc; ne rimarkis, ke silento <tld/>i&gcirc;as kaj
da&ubreve;ras
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">esdevenir</trd>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>produire</ind></trd>,
    <trd>survenir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&eacute;tes&uuml;l</trd>,
    <trd>l&eacute;trej&ouml;n</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>generarsi</trd>,
    <trd>formarsi</trd>
    <trd>sorgere</trd>
  </trdgrp>
  <trd lng="nl">worden</trd>
  <trdgrp lng="pl">
    <trd>wywi&aogonek;za&cacute; si&eogonek;</trd>,
    <trd>zaistnie&cacute;</trd>
    <trd>zostawa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>

<drv mrk="est.al0i">
  <kap>al<tld/>i, apud<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="est.cxe0i">&ccirc;eesti</ref>
  <rim>
    La direkta <em>al-</em> neniel ta&ubreve;gas por la senco
    &leftquot;&ccirc;e<tld/>i&rightquot;, kaj povis aperi nur pro
misapliko de alilingva
    tradicio.
    &leftquot;Apud<tld/>i&rightquot; formale estus tute ta&ubreve;ga,
sed por pli flanka,
    periferia situo.
    <aut>Sergio Pokrovskij</aut>
  </rim>
</drv>

<drv mrk="est.cxe0i">
  <kap>&ccirc;e<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <tld lit="E"/>i en iu loko, kiam io okazas:
      <ekz>
        &ccirc;e<tld/>i kunvenon;
      </ekz>
      <ekz>
        &ccirc;e<tld/>i en, &ccirc;e kunveno;
      </ekz>
      <ekz>
        en <ind>&ccirc;e<tld/>o</ind> amata, en for<tld/>' insultata;
        <trd
lng="be">&c_p;&c_r;&c_y;&c_s;&c_u;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
        <trdgrp lng="fr">
          <trd>assistance<klr tip="ind"> (fait d'assister)</klr></trd>,
          <trd>pr&eacute;sence</trd>
        </trdgrp>
        <trd lng="hu">jelenl&eacute;t</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        multnombra <ind>&ccirc;e<tld/>antaro</ind>.
        <trd lng="fr">assistance<klr tip="ind"> (personnes)</klr></trd>
        <trd lng="hu">jelenl&eacute;v&odblac;k</trd>
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_p;&c_r;&c_y;&c_s;&c_u;&c_t;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ser present</trd>
  <trd lng="de">dabei sein</trd>
  <trdgrp lng="fr">
    <trd>assister (&agrave;)</trd>,
    <trd>&ecirc;tre pr&eacute;sent</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jelen van</trd>,
    <trd>ott van</trd>
  </trdgrp>
    <trdgrp lng="it">
    <trd>presenziare</trd>,
    <trd>partecipare</trd>
    <trd>assistere</trd>
    <trd>intervenire</trd>
  </trdgrp>
  <trd lng="nl">aanwezig zijn</trd>
  <trd lng="pl">by&cacute; obecnym</trd>
  <trd
lng="ru">&c_p;&c_r;&c_i;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="est.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Komenci <tld/>i,
      da&ubreve;re (<ref tip="sin" cel="est.0igxi"><tld/>i&gcirc;i</ref>)
      a&ubreve; nur por nelonga tempo:
      <ekz>
        por unu minuto ek<tld/>is embarasa silento
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        li anka&ubreve; devis demandi, kiel Komenio kondutu en la situacio,
        kiu ek<tld/>is post la duelo kun Nigrin
        <fnt>
        <aut>F. Ko&zcaron;&iacute;k, trad. Z. Rus&iacute;n kaj M.
Malovec</aut>,
        <vrk>Lumo en tenebroj</vrk>.
        </fnt>;
      </ekz>
      <ekz>
        la problemo estas kompreneble solvita <ctl>itisme</ctl>,
        &ccirc;ar la lingvo &gcirc;in tiel solvis jam anta&ubreve; ol la
fama
        disputo ek<tld/>is
        <fnt>
        <aut>J. Lindstedt</aut>,
        <vrk>Recenza eseo pri PAG</vrk>,
        <lok>Planlingvistiko 1 (1982)</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">compar&egrave;ixer</trd>
  <trd lng="fr">survenir</trd>
  <trdgrp lng="hu">
    <trd>be&aacute;ll<klr tip="ind"> (hirtelen)</klr></trd>,
    <trd>lenni kezd</trd>
  </trdgrp>
  <trd lng="pl">zaistnie&cacute; <klr>(na kr&oacute;tko)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>

<drv mrk="est.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="E"/>i malproksima, ne &ccirc;e<tld/>i:
      <ekz>
        mi for<tld/>is dum du tagoj;
      </ekz>
      <ekz>
        for<tld/>i el oficejo;
      </ekz>
      <ekz>
        <tld/>i kondamnita for<tld/>ante;
      </ekz>
      <ekz>
        <ind>for<tld/>o</ind> de ofendo estas plej bona defendo
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">absence</trd>
        <trd lng="hu">t&aacute;voll&eacute;t</trd>
      </ekz>
      <ekz>
        la <ind>for<tld/>anto</ind> &ccirc;iam estas malprava
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">absent<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="hu">t&aacute;voll&eacute;v&odblac;</trd>
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_a;&c_d;&c_s;&c_u;&c_t;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;
<klr tip="amb">(&c_b;&c_y;&c_c;&c_mol; &c_u; &c_ib;&c_n;&c_sh;&c_y;&c_m;
  &c_m;&c_je;&c_s;&c_c;&c_y;)</klr></trd>
  <trd lng="ca">ser absent</trd>
  <trd lng="de">abwesend sein</trd>
  <trd lng="es">estar ausente</trd>
  <trd lng="nl">afwezig zijn</trd>
  <trd lng="fr">&ecirc;tre <ind>absent</ind></trd>
  <trd lng="hu">t&aacute;vol van</trd>
  <trd lng="it">assentarsi</trd>
  <trd lng="nl">afwezig zijn</trd>
  <trd lng="pl">by&cacute; nieobecnym</trd>
  <trd
lng="ru">&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr 

tip="amb"> (&c_b;&c_y;&c_t;&c_mol; &c_v; &c_d;&c_r;&c_u;&c_g;&c_o;&c_m;
  &c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
</drv>

<drv mrk="est.for0o">
  <kap>for<tld/>o</kap>
  <snc>
    <dif>
      Stato <tld/>i malproksima, ne &ccirc;e<tld/>i.
    </dif>
  </snc>
  <trd lng="de">Abwesenheit</trd>
  <trd lng="pl">nieobecno&sacute;&cacute;</trd>
  <trdgrp lng="ro">
    <trd>absen&#x163;&#x103;</trd>,
    <trd>lips&#x103;</trd>
  </trdgrp>
</drv>

<drv mrk="est.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ne <ref tip="ant" cel="ekzist.0i">ekzisti</ref>,
      <ref tip="sin" cel="mank.0i">manki</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_s;&c_u;&c_t;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;
<ind>&c_ib;&c_s;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>manca</trd>,
    <trd>falta</trd>
  </trdgrp>
  <trd lng="de">fehlen</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>d&eacute;faut</ind></trd>,
    <trd>manquer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nincs</trd>,
    <trd>nem <ind>l&eacute;tezik</ind></trd>
  </trdgrp>
  <trd lng="nl">ontbreken</trd>
  <trd lng="pl">nie <ind>istnie&cacute;</ind></trd>
  <trd
lng="ru">&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr 

tip="amb"> (&c_n;&c_je;
&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="est.ne0o">
  <kap>ne<tld/>o</kap>
  <snc>
    <dif>
      Neekzisto:
      <ekz>
        kontra&ubreve; ne<tld/>o ne helpas
protesto<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_b;&c_y;&c_t;</trd>
  <trd lng="ca">inexist&egrave;ncia</trd>
  <trd lng="fr">inexistence</trd>
  <trd lng="es">inexistencia</trd>
  <trd lng="de">Nichtsein</trd>
  <trdgrp lng="hu">
    <trd>hi&aacute;ny</trd>,
    <trd>neml&eacute;t</trd>
  </trdgrp>
  <trd lng="pl">nieistnienie</trd>
  <trd lng="ru">&c_n;&c_je;&c_b;&c_y;&c_t;&c_i;&c_je;</trd>
</drv>

<drv mrk="est.tio0as">
  <kap>tio <tld/>as</kap>
  <mlg>t. e.</mlg>
  <snc>
    <dif>
      Esprima&jcirc;o ordinare uzata adverbe, samsignife
      kiel <ref tip="sin" cel="nom.0e.te">nome<sncref/></ref>:
      <ekz>
        la simpla esperantisteco, t.e. la simpla lernado kaj
        uzado de Esperanto, neniun devigas ali&gcirc;i al ia
        ideo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la akuzativo montras direkton,
        t.e. movadon al ia loko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        por la hodia&ubreve;a tago mi ricevis duoblan pagon, t.e. dek
        frankojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        por esprimi direkton ni aldonas la
        fina&jcirc;on n, t.e. la fina&jcirc;o, kiu signas la akuzativon.
      </ekz>
    </dif>
    <ref tip="vid" cel="vort.ali0e">alivorte</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>es a <ind>dir</ind> <klr>(formal i coloq.)</klr></trd>,
    <trd>o sigui<klr> (coloq.)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>es <ind>decir</ind> <klr>(formal y coloq.)</klr></trd>,
    <trd>o sea<klr> (coloq.)</klr></trd>
  </trdgrp>
  <trd lng="fr">c'<ind>est-&agrave;-dire</ind></trd>
  <trdgrp lng="hu">
    <trd>azaz</trd>,
    <trd>nevezetesen</trd>
  </trdgrp>
  <trd lng="pl">to jest <klr>(tj.)</klr></trd>
  <trd lng="ru">&c_t;&c_o; &c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="de">das hei&szlig;t <klr>d.h.</klr></trd>
</drv>
<adm>
