<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
    <ofc>*</ofc>
    <rad>est</rad>/i
    </kap>

    <drv mrk="est.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="est.0i.ekzisti">
    	<dif>
    	  <ref tip="sin" cel="ekzist.0i">Ekzisti</ref>:
    	  <ekz>
    	    mi pensas, do mi <tld/>as;
    	  </ekz>
    	  <ekz>
    	    Dio diris: <tld/>u lumo
    	    <fnt><bib>MT</bib>, <lok>&Gen; 1:3.</lok>)</fnt>;
    	  </ekz>
    	  <ekz>
    	    la homo konsideru tion, kio li <tld/>as, rilate al tio, kio
    	    <tld/>as;
    	  </ekz>
    	  <ekz>
    	    sen regalo ne <tld/>as balo<fnt><bib>PrV</bib></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <snc mrk="est.0i.ie">
    	<dif>
    	  Trovi&gcirc;i (en loko a&ubreve; en situacio):
    	  <ekz>
    	    <tld/>i en urbo, en dubo, en embaraso;
    	  </ekz>
    	  <ekz>
    	    neniu <tld/>is<fnt>Z</fnt> <klr>(&ccirc;eestis)</klr>.
    	  </ekz>
    	</dif>
      </snc>
      <snc mrk="est.0i.io">
    	<dif>
    	  Havi tiun econ, havi tiun identecon, esti de
    	  tiu speco, kiun esprimas la predikativo:
    	  <ekz>
    	    ri&ccirc;eco <tld/>as frato de fiereco<fnt><bib>PrV</bib></fnt>;
    	  </ekz>
    	  <ekz>
    	    pri havo najbara oni <tld/>as malavara<fnt><bib>PrV</bib></fnt>;
    	  </ekz>
    	  <ekz>
    	    <tld/>is silento en kampoj<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    kio <tld/>as <klr>(okazas)</klr> al vi?<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    <tld/>is vespero<fnt>Z</fnt>.
    	  </ekz>
    	</dif>
    	<rim>
    	  Specialan sencon havas la esprimo
          <ref cel="est.tio0as">tio <tld/>as</ref>.
    	</rim>
      </snc>
      <snc>
    	<dif>
    	  Stati (= fari a&ubreve; ricevi agon) tiel, kiel esprimas
    	  la predikativa participo:
    	  <ekz>
    	    <tld/>i amata;
    	  </ekz>
    	  <ekz>
    	    vi <tld/>os punata
    	    <fnt>(<bib>MT</bib>, <lok>&Jes; 29:6.</lok>)</fnt>;
    	  </ekz>
    	  <ekz>
    	    la&ubreve; la projekto de la in&gcirc;enieroj tiu &ccirc;i
    	    fervojo <tld/>as konstruota en la da&ubreve;ro de du jaroj, sed mi
    	    pensas, ke &gcirc;i <tld/>os konstruata pli ol tri jarojn<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    &ccirc;iuj jam de longe <tld/>is foririntaj, kiam mi alvenis.
    	  </ekz>
    	</dif>
      </snc>
      <snc>
    	<dif>
    	  Stati tiel, kiel esprimas la adverba predikativo
    	  (uzata en sensubjektaj frazoj por esprimi ies impreson):
    	  <ekz>
    	    &ccirc;ie <tld/>as varme, sed hejme plej
    	    &ccirc;arme<fnt><bib>PrV</bib></fnt>;
    	  </ekz>
    	  <ekz>
    	    <tld/>as al mi agrable revidi vin;
    	  </ekz>
    	  <ekz>
    	    <tld/>as al mi bone, komforte tie &ccirc;i;
    	  </ekz>
    	  <ekz>
    	    kiel <tld/>as al vi?
    	    <klr>(= 1 kiel vi fartas? 2 en kia stato vi <tld/>as?)</klr>;
    	  </ekz>
    	  <ekz>
    	    <tld/>is al &scirc;i kiel se &scirc;i sentus, ke ...
    	    <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="de">sein</trd>
      <trd lng="en">is</trd>
      <trdgrp lng="es">
    	<trd>ser</trd>,
    	<trd>estar</trd>
      </trdgrp>
      <trdgrp lng="fr">
    	<trd>&ecirc;tre</trd>,
    	<trd>faire</trd>,
    	<trd>avoir<klr tip="amb"> (il y a)</klr></trd>
      </trdgrp>
      <trd lng="nl">zijn<klr tip="ind"> (werkw.)</klr></trd>
      <trd lng="ru">&c_b;&c_y;&c_t;&c_mol;</trd>
      <trd lng="vo">bin&ouml;n</trd>
    </drv>

    <drv mrk="est.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="est.0ajxo.ento">
    	<dif>
    	  Io, kio iel ekzistas:
    	  <ekz>
    	    &ccirc;iu viva <tld/>a&jcirc;o
    	    <fnt><bib>MT</bib>, <lok>&Gen; 9:12.</lok></fnt>;
    	  </ekz>
    	  <ekz>
    	    la komunaj nomoj distingas certan specon de objektoj kaj
    	    <tld/>a&jcirc;oj (lampo, libro, floro, pomo, se&gcirc;o, homo, amo),
    	    dum la propraj nomoj indikas unu solan, certan objekton,
    	    lokon a&ubreve; <tld/>a&jcirc;on kaj diferencigas &gcirc;in de
    	    &ccirc;iuj aliaj
    	    el la sama speco
    	    <fnt><bib>LtE</bib>, <lok>Leciono 1a, &para;10.</lok></fnt>.
    	  </ekz>
    	</dif>
    	<ref tip="sin" cel="ent.0o">ento</ref>
    	<trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t;
    	<klr tip="amb">(&c_o;&c_b;&c_malmol;&c_je;&c_k;&c_t;)</klr></trd>
      </snc>
      <snc mrk="est.0ajxo.ulo">
    	<dif>
    	  <ref tip="dif" cel="est.0ulo"><tld lit="E"/>ulo</ref>.
    	</dif>
    	<rim>
    	  Tiu senco de la vorto ne koheras kun ties vortelementoj
    	  kaj aperas precipe en tradukoj.
    	  <aut>Sergio Pokrovskij</aut>
    	</rim>
    	<trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;
    	<klr>(&c_zh;&c_i;&c_v;&c_o;&c_je;)</klr></trd>
      </snc>
      <trdgrp lng="de">
    	<trd>Wesen</trd>,
    	<trd>Gesch&ouml;pf</trd>
      </trdgrp>
      <trd lng="fr">&ecirc;tre</trd>
      <trd lng="nl">wezen</trd>
    </drv>

    <drv mrk="est.0ulo">
      <kap><tld/>ulo</kap>
      <snc>
    	<dif>
    	  Ula <ref cel="est.0ajxo.ento"><tld/>a&jcirc;o<sncref/></ref>,
    	  ekzistanta homo, besto, a&ubreve; spirito:
    	  <ekz>
    	    <klr>[la vorto]</klr> <em>feo</em>, kiun oni povus uzi,
    	    por montri fab<klr>[e]</klr>lan <tld/>ulon, ekz-e Oberono,
    	    kaj kiun oni povas devenigi de
    	    <klr>[la vorto]</klr> <em>feino</em>
    	    <fnt>
    	    <aut>Emile Boirac</aut>:
    	    <vrk>Pri la deveno de la vorto &leftquot;edzo&rightquot;</vrk>
    	    <lok>
    	    Oficiala Gazeto, 1913, Decembro
    	    (eniris la Lingvajn Respondojn).
    	    </lok>
    	    </fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="de">Lebewesen</trd>
      <trd lng="fr">&ecirc;tre</trd>
      <trd lng="nl">levend <ind>wezen</ind></trd>
      <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;
      <klr>(&c_zh;&c_i;&c_v;&c_o;&c_je;)</klr></trd>
    </drv>

    <drv mrk="est.0anteco">
      <kap><tld/>anteco</kap>
      <snc>
    	<dif>
    	  Nuna tempo.
    	</dif>
      </snc>
      <trd lng="de">Gegenwart</trd>
      <trd lng="fr">pr&eacute;sent<klr tip="ind"> (temps)</klr></trd>
      <trd lng="nl">tegenwoordige <ind>tijd</ind></trd>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_je;&c_je;
      <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
    </drv>

    <drv mrk="est.0inteco">
      <kap><tld/>inteco</kap>
      <snc>
    	<dif>
    	  Pasinta tempo.
    	</dif>
      </snc>
      <trd lng="de">Vergangenheit</trd>
      <trd lng="fr">pass&eacute;<klr tip="ind"> (temps)</klr></trd>
      <trd lng="nl">verleden <ind>tijd</ind></trd>
      <trdgrp lng="ru">
    	<trd>&c_p;&c_r;&c_o;&c_sh;&c_l;&c_o;&c_je;</trd>,
    	<trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;
    	<klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="est.0onteco">
      <kap><tld/>onteco</kap>
      <snc>
    	<dif>
    	  Venonta tempo.
    	</dif>
      </snc>
      <trd lng="de">Zukunft</trd>
      <trd lng="fr">futur<klr tip="ind"> (temps)</klr></trd>
      <trd lng="nl">toekomende <ind>tijd</ind></trd>
      <trd lng="ru">&c_b;&c_u;&c_d;&c_u;&c_shch;&c_je;&c_je;
      <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
    </drv>

    <drv mrk="est.0igi">
      <kap><tld/>igi</kap>
      <snc>
    	<dif>
    	  Ka&ubreve;zi, formi, produkti.
    	</dif>
    	<refgrp tip="vid">
    	  <ref cel="nask.0i">naski</ref>,
    	  <ref cel="port.al0i">alporti</ref>,
    	  <ref cel="vok.al0i">alvoki</ref>,
    	  <ref cel="vek.0i">veki</ref>,
    	  <ref cel="kre.0i">krei</ref>,
    	  <ref cel="ven">venigi</ref>,
    	  <ref cel="okaz.0igi">okazigi</ref>,
    	  <ref cel="kauxz.0i">ka&ubreve;zi</ref>,
    	  <ref cel="produk.0i">produkti</ref>,
    	  <ref cel="star.0igi">starigi</ref>,
    	  <ref cel="teks.0i">teksi</ref>.
    	</refgrp>
      </snc>
      <trdgrp lng="de">
    	<trd>hervorrufen</trd>,
    	<trd>herstellen</trd>
      </trdgrp>
      <trdgrp lng="fr">
    	<trd>causer<klr tip="ind"> (occasionner)</klr></trd>,
    	<trd>cr&eacute;er<klr tip="ind"> (occasionner)</klr></trd>,
    	<trd>faire</trd>,
    	<trd>faire na&icirc;tre</trd>,
    	<trd>g&eacute;n&eacute;rer</trd>,
    	<trd>occasionner</trd>,
    	<trd>susciter<klr tip="ind"> (occasionner)</klr></trd>
      </trdgrp>
      <trd lng="nl">veroorzaken</trd>
      <trdgrp lng="ru">
    	<trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>,
    	<trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; &c_k; &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ju;</trd>
      </trdgrp>
    </drv>

    <drv mrk="est.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
    	<dif>
    	  I&gcirc;i ekzistanta.
    	</dif>
      </snc>
      <trdgrp lng="fr">
    	<trd>se <ind>produire</ind></trd>,
    	<trd>survenir</trd>
      </trdgrp>
      <trd lng="nl">worden</trd>
      <trd lng="ru">&c_v;&c_o;&c_z;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="est.al0i">
      <kap>apud<tld/>i, apud<tld/>i</kap>
      <gra><vspec>x</vspec></gra>
      <uzo tip="stl">EVI</uzo>
      <ref tip="dif" cel="est.cxe0i">&ccirc;eesti</ref>
      <rim>
         La direkta <em>al-</em> neniel ta&ubreve;gas por la senco
         &leftquot;&ccirc;e<tld/>i&rightquot;, kaj povis aperi nur pro misapliko de alilingva
         tradicio.

         &leftquot;Apud<tld/>i&rightquot; formale estus tute ta&ubreve;ga, sed por pli flanka,
	 periferia situo.
	 <aut>Sergio Pokrovskij</aut>
      </rim>
    </drv>

    <drv mrk="est.cxe0i">
      <kap>&ccirc;e<tld/>i</kap>
      <gra><vspec>x</vspec></gra>
      <snc>
    	<dif>
    	  <tld lit="E"/>i en iu loko, kiam io okazas:
    	  <ekz>
    	    &ccirc;e<tld/>i kunvenon;
    	  </ekz>
    	  <ekz>
    	    &ccirc;e<tld/>i en, &ccirc;e kunveno;
    	  </ekz>
    	  <ekz>
    	    en <ind>&ccirc;e<tld/>o</ind> amata, en for<tld/>' insultata;
    	    <trdgrp lng="fr">
    	      <trd>assistance<klr tip="ind"> (fait d'assister)</klr></trd>,
    	      <trd>pr&eacute;sence</trd>
    	    </trdgrp>
    	    <fnt><bib>PrV</bib></fnt>
    	  </ekz>
    	  <ekz>
    	    multnombra <ind>&ccirc;e<tld/>antaro</ind>.
    	    <trd lng="fr">assistance<klr tip="ind"> (personnes)</klr></trd>
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="de">dabeisein</trd>
      <trdgrp lng="fr">
    	<trd>assister (&agrave;)</trd>,
    	<trd>&ecirc;tre pr&eacute;sent</trd>
      </trdgrp>
      <trd lng="nl">aanwezig zijn</trd>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="est.ek0i">
      <kap>ek<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
    	<dif>
    	  Komenci <tld/>i,
          da&ubreve;re (<ref tip="sin" cel="est.0igxi"><tld/>i&gcirc;i</ref>)
          a&ubreve; nur por nelonga tempo:
    	  <ekz>
            por unu minuto ek<tld/>is embarasa silento
            <fnt><bib>Metrop</bib></fnt>;
    	  </ekz>
    	  <ekz>
            li anka&ubreve; devis demandi, kiel Komenio kondutu en la situacio,
            kiu ek<tld/>is post la duelo kun Nigrin
            <fnt>
              <aut>F. Ko&zcaron;&iacute;k, tr. Z. Rus&iacute;n kaj M. Malovec</aut>:
              <vrk>Lumo en tenebroj</vrk>.
            </fnt>;
    	  </ekz>
          <ekz>
            la problemo estas kompreneble solvita <ctl>itisme</ctl>,
            &ccirc;ar la lingvo &gcirc;in tiel solvis jam anta&ubreve; ol la fama
            disputo ek<tld/>is
            <fnt>
              <aut>J. Lindstedt</aut>:
              <vrk>Recenza eseo pri PAG</vrk>,
              <lok>Planlingvistiko 1 (1982)</lok>.
            </fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="ru">&c_v;&c_o;&c_z;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
    </drv>
        
    <drv mrk="est.for0i">
      <kap>for<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
    	<dif>
    	  <tld lit="E"/>i malproksima, ne &ccirc;e<tld/>i:
    	  <ekz>
    	    mi for<tld/>is dum du tagoj;
    	  </ekz>
    	  <ekz>
    	    for<tld/>i el oficejo;
    	  </ekz>
    	  <ekz>
    	    <tld/>i kondamnita for<tld/>ante;
    	  </ekz>
    	  <ekz>
    	    <ind>for<tld/>o</ind> de ofendo estas plej bona defendo
    	    <fnt><bib>PrV</bib></fnt>;
    	    <trd lng="fr">absence</trd>
    	  </ekz>
    	  <ekz>
    	    la <ind>for<tld/>anto</ind> &ccirc;iam estas malprava
    	    <fnt><bib>PrV</bib></fnt>.
    	    <trd lng="fr">absent<klr tip="ind"> (subst.)</klr></trd>
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="nl">afwezig zijn</trd>
      <trd lng="ru">&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_b;&c_y;&c_t;&c_mol; &c_v; &c_d;&c_r;&c_u;&c_g;&c_o;&c_m; &c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
    </drv>

    <drv mrk="est.mal0i">
      <kap>mal<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
    	<dif>
    	  Ne <ref tip="ant" cel="ekzist.0i">ekzisti</ref>,
          <ref tip="sin" cel="mank.0i">manki</ref>.
    	</dif>
      </snc>
      <trd lng="de">fehlen</trd>
      <trdgrp lng="fr">
    	<trd>faire <ind>d&eacute;faut</ind></trd>,
    	<trd>manquer</trd>
      </trdgrp>
      <trd lng="nl">ontbreken</trd>
      <trd lng="ru">&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_n;&c_je; &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr></trd>
    </drv>

    <drv mrk="est.ne0o">
      <kap>ne<tld/>o</kap>
      <snc>
    	<dif>
    	  Neekzisto:
    	  <ekz>
    	    kontra&ubreve; ne<tld/>o ne helpas protesto<fnt><bib>PrV</bib></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="de">Nichtsein</trd>
      <trd lng="ru">&c_n;&c_je;&c_b;&c_y;&c_t;&c_i;&c_je;</trd>
    </drv>

    <drv mrk="est.tio0as">
      <kap>tio <tld/>as, t.e.</kap>
      <snc>
    	<dif>
    	  Esprima&jcirc;o, ofte mallongigite skribata <em>t.e.</em>,
          ordinare uzata adverbe, samsignife
    	  kiel <ref tip="sin" cel="nom.0e.te">nome<sncref/></ref>:
    	  <ekz>
    	    la simpla esperantisteco, t.e. la simpla lernado kaj
    	    uzado de Esperanto, neniun devigas ali&gcirc;i al ia
    	    ideo<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    la akuzativo montras direkton,
    	    t.e. movadon al ia loko<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    por la hodia&ubreve;a tago mi ricevis duoblan pagon, t.e. dek
    	    frankojn<fnt>Z</fnt>;
    	  </ekz>
    	  <ekz>
    	    por esprimi direkton ni aldonas la
    	    fina&jcirc;on n, t.e. la fina&jcirc;o, kiu signas la akuzativon.
    	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_t;&c_o; &c_je;&c_s;&c_t;&c_mol;</trd>
    </drv>
    <adm>
    ~ant(ec)o, ktp: aldoni ekz GRA. <aut>MB</aut>
    </adm>
  </art>
