    <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;
<klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="est.0onteco">
  <kap><tld/>onteco</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
