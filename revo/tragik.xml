<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>tragik</rad>/a
</kap>
<drv mrk="tragik.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="traged.0a.FIG">tragedia <sncref
    ref="traged.0a.FIG"/></ref>
    <ekz>
      Argentino, nacio naskita post la fino de la plej kruela kaj <tld/>a
      ekspluatado kaj konkero de Ameriko, fare de e&ubreve;ropanoj<fnt><aut>Ro
      Sartor</aut>: <vrk>Ploru pri Argentino!</vrk>,
      <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 8a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>tragisch</trd>,
    <trd>unheilvoll</trd>,
    <trd>unseelig</trd>,
    <trd>ersch&uuml;tternd</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tragique<klr> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="nl">tragisch</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
