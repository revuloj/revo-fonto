<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tron.xml,v 1.28 2024/04/12 06:28:57 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>tron</rad>/o
    </kap>

    <drv mrk="tron.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="tron.0o.meblo">
        <dif>
          Ceremonia alta <ref tip="super" cel="segx.0o.meblo">se&gcirc;o</ref>
          de re&gcirc;o a&ubreve; alia suvereno.
        </dif>
        <trd lng="pl">tron</trd>
      </snc>
      <snc mrk="tron.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          La re&gcirc;a a&ubreve; suverena potenco.
        </dif>
        <ref tip="sin" cel="kron.0o.regxeco">krono<sncref/></ref>
        <trdgrp lng="pl">
          <trd>tron</trd>,
          <trd>stolec<klr> (krzes&lstroke;o)</klr></trd>
        </trdgrp>
      </snc>
  <trd lng="bo">&#xF41;&#xFB2;&#xF72;&#xF0B;</trd>
  <trd lng="es">trono</trd>
      <trdgrp lng="ja">
    <trd>&#29577;&#x5EA7;  <pr>&#x304E;&#12423;&#x304F;&#12374;</pr></trd>,
    <trd>&#x738B;&#x5EA7;  <pr>&#x304A;&#12358;&#12374;</pr></trd>,
    <trd>&#x541B;&#x4E3B;&#x306E;&#x5EA7;&#x5E2D; 
    <pr>&#x304F;&#12435;&#12375;&#12421;&#x306E;&#12374;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x738B;&#x4F4D;  <pr>&#x304A;&#12358;&#12356;</pr></trd>,
    <trd>&#x5E1D;&#x4F4D;  <pr>&#12390;&#12356;&#12356;</pr></trd>,
    <trd>&#x738B;&#x6A29;  <pr>&#x304A;&#12358;&#12369;&#12435;</pr></trd>,
    <trd>&#x5E1D;&#x6A29;  <pr>&#x307F;&#x304B;&#12393;&#12369;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="lat">solium, ii</trd>
  <trdgrp lng="be">
        <trd>&c_t;&c_r;&c_o;&c_n;</trd>,
        <trd>&c_p;&c_a;&c_s;&c_a;&c_d;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>panovnick&yacute; stolec</trd>,
    <trd>tr&uring;n</trd>
  </trdgrp>
  <trd lng="de">Thron</trd>
  <trd lng="en">throne</trd>
  <trd lng="fr">tr&ocirc;ne</trd>
  <trdgrp lng="hu">
        <trd>tr&oacute;nus</trd>,
        <trd>tr&oacute;nsz&eacute;k</trd>
      </trdgrp>
  <trd lng="nl">troon</trd>
  <trd lng="pt">trono</trd>
  <trdgrp lng="ru">
        <trd>&c_t;&c_r;&c_o;&c_n;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_o;&c_l;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>kr&aacute;&lcaron;ovsk&aacute; moc</trd>,
    <trd>prestol</trd>,
    <trd>tr&oacute;n</trd>,
    <trd>vl&aacute;da</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&#x43E;&#x43D;</trd>,
    <trd>&#x43F;&c_r;&c_je;&c_s;&c_t;&#x43E;&#x43B;</trd>
  </trdgrp>
</drv>

    <drv mrk="tron.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="tron.0i.meblo">
        <dif>
          Sidi sur <tld/>o.
        </dif>
        <trd lng="be">&c_s;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol; &c_n;&c_a;
        <ind>&c_t;&c_r;&c_o;&c_n;&c_je;</ind></trd>
        <trd lng="en">sit enthroned</trd>
        <trd lng="pl">zasiada&cacute; na <ind>tronie</ind></trd>
        <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_je;&c_d;&c_a;&c_t;&c_mol;
        &c_n;&c_a; &c_t;&c_r;&c_o;&c_n;&c_je;</trd>
      </snc>
      <snc mrk="tron.0i.FIG_regi">
        <dif>
          Esti re&gcirc;o a&ubreve; suvereno.
        </dif>
        <trd lng="be">&c_v;&c_a;&c_l;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
        <trd lng="en">reign</trd>
        <trdgrp lng="pl">
          <trd>kr&oacute;lowa&cacute;</trd>,
          <trd>panowa&cacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="tron.0i.FIG_mieni">
        <uzo tip="klr">(f. iu)</uzo>
        <dif>
          Afekti imponan mienon de grava a&ubreve;toritata persono:
          <ekz>
            la markizo &ccirc;iam kaj &ccirc;ie volis
            <tld/>i<fnt>B</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_v;&c_a;&c_zh;&c_n;&c_y; <ind>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</ind></trd>,
          <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_c;&c_a; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
        </trdgrp>
        <trd lng="en">lord it</trd>
        <trdgrp lng="pl">
          <trd>wywy&zdot;sza&cacute; si&eogonek;</trd>,
          <trd>panowa&cacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="tron.0i.FIG_alti">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Majeste alte stari&gcirc;i; impone montri&gcirc;i:
          <ekz>
            kastelo fiere <tld/>as sur la alta roko;
          </ekz><ekz>
            la vulkano <tld/>as en malanta&ubreve;a&jcirc;o;
          </ekz><ekz>
            sur lia frunto <tld/>as fiero.
          </ekz>
        </dif>
        <trd lng="be">&c_u;&c_z;&c_v;&c_y;&c_sh;&c_a;&c_c;&c_c;&c_a;</trd>
        <trdgrp lng="pl">
          <trd>g&oacute;rowa&cacute;</trd>,
          <trd>panowa&cacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_y;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </snc>
  <trd lng="cs">tr&uring;nit</trd>
  <trd lng="de">thronen</trd>
  <trd lng="fr">tr&ocirc;ner</trd>
  <trdgrp lng="hu">
        <trd>tr&oacute;nol</trd>,
        <trd>tr&oacute;nsz&eacute;ken &uuml;l</trd>
      </trdgrp>
  <trdgrp lng="ja">
    <trd>&#29577;&#x5EA7;&#x306B;&#12377;&#x308F;&#12387;&#12390;&#12356;&#x308B; 
    <pr>&#x305F;&#x307E;&#12374;&#x306B;&#12377;&#x308F;&#12387;&#12390;&#12356;&#x308B;</pr></trd>,
    <trd>&#x541B;&#x81E8;&#12377;&#x308B;  <pr>&#x304F;&#12435;&#x308A;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#12391;&#12435;&#12392;&#x5EA7;&#12387;&#12390;&#12356;&#x308B;  <pr>&#12391;&#12435;&#12392;&#12377;&#x308F;&#12387;&#12390;&#12356;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">tronen</trd>
  <trdgrp lng="sk">
    <trd>sedie&tcaron; na  tr&oacute;ne</trd>,
    <trd>tr&oacute;ni&tcaron;</trd>,
    <trd>vl&aacute;dnu&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_c;&c_a;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_a;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43D;&c_a; &#x43F;&c_r;&c_je;&c_s;&c_t;&#x43E;&#x43B;&c_ib;</trd>
  </trdgrp>
</drv>

    <drv mrk="tron.de0igi">
      <kap>de<tld/>igi</kap>
      <snc>
        <dif>
          Senigi je la (re&gcirc;a) potenco:
          <ekz>
            <klr>[li]</klr> de<tld/>igis re&gcirc;ojn
            <fnt><bib>MT</bib><lok>&Jes; 41:2</lok></fnt>;
          </ekz><ekz>
            la ideo de<tld/>igi Jesuon Kriston <klr>[...]</klr> neniam
            e&ccirc;
            por unu minuto venis en la kapon de iu homarano
            <fnt><bib>Homaranismo</bib><lok/></fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_s;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol; &c_z;
        <ind>&c_t;&c_r;&c_o;&c_n;&c_u;</ind></trd>,
        <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_a;&c_c;&c_mol; &c_z;
        <ind>&c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</ind></trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>detronizovat</trd>,
    <trd>svrhnout z tr&uring;nu</trd>,
    <trd>zbavit tr&uring;nu</trd>
  </trdgrp>
  <trdgrp lng="en">
        <trd>dethrone</trd>,
        <trd>depose</trd>
      </trdgrp>
  <trd lng="es">destronar</trd>
      <trdgrp lng="fr">
        <trd>d&eacute;tr&ocirc;ner</trd>,
        <trd>d&eacute;poser<klr tip="ind"> (un souverain)</klr></trd>,
        <trd>destituer<klr tip="ind"> (un souverain)</klr></trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>detroniz&aacute;l</trd>,
        <trd>tr&oacute;nj&aacute;t&oacute;l megfoszt</trd>
      </trdgrp>
  <trd lng="ja">&#36864;&#x4F4D;&#12373;&#x305B;&#x308B;  <pr>&#x305F;&#12356;&#12356;&#12373;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
        <trd>onttronen</trd>,
        <trd>van de <ind>troon</ind> stoten</trd>
      </trdgrp>
  <trdgrp lng="pl">
        <trd>zrzuca&cacute; z <ind>tronu</ind></trd>,
        <trd>pozbawia&cacute; <ind>tronu</ind></trd>
      </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zosadi&tcaron;</trd>,
    <trd>zvrhn&uacute;&tcaron; z tr&oacute;nu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&c_i;&#x43D;&c_u;&c_t;&c_i; &c_z; &#x43F;&c_r;&c_je;&c_s;&c_t;&#x43E;&#x43B;&c_u;</trd>,
    <trd>&#x43F;&#x43E;&c_v;&c_a;&#x43B;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="tron.sur0igi">
      <kap>sur<tld/>igi</kap>
      <snc>
        <dif>
          Havigi la (re&gcirc;an) potencon:
          <ekz>
            la posteuloj de tiu sama hereda &ccirc;efpastraro, kiu havis
            la povon
            sur<tld/>igi, nun havas la povon de<tld/>igi ilin
            <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 19</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_n;&c_a;
        <ind>&c_t;&c_r;&c_o;&c_n;</ind></trd>,
        <trd>&c_u;&c_z;&c_mol;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib;
        &c_n;&c_a; <ind>&c_t;&c_r;&c_o;&c_n;</ind></trd>,
        <trd>&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_n;&c_a;
        <ind>&c_p;&c_a;&c_s;&c_a;&c_d;</ind></trd>
      </trdgrp>
  <trd lng="cs">nastolit <klr>(na tr&uring;n)</klr></trd>
  <trdgrp lng="de">
        <trd>inthronieren</trd>
      </trdgrp>
  <trd lng="en">enthrone</trd>
  <trdgrp lng="fr">
        <trd>introniser<klr tip="ind"> (mettre sur le
        tr&ocirc;ne)</klr></trd>,
        <trd>couronner<klr tip="ind"> (un souverain)</klr></trd>,
        <trd>porter au <ind>pouvoir</ind></trd>,
        <trd>mettre sur le tr&ocirc;ne</trd>
      </trdgrp>
  <trd lng="hu">tr&oacute;nra emel</trd>
  <trd lng="ja">&#21363;&#x4F4D;&#12373;&#x305B;&#x308B;  <pr>&#x305D;&#x304F;&#12356;&#12373;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">op de <ind>troon</ind> zetten</trd>
  <trdgrp lng="pl">
        <trd>osadza&cacute; na <ind>tronie</ind></trd>,
        <trd>posadzi&cacute; na <ind>tronie</ind></trd>,
        <trd>wyniosi&cacute; na <ind>tron</ind></trd>
      </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_n;&c_a;
      &c_p;&c_r;&c_je;&c_s;&c_t;&c_o;&c_l;</trd>
  <trd lng="sk">nastoli&tcaron; <klr>(na tr&oacute;n)</klr></trd>
  <trdgrp lng="uk">
    <trd>(&#x43F;&#x43E;)&c_s;&c_a;&c_d;&c_i;&c_t;&c_i; &#x43D;&c_a; &#x43F;&c_r;&c_je;&c_s;&c_t;&#x43E;&#x43B;/&c_t;&c_r;&#x43E;&#x43D;</trd>,
    <trd>&c_ib;&#x43D;&c_t;&c_r;&#x43E;&#x43D;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
  </art>
