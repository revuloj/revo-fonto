<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>krab</rad>/o
</kap>
<drv mrk="krab.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Iu el kraboj:
      <ekz>
        sur la bordo malaltaj fortika&jcirc;oj plati&gcirc;is kiel <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Krabbe</trd>
  <trd lng="en">crab</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_b;</trd>
</drv>

<drv mrk="krab.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subordo el la ordo de
      <ref tip="super" cel="krust.0uloj">dekpiedaj krustuloj</ref>
      (<trd lng="la">Brachyura</trd>), kies malgranda abdomeno estas
      ka&scirc;ita sub la cetera korpo.
    </dif>
  </snc>
  <trd lng="de">Krabben <klr tip="ind">(Unterordn.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_b;&c_y;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_o;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_je; &c_r;&c_a;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
</art>
