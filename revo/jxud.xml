<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&jcirc;ud</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jxud.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Japan-devena lukto-<ref tip="super" cel="sport.0o">sporto</ref>
      sindefenda, en kiu oni kaptas, for&jcirc;etas, a&ubreve; senmovigas
      kontra&ubreve;ulon.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ju;&c_d;&c_o;</trd>
  <trd lng="en">judo</trd>
  <trd lng="de">Judo</trd>
  <trd lng="es">judo</trd>
  <trd lng="it">jud&ograve;</trd>
  <trd lng="fr">judo</trd>
  <trdgrp lng="hu">
    <trd>cselg&aacute;ncs</trd>,
    <trd>dzs&uacute;d&oacute;</trd>
  </trdgrp>
  <trd lng="pt">jud&ocirc;</trd>
</drv>

<drv mrk="jxud.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Praktikanto de <tld/>o:
      <ekz>
        &ccirc;iuj <tld/>istoj portas sur si diverskolorajn zonojn por montri
        sian klason a&ubreve; rangon
        <fnt><bib>EncJap</bib>, <lok>artikolo &leftquot;&jcirc;udo&rightquot;, p. 78a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">judoka</trd>
  <trdgrp lng="hu">
    <trd>cselg&aacute;ncsoz&oacute;</trd>,
    <trd>dzs&uacute;d&oacute;&oacute;s</trd>
  </trdgrp>
</drv>
</art>
