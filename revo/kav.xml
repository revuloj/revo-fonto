<drv mrk="kav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kav.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="plen.mal0ajxo">Malplena&jcirc;o</ref> en solida materio a&ubreve; en organo:
      <ekz>
        <tld/>oj en tero, en arbo, en pano, en froma&gcirc;o;
      </ekz>
      <ekz>
        iu malfermos <tld/>on, a&ubreve; elfosos <tld/>on, kaj ne kovros &gcirc;in, kaj falos tien
        bovo
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 21:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la muroj de la domo <klr>[estas]</klr> <tld/>etoj verdetaj
        <fnt>
          <bib>MT</bib>
          <lok>&Lev; 14:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj ka&scirc;is sin en kavernoj, en arbustoj, en rokoj, en <tld/>oj, kaj en putoj
        <fnt>
          <bib>MT</bib>
          <lok>&Sa1; 13:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li fosis <tld/>on kaj profundigis &gcirc;in, kaj li falis en la <tld/>on, kiun li
        pretigis
        <fnt>
          <bib>MT</bib>
          <lok>&Psa; 7:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pelis pavon, falis <tld/>on</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>
        <klr>(malsukcesi)</klr>;
      </ekz>
      <ekz>
        al <tld/>o senfunda &scirc;topado ne helpas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la valo <nom>Sidim</nom> estis multe da bitumaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 14:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>vojo</ind>
        <klr>(profunde fosita en tero)</klr>;
        <trd lng="fr">chemin creux</trd>
        <trd lng="hu">m&eacute;ly&uacute;t</trd>
      </ekz>
      <ekz>
        kot<tld/>a&jcirc;o;
      </ekz>
      <ekz>
        la &ccirc;armaj <ind><tld/>eto</ind>j de fra&ubreve;linaj
        vangoj;
        <trdgrp lng="fr">
          <trd>petit <ind>creux</ind></trd>,
          <trd>fossette</trd>
        </trdgrp>
        <trd lng="hu">g&ouml;dr&ouml;cske</trd>
        <trd lng="ru">&c_ja;&c_m;&c_o;&c_ch;&c_k;&c_a; <klr tip="ind">(&c_n;&c_a;
        &c_shch;&c_je;&c_k;&c_je;)</klr></trd>
      </ekz>
      <ekz>
        <uzo tip="fak">ANA</uzo>
        <ind>kor<tld/>eto</ind>j
        <klr>(vd <ref tip="vid" cel="kamer.0o.SCI">kamero</ref>)</klr>.
        <trd lng="fr">cavit&eacute; du c&oelig;ur</trd>
        <trd lng="hu">sz&iacute;v&uuml;reg</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kavern.0o">kaverno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_w;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>,
