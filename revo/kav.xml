<drv mrk="kav.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kav.0igi.KOMUNE">
    <dif>
      Igi <tld/>a a&ubreve; estigi <tld/>ojn en:
      <ekz>
        li <klr>[&#8230;]</klr> fosis kaj profunde <tld/>igis, kaj metis fundamenton sur la
        roko
        <fnt>
          <bib>NT</bib>
          <lok>&Luk; 6:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> trinkis el sia <tld/>igita mano
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igita peco da froma&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        guto da pluvo <klr>[&#8230;]</klr> <tld/>igas la malmolan
        &scirc;tonon
        <fnt><bib>Fab4</bib> <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        se&gcirc;oj kun pajla reto, parte dis&scirc;irita kaj
        <tld/>igita
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">aush&ouml;hlen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_m;</ind></trd>,
    <trd>&c_p;&c_a;&c_g;&c_l;&c_y;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hloubit</trd>,
    <trd>prohloubit</trd>
  </trdgrp>
  <trd lng="en">hollow out</trd>
  <trd lng="es">cavar</trd>
  <trd lng="fr">creuser</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;j</trd>,
    <trd>homor&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31354;&#27934;&#12395;&#12377;&#12427;  <pr>&#12367;&#12358;&#12393;&#12358;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#12367;&#12412;&#12414;&#12379;&#12427;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>hol</ind> maken</trd>,
    <trd>uithollen</trd>
