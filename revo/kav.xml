<drv mrk="kav.0a">
  <kap><tld/>a</kap>
  <snc mrk="kav.0a.KOMUNE">
    <dif>
      <tld lit="K"/>oforma a&ubreve; enhavanta <tld/>ojn:
      <ekz>
        <tld/>a vojo, relo, dento;
      </ekz>
      <ekz>
        <tld/>aj okuloj, vangoj;
      </ekz>
      <ekz>
        <tld/>eca <klr>(kaverna)</klr> vo&ccirc;o;
      </ekz>
      <ekz>
        lago kun kurbaj bordoj kaj <tld/>a fundo
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu loko estis <tld/>a dezerto, &ccirc;irka&ubreve;ita de amfiteatro de nudaj montoj
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a brusto ektremis sub la antikvamoda manteleto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> alportis akvon en sia mano, kiu ka&ubreve;ze de sia na&gcirc;membrano
        fari&gcirc;is pli granda kaj pli <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        alta saliko, <klr>[kiu]</klr> en la mezo &gcirc;i estas disfendita, kaj el la
        <tld/>a&jcirc;o elkreskas herbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la pontokonstruado la ligna&jcirc;isto elfosis rondan <tld/>a&jcirc;on je la suda
        fosa&jcirc;limo
       <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_y;</trd>
  <trd lng="cs">dut&yacute;</trd>
  <trdgrp lng="de">
    <trd>hohl</trd>,
    <trd>ausgeh&ouml;hlt</trd>
  </trdgrp>
  <trd lng="en">hollow</trd>
  <trdgrp lng="fr">
    <trd>creux<klr tip="ind"> (adj.)</klr></trd>,
    <trd>creus&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>cave<klr tip="ind"> (creus&eacute;)</klr></trd>,
    <trd>encaiss&eacute;<klr tip="ind"> (creux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;reges</trd>,
    <trd>g&ouml;dr&ouml;s</trd>,
    <trd>odvas</trd>,
    <trd>k&aacute;ty&uacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20013;&#31354;&#12398;  <pr>&#12385;&#12421;&#12358;&#12367;&#12358;&#12398;</pr></trd>,
    <trd>&#31354;&#27934;&#12398;  <pr>&#12367;&#12358;&#12393;&#12358;&#12398;</pr></trd>,
    <trd>&#12367;&#12412;&#12435;&#12384;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hol</trd>,
    <trd>concaaf</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pusty</trd>,
    <trd>wkl&eogonek;s&lstroke;y</trd>,
    <trd>zag&lstroke;&eogonek;biony</trd>
  </trdgrp>
  <trdgrp lng="pt">
