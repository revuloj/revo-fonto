    <trd>cavidade</trd>,
    <trd>fosso</trd>,
    <trd>buraco</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>b&acirc;rlog</trd>,
    <trd>cavitate </trd>,
    <trd>depresie </trd>,
    <trd>fund</trd>,
    <trd>groap&abreve;</trd>,
    <trd>pe&#x219;tera</trd>,
    <trd>&#x219;an&#x21B;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_v;&c_p;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">v&yacute;mo&lcaron; na ceste</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_r;&c_ib;&c_v;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_i;&c_b;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_i;&c_b;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_b;&c_o;&c_ji;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_p;&c_l;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_ja;&c_m;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kav.0a">
  <kap><tld/>a</kap>
  <snc mrk="kav.0a.KOMUNE">
    <dif>
      <tld lit="K"/>oforma a&ubreve; enhavanta <tld/>ojn:
      <ekz>
        <tld/>a vojo, relo, dento;
      </ekz>
      <ekz>
        <tld/>aj okuloj, vangoj;
      </ekz>
      <ekz>
        <tld/>eca <klr>(kaverna)</klr> vo&ccirc;o;
      </ekz>
      <ekz>
        lago kun kurbaj bordoj kaj <tld/>a fundo
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu loko estis <tld/>a dezerto, &ccirc;irka&ubreve;ita de amfiteatro de nudaj montoj
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a brusto ektremis sub la antikvamoda manteleto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> alportis akvon en sia mano, kiu ka&ubreve;ze de sia na&gcirc;membrano
        fari&gcirc;is pli granda kaj pli <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        alta saliko, <klr>[kiu]</klr> en la mezo &gcirc;i estas disfendita, kaj el la
        <tld/>a&jcirc;o elkreskas herbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la pontokonstruado la ligna&jcirc;isto elfosis rondan <tld/>a&jcirc;on je la suda
        fosa&jcirc;limo
       <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_y;</trd>
  <trd lng="cs">dut&yacute;</trd>
  <trdgrp lng="de">
    <trd>hohl</trd>,
    <trd>ausgeh&ouml;hlt</trd>
  </trdgrp>
  <trd lng="en">hollow</trd>
  <trdgrp lng="fr">
    <trd>creux<klr tip="ind"> (adj.)</klr></trd>,
    <trd>creus&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>cave<klr tip="ind"> (creus&eacute;)</klr></trd>,
    <trd>encaiss&eacute;<klr tip="ind"> (creux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;reges</trd>,
    <trd>g&ouml;dr&ouml;s</trd>,
    <trd>odvas</trd>,
    <trd>k&aacute;ty&uacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E2D;&#x7A7A;&#x306E;  [<ind>&#x3061;&#x3085;&#x3046;&#x304F;&#x3046;&#x306E;</ind>]</trd>,
    <trd>&#x7A7A;&#x6D1E;&#x306E;  [<ind>&#x304F;&#x3046;&#x3069;&#x3046;&#x306E;</ind>]</trd>,
    <trd>&#x304F;&#x307C;&#x3093;&#x3060;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hol</trd>,
    <trd>concaaf</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pusty</trd>,
    <trd>wkl&eogonek;s&lstroke;y</trd>,
    <trd>zag&lstroke;&eogonek;biony</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>vazio</trd>,
    <trd>oco</trd>,
    <trd>cavo</trd>,
    <trd>cavado</trd>,
    <trd>c&ocirc;ncavo</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>concav</trd>,
    <trd>gol</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_p;&c_a;&c_l;&c_y;&c_j;</trd>
  <trd lng="sk">dut&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_u;&c_p;&c_l;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_b;&c_o;&c_ji;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_p;&c_a;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kav.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        nigrajn rokojn ondoj lavas, ||
        nur malla&ubreve;te, lulmurmure; ||
        &ccirc;ar belego ilin ravas: ||
        son&gcirc;as, kie rokoj <tld/>as, ||
        blond-harara <nom>&Scirc;una</nom> pure
        <fnt>
        <aut>J. Francis</aut> <vrk>Hebridea Romanco</vrk>
        <lok>La Nica literatura revuo, numero 4:4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>b&yacute;t dut&yacute;</trd>,
    <trd>zet</trd>
  </trdgrp>

  <trd lng="en">to be hollow</trd>
  <trd lng="es">estar cavado</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>creux</ind></trd>,
    <trd>&ecirc;tre <ind>cave</ind></trd>,
    <trd>&ecirc;tre <ind>encaiss&eacute;</ind></trd>,
    <trd>b&eacute;er</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;reges</trd>,
    <trd>g&ouml;dr&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A7A;&#x6D1E;&#x3067;&#x3042;&#x308B;  [<ind>&#x304F;&#x3046;&#x3069;&#x3046;&#x3067;&#x3042;&#x308B;</ind>]</trd>,
    <trd>&#x304F;&#x307C;&#x3093;&#x3067;&#x3044;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl"><ind>hol</ind> zijn</trd>
  <trdgrp lng="pl">
    <trd>kopa&cacute;</trd>,
    <trd>dr&aogonek;&zdot;y&cacute;</trd>
  </trdgrp>
  <trd lng="pt">ser <ind>cavado</ind></trd>
  <trdgrp lng="ro">
    <trd>s&abreve;pa</trd>,
    <trd>g&abreve;uri</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_i;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">by&tcaron; dut&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_z;&c_a;&c_p;&c_a;&c_l;&c_i;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_s;&c_t;&c_i;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_d;&c_u;&c_p;&c_l;&c_i;&c_s;&c_t;&c_i;&c_m; &c_ib; &c_t;.&c_p;.</trd>
  </trdgrp>
</drv>

<drv mrk="kav.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kav.0igi.KOMUNE">
    <dif>
      Igi <tld/>a a&ubreve; estigi <tld/>ojn en:
      <ekz>
        li <klr>[&#x2026;]</klr> fosis kaj profunde <tld/>igis, kaj metis fundamenton sur la
        roko
        <fnt>
          <bib>NT</bib>
          <lok>&Luk; 6:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> trinkis el sia <tld/>igita mano
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igita peco da froma&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        guto da pluvo <klr>[&#x2026;]</klr> <tld/>igas la malmolan
        &scirc;tonon
        <fnt><bib>Fab4</bib> <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        se&gcirc;oj kun pajla reto, parte dis&scirc;irita kaj
        <tld/>igita
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">aush&ouml;hlen</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hloubit</trd>,
    <trd>prohloubit</trd>
  </trdgrp>
  <trd lng="en">hollow out</trd>
  <trd lng="es">cavar</trd>
  <trd lng="fr">creuser</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;j</trd>,
    <trd>homor&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A7A;&#x6D1E;&#x306B;&#x3059;&#x308B;  [<ind>&#x304F;&#x3046;&#x3069;&#x3046;&#x306B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x304F;&#x307C;&#x307E;&#x305B;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>hol</ind> maken</trd>,
    <trd>uithollen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dr&aogonek;&zdot;y&cacute;</trd>,
    <trd>wydr&aogonek;&zdot;y&cacute;</trd>,
    <trd>wy&zdot;&lstroke;abia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">cavar</trd>
  <trdgrp lng="ro">
    <trd>foraj</trd>,
    <trd>tr&acirc;nti</trd>,
    <trd>s&abreve;pa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyh&lacute;bi&tcaron; <klr>(dutinu)</klr></trd>
</drv>

<drv mrk="kav.en0a">
  <kap>en<tld/>a</kap>
  <snc mrk="kav.en0a.KOMUNE">
    <dif>
      Prezentanta kvaza&ubreve; enfalintan parton, <tld/>on en surfaco:
      <ekz>
        liaj okuloj, kvankam malgrandaj kaj en<tld/>aj, ne estis kulpaj
        <fnt><bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 6a, nokta aventuro de Lemuel</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>eingefallen <klr>(Augen)</klr></trd>,
      <trd>tiefliegend</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>depressed</trd>,
    <trd>sunken</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>creus&eacute;</trd>,
    <trd>cave<klr tip="ind"> (creus&eacute;)</klr></trd>,
    <trd>d&eacute;prim&eacute;<klr tip="ind"> (formant un
    creux)</klr></trd>,
    <trd>enfonc&eacute;<klr tip="ind"> (formant un creux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;lyen &uuml;l&odblac;</trd>,
    <trd>beesett<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trd lng="nl">uitgehold</trd>
  <trdgrp lng="pl">
    <trd>zag&lstroke;&eogonek;biony</trd>,
    <trd>wkl&eogonek;s&lstroke;y</trd>
