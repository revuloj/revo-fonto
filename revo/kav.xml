<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kav</rad>/o
</kap>
<drv mrk="kav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malplena&jcirc;o en solida materio a&ubreve; en organo:
      <ekz>
        <tld/>oj en tero, en arbo, en pano, en froma&gcirc;o;
      </ekz>
      <ekz>
        peli pavon, fali <tld/>on<fnt>Z</fnt>
        <klr>(malsukcesi)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>vojo</ind>
        <klr>(profunde fosita en tero)</klr>;
        <trd lng="fr">chemin creux</trd>
        <trd lng="hu">m&eacute;ly&uacute;t</trd>
      </ekz>
      <ekz>
        kot<tld/>a&jcirc;o;
      </ekz>
      <ekz>
        <tld/>ego en monto
        <klr>
        (<refgrp tip="vid">
          <ref cel="abism.0o">abismo</ref>,
          <ref cel="profun.0ego0egajxo">profundega&jcirc;o</ref>
        </refgrp>)</klr>;
      </ekz>
      <ekz>
        la &ccirc;armaj <ind><tld/>eto</ind>j de fra&ubreve;linaj vangoj<fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>petit <ind>creux</ind></trd>,
          <trd>fossette</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="fak">ANA</uzo>
        <ind>kor<tld/>eto</ind>j
        <klr>(<ref tip="vid" cel="kamer.0o.ANA">kamero</ref>)</klr>;
        <trd lng="fr">cavit&eacute; du c&oelig;ur</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">ANA</uzo>
        <ind>okul<tld/>o</ind>j.
        <trd lng="fr">orbite<klr tip="ind"> (de l'&oelig;il)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kavern.0o">kaverno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_w;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_h;&c_l;&c_ja;&c_b;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>H&ouml;hle</trd>,
    <trd>H&ouml;hlung</trd>,
    <trd>Hohlraum</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cavit&eacute;</trd>,
    <trd>creux<klr tip="ind"> (subst., cavit&eacute;)</klr></trd>,
    <trd>d&eacute;pression<klr tip="ind"> (cavit&eacute;)</klr></trd>,
    <trd>fosse<klr tip="ind"> (cavit&eacute;)</klr></trd>,
    <trd>renfoncement<klr tip="ind"> (cavit&eacute;)</klr></trd>,
    <trd>retrait<klr tip="ind"> (cavit&eacute;)</klr></trd>,
    <trd>trou<klr tip="ind"> (cavit&eacute;)</klr></trd>,
    <trd>vide<klr tip="ind"> (subst., cavit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;reg</trd>,
    <trd>g&ouml;d&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>holte</trd>,
    <trd>kuil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_v;&c_p;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kav.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>oforma a&ubreve; enhavanta <tld/>ojn:
      <ekz>
        <tld/>a vojo, relo, dento;
      </ekz>
      <ekz>
        <tld/>aj okuloj, vangoj;
      </ekz>
      <ekz>
        <tld/>eca <klr>(kaverna)</klr> vo&ccirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_y;</trd>
  <trd lng="de">hohl</trd>
  <trdgrp lng="fr">
    <trd>creux<klr tip="ind"> (adj.)</klr></trd>,
    <trd>creus&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>cave<klr tip="ind"> (creus&eacute;)</klr></trd>,
    <trd>encaiss&eacute;<klr tip="ind"> (creux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;reges</trd>,
    <trd>g&ouml;dr&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hol</trd>,
    <trd>concaaf</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_p;&c_a;&c_l;&c_y;&c_j;</trd>
</drv>

<drv mrk="kav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>creux</ind></trd>,
    <trd>&ecirc;tre <ind>cave</ind></trd>,
    <trd>&ecirc;tre <ind>encaiss&eacute;</ind></trd>,
    <trd>b&eacute;er</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;reges</trd>,
    <trd>g&ouml;dr&ouml;s</trd>
  </trdgrp>
  <trd lng="nl">hol zijn</trd>
</drv>

<drv mrk="kav.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi <tld/>a a&ubreve; estigi <tld/>ojn en:
      <ekz>
        trinki el sia <tld/>igita mano<fnt>Z</fnt>;
      </ekz>
      <ekz>
        guto da pluvo <klr>[...]</klr> <tld/>igas la malmolan &scirc;tonon
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        se&gcirc;oj kun pajla reto, parte dis&scirc;irita kaj <tld/>igita
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        (en)<tld/>i&gcirc;intaj vangoj, okuloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">creuser</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;j</trd>,
    <trd>homor&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">uithollen</trd>
</drv>

<drv mrk="kav.dis0ajxo">
  <kap>dis<tld/>a&jcirc;o</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      Malregulforma natura <tld/>a&jcirc;o.
    </dif>
  </snc>
  <adm>
    Malklara kaj misformita derivajxo. (Cetere la difino
    enhavas la nedifinitan terminon "kavajxo"). Indus trovi,
    kion gxi celis, aux tute forigi.
    <aut>MB</aut>
  </adm>
  <trd lng="fr">anfractuosit&eacute;</trd>
  <trd lng="nl">uitholling</trd>
</drv>

<drv mrk="kav.en0a">
  <kap>en<tld/>a</kap>
  <snc>
    <dif>
      Prezentanta kvaza&ubreve; enfalintan parton, <tld/>on en surfaco:
      <ekz>
        liaj okuloj, kvankam malgrandaj kaj en<tld/>aj, ne estis kulpaj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 6a, nokta aventuro de
        Lemuel</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>creus&eacute;</trd>,
    <trd>cave<klr tip="ind"> (creus&eacute;)</klr></trd>,
    <trd>d&eacute;prim&eacute;<klr tip="ind"> (formant un creux)</klr></trd>,
    <trd>enfonc&eacute;<klr tip="ind"> (formant un creux)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kav.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Enfali, i&gcirc;i <tld/>a el anta&ubreve;e plena formo:
      <ekz>
        liaj okuloj estis profunde en<tld/>i&gcirc;intaj
        <fnt><bib>Fab4</bib>, <lok>la vento rakontas pri Valdemaro Doe kaj
        pri liaj filinoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>creuser</ind></trd>
</drv>

<drv mrk="kav.fos0o">
  <kap>fos<tld/>o</kap>
  <snc>
    <dif>
      Arta, fosita <tld/>o, precipe en tero:
      <ekz>
        nun li mem vidis la kastelon kun &gcirc;iaj duoblaj fos<tld/>oj
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fos.0ajxo">fosa&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>douve</trd>,
    <trd>foss&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kav.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <ref tip="dif" cel="konvek.0a">konveksa</ref>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>bomb&eacute;</trd>,
    <trd>convexe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dombor&uacute;</trd>,
    <trd>konvex</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bol</trd>,
    <trd>convex</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_y;&c_j;</trd>
</drv>

<drv mrk="kav.man0o">
  <kap>man<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Interna flanko de mano <tld/>e tenata:
      <ekz>
        kiu mezuris per sia man<tld/>o la akvon
        <fnt><bib>MT</bib>, <lok>&Jes; 40:12</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">creux de la <ind>main</ind></trd>
</drv>

<drv mrk="kav.rado0o">
  <kap>rado<tld/>o</kap>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      Duopa sulko, <tld/>o farita de veturilradoj en tera vojo.
    </dif>
  </snc>
  <trd lng="fr">orni&egrave;re</trd>
  <trd lng="hu">k&aacute;ty&uacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_ja;</trd>
</drv>
</art>
