<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kav</rad>/o
</kap>
<drv mrk="kav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malplena&jcirc;o en solida materio a&ubreve; en organo:
      <ekz>
        <tld/>oj en tero, en arbo, en pano, en froma&gcirc;o;
      </ekz>
      <ekz>
        peli pavon, fali <tld/>on<fnt>Z</fnt>
        <klr>(malsukcesi)</klr>;
      </ekz>
      <ekz>
        <tld/>vojo
        <klr>(profunde fosita en tero)</klr>;
      </ekz>
      <ekz>kot<tld/>a&jcirc;o;</ekz>
      <ekz>
        <ind>rado<tld/>oj</ind>
        <klr>(faritaj de veturilradoj en ne pavimita vojo)</klr>;
        <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_ja;</trd>
      </ekz>
      <ekz>
        <tld/>ego en monto
        (<refgrp tip="sin">
          <ref cel="profun.0ego0egajxo">profundega&jcirc;o</ref>,
          <ref cel="abism.0o">abismo</ref>
        </refgrp>);
      </ekz>
      <ekz>la &ccirc;armaj <tld/>etoj de fra&ubreve;linaj vangoj<fnt>Z</fnt></ekz>
      <ekz>
        kor<tld/>etoj (<ref tip="sin" cel="kamer.0o">kamero</ref>);
      </ekz>
      <ekz>okul<tld/>oj;</ekz>
      <ekz>
        internaj naz<tld/>a&jcirc;oj 
        (<ref tip="sin" cel="naz.0o">naztruoj</ref>); 
      </ekz>
      <ekz>man<tld/>o<fnt>Z</fnt>.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fos.0ajxo">fosa&jcirc;o</ref>, 
      <ref cel="trancx1.0o">tran&ccirc;eo</ref>, 
      <ref cel="kavern.0o">kaverno</ref>, 
      <ref cel="trog.0o">trogo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>H&ouml;hle</trd>,
     <trd>H&ouml;hlung</trd>,
     <trd>Hohlraum</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>holte</trd>,
    <trd>kuil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_v;&c_p;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="kav.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>oforma a&ubreve; enhavanta <tld/>ojn:
      <ekz><tld/>a vojo, relo, dento;</ekz>
      <ekz><tld/>aj okuloj, vangoj;</ekz>
      <ekz><tld/>eca (kaverna) vo&ccirc;o.</ekz>
    </dif>
  </snc>
  <trd lng="de">hohl</trd>
  <trdgrp lng="nl">
    <trd>hol</trd>,
    <trd>concaaf</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_p;&c_a;&c_l;&c_y;&c_j;</trd>
</drv>
<drv mrk="kav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a. 
    </dif>
  </snc>
  <trd lng="nl">hol zijn</trd>
</drv>
<drv mrk="kav.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Igi <tld/>a a&ubreve; estigi <tld/>ojn en:
      <ekz>trinki el sia <tld/>igita mano<fnt>Z</fnt>;</ekz>
      <ekz>pluvo <tld/>igis la teron;</ekz>
      <ekz><tld/>igi vojon, pajlse&gcirc;on;</ekz>
      <ekz><tld/>itaj trunkoj<fnt>Z</fnt>;</ekz>
      <ekz>(en)<tld/>i&gcirc;intaj vangoj, okuloj.</ekz>
    </dif>
  </snc>
  <trd lng="nl">uithollen</trd>
</drv>
<drv mrk="kav.dis0ajxo">
  <kap>dis<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Malregulforma natura <tld/>a&jcirc;o. 
    </dif>
  </snc>
  <trd lng="nl">uitholling</trd>
</drv>
<drv mrk="kav.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Konveksa.  
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>bol</trd>,
    <trd>convex</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_y;&c_j;</trd>
</drv>
</art>
