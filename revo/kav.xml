    <trd>g&ouml;d&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A74;  [<ind>&#x3081;&#x3069;</ind>]</trd>,
    <trd>&#x7A7A;&#x6D1E;  [<ind>&#x304F;&#x3046;&#x3069;&#x3046;</ind>]</trd>,
    <trd>&#x304F;&#x307C;&#x307F;</trd>,
    <trd>&#x3078;&#x3053;&#x307F;</trd>,
    <trd>&#x8154;  [<ind>&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x7AA9;</trd>,
    <trd>&#x8C37;  [<ind>&#x305F;&#x306B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>holte</trd>,
    <trd>kuil</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>d&oacute;&lstroke;</trd>,
