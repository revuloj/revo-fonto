    <trd>&c_k;&c_i;&c_l;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">vrah</trd>
  <trd lng="uk">&c_n;&c_a;&c_j;&c_m;&c_a;&c_n;&c_i;&c_j; &c_u;&c_b;&c_i;&c_v;&c_c;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#20807;&#25163; <pr>xi&omacron;ngsh&#466;u</pr></trd>,
    <trd>&#20982;&#25163; <pr>xi&omacron;ngsh&#466;u</pr></trd>,
    <trd>&#26432;&#25163; <pr>sh&amacron;sh&#466;u</pr></trd>,
    <trd>&#27578;&#25163; <pr>sh&amacron;sh&#466;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murd.amas0i">
  <kap>amas<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="M"/>i multajn homojn:
      <ekz>
        kia kruela dio devas esti, je kies nomo krimuloj brutalege
        amas<tld/>as
        <fnt>
          <aut>S. Maul</aut>
          <vrk>Murdi je nomo de dio</vrk>
          <bib>Monato</bib>
          <lok>2001:10, p. 5a</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="masakr.0i">masakri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_o;&c_n;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_je; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>massakrieren</trd>,
    <trd>massenmorden</trd>
  </trdgrp>
  <trd lng="en">mass murder</trd>
  <trd lng="es">masacrar</trd>
  <trdgrp lng="fr">
    <trd>massacrer<klr> (assassiner en masse)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&ouml;meggyilkoss&aacute;got k&ouml;vet el</trd>
  <trdgrp lng="it">
    <trd>massacrare</trd>
    <trd>far strage</trd>
  </trdgrp>
  <trd lng="nl">een <ind>massamoord</ind> plegen</trd>
  <trd lng="pl">dokonywa&cacute; <ind>ludob&oacute;jstwa</ind></trd>
  <trdgrp lng="zh">
    <trd>&#26432;&#25134; <pr>sh&amacron;l&ugrave;</pr></trd>,
    <trd>&#27578;&#25134; <pr>sh&amacron;l&ugrave;</pr></trd>,
    <trd>&#23648;&#26432; <pr>t&uacute;sh&amacron;</pr></trd>,
    <trd>&#23648;&#27578; <pr>t&uacute;sh&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murd.amas0isto">
  <kap>amas<tld/>isto</kap>
  <snc>
    <dif>
      Plenuminto a&ubreve; ordoninto de amas<tld/>o:
      <ekz>
        &ccirc;u ili mislernis, &ccirc;ar kronikistoj raportis pri
        &leftquot;grandaj viroj&rightquot;,
        kiuj vere ne malofte estis amas<tld/>istoj
        <fnt>
          <aut>J. Ipfelkofer</aut>
          <vrk>Maksimo ankora&ubreve; lernenda</vrk>
          <bib>Monato</bib>
          <lok>2001:3, p. 7a</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Massenm&ouml;rder</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>boucher<klr> (massacreur)</klr></trd>,
      <trd>massacreur</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_w;&c_c;&c_a; &c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_g;&c_a; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trd lng="en">mass murderer</trd>
  <trd lng="hu">t&ouml;meggyilkos</trd>
  <trdgrp lng="it">
    <trd>massacratore</trd>
    <trd>stragista</trd>
  </trdgrp>
  <trd lng="nl">massamoordenaar</trd>
  <trd lng="pl">ludob&oacute;jca</trd>
</drv>

<drv mrk="murd.amas0o">
  <kap>amas<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="M"/>ado de multaj homoj:
      <ekz>
        sub emocia sperto pri amas<tld/>oj usonanoj kaj alinaciuloj aprobas
        militon
        <fnt><aut>M. Marko</aut>
        <vrk>Sekretaj organizantoj</vrk>
        <bib>Monato</bib>
        <lok>2002:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_je; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Massaker</trd>,
    <trd>Massenmord</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mass murder</trd>,
    <trd>massacre</trd>
  </trdgrp>
  <trd lng="es">masacre</trd>
  <trdgrp lng="fr">
    <trd>massacre<klr> (meurtre de masse)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&ouml;meggyilkoss&aacute;g</trd>
  <trdgrp lng="it">
    <trd>strage</trd>
    <trd>massacro</trd>
  </trdgrp>
  <trd lng="nl">massamoord</trd>
  <trd lng="pl">ludob&oacute;jstwo</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_o;&c_v;&c_o;&c_je; <ind>&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</ind></trd>
  <trdgrp lng="sk">
    <trd>hromadn&aacute; vra&zcaron;da</trd>,
    <trd>masakra</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23648;&#25134; <pr>t&uacute;l&ugrave;</pr></trd>,
    <trd>&#22823;&#23648;&#26432; <pr>d&agrave;t&uacute;sh&amacron;</pr></trd>,
    <trd>&#22823;&#23648;&#27578; <pr>d&agrave;t&uacute;sh&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murd.gento0o">
  <kap>gento<tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Ekstermo de anoj de gento cele al ties malaperigo;
      <ref tip="sin" cel="genocid.0o">genocido</ref>:
      <ekz>
        &ccirc;iuj religioj respondecis pri militoj, tiranismo, subpremado de la
        vero, gento<tld/>o, masakro, torturo
        <fnt>
          <aut>W. H. Simcock</aut>
          <vrk>Kontra&ubreve;dira koncepto de la dio (2)</vrk>
          <bib>Monato</bib>
          <lok>2002:1, p. 6a</lok>
        </fnt>.
      </ekz>
