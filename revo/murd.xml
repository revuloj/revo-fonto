<drv mrk="murd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="murd.0i.JUR">
    <dif>
      Intence krime <ref tip="super" cel="mort.0igi.malvivigi">mortigi</ref>:
      <ekz>
        oni <tld/>is lian edzinon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i je nomo de dio
        <fnt>
          <aut>S. Maul</aut>
          <vrk>Murdi je nomo de dio</vrk>,
          <bib>Monato</bib>
          <lok>2001:10, p. 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &scirc;atus <tld/>i la a&ccirc;an koboldon, kiu inspiras al vi tiel &gcirc;enajn hipotezojn
        <fnt>
          <bib>ChV</bib>
          <lok>28</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas tro da kialoj por <tld/>i lin, oni neniam kredos min senkulpa
        <fnt>
          <bib>CKv</bib>
          <lok>13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>mordovat <klr>(hovor.)</klr></trd>,
    <trd>vra&zcaron;dit</trd>,
    <trd>zavra&zcaron;dit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ermorden</trd>,
    <trd>umbringen <klr>(ermorden)</klr></trd>,
    <trd>morden</trd>
  </trdgrp>
  <trd lng="en">murder</trd>
  <trd lng="es">asesinar</trd>
  <trdgrp lng="fr">
    <trd>assassiner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyilkol</trd>,
    <trd>meggyilkol</trd>,
    <trd>&ouml;l</trd>,
    <trd>meg&ouml;l</trd>
  </trdgrp>
  <trd lng="io">ocidar</trd>
  <trd lng="it">assassinare</trd>
  <trdgrp lng="ja">
    <trd>&#27578;&#23475;&#12377;&#12427;  <pr>&#12373;&#12388;&#12364;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#27578;&#12377;  <pr>&#12371;&#12429;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">vermoorden</trd>
  <trd lng="pl">mordowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>assassinar</trd>,
    <trd>matar <klr tip="amb">(assassinar)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</ind></trd>
  </trdgrp>
  <trd lng="sk">zavra&zcaron;di&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_i;&c_n;&c_i;&c_t;&c_i; &c_u;&c_b;&c_i;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="vo">sasen&ouml;n</trd>
  <trdgrp lng="zh">
    <trd>&#26432;&#23475; <pr>sh&amacron;h&agrave;i</pr></trd>,
    <trd>&#27578;&#23475; <pr>sh&amacron;h&agrave;i</pr></trd>,
    <trd>&#35584;&#27578; <pr>m&oacute;ush&amacron;</pr></trd>,
    <trd>&#35851;&#26432; <pr>m&oacute;ush&amacron;</pr></trd>,
    <trd>&#34987;&#26432; <pr>b&egrave;ish&amacron;</pr></trd>,
    <trd>&#34987;&#27578; <pr>b&egrave;ish&amacron;</pr></trd>,
    <trd>&#20807;&#27578; <pr>xi&omacron;ngsh&amacron;</pr></trd>,
    <trd>&#20982;&#26432; <pr>xi&omacron;ngsh&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc mrk="murd.0o.JUR">
    <dif>
      Intenca krima mortigo de homo:
      <ekz>
        <klr>[li]</klr> perfidis lin <klr>[&#8230;]</klr> kaj elmetis al <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o maskita kiel akcidento
        <fnt>
          <bib>ChB</bib>
          <lok>10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la judoj tute ne kulpigas la tutan polan popolon pri la okazintaj
        <tld/>oj
        <fnt>
          <aut>Z. Banet-Fornalowa</aut>
          <vrk>Kadi&scirc; por Jedwabne</vrk>
          <bib>Monato</bib>
          <lok>2002:1, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>mord <klr>(hovor.)</klr></trd>,
    <trd>vra&zcaron;da</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mord</trd>,
    <trd>Ermordung</trd>
  </trdgrp>
  <trd lng="en">murder</trd>
  <trdgrp lng="es">
    <trd>asesinato</trd>,
    <trd>homicidio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assassinat</trd>,
    <trd>homicide<klr> (meurtre)</klr></trd>,
    <trd>meurtre</trd>
  </trdgrp>
  <trd lng="hu">gyilkoss&aacute;g</trd>
  <trd lng="io">ocido</trd>
  <trd lng="it">omicidio</trd>
  <trdgrp lng="ja">
    <trd>&#27578;&#23475;  <pr>&#12373;&#12388;&#12364;&#12356;</pr></trd>,
    <trd>&#27578;&#20154;  <pr>&#12373;&#12388;&#12376;&#12435;</pr></trd>,
    <trd>&#20154;&#27578;&#12375;  <pr>&#12402;&#12392;&#12372;&#12429;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">moord</trd>
  <trdgrp lng="pl">
    <trd>mord</trd>,
    <trd>morderstwo</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">vra&zcaron;da</trd>
  <trd lng="uk">&c_u;&c_b;&c_i;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#35584;&#27578; <pr>m&oacute;ush&amacron;</pr></trd>,
    <trd>&#35851;&#26432; <pr>m&oacute;ush&amacron;</pr></trd>,
    <trd>&#20807;&#27578; <pr>xi&omacron;ngsh&amacron;</pr></trd>,
    <trd>&#20982;&#26432; <pr>xi&omacron;ngsh&amacron;</pr></trd>,
    <trd>&#26432;&#20154; <pr>sh&amacron;r&eacute;n</pr></trd>,
    <trd>&#27578;&#20154; <pr>sh&amacron;r&eacute;n</pr></trd>,
    <trd>&#35584;&#27578;&#26696; <pr>m&oacute;ush&amacron;&agrave;n</pr></trd>,
    <trd>&#35851;&#26432;&#26696; <pr>m&oacute;ush&amacron;&agrave;n</pr></trd>,
    <trd>&#26432;&#23475; <pr>sh&amacron;h&agrave;i</pr></trd>,
    <trd>&#27578;&#23475; <pr>sh&amacron;h&agrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murd.0into">
  <kap><tld/>into</kap>
  <snc mrk="murd.0into.JUR">
    <dif>
      Plenuminto de <tld/>o:
      <ekz>
        oni persekutas la <tld/>intojn de sinjorego
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu alia spuro de la <tld/>into rimarki&gcirc;is
        <fnt>
          <bib>ChR</bib>
          <lok>ABZ de murdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        organizintoj kiuj financis la <tld/>intojn <klr>[&#8230;]</klr> mem ne estis
        kondamnitaj
        <fnt>
          <aut>M. Marko</aut>
          <vrk>Sekretaj organizantoj</vrk>,
          <bib>Monato</bib>
          <lok>2002:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_o;&c_j;&c_c;&c_a;</trd>
  <trd lng="cs">vrah</trd>
  <trd lng="de">M&ouml;rder</trd>
  <trd lng="en">murderer</trd>
  <trd lng="es">asesino</trd>
  <trdgrp lng="fr">
    <trd>assassin</trd>,
    <trd>homicide<klr> (meurtrier)</klr></trd>,
    <trd>meurtrier</trd>
  </trdgrp>
  <trd lng="hu">gyilkos</trd>
  <trd lng="it">omicida</trd>
  <trdgrp lng="ja">
    <trd>&#27578;&#20154;&#32773;  <pr>&#12373;&#12388;&#12376;&#12435;&#12375;&#12419;</pr></trd>,
    <trd>&#27578;&#20154;&#29359;  <pr>&#12373;&#12388;&#12376;&#12435;&#12399;&#12435;</pr></trd>,
    <trd>&#20154;&#27578;&#12375;  <pr>&#12402;&#12392;&#12372;&#12429;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">moordenaar</trd>
  <trd lng="pl">morderca</trd>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_j;&c_c;&c_a;</trd>
  <trd lng="sk">vrah</trd>
  <trd lng="uk">&c_u;&c_b;&c_i;&c_v;&c_c;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#20807;&#25163; <pr>xi&omacron;ngsh&#466;u</pr></trd>,
    <trd>&#20982;&#25163; <pr>xi&omacron;ngsh&#466;u</pr></trd>,
    <trd>&#26432;&#20154;&#29359; <pr>sh&amacron;r&eacute;nf&agrave;n</pr></trd>,
    <trd>&#27578;&#20154;&#29359; <pr>sh&amacron;r&eacute;nf&agrave;n</pr></trd>,
    <trd>&#20807;&#29359; <pr>xi&omacron;ngf&agrave;n</pr></trd>,
    <trd>&#20982;&#29359; <pr>xi&omacron;ngf&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murd.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="murd.0isto.JUR">
    <dif>
      Homo, kiu kontra&ubreve; pago, la&ubreve;mende <tld/>as:
      <ekz>
        ili suspektas ne &ccirc;iujn, sed distingas nur ses suspektatojn kaj
        konscience kaj celkonscie ser&ccirc;as motivojn (anka&ubreve;
        &ndash; alibiojn) de l' <tld/>isto
        <fnt>
          <aut>N. Stepanov</aut>
          <vrk>Dinamika, vigla, facila, simpla</vrk>
          <bib>Monato</bib>
          <lok>1995:6, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_o;&c_j;&c_c;&c_a; <klr>(&c_n;&c_a;&c_j;&c_m;&c_a;&c_n;&c_y;, &c_p;&c_r;&c_a;&c_f;&c_e;&c_s;&c_ib;&c_j;&c_n;&c_y;)</klr></trd>,
    <trd>&c_k;&c_ib;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mord&yacute;&rcaron;</trd>,
    <trd>vrah</trd>
  </trdgrp>
  <trd lng="de">Auftragsm&ouml;rder</trd>
  <trdgrp lng="en">
    <trd>murderer</trd>,
    <trd>assassin</trd>
  </trdgrp>
  <trd lng="es">asesino</trd>
  <trdgrp lng="fr">
    <trd>tueur</trd>
  </trdgrp>
  <trd lng="hu">b&eacute;rgyilkos</trd>
  <trd lng="it">sicario</trd>
  <trdgrp lng="ja">
    <trd>&#27578;&#12375;&#23627;  <pr>&#12371;&#12429;&#12375;&#12420;</pr></trd>,
    <trd>&#21050;&#23458;  <pr>&#12375;&#12363;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">huurmoordenaar</trd>
  <trdgrp lng="pl">
    <trd>zawodowy <ind>morderca</ind></trd>,
    <trd>p&lstroke;atny <ind>morderca</ind></trd>,
    <trd>assasin</trd>,
    <trd>kiler <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
