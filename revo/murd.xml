<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: murd.xml,v 1.36 2022/06/17 13:00:17 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>murd</rad>/i
</kap>

<drv mrk="murd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="murd.0i.JUR">
    <dif>
      Intence krime mortigi:
      <ekz>
        <tld/>i je nomo de dio
        <fnt>
          <aut>S. Maul</aut>
          <vrk>Murdi je nomo de dio</vrk>,
          <bib>Monato</bib>
          <lok>2001:10, p. 5a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>mordovat <klr>(hovor.)</klr></trd>,
    <trd>vra&zcaron;dit</trd>,
    <trd>zavra&zcaron;dit</trd>
  </trdgrp>
  <trd lng="de">morden</trd>
  <trd lng="en">murder</trd>
  <trd lng="es">asesinar</trd>
  <trdgrp lng="fr">
    <trd>assassiner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyilkol</trd>,
    <trd>meggyilkol</trd>,
    <trd>&ouml;l</trd>,
    <trd>meg&ouml;l</trd>
  </trdgrp>
  <trd lng="io">ocidar</trd>
  <trd lng="it">assassinare</trd>
  <trd lng="nl">vermoorden</trd>
  <trd lng="pl">mordowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>assassinar</trd>,
    <trd>matar <klr tip="amb">(assassinar)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</ind></trd>
  </trdgrp>
  <trd lng="sk">zavra&zcaron;di&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_i;&#x43D;&c_i;&c_t;&c_i; &c_u;&c_b;&c_i;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
  <trd lng="vo">sasen&ouml;n</trd>
</drv>

<drv mrk="murd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc mrk="murd.0o.JUR">
    <dif>
      Intenca krima mortigo de homo:
      <ekz>
        la judoj tute ne kulpigas la tutan polan popolon pri la okazintaj 
        <tld/>oj
        <fnt>
          <aut>Z. Banet-Fornalowa</aut>
          <vrk>Kadi&scirc; por Jedwabne</vrk>
          <bib>Monato</bib>
          <lok>2002:1, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>mord <klr>(hovor.)</klr></trd>,
    <trd>vra&zcaron;da</trd>
  </trdgrp>
  <trd lng="de">Mord</trd>
  <trd lng="en">murder</trd>
  <trdgrp lng="es">
    <trd>asesinato</trd>,
    <trd>homicidio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assassinat</trd>,
    <trd>homicide<klr> (meurtre)</klr></trd>,
    <trd>meurtre</trd>
  </trdgrp>
  <trd lng="hu">gyilkoss&aacute;g</trd>
  <trd lng="io">ocido</trd>
  <trd lng="it">omicidio</trd>
  <trd lng="nl">moord</trd>
  <trdgrp lng="pl">
    <trd>mord</trd>,
    <trd>morderstwo</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">vra&zcaron;da</trd>
  <trd lng="uk">&c_u;&c_b;&c_i;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>
</drv>

<drv mrk="murd.0into">
  <kap><tld/>into</kap>
  <snc mrk="murd.0into.JUR">
    <dif>
      Plenuminto de <tld/>o:
      <ekz>
        organizintoj kiuj financis la <tld/>intojn kaj kiuj mem ne estis
        kondamnitaj
        <fnt>
          <aut>M. Marko</aut>
          <vrk>Sekretaj organizantoj</vrk>,
          <bib>Monato</bib>
          <lok>2002:2, p. 6a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_o;&c_j;&c_c;&c_a;</trd>
  <trd lng="cs">vrah</trd>
  <trd lng="de">Ermordeter</trd>
  <trd lng="en">murderer</trd>
  <trd lng="es">asesino</trd>
  <trdgrp lng="fr">
    <trd>assassin</trd>,
    <trd>homicide<klr> (meurtrier)</klr></trd>,
    <trd>meurtrier</trd>
  </trdgrp>
  <trd lng="hu">gyilkos</trd>
  <trd lng="it">omicida</trd>
  <trd lng="nl">moordenaar</trd>
  <trd lng="pl">morderca</trd>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_j;&c_c;&c_a;</trd>
  <trd lng="sk">vrah</trd>
  <trd lng="uk">&c_u;&c_b;&c_i;&c_v;&c_c;&#x44F;</trd>
</drv>

<drv mrk="murd.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="murd.0isto.JUR">
    <dif>
      Homo, kiu kontra&ubreve; pago, la&ubreve;mende <tld/>as:
      <ekz>
        ili suspektas ne &ccirc;iujn, sed distingas nur ses suspektatojn kaj
        konscience kaj celkonscie ser&ccirc;as motivojn (anka&ubreve;
        &ndash; alibiojn) de l' <tld/>isto
        <fnt>
          <aut>N. Stepanov</aut>
          <vrk>Dinamika, vigla, facila, simpla</vrk>
          <bib>Monato</bib>
          <lok>1995:6, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_o;&c_j;&c_c;&c_a; <klr>(&c_n;&c_a;&c_j;&c_m;&c_a;&c_n;&c_y;, &c_p;&c_r;&c_a;&c_f;&c_e;&c_s;&c_ib;&c_j;&c_n;&c_y;)</klr></trd>,
    <trd>&c_k;&c_ib;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mord&yacute;&rcaron;</trd>,
    <trd>vrah</trd>
  </trdgrp>
  <trd lng="de">M&ouml;rder</trd>
  <trdgrp lng="en">
    <trd>murderer</trd>,
    <trd>assassin</trd>
  </trdgrp>
  <trd lng="es">asesino</trd>
  <trdgrp lng="fr">
    <trd>tueur</trd>
  </trdgrp>
  <trd lng="hu">b&eacute;rgyilkos</trd>
  <trd lng="it">sicario</trd>
  <trd lng="nl">huurmoordenaar</trd>
  <trdgrp lng="pl">
    <trd>zawodowy <ind>morderca</ind></trd>,
    <trd>p&lstroke;atny <ind>morderca</ind></trd>,
    <trd>assasin</trd>,
    <trd>kiler <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y;&c_j; <ind>&c_u;&c_b;&c_i;&c_j;&c_c;&c_a;</ind></trd>,
    <trd>&c_k;&c_i;&c_l;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">vrah</trd>
  <trd lng="uk">&#x43D;&c_a;&c_j;&#x43C;&c_a;&#x43D;&c_i;&c_j; &c_u;&c_b;&c_i;&c_v;&c_c;&#x44F;</trd>
</drv>

<drv mrk="murd.amas0i">
  <kap>amas<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="M"/>i multajn homojn:
      <ekz>
        kia kruela dio devas esti, je kies nomo krimuloj brutalege 
        amas<tld/>as
        <fnt>
          <aut>S. Maul</aut>
          <vrk>Murdi je nomo de dio</vrk>
          <bib>Monato</bib>
          <lok>2001:10, p. 5a</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="masakr.0i">masakri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_o;&c_n;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_je; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>massakrieren</trd>,
    <trd>massenmorden</trd>
  </trdgrp>
  <trd lng="en">mass murder</trd>
  <trd lng="es">masacrar</trd>
  <trdgrp lng="fr">
    <trd>massacrer<klr> (assassiner en masse)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&ouml;meggyilkoss&aacute;got k&ouml;vet el</trd>
  <trdgrp lng="it">
    <trd>massacrare</trd>
    <trd>far strage</trd>
  </trdgrp> 
  <trd lng="nl">een <ind>massamoord</ind> plegen</trd>
  <trd lng="pl">dokonywa&cacute; <ind>ludob&oacute;jstwa</ind></trd>
</drv>

<drv mrk="murd.amas0isto">
  <kap>amas<tld/>isto</kap>
  <snc>
    <dif>
      Plenuminto a&ubreve; ordoninto de amas<tld/>o:
      <ekz>
        &ccirc;u ili mislernis, &ccirc;ar kronikistoj raportis pri
        &leftquot;grandaj viroj&rightquot;,
        kiuj vere ne malofte estis amas<tld/>istoj
        <fnt>
          <aut>J. Ipfelkofer</aut>
          <vrk>Maksimo ankora&ubreve; lernenda</vrk>
          <bib>Monato</bib>
          <lok>2001:3, p. 7a</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Massenm&ouml;rder</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>boucher<klr> (massacreur)</klr></trd>,
      <trd>massacreur</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_w;&c_c;&c_a; &c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_g;&c_a; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trd lng="en">mass murderer</trd>
  <trd lng="hu">t&ouml;meggyilkos</trd>  
  <trdgrp lng="it">
    <trd>massacratore</trd>
    <trd>stragista</trd>
  </trdgrp> 
  <trd lng="nl">massamoordenaar</trd>
  <trd lng="pl">ludob&oacute;jca</trd>
</drv>

<drv mrk="murd.amas0o">
  <kap>amas<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="M"/>ado de multaj homoj:
      <ekz>
        sub emocia sperto pri amas<tld/>oj usonanoj kaj alinaciuloj aprobas
        militon
        <fnt><aut>M. Marko</aut>
        <vrk>Sekretaj organizantoj</vrk>
        <bib>Monato</bib>
        <lok>2002:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_je; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Massaker</trd>,
    <trd>Massenmord</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mass murder</trd>,
    <trd>massacre</trd>
  </trdgrp>
  <trd lng="es">masacre</trd>
  <trdgrp lng="fr">
    <trd>massacre<klr> (meurtre de masse)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&ouml;meggyilkoss&aacute;g</trd>
  <trdgrp lng="it">
    <trd>strage</trd>
    <trd>massacro</trd>
  </trdgrp>
  <trd lng="nl">massamoord</trd>
  <trd lng="pl">ludob&oacute;jstwo</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_o;&c_v;&c_o;&c_je; <ind>&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</ind></trd>
  <trdgrp lng="sk">
    <trd>hromadn&aacute; vra&zcaron;da</trd>,
    <trd>masakra</trd>
  </trdgrp>
</drv>

<drv mrk="murd.gento0o">
  <kap>gento<tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Ekstermo de anoj de gento cele al ties malaperigo;
      <ref tip="sin" cel="genocid.0o">genocido</ref>:
      <ekz>
        &ccirc;iuj religioj respondecis pri militoj, tiranismo, subpremado de la 
        vero, gento<tld/>o, masakro, torturo
        <fnt>
          <aut>W. H. Simcock</aut>
          <vrk>Kontra&ubreve;dira koncepto de la dio (2)</vrk>
          <bib>Monato</bib>
          <lok>2002:1, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_n;&c_a;&c_c;&c_y;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Genozid</trd>,
    <trd>V&ouml;lkermord</trd>
  </trdgrp>
  <trd lng="en">genocide</trd>
  <trd lng="es">genocidio</trd> 
  <trdgrp lng="fr">
    <trd>g&eacute;nocide</trd>,
    <trd>holocauste<klr> (g&eacute;nocide)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;pirt&aacute;s</trd>,
    <trd>genoc&iacute;cium</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>genocidio</trd>
    <trd>eccidio</trd>
    <trd>sterminio</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>volkerenmoord</trd>,
    <trd>genocide</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zag&lstroke;ada</trd>,
    <trd>eksterminacja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_n;&c_o;&c_c;&c_i;&c_d;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_o;&c_k;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="murd.rabo0o">
  <kap>rabo<tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <tld lit="M"/>o farita okaze de rabado, por sendefendigi a&ubreve;
      silentigi la viktimon:
      <ekz>
        unua tezo estas, ke povas temi pri rabo<tld/>o; sed la fakto, ke 
        &scirc;ajne nenio estis forprenita, e&ccirc; ne la mono, donas 
        malmulte da subteno al tia ideo. <tld/>o
        <fnt>
          <aut>W. M. A. De Smet</aut>: 
          <vrk>Kiu murdis Dian Fossey?</vrk>, 
          <bib>Monato</bib>
          <lok>1995:12, p. 23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a; &c_z; &c_m;&c_e;&c_t;&c_a;&c_j; &c_r;&c_a;&c_b;&c_u;&c_n;&c_k;&c_u;</trd>
  <trd lng="cs">loupe&zcaron;n&aacute; vra&zcaron;da</trd>
  <trd lng="de">Raubmord</trd>
  <trdgrp lng="fr">
    <trd>crime <klr> (meurtre)</klr> crapuleux</trd>
  </trdgrp>
  <trd lng="hu">rabl&oacute;gyilkoss&aacute;g</trd>
  <trd lng="nl">roofmoord</trd>
  <trd lng="pl">mord rabunkowy</trd>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o; &c_s; &c_c;&c_je;&c_l;&c_mol;&c_ju; &c_o;&c_g;&c_r;&c_a;&c_b;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">l&uacute;pe&zcaron;n&aacute; vra&zcaron;da</trd>
</drv>

<drv mrk="murd.seri0isto">
  <kap>
    seri<tld/>isto,
    <var><kap>seria <tld/>isto</kap></var>
  </kap>
  <snc mrk="murd.seri0isto.JUR">
    <dif>
      Tiu, kiu <tld/>is plurajn homojn, ofte obsede, ne nepre por profito:
      <ekz>
        Pe&ccirc;jo la Murdisto estas la plej fama seria <tld/>isto de Brazilo
        <fnt>
          <aut>La Malfamuloj</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_os3bkvzBXf0/">Mondaj Sekretoj Ne Tiom
          Sekretaj #2</url></vrk>
          <lok>2022-01-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_r;&c_y;&c_j;&c_n;&c_y; <ind>&c_z;&c_a;&c_b;&c_o;&c_j;&c_c;&c_a;</ind></trd>
  <trd lng="en">serial killer</trd>
  <trd lng="fr">tueur en s&eacute;rie</trd>
</drv>

<drv mrk="murd.vengxo0o">
  <kap>ven&gcirc;o<tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <tld lit="M"/>o plenumita ven&gcirc;e kontra&ubreve; <tld/>into:
      <ekz>
        la <tld/>o de la hinduoj lan&ccirc;is ondon da 
        ven&gcirc;o<tld/>oj
        <fnt>
        <aut>A.Giridhar Rao</aut>
        <vrk>La koloro de Holi</vrk>
        <bib>Monato</bib>
        <lok>2002:6, p. 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a; &c_z; &c_p;&c_o;&c_m;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_w;&c_n;&c_a;&c_ja; <ind>&c_p;&c_o;&c_m;&c_s;&c_t;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Blutrache</trd>
  <trdgrp lng="en">
    <trd>vengeance murder</trd>,
    <trd>blood revenge</trd>,
    <trd>vendetta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>meurtre de <ind>vendetta</ind></trd>
  </trdgrp>
  <trd lng="hu">v&eacute;rbossz&uacute;</trd>
  <trdgrp lng="it">
    <trd>faida</trd>
    <trd>vendetta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bloedwraak</trd>,
    <trd>vendetta</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wendeta</trd>,
    <trd>wr&oacute;&zdot;da</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>assassinato por vingan&ccedil;a</trd>,
   <trd>vendeta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_v;&c_n;&c_a;&c_ja; &c_m;&c_je;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
