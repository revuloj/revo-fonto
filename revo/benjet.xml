<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>benjet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="benjet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frit.0ajxo">Frita&jcirc;o</ref> el franda&jcirc;o &ndash;
      ekzemple frukto,
      viandopeco... &ndash; envolvita en pasto:
      <ekz>
        ekzistas personoj, kiuj skribas kaj eldonas libron
        facile kaj sen&gcirc;ene kvaza&ubreve; ili farus <tld/>ojn
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 3a, p.
        412a-413a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="pt">bolinho</trd>
  <trd lng="fr">beignet</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>

