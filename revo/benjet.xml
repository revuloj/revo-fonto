<art mrk="$Id: benjet.xml,v 1.15 2022/03/12 20:16:57 revo Exp $">
<kap>
  <rad>benjet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="benjet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frit.0ajxo">Frita&jcirc;o</ref> el
      franda&jcirc;o &ndash;
      ekzemple frukto,
      viandopeco... &ndash; envolvita en pasto:
      <ekz>
        ekzistas personoj, kiuj skribas kaj eldonas libron
        facile kaj sen&gcirc;ene kvaza&ubreve; ili farus <tld/>ojn
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 3a, p.
        412a-413a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_n;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="cs">kobliha s n&aacute;divkou</trd>
  <trdgrp lng="de">
    <trd>Krapfen</trd>,
    <trd>Beignet</trd>
  </trdgrp>
  <trd lng="en">beignet</trd>
  <trd lng="es">bu&ntilde;uelo</trd>
  <trd lng="fr">beignet</trd>
  <trd lng="he">&#x5EA;&#x5E4;&#x5D5;&#x5D7; &#x5D1;&#x5DC;&#x5D1;&#x5D9;&#x5D1;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;lt&ouml;tt <ind>f&aacute;nk</ind></trd>,
    <trd>pirog</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beignet</trd>,
    <trd>smoutebol</trd>,
    <trd>oliebol</trd>
  </trdgrp>
  <trd lng="pl">p&aogonek;czek</trd>
  <trd lng="pt">bolinho</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;i&scaron;ka s plnkou</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43B;&#x430;&#x434;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x430;&#x43C;&#x43F;&#x443;&#x448;&#x43A;&#x430;</trd>,
    <trd>&#x43C;&#x43B;&#x438;&#x43D;&#x447;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

