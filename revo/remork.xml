<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>remork</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="remork.0o">
  <kap><tld/>o</kap>
  <snc mrk="remork.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <ref tip="dif" cel="vetur.post0ilo.TRA">postveturilo</ref>
    <ekz>
      la tieaj kulturistoj rikoltas tritikon plenigante
      <tld/>ojn
      <fnt>
        <aut>J.-J. Moro</aut>,
        <vrk>Periplo tra la Baltaj Landoj</vrk>,
        <lok><url ref="http://eomikelo.free.fr/lukoj/luko23.pdf">
        Turan&gcirc;ia Luko, 2011:3 (23)</url></lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="fr">remorque</trd>
  <trdgrp lng="hu">
    <trd>ut&aacute;nfut&oacute;</trd>,
    <trd>p&oacute;tkocsi</trd>
  </trdgrp>
  <trd lng="pl">przyczepa</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_je;&c_p;</trd>
  <trd lng="sv">sl&auml;p</trd>
  <trd lng="tr">r&ouml;mork</trd>
</drv>

<drv mrk="remork.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="remork.0i.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Tiradi kiel postveturilon:
      <ekz>
        e&ccirc; tre junaj infanoj povas partopreni
        side en trenveturiloj bicikle <tld/>ataj
        de siaj gepatroj
        <fnt>
          <aut>Johano</aut>,
          <vrk>Radio Verda</vrk>, 
          <lok><url ref="">RV102</url>, 2007-05-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">remorquer</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>