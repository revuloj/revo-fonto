<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filade.xml,v 1.22 2022/03/20 10:19:22 revo Exp $">
<kap>
  <rad>filadelf</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="filade.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="hidrangeac.0oj">hidrangeacoj</ref>
      (<trd lng="la">Philadelphus</trd>), arbedo kun blankaj kaj tre
      bonodoraj floroj:
      <ekz>
        <tld/>o estas genro de falfoliaj arbustoj kun simplaj, kontra&ubreve;e duopaj
        folioj, kun multaj, ofte blankaj kaj odoraj floroj en apeksaj cumoj
        a&ubreve; mallongaj grapoloj, kaj kun ledecaj kapsuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21015">Filadelfo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/2/2a/Philadelphus_coronarius_15355.JPG/640px-Philadelphus_coronarius_15355.JPG" prm="CC0">floroj kaj folioj (Philadelphus coronarius)
       <fnt>
         <aut>I.S&aacute;&ccaron;ek, senior, propra verko</aut>
         <lok><url ref="&WCW;/File:Philadelphus_coronarius_15355.JPG">&WCW;/File:Philadelphus_coronarius_15355.JPG</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_ja;&c_z;&c_mol;&c_m;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Pfeiffenstrauch</trd>,
    <trd>Falscher <ind>Jasmin</ind></trd>
  </trdgrp>
  <trd lng="fr">seringa</trd>
  <trd lng="hu">jezs&aacute;men</trd>
  <trd lng="pl">ja&sacute;minowiec</trd>
  <trdgrp lng="pt">
    <trd>siringa</trd>,
    <trd>lil&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_d;&c_o;&c_v;&c_y;&c_j; &c_zh;&c_a;&c_s;&c_m;&c_i;&c_n;</trd>,
    <trd>&c_ch;&c_u;&c_b;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x430;&#x434;&#x43E;&#x432;&#x438;&#x439; &#x436;&#x430;&#x441;&#x43C;&#x438;&#x43D;</trd>
</drv>

</art>
