<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>filadelf</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="filade.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="hidrangeac.0oj">hidrangeacoj</ref>
      (<trd lng="la">Philadelphus</trd>), arbedo kun blankaj kaj tre
      bonodoraj floroj:
      <ekz>
        <tld/>o estas genro de falfoliaj arbustoj kun simplaj, kontra&ubreve;e duopaj
        folioj, kun multaj, ofte blankaj kaj odoraj floroj en apeksaj cumoj
        a&ubreve; mallongaj grapoloj, kaj kun ledecaj kapsuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21015">Filadelfo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/filade.jpg">
    floroj kaj folioj
    <klr>(Philadelphus coronarius)</klr>
  </bld>
  <trdgrp lng="de">
    <trd>Pfeiffenstrauch</trd>,
    <trd>Falscher <ind>Jasmin</ind></trd>
  </trdgrp>
  <trd lng="fr">seringa</trd>
  <trd lng="hu">jezs&aacute;men</trd>
  <trd lng="pl">ja&sacute;minowiec</trd>
  <trdgrp lng="pt">
    <trd>siringa</trd>,
    <trd>lil&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_d;&c_o;&c_v;&c_y;&c_j; &c_zh;&c_a;&c_s;&c_m;&c_i;&c_n;</trd>,
    <trd>&c_ch;&c_u;&c_b;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

</art>
