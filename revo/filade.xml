<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filadelf</rad>/o
</kap>
<drv mrk="filade.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="filade.0acoj"><tld/>acoj</ref>,
      (<trd lng="la">Philadelphus</trd>), arbeto kun blankaj kaj tre bonodoraj
      floroj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pfeiffenstrauch</trd>,
    <trd>Falscher <ind>Jasmin</ind></trd>
  </trdgrp>
</drv>
<drv mrk="filadelf.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de arbedoj (<trd lng="la">Philadelphaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Pfeiffenstrauchgew&auml;chse</trd>
</drv>
</art>
