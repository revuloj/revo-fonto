<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filadelf</rad>/o
</kap>
<drv mrk="filade.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="filade.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Philadelphus</trd>), arbedo kun blankaj kaj tre bonodoraj
      floroj.
    </dif>
  </snc>
  <bld lok="../bld/filade.jpg">
    floroj kaj folioj
    <klr>(Philadelphus coronarius)</klr>
  </bld>
  <trdgrp lng="de">
    <trd>Pfeiffenstrauch</trd>,
    <trd>Falscher <ind>Jasmin</ind></trd>
  </trdgrp>
</drv>
<drv mrk="filade.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      arbedoj (<trd lng="la">Philadelphaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Pfeiffenstrauchgew&auml;chse</trd>
</drv>
</art>
