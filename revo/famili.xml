  <trd lng="id"><ind>kepala</ind> keluarga</trd>
  <trdgrp lng="ja">
    <trd>&#x5BB6;&#x9577;  <pr>&#x304B;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4E16;&#x5E2F;&#x4E3B;  <pr>&#x305B;&#x305F;&#x3044;&#x306C;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="pl">g&lstroke;owa  <ind>rodziny</ind></trd>
