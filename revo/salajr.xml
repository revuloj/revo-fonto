<drv mrk="salajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salajr.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mono periode pagata al dungito por servado, laboro, ofico:
      <ekz>
        por la kantistoj estis difinita &ccirc;iutaga <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 11:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        alvoku la laboristojn, kaj donu al ili la <tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 20:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi maljuni&gcirc;as, vi ricevas sufi&ccirc;an emeritan <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>on por &scirc;iaj servoj mi donas al &scirc;i mian dometon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ofica <tld/>o ne sufi&ccirc;as e&ccirc; por teo kaj sukero
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li asignis la <tld/>on jaran
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ricevas <tld/>on de ducent kvindek rubloj jare
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>III</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandi&gcirc;is la infanaro kaj estis nesufi&ccirc;a la <tld/>o
        <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 2a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honora.0o">honorario</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="bo">&t_ga;&t_lata;&t_tsheg;&t_cha;&t_tsheg;</trd>
