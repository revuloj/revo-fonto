<drv mrk="salajr.krom0o">
  <kap>krom<tld/>o</kap>
  <snc mrk="salajr.krom0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ekstra pago ordinare kalkulata la&ubreve;
      interkonsento;
      <ref tip="sin" cel="bonifik.0o.EKON">bonifiko</ref>:
      <ekz>
        la regula novjara krom<tld/>o estos du j&ubreve;anoj
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>;
      </ekz>
      <ekz>
        li planis repagi monate 160&#x202F;000 enojn <klr>[&#8230;]</klr>, vintre, pro specialaj
        krom<tld/>oj, 450&#x202F;000 enojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009911.php"
              >Rifu&gcirc;intoj pro domperdo</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_e;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_o;&c_n;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odm&ecaron;na</trd>,
    <trd>pr&eacute;mie</trd>,
    <trd>p&rcaron;&iacute;platek ke mzd&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zulage <klr>(zum Gehalt)</klr></trd>,
    <trd>Sonderverg&uuml;tung</trd>,
    <trd>Bonus <klr>(zum Gehalt)</klr></trd>,
    <trd>Pr&auml;mie <klr>(zum Gehalt)</klr></trd>
  </trdgrp>
  <trd lng="en">bonus</trd>
  <trd lng="fr">prime<klr> (compl&eacute;ment de salaire)</klr></trd>
  <trdgrp lng="id">
    <trd>bonus</trd>,
    <trd>insentif</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DC;&#x30FC;&#x30CA;&#x30B9;  <pr>&#x307C;&#x30FC;&#x306A;&#12377;</pr></trd>,
    <trd>&#x8CDE;&#x4E0E;  <pr>&#12375;&#12423;&#12358;&#12424;</pr></trd>
  </trdgrp>
  <trd lng="pl">dieta</trd>
  <trdgrp lng="sk">
    <trd>odmena</trd>,
    <trd>pr&eacute;mie</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_o;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a; &c_o;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
