<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salajr</rad>/o
</kap>
<drv mrk="salajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Sumo periode pagata por servado, laboro, ofico.
    </dif>
  </snc>
  <trd lng="de">Gehalt</trd>
  <trd lng="nl">salaris</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_n;&c_mol;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Homo ricevanta <tld/>on.
    </dif>
    <refgrp tip="sin">
      <ref cel="pag">pago</ref>,
      <ref cel="rekomp.0o">rekompenco</ref>,
      <ref cel="honora.0o">honorario</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Angestellter</trd>
  <trdgrp lng="nl">
    <trd>loontrekkende</trd>,
    <trd>bediende</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Doni <tld/>on al iu:
      <ekz>
        oni ja ne devas forgesi, ke la komitatanoj ne estas homoj
        <tld/>ataj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bezahlen</trd>
  <trd lng="nl">salari&euml;ren</trd>
</drv>
</art>
