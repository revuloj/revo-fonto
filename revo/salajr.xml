<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salajr</rad>/o
</kap>
<drv mrk="salajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mono periode pagata al dungito por servado, laboro, ofico:
      <ekz>grandi&gcirc;is la infanaro kaj estis nesufi&ccirc;a la <tld/>o
      <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 2a, p. 23a</lok></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honora.0o">honorario</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="de">
      <trd>Gehalt</trd>,
      <trd>Sal&auml;r<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>salary</trd>,
    <trd>wages</trd>
  </trdgrp>
  <trd lng="es">salario</trd>
  <trdgrp lng="fr">
    <trd>salaire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&eacute;r</trd>,
    <trd>fizet&eacute;s</trd>,
    <trd>d&iacute;jaz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>salario</trd>,
    <trd>stipendio</trd>
  </trdgrp>
  <trd lng="nl">salaris</trd>
  <trdgrp lng="pt">
   <trd>sal&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_n;&c_mol;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Homo ricevanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y; &c_p;&c_r;&c_a;&c_c;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Angestellter</trd>,
    <trd>Gehaltsempf&auml;nger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>salaried person</trd>,
    <trd>employee</trd>
  </trdgrp>
  <trd lng="es">asalariado</trd>
  <trdgrp lng="fr">
    <trd>salari&eacute;<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&eacute;rmunk&aacute;s</trd>,
    <trd>fizet&eacute;sb&odblac;l &eacute;l&odblac;</trd>,
    <trd>alkalmazott</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>salariato</trd>,
    <trd>stipendiato</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loontrekkende</trd>,
    <trd>bediende</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Doni <tld/>on al iu:
      <ekz>
        oni ja ne devas forgesi, ke la komitatanoj ne estas homoj
        <tld/>ataj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dung.0i">dungi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol; &c_z;&c_a;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol; &c_p;&c_r;&c_a;&c_c;&c_u;</trd>
  </trdgrp>
  <trd lng="de">bezahlen</trd>
  <trdgrp lng="en">
    <trd>pay a salary</trd>,
    <trd>employ</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pagar un salario</trd>,
    <trd>asalariar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>salarier</trd>
  </trdgrp>
  <trd lng="hu">fizet<klr tip="ind"> (b&eacute;rt)</klr></trd>
  <trdgrp lng="it">
    <trd>salariare</trd>,
    <trd>stipendiare</trd>, 
    <trd>retribuire</trd>
  </trdgrp>
  <trd lng="nl">salari&euml;ren</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_r;&c_p;&c_l;&c_a;&c_t;&c_u;</trd>
</drv>
</art>
