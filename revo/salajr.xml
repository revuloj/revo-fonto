<drv mrk="salajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="salajr.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Doni <tld/>on al iu:
      <ekz>
        mia mastro ne havis monon por min <tld/>i
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ja ne devas forgesi, ke la komitatanoj ne estas homoj
        <tld/>ataj
        <fnt>
          <bib>OV</bib>
          <vrk><url ref="https://eo.wikisource.org/wiki/Originala_Verkaro/V/36"
              >letero al franca esperantisto, 1908-04-24</url></vrk>
          <lok>presitaj leteroj, n-ro 36</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i oficiston por TEJO
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 4 (102)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dung.0i">dungi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol; &c_z;&c_a;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol; &c_p;&c_r;&c_a;&c_c;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pagar el sou</trd>,
    <trd>assalariar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>platit mzdu</trd>,
    <trd>vypl&aacute;cet mzdu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gehalt <ind>auszahlen</ind></trd>,
    <trd>bezahlen</trd>,
    <trd>l&ouml;hnen</trd>
  </trdgrp>
  <trdgrp lng="en">
