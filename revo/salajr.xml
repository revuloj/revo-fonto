<kap>
  <ofc>*</ofc>
  <rad>salajr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="salajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salajr.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mono periode pagata al dungito por servado, laboro, ofico:
      <ekz>
        grandi&gcirc;is la infanaro kaj estis nesufi&ccirc;a la <tld/>o
        <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 2a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honora.0o">honorario</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="bo">&#xF42;&#xFB3;&#xF0B;&#xF46;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>salari</trd>,
    <trd>sou</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mzda</trd>,
    <trd>odm&ecaron;na za pr&aacute;ci</trd>,
    <trd>plat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Gehalt</trd>,
      <trd>Sal&auml;r<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>salary</trd>,
    <trd>wages</trd>
  </trdgrp>
  <trd lng="es">salario</trd>
  <trd lng="fr">salaire</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;r</trd>,
    <trd>fizet&eacute;s</trd>,
    <trd>d&iacute;jaz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>gaji</trd>,
    <trd>upah</trd>,
    <trd>salaris</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>salario</trd>,
    <trd>stipendio</trd>
  </trdgrp>
  <trd lng="nl">salaris</trd>
  <trdgrp lng="pt">
   <trd>sal&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_n;&c_mol;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mzda</trd>,
    <trd>plat</trd>,
    <trd>pl&aacute;ca</trd>,
    <trd>slu&zcaron;n&eacute;</trd>,
    <trd>v&yacute;plata</trd>,
    <trd>z&aacute;robok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x440;&#x43E;&#x431;&#x456;&#x442;&#x43D;&#x430; &#x43F;&#x43B;&#x430;&#x442;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x440;&#x43F;&#x43B;&#x430;&#x442;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x440;&#x43E;&#x431;&#x456;&#x442;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&#x43B;&#x430;&#x442;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="salajr.0ulo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Homo ricevanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y; &c_p;&c_r;&c_a;&c_c;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">assalariat</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;mezdn&iacute; pracovn&iacute;k</trd>,
    <trd>zam&ecaron;stnanec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Angestellter</trd>,
    <trd>Gehaltsempf&auml;nger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>salaried person</trd>,
    <trd>employee</trd>
  </trdgrp>
  <trd lng="es">asalariado</trd>
  <trd lng="fr">salari&eacute;<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>b&eacute;rmunk&aacute;s</trd>,
    <trd>fizet&eacute;sb&odblac;l &eacute;l&odblac;</trd>,
    <trd>alkalmazott</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>buruh</trd>,
    <trd>karyawan</trd>,
    <trd>pe<ind>kerja</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>salariato</trd>,
    <trd>stipendiato</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loontrekkende</trd>,
    <trd>bediende</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">n&aacute;mezdn&yacute; pracovn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x43B;&#x430;&#x447;&#x443;&#x432;&#x430;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x439;&#x43C;&#x430;&#x43D;&#x438;&#x439; &#x43F;&#x440;&#x430;&#x446;&#x456;&#x432;&#x43D;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="salajr.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Doni <tld/>on al iu:
      <ekz>
        oni ja ne devas forgesi, ke la komitatanoj ne estas homoj
        <tld/>ataj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dung.0i">dungi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol; &c_z;&c_a;&c_r;&c_o;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol; &c_p;&c_r;&c_a;&c_c;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pagar el sou</trd>,
    <trd>assalariar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>platit mzdu</trd>,
    <trd>vypl&aacute;cet mzdu</trd>
  </trdgrp>
  <trd lng="de">bezahlen</trd>
  <trdgrp lng="en">
    <trd>pay a salary</trd>,
    <trd>employ</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pagar un salario</trd>,
    <trd>asalariar</trd>
  </trdgrp>
  <trd lng="fr">salarier</trd>
  <trd lng="hu">fizet<klr tip="ind"> (b&eacute;rt)</klr></trd>
  <trd lng="id">meng<ind>gaji</ind></trd>
  <trdgrp lng="it">
    <trd>salariare</trd>,
    <trd>stipendiare</trd>, 
    <trd>retribuire</trd>
  </trdgrp>
  <trd lng="nl">salari&euml;ren</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_r;&c_p;&c_l;&c_a;&c_t;&c_u;</trd>
  <trd lng="sk">d&aacute;va&tcaron; mzdu</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x43B;&#x430;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43F;&#x440;&#x430;&#x446;&#x44E;</trd>,
    <trd>&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x437;&#x430;&#x440;&#x43E;&#x431;&#x456;&#x442;&#x43D;&#x443; &#x43F;&#x43B;&#x430;&#x442;&#x443;</trd>
  </trdgrp>
</drv>

<drv mrk="salajr.krom0o">
  <kap>krom<tld/>o</kap>
  <snc mrk="salajr.krom0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ekstra pago ordinare kalkulata la&ubreve; 
      interkonsento;
      <ref tip="sin" cel="bonifik.0o.EKON">bonifiko</ref>:
      <ekz>
        la regula novjara krom<tld/>o estos du j&ubreve;anoj
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_e;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_o;&c_n;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odm&ecaron;na</trd>,
    <trd>pr&eacute;mie</trd>,
    <trd>p&rcaron;&iacute;platek ke mzd&ecaron;</trd>
  </trdgrp>
  <trd lng="fr">prime<klr> (compl&eacute;ment de salaire)</klr></trd>
  <trdgrp lng="sk">
    <trd>odmena</trd>,
    <trd>pr&eacute;mie</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x43E;&#x434;&#x430;&#x442;&#x43A;&#x43E;&#x432;&#x430; &#x43E;&#x43F;&#x43B;&#x430;&#x442;&#x430;</trd>
</drv>
</art>
