<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mor.0o.MOR">
    <dif>
      &Gcirc;enerala vivmaniero kaj kutima konduto de iu popolo
      a&ubreve; persono:
      <ekz>
        <ind>alia tempo, aliaj <tld/>oj</ind>;
        <trd lng="fr">autres temps autres m&oelig;urs</trd>
        <trd lng="ru">&c_d;&c_r;&c_u;&c_g;&c_i;&c_je; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_a;, &c_d;&c_r;&c_u;&c_g;&c_i;&c_je; &c_n;&c_r;&c_a;&c_v;&c_y;</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        le&gcirc;o malaperas, <tld/>o da&ubreve;ras
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la naturo konservas sian <tld/>on, kion ajn la honto
        dirus<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari&gcirc;is <tld/>o en Izrael, ke &ccirc;iujare...<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la tradiciaj, la novaj <ind><tld/>oj</ind>;
        <trdgrp lng="ru">
          <trd>&c_n;&c_r;&c_a;&c_v;&c_y;</trd>,
          <trd>&c_o;&c_b;&c_y;&c_ch;&c_a;&c_i;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        mildigi la <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Sitte</trd>,
    <trd>Lebensart</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>general custom</trd>,
    <trd>habitual conduct</trd>,
    <trd>mores</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>costumbre</trd>,
    <trd>modo de vida</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&oelig;urs</trd>,
    <trd>style de vie</trd>,
    <trd>usages</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zede</trd>,
    <trd>gebruik<klr tip="ind"> (zede)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>costume <klr tip="amb">(de um povo)</klr></trd>,
    <trd>comportamento</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_y;&c_ch;&c_a;&c_j;</trd>
</drv>

<drv mrk="mor.bon0oj">
  <kap>bon<tld/>oj</kap>
  <snc mrk="mor.bon0oj.MOR">
    <dif>
      <tld lit="M"/>oj konformaj al la moralo a&ubreve; al la dececo.
    </dif>
    <refgrp tip="vid">
      <ref cel="ton.bon0o">bontono</ref>
    </refgrp>
  </snc>
  <trd lng="fr">bonnes m&oelig;urs</trd>
</drv>

<drv mrk="mor.sever0a">
  <kap>sever<tld/>a</kap>
  <snc mrk="mor.sever0a.MOR">
    <dif>
      Aganta skrupule la&ubreve; la moroj, sen devoji&gcirc;o,
      sen facilanimeco.
      <ekz>
        sever<tld/>a homo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">aust&egrave;re<klr> (personne, milieu)</klr></trd>
</drv>
</art>
