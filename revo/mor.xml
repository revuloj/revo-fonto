<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mor.xml,v 1.28 2025/12/15 22:33:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mor</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="mor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mor.0o.MOR">
    <dif>
      &Gcirc;enerala <ref tip="super" cel="manier.viv0o">vivmaniero</ref> kaj kutima <ref tip="super" cel="kondut.0o">konduto</ref>
      de iu popolo a&ubreve; persono:
      <ekz>
        religiaj <tld/>oj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi eduki&gcirc;is en bonaj <tld/>oj
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i metas al siaj laboristinoj nur unu postulon: nome, ke iliaj <tld/>oj estu
        senmakulaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        konforme al la &gcirc;isnuna <tld/>o, mi komencas mian parolon per <klr>[&#x2026;]</klr>
        danko al la lando, kiu gastame nin akceptis
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Las Casas</nom> predikis kontra&ubreve; la <tld/>o de fumado, kiun oni heredis de la
        kolonianoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari&gcirc;is <tld/>o en <nom>Izrael</nom>, ke &ccirc;iujare
        la filinoj de <nom>Izrael</nom> iras priplori la filinon de <nom>Jifta&hcirc;</nom>
        <fnt>
          <bib>MT</bib>
          <lok>&Jug; 11:39-40</lok>
        </fnt>;
      </ekz>
      <ekz>
        la naturo &parsl; Konservas sian moron, kion ajn &parsl; La honto dirus
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tradiciaj, la novaj <ind><tld/>oj</ind>;
        <trdgrp lng="ru">
          <trd>&c_n;&c_r;&c_a;&c_v;&c_y;</trd>,
          <trd>&c_o;&c_b;&c_y;&c_ch;&c_a;&c_i;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>alia tempo, aliaj <tld/>oj</ind>;
        <trd lng="fr">autres temps autres m&oelig;urs</trd>
        <trd lng="ru">&c_d;&c_r;&c_u;&c_g;&c_i;&c_je; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_a;, &c_d;&c_r;&c_u;&c_g;&c_i;&c_je; &c_n;&c_r;&c_a;&c_v;&c_y;</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        novaj sinjoroj &dash; novaj <tld/>oj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        le&gcirc;o malaperas, <tld/>o da&ubreve;ras
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konven1.0o.gxenerale">konvencio<sncref/></ref>,
      <ref cel="kutim.0o.MOR">kutimo<sncref/></ref>,
      <ref cel="tradic.0o">tradicio</ref>,
      <ref cel="uzanc.0o">uzanco</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>nadlo&zcaron;n&iacute; humus</trd>,
    <trd>pokryvn&yacute; humus</trd>,
    <trd>povrchov&yacute; humus</trd>,
    <trd>surov&yacute; humus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sitte</trd>,
    <trd>Brauch</trd>,
    <trd>Lebensart</trd>,
    <trd>Usus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>general custom</trd>,
    <trd>habitual conduct</trd>,
    <trd>mores</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>costumbre</trd>,
    <trd>modo de vida</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&oelig;urs</trd>,
    <trd>style de vie</trd>,
    <trd>usages</trd>
  </trdgrp>
  <trd lng="id">adat</trd>
  <trdgrp lng="ja">
    <trd>&#x98A8;&#x4FD7;  <pr>&#x3075;&#x3046;&#x305E;&#x304F;</pr></trd>,
    <trd>&#x6163;&#x7FD2;  <pr>&#x304B;&#x3093;&#x3057;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x98A8;&#x7FD2;  <pr>&#x3075;&#x3046;&#x3057;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x7FD2;&#x308F;&#x3057;  <pr>&#x306A;&#x3089;&#x308F;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zede</trd>,
    <trd>gebruik<klr tip="ind"> (zede)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obyczaj</trd>,
    <trd>zwyczaj</trd>,
    <trd>obrz&eogonek;d</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>costume <klr tip="amb">(de um povo)</klr></trd>,
    <trd>comportamento</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>obicei</trd>,
    <trd>manier&abreve;</trd>,
    <trd>rit</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_y;&c_ch;&c_a;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mrav</trd>,
    <trd>oby&ccaron;aj</trd>,
    <trd>spr&aacute;vanie sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;</trd>,
    <trd>&c_n;&c_o;&c_r;&c_o;&c_v;</trd>,
    <trd>&c_z;&c_v;&c_i;&c_ch;&c_k;&c_a; (&c_t;&c_v;&c_a;&c_r;&c_i;&c_n;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E60;&#x4FD7; <pr>x&iacute;s&uacute;</pr></trd>,
    <trd>&#x7FD2;&#x4FD7; <pr>x&iacute;s&uacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mor.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="mor.bon0a.MOR">
    <dif>
      Kiu havas bonajn <tld/>ojn, kondutas la&ubreve;orde:
      <ekz>
        tie knabinoj de bon<tld/>aj familioj vivas sendistinge
        kun eksaj karceraninoj, publikulinoj, narkotistinoj
        <fnt>
          <aut>B. Cadei</aut>,
          <vrk><url ref="http://www.esperokatolika.org/ek19811985/ek1984_10.htm#7">
          Se Dio frapetas la pordon de malliberejo</url></vrk>,
          <lok>Espero Katolika, 1984:10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_o;&c_k;&c_a;&c_m;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gesittet</trd>,
    <trd>wohlgesittet</trd>
  </trdgrp>
  <trd lng="fr">de bonnes m&oelig;urs</trd>
  <trdgrp lng="pl">
    <trd>obyczajny</trd>,
    <trd>moralny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>moral</trd>,
    <trd>modest</trd>
  </trdgrp>
</drv>

<drv mrk="mor.bon0oj">
  <kap>bon<tld/>oj, <var><kap>bona <tld/>o</kap></var></kap>
  <snc mrk="mor.bon0oj.MOR">
    <dif>
      <tld lit="M"/>oj konformaj al la moralo a&ubreve; al la dececo:
      <ekz>
        viaj gepatroj instruis vin pri bona konduto kaj bona <tld/>o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ton.bon0o">bontono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_jo;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">die guten <ind>Sitte</ind>n</trd>
  <trd lng="fr">bonnes m&oelig;urs</trd>
  <trd lng="pl">moralno&sacute;&cacute;</trd>
  <trd lng="ro">moral&abreve;</trd>
</drv>

<drv mrk="mor.facil0a">
  <kap>facil<tld/>a</kap>
  <snc mrk="mor.facil0a.MOR">
    <dif>
      Kiu ne serioze, ne respondece rilatas al homoj
      la&ubreve; la komunaj <tld/>oj:
      <ekz>
        en sia juneco <nom>Liu Bang</nom> estis facil<tld/>a kaj malatentis
        klerulojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="&CRI;chinaabc/chapter17/chapter170102.htm">
          Liu Bang &ndash; fondinto de la dinastio Okcidenta Han</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, [vidita en 2016]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_ib;&c_h; <ind>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_ja;&c_w;</ind></trd>
  <trdgrp lng="de">
    <trd>freiz&uuml;gig</trd>,
    <trd>ungezwungen</trd>,
    <trd>unbek&uuml;mmert</trd>,
    <trd>leichtf&uuml;&szlig;ig</trd>
  </trdgrp>
  <trd lng="fr">de m&oelig;urs l&eacute;g&egrave;res</trd>
  <trd lng="pl">lekkich obyczaj&oacute;w</trd>
  <trd lng="ro">maniere u&scomma;oare</trd>
</drv>

<drv mrk="mor.facil0eco">
  <kap>facil<tld/>eco</kap>
  <snc mrk="mor.facil0eco.MOR">
    <dif>
      Senzorga vivado la&ubreve; facilaj moroj,
      kun eventuala inklino al dibo&ccirc;o.
    </dif>
    <refgrp tip="ant">
      <ref cel="mor.sever0eco.MOR">sever<tld/>eco</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_ib;&c_ja; <ind>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_ib;</ind></trd>
  <trdgrp lng="de">
    <trd>Freiz&uuml;gigkeit</trd>,
    <trd>Ungezwungenheit</trd>,
    <trd>Unbek&uuml;mmertheit</trd>,
    <trd>Leichtf&uuml;&szlig;igkeit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;vergondage</trd>,
    <trd>l&eacute;g&egrave;ret&eacute; de m&oelig;urs</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezwstyd</trd>,
    <trd>lekkie maniery</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>tupeu</trd>,
    <trd>maniere u&scomma;oare</trd>
  </trdgrp>
</drv>

<drv mrk="mor.sever0a">
  <kap>sever<tld/>a</kap>
  <snc mrk="mor.sever0a.MOR">
    <dif>
      Aganta skrupule la&ubreve; principoj, sen devoji&gcirc;o,
      sen facilanimeco:
      <ekz>
        timinda, sever<tld/>a dommastrino
        <fnt>
          <bib>KrM</bib>
          <lok>1. Kiu Sin Enjungis, Devas Tiri.</lok>
        </fnt>;
      </ekz>
      <ekz>
        levi&gcirc;i de l&#x2019; re&gcirc;a kasto &gcirc;is la bramana kasto per longa sever<tld/>a vivo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;&c_h; <ind>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_ja;&c_w;</ind></trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y; <klr>(&c_u; &c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">sittenstreng</trd>
  <trd lng="fr">aust&egrave;re<klr> (personne, milieu)</klr></trd>
  <trd lng="ja">&#x6212;&#x5F8B;&#x306E;&#x53B3;&#x3057;&#x3044;  <pr>&#x304B;&#x3044;&#x308A;&#x3064;&#x306E;&#x304D;&#x3073;&#x3057;&#x3044;</pr></trd>
  <trd lng="pl">o ostrych obyczajach</trd>
  <trd lng="ro">cu moral&abreve; ascu&tcomma;it&abreve;</trd>
  <trd lng="zh">&#x6E05;&#x82E6; <pr>q&imacron;ngk&#x1D4;</pr></trd>
</drv>

<drv mrk="mor.sever0eco">
  <kap>sever<tld/>eco</kap>
  <snc mrk="mor.sever0eco.MOR">
    <dif>
      Vivado la&ubreve; striktaj principoj,
      atente al devoj prefere ol al plezuroj:
      <ekz>
        &ccirc;e tiaj personoj vidi&gcirc;as nur malafabla seriozeco, malkvietaj
        suspektoj, senkora sever<tld/>eco
        <fnt>
          <aut>Ramo</aut>,
          <vrk>Sankta Tereso de Jesu-infano</vrk>,
          <lok>Espero Katolika, 1925-05 (9), p. 142a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="mor.facil0eco.MOR">facil<tld/>eco</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="purita.0ismo.FIG">puritanismo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;&c_ja; <ind>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_ib;</ind></trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr>(&c_u; &c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">Sittenstrenge</trd>
  <trd lng="fr">aust&eacute;rit&eacute;</trd>
  <trdgrp lng="pl">
    <trd>surowo&sacute;&cacute;</trd>,
    <trd>szorstko&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>austeritate</trd>,
    <trd>ferocitate</trd>
  </trdgrp>
</drv>
</art>
