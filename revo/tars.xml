<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tars.xml,v 1.19 2021/06/16 06:38:30 revo Exp $">
<kap>
  <rad>tars</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tars.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="tars.0o.de_piedo">
    <dif>
      La meza malanta&ubreve;a parto de la <ref tip="malprt" cel="pied.0o.ANA">piedo</ref>,
      konsistanta el sep ostoj kaj al kiu estas artikitaj la fingraj ostoj.
    </dif>
  </snc>
  <snc mrk="tars.0o.de_palpebro">
    <dif>
      Volbita forti&gcirc;o de la <ref tip="malprt" cel="palpeb.0o.ANA">palpebro</ref>:
      <ekz>
        la efiko, kiun faras sur la konjunktivon la konstanta kuntu&scirc;i&gcirc;ado kun
        la aero, montri&gcirc;as la plej bone &ccirc;e la ektropio, &ccirc;e kiu la neka&scirc;ita 
        konjunktivo de la <tld/>o fari&gcirc;as forte ru&gcirc;a kaj dika, velursimila a&ubreve; e&ccirc; tuberplena
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lidger&uuml;st</trd>
    <trd lng="la">Tarsus palpebrae</trd>
  </snc>
  <ref tip="hom" cel="tars1.0o">Tarso (urbo)</ref>
  <trdgrp lng="bg">
    <trd>&c_z;&c_a;&c_d;&c_n;&c_o;&c_h;&c_o;&c_d;&c_i;&c_l;&c_n;&c_i;&c_t;&c_je; &c_k;&c_o;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_z;&c_a;&c_l;&c_n;&c_i; &c_k;&c_o;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_z;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">tars</trd>
  <trd lng="cs">z&aacute;n&aacute;rt&iacute;</trd>
  <trdgrp lng="de">
    <trd>Tarsus</trd>,
    <trd>Fu&szlig;wurzel</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>tarsus</trd>, <trd>bony tarsus</trd></trdgrp>
  <trd lng="fr">tarse</trd>
  <trd lng="hu">l&aacute;bt&odblac;</trd>
  <trd lng="it">tarso</trd>
  <trd lng="nl">voetwortel</trd>
  <trdgrp lng="pl">
    <trd>st&eogonek;p</trd>,
    <trd>ko&sacute;ci <ind>st&eogonek;pu</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tarso</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_l;&c_ju;&c_s;&c_n;&c_a;</trd>
  <trd lng="sk">priehlavok <klr>(na nohe)</klr></trd>
</drv>
</art>
