<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nebuloz</rad>/o
</kap>
<drv mrk="nebulo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Giganta nubo el gaso kaj mikroskopaj polveroj, kiu okupas
      difinitan lokon en nia galaksio:
      <ekz>la Kraba <tld/>o.</ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="galaksi.0o.stelamaso">galaksio</ref>
    <ekz>
      la Lakta Vojo estas <tld/>o.
    </ekz>
  </snc>
  <trd 
lng="be">&c_t;&c_u;&c_m;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Nebel</trd>,
    <trd>Sternennebel</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&phi;&epsilon_ton;&lambda;&omega;&my;&alfa;</trd>
  <trd lng="hu">csillagk&ouml;d</trd>
  <trd lng="nl">nevelvlek</trd>
  <trd lng="pt">nebulosa</trd>
  <trd lng="ru">&c_t;&c_u;&c_m;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
