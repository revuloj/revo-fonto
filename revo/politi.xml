<drv mrk="politi.0o">
  <kap><tld/>o</kap>
  <snc mrk="politi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Scienco a&ubreve; arto rilata al la regado de regno:
      <ekz>
        la <tld/>o de la re&gcirc;a partio
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 15a</lok></fnt>;
      </ekz>
      <ekz>
        la parlamento ne sufi&ccirc;e influas
        la nacian norvegan <tld/>on
        <fnt>
        <aut>B. Hekland</aut>,
        <vrk>Tero kaj akvo</vrk>,
        <bib>Monato</bib>,
        <lok>2001:12, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">politics</trd>
  <trd lng="id">politik</trd>
  </snc>
  <snc mrk="politi.0o.agmaniero">
    <dif>
      &Gcirc;enerala agmetodo:
      <ekz>
        &ccirc;iuj markoj de trinka&jcirc;oj &scirc;an&gcirc;os sian
        <tld/>on favore al reuzeblaj boteloj
        <fnt>
        <aut>G. Van Damme</aut>,
        <vrk>Trinka&jcirc;oj/Uzi la le&gcirc;on por reklami</vrk>,
        <bib>Monato</bib>,
        <lok>1995:12, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">policy</trd>
  <trd lng="id"><baz>bijak</baz>kebijakan</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_k;</trd>
  <trd lng="bo">&t_cha;&t_ba;&t_tsheg;&t_sa;&t_rata;&t_gigu;&t_da;&t_tsheg;</trd>
  <trd lng="ca">pol&iacute;tica</trd>
  <trd lng="cs">politika</trd>
  <trd lng="de">Politik</trd>
  <trd lng="es">pol&iacute;tica<klr> (subst.)</klr></trd>
  <trd lng="fr">politique<klr> (subst.)</klr></trd>
  <trd lng="hu">politika</trd>
  <trdgrp lng="ja">
    <trd>&#25919;&#27835;  <pr>&#12379;&#12356;&#12376;</pr></trd>,
    <trd>&#25919;&#31574;  <pr>&#12379;&#12356;&#12373;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">politiek</trd>
  <trd lng="pl">polityka</trd>
  <trd lng="pt">pol&iacute;tica</trd>
  <trd lng="ro">politic&abreve;</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">politika</trd>
  <trd lng="sv">politik</trd>
  <trd lng="uk">&c_p;&c_o;&c_l;&c_ib;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#25919;&#27835; <pr>zh&egrave;ngzh&igrave;</pr></trd>,
    <trd>&#25919; <pr>zh&egrave;ng</pr></trd>,
    <trd>&#26041;&#37341; <pr>f&amacron;ngzh&emacron;n</pr></trd>,
    <trd>&#26041;&#38024; <pr>f&amacron;ngzh&emacron;n</pr></trd>,
    <trd>&#25919;&#31574; <pr>zh&egrave;ngc&egrave;</pr></trd>,
    <trd>&#22269;&#20107; <pr>gu&oacute;sh&igrave;</pr></trd>,
    <trd>&#22283;&#20107; <pr>gu&oacute;sh&igrave;</pr></trd>,
    <trd>&#25919;&#20107; <pr>zh&egrave;ngsh&igrave;</pr></trd>,
    <trd>&#25919;&#29702; <pr>zh&egrave;ngl&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="politi.0a">
  <kap><tld/>a</kap>
  <snc mrk="politi.0a.POL">
    <dif>
      Kiu rilatas al <tld/>o,
      al vivo de regnoj:
      <ekz>
        ne kunvenis regnestroj, nek ministroj, por &scirc;an&gcirc;i la
        <tld/>an karton de la mondo
        <fnt>
          <bib>Paroloj</bib>
          <lok>Unua datreveno de la fondo de la Londona
          Esperanto-Klubo, 1904</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">pol&iacute;tic</trd>
  <trd lng="en">political</trd>
  <trd lng="es">pol&iacute;tico<klr> (adj.)</klr></trd>
  <trd lng="fr">politique<klr> (adj.)</klr></trd>
  <trd lng="hu">politikai<klr> (adj.)</klr></trd>
  <trd lng="id">politis</trd>
  <trdgrp lng="ja">
    <trd>&#25919;&#27835;&#12398;  <pr>&#12379;&#12356;&#12376;&#12398;</pr></trd>,
    <trd>&#25919;&#31574;&#12398;  <pr>&#12379;&#12356;&#12373;&#12367;&#12398;</pr></trd>,
    <trd>&#25919;&#27835;&#30340;&#12394;  <pr>&#12379;&#12356;&#12376;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#25919;&#31574;&#30340;&#12394;  <pr>&#12379;&#12356;&#12373;&#12367;&#12390;&#12365;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="pl">polityczny</trd>
  <trd lng="ro">politic</trd>
  <trd lng="uk">&c_p;&c_o;&c_l;&c_ib;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trd lng="zh">&#25919;&#27835; <pr>zh&egrave;ngzh&igrave;</pr></trd>
</drv>

<drv mrk="politi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="politi.0isto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tata, partia, sindikata registo influa en <tld/>o:
      <ekz>
        Josip Broz Tito, kontra&ubreve;fa&scirc;isto, komunisto kaj monda
        <tld/>isto, restas granda mistero
        <fnt>
        <aut>R. Petrovi&ccirc;</aut>,
        <vrk>Vo&ccirc;o de hiera&ubreve; - hodia&ubreve;</vrk>,
        <bib>Monato</bib>,
        <lok>1997:10, p. 9a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;</trd>
  <trd lng="bo">&t_cha;&t_ba;&t_tsheg;&t_sa;&t_rata;&t_gigu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ca">pol&iacute;tic</trd>
  <trd lng="cs">politik</trd>
  <trd lng="de">Politiker</trd>
  <trd lng="es">pol&iacute;tico</trd>
  <trdgrp lng="fr">
    <trd>politicien</trd>,
    <trd>politique<klr> (subst., politicien)</klr></trd>
  </trdgrp>
  <trd lng="hu">politikus</trd>
  <trdgrp lng="id">
    <trd>politikus</trd>,
    <trd>politisi</trd>
  </trdgrp>
  <trd lng="ja">&#25919;&#27835;&#23478;  <pr>&#12379;&#12356;&#12376;&#12363;</pr></trd>
  <trd lng="nl">politicus</trd>
  <trd lng="pl">polityk</trd>
  <trd lng="ro">politician</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">politik</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_t;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j; &c_d;&c_ib;&c_ja;&c_ch;</trd>
  </trdgrp>
  <trd lng="zh">&#25919;&#27835;&#23478; <pr>zh&egrave;ngzh&igrave;ji&amacron;</pr></trd>
</drv>

<drv mrk="politi.lingvo0o">
  <kap>lingvo<tld/>o</kap>
  <snc mrk="politi.lingvo0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Maniero, kiel &scirc;tato a&ubreve; alia a&ubreve;toritato gvidas la uzojn kaj
      evoluojn de lingvoj de la regatoj:
      <ekz>
        la sveda tre longe estis tiel memkomprenebla en Svedio ke oni
        ne bezonis iun specialan lingvo<tld/>on
        <fnt>
          <aut>C. Kiselman</aut>,
          <vrk>La sveda faklingvo en tekniko, matematiko kaj
          natursciencoj</vrk>,
          <lok>en: Studoj pri interlingvistiko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_w;&c_n;&c_a;&c_ja; &c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">pol&iacute;tica ling&uuml;&iacute;stica</trd>
  <trd lng="de">Sprachpolitik</trd>
  <trd lng="es">pol&iacute;tica lingu&iacute;stica</trd>
  <trd lng="fr">politique linguistique</trd>
  <trd lng="hu">nyelvpolitika</trd>
  <trd lng="id">politik bahasa</trd>
  <trd lng="ja">&#35328;&#35486;&#25919;&#31574;  <pr>&#12370;&#12435;&#12372;&#12379;&#12356;&#12373;&#12367;</pr></trd>
  <trd lng="nl">taalpolitiek</trd>
  <trd lng="pl">polityka j&eogonek;zykowa</trd>
  <trd lng="ro">politic&abreve; lingvistic&abreve;</trd>
  <trd lng="uk">&c_m;&c_o;&c_v;&c_n;&c_a; &c_p;&c_o;&c_l;&c_ib;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="politi.ne0a">
  <kap>ne<tld/>a</kap>
  <snc mrk="politi.ne0a.POL">
    <dif>
      Kiu ne havas <tld/>an koncernon a&ubreve; sekvon:
      <ekz>
        &gcirc;is nun mi estis sufi&ccirc;e ne<tld/>a homo
        <fnt>
          <aut>Jim</aut>,
          <vrk>Ni &ccirc;iuj estas partoj de la sama mondo</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">apol&iacute;tic</trd>
  <trd lng="cs">apolitick&yacute;</trd>
  <trd lng="es">apol&iacute;tico</trd>
  <trd lng="fr">apolitique</trd>
  <trd lng="id">apolitis</trd>
  <trd lng="pl">niepolityczny</trd>
  <trd lng="pt">apol&iacute;tico</trd>
  <trd lng="ro">apolitic</trd>
  <trd lng="sk">apolitick&yacute;</trd>
</drv>

<drv mrk="politi.real0o">
  <kap>real<tld/>o</kap>
  <snc mrk="politi.real0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politiko malidealisma, la&ubreve;reale taksanta la helpojn, malhelpojn
      kaj rimedojn por atingi siajn celojn:
      <ekz>
        la re&gcirc;isoro Benegal sukcese skizas la kompleksan kombinon de
        metafiziko kaj real<tld/>o evoluigitan de Gandhi
        <fnt>
        <aut>A. G. Rao</aut>,
        <vrk>La formi&gcirc;o de Mahatmo</vrk>,
        <bib>Monato</bib>,
        <lok>1997:7, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;&c_a;</ind></trd>
  <trd lng="ca">Realpolitik <klr tip="amb">(al.)</klr></trd>
  <trd lng="de">Realpolitik</trd>
  <trdgrp lng="es">
    <trd>pol&iacute;tica realista</trd>,
    <trd>Realpolitik</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>politique r&eacute;aliste</trd>,
    <trd>realpolitik</trd>
  </trdgrp>
  <trd lng="hu">re&aacute;lpolitikus</trd>
  <trdgrp lng="id">
    <trd>realpolitik</trd>,
    <trd>realisme politik</trd>
  </trdgrp>
  <trd lng="nl">realpolitik</trd>
  <trdgrp lng="pl">
    <trd>polityka realistyczna</trd>,
    <trd>realpolitik</trd>
  </trdgrp>
  <trd lng="ro">realpolitik</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    <ind>&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_k;&c_a;</ind></trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja;
    <ind>&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22269;&#38469;&#20851;&#31995;&#29616;&#23454;&#20027;&#20041; <pr>gu&oacute;j&igrave;gu&amacron;nx&igrave;xi&agrave;nsh&iacute;zh&#468;y&igrave;</pr></trd>,
    <trd>&#22283;&#38555;&#38364;&#20418;&#29694;&#23526;&#20027;&#32681; <pr>gu&oacute;j&igrave;gu&amacron;nx&igrave;xi&agrave;nsh&iacute;zh&#468;y&igrave;</pr></trd>
  </trdgrp>
</drv>
