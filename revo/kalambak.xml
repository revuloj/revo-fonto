<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalambak</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kalambak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="rutac.0oj">rutacoj</ref>
      (<trd lng="la">Aquilaria agallocha</trd>):
      <ekz>
        e&ubreve;nukoj alportis donacojn <klr>[...]</klr> konsistantajn el ora
        statuo <klr>[kaj]</klr> <tld/>a apogbastono
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 71a, p. 498a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
 <trdgrp lng="en">
    <trd>agarwood</trd>,
    <trd>aloeswood</trd>,
    <trd>eagle wood</trd>
 </trdgrp>
 <trdgrp lng="fr">
    <trd>bois d'agar</trd>,
    <trd>bois d'aigle</trd>
  </trdgrp>
  <trd lng="hu">sasfa</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_o;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_o;&c_r;&c_l;&c_i;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_m;&c_b;&c_a;&c_k;</trd>
  </trdgrp>
</drv>
</art>

