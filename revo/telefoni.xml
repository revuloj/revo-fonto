<kap>
  <rad>telefoni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="telefoni.0o">
  <kap><tld/>o</kap>
  <snc mrk="telefoni.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref cel="sistem.komunik0o.TEL">Komuniksistemo<sncref/></ref>
      ebliganta distancan inter&scirc;an&gcirc;on de parolaj informoj inter du a&ubreve; pluraj partoprenantoj:
      <ekz>
        telegrafio kaj <tld/>o kreis el la mondo flustrogalerion
        <fnt>
        <aut>R. W. Mason, trad. W. Patterson</aut>,
        <vrk><url ref="&Gutenberg;cache/epub/11511/pg11511.html">
        anta&ubreve;parolo al &leftquot;Robinsono Kruso&rightquot;</url></vrk>,
        <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;La Evoluo de <tld lit="T"/>o&rightquot; estas 62-pa&gcirc;a libro originale verkita en
        Esperanto de in&gcirc;eniero J. R. G. Isbr&uuml;cker
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24754">La Evoluo de Telefonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="radiot.0o">radiotelefonio</ref>
    <ref tip="prt" cel="telefo.0o.TEL">telefono</ref>
    <trd lng="de">Telefonie</trd>
  </snc>
  <trd lng="en">telephony</trd>
  <trd lng="fr">t&eacute;l&eacute;phonie</trd>
  <trd lng="pl">telefonia</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x43B;&#x435;&#x444;&#x43E;&#x43D;&#x456;&#x44F;</trd>,
    <trd>&#x442;&#x435;&#x43B;&#x435;&#x444;&#x43E;&#x43D;&#x43D;&#x438;&#x439; &#x437;&#x432;&#x2019;&#x44F;&#x437;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

