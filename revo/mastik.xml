<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mastik.xml,v 1.19 2022/03/23 14:01:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mastik</rad>/o
</kap>
<drv mrk="mastik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mastik.0o.KOMUNE">
    <dif>
      Iu el diversaj <ref cel="past.0o.molajxo">past</ref>ecaj substancoj uzataj por &scirc;topi truojn a&ubreve;
      kunigi objektojn:
      <ekz>
        li sigelis la &ccirc;erkon per <tld/>o
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 32a, p.
        464a</lok></fnt>;
      </ekz>
      <ekz>
        vi vin okupas per la &hcirc;emio? tio estas mia pasio, mi e&ccirc; mem elpensis <tld/>on
        <klr>[&#x2026;]</klr> por fari pupojn, la kapojn de l&#x2019; pupoj
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
    </dif>
    <subsnc mrk="mastik.0o.kreta">
      <dif>
        Miksa&jcirc;o el <ref tip="prt" cel="kret.0o.kalko">kreto</ref> kaj <ref tip="prt" cel="ole.0o">oleo</ref>, 
        kiu malmoli&gcirc;as seki&gcirc;ante kaj servas por alfiksi vitrojn al <ref tip="malprt" cel="fenest.0o.baro">fenestro</ref>j:
        <ekz>
          la vitra&jcirc;o, rompita dum la mateno kaj anstata&ubreve;ita dum la tagmezo, estis
          subtenita nur de <tld/>o ankora&ubreve; fre&scirc;a, kiun sen malfacileco li formetis per
          la pinto de sia tran&ccirc;ilo
          <fnt>
            <bib>KPr</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ca">m&agrave;stic</trd>
      <trdgrp lng="hu">
        <trd>gitt</trd>,
        <trd>kitt</trd>,
        <trd>ablakragasz</trd>
      </trdgrp>
      <trd lng="nl">mastiek</trd>
      <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </subsnc>
    <ref tip="vid" cel="past.modlo0o">modlopasto</ref>
    <trd lng="de">Kitt</trd>
    <trdgrp lng="hu">
      <trd>masztix</trd>,
      <trd>gyantaragaszt&oacute;</trd>,
      <trd>ragaszt&oacute;m&eacute;zga</trd>
    </trdgrp>
    <trd lng="nl">stopverf</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">mastik</trd>
  <trd lng="ca">massilla</trd>
  <trdgrp lng="cs">
    <trd>kyt</trd>,
    <trd>lepidlo</trd>,
    <trd>mastix</trd>,
    <trd>tmel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spachtelmasse</trd>,
    <trd>Knetmasse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mastic</trd>,
    <trd>putty</trd>
  </trdgrp>
  <trd lng="fr">mastic</trd>
  <trd lng="pl">kit</trd>
  <trdgrp lng="pt">
    <trd>m&aacute;stique</trd>,
    <trd>massa de vidraceiro</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>git</trd>,
    <trd>tmel</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x441;&#x442;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x43C;&#x430;&#x437;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="mastik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxtop.0i.fermi">&Scirc;topi</ref> 
      a&ubreve; <ref tip="super" cel="kun.0igi">kunigi</ref> per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">mastika&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>massillar</trd>,
    <trd>empastar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kytovat</trd>,
    <trd>tmelit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kitten</trd>,
    <trd>zuspachteln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stop<klr tip="amb"> (hole)</klr></trd>,
    <trd>putty</trd>,
    <trd>fill in</trd>
  </trdgrp>
  <trd lng="fr">mastiquer<klr tip="amb"> (mettre du mastic)</klr></trd>
  <trd lng="hu">gittel</trd>
  <trd lng="nl">plamuren</trd>
  <trd lng="pl">kitowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>calafetar</trd>,
    <trd>almecegar</trd>,
    <trd>aplicar <ind>m&aacute;stique</ind></trd>    
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>tmeli&tcaron;</trd>,
    <trd>zatmeli&tcaron;</trd>,
    <trd>zatrie&tcaron; tmelom</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x442;&#x438;&#x440;&#x430;&#x442;&#x438; &#x43C;&#x430;&#x441;&#x442;&#x438;&#x43A;&#x43E;&#x44E;</trd>,
    <trd>&#x437;&#x430;&#x448;&#x43F;&#x430;&#x440;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438; &#x437;&#x430;&#x43C;&#x430;&#x437;&#x43A;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
