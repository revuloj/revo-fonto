<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mastik</rad>/o
</kap>
<drv mrk="mastik.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Miksa&jcirc;o el kreto kaj oleo, kiu malmoli&gcirc;as seki&gcirc;ante kaj servas por
      alfiksi vitrojn al fenestroj.
    </dif>
    <trdgrp lng="hu">
      <trd>gitt</trd>,
      <trd>kitt</trd>,
      <trd>ablakragasz</trd>
    </trdgrp>
    <trd lng="nl">mastiek</trd>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Nomo de diversaj pastecaj substancoj uzataj por &scirc;topi truojn a&ubreve;
      kunigi objektojn.
    </dif>
    <trd lng="de">Kitt</trd>
    <trdgrp lng="hu">
      <trd>masztix</trd>,
      <trd>gyantaragaszt&oacute;</trd>,
      <trd>ragaszt&oacute;m&eacute;zga</trd>
    </trdgrp>
    <trd lng="nl">stopverf</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
  </trdgrp>           
  <trd lng="br">mastik</trd>
  <trd lng="fr">mastic</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
</drv>

<drv mrk="mastik.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi a&ubreve; kunigi per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>                
  <trd lng="br">mastika&ntilde;</trd>
  <trd lng="de">kitten</trd>
  <trd lng="fr">mastiquer</trd>
  <trd lng="hu">gittel</trd>  
  <trd lng="nl">plamuren</trd>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_o;&c_j;</trd>
</drv>
</art>
