<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uln.xml,v 1.40 2024/07/30 18:33:52 revo Exp $">
<kap>
    <ofc>*</ofc>
    <rad>uln</rad>/o
    <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="uln.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="uln.0o.mezurunuo">
     <fnt>(<aut>Kurzweil, Peter</aut>: <vrk>Das Vieweg
      Einheiten-Lexikon.
      Wiesbaden 1999</vrk>)</fnt>
      <dif>
        Malnova <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">mezurunuo</ref> de
        longeco responda al distanco inter pinto de mezfingro &gcirc;is
        kubuto,
        tamen varianta la&ubreve;kulture inter 0,37 m kaj 1,188 m:
        <ekz>
          ili ekvidis nanon kun maljuna velkinta viza&gcirc;o kaj kun blanka
          barbo, <tld/>on longa
          <fnt>
            <bib>ElektFab</bib>
            <lok>Ne&gcirc;ulino kaj Rozulino</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          tiajn homojn oni ne povas mezuri per la ordinara
          <tld/>o
          <fnt>
            <bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p. 127a</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          mezuri &ccirc;ion per <tld/>o de racio.
        </ekz>
      </dif>
      <trd lng="be">&c_l;&c_o;&c_k;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_je;&c_r;&c_a; &c_d;&c_a;&c_w;&c_zh;&c_y;&c_n;&c_ib;)</klr></trd>
      <trd lng="br">ilinad</trd>
      <trdgrp lng="ca">
        <trd>colze</trd>,
        <trd>colzada</trd>
      </trdgrp>
      <trd lng="da">alen</trd>
      <trdgrp lng="en">
        <trd>cubit</trd>,
        <trd>ell</trd>,
        <trd>arm's length</trd>
      </trdgrp>
      <trd lng="de">Elle</trd>
      <trd lng="es">codo</trd>
      <trd lng="fa">&a_A;&a_fatha;&a_r;&a_fatha;&a_sh;</trd>
      <trd lng="fi">kyyn&auml;r&auml;</trd>
      <trdgrp lng="fr">
        <trd>coud&eacute;e</trd>,
        <trd>aune de Paris</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>r&odblac;f</trd>,
        <trd>k&ouml;ny&ouml;k<klr tip="ind"> (hosszm&eacute;rt&eacute;k)</klr></trd>
      </trdgrp>
      <trd lng="id">hasta</trd>
      <trd lng="it">cubito</trd>
      <trd lng="nl">el</trd>
      <trd lng="pl">&lstroke;okie&cacute;</trd>
      <trdgrp lng="pt">
        <trd>c&ocirc;vado</trd>,
        <trd>c&uacute;bito</trd>
      </trdgrp>
      <trd lng="ru">&c_l;&c_o;&c_k;&c_o;&c_t;&c_mol;</trd>
      <trd lng="sv">aln</trd>
    </snc>
    <snc mrk="uln.0o.mezurilo">
       <dif>
          Mezurilo, bastono longe je unu <ref tip="vid" cel="uln.0o.mezurunuo">ulno<sncref/></ref>:
          <ekz>
            vi eksentos la <tld/>on sur viaj oreloj
            <fnt>
              <bib>ElektFab</bib>
              <lok>La kura&gcirc;a tajloreto</lok>
            </fnt>;
          </ekz>
       </dif>
      <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_k;&c_a; <klr>(&c_d;&c_a;&c_w;&c_zh;&c_y;&c_n;&c_jo;&c_j; &c_u; &c_l;&c_o;&c_k;&c_a;&c_c;&c_mol;)</klr></trd>
      <trd lng="de">Zollstock</trd>
    </snc>
    <snc mrk="uln.0o.kubutosto">
      <dif>
        Kubut<ref tip="super" cel="ost.0o">osto</ref>:
         <ekz>
          inter <ref cel="radius.0o.ANA">radiuso</ref> kaj <tld/>o
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=32776">Artiko</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_l;&c_o;&c_k;&c_c;&c_je;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
      <trd lng="br">kefilin</trd>
      <trdgrp lng="ca">
        <trd>c&uacute;bit</trd>,
        <trd>ulna</trd>
      </trdgrp>
      <trd lng="de">Elle</trd>
      <trd lng="en">ulna</trd>
      <trd lng="es">c&uacute;bito</trd>
      <trd lng="fa">&a_z;&a_n;&a_d; &a_z;&f_y;&a_r;&f_y;&a_n;</trd>
      <trd lng="fi">kyyn&auml;rluu</trd>
      <trdgrp lng="fr">
        <trd>cubitus</trd>,
        <trd>ulna</trd>
      </trdgrp>
      <trd lng="hu">singcsont</trd>
      <trd lng="nl">ellepijp</trd>
      <trd lng="pl">ko&sacute;&cacute; <ind>&lstroke;okciowa</ind></trd>
      <trdgrp lng="pt">
        <trd>c&uacute;bito</trd>,
        <trd>ulna</trd>
      </trdgrp>
      <trd lng="ru">&c_l;&c_o;&c_k;&c_t;&c_je;&c_v;&c_a;&c_ja;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
      <trdgrp lng="sv">
        <trd>armb&aring;gsben</trd>,
        <trd>ulna</trd>
      </trdgrp>
      <trdgrp lng="zh">
    <trd>&#x5C3A;&#x9AA8; <pr>ch&#x1D0;g&#x1D4;</pr></trd>,
    <trd>&#32920;&#x5C3A; <pr>zh&#x1D2;uch&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>kost loketn&iacute; <klr>(ulna)</klr></trd>,
    <trd>ulna</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C3A;&#x9AA8;  <pr>&#12375;&#12419;&#12387;&#12371;&#12388;</pr></trd>,
    <trd>&#33109;&#x5C3A;  <pr>&#12358;&#12391;&#12375;&#12419;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="sk">lake&tcaron; <klr>(miera)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&#x43A;&#x43E;&c_t;&#x44C; (&#x43C;&c_ib;&c_r;&c_a; &c_d;&#x43E;&c_v;&c_zh;&c_i;&#x43D;&c_i;)</trd>,
    <trd>&#x43B;&c_ib;&#x43A;&c_t;&#x44C;&#x43E;&c_v;&c_a; &#x43A;&c_ib;&c_s;&c_t;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
