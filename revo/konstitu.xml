<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konstitu</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konstitu.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="konsis.0igi">konsistigi</ref>
    <ekz>
      la imito de la realo <tld/>as la esencon de la teatro
      <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 48a, p.
      360a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="nl">
    <trd>vormen</trd>,
    <trd><ind>bestaan</ind> uit</trd>,
    <trd>constitueren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>constituir</trd>,
    <trd>compor</trd>,
    <trd>formar</trd>
  </trdgrp>
</drv>
</art>

