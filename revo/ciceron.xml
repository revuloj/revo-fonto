<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ciceron.xml,v 1.14 2023/09/30 09:21:55 revo Exp $">
<kap>
  <rad>Ciceron</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ciceron.0o">
  <kap><tld/>o</kap>
  <snc mrk="ciceron.0o.PERS">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Romia <ref tip="lst" lst="voko:historiaj_personoj" cel="politi.0isto">politikisto</ref> kaj
      latina beletristo
      (106 a.K. &ndash; 43 a.K.), a&ubreve;toro
      de paroladoj kaj filozofiaj verkoj, fama kiel retorikisto:
      <ekz>
        &ccirc;e vi kun &ccirc;iu vorto kvaza&ubreve; <tld/>o deflugas
        de la lango
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="cicer.0o">cicero</ref>
  <trd lng="be">&c_C;&c_y;&c_c;&c_e;&c_r;&c_o;&c_n;</trd>
  <trd lng="ca">Cicer&oacute;</trd>
  <trd lng="de">Cicero</trd>
  <trd lng="es">Cicer&oacute;n</trd>
  <trd lng="fa">&a_m;&a_A;&a_r;&f_k;&a_w;&a_s; &a_t;&a_w;&a_l;&f_y;&a_w;&a_s; <ind>&a_s;&f_y;&a_s;&a_r;&a_w;</ind></trd>
  <trd lng="fr">Cic&eacute;ron</trd>
  <trd lng="hu">Cicero</trd>
  <trd lng="ja">&#x30AD;&#x30B1;&#x30ED;</trd>
  <trd lng="lat">Marcus Tullius <ind>Cicero</ind></trd>
  <trd lng="nl">Cicero</trd>
  <trd lng="pl">Cycero</trd>
  <trd lng="ru">&c_C;&c_i;&c_c;&c_je;&c_r;&c_o;&c_n;</trd>
  <trd lng="it">Cicerone</trd>
  <trd lng="uk">&c_C;&c_ib;&c_c;&c_je;&c_r;&#x43E;&#x43D;</trd>
</drv>
</art>

