<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>stuk</rad>/i
</kap>
<drv mrk="stuk.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc num="1">
<dif>
&Scirc;mirpentri muron per miksa&jcirc;o de kalko k sablo. 
</dif>
</snc>
<snc num="2">
<dif>
Kovri ion per tavolo el speco de mortero similanta marmoron. 
</dif>
</snc>
<trdgrp lng="de">
   <trd>stuckieren</trd>,
   <trd>mit <ind>Stuck</ind> versehen</trd>
</trdgrp>
<trd lng="en">plaster</trd>
</drv>
<drv mrk="stuk.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Substanco servanta por <tld/>i. 
</dif>
<refgrp tip="sin">
<ref cel="gips.0o">gipso</ref>.
</refgrp>
</snc>
<trd lng="de">Stuck</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>

