<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stuk</rad>/i
</kap>
<drv mrk="stuk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;mirpentri muron per miksa&jcirc;o de kalko kaj sablo.
    </dif>
  </snc>
  <snc>
    <dif>
      Kovri ion per tavolo el speco de mortero similanta marmoron.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_n;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>stuckieren</trd>,
    <trd>mit <ind>Stuck</ind> versehen</trd>
  </trdgrp>
  <trd lng="en">plaster</trd>
  <trd lng="nl">pleisteren</trd>
  <trd lng="ru">&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="stuk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Substanco servanta por <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="gips.0o">gipso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_n;&c_k;</trd>, 
    <trd>&c_t;&c_y;&c_n;&c_k;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Stuck</trd>
  <trdgrp lng="nl">
    <trd>pleisterkalk</trd>,
    <trd>gips</trd>,
    <trd>plafondmortel</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_k;&c_a;</trd>
</drv>
</art>
