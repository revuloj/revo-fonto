<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arist.xml,v 1.18 2023/10/13 16:46:48 revo Exp $">
<kap>
  <rad>arist</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arist.0o">
  <kap><tld/>o</kap>
  <snc mrk="arist.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longaj kaj pintaj ostoj de kelkaj fi&scirc;oj.
    </dif>
    <trd lng="en">bristle</trd>
    <trd lng="hu">halt&uuml;ske</trd>
    <trd lng="sk">kostica</trd>
  </snc>
  <snc mrk="arist.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Rigida harforma plilongiga&jcirc;o a&ubreve; alpenda&jcirc;o
      &ccirc;e la pinto a&ubreve; dorso de <ref tip="malprt" cel="brakte.0o">brakteo</ref>, precipe &ccirc;e la ekstera
      <ref tip="malprt" cel="pale.0o">paleo</ref> de poacoj:
      <ekz>
        tritikaj <tld/>oj
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Granne</trd>
    <trdgrp lng="en">
      <trd>arista</trd>,
      <trd>awn</trd>,
      <trd>beard</trd>
    </trdgrp>
    <trd lng="fr">barbe<klr> (cartilage, filaments
    v&eacute;g&eacute;taux...)</klr></trd>
    <trdgrp lng="hu">
      <trd>tokl&aacute;sz</trd>,
      <trd>sz&aacute;lka <klr tip="ind">(kal&aacute;szon)</klr></trd>,
      <trd>szak&aacute;ll <klr tip="ind">(kal&aacute;szon)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_n;&c_mol;<klr tip="ind">
(&c_b;&c_o;&c_t;.)</klr></trd>
    </trdgrp>
    <trd lng="sk">&ocirc;stie</trd>
  </snc>
  <trd lng="cs">osina</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D9;&#x5E1;&#x5D8;&#x5D5;</trd>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x304E;</trd>,
    <trd>&#x89E6;&#x9B1A;  <pr>&#x3057;&#x3087;&#x304F;&#x3057;&#x3085;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>1.kostica</trd>,
    <trd>2.&ocirc;stie</trd>,
    <trd>hovor: f&uacute;zy (obilia)</trd>,
    <trd>kosti&ccaron;ka <klr>(r&yacute;b)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43E;&#x441;&#x442;&#x44E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
