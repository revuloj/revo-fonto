<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>arist</rad>/o
</kap>
<drv mrk="arist.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longaj kaj pintaj ostoj de kelkaj fi&scirc;oj.
    </dif>
    <trd lng="en">bristle</trd>
    <trd lng="hu">halt&uuml;ske</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Rigida harforma plilongiga&jcirc;o a&ubreve; alpenda&jcirc;o
      &ccirc;e la pinto a&ubreve; dorso de <ref tip="malprt"
      cel="brakte.0o">brakteo</ref>, precipe &ccirc;e la ekstera
      <ref tip="malprt" cel="pale.0o">paleo</ref> de poacoj.
    </dif>
    <trd lng="de">Granne</trd>
    <trdgrp lng="en">
      <trd>arista</trd>,
      <trd>awn</trd>,
      <trd>beard</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tokl&aacute;sz</trd>,
      <trd>sz&aacute;lka <klr tip="ind">(kal&aacute;szon)</klr></trd>,
      <trd>szak&aacute;ll <klr tip="ind">(kal&aacute;szon)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_n;&c_mol;<klr tip="ind">
(&c_b;&c_o;&c_t;.)</klr></trd>
    </trdgrp>
  </snc>
</drv>
</art>
