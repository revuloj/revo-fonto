  <trd lng="he">&qof;&bet;&vav;&tsadi;&he;
  <klr tip="ind">(&samekh;&pe;&vav;&resh;&tet;)</klr></trd>
  <trd lng="hi">&#x91F;&#x940;&#x92E;</trd>
  <trd lng="hr">tim</trd>
  <trd lng="hu">csapat</trd>
  <trd lng="hy">&#x569;&#x56B;&#x574;</trd>
  <trd lng="is">li&#xF0;</trd>
