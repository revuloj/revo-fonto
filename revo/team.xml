<drv mrk="team.0a">
  <kap><tld/>a</kap>
  <snc mrk="team.0a.KOMUNE">
    <dif>
      Koncernanta <tld/>on, farata de <tld/>o:
      <ekz>
        merkatiko estas <tld/>a laboro, kuna laboro
        <fnt>
          <aut>D. Keefe</aut>,
          <vrk><url ref="&Sezonoj;2014/12/keefe-4/">
          Merkatiko kaj &gcirc;iaj limoj en la mondo de Esperanto</url></vrk>,
          <lok>La Balta Ondo, 2014-12-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>collectif<klr> (adj.)</klr></trd>,
    <trd>d'<ind>&eacute;quipe</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30FC;&#x30E0;&#x306E;  <pr>&#12385;&#x30FC;&#12416;&#x306E;</pr></trd>,
