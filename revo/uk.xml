<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: uk.xml,v 1.4 2022/05/06 12:17:08 revo Exp $">
<kap>
  -<rad>uk</rad>/
</kap>
<drv mrk="uk.0">
  <kap>-<tld/></kap>
  <gra><vspec>sufikso</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="uk.0.KOMUNE">
    <dif>
      <ref tip="vid" cel="kastr.0i">Kastrita</ref> besto; <ref tip="sin"
      cel="eks.0.kastro">eks-<sncref/></ref>:
      <ekz>
        bovo &#8594; *<ind>bov<tld/>o</ind>* = &rightquot;kastrita bovo&#8221;
        <trd lng="io">kastrita bovo</trd>
        <fnt>
          <aut>Bertilo Wennergren</aut>
          <vrk>Plena Manlibro de Esperanta Gramatiko</vrk>
          <lok>
            <url
            ref="&PMEG;vortfarado/neoficialaj_afiksoj/sufiksoj/uk.html">UK</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        virkato &#8594; *<ind>virkat<tld/>o</ind>*
        <trdgrp lng="en">
          <trd>castrated male cat</trd>,
          <trd>castrated he-cat</trd>,
          <trd>castrated tomcat</trd>,
          <trd>castrated tom</trd>
        </trdgrp>
        <trd lng="io">kastrita katulo</trd>
        <trd lng="vo">hokat</trd>
        <fnt>
          <aut>Bertilo Wennergren</aut>
          <vrk>Plena Manlibro de Esperanta Gramatiko</vrk>
          <lok>
            <url
            ref="&PMEG;vortfarado/neoficialaj_afiksoj/sufiksoj/uk.html">UK</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evalino &#8594; *<ind>&ccirc;evalin<tld/>o</ind>*
        <trdgrp lng="en">
          <trd>castrated female horse</trd>,
          <trd>castrated she-horse</trd>
        </trdgrp>
        <trd lng="io">kastrita kavalino</trd>
        <trd lng="vo">jojevod</trd>
        <fnt>
          <aut>Bertilo Wennergren</aut>
          <vrk>Plena Manlibro de Esperanta Gramatiko</vrk>
          <lok>
            <url
            ref="&PMEG;vortfarado/neoficialaj_afiksoj/sufiksoj/uk.html">UK</url>
          </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="euxnuk.0o">e&ubreve;nuko</ref>
  <trdgrp lng="vo">
    <trd>ho-<klr> (kastrita virbesto)</klr></trd>,
    <trd>jo-<klr> (kastrita bestino)</klr></trd>
  </trdgrp>
</drv>
</art>

