<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>real</rad>/a
  <ofc>1</ofc>
</kap>
<drv mrk="real.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Efektive ekzistanta kaj kontrolebla:
      <ekz>
        la <tld/>a mondo, vivo;
      </ekz>
      <ekz>
        la fera mano de la <tld/>o frakasis lian revon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="efekti.0a">efektiva</ref>,
      <ref cel="fakt">fakta</ref>,
      <ref cel="vid">videbla</ref>,
      <ref cel="palp.0ebla">palpebla</ref>,
      <ref cel="sent">sentebla</ref>,
      <ref cel="prakti.0a">praktika</ref>,
      <ref cel="viv">vivanta</ref>,
      <ref cel="karn">karna</ref>,
      <ref cel="objekt1.0a">objektiva</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>wirklich</trd>,
    <trd>real</trd>
  </trdgrp>
  <trd lng="en">actual</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="nl">
    <trd>werkelijk</trd>
    <trd>echt</trd>
  </trdgrp>
</drv>

<drv mrk="real.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio, kio <tld/>e ekzistas:
      <ekz>
        perdi kontakton kun la <tld/>o;
      </ekz>
      <ekz>
        nenio estas pli dan&gcirc;era ol blinda entuziasmo, kiu ne
        vidas la <tld/>on
        <fnt>(<aut>Lanti</aut>:
        <vrk>Signo pri maturi&gcirc;o de klaso</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Wirklichkeit</trd>,
    <trd>Realit&auml;t</trd>
  </trdgrp>
  <trd lng="en">reality</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="nl">werkelijkheid</trd>
</drv>

<drv mrk="real.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref cel="real.0o">Realo</ref>:
      <ekz>
        &scirc;i rememoris sian son&gcirc;on, kiu staris ankora&ubreve; anta&ubreve;
        &scirc;i tiel vive, kvaza&ubreve; &gcirc;i estus <tld/>a&jcirc;o
        <fnt>(<aut>Andersen, trad. Z</aut>:
        <vrk>Filino de la mar&ccirc;a re&gcirc;o</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Fenomeno de la reala vivo, precipe aparta&jcirc;o de epoko a&ubreve;
      loko:
      <ekz>
        la krudaj <tld/>a&jcirc;oj de la vivo en grandegaj urboj
        <fnt>PIV</fnt>;
      </ekz>
      <ekz>
        [per tiu nom&scirc;an&gcirc;o la a&ubreve;toro] eble volis eviti plenan
        identigon kun la historiaj reala&jcirc;oj<fnt>(<aut>Bulgakov,
        trad. S.Pokrovskij</aut>:
        <vrk>La majstro kaj Margarita</vrk>,
        <lok>Notoj pri &ccirc;ap.2, p.349</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">realia</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_ja;</trd>
</drv>

<drv mrk="real.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion (planitan, projektitan, kontraktitan) <tld/>e
      ekzistanta:
      <ekz>
        mia revo komencas <tld/>i&gcirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi sian programon;
      </ekz>
      <ekz>
        kiam la
        a&ubreve;toroj de tiuj &ccirc;i projektoj alpa&scirc;as al ilia <tld/>igado,
        ili tuj konvinki&gcirc;as ... ke tio, kio en teorio &scirc;ajnis afero
        tiel facila, en la praktiko montri&gcirc;as tre malfacila kaj
        neplenumebla <fnt>(<aut>Z</aut>: <vrk>Esenco kaj
        estonteco</vrk>, <lok>VI</lok>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="efekti.0igi">efektivigi</ref>,
      <ref cel="plen.0umi">plenumi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>verwirklichen</trd>,
    <trd>realisieren</trd>
  </trdgrp>
  <trd lng="en">implement</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="nl">
    <trd>verwerkelijken</trd>,
    <trd>realiseren</trd>
  </trdgrp>
</drv>

<drv mrk="real.0igo">
  <kap><tld/>igo</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <tld lit="R"/>igado a&ubreve; <tld/>iga&jcirc;o:
      <ekz>
        por tiu komputilo estas pluraj <tld/>igoj de C.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Implementierung</trd>
  <trd lng="en">implementation</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="real.ne0amal0a">
  <kap>ne<tld/>a, mal<tld/>a</kap>
  <snc>
    <dif>
      Iluzia
    </dif>
    <refgrp tip="sin">
      <ref cel="vent.0a">venta</ref>,
      <ref cel="fantom">fantoma</ref>,
      <ref cel="aer.0a">aera</ref>,
      <ref cel="tromp.0a">trompa</ref>,
      <ref cel="rev">reva</ref>,
      <ref cel="songx">son&gcirc;a</ref>,
      <ref cel="subjek1.0a">subjektiva</ref>,
      <ref cel="utopi">utopia</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>unwirklich</trd>,
    <trd>irreal</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="real.0ismo">
  <kap><tld/>ismo <ofc>3</ofc></kap>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <dif>
      Platona kaj skolastika doktrino, kiu konsideras la &gcirc;eneralajn
      ideojn kiel objektive ekzistantajn.
    </dif>
    <refgrp tip="sin">
      <ref cel="nomina.0ismo">nominalismo</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Doktrino la&ubreve; kiu la arto devas prezenti la vivon tia kia &gcirc;i
      <tld/>e estas, sen la plinobligo postulata de la
      <ref tip="vid" cel="klasik.0ismo">klasikismo</ref> a&ubreve;
      fantaziemo de la
      <ref tip="vid" cel="romant.0o">romantiko</ref>.
    </dif>
  </snc>
  <trd lng="de">Realismus</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="real.0isma">
  <kap><tld/>isma</kap>
  <snc>
    <dif>
      Prudenta, bazita sur la <tld/>o, <tld/>igebla:
      <ekz>
        liaj postuloj, planoj ne estas <tld/>ismaj;
      </ekz>
      <ekz>
        <tld/>isme pensante, ni ne atendu ke la Fina Venko
        okazos jam morga&ubreve;.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">realistisch</trd>
  <trd lng="en">realistic</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<adm>
  Mankas SU(PE)RREALISMO, HIPERREALISMO
</adm>
</art>
