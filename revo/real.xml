<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: real.xml,v 1.51 2023/10/20 19:20:37 revo Exp $">
<!-- la varia&jcirc;o realism/o estas intence malsupre &ccirc;i, por aldoni la oficialan
     radikon realism/ la&ubreve; 3a OA, kaj samtempe montri ke ni konsideras &gcirc;in
     hodia&ubreve; deriva&jcirc;o de real/. Bv. ne forigi a&ubreve; &scirc;an&gcirc;i! -->
<kap>
  <ofc>1</ofc>
  <rad>real</rad>/a,
  <var><kap>
    <ofc>3</ofc>
    <rad>realism</rad>/o
  </kap></var>
</kap>
<drv mrk="real.0a">
  <kap><tld/>a</kap>
  <snc mrk="real.0a.KOMUNE">
    <dif>
      Efektive ekzistanta kaj kontrolebla:
      <ekz>
        nia <tld/>a, <ref cel="proz.0a.FIG">proza</ref> tempo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fantoma lago fari&gcirc;os <tld/>a, kaj la tero senakva fari&gcirc;os fonto de akvo
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 35:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne sciis, ke <tld/>a estas tio <klr>[&#x2026;]</klr> sed li pensis, ke li vidas
        vizion
        <fnt>
          <bib>NT</bib>
          <lok>&Ago; 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ordinaraj historioj, da kiuj ni havas tute sufi&ccirc;e en la <tld/>a vivo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;an&gcirc;i&gcirc;antaj pensoj en &scirc;ia son&gcirc;o akceptis la formon de <tld/>aj bildoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne malbonigu al mi miajn knabinojn kaj ne parolu kun ili, se vi ne havas pli
        <tld/>ajn intencojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        veran viron mongajnantan kaj <tld/>an vi elektu, ne revulon
        <fnt>
          <bib>VivZam</bib>
        </fnt>!
      </ekz>
      <ekz>
        la imagataj teruroj malaperis en la &ccirc;eesto de la <tld/>aj tor&ccirc;oj
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekti.0a">efektiva</ref>,
      <ref cel="fakt">fakta</ref>,
      <ref cel="karn">karna</ref>,
      <ref cel="objekt1.0a">objektiva</ref>,
      <ref cel="palp.0ebla">palpebla</ref>,
      <ref cel="prakti.0a">praktika</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>re&aacute;ln&yacute;</trd>,
    <trd>skute&ccaron;n&yacute;</trd>,
    <trd>v&ecaron;cn&yacute; <klr>(t&yacute;kaj&iacute;c&iacute; se v&ecaron;ci)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wirklich</trd>,
    <trd>real</trd>
  </trdgrp>
  <trd lng="en">actual</trd>
  <trd lng="es">real</trd>
  <trd lng="fr">r&eacute;el</trd>
  <trdgrp lng="hu">
    <trd>val&oacute;s</trd>,
    <trd>t&eacute;nyleges</trd>,
    <trd>val&oacute;s&aacute;gos</trd>,
    <trd>re&aacute;l-</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x73FE;&#x5B9F;&#x4E3B;&#x7FA9;&#x306E;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;&#x3057;&#x3085;&#x304E;&#x306E;</pr></trd>,
    <trd>&#x73FE;&#x5B9F;&#x306B;&#x5373;&#x3057;&#x305F;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;&#x306B;&#x305D;&#x304F;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x73FE;&#x5B9F;&#x7684;&#x306A;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>werkelijk</trd>,
    <trd>echt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>istniej&aogonek;cy</trd>,
    <trd>realny</trd>,
    <trd>rzeczywisty</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>re&aacute;lny</trd>,
    <trd>skuto&ccaron;n&yacute;</trd>,
    <trd>vecn&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_a;&#x43B;&c_ib;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;,</trd>
</drv>

<drv mrk="real.0o">
  <kap><tld/>o</kap>
  <snc mrk="real.0o.ekzisto">
    <dif>
      Tio, kio <tld/>e ekzistas:
      <ekz>
        perdi kontakton kun la <tld/>o;
      </ekz>
      <ekz>
        revo kaj <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la svagaj limoj inter <tld/>o kaj son&gcirc;o, inter efektivo kaj imago, inter frenezo
        kaj racio
        <fnt>
       <aut>Jorge Camacho</aut>
          <vrk><url ref="&Monato;/2000/006836.php">Pri realo kaj la malo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenio estas pli dan&gcirc;era ol blinda entuziasmo, kiu ne
        vidas la <tld/>on
        <fnt>
          <bib>Lanti</bib>,
          <vrk>Signo pri maturi&gcirc;o de klaso</vrk>
          <lok>1925-07-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj nombroj ne spegulas la <tld/>on
        <fnt><aut>Ionel One&#x21B;</aut>:
          <vrk><url ref="&Monato;/2000/007036.php">Pri kaldronoj kaj potoj: &ccirc;u amba&ubreve;
          kotaj?</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/07, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brazilaj amasinformiloj plene maskis la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2003/008040.html">Kvina povo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro ne konas la <tld/>on de la kuba situacio kaj simple ripetas la opinion
        distrumpetatan de la amaskomunikiloj
        <fnt>
       <aut>Radio Havano</aut>
          <vrk><url ref="&Monato;/2000/006888.php">Castro apud Pinochet</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_e;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>Wirklichkeit</trd>,
      <trd>Realit&auml;t</trd>
    </trdgrp>
    <trd lng="en">reality</trd>
    <trd lng="es">realidad</trd>
    <trdgrp lng="fr">
      <trd>r&eacute;alit&eacute;<klr> (la r&eacute;alit&eacute; en g&eacute;n&eacute;ral)</klr></trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>(a) <ind>val&oacute;s&aacute;g</ind></trd>,
    <trd>(a) <ind>realit&aacute;s</ind></trd>
  </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    <trd lng="nl">werkelijkheid</trd>
  </snc>
  <snc mrk="real.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 15</lok></fnt>
    <ref tip="dif" cel="reel.0o.MAT">reelo.</ref>
  </snc>
  <ref tip="hom" cel="real1.0o">realo (monero)</ref>
  <trdgrp lng="cs">
    <trd>hmotn&yacute; sv&ecaron;t</trd>,
    <trd>re&aacute;lno</trd>,
    <trd>skute&ccaron;no</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x73FE;&#x5B9F;&#x4E3B;&#x7FA9;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;&#x3057;&#x3085;&#x304E;</pr></trd>,
    <trd>&#x5199;&#x5B9F;&#x4E3B;&#x7FA9;  <pr>&#x3057;&#x3083;&#x3058;&#x3064;&#x3057;&#x3085;&#x304E;</pr></trd>,
    <trd>&#x30EA;&#x30A2;&#x30EA;&#x30BA;&#x30E0;</trd>,
    <trd>&#x5B9F;&#x5728;&#x8AD6;  <pr>&#x3058;&#x3064;&#x3056;&#x3044;&#x308D;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">rzeczywisto&sacute;&cacute;</trd>
  <trdgrp lng="sk">
    <trd>hmotn&yacute; svet</trd>,
    <trd>re&aacute;lno</trd>,
    <trd>skuto&ccaron;no</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_a;&#x43B;&c_ib;&c_z;&#x43C;</trd>
</drv>

<drv mrk="real.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="real.0ajxo.ARK">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref cel="real.0o">Realo</ref>:
      <ekz>
        &scirc;i rememoris sian son&gcirc;on, kiu staris ankora&ubreve; anta&ubreve;
        &scirc;i tiel vive, kvaza&ubreve; &gcirc;i estus <tld/>a&jcirc;o
        <fnt><bib>Fab4</bib> <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        liaj son&gcirc;oj estas bazitaj sur <tld/>a&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        li opiniis, ke li enveturas rekte en la landon de aventuroj, kaj tamen &ccirc;irka&ubreve;e
        estis nuda <tld/>a&jcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="real.0ajxo.fenomeno">
    <dif>
      Fenomeno de la reala vivo, precipe aparta&jcirc;o de epoko a&ubreve;
      loko; <ref tip="sin" cel="fakt.0o">fakto</ref>:
      <ekz>
        tio ne estas plu plano, sed <tld/>a&jcirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 11a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en multaj partoj de l' mondo milito pro akvo povus fari&gcirc;i brutala <tld/>a&jcirc;o
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006866.php">La blua &leftquot;oro&#x201D; &ccirc;iam malsufi&ccirc;os</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[per tiu nom&scirc;an&gcirc;o la a&ubreve;toro]</klr> eble volis eviti plenan
        identigon kun la historiaj reala&jcirc;oj
        <fnt>
          <bib>MkM</bib>,
          <lok>notoj pri &ccirc;ap. 2a, p. 349a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="en">realia</trd>
  <trd lng="es">realidad</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;alit&eacute;<klr> (une r&eacute;alit&eacute; particuli&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>val&oacute; dolog</trd>,
    <trd>l&eacute;tez&odblac; dolog</trd>,
    <trd>realit&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">realiteit</trd>
  <trd lng="pl">rzecz rzeczywista</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_ja;</trd>
</drv>

<drv mrk="real.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="real.0igi.fari">
    <dif>
      Fari ion (planitan, projektitan, kontraktitan) <tld/>e
      ekzistanta:
      <ekz>
        la unuan fojon la revo de miljaroj komencas <tld/>i&gcirc;i
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi sian programon;
      </ekz>
      <ekz>
        sinjoro Eduardo havis simpation al mi, <klr>[&#x2026;]</klr> mi havis la a&gcirc;on de <esc>dekok</esc>
        jaroj kaj mi estis bela... li <tld/>igis tiun simpation en tia maniero, kiu
        certe estis por li agrabla
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam la
        a&ubreve;toroj de tiuj &ccirc;i projektoj alpa&scirc;as al ilia <tld/>igado,
        ili tuj konvinki&gcirc;as... ke tio, kio en teorio &scirc;ajnis afero
        tiel facila, en la praktiko montri&gcirc;as tre malfacila kaj
        neplenumebla
        <fnt><bib>EE</bib> <lok>VI</lok></fnt>;
      </ekz>
      <ekz>
        sukcese <tld/>igitaj projektoj
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007013.php">Ekspozicio de famaj artistoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi politikon celantan egalrajtecon
        <fnt>
	<aut>Evgeni Georgiev</aut>
	<vrk><url ref="&Monato;/2000/006818.php">Nova &scirc;anco por &ccirc;iuj etnoj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="efekti.0igi">efektivigi</ref>,
      <ref cel="plen.0umi">plenumi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_s;&c_malmol;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>prov&aacute;d&ecaron;t</trd>,
    <trd>prov&eacute;st</trd>,
    <trd>realizovat</trd>,
    <trd>uskute&ccaron;nit</trd>,
    <trd>uv&aacute;d&ecaron;t v &zcaron;ivot</trd>,
    <trd>vykonat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verwirklichen</trd>,
    <trd>realisieren</trd>
  </trdgrp>
  <trd lng="en">implement</trd>
  <trd lng="es">realizar</trd>
  <trdgrp lng="fr">
    <trd>accomplir</trd>,
    <trd>effectuer</trd>,
    <trd>r&eacute;aliser<klr> (effectuer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megval&oacute;s&iacute;t</trd>,
    <trd>realiz&aacute;l</trd>,
    <trd>beteljes&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwerkelijken</trd>,
    <trd>realiseren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>urzeczywistnia&cacute;</trd>,
    <trd>wykonywa&cacute;</trd>,
    <trd>realizowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>realizova&tcaron;</trd>,
    <trd>uskuto&ccaron;ni&tcaron;</trd>,
    <trd>vykona&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="real.0igo">
  <kap><tld/>igo</kap>
  <snc mrk="real.0igo.efektivigo">
    <dif>
      Efektivigo:
      <ekz>
        la fido estas <tld/>igo de esperata&jcirc;oj, provado de aferoj ne vidataj
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 11:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grandegajn elspezojn por la <tld/>igo de siaj artprojektoj amba&ubreve; artistoj mem
        transprenis
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007013.php">Ekspozicio de famaj artistoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la partipolitika konstelacio &ccirc;iam malhelpis <tld/>igon de tia reformo
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007124.php">Majstroverko, sed nur taktika</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        por tiu komputilo estas pluraj <ind><tld/>igo</ind>j de C.
        <trdgrp lng="cs">
          <trd>proveden&iacute;</trd>,
          <trd>realizace</trd>,
          <trd>uskute&ccaron;n&ecaron;n&iacute;</trd>,
          <trd>v&yacute;kon</trd>
        </trdgrp>
        <trd lng="de">Implementierung</trd>
        <trd lng="en">implementation</trd>
        <trdgrp lng="fr">
          <trd>implantation</trd>,
          <trd>impl&eacute;mentation</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>megval&oacute;s&iacute;t&aacute;s</trd>,
          <trd>implement&aacute;l&aacute;s</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>realisatie</trd>,
          <trd>implementatie</trd>
        </trdgrp>
        <trd lng="pl">realizacja</trd>
        <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
        <trdgrp lng="sk">
          <trd>prevedenie</trd>,
          <trd>realiz&aacute;cia</trd>,
          <trd>uskuto&ccaron;nenie</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="de">Realisierung</trd>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
</drv>

<drv mrk="real.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="real.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a, ne plu virtuala,
      <ref tip="vid" cel="efekti.0igxi">efektivi&gcirc;i</ref>:
      <ekz>
        la revo de miljaroj komencas <tld/>i&gcirc;i
        <fnt>
          <bib>Paroloj</bib>
          <aut>L. L. Zamenhof</aut>
          <vrk>Unua Kongreso 1905 en Boulogne-sur-Mer</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Konfuzebla kun <ref tip="hom" cel="al.re0igxi.KOMUNE">re&middot;al&middot;i&gcirc;&middot;i</ref>, ali&gcirc;i denove.
  </rim>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_j;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">se <ind>r&eacute;aliser</ind></trd>
  <trdgrp lng="pl">
    <trd>urzeczywistnia&cacute; si&eogonek;</trd>,
    <trd>tworzy&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="real.0isma">
  <kap><tld/>isma</kap>
  <snc mrk="real.0isma.prudenta">
    <dif>
      Prudenta, bazita sur la <tld/>o, <tld/>igebla:
      <ekz>
        liaj postuloj, planoj ne estas <tld/>ismaj;
      </ekz>
      <ekz>
        <tld/>isme pensante, ni ne atendu ke la Fina Venko
        okazos jam morga&ubreve;;
      </ekz>
      <ekz>
        tio &scirc;ajnas beda&ubreve;rinde <tld/>isma, sed ne centprocente vera
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni restu <tld/>ismaj: dekstregaj partioj ekzistas ne nur en Germanio, sed anka&ubreve;
        en aliaj demokrataj &scirc;tatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008450.html">Ne nur protesto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni levas la elspezkriterion al pli <tld/>ismaj niveloj
        <fnt>
          <bib>Monato</bib>
          <aut>A. Giridhar Rao</aut>
          <vrk><url ref="&Monato;/2008/009532.php">La barata ekonomio: miraklo neefika</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_e;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>realistick&yacute;</trd>,
    <trd>st&rcaron;&iacute;zliv&yacute;</trd>
  </trdgrp>
  <trd lng="de">realistisch</trd>
  <trd lng="en">realistic</trd>
  <trd lng="es">realista</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;aliste<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>re&aacute;lis</trd>,
    <trd>realisztikus</trd>
  </trdgrp>
  <trd lng="nl">realistisch</trd>
  <trd lng="pl">realistyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">realistick&yacute;</trd>
</drv>

<drv mrk="real.0ismo">
  <kap><ofc>3</ofc> <tld/>ismo </kap>
  <snc mrk="real.0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Platona kaj skolastika doktrino, kiu konsideras la &gcirc;eneralajn
      ideojn kiel objektive ekzistantajn:
      <ekz>
        Se vi estus filozofo / El la skol' de l' <tld/>ism', / Vi komprenus, ke
        <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Brumm Kaj Pipenbek</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="nomina.0ismo">nominalismo</ref>.
    </refgrp>
  </snc>
  <snc mrk="real.0ismo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      &Ccirc;ia arta stilo, kiu celas prezenti la mondon <tld/>-konforme
      sen plinobligo a&ubreve; fantaziemo:
      <ekz>
        e&ubreve;ropaj kritikistoj emas paroli pri <tld/>ismo a&ubreve; <ref cel="kub.0ismo">kubismo</ref>
        a&ubreve; <ref cel="ekspresionism.0o">ekspresionismo</ref> a&ubreve;
        <ref cel="real.sur0ismo">surrealismo</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jorge Camacho</nom> pintas mejlo&scirc;tone en la <tld/>isma tradicio de la esperanta
        literaturo
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2005/008606.php">Du vivpoemaroj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La 3a OA registras realism/o kun propra radiko, sed ni konsideras &gcirc;in la&ubreve;regula deriva&jcirc;o de
    real/a.
  </rim>
  <trd lng="be">&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Realismus</trd>
  <trd lng="es">realismo</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;alisme</trd>
  </trdgrp>
  <trd lng="hu">realizmus</trd>
  <trd lng="nl">realisme</trd>
  <trd lng="pl">realizm</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="real.ne0a">
  <kap>ne<tld/>a, <var><kap>mal<tld/>a</kap></var></kap>
  <snc mrk="real.ne0a.iluzia">
    <dif>
      Iluzia:
      <ekz>
        Dio elektis <klr>[objektojn]</klr> ne<tld/>ajn, por neniigi la <tld/>ajn
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 1:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rakonto &scirc;ajnas al mi tre ne<tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2012/010432.php">Romantika romano, sed &ccirc;u sukcesa?</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ektimis ne<tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2005/008559.php">Pri la aferoj misteraj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Peterburgo la fantasta, mal<tld/>a, freneziga, groteska urbo
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2003/008014.html">Ukrainaj kantoj kaj peterburgaj
          groteskoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aer.0a">aera</ref>,
      <ref cel="fantom">fantoma</ref>,
      <ref cel="rev">reva</ref>,
      <ref cel="subjek1.0a">subjektiva</ref>,
      <ref cel="tromp.0a">trompa</ref>,
      <ref cel="utopi">utopia</ref>,
      <ref cel="vent.0a">venta</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_r;&c_e;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ire&aacute;ln&iacute;</trd>,
    <trd>nere&aacute;ln&yacute;</trd>,
    <trd>neskute&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unwirklich</trd>,
    <trd>irreal</trd>
  </trdgrp>
  <trd lng="es">irreal</trd>
  <trdgrp lng="fr">
    <trd>irr&eacute;el</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nem <ind>val&oacute;s&aacute;gos</ind></trd>,
    <trd>irre&aacute;lis</trd>
  </trdgrp>
  <trd lng="nl">irre&euml;el</trd>
  <trdgrp lng="pl">
    <trd>nierealny</trd>,
    <trd>nierzeczywisty</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">neskuto&ccaron;n&yacute;</trd>
</drv>

<drv mrk="real.sur0ismo">
  <kap>sur<tld/>ismo</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="real.sur0ismo.superrealismo">
    <dif>
      <ref tip="dif" cel="real.super0ismo">Superrealismo</ref>:
      <ekz>
        sur<tld/>ismo (enkondukanta imagitan <tld/>econ)
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pentra&jcirc;oj de <nom>Germaine Hoffmann</nom> pere de esprimivaj rimedoj estas formale
        simbolismaj krea&jcirc;oj, portantaj iufoje sur<tld/>isman signifon
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008091.html">Strebo al la efika figuro</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        alia fonto <klr>[de]</klr> sur<tld/>ismo estas la psikanalizo
        <fnt><aut>G. Cappa</aut>, <vrk>Rilatoj inter lingva kaj
        literatura kreemoj</vrk>, <bib>LF</bib> <lok>jaro 1982a,
        numero 76a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ju;&c_r;&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">surrealizmus</trd>
  <trd lng="es">surrealismo</trd>
  <trd lng="hu">sz&uuml;rrealizmus</trd>
  <trd lng="nl">surrealisme</trd>
  <trd lng="pl">surrealizm</trd>
  <trd lng="ru">&c_s;&c_ju;&c_r;&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">surrealizmus</trd>
</drv>

