<drv mrk="bon.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="bon.0a.tauga">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Plene <ref cel="tauxg.0a">ta&ubreve;ga</ref> por &gcirc;ia uzo la&ubreve;destina,       
      posedanta &ccirc;iujn necesajn ecojn de sia speco:
      <ekz>
        prenu tri mezurojn da plej <tld/>a faruno, knedu, kaj faru kukojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenis bovidon delikatan kaj <tld/>an kaj <klr>[&#8230;]</klr> pretigis &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la plej <tld/>a loko de la lando lo&gcirc;igu <klr>[&#8230;]</klr> viajn fratojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas <tld/>ajn rilatojn kun preska&ubreve; &ccirc;iuj
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>a ordo tra la pordo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        interkonsento estas pli <tld/>a ol mono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        foresto de ofendo estas plej <tld/>a defendo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; plej <tld/>an &scirc;ipon mal<tld/>igas la ventoj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor.0a.celkonforma">konforma<sncref/></ref>,
      <ref cel="konven.0a.alsituacio">konvena<sncref/></ref>,
      <ref cel="regul.0a.konforma">regula<sncref/></ref>
    </refgrp>
    <trdgrp lng="no">
      <trd>god</trd>,
      <trd>bra</trd>
    </trdgrp>
    <trd lng="ru">&c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="bon.0a.kompetenta">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      <ref tip="sin" cel="kompet.0a.kapabla">Kompetenta</ref> en sia metio, fako, okupo 
      kaj plenumanta &gcirc;in konforme al la reguloj kaj
      atendoj:
      <ekz>
        li estas <tld/>a parolanto
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a mastrino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj instruistoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a teatra aktoro
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas <tld/>a me&hcirc;anikisto
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> ne estas <tld/>a justico, kiu kondamnas homon senpruve
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i akiros reputacion de bona tradukistino kaj ricevados multajn mendojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        interesas min scii kion vi pensas tiurilate, &ccirc;ar vi estas <tld/>a karakterju&gcirc;anto
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plej <tld/>a kuracisto estas la tempo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        al <tld/>a &ccirc;asisto iras mem la besto
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bon.0a.felicxa">
    <uzo tip="klr">(io)</uzo>
    <dif>
      <ref tip="sin" cel="felicx.0a.favora">Feli&ccirc;a</ref>, <ref tip="sin" cel="favor.0a">favora</ref>, 
      profitiga:
      <ekz>
        atendi <tld/>an veteron
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>an matenon</ind>
        <trd lng="id"><ind>selamat</ind> pagi!</trd>
        <trd lng="io">bona <ind>matino</ind>!</trd>
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>!
      </ekz><ekz>
        <ind><tld/>an tagon!</ind>
        <trd lng="br">demat!</trd>
        <trd lng="de">guten Tag!</trd>
        <trd lng="en">good afternoon!</trd>
        <trd lng="es">¡buenos d&iacute;as!</trd>
        <trd lng="fr">bonjour!</trd>
        <trd lng="id"><ind>selamat</ind> siang!</trd>
        <trd lng="nl">goedendag</trd>
        <trd lng="no">god dag!</trd>
        <trd lng="pl">dzie&nacute; dobry</trd>
        <trd lng="ro">bun&abreve; ziua !</trd>
        <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_y;&c_j; &c_d;&c_je;&c_n;&c_mol;!</trd>
      </ekz><ekz>
        <ind><tld/>an voja&gcirc;on!</ind>
        <trd lng="br">beaj vat!</trd>
        <trd lng="io">bona <ind>voyajo</ind>!</trd>
        <trd lng="nl">goede <ind>reis</ind>!</trd>
        <trd lng="no">god <ind>reise</ind>!</trd>
        <trd lng="pl">szcz&eogonek;&sacute;liwej podr&oacute;&zdot;y</trd>
        <trd lng="ru">&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_g;&c_o;  &c_p;&c_u;&c_t;&c_i;!</trd>
      </ekz><ekz>
        <ind>en <tld/>a horo</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="br">e <ind>koulz</ind></trd>
        <trd lng="pl">w dobr&aogonek; godzin&eogonek;</trd>
        <trd lng="ru">&c_v; &c_d;&c_o;&c_b;&c_r;&c_y;&c_j;
        &c_ch;&c_a;&c_s;</trd>
      </ekz><ekz>
        <tld/>a nova&jcirc;o, ideo;
      </ekz><ekz>
        <tld/>a afero;
      </ekz><ekz>
        plej <tld/>a estos se vi &gcirc;in forgesos
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="no">god</trd>
    <trdgrp lng="ru">
       <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_j;</trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bon.0a.humana">
    <dif>
      <ref tip="dif" cel="human.0a">Humana</ref>, 
      <ref tip="sin" cel="kompat.0ema">kompatema</ref>,
      <ref tip="sin" cel="help.0ema">helpema</ref>:
      <ekz>
