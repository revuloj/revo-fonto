    </dif>
    <trd lng="no">god</trd>
    <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="bon.0a.afabla">
    <dif>
      <ref tip="dif" cel="afabl.0a.KOMUNE">Afabla</ref>,
      <ref tip="sin" cel="komple4.0ema">komplezema</ref>:
      <ekz>
        <ind>estu tiel <tld/>a</ind> kaj proksimi&gcirc;u;
        <trd lng="pl">b&aogonek;d&zacute; tak dobry</trd>
        <trd lng="ru">&c_b;&c_u;&c_d;&c_mol;&c_t;&c_je; &c_t;&c_a;&c_k; &c_d;&c_o;&c_b;&c_r;&c_y;</trd>
      </ekz><ekz>
        <ind>havi la <tld/>econ...</ind>
        <fnt>
          <bib>FK</bib>
        </fnt>.
        <trd lng="br">kaout ar vadelezh da...</trd>
        <trd lng="de">die <ind>G&uuml;te</ind> haben</trd>
      </ekz>
      <ekz>
        de <tld/>a vorto lango ne doloras
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="no">god</trd>
    <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
