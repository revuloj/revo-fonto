<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>bon</rad>/a</kap>
<drv mrk="bon.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="bon.0a.tauga">
    <dif>
      Posedanta la karakterizajn ecojn de sia speco; konforma al sia celo,
      destino, al la regulo, al la devo; ta&ubreve;ga la&ubreve;regula:
      <ekz>
        li estas <tld/>a parolanto
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;tofo, instruisto, aparato;
      </ekz>
      <ekz>
        <tld/>a konduto;
      </ekz>
      <ekz>
        <tld/>a stilo.
      </ekz>
    </dif>
    <trd lng="ru">&c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="bon.0a.felicxa">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Feli&ccirc;a, favora, profitiga:
      <ekz>
        <tld/>an matenon
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>!
      </ekz>
      <ekz>
        <ind><tld/>an tagon!</ind>
        <trd lng="de">guten Tag!</trd>
        <trd lng="en">good afternoon!</trd>
        <trd lng="fr">bonjour!</trd>
        <trd lng="nl">goedendag</trd>
        <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_y;&c_j; &c_d;&c_je;&c_n;&c_mol;!</trd>
      </ekz>
      <ekz>
        <ind><tld/>an voja&gcirc;on!</ind>
        <trd lng="nl">goede <ind>reis</ind></trd>
        <trd lng="ru">&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_g;&c_o; &c_p;&c_u;&c_t;&c_i;!</trd>
      </ekz>
      <ekz>
        <ind>en <tld/>a horo</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="ru">&c_v; &c_d;&c_o;&c_b;&c_r;&c_y;&c_j; &c_ch;&c_a;&c_s;</trd>
      </ekz>
      <ekz>
        <tld/>a nova&jcirc;o, ideo;
      </ekz>
      <ekz>
        <tld/>a afero;
      </ekz>
      <ekz>
        plej <tld/>a estos se vi &gcirc;in forgesos<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bon.0a.humana">
    <dif>
      Humana, kompatema, helpema:
      <ekz>
        mia onklino estas <tld/>a virino
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj sentoj, <tld/>a koro.
      </ekz>
    </dif>
    <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="bon.0a.afabla">
    <dif>
      Afabla:
      <ekz>
        <ind>estu tiel <tld/>a</ind> kaj proksimi&gcirc;u;
        <trd lng="ru">&c_b;&c_u;&c_d;&c_mol;&c_t;&c_je; &c_t;&c_a;&c_k; &c_d;&c_o;&c_b;&c_r;&c_y;</trd>
      </ekz>
      <ekz>
        havi la <tld/>econ...<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_b;&c_r;&c_y;</trd>
  <trd lng="cs">dobr&yacute;</trd>
  <trd lng="de">gut</trd>
  <trd lng="en">good</trd>
  <trd lng="es">bueno</trd>
  <trd lng="fr">bon</trd>
  <trd lng="nl">goed</trd>
  <trd lng="pl">dobry</trd>
  <trd lng="sk">dobr&yacute;</trd>
  <trd lng="sw">-zuri, -ema</trd>
  <trd lng="vo">gudik</trd>
  <trd lng="zh">&#22909;<klr> [h&#462;o]</klr></trd>
</drv>

<drv mrk="bon.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc mrk="bon.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        la tran&ccirc;ilo tran&ccirc;as <tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e labori;
      </ekz>
      <ekz>
        <tld/>e al ni estas, ke vi alvenis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e fari al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e <klr>(sufi&ccirc;e, plene, tre)</klr> scii<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu <tld/>e <klr>(komforte)</klr> sidas,
        tiu lokon ne &scirc;an&gcirc;u<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pli<tld/>e <klr>(prefere)</klr> fleksi&gcirc;i ol
        rompi&gcirc;i<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bon.0e.konsentite">
    <dif>
      Konsentite, mi aprobas:
      <ekz>
        nu <tld/>e, iru<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nu, <tld/>e, mi sekvos vian konsilon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ja.0">ja</ref>,
      <ref cel="jes.0">jes</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_b;&c_r;&c_a;</trd>
  <trd lng="de">gut</trd>
  <trd lng="fr">bien<klr tip="ind"> (adv.)</klr></trd>
  <trd lng="nl">goed<klr tip="ind"> (bijw.)</klr></trd>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_o;&c_sh;&c_o;</trd>
  <trd lng="sk">dobre</trd>
  <trd lng="sw">vizuri</trd>
</drv>

<drv mrk="bon.0o">
  <kap><tld/>o, <tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        ne repagu <tld/>on <klr>(bonfaron)</klr> per mal<tld/>o;
      </ekz>
      <ekz>
        la homoj sen&ccirc;ese celadas al sia <tld/>o<fnt>Z</fnt>
        <klr>(utilo, feli&ccirc;o)</klr>;
      </ekz>
      <ekz>
        tio estas tre grava por la <tld/>o <klr>(sukceso)</klr> de nia
        afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o posedata ne estas &scirc;atata<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>pli<tld/>o estas malamiko de <tld/>o</ind>
        <trd lng="fr">le <ind>mieux</ind> est l'ennemi du bien</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu mal<tld/>o estas iom da <tld/>o<fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avanta.0o">avanta&gcirc;o</ref>,
      <ref cel="prefer.0indajxo">preferinda&jcirc;o</ref>,
      <ref cel="profit.0o">profito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_b;&c_r;&c_o;</trd>
  <trdgrp lng="de">
    <trd>Gutes</trd>,
    <trd>Erfolg</trd>,
    <trd>Nutzen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bonne action</trd>,
    <trd>bien<klr tip="ind"> (opp. au mal)</klr></trd>
  </trdgrp>
  <trd lng="nl">het <ind>goede</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_o;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sw">
    <trd>kitu kizuri</trd>,
    <trd>uzuri</trd>,
    <trd>faida</trd>
  </trdgrp>
</drv>

<drv mrk="bon.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="bon.0ulo.virtulo">
    <dif>
      Virtulo:
      <ekz>
        la <tld/>uloj kaj la mal<tld/>uloj.
      </ekz>
    </dif>
    <trd lng="de">guter Mensch</trd>
    <trd lng="fr">homme de bien</trd>
    <trd lng="nl">goed mens</trd>
    <trd lng="ru">&c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
    <trd lng="sw">mtu mzuri</trd>
  </snc>
  <snc mrk="bon.0ulo.simplanima">
    <dif>
      <tld lit="B"/>kora kaj simplanima homo.
    </dif>
    <trd lng="de">Gutmensch</trd>
    <trd lng="fr">brave homme</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_b;&c_r;&c_ja;&c_k;</trd>,
      <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
    </trdgrp>
    <trd lng="sk">dobr&aacute;k</trd>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_b;&c_r;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
</drv>

<drv mrk="bon.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc mrk="bon.mal0a.maltauxga">
    <dif>
      Ne ta&ubreve;ga por sia funkcio, celo, ne konforma al la atendoj:
      <ekz>
        el &scirc;iaj multaj infanoj unuj estas <tld/>aj kaj aliaj estas
        mal<tld/>aj
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">schlecht</trd>
    <trd lng="nl">slecht</trd>
  </snc>
  <snc mrk="bon.mal0a.malfelicxa">
    <dif>
      Malfavora, &ccirc;agrena, &gcirc;ena:
      <ekz>
        en mal<tld/>a vetero oni povas facile malvarmumi
        <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">schlecht</trd>
    <trd lng="nl">slecht</trd>
  </snc>
  <snc mrk="bon.mal0a.malhumana">
    <dif>
      Mal<tld/>aga, malica, ka&ubreve;zanta mison a&ubreve; suferon:
      <ekz>
        mal<tld/>aj infanoj amas turmenti bestojn
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">b&ouml;se</trd>
    <trd lng="nl">boos</trd>
  </snc>
  <trd lng="fr">mauvais</trd>
  <trd lng="de">schlecht</trd>
</drv>

<drv mrk="bon.mal0igi">
  <kap><ofc>*</ofc>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari ion mal<tld/>a, difekti ion:
      <ekz>
        mi mal<tld/>igis mian tutan laboron
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        akcidento mal<tld/>igis la veturilon;
      </ekz>
      <ekz>
        alkoholo mal<tld/>igas la sanon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_a;&c_r;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">verschlechtern</trd>
  <trd lng="es">empeorar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;t&eacute;riorer</trd>,
    <trd>ab&icirc;mer<klr tip="ind"> (d&eacute;t&eacute;riorer)</klr></trd>
  </trdgrp>
  <trd lng="nl">slecht maken</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_h;&c_u;&c_d;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_u;&c_d;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="bon.plej0eco">
  <kap>plej<tld/>eco</kap>
  <snc>
    <dif>
      Plej alta kvalito, plej bona stato, kiun io povas atingi:
      <ekz>
        la verkisto, kiu <klr>...</klr> influos la evoluon de la lingvo en
        la direkto de &gcirc;ia plej<tld/>eco <klr>...</klr> ankora&ubreve; ne vere
        montri&gcirc;is
        <fnt><bib>BonaLingvo</bib>, <lok>p. 95a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="perfek.0a">perfekta</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_o;&c_t;&c_p;&c_y;&c_m;&c_u;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_j;&c_l;&c_je;&c_p;&c_sh;&c_y;&c_ja; &c_w;&c_m;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_j;&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_je;&c_j;&c_sh;&c_y;&c_ja; &c_w;&c_m;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">optimum<klr> (subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_t;&c_i;&c_m;&c_u;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_i;&c_l;&c_u;&c_ch;&c_sh;&c_i;&c_je; &c_u;&c_s;&c_l;&c_o;&c_v;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="bon.pli0igi">
  <kap>pli<tld/>igi</kap>
  <snc>
    <dif>
      Meti en pli <tld/>an staton, forigante difektojn:
      <ekz>
        pli<tld/>igi aparaton, metodon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amend">amendi</ref>,
      <ref cel="korekt.0i">korekti</ref>,
      <ref cel="form.re0i">reformi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ja;&c_p;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_p;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">verbessern</trd>
  <trd lng="es">mejorar</trd>
  <trd lng="fr">am&eacute;liorer</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_l;&c_u;&c_ch;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_u;&c_ch;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="nl">verbeteren</trd>
  <trd lng="sk">zlep&scaron;i&tcaron;</trd>
  <trd lng="sw">-boresha</trd>
</drv>

<drv mrk="bon.re0igi">
  <kap>re<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti denove en <tld/>an staton, ripari:
      <ekz>
        re<tld/>igi difektitan ma&scirc;inon, eraron<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">wiedergutmachen</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;parer</trd>,
    <trd>remettre en <ind>&eacute;tat</ind></trd>
  </trdgrp>
  <trd lng="nl">opnieuw <ind>goedmaken</ind></trd>
  <trd lng="sk">oprai&tcaron;</trd>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="bon.0trovo">
  <kap><tld/>trovo</kap>
  <snc>
    <dif>
      Propra pli&scirc;ato, neatenta al alies opinioj:
      <ekz>
        diru al tiuj, kiuj profetas la&ubreve; sia propra <tld/>trovo
        <fnt><bib>MT</bib>, <lok>&Jeh; 13:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bon <ind>plaisir</ind></trd>,
    <trd>fantaisie<klr> (personnelle)</klr></trd>,
    <trd><klr>(&agrave; sa) </klr>guise</trd>
  </trdgrp>
  <trd lng="nl">goeddunken</trd>
</drv>

<drv mrk="bon.0veni">
  <kap><tld/>veni</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti akceptata kun kora kontenteco:
      <ekz>
        &gcirc;i estas al li tre <tld/>vena<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        estu <tld/>venintaj en mia domo!
      </ekz>
      <ekz>
        <ind><tld/>venon!</ind>
        <trd lng="de">willkommen!</trd>
        <trd lng="en">welcome!</trd>
        <trdgrp lng="fr">
          <trd>soyez le bienvenu!</trd>,
          <trd>soyez la bienvenue!</trd>,
          <trd>soyez les bienvenus!</trd>
        </trdgrp>
        <trd lng="nl">welkom</trd>
        <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_o; &c_p;&c_o;&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;!</trd>
        <trd lng="sw">karibu!</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_v;&c_ib;&c_t;&c_a;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_zh;&c_a;&c_d;&c_a;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; <ind>v&iacute;tan&yacute;</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="bon.0venigi">
  <kap><tld/>venigi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Saluti iun, certigante al li, ke li estas akceptata kun kora
      kontenteco.
    </dif>
    <ref tip="vid" cel="salut.0i">saluti</ref>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="es">dar la <ind>bienbevida</ind></trd>
  <trd lng="fr">accueillir<klr> (souhaiter la bienvenue)</klr></trd>
  <trd lng="de">willkommen hei&szlig;en</trd>
  <trd lng="nl">verwelkomen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
