<drv mrk="bon.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bon.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i mal<tld/>a, ne funkcianta, difektita, misa:
      <ekz>
        por esplori ovojn, &ccirc;u ili ne mal<tld/>i&gcirc;is,
        oni povas teni ilin kontra&ubreve; la lumo de la suno
        <fnt>
          <aut>-</aut>
          <vrk>El la mondo</vrk>
          <lok><url
          ref="http://anno.onb.ac.at/cgi-content/anno-plus?aid=e0a&amp;datum=1892&amp;page=28">
          La esperantisto, 1892:2, p. 28a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="degene.0i">degeneri</ref>,
      <ref cel="fal.dis0i">disfali</ref>,
      <ref cel="kaduk.0igxi">kaduki&gcirc;i</ref>,
      <ref cel="putr.0i">putri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>verschlechtern</ind></trd>,
    <trd>sich <ind>verschlimmern</ind></trd>
  </trdgrp><trdgrp lng="fr">
    <trd>se <ind>corrompre</ind></trd>,
    <trd>se <ind>d&eacute;grader</ind></trd>,
    <trd>se <ind>g&acirc;ter</ind></trd>
  </trdgrp>
  <trd lng="id">mem<ind>buruk</ind></trd>
</drv>

