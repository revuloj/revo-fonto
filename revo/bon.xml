<drv mrk="bon.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc mrk="bon.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        la tran&ccirc;ilo tran&ccirc;as <tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
        li fartas <tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e ellerni la lingvon
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        tre <tld/>e vestita
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi scias tre <tld/>e, ke la verko <klr>[&#8230;]</klr> ne povas esti senerara
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        en tiu &ccirc;i lernejo la infanoj estas edukataj tre <tld/>e
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        metiisto ne povas <tld/>e labori, se li ne majstras sian ilaron
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>malsato plej <tld/>e gustigas la man&gcirc;on</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">Hunger ist der beste Koch</trd>
      </ekz>
      <ekz>
        <ind>nova balailo <tld/>e balaas</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">neue Besen kehren gut</trd>
      </ekz>
      <ekz>
        kiu <tld/>e <klr>(komforte)</klr> sidas,
        tiu lokon ne &scirc;an&gcirc;u<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pli<tld/>e <klr>(prefere)</klr> fleksi&gcirc;i ol
        rompi&gcirc;i<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="no">
      <trd>godt</trd>,
      <trd>bra</trd>
    </trdgrp>
    <trdgrp lng="th">
      <trd>&#3629;&#3618;&#3656;&#3634;&#3591;&#3604;&#3637;</trd>,
      <trd>&#3629;&#3618;&#3656;&#3634;&#3591;&#3604;&#3637;&#3648;&#3618;&#3637;&#3656;&#3618;&#3617;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bon.0e.konsentite">
    <dif>
      Konsentite, mi aprobas:
      <ekz>
        nu, <tld/>e <klr>[&#8230;]</klr> se vi volas, prenu <klr>[&#8230;]</klr>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu, <tld/>e, mi konsentas
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
      <ekz>
        &leftquot;do <tld/>e&rightquot;, cedis Petro, &leftquot;sed mi avertas, ke &gcirc;i estas tute nefarebla&rightquot;
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ja.0">ja</ref>,
      <ref cel="jes.0">jes</ref>
    </refgrp>
    <trd lng="de">einverstanden!</trd>
    <trdgrp lng="id">
      <trd><ind>baik</ind>lah</trd>,
      <trd>oke</trd>
    </trdgrp>
    <trd lng="no">greit</trd>
    <trdgrp lng="th">
      <trd>&#3605;&#3585;&#3621;&#3591;</trd>,
      <trd>&#3650;&#3629;&#3648;&#3588;</trd>
    </trdgrp>
  </snc>
