<drv mrk="bon.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="bon.mal0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      La negativa principo morala:
      <ekz>
        Deklini&gcirc;u de mal<tld/>o kaj faru <tld/>on
        <fnt><bib>MT</bib>, <lok>&Psa; 34:14</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="bon.0o.FIL">mal<tld/>o<sncref/></ref>
  </snc>
  <snc mrk="bon.mal0o.ajxo">
    <ref tip="dif" cel="bon.mal0ajxo">mal<tld/>a&jcirc;o</ref>
    <ekz>
      El du mal<tld/>oj pli malgrandan elektu
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      Grandaj mal<tld/>oj -- grandaj rimedoj
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      Pli facile estas mal<tld/>on eviti, ol korekti
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
  </snc>
  <snc mrk="bon.mal0o.faro">
    <ref tip="dif" cel="far.malbon0o">mal<tld/>faro</ref>
    <ekz>
      E&ccirc; plej granda mal<tld/>o al <tld/>o kondukas
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      Kie &jcirc;uron vi a&ubreve;das, mal<tld/>on suspektu
      <fnt><bib>PrV</bib></fnt>.
    </ekz>      
  </snc>
  <trd lng="en">evil</trd>
  <trd lng="ru">&c_z;&c_l;&c_o;</trd>
</drv>

<drv mrk="bon.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="bon.mal0o.KOMUNE">
    <dif>
      Io mal<tld/>a:
      <ekz>
        eviti samtempe &ccirc;iujn tri mal<tld/>a&jcirc;ojn (t.e. eraron,
        kontra&ubreve;kutimecon kaj konfuzon) ni povas tre bone
        <fnt><bib>LR</bib>,
	      <lok>Respondo 47ª, Oficiala Gazeto, 1911, p. 291.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_z;&c_l;&c_o;</trd>
</drv>
      
<drv mrk="bon.mal0ega">
  <kap>mal<tld/>ega</kap>
  <snc mrk="bon.mal0ega.maltauxga">
    <dif>
      Tre mal<tld/>a:
      <ekz>
        mal<tld/>ega kanajla&jcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 18:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia famo estis malbonega, &ccirc;ar li trovi&gcirc;is en stato de sen&ccirc;esa vagado
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/>egan veteron <klr>[&#8230;]</klr> li longan tempon anta&ubreve;e sentis en siaj
        membroj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vivas en mal<tld/>egaj kondi&ccirc;oj
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_s;&c_k;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_ch;&c_je;&c_n;&c_mol; &c_p;&c_l;&c_o;&c_h;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

