        <trd lng="io">bona <ind>voyajo</ind>!</trd>
        <trd lng="nl">goede <ind>reis</ind>!</trd>
        <trd lng="no">god <ind>reise</ind>!</trd>
        <trd lng="pl">szcz&eogonek;&sacute;liwej podr&oacute;&zdot;y</trd>
        <trd lng="ru">&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_g;&c_o;  &c_p;&c_u;&c_t;&c_i;!</trd>
      </ekz><ekz>
        <ind>en <tld/>a horo</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="br">e <ind>koulz</ind></trd>
        <trd lng="pl">w dobr&aogonek; godzin&eogonek;</trd>
        <trd lng="ru">&c_v; &c_d;&c_o;&c_b;&c_r;&c_y;&c_j;
        &c_ch;&c_a;&c_s;</trd>
      </ekz><ekz>
        <tld/>a nova&jcirc;o, ideo;
      </ekz><ekz>
        <tld/>a afero;
      </ekz><ekz>
        plej <tld/>a estos se vi &gcirc;in forgesos<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="no">god</trd>
    <trdgrp lng="ru">
       <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_j;</trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bon.0a.humana">
    <dif>
      Humana, kompatema, helpema:
      <ekz>
        mia onklino estas <tld/>a virino
