<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kardon</rad>/o
</kap>
<drv mrk="kardon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="artisx.0o.genro">arti&scirc;oko</ref>
      (<trd lng="la">Cynara cardunculus</trd>), uzata kiel legomo.
    </dif>
    <ref tip="sin" cel="artisx.hispana0o">hispana arti&scirc;oko</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Kardone</trd>,
    <trd>Cardy</trd>,
    <trd>Spanische Artischocke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>atichoke thistle</trd>,
    <trd>cardoon</trd>
  </trdgrp>
  <trd lng="fr">cardon</trd>
</drv>
</art>
