<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kardon.xml,v 1.19 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kardon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kardon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kardon.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Specio de <ref tip="super" cel="artisx.0o.genro">arti&scirc;oko</ref>
      kun longaj kaj dikaj, man&gcirc;eblaj tigoj uzataj kiel <ref
      tip="super" cel="legom.0o.KUI">legomo</ref>:
      <ekz>
        mi revenis iun posttagmezon de la legomejo kun korbo da
        <tld/>oj sur la brako
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>,
        <lok>FEL, 1999</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="artisx.hispana0o">hispana arti&scirc;oko</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;</trd>,
    <trd>&c_ib;&c_s;&c_p;&c_a;&c_n;&c_s;&c_k;&c_ib; <ind>&c_a;&c_r;&c_t;&c_y;&c_sh;&c_o;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="ca">card</trd>
  <trdgrp lng="cs">
    <trd>artyčok kardový</trd>,
    <trd>artyčok planý</trd>,
    <trd>kardus zahradní</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Cardy</trd>,
    <trd>Kardone</trd>,
    <trd>Gem&uuml;se-Artischocke</trd>,
    <trd>Spanische <ind>Artischocke</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>artichoke thistle</trd>,
    <trd>cardoon</trd>
  </trdgrp>
  <trd lng="fr">cardon</trd>
  <trd lng="hu">sz&uacute;r&oacute;s <ind>artics&oacute;ka</ind></trd>
  <trd lng="nl">kardoen</trd>
  <trd lng="pl">kard</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j; <ind>&c_a;&c_r;&c_t;&c_i;&c_sh;&c_o;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="sk">španielska artičoka</trd>
  <trd lng="tr">yabanienginar</trd>
</drv>
</art>
