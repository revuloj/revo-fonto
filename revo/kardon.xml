<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kardon</rad>/o
</kap>
<drv mrk="kardon.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="artisx.0o.genro">arti&scirc;oko</ref>
      (<trd lng="la">Cynara cardunculus</trd>), uzata kiel legomo.
    </dif>
    <ref tip="sin" cel="artisx.hispana0o">hispana arti&scirc;oko</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;</trd>,
    <trd>&c_ib;&c_s;&c_p;&c_a;&c_n;&c_s;&c_k;&c_ib;
<ind>&c_a;&c_r;&c_t;&c_y;&c_sh;&c_o;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kardone</trd>,
    <trd>Cardy</trd>,
    <trd>Spanische <ind>Artischocke</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>artichoke thistle</trd>,
    <trd>cardoon</trd>
  </trdgrp>
  <trd lng="fr">cardon</trd>
  <trd lng="hu">sz&uacute;r&oacute;s <ind>artics&oacute;ka</ind></trd>
  <trd lng="nl">kardoen</trd>
  <trd lng="pl">kard</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;
<ind>&c_a;&c_r;&c_t;&c_i;&c_sh;&c_o;&c_k;</ind></trd>
  </trdgrp>
</drv>
</art>
