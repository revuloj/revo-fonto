<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kript1.xml,v 1.11 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>kript</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kript1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <klr>(p.p. pri teksto)</klr> <ref tip="vid" cel="cxifr.0i">&Ccirc;ifrita</ref>; 
      ne legebla pro manko de scio pri la uzata signaro a&ubreve; kodo:
      <ekz>
        kriptologo, kiu konfronti&gcirc;as kun <tld/>aj mesa&gcirc;oj apena&ubreve;
        tradukeblaj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2005/008430.php">Suspenso
          de komenco &gcirc;is fino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="kript.0o">kripto</ref>
    <klr>(tombejo)</klr>.
  </rim>
  <trdgrp lng="de">
    <trd>kryptisch</trd>,
    <trd>verschl&uuml;sselt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9ED9;&#x5951;&#x306E;  <pr>&#x3082;&#x304F;&#x3061;&#x304E;&#x306E;</pr></trd>,
    <trd>&#x79D8;&#x5BC6;&#x306E;  <pr>&#x3072;&#x307F;&#x3064;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ukryty</trd>,
    <trd>tajny</trd>
  </trdgrp>   
  <trd lng="pt">criptografado</trd>
  <trd lng="ro">ascuns</trd>
  <trd lng="uk">&#x442;&#x430;&#x454;&#x43C;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x52A0;&#x5BC6;&#x540E;&#x7684; <pr>ji&#x101;m&#xEC;h&#xF2;ude</pr></trd>,
    <trd>&#x52A0;&#x5BC6;&#x5F8C;&#x7684; <pr>ji&#x101;m&#xEC;h&#xF2;ude</pr></trd>,
    <trd>&#x9690;&#x79D8;&#x7684; <pr>y&#x1D0;nm&#xEC;de</pr></trd>,
    <trd>&#x96B1;&#x79D8;&#x7684; <pr>y&#x1D0;nm&#xEC;de</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kript1.el0igi">
  <kap>el<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="cxifr.mal0i">Mal&ccirc;ifri</ref>:
      <ekz>
        spicita per latinaj cita&jcirc;oj kaj aludoj al diversaj aferoj, kiujn
        oni povas malfacile el<tld/>igi
        <fnt>
          <bib>Monato</bib>
          <aut>Marcel Lemeltier</aut>
          <vrk><url ref="&Monato;/2012/010235.php">&Ccirc;u
          forgesita trezoro?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la homo <klr>...</klr> el<tld/>igis la genetikan kodon
        <fnt>
        <aut>Roland Levreaud, Gaston Waringhien</aut>,
        <bib>SPIV</bib>,
        <lok>anta&ubreve;parolo, p. 3a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>entschl&uuml;sseln</trd>,
    <trd>entziffern</trd>
  </trdgrp>
  <trd lng="nl">ontcijferen<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">deszyfrowa&cacute;</trd>
  <trd lng="pt">decifrar</trd>
  <trd lng="ro">decripta</trd>
  <trdgrp lng="sk">
    <trd>de&scaron;ifrova&tcaron;</trd>,
    <trd>rozl&uacute;&scaron;ti&tcaron; tajn&eacute; p&iacute;smo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x448;&#x438;&#x444;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x447;&#x438;&#x442;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7834;&#x8B6F; <pr>p&#xF2;y&#xEC;</pr></trd>,
    <trd>&#x7834;&#x8BD1; <pr>p&#xF2;y&#xEC;</pr></trd>,
    <trd>&#x89E3;&#x8B80; <pr>ji&#x11B;d&#xFA;</pr></trd>,
    <trd>&#x89E3;&#x8BFB; <pr>ji&#x11B;d&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>

