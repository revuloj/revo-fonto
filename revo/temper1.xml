<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>temperatur</rad>/o
    </kap>

    <drv mrk="temper1.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Fizika grando, karakterizanta la staton de termodinamika
	  ekvilibro de sistemo; perceptata kiel varmeco a&ubreve; malvarmeco
	  kaj mezurata per <ref cel="termom.0o">termometro</ref>:
	  <ekz>
	    la avera&gcirc;aj temperaturoj jare estas 0&deg;, kaj somere ne superas 10&deg;C
	    <fnt>
	      <aut>Jos&eacute; de N. Marques</aut>: <vrk>Voja&gcirc;o al Ushuaia</vrk>,
	      <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 18a</lok>
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="de">Temperatur</trd>
      <trd lng="en">temperature</trd>
      <trdgrp lng="fr">
	<trd>temp&eacute;rature</trd>
      </trdgrp>
      <trd lng="nl">temperatuur</trd>
      <trd lng="pl">temperatura</trd>
      <trd lng="ru">&c_t;&c_je;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
      <trd lng="sv">temperatur</trd>
      <trd lng="sk">teplota</trd>
    </drv>

    <drv mrk="temper1.alt0a">
      <kap>alt<tld/>a</kap>
      <uzo tip="fak">SCI</uzo>
      <snc>
	<dif>
	  Pli varma ol ordinare, varmega:
	  <ekz>
	    severa plenumado de &ccirc;iuj higienaj postuloj (uzado de malvarmigejoj,
	    alt-temperatura kuirado, ktp)
	    <fnt><aut>Vladimir Lemelev</aut>: <vrk>Nova infekta
		malsano postulanta internacian kunlaboron</vrk>,
	      <bib>Monato</bib><lok>jaro 2001a, numero 5a, p. 17a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="de">
        <trd>Hitze<klr> [&gcirc;enerale]</klr></trd>,
        <trd>Hochtemperatur<klr> [ekz-e &ccirc; te&hcirc;nikaj procesoj]</klr></trd>,
        <trd>Fieber <klr> [lata temp. de la homa korpo]</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
	<trd>&agrave; haute <ind>temp&eacute;rature</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_t;&c_je;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
    </drv>
  </art>
