<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cis</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cis.0o">
  <kap><tld/></kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>prepozicio</vspec></gra>
  <snc mrk="cis.0o.KOMUNE">
    <dif>
      Mal<ref tip="ant" cel="trans.0.loko">trans</ref>,
      <ref tip="sin" cel="flank.0ede">&ccirc;i-flanke de</ref>,
      pli proksime ol:
      <ekz>
        lia bieno sterni&gcirc;as <tld/> la rivero;
      </ekz>
      <ekz>
        restu <tld/> &ccirc;i tiu linio!
      </ekz>
    </dif>
    <rim>
      tre malofte uzata, la sinonimo <em>maltrans</em>
      a&ubreve; <em>&ccirc;i-flanke de</em> estas preferata.
      <aut>Damien BRUCKER</aut>
    </rim>
  </snc>
  <trd lng="fr">de ce c&ocirc;t&eacute;-ci de</trd>
</drv>
</art>

