<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kloroform</rad>/o
</kap>
<drv mrk="klorof1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Likva senkolora substanco, konsistanta el karbono, kloro kaj
      hidrogeno kaj kiun oni uzas por anestezi. 
    </dif>
  </snc>
  <trd lng="de">Chloroform</trd>
  <trd lng="en">chloroform</trd>
  <trd lng="nl">chloroform</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_o;&c_r;&c_m;</trd>
</drv>
<drv mrk="klorof1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anestezi per <tld/>o. 
    </dif>
  </snc>
  <trd lng="de">chloroformieren</trd>
  <trd lng="en">chloroform</trd>
  <trd lng="en">chloroformeren</trd>
<trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_o;&c_r;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="klorof1.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.  
    </dif>
  </snc>
<trd lng="en">chloroformisatie</trd>
</drv>
</art>
