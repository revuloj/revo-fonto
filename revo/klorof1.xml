<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kloroform</rad>/o
</kap>
<drv mrk="klorof1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Likva senkolora substanco, konsistanta el karbono, kloro kaj
      hidrogeno kaj kiun oni uzas por anestezi.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_ja;&c_r;&c_a;&c_f;&c_o;&c_r;&c_m;</trd>
  <trd lng="bg">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_o;&c_r;&c_m;</trd>
  <trd lng="ca">cloroform</trd>
  <trd lng="de">Chloroform</trd>
  <trd lng="en">chloroform</trd>
  <trd lng="es">cloroformo</trd>
  <trd lng="fr">chloroforme</trd>
  <trd lng="hu">kloroform</trd>
  <trd lng="nl">chloroform</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_o;&c_r;&c_m;</trd>
  <trd lng="sk">chloroform</trd>
</drv>

<drv mrk="klorof1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anestezi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_ja;&c_r;&c_a;&c_f;&c_a;&c_r;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">chloroformieren</trd>
  <trd lng="en">chloroform</trd>
  <trd lng="fr">chloroformer</trd>
  <trd lng="hu">kloroformoz</trd>  
  <trd lng="nl">chloroformeren</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_o;&c_r;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="klorof1.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_ja;&c_r;&c_a;&c_f;&c_a;&c_r;&c_m;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Chloroformierung</trd>
  <trd lng="fr">chloroformisation</trd>
  <trd lng="hu">kloroformoz&aacute;s</trd>
  <trd lng="nl">chloroformisatie</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_o;&c_r;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">anest√©zia chloroformom</trd>
</drv>
