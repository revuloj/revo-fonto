<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tambur.xml,v 1.28 2019/07/03 07:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tambur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tambur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tambur.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika <ref tip="lst" cel="instru2.frap0o"
      lst="voko:frapinstrumentoj">frapinstrumento</ref>
      konsistanta el malplena cilindro, kies amba&ubreve; bazaj flankoj
      estas fermitaj per du maldikaj stre&ccirc;itaj membranoj el ledo,
      kiujn oni frapas ordinare per bastonoj.
    </dif>
    <ekz>
      tondrigante <tld/>ojn pro bagatelo
      <fnt><bib>RugxDom</bib>
      <lok>&ccirc;apitro 62a, p. 330a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
  <trd lng="ca">tambor</trd>
  <trdgrp lng="cs">
    <trd>buben</trd>,
    <trd>tamburo</trd>
  </trdgrp>
  <trd lng="de">Trommel</trd>
  <trd lng="en">drum</trd>
  <trd lng="fr">tambour</trd>
  <trd lng="hu">dob</trd>
  <trdgrp lng="id">
    <trd>genderang</trd>,
    <trd>tambur</trd>
  </trdgrp>
  <trd lng="it">tamburo</trd>
  <trd lng="nl">trommel</trd>
  <trd lng="pl">b&eogonek;ben</trd>
  <trdgrp lng="pt">
    <trd>tambor</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
  <trd lng="sk">bubon</trd>
  <trd lng="sv">trumma</trd>
  <trd lng="zh">&#40723; [<ind>g&#468;</ind>]</trd>
</drv>

<drv mrk="tambur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="tambur.0i.SON">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      A&ubreve;digi sonon similan al tiu de <tld/>o:
      <ekz>
        <tld/>i per la fingroj sur la tablo;
      </ekz>
      <ekz>
        la bruego de la ma&scirc;inoj <tld/>as sur la orel<tld/>o
        <klr>(timpano)</klr>;
      </ekz>
      <ekz>
        en &scirc;ia koro <tld/>as la senpacienco;
      </ekz>
      <ekz>
        <tld/>pafado.
      </ekz>
    </dif>
    <trd lng="ca">tamborinar</trd>
    <trd lng="fr">tambouriner</trd>
    <trd lng="pl">b&eogonek;bni&cacute;</trd>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Anonci <tld/>ante:
      <ekz>
        <tld/>i alarmon.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>tamborinejar</trd>,
      <trd>fer sonar tambors</trd>
    </trdgrp>
    <trd lng="fr">battre du tambour</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">tlouci na buben</trd>
  <trd lng="hu">dobol</trd>
  <trdgrp lng="it">
    <trd>tamburellare</trd>,
    <trd>tambureggiare</trd>,
    <trd>stamburare</trd>
  </trdgrp>
  <trd lng="nl">trommelen</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">bubnova&tcaron;</trd>
  <trd lng="sv">trumma</trd>
</drv>

<drv mrk="tambur.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="tambur.0umo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Cilindra peco a&ubreve; parto de konstrua&jcirc;o, ekzemple bazo 
      de <ref tip="vid" cel="kupol.0o.ARKI">kupolo</ref>:
      <ekz>
        luksegaj mozaikoj kun ora fono <klr>...</klr> tegas 
        la kupolon kaj &gcirc;ian <tld/>umon
        <fnt><aut>G. Waringhien</aut>, <vrk>Ni kaj &Gcirc;i</vrk>, 
        <lok>1972</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tambor</trd>
  <trd lng="fr">tambour<klr> (archi.)</klr></trd>
  <trd lng="sk">tambor</trd>
</drv>

<drv mrk="tambur.orel0o">
  <kap>orel<tld/>o<fnt>Z</fnt></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="tambur.orel0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="timpan.0o.ANA">timpano</ref>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">taboulin<klr tip="ind"> (ar skouarn)</klr></trd>
  <trd lng="ca">timp&agrave;</trd>
  <trd lng="cs">u&scaron;n&iacute; bub&iacute;nek</trd>
  <trd lng="de">Trommelfell</trd>
  <trd lng="es">t&iacute;mpano</trd>
  <trd lng="fr">tympan<klr> (anatomie)</klr></trd>
  <trdgrp lng="he">
    <trd>&tav;&vav;&fpe; &he;&alef;&vav;&zayin;&fnun;</trd>,
    <trd>&ayin;&vav;&resh; &he;&tav;&vav;&fpe;</trd>
  </trdgrp>
  <trd lng="hu">dobh&aacute;rtya</trd>
  <trd lng="id">timpani</trd>
  <trd lng="it">timpano</trd>
  <trd lng="nl">trommelvlies</trd>
  <trd lng="pl">b&eogonek;benek uszny</trd>
  <trd lng="pt">t&iacute;mpano</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">u&scaron;n&yacute; bubienok</trd>
</drv>
</art>
