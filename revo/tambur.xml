<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tambur.xml,v 1.34 2023/10/21 13:02:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tambur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tambur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tambur.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">frapinstrumento</ref>
      konsistanta el malplena cilindro, kies amba&ubreve; bazaj flankoj
      estas fermitaj per du maldikaj stre&ccirc;itaj membranoj el ledo,
      kiujn oni frapas ordinare per bastonoj:
      <ekz>
        &ccirc;u vi a&ubreve;das la <tld/>on? bum, bum
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>!
      </ekz>
      <ekz>
        promenadi la&ubreve; la takto de <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> frapadis sin sur la ventron, &ccirc;ar <klr>[l]</klr>i ne havis
        <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu <klr>[&#x2026;]</klr> komencis ludi duoblan fluton, la alia akompanis &scirc;in per
        <tld/>o, kaj du pli junaj dancis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        li metis la manojn kun du bastonetoj sur la <tld/>on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        mar&scirc;is soldatoj, blovis trumpetojn kaj batis tamburojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Margarita</nom> postrigardis la procesion, a&ubreve;skultante silenti&gcirc;i en la foro la
        melankolian turkan <tld/>on
        <fnt>
          <bib>MkM</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        tondrigante <tld/>ojn pro bagatelo
        <fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 62a, p. 330a</lok></fnt>;
      </ekz>
      <ekz>
        malsa&gcirc;ulo, kiel <tld/>o &dash; kiu pasas, lin batas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bong.0o">bongo</ref>,
      <ref cel="tambur1.0o">tamburino</ref>,
      <ref cel="timpan.0o.MUZ">timpano<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
  <trd lng="ca">tambor</trd>
  <trdgrp lng="cs">
    <trd>buben</trd>,
    <trd>tamburo</trd>
  </trdgrp>
  <trd lng="de">Trommel</trd>
  <trd lng="en">drum</trd>
  <trd lng="fr">tambour</trd>
  <trd lng="hu">dob</trd>
  <trdgrp lng="id">
    <trd>genderang</trd>,
    <trd>tambur</trd>
  </trdgrp>
  <trd lng="it">tamburo</trd>
  <trdgrp lng="ja">
    <trd>&#x30C9;&#x30E9;&#x30E0;</trd>,
    <trd>&#x592A;&#x9F13;  <pr>&#x305F;&#x3044;&#x3053;</pr></trd>
  </trdgrp>
  <trd lng="nl">trommel</trd>
  <trd lng="pl">b&eogonek;ben</trd>
  <trdgrp lng="pt">
    <trd>tambor</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
  <trd lng="sk">bubon</trd>
  <trd lng="sv">trumma</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_b;&c_u;&c_b;&c_o;&c_n;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_b;&c_u;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#x9F13; <pr>g&#x1D4;</pr></trd>
</drv>

<drv mrk="tambur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="tambur.0i.SON">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Sonigi <tld/>on; ritme sonigi ion kvaza&ubreve; <tld/>on:
      <ekz>
        soldatoj <tld/>as kaj ludas flutojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Erich</nom> <tld/>as per la fingroj sur la tablon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en li vibras printempo, lia koro <tld/>as
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        tondro longe&hcirc;e <tld/>is
        <fnt>
          <bib>ChV</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        komerca voja&gcirc;isto <tld/>is per la piedoj por lin ur&gcirc;igi
        <fnt>
          <bib>VRA</bib>
          <lok>Harald Beijer</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto <klr>[&#x2026;]</klr> transponis la kanton por <tld/>o kaj <tld/>is &gcirc;in
        en la urbo, sur &ccirc;iuj stratoj kaj stratetoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kvar edzinoj de <nom>Umka</nom> <tld/>as la&ubreve;vice sian familian tamburinon kaj kantas
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Dua skizo:</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.produktiSonon">frapi<sncref/></ref>,
      <ref cel="martel.0i.batadi">marteli<sncref/></ref>,
      <ref cel="perkut.0i.muziki">perkuti<sncref/></ref>
    </refgrp>
    <trd lng="ca">tamborinar</trd>
    <trd lng="fr">tambouriner</trd>
    <trd lng="pl">b&eogonek;bni&cacute;</trd>
  </snc>
  <snc mrk="tambur.0i.anonci">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <ref tip="super" cel="anonc.0i.publike">Anonci</ref> <tld/>ante:
      <ekz>
        <tld/>i alarmon;
      </ekz>
      <ekz>
        li &jcirc;etis sin al la najbaro kaj <tld/>is la pordon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u, <tld/>u! dan&gcirc;ero, dan&gcirc;ero
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alarm.0i">alarmi</ref>,
      <ref cel="signal.0i">signali</ref>,
      <ref cel="tamtam.0i.FIG">tamtami<sncref/></ref>,
      <ref cel="trumpe.0i.signali">trumpeti<sncref/></ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>tamborinejar</trd>,
      <trd>fer sonar tambors</trd>
    </trdgrp>
    <trd lng="fr">battre du tambour</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">tlouci na buben</trd>
  <trdgrp lng="de">
    <trd>trommeln</trd>,
    <trd>h&auml;mmern</trd>
  </trdgrp>
  <trd lng="hu">dobol</trd>
  <trdgrp lng="it">
    <trd>tamburellare</trd>,
    <trd>tambureggiare</trd>,
    <trd>stamburare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C9;&#x30E9;&#x30E0;&#x3092;&#x6253;&#x3064;  <pr>&#x3069;&#x3089;&#x3080;&#x3092;&#x3046;&#x3064;</pr></trd>,
    <trd>&#x592A;&#x9F13;&#x3092;&#x305F;&#x305F;&#x304F;  <pr>&#x305F;&#x3044;&#x3053;&#x3092;&#x305F;&#x305F;&#x304F;</pr></trd>,
    <trd>&#x592A;&#x9F13;&#x3067;&#x77E5;&#x3089;&#x305B;&#x308B;  <pr>&#x305F;&#x3044;&#x3053;&#x3067;&#x3057;&#x3089;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">trommelen</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">bubnova&tcaron;</trd>
  <trd lng="sv">trumma</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_b;&c_n;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="tambur.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Iu kies okupo estas <tld/>ado:
      <ekz>
        en plena sunlumo sidis la <tld/>isto kaj fantaziis sur sia granda <tld/>o
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la centro estas la obsedoj de la <tld/>isto, ties &scirc;amana elvoko de la
        pasinteco per la <tld/>o, la ser&ccirc;ado de la origina harmonio
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;8-9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Trommler</trd>
  <trd lng="en">drummer</trd>
  <trd lng="fr">batteur</trd>
  <trdgrp lng="ja">
    <trd>&#x9F13;&#x624B;  <pr>&#x3053;&#x3057;&#x3085;</pr></trd>,
    <trd>&#x30C9;&#x30E9;&#x30DE;&#x30FC;</trd>
  </trdgrp>
</drv>









<drv mrk="tambur.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="tambur.0umo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Cilindra peco a&ubreve; parto de konstrua&jcirc;o, ekzemple bazo 
      de <ref tip="vid" cel="kupol.0o.ARKI">kupolo</ref>:
      <ekz>
        luksegaj mozaikoj kun ora fono <klr>[&#x2026;]</klr> tegas 
        la kupolon kaj &gcirc;ian <tld/>umon
        <fnt><aut>G. Waringhien</aut>, <vrk>Ni kaj &Gcirc;i</vrk>, 
        <lok>1972</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_a;&c_r;&c_h;., &c_b;&c_u;&c_d;.)</klr></trd>
  <trd lng="cs">tambor</trd>
  <trd lng="de">Tambour <klr>(Architektur)</klr></trd>
  <trd lng="fr">tambour<klr> (archi.)</klr></trd>
  <trd lng="sk">tambor</trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
</drv>

<drv mrk="tambur.orel0o">
  <kap>orel<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="tambur.orel0o.timpano">
    <dif>
      <ref tip="dif" cel="timpan.0o.ANA">Timpano</ref>:
      <ekz>
        li en&scirc;ovis la fingrojn pli profunden en siajn amba&ubreve; orelojn, &ccirc;iam pli profunden,
        kaj fine krevis al li orel<tld/>oj, kaj nun li jam plu nenion a&ubreve;dis
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">taboulin<klr tip="ind"> (ar skouarn)</klr></trd>
  <trd lng="ca">timp&agrave;</trd>
  <trd lng="cs">u&scaron;n&iacute; bub&iacute;nek</trd>
  <trd lng="de">Trommelfell</trd>
  <trd lng="es">t&iacute;mpano</trd>
  <trd lng="fr">tympan<klr> (anatomie)</klr></trd>
  <trdgrp lng="he">
    <trd>&tav;&vav;&fpe; &he;&alef;&vav;&zayin;&fnun;</trd>,
    <trd>&ayin;&vav;&resh; &he;&tav;&vav;&fpe;</trd>
  </trdgrp>
  <trd lng="hu">dobh&aacute;rtya</trd>
  <trd lng="id">timpani</trd>
  <trd lng="it">timpano</trd>
  <trd lng="ja">&#x4E2D;&#x8033;  <pr>&#x3061;&#x3085;&#x3046;&#x3058;</pr></trd>
  <trd lng="nl">trommelvlies</trd>
  <trd lng="pl">b&eogonek;benek uszny</trd>
  <trd lng="pt">t&iacute;mpano</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">u&scaron;n&yacute; bubienok</trd>
</drv>
