<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tambur</rad>/o
</kap>
<drv mrk="tambur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika <ref tip="super" cel="instru2.frap0o">frapinstrumento</ref>
      konsistanta el malplena cilindro, kies amba&ubreve; bazaj flankoj estas
      fermitaj per du maldikaj stre&ccirc;itaj membranoj el ledo, kiujn oni frapas
      per bastonoj.
    </dif>
    <ekz>tondrigante <tld/>ojn pro bagatelo
    <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 330a</lok></fnt>.</ekz>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
  <trd lng="de">Trommel</trd>
  <trd lng="en">drum</trd>
  <trdgrp lng="fr">
    <trd>tambour</trd>
  </trdgrp>
  <trd lng="hu">dob</trd>
  <trd lng="nl">trommel</trd>
  <trdgrp lng="pt">
   <trd>tambor</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;</trd>
  <trd lng="sv">trumma</trd>
</drv>

<drv mrk="tambur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      A&ubreve;digi sonon similan al tiu de <tld/>o:
      <ekz>
        <tld/>i per la fingroj sur la tablo;
      </ekz>
      <ekz>
        la bruego de la ma&scirc;inoj <tld/>as sur la orel<tld/>o
        <klr>(timpano)</klr>;
      </ekz>
      <ekz>
        en &scirc;ia koro <tld/>as la senpacienco;
      </ekz>
      <ekz>
        <tld/>pafado.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>tambouriner</trd>
    </trdgrp>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Anonci <tld/>ante:
      <ekz>
        <tld/>i alarmon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>battre du tambour</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="hu">dobol</trd>
  <trd lng="nl">trommelen</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">trumma</trd>
</drv>
</art>
