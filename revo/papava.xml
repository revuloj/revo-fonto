<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: papava.xml,v 1.16 2021/11/07 22:29:39 revo Exp $">
<kap><rad>papavac</rad>/oj</kap>
<drv mrk="papava.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj"
      cel="famili.0o.BIO">Familio</ref> de herboj unujaraj a&ubreve; sta&ubreve;doj
      el la ordo de ranunkoloparencaj
      <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Papaveraceae</trd>) kun:
      ofte lakta suko;
      floroj stelaj;
      du sepaloj frue defalantaj;
      kvar petaloj;
      multaj stamenoj;
      ovolujo supra, dufolia a&ubreve; multfolia;
      kapsulo kun poroj a&ubreve; klapoj:
      <ekz>
        anka&ubreve; ellaboreblas kun sezamsemoj kaj/a&ubreve; semoj el <tld/>oj sur
        la 
        lava&scirc;o anta&ubreve; la bakado, kvankam tio tre maloftas en Armenio
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=326269'>Lava&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_m;&c_a;&c_k;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">papaver&agrave;cies</trd>
  <trd lng="cs">m&aacute;kovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Mohngew&auml;chse</trd>
  <trd lng="fr">Papav&eacute;rac&eacute;es</trd>
  <trd lng="hu">m&aacute;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">papaverfamilie</trd>
  <trd lng="pl">makowate</trd>
  <trd lng="pt">papaver&aacute;ceas</trd>
  <trd lng="ro">Papaveraceae</trd>
  <trd lng="ru">&c_m;&c_a;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">makovit&eacute;</trd>
</drv>
</art>

