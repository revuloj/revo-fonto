<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sake</rad>/o <fnt><bib>JapEnc</bib></fnt>
</kap>
<drv mrk="sake.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Riz<ref tip="super" cel="brand.0o">brando</ref>
      kutime proksimume 17-elcente alkohola.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_k;&c_e;</trd>
  <trdgrp lng="de">
     <trd>Sake</trd>,
     <trd>Sake-Wein</trd>,
     <trd>japanischer <ind>Reiswein</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sak&eacute;</trd>
  </trdgrp>
  <trd lng="he">&samekh;&alef;&qof;&yod;</trd>
  <trdgrp lng="hu">
     <trd>szak&eacute;</trd>,
     <trd>rizsp&aacute;linka</trd>
  </trdgrp>
  <trd lng="nl">sake</trd>
  <trd lng="ru">&c_s;&c_a;&c_k;&c_je;</trd>
</drv>
</art>

