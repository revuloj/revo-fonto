<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mentor</rad>/o
</kap>
<drv mrk="mentor.0o">
  <kap><tld/>o</kap>
  <snc mrk="mentor.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Sa&gcirc;a konsilanto kaj gvidanto de junulo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">mentor</trd>
  <trd lng="de">Mentor</trd>
  <trd lng="en">mentor</trd>
  <trd lng="es">mentor</trd>
  <trd lng="fr">mentor</trd>
  <trdgrp lng="nl">
    <trd>mentor</trd>,
    <trd>leidsman</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>atyai j&oacute;bar&aacute;t</trd>,
    <trd>mentor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_n;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="mentor.0i">
  <kap><tld/>i</kap>
  <snc mrk="mentor.0i.PED">
    <dif>
      <ref tip="super" cel="admon.0i.instigi">Admoni</ref>, eduke 
      konsili pli junan homon:
      <ekz>
        &leftquot;ne parolu, knabo, senkerne,&rightquot; <tld/>is lin la panjo
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve; 
        fabeloj</url></vrk>,
        <lok>La granda katfabelo, [2002?]</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">admonester</trd>
</drv>
</art>
