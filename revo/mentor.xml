<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mentor.xml,v 1.18 2015/10/01 11:10:55 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mentor</rad>/o
</kap>
<drv mrk="mentor.0o">
  <kap><tld/>o</kap>
  <snc mrk="mentor.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Sa&gcirc;a konsilanto kaj gvidanto de junulo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">mentor</trd>
  <trdgrp lng="cs">
    <trd>mentor</trd>,
    <trd>mravok&aacute;rce</trd>,
    <trd>pou&ccaron;ovatel</trd>,
    <trd>r&aacute;dce</trd>
  </trdgrp>
  <trd lng="de">Mentor</trd>
  <trd lng="en">mentor</trd>
  <trd lng="es">mentor</trd>
  <trd lng="fr">mentor</trd>
  <trdgrp lng="hu">
    <trd>atyai j&oacute;bar&aacute;t</trd>,
    <trd>mentor</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mentor</trd>,
    <trd>leidsman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mentor</trd>,
    <trd>wychowawca</trd>
  </trdgrp>
  <trd lng="pt">mentor</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_n;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mentor</trd>,
    <trd>mravok&aacute;rca</trd>,
    <trd>pou&ccaron;ovate&lcaron;</trd>,
    <trd>radca</trd>
  </trdgrp>
</drv>

<drv mrk="mentor.0i">
  <kap><tld/>i</kap>
  <snc mrk="mentor.0i.PED">
    <dif>
      <ref tip="super" cel="admon.0i.instigi">Admoni</ref>, eduke 
      konsili pli junan homon:
      <ekz>
        &leftquot;ne parolu, knabo, senkerne,&rightquot; <tld/>is lin la panjo
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url
        ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve; 
        fabeloj</url></vrk>,
        <lok>La granda katfabelo, [2002?]</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">to mentor</trd>
  <trdgrp lng="cs">
    <trd>k&aacute;rat</trd>,
    <trd>vyt&yacute;kat</trd>
  </trdgrp>
  <trd lng="fr">admonester</trd>
  <trdgrp lng="pl">
    <trd>upomina&cacute;</trd>,
    <trd>napomina&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>k&aacute;ra&tcaron;</trd>,
    <trd>vyt&yacute;ka&tcaron;</trd>
  </trdgrp>
</drv>
</art>
