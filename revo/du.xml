<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>du</rad>
</kap>
<drv mrk="du.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Unu kaj unu. Matematika simbolo 2.
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_d;&c_v;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dva</trd>,
    <trd>dv&ecaron;</trd>
  </trdgrp>
  <trd lng="de">zwei</trd>
  <trd lng="en">two</trd>
  <trd lng="es">dos</trd>
  <trd lng="fr">deux</trd>
  <trd lng="it">due</trd>
  <trd lng="nl">twee</trd>
  <trd lng="pl">dwa</trd>
  <trd lng="ru">&#x434;&#x432;&#x430;</trd>
  <trd lng="sv">tv&aring;</trd>
  <trd lng="tr">iki</trd>
</drv>

<drv mrk="du.0ono">
  <kap><tld/>ono</kap>
  <snc>
    <dif>
      Unu el la 2 egalaj dividoj de tuto:
      <ekz>
        je la tria <klr>(horo)</klr> kaj <tld/>ono;
      </ekz>
      <ekz>
        <tld/>onfunto;
      </ekz>
      <ekz>
        ne havi <tld/>onon da penso en la kapo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        komenco bona, laboro <tld/>ona<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>one fari ion <klr>(fu&scirc;i)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_o;&c_v;&c_i;&c_n;</trd>
  <trd lng="de">H&auml;lfte</trd>
  <trd lng="fr">moiti&eacute;</trd>
  <trd lng="nl">half</trd>
  <trd lng="ru">&#x43f;&#x43e;&#x43b;&#x43e;&#x432;&#x438;&#x43d;&#x430;</trd>
  <trd lng="sv">halv</trd>
  <trd lng="tr">yar&inodot;</trd>
</drv>

<drv mrk="du.0obla">
  <kap><tld/>obla</kap>
  <snc>
    <dif>
      <tld lit="D"/>foje pli granda:
      <ekz>
        <tld/>obla profito;
      </ekz>
      <ekz>
        <tld/>obla sumo da mono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ricevi <tld/>oble pli<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Konsistanta el <tld/> partoj:
      <ekz>
        <tld/>obla kaverno
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Genezo 23, 19</lok>)</fnt>;
      </ekz>
      <ekz>
        ili faris la surbrusta&jcirc;on <tld/>obla
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Eliro 39, 9</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="D"/>foja:
      <ekz>
        <tld/>obla frapo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jeremia 17, 18</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">doppelt</trd>
  <trd lng="fr">double<klr tip="ind"> (deux fois)</klr></trd>
  <trd lng="ru">&#x434;&#x432;&#x43e;&#x439;&#x43d;&#x43e;&#x439;</trd>
  <trd lng="sv">dubbel</trd>
</drv>

<drv mrk="du.0oniganto">
  <kap><tld/>oniganto</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="du.0oniganto.punkto">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="mez.0o.punkto">mezo.</ref>
  </snc>
  <snc mrk="du.0oniganto.rekto">
    <ref tip="dif" cel="sekc.du0anto">dusekcanto.</ref>
    <rim>
      &Ccirc;i-sence Bricard
      <fnt><bib>MatTerm</bib><lok>p. 26</lok></fnt>
      uzas la formon <ctl><tld/>onanto</ctl>, kiu ne &scirc;ajnas
      imitinda kunmeta&jcirc;o.
    </rim>
    <trd lng="de">Halbierende</trd>
    <trd lng="en">bisectrix</trd>
    <trd lng="fr">bissectrice</trd>
    <trd lng="nl">bissectrice</trd>
    <trd lng="ru">&c_b;&c_i;&c_s;&c_s;&c_je;&c_k;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>
  </snc>
</drv>

<drv mrk="du.0uma">
  <kap><tld/>uma</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="fak">KOMP</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="vid" cel="um.0.nombrosistemo">-um.</ref>
    <trd lng="de">bin&auml;r</trd>
    <trd lng="en">binary</trd>
    <trd lng="fr">binaire</trd>
    <trd lng="pl">dw&oacute;jkowy</trd>
    <trd lng="ru">&#x434;&#x432;&#x43e;&#x438;&#x447;&#x43d;&#x44b;&#x439;</trd>
    <trd lng="sv">bin&auml;r</trd>
  </snc>
</drv>
</art>
