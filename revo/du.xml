<drv mrk="du.0">
  <kap><ofc>*</ofc><tld/></kap>
  <mlg>2</mlg>
  <snc mrk="du.0.MAT">
    <dif>
      Unu kaj unu:
      <ekz>
        <tld/> homoj povas pli multe fari ol unu
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>;
      </ekz>
      <ekz>
        en la da&ubreve;ro de kelke da minutoj mi a&ubreve;dis <tld/> pafojn
        <fnt>
          <bib>F</bib>
          <lok>&FE; 40</lok>
        </fnt>;
      </ekz>
      <ekz>
        per unu &scirc;tono oni <tld/> &jcirc;etojn ne faras
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la popolo sin dividis en <tld/> partiojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu urbo estas plu dividita je <tld/> partoj &#8208; serboj lo&gcirc;antaj
        norde, kaj albanoj sude
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url
          ref='http://www.esperanto.be/fel/mon/007007.html'>Jugoslavio:
          Atenco ne preteratentita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
     <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj"
     val="2">nombro</ref>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_v;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>daou<klr> [vira]</klr></trd>,
    <trd>div<klr> [ina]</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dos<klr> (vira)</klr></trd>,
    <trd>dues<klr> (ina)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dv&ecaron;</trd>,
    <trd>dva</trd>,
    <trd>dv&ecaron;</trd>
  </trdgrp>
  <trd lng="de">zwei</trd>
  <trd lng="en">two</trd>
  <trd lng="es">dos</trd>
  <trd lng="fi">kaksi</trd>
  <trd lng="fr">deux</trd>
  <trd lng="he">&shin;&nun;&yod;&yod;&fmem;</trd>
  <trd lng="hi">&#2342;&#2379; [<ind>d&#333;</ind>]</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;t</trd>,
    <trd>kett&odblac;</trd>
  </trdgrp>
  <trd lng="id">dua</trd>
  <trd lng="it">due</trd>
  <trd lng="ja">&#20108; [<ind>&#12395;</ind>]</trd>
  <trd lng="ku">du</trd>
  <trd lng="nl">twee</trd>
  <trd lng="no">to</trd>
  <trd lng="pl">dwa</trd>
  <trd lng="pt">dois</trd>
  <trd lng="ru">&c_d;&c_v;&c_a;</trd>
  <trd lng="sk">dva</trd>
  <trd lng="sv">tv&aring;</trd>
  <trd lng="sw">-wili</trd>
  <trd lng="tp">tu</trd>
  <trd lng="tr">iki</trd>
