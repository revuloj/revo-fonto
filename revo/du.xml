<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>du</rad>
    </kap>

    <drv mrk="du.0">
      <kap><ofc>*</ofc><tld/></kap>
      <snc mrk="du.0.MAT">
        <dif>
          Unu kaj unu. Matematike 2:
          <ekz>
            <tld/> homoj povas pli multe fari ol unu
            <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_d;&c_v;&c_a;</trd>
      <trdgrp lng="br">
        <trd>daou<klr> [vira]</klr></trd>,
        <trd>div<klr> [ina]</klr></trd>
      </trdgrp>
      <trdgrp lng="bg">
        <trd>&c_d;&c_v;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>dos<klr> (vira)</klr></trd>,
        <trd>dues<klr> (ina)</klr></trd>
      </trdgrp>
      <trdgrp lng="cs">
        <trd>dva</trd>,
        <trd>dv&ecaron;</trd>
      </trdgrp>
      <trd lng="de"><ofc>*</ofc>zwei</trd>
      <trd lng="en"><ofc>*</ofc>two</trd>
      <trd lng="es">dos</trd>
      <trd lng="fr"><ofc>*</ofc>deux</trd>
      <trd lng="he">&shin;&nun;&yod;&yod;&fmem;</trd>
      <trdgrp lng="hu">
        <trd>k&eacute;t</trd>,
        <trd>kett&odblac;</trd>
      </trdgrp>
      <trd lng="it">due</trd>
      <trd lng="nl">twee</trd>
      <trd lng="pl"><ofc>*</ofc>dwa</trd>
      <trd lng="ru"><ofc>*</ofc>&c_d;&c_v;&c_a;</trd>
      <trd lng="sv">tv&aring;</trd>
      <trd lng="sw">-wili</trd>
      <trd lng="tr">iki</trd>
      <trd lng="vo">tel</trd>
      <trd lng="zh">&#20108;<klr> [&egrave;r]</klr></trd>
    </drv>

    <drv mrk="du.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="du.0a.MAT">
        <dif>
          Vicmontra numeralo responda al <tld/>; kiu (la&ubreve; iu ordo) venas
          venas tuj post la unua:
          <ekz>
            februaro estas la <tld/>a monato de la jaro.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="br">
        <trd>eil</trd>,
        <trd>daouvet<klr> [vira]</klr></trd>,
        <trd>divvet<klr> [ina]</klr></trd>
      </trdgrp>
      <trd lng="de">zweite</trd>
      <trd lng="fr">deuxi&egrave;me</trd>
      <trd lng="sw">-a pili</trd>
      <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_o;&c_j;</trd>
    </drv>

    <drv mrk="du.0e">
      <kap><ofc>*</ofc><tld/>e</kap>
      <snc mrk="du.0e.MAT">
        <dif>
          En la <tld/>a loko, post unu anta&ubreve;a:
          <ekz>
            unue mi redonas al vi la monon <klr>...</klr>
            <tld/>e mi dankas vin
            <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">da <ind>eil</ind></trd>
      <trd lng="de">zweitens</trd>
      <trd lng="fr">deuxi&egrave;mement</trd>
      <trd lng="sw">pili</trd>
      <trd lng="ru">&c_v;&c_o;-&c_v;&c_t;&c_o;&c_r;&c_y;&c_h;</trd>
    </drv>

    <drv mrk="du.0ono">
      <kap><ofc>*</ofc><tld/>ono</kap>
      <snc mrk="du.0ono.MAT">
        <dif>
          Unu el la <tld/> egalaj dividoj de tuto:
          <ekz>
            je la tria <klr>(horo)</klr> kaj <tld/>ono;
          </ekz><ekz>
            tri estas <tld/>ono de ses
            <fnt><bib>F</bib>, <lok>&FE;14.</lok></fnt>;
          </ekz><ekz>
            <tld/>onfunto;
          </ekz><ekz>
            ne havi <tld/>onon da penso en la kapo<fnt>Z</fnt>;
          </ekz><ekz>
            komenco bona, laboro <tld/>ona<fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <tld/>one fari ion <klr>(fu&scirc;i)</klr>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_l;&c_o;&c_v;&c_a;</trd>
      <trd lng="br">hanter</trd>
      <trd lng="ca">meitat</trd>
      <trd lng="bg">&c_p;&c_o;&c_l;&c_o;&c_v;&c_i;&c_n;</trd>
      <trd lng="de">H&auml;lfte</trd>
      <trd lng="en">half</trd>
      <trd lng="es">mitad</trd>
      <trd lng="fr">moiti&eacute;</trd>
      <trd lng="he">&het;&tsadi;&yod;</trd>
      <trd lng="hu">f&eacute;l</trd>
      <trd lng="nl">helft</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
      <trd lng="sv">halv</trd>
      <trd lng="sw">nusu</trd>
      <trd lng="tr">yar&inodot;</trd>
    </drv>

    <drv mrk="du.0obla">
      <kap><ofc>*</ofc><tld/>obla</kap>
      <snc>
        <dif>
          <tld lit="D"/>foje pli granda:
          <ekz>
            por la hodia&ubreve;a tago mi ricevis <tld/>oblan pagon
            <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
          </ekz><ekz>
            <tld/>obla profito;
          </ekz><ekz>
            ricevi <tld/>oble pli<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <snc>
        <dif>
          Konsistanta el <tld/> partoj:
          <ekz>
            <tld/>obla kaverno
            <fnt><bib>MT</bib>, <lok>&Gen; 23:19</lok></fnt>;
          </ekz><ekz>
            ili faris la surbrusta&jcirc;on <tld/>obla
            <fnt><bib>MT</bib>, <lok>&Eli; 39:9</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <snc>
        <dif>
          <tld lit="D"/>foja:
          <ekz>
            <tld/>obla frapo
            <fnt><bib>MT</bib>, <lok>&Jer; 17:18</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_d;&c_v;&c_a;&c_j;&c_n;&c_y;</trd>,
        <trd>&c_p;&c_a;&c_d;&c_v;&c_o;&c_j;&c_n;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="br">
        <trd>doubl</trd>,
        <trd>daougement</trd>
      </trdgrp>
      <trd lng="ca">doble</trd>
      <trd lng="de">doppelt</trd>
      <trd lng="es">doble</trd>
      <trd lng="fr">double<klr tip="ind"> (deux fois)</klr></trd>
      <trd lng="he">&kaf;&pe;&vav;&lamed;</trd>
      <trdgrp lng="hu">
        <trd>kett&odblac;s</trd>,
        <trd>k&eacute;tszeres</trd>,
        <trd>dupla</trd>
      </trdgrp>
      <trd lng="nl">dubbel</trd>
      <trd lng="ru">&c_d;&c_v;&c_o;&c_j;&c_n;&c_o;&c_j;</trd>
      <trd lng="sv">dubbel</trd>
    </drv>

    <drv mrk="du.0oniganto">
      <kap><tld/>oniganto</kap>
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>PIV1</bib></fnt>
      <snc mrk="du.0oniganto.punkto">
        <uzo tip="stl">EVI</uzo>
        <ref tip="dif" cel="mez.0o.punkto">mezo.</ref>
      </snc>
      <snc mrk="du.0oniganto.rekto">
        <ref tip="dif" cel="sekc.du0anto">dusekcanto.</ref>
        <rim>
          &Ccirc;i-sence Bricard
          <fnt><bib>MatTerm</bib>, <lok>p. 26</lok></fnt>
          uzas la formon <ctl><tld/>onanto</ctl>, kiu ne &scirc;ajnas
          imitinda kunmeta&jcirc;o.
        </rim>
        <trd lng="be">&c_b;&c_ib;&c_s;&c_e;&c_k;&c_t;&c_r;&c_y;&c_s;&c_a;</trd>
        <trd lng="ca">bisectriu</trd>
        <trd lng="de">Halbierende</trd>
        <trdgrp lng="en">
          <trd>bisector</trd>,
          <trd>bisectrix</trd>
        </trdgrp>
        <trd lng="fr">bissectrice</trd>
        <trd lng="hu">sz&ouml;gfelez&odblac;</trd>
        <trd lng="nl">bissectrice</trd>
        <trd lng="pl">dwusieczna<klr tip="ind"> (k&aogonek;ta)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_b;&c_i;&c_s;&c_s;&c_je;&c_k;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>,
          <trd>&c_r;&c_a;&c_v;&c_n;&c_o;&c_d;&c_je;&c_l;&c_ja;&c_shch;&c_a;&c_ja;</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="du.0uma">
      <kap><tld/>uma</kap>
      <uzo tip="fak">MAT</uzo>
      <uzo tip="fak">KOMP</uzo>
      <fnt><bib>PIV1</bib></fnt>
      <snc>
        <ref tip="vid" cel="um.0.nombrosistemo">-um.</ref>
        <trd lng="be">&c_d;&c_v;&c_a;&c_j;&c_k;&c_o;&c_v;&c_y;</trd>
        <trd lng="ca">binari</trd>
        <trd lng="de">bin&auml;r</trd>
        <trd lng="en">binary</trd>
        <trd lng="es">binario</trd>
        <trd lng="fr">binaire</trd>
        <trd lng="he">&bet;&yod;&nun;&alef;&resh;&yod;</trd>
        <trdgrp lng="hu">
          <trd>kett&odblac;s<klr tip="ind"> (bin&aacute;ris)</klr></trd>,
          <trd>bin&aacute;ris</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>binair</trd>,
          <trd>tweedelig</trd>
        </trdgrp>
        <trd lng="pl">dw&oacute;jkowy</trd>
        <trd lng="ru">&c_d;&c_v;&c_o;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
        <trd lng="sv">bin&auml;r</trd>
      </snc>
    </drv>

    <drv mrk="du.jar0ono">
      <kap>jar<tld/>ono</kap>
      <snc mrk="du.jar0ono.KAL">
        <uzo tip="fak">KAL</uzo>
        <dif>
          <tld lit="D"/>ono de la kalendara
          <ref tip="malprt" cel="jar.0o.AST">jaro<sncref/></ref>,
          kiu komenci&gcirc;as (la unua jar<tld/>ono) a&ubreve; fini&gcirc;as
          (la <tld/>a jar<tld/>ono) kun tia jaro:
          <ekz>
            la unua jar<tld/>ono konsistas el la du unuaj
            <ref tip="prt" cel="kvar.jar0ono.KAL">jarkvaronoj</ref>,
            la dua jar<tld/>ono konsistas el la du lastaj jarkvaronoj;
          </ekz><ekz>
            la altigo de administrataj prezoj ka&ubreve;zos turnon en la
            inflacia evoluo en la dua jar<tld/>ono
            <fnt>
              <aut>Evgeni GEORGIEV</aut>:
              <vrk>Inflacio jam ne estigas teruron</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_g;&c_o;&c_d;&c_i;&c_je;</trd>
      </snc>
      <snc mrk="du.jar0ono.AST">
        <uzo tip="fak">AST</uzo>
        <ref tip="dif" cel="jar.duon0o.AST"><tld/>onjaro<sncref/></ref>
        <ekz>
          ekde kiam last-a&ubreve;guste kelkaj el ni konferencis en la franca
          Montpeliero jam pasis preska&ubreve; jar<tld/>ono
          <fnt>
            <aut>M. La Torre</aut>,
            <vrk><url ref="http://www.ilei.info/ipr/kion_fari.htm"
                >Kion fari por (kaj per) ILEI?</url></vrk>,
            <lok>1999-01-17</lok>
          </fnt>
        </ekz>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_g;&c_o;&c_d;&c_a;</trd>
      </snc>
      <trd lng="fr">semestre</trd>
    </drv>
  </art>
