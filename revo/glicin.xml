<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glicin.xml,v 1.20 2023/11/10 18:31:36 revo Exp $">
<kap>
  <rad>glicin</rad>/o
</kap>
<drv mrk="glicin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="acid.amin0o">Aminacido</ref>
      NH<sub>2</sub>CH<sub>2</sub>COOH,
      anka&ubreve; uzata kiel <ref tip="super" cel="rivel.0ilo">rivelilo</ref>:
      <ekz>
         <tld/>o, anka&ubreve; nomata glikokolo a&ubreve; aminoaceto (simbolo Gly a&ubreve; G),
         estas blanka solida&jcirc;o <klr>[... kaj]</klr> la plej simpla kaj
         malpeza aminoacido
         <fnt>
           <bib>Viki</bib>,
           <vrk><url ref="&Viki;/Glicino">Glicino</url></vrk>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    <ref cel="glicin2.0o">glicino (genro, sojo)</ref>,
    <ref cel="glicin1.0o">glicineo, glicinio (ornamplanto)</ref>
  </refgrp>
  <trd lng="be">&c_g;&c_l;&c_ib;&c_c;&c_y;&c_n;</trd>
  <trd lng="bg">&c_g;&c_l;&c_i;&c_c;&c_i;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>glic&iacute;</trd>,
    <trd>glicina</trd>,
    <trd>fotoglicina</trd>
  </trdgrp>
  <trd lng="cs">hl&iacute;st</trd>
  <trdgrp lng="de">
     <trd>Glyzin</trd>,
     <trd>Leims&uuml;&szlig;</trd>,
     <trd>Aminoessigs&auml;ure</trd>
  </trdgrp>
  <trd lng="en">glycine</trd>
  <trd lng="fa">&f_g;&a_l;&f_y;&a_s;&f_y;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>glycin</trd>,
    <trd>glycine<klr tip="ind">
(r&eacute;v&eacute;lateur)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>glicin</trd>,
    <trd>glikokoll</trd>,
    <trd>enyvcukor</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C0;&#x30A4;&#x30BA;&#x5C5E;
    <pr>&#x30C0;&#x30A4;&#x30BA;&#x305E;&#x304F;</pr></trd>,
    <trd>&#x30B0;&#x30EA;&#x30B7;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">glycine</trd>
  <trd lng="pl">glicyna</trd>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_c;&c_i;&c_n;</trd>
  <trd lng="sk">hl&iacute;sta</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43B;&c_ib;&c_c;&c_i;&#x43D;&c_ib;&#x44F;</trd>,
    <trd>&c_g;&#x43B;&c_ib;&c_c;&c_i;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
