       <ref cel="altern2.0o.dilemo">alternativo<sncref/></ref>,
       <ref cel="opozic.0o.FON">opozicio</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Standpunkt</trd>,
      <trd>Einstellung</trd>,
      <trd>Ansicht <klr>(Position)</klr></trd>
    </trdgrp>
  </snc>
