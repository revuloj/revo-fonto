  <trd lng="hu">kotta</trd>
  <trd lng="id">partitur</trd>
  <trd lng="ja">&#x697D;&#x8B5C; <pr>&#x304C;&#x304F;&#x3075;</pr></trd>
  <trdgrp lng="nl">
    <trd>noten</trd>,
    <trd>bladmuziek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nuty</trd>,
    <trd>partytura</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_t;&c_y;</trd>
