<drv mrk="not.pied0i">
  <kap>pied<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="not.pied0i.TIP">
    <dif>
      Krei pied<tld/>on:
      <ekz>
        pri personaj nomoj ofte uzataj en &ccirc;i tiu libro mi
        pied<tld/>as nur unufoje
        <fnt>
          <aut>D. Broadribb</aut>
          <vrk>Anta&ubreve;parolo de la Tradukinto</vrk>
          <lok>en: La Respubliko, 1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        ioma pied<tld/>ado necesas por avera&gcirc;aj legantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2007/009029.php">Irigu vin tra intrigo
          vintra</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_z;&c_n;&c_o;&c_s;&c_k;&c_u;</ind></trd>
  <trd lng="de">anmerken <klr>(per Fu&szlig;note)</klr></trd>
  <trd lng="fr">annoter<klr> (par note en pied de page)</klr></trd>
</drv>

<drv mrk="not.pied0o">
  <kap>pied<tld/>o
    <fnt>
      <bib>FK</bib>
    </fnt>
  </kap>
  <snc mrk="not.pied0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Komenta <ref tip="super" cel="not.0o.memoriga"><tld/>o</ref> de 
      skriba verko, kiu aperas en la malsupra, pieda parto de
      pa&gcirc;o:
      <ekz>
        la nun a&ccirc;etebla eldono de 1954 enhavas multajn
        pied<tld/>ojn
        <fnt><bib>Plumamikoj</bib>, <lok>kelkaj specialaj
        verkoj</lok></fnt>;
      </ekz>
      <ekz>
        aperis nur unu pied<tld/>o sub legantoletero dum &ccirc;i tiuj preska&ubreve; tri jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Aimo Rantanen</aut>
          <vrk><url ref="&Monato;/2004/008379.html">Senkura&gcirc;o malbonas kiel
          konsilanto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Irako fari&gcirc;as nura pied<tld/>o <klr>(negrava&jcirc;o)</klr> en televidaj nova&jcirc;bultenoj,
        duonkolumna kroma&jcirc;o
        e&ccirc; en &leftquot;seriozaj&rightquot; gazetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2003/008119.html">Taglibro de l' milito ... el la
          lando de &Scirc;ekspiro</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        enhava &gcirc;ustigo de <nom>Brian</nom> plenumita en la teksto, kvankam mi preferus
        pied<tld/>igi &gcirc;in
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006884.php">&Ccirc;u veneno en Vieno?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_n;&c_o;&c_s;&c_k;&c_a; <klr tip="amb">(&c_u; &c_t;&c_e;&c_k;&c_s;&c_mol;&c_c;&c_je;)</klr></trd>
  <trd lng="br">notenn diaz ar bajenn</trd>
  <trd lng="cs">pozn&aacute;mka pod &ccaron;arou</trd>
  <trd lng="de">Fu&szlig;note</trd>
  <trd lng="en">footnote</trd>
  <trd lng="es">nota al pie</trd>
  <trd lng="fr">note de bas de page</trd>
  <trd lng="ja">&#33050;&#27880; [<ind>&#12385;&#12419;&#12367;&#12385;&#12421;&#12358;</ind>]</trd>
  <trd lng="nl">voetnoot</trd>
  <trdgrp lng="pl">
    <trd>stopka</trd>,
    <trd>przypis</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_o;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">pozn&aacute;mka pod &ccaron;iarou</trd>
  <trd lng="uk">&c_v;&c_i;&c_n;&c_o;&c_s;&c_k;&c_a; (&c_v; &c_k;&c_ib;&c_n;&c_c;&c_ib; &c_s;&c_t;&c_o;&c_r;&c_ib;&c_n;&c_k;&c_i;)</trd>
</drv>
