      mallongan memorigan resumon:
      <ekz>
        <tld/>u la daton;
      </ekz>
      <ekz>
        dika nigra libro, en kiu oni <tld/>is malaperintajn &scirc;ipojn
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        24a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>notieren</trd>,
      <trd>vermerken</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>feljegyez</trd>,
      <trd>lejegyez</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#x30E1;&#x30E2;&#x3059;&#x308B; <pr>&#x3081;&#x3082;&#x3059;&#x308B;</pr></trd>,
      <trd>&#x899A;&#x66F8;&#x3059;&#x308B; <pr>&#x304A;&#x307C;&#x3048;&#x304C;&#x304D;&#x3059;&#x308B;</pr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>notowa&cacute;</trd>,
      <trd>robi&cacute; <ind>notatki</ind></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x8A18;&#x4E0B; <pr>j&#xEC;xi&#xE0;</pr></trd>,
    <trd>&#x8BB0;&#x4E0B; <pr>j&#xEC;xi&#xE0;</pr></trd>,
