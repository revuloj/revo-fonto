<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <ofc>*</ofc>
       <rad>not</rad>/o
     </kap>

     <drv mrk="not.0o">
       <kap><ofc>*</ofc><tld/>o</kap>
       <snc mrk="not.0o.memoriga">
	<dif>
	  Mallonga memoriga enskribo:
	  <ekz>
	    fari, konservi <tld/>ojn pri fakto, parolado, adreso, dato;
	  </ekz><ekz>
	    fari <tld/>on en <tld/>libro.
	  </ekz>
	</dif>
	<trdgrp lng="be">
	  <trd>&c_n;&c_a;&c_t;&c_a;&c_t;&c_k;&c_a;</trd>,
	  <trd>&c_z;&c_a;&c_c;&c_je;&c_m;&c_k;&c_a;</trd>
	</trdgrp>
	<trd lng="de">Notiz</trd>
	<trdgrp lng="nl">
	  <trd>nota</trd>
	</trdgrp>
	<trd lng="ru">&c_z;&c_a;&c_m;&c_je;&c_t;&c_k;&c_a;</trd>
       </snc>
       <snc mrk="not.0o.komento">
	<dif>
	  Mallonga skriba rimarko a&ubreve; komento:
	  <ekz>
	    libro kun klarigaj <tld/>oj;
	  </ekz><ekz>
	    presigi <tld/>ojn pri lingvo internacia<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trdgrp lng="be">
	  <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_g;&c_a;</trd>,
	  <trd>&c_z;&c_a;&c_c;&c_je;&c_m;&c_k;&c_a;</trd>
	</trdgrp>
	<trd lng="de">Anmerkung</trd>
	<trdgrp lng="nl">
	  <trd>aantekening</trd>,
	  <trd>notitie</trd>
	</trdgrp>
	<trdgrp lng="ru">
	  <trd>&c_z;&c_a;&c_m;&c_je;&c_t;&c_k;&c_a;</trd>,
	  <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>
	</trdgrp>
       </snc>
       <snc mrk="not.0o.MUZ">
	<uzo tip="fak">MUZ</uzo>
	<dif>
	  Skriba signo lokita sur muzika liniaro por montri la altecon
	  kaj da&ubreve;ron de la tono.
	</dif>
	<subsnc mrk="not.0o.tono">
	  <dif>
	    La sono mem, prezentata per <tld/>o<sncref ref="not.0o.MUZ"/>,
	    en kantado a&ubreve; en muzikila plenumo:
	    <ekz>
	      la fingroj trovu la ekzaktan lokon por ludi ekzaktan <tld/>on
	      <fnt>
		<lok><url
		ref="http://aliso.cnice.mecd.es/~jheras1/tarrego.htm"
		    >Vivo kaj Muziko de Francisco T&aacute;rrega</url></lok>
	      </fnt>;
	    </ekz><ekz>
	      <ind>falsa <tld/>o</ind>
	      <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_a;&c_ja; &c_n;&c_o;&c_t;&c_a;</trd>
	      <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_a;&c_ja; &c_n;&c_o;&c_t;&c_a;</trd>
	    </ekz>.
	  </dif>
	</subsnc>
	<subsnc mrk="not.0o.nuanco">
	  <uzo tip="stl">FIG</uzo>
	  <dif>
	    Aparta distingiga eco a&ubreve; nuanco (la&ubreve; <tld/>o<sncref 
ref="not.0o.tono"/>):
	    <ekz>
	      lia stilo havas tre personan <tld/>on;
	    </ekz><ekz>
	      tio portis funebran <tld/>on en la &gcirc;ojan feston.
	    </ekz>
	  </dif>
	  <trd lng="de">Note</trd>
	  <trd lng="nl">noot</trd>
	  <trd lng="ru">&c_n;&c_o;&c_t;&c_a;</trd>
	</subsnc>
	<trd lng="be">&c_n;&c_o;&c_t;&c_a;</trd>
	<trd lng="de">Note</trd>
	<trdgrp lng="nl">
	  <trd>noot</trd>,
	  <trd>muzieknoot</trd>
	</trdgrp>
	<trd lng="ru">&c_n;&c_o;&c_t;&c_a;<klr tip="amb"> 
(&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;)</klr></trd>
       </snc>
       <snc mrk="not.0o.lerneja">
	<dif>
	  Oficiale uzata signo por indiki takson se scioj a&ubreve; konduto
	  de lernanto:
	  <ekz>
	    la <tld/>o <ctl>kvar</ctl> signifas <ctl>bone</ctl>,
	    la <tld/>o <ctl>kvin</ctl> signifas <ctl>bonege</ctl>
	    <fnt><aut>&Z;</aut>, <lok>la&ubreve; PIV2, p. 791.</lok></fnt>.
	  </ekz>
	</dif>
	<ref tip="super" cel="poent.0o">poento</ref>
	<trd lng="be">&c_a;&c_d;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>
	<trdgrp lng="ru">
	  <trd>&c_o;&c_c;&c_je;&c_n;&c_k;&c_a;</trd>,
	  <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_k;&c_a;</trd>
	</trdgrp>
    </snc>
    <trd lng="cs">pozn&aacute;mka</trd>
    <trd lng="pt">nota</trd>
    <trd lng="fr">note</trd>
  </drv>

     <drv mrk="not.0oj">
       <kap><tld/>oj</kap>
       <snc mrk="not.0oj.MUZ">
	<uzo tip="fak">MUZ</uzo>
	<dif>
	  Teksto de muzika verko prezentita per <tld/>oj<sncref ref="not.0o.MUZ"/>:
	  <ekz>
	    &scirc;i konservis &ccirc;ion, kio lin povis rememorigi: la librojn,
	    de li iam legitajn, <klr>...</klr> <tld/>ojn kaj versojn,
	    kiujn li por &scirc;i transskribis
	    <fnt>
	      <aut>A. Pu&scirc;kin, tr. A. Grabowski</aut>:
	      <vrk>La ne&gcirc;a blovado</vrk>.
	    </fnt>;
	  </ekz><ekz>
	    <tld/>fako en biblioteko.
	  </ekz>
	</dif>
       </snc>
       <trd lng="be">&c_n;&c_o;&c_t;&c_y;</trd>
       <trd lng="de">Noten</trd>
       <trd lng="en">music</trd>
       <trd lng="fr">partition</trd>
       <trd lng="ru">&c_n;&c_o;&c_t;&c_y;</trd>
     </drv>

     <drv mrk="not.0i">
       <kap><tld/>i</kap>
       <gra><vspec>tr</vspec></gra>
       <snc>
	<dif>
	  Skribi pri io mallongan memorigan resumon; enskribi en sian
	  <tld/>kajeron:
	  <ekz>
	    <tld/>u la daton.
	  </ekz>
	</dif>
	<trd lng="de">notieren</trd>
       </snc>
       <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Priatenti kaj konservi en sia memoro:
	  <ekz>
	    <tld/>u al vi tiun konkludon en la kapo<fnt>Z</fnt>,
	  </ekz><ekz>
	    en la memoro<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>u ke mi venos.
	  </ekz>
	</dif>
	<trd lng="de">merken</trd>
    </snc>
    	<trdgrp lng="be">
    	  <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    	  <trd>&c_p;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    	  <trd>&c_z;&c_a;&c_c;&c_je;&c_m;&c_ib;&c_c;&c_mol;</trd>,
    	  <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    	  <trd>&c_z;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    	</trdgrp>
       <trd lng="en">note</trd>
       <trd lng="nl">noteren</trd>
       <trd lng="pt">anotar</trd>
       <trd lng="fr">noter</trd>
       <trdgrp lng="ru">
         <trd>&c_p;&c_o;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
         <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
         <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_z;&c_a;&c_m;&c_je;&c_t;&c_k;&c_u;</ind></trd>
       </trdgrp>
     </drv>

     <drv mrk="not.diplomatia0o">
       <kap>diplomatia <tld/>o</kap>
       <snc>
	<dif>
	  Oficiala diplomatia dokumento, formale avizanta pri problemo
	  en inter&scirc;tataj rilatoj (protesto, sciigo pri iu incidento ktp).
	</dif>
       </snc>
       <ref tip="vid" cel="memora.0o">memorando</ref>
       <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_n;&c_o;&c_t;&c_a;</ind></trd>
       <trd lng="fr"><ind>note</ind> diplomatique</trd>
       <trd lng="ru">&c_n;&c_o;&c_t;&c_a;<klr tip="amb"> (&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
     </drv>

   </art>
