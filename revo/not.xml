    </trdgrp>
    <trdgrp lng="nl">
      <trd>(school)<ind>cijfer</ind></trd>,
      <trd>beoordeling<klr tip="ind"> (m.n. van leerlingen)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>nota</trd>,
      <trd>ocena</trd>,
      <trd>stopie&nacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_c;&c_je;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
