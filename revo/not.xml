<drv mrk="not.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="not.0i.skribi">
    <dif>
      <ref tip="super" cel="skrib.0i.signojn">Skribi</ref> pri io
      mallongan memorigan resumon:
      <ekz>
        Johano <tld/>is la adreson
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        skribistoj <klr>[&#x2026;]</klr> <tld/>is la menditajn man&gcirc;a&jcirc;ojn kaj trinka&jcirc;ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        multajn el niaj fabeloj ni <tld/>is tiamaniere vorton post vorto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mezuris fenestrojn kaj pordojn, <tld/>is kaj prikalkulis
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        dika nigra libro, en kiu oni <tld/>is malaperintajn &scirc;ipojn
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        24a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>notieren</trd>,
      <trd>vermerken</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>feljegyez</trd>,
      <trd>lejegyez</trd>
    </trdgrp>
    <trd lng="id"><baz>catat</baz>mencatat</trd>
    <trdgrp lng="ja">
      <trd>&#x30E1;&#x30E2;&#x3059;&#x308B; <pr>&#x3081;&#x3082;&#x3059;&#x308B;</pr></trd>,
      <trd>&#x899A;&#x66F8;&#x3059;&#x308B; <pr>&#x304A;&#x307C;&#x3048;&#x304C;&#x304D;&#x3059;&#x308B;</pr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>notowa&cacute;</trd>,
      <trd>robi&cacute; <ind>notatki</ind></trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x8A18;&#x4E0B; <pr>j&igrave;xi&agrave;</pr></trd>,
      <trd>&#x8BB0;&#x4E0B; <pr>j&igrave;xi&agrave;</pr></trd>,
      <trd>&#x5F00;&#x4EF7; <pr>k&amacron;iji&agrave;</pr></trd>,
      <trd>&#x958B;&#x50F9; <pr>k&amacron;iji&agrave;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="not.0i.memori">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="atent.pri0i">Priatenti</ref> kaj konservi en sia memoro:
      <ekz>
        ni petas vin <tld/>i al vi en la memoro tiun konkludon
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/><klr>[i&#x2026;]</klr> en sia koro
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 7:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la estraro havas konsiderojn subtilajn: malgra&ubreve; ke estas
        malproksime, &gcirc;i tamen <tld/>as al si en la cerbo
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 1a</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>is al mi, kien li iris
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas interese <tld/>i, ke tiu romano ne estis tre &scirc;atata, kiam &gcirc;i aperis
        <fnt>
          <bib>Kandid</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is je la unua ekrigardo, ke li havis neordinaran modestecon
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rimark.0i.mense">rimarki<sncref/></ref>,
      <ref cel="memor.0i">memori</ref>,
      <ref cel="regist.0i.FIG">registri<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>merken</trd>,
      <trd><ind>Notiz</ind> nehmen</trd>
    </trdgrp>
    <trd lng="hu">megjegyez</trd>
    <trd lng="id"><baz>ingat</baz>mengingat</trd>
    <trdgrp lng="ja">
      <trd>&#x8A18;&#x61B6;&#x306B;&#x7559;&#x3081;&#x308B;
      <pr>&#x304D;&#x304A;&#x304F;&#x306B;&#x3068;&#x3069;&#x3081;&#x308B;</pr></trd>,
      <trd>&#x6C17;&#x306B;&#x7559;&#x3081;&#x308B; <pr>&#x304D;&#x306B;&#x3068;&#x3069;&#x3081;&#x308B;</pr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>notowa&cacute;</trd>,
      <trd>zanotowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x6CE8; <pr>zh&ugrave;</pr></trd>,
      <trd>&#x8A3B; <pr>zh&ugrave;</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_je;&c_m;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">notenna&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>poznamenat si</trd>,
    <trd>pozna&ccaron;it</trd>,
    <trd>vz&iacute;t na v&ecaron;dom&iacute;</trd>,
    <trd>zaznamenat</trd>
  </trdgrp>
  <trd lng="en">note</trd>
  <trd lng="es">anotar</trd>
  <trd lng="fr">noter</trd>
  <trd lng="nl">noteren</trd>
  <trd lng="pt">anotar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_z;&c_a;&c_m;&c_je;&c_t;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zazna&ccaron;i&tcaron;</trd>,
    <trd>zna&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_o;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_o;&c_t;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_n;&c_o;&c_t;&c_a;&c_t;&c_k;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="not.0o">
  <kap><ofc>*</ofc><tld/>o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5127">not'o
  (muzik'a)</url></lok></fnt></kap>
  <snc mrk="not.0o.memoriga">
    <dif>
      Mallonga enskribo memoriga a&ubreve; komenta:
      <ekz>
        fari, konservi <tld/>ojn pri fakto, parolado, adreso, dato;
      </ekz>
      <ekz>
        mallongaj <tld/>oj komercaj
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, al la
        historio de la provoj de lingvoj tutmondaj de Leibnitz &gcirc;is
        la nuna tempo</lok></fnt>.
      </ekz>
      <ekz>
        gazeta felietonisto skribas pri ili spritajn artikolojn kaj <tld/>ojn
        <klr>(vd <ref cel="notic.0o">notico</ref>)</klr>
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        esploru la aferon kaj ree faru <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        klini&gcirc;inte super amaso da <tld/>oj kaj kalkuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la diversloke en la kajeroj trovi&gcirc;antaj <tld/>oj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi konservis &ccirc;iujn miajn <tld/>ojn metode kaj la&ubreve;jare klasigitajn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj donas al la oratoro la necesan sekurecon
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Gratulaj <tld lit="N"/>oj de l' Konstanta Kritiko
        <fnt>
          <bib>LSB</bib>
          <lok>Sibilla Orakolas</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koment1.0o">komento</ref>,
      <ref cel="not.pri0o">prinoto</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_t;&c_a;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_g;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_c;&c_je;&c_m;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Vermerk</trd>,
      <trd>Notiz</trd>
    </trdgrp>
    <trd lng="en">note</trd>
    <trdgrp lng="hu">
      <trd>feljegyz&eacute;s</trd>,
      <trd>megjegyz&eacute;s</trd>,
      <trd>jegyzet</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>catat</baz>catatan</trd>,
      <trd>nota</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#x30E1;&#x30E2; <pr>&#x3081;&#x3082;</pr></trd>,
      <trd>&#x899A;&#x66F8; <pr>&#x304A;&#x307C;&#x3048;&#x304C;&#x304D;</pr></trd>,
      <trd>&#x6CE8;&#x91C8; <pr>&#x3061;&#x3085;&#x3046;&#x3057;&#x3083;&#x304F;</pr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>aantekening</trd>,
      <trd>noot</trd>,
      <trd>notitie</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>notatka</trd>,
      <trd>notka</trd>,
      <trd>adnotacja</trd>,
      <trd>dopisek</trd>,
      <trd>zapis</trd>,
      <trd>zapisek</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_m;&c_je;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x624B;&#x8A18; <pr>sh&#x1D2;uj&igrave;</pr></trd>,
      <trd>&#x624B;&#x8BB0; <pr>sh&#x1D2;uj&igrave;</pr></trd>,
      <trd>&#x4FBF;&#x6761; <pr>bi&agrave;nti&aacute;o</pr></trd>,
      <trd>&#x4FBF;&#x689D; <pr>bi&agrave;nti&aacute;o</pr></trd>,
      <trd>&#x65B9;&#x4FBF;&#x8CBC; <pr>f&amacron;ngbi&agrave;nti&emacron;</pr></trd>,
      <trd>&#x65B9;&#x4FBF;&#x8D34; <pr>f&amacron;ngbi&agrave;nti&emacron;</pr></trd>,
      <trd>&#x5099;&#x5FD8;&#x9304; <pr>b&egrave;iw&agrave;ngl&ugrave;</pr></trd>,
      <trd>&#x5907;&#x5FD8;&#x5F55; <pr>b&egrave;iw&agrave;ngl&ugrave;</pr></trd>,
      <trd>&#x4FBF;&#x7B3A; <pr>bi&agrave;nji&amacron;n</pr></trd>,
      <trd>&#x4FBF;&#x7B8B; <pr>bi&agrave;nji&amacron;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="not.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Skriba signo lokita sur muzika liniaro por montri la altecon
      kaj da&ubreve;ron de la tono.
    </dif>
    <subsnc mrk="not.0o.tono">
      <dif>
        La sono mem, prezentata per <tld/>o<sncref ref="not.0o.MUZ"/>,
        en kantado a&ubreve; en muzikila plenumo:
        <ekz>
          la fingroj trovu la ekzaktan lokon por ludi ekzaktan <tld/>on
          <fnt>
            <lok>
            <url ref="http://aliso.cnice.mecd.es/~jheras1/tarrego.htm">
            Vivo kaj Muziko de Francisco T&aacute;rrega</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          en lia rido eksonis <ind>falsa <tld/>o</ind>, en kiu tremis io simila al kompato
          <fnt>
            <bib>Marta</bib>
          </fnt>;
          <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_a;&c_ja; &c_n;&c_o;&c_t;&c_a;</trd>
          <trd lng="de">falscher Ton</trd>
          <trd lng="hu">hamis hang</trd>
          <trd lng="nl">valse <ind>noot</ind></trd>
          <trd lng="pl">fa&lstroke;szywa <ind>nuta</ind></trd>
          <trd lng="ru">
          &c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_a;&c_ja;
          &c_n;&c_o;&c_t;&c_a;</trd>
        </ekz>
        <ekz>
          la bodego <klr>[&#x2026;]</klr> ludas la <tld/>on F
          <fnt>
            <bib>Kon11</bib>
            <lok>Intervjuo</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>oj de la citro miksi&gcirc;is kun la pla&ubreve;dado de fontano
          <fnt>
            <bib>MJA</bib>
            <lok>Parto Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;iu sireno sonigis unu <tld/>on kaj la <tld/>oj kune konsistigis oktavon
          <fnt>
            <bib>Rsp</bib>
            <lok>Parto Dek</lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="not.0oj">notoj</ref>
      <trd lng="hu">hangjegy</trd>
      <trdgrp lng="nl">
        <trd>noot</trd>,
        <trd>muzieknoot</trd>
      </trdgrp>
      <trd lng="pl">nuta</trd>
    </subsnc>
    <subsnc mrk="not.0o.nuanco">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Aparta distingiga eco a&ubreve; <ref cel="nuanc.0o">nuanco</ref> (la&ubreve;
        <tld/>o<sncref ref="not.0o.tono"/>):
        <ekz>
          lia stilo havas tre personan <tld/>on;
        </ekz>
        <ekz>
          li respondis kun <tld/>o
          <klr>(iom da, tono de)</klr>
          de ironio en la vo&ccirc;o;
        </ekz>
        <ekz>
          tio portis funebran <tld/>on en la &gcirc;ojan feston.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="agord.0o.humoro">agordo<sncref/></ref>,
        <ref cel="etos.0o.KOMUNE">etoso</ref>,
        <ref cel="stil.0o.BELA.loka">stilo<sncref/></ref>,
        <ref cel="ton.sub0o">subtono</ref>,
        <ref cel="ton.0o.FON">tono<sncref/></ref>,
        <ref cel="trajt.0o.karakteriza">trajto<sncref/></ref>
      </refgrp>
      <rim>
        Mi ne trovis efektivan uzon de tiu senco ekster vortaroj kaj a&ubreve;tomataj tradukoj.
        Preferinde estas elekti unu el la referencitaj proksimsignifaj vortoj.
        <aut>Wolfram Diestel</aut>
      </rim>
      <trd lng="de">Note</trd>
      <trd lng="hu">t&oacute;nus</trd>
      <trd lng="nl">noot</trd>
      <trd lng="pl">nuta</trd>
      <trd lng="ru">&c_n;&c_o;&c_t;&c_a;</trd>
    </subsnc>
    <trd lng="be">&c_n;&c_o;&c_t;&c_a;</trd>
    <trd lng="de">Note</trd>
    <trd lng="en">note</trd>
    <trdgrp lng="id">
      <trd>not</trd>,
      <trd>titinada</trd>
    </trdgrp>
    <trd lng="ja">&#x97F3;&#x7B26; <pr>&#x304A;&#x3093;&#x3077;</pr></trd>
    <trd lng="ru">&c_n;&c_o;&c_t;&c_a;<klr tip="amb">
    (&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;)
    </klr></trd>
    <trdgrp lng="zh">
      <trd>&#x58F0;&#x8C03; <pr>sh&emacron;ngdi&agrave;o</pr></trd>,
      <trd>&#x8072;&#x8ABF; <pr>sh&emacron;ngdi&agrave;o</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="not.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Oficiale uzata signo por indiki takson de scioj a&ubreve; konduto
      de lernanto:
      <ekz>
        ricevi bonan <tld/>on pri matematiko;
      </ekz>
      <ekz>
        &ccirc;iuj, kiuj havis la <tld/>on &leftquot;tre bone&rightquot;
        a&ubreve; &leftquot;bonege&rightquot;, ricevis lokon anta&ubreve;e
        <fnt><bib>Fab2</bib>, <lok>dio de dormo,
        diman&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        tiu vino &ccirc;e ekzameno ricevis la plej bonan <tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj bu&scirc;aj <klr>(bu&scirc;ekzamenaj)</klr> <tld/>oj devos pliboni&gcirc;i
        <fnt>
          <bib>FSI</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="poent.0o">poento</ref>
    <trd lng="be">&c_a;&c_d;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Note<klr tip="ind"> (z. B. in der Schule)</klr></trd>,
      <trd>Schulnote</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>(school) <ind>grade</ind></trd>,
      <trd>mark<klr tip="ind"> (e.g. at school)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;rdemjegy</trd>,
      <trd>oszt&aacute;lyzat</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>nilai</trd>,
      <trd>poin</trd>,
      <trd>ponten</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd><klr tip="amb">(&#x5B66;&#x696D;&#x306E;)</klr>&#x6210;&#x7E3E;
