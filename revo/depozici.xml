<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: depozici.xml,v 1.16 2024/03/25 13:25:22 revo Exp $">
<kap>
  <rad>depozici</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="depozici.0i">
  <kap><tld/>i</kap>
  <snc mrk="depozici.0i.JUR">
    <dif>
      <ref tip="super" cel="atest.0i.JUR">Atesti</ref> en akto,
      responde al oficiala enketo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_c;&c_mol; <klr>(&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>dosv&#x11B;d&#x10D;it <klr>(u soudu)</klr></trd>,
    <trd>sv&#x11B;d&#x10D;it</trd>,
    <trd>vydat sv&#x11B;dectv&#xED;</trd>,
    <trd>vypov&#xED;dat jako sv&#x11B;dek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aussagen</trd>,
    <trd>bezeugen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;poser<klr> (jur.)</klr></trd>,
    <trd>faire une <ind>d&eacute;position</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E2;&#x5D9;&#x5D3;</trd>
  <trd lng="hu">tan&uacute;vallom&aacute;st tesz</trd>
  <trd lng="ja">&#x8A3C;&#x8A00;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12358;&#12370;&#12435;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>getuigen</trd>,
    <trd>getuigenis afleggen</trd>
  </trdgrp>
  <trd lng="pl">sk&lstroke;ada&cacute; <ind>zeznanie</ind></trd>
  <trdgrp lng="pt">
    <trd>depor</trd>,
    <trd>prestar <ind>depoimento</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="sk">vypoveda&tcaron; pred s&#xFA;dom ako predvolan&#xFD; svedok</trd>
  <trd lng="uk">&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_s;&c_v;&c_ib;&c_d;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="depozici.0o">
  <kap><tld/>o</kap>
  <snc mrk="depozici.0o.ado">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficiala parola a&ubreve; skriba atesto en jurafero:
      <ekz>
        la sekretario <klr>...</klr> kurbi&gcirc;ante super la malalta
        tablo
        protokolis la <tld/>on
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 2a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="cs">sv&#x11B;deck&#xE1; v&#xFD;pov&#x11B;&#x10F;</trd>
  <trdgrp lng="de">
    <trd>Zeugenaussage</trd>,
    <trd>Aussage<ind>(vor Beh&ouml;rden)</ind></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;position<klr> (jur.)</klr></trd>
  <trd lng="he">&#x5EA;&#x5B7;&#x5E6;&#x5D4;&#x5B4;&#x5D9;&#x5E8;</trd>
  <trd lng="hu">tan&uacute;vallom&aacute;s</trd>
  <trd lng="ja">&#x5BA3;&#x8A93;&#x8A3C;&#x8A00;  <pr>&#x305B;&#12435;&#x305B;&#12356;&#12375;&#12423;&#12358;&#12370;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>getuigenverklaring</trd>,
    <trd>depositie</trd>,
    <trd>getuigenis</trd>,
    <trd>verklaring</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zeznanie <klr>&sacute;wiadka</klr></trd>,
    <trd>depozycja</trd>
  </trdgrp>
  <trd lng="pt">depoimento</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_ju;&c_r;.)</klr></trd>
  <trd lng="sk">svedeck&#xE1; v&#xFD;pove&#x10F;</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43A;&c_a;&c_z;&c_a;&#x43D;&#x43D;&#x44F; &c_s;&c_v;&c_ib;&c_d;&#x43A;&c_ib;&c_v;</trd>
  <trd lng="zh">&#x8B49;&#x8A5E; <pr>zh&#xE8;ngc&#xED;</pr></trd>
</drv>
</art>

