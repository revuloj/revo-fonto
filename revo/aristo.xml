<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aristo.xml,v 1.26 2025/07/16 10:00:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>aristokrat</rad>/o
  <fnt>
    <bib>JED</bib>
  </fnt>
</kap>
<drv mrk="aristo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="nobel.0o">Nobelo</ref>:
      <ekz>
        mia patro estis tre ri&ccirc;a homo, natura <tld/>o, bonege edukita, sed beda&ubreve;rinde senkaraktera
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; sesdek talentojn oni elspezis por donacoj a&ubreve; pago de &scirc;uldoj de diversaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tre respekta kaj plej <tld/>a maniero
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ekscito de la <tld/>aro estis grandega, sed neniu sin perfidis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="aristo1.0o">aristokratio</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_k;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">arist&ograve;crata</trd>
  <trdgrp lng="cs">
    <trd>aristokrat</trd>,
    <trd>&scaron;lechtic</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Adliger</trd>,
     <trd>Adeliger</trd>,
     <trd>Edelmann</trd>,
     <trd>Aristokrat</trd>
  </trdgrp>
  <trd lng="en">aristrocrat</trd>
  <trd lng="es">arist&oacute;crata</trd>
  <trdgrp lng="fr">
    <trd>aristocrate</trd>,
    <trd>noble</trd>
  </trdgrp>
  <trd lng="he">&alef;&#1458;&resh;&#1460;&yod;&samekh;&tet;&vav;&#1465;&qof;&resh;&#1464;&tet;</trd>
  <trd lng="hu">arisztokrata</trd>
  <trdgrp lng="ja">
    <trd>&#36020;&#26063;  <pr>&#12365;&#12382;&#12367;</pr></trd>,
    <trd>&#29305;&#27177;&#38542;&#32026;&#12398;&#20154;  <pr>&#12392;&#12387;&#12369;&#12435;&#12363;&#12356;&#12365;&#12421;&#12358;&#12398;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aristocraat</trd>,
    <trd>edelman</trd>
  </trdgrp>
  <trd lng="no">aristokrat</trd>
  <trd lng="ru">&c_a;&c_r;&c_i;&c_s;&c_t;&c_o;&c_k;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>aristokrat</trd>,
    <trd>&scaron;&lcaron;achtic</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_r;&c_i;&c_s;&c_t;&c_o;&c_k;&c_r;&c_a;&c_t;</trd>
</drv>
</art>
