<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aristo.xml,v 1.22 2022/03/12 11:35:08 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>aristokrat</rad>/o
  <fnt>
    <bib>FK</bib>
  </fnt>
</kap>
<drv mrk="aristo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="nobel.0o">Nobelo</ref>:
      <ekz>
        mia patro estis tre ri&ccirc;a homo, natura <tld/>o, bonege edukita, sed beda&ubreve;rinde senkaraktera
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; sesdek talentojn oni elspezis por donacoj a&ubreve; pago de &scirc;uldoj de diversaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tre respekta kaj plej <tld/>a maniero
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ekscito de la <tld/>aro estis grandega, sed neniu sin perfidis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="aristo1.0o">aristokratio</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_k;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>aristokrat</trd>,
    <trd>&scaron;lechtic</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Adliger</trd>,
     <trd>Adeliger</trd>,
     <trd>Edelmann</trd>,
     <trd>Aristokrat</trd>
  </trdgrp>
  <trd lng="en">aristrocrat</trd>
  <trd lng="es">arist&oacute;crata</trd>
  <trdgrp lng="fr">
    <trd>aristocrate</trd>,
    <trd>noble</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5B2;&#x5E8;&#x5B4;&#x5D9;&#x5E1;&#x5D8;&#x5D5;&#x5B9;&#x5E7;&#x5E8;&#x5B8;&#x5D8;</trd>
  <trd lng="hu">arisztokrata</trd>
  <trdgrp lng="nl">
    <trd>aristocraat</trd>,
    <trd>edelman</trd>
  </trdgrp>
  <trd lng="no">aristokrat</trd>
  <trd lng="ru">&c_a;&c_r;&c_i;&c_s;&c_t;&c_o;&c_k;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>aristokrat</trd>,
    <trd>&scaron;&lcaron;achtic</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x440;&#x438;&#x441;&#x442;&#x43E;&#x43A;&#x440;&#x430;&#x442;</trd>
</drv>
</art>
