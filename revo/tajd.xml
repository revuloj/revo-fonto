<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tajd</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tajd.0o">
  <kap><tld/>o</kap>
  <snc mrk="tajd.0o.MAR">
  <uzo tip="fak">MAR</uzo>
    <dif>
      Alterna kaj perioda movo de la mara akvo:
      <ekz>
        la <tld/>o flusas kaj malflusas;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili ne povos reteni la <tld/>on de homa progreso;
      </ekz>
      <ekz>
      <ctl>La tajdo</ctl>
      <klr>(poemaro de Nikolao Hohlov)</klr>
      <fnt><aut>Hohlov Nikolai</aut>, <vrk>La tajdo. - 1928</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_l;&c_ib;&c_w; &c_ib;
  &c_a;&c_d;&c_l;&c_ib;&c_w;</trd>
  <trdgrp lng="de">
    <trd>Gezeit</trd>,
    <trd>Gezeiten</trd>,
    <trd>Tide</trd>,
    <trd>Tiden</trd>,
    <trd>Ebbe und Flut</trd>
  </trdgrp>
  <trd lng="fr">mar&eacute;e</trd>
  <trd lng="hu">&aacute;rap&aacute;ly</trd>
  <trd lng="nl">getij</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_i;&c_v; &c_i; &c_o;&c_t;&c_l;&c_i;&c_v;</trd>
  <trd lng="sv">tidvatten</trd>
</drv>
</art>
