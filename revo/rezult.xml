<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>2</ofc>
      <rad>rezult</rad>/i
    </kap>
    <drv mrk="rezult.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Logike sekvi el io:
          <ekz>
            el tiu principo <tld/>as, ke.
          </ekz>
        </dif>
        <trd lng="ru">&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_a;&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;)</klr></trd>
      </snc>
      <snc>
        <dif>
          Rezultati:
          <ekz>
            alia aran&gcirc;o eble <tld/>os el via diskutado<fnt>Z</fnt>.
          </ekz> 
        </dif>
        <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; <klr>(&c_k;&c_a;&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;)</klr></trd>
      </snc>
      <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">resultieren</trd>
      <trd lng="en">result</trd>
      <trdgrp lng="es">
        <trd>resultar</trd>,
        <trd>dar como <ind>resultado</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>ad&oacute;dik</trd>,
        <trd>kij&ouml;n <klr tip="ind">(eredm&eacute;nyk&eacute;nt)</klr></trd>,
        <trd>k&ouml;vetkezik</trd>,
        <trd>sz&aacute;rmazik</trd>
      </trdgrp>      
      <trdgrp lng="nl">
        <trd>opleveren</trd>,
        <trd>volgen</trd>
      </trdgrp>
        <trd lng="pt">resultar</trd>
      <trdgrp lng="ru">
        <trd>&c_ja;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_o;&c_m;</ind></trd>,
        <trd>&c_p;&c_r;&c_o;&c_i;&c_s;&c_t;&c_je;&c_ch;&c_mol;</trd>,
        <trd>&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>
    <drv mrk="rezult.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Logika sekvo; konsekvenco. 
        </dif>
      </snc>
      <snc mrk="rezult.0o.produkto">
        <dif>
          Rezultato:
          <ekz>de elekto tro multa plej malbona <tld/>o
<fnt><bib>PrV</bib></fnt>.</ekz><ekz>
            <tld/>o de aritmetika operacio, de kalkulo, de komputo.
          </ekz> 
        </dif>
      </snc>
      <trd lng="ca">resultat</trd>
      <trd lng="de">Resultat</trd>
      <trdgrp lng="nl">
        <trd>resultaat</trd>,
        <trd>gevolg</trd>
      </trdgrp>
      <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;</trd>
      <trd lng="es">resultado</trd>
      <trdgrp lng="hu">
        <trd>eredm&eacute;ny</trd>,
        <trd>k&ouml;vetkezm&eacute;ny</trd>
      </trdgrp>      
      <trd lng="ru">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;</trd>
    </drv>
    <drv mrk="rezult.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Sekvigi kiel <tld/>on.  
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>eredm&eacute;nyez</trd>,
        <trd>okoz</trd>,
        <trd>hoz <klr tip="ind">(eredm&eacute;ny&uuml;l)</klr></trd>
      </trdgrp>      
      <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i;
        <klr>(&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_u;, &c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_ju;)</klr></trd>
    </drv>
  </art>
