<drv mrk="rezult.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Kiel <tld/>o:
      <ekz>
        oni faras finon al la mallumo kaj <tld/>e oni trovas la &scirc;tonojn
        el grandega
        mallumo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 28:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e de militoj kaj konfliktoj, 13 milionoj da infanoj estas
        senhejmaj
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2000/007072.php">Plej senhelpaj viktimoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 13</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u; <ind>&c_v;&c_y;&c_n;&c_ib;&c_k;&c_u;</ind></trd>
  <trd lng="de">im <ind>Ergebnis</ind></trd>
  <trdgrp lng="pl">
    <trd>wskutek</trd>,
    <trd>w wyniku</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6240;&#x81F4; <pr>su&#x1D2;zh&#xEC;</pr></trd>,
    <trd>&#x7D50;&#x679C; <pr>ji&#xE9;gu&#x1D2;</pr></trd>,
    <trd>&#x7ED3;&#x679C; <pr>ji&#xE9;gu&#x1D2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rezult.0eke">
  <kap><tld/>e ke</kap>
  <gra><vspec>subordigilo</vspec></gra>
  <snc mrk="rezult.0eke.KOMUNE">
    <dif>
      Ka&ubreve;zante tion ke..., kun la <tld/>o ke...,
      <ref tip="vid" cel="tiel.0ke.maniere">tiel ke<sncref/></ref>:
      <ekz>
        la peza jako kaj plejdo ne nur &scirc;vitigas, sed
        anka&ubreve; malpermesas forvapori&gcirc;on, <tld/>e ke lia
        torso suferas efektivan vaporbanon
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">mit dem <ind>Ergebnis</ind>, dass</trd>
  </snc>
  <trd lng="be">&c_u; <ind>&c_v;&c_y;&c_n;&c_ib;&c_k;&c_u;</ind> &c_ch;&c_a;&c_g;&c_o;</trd>
  <trd lng="fr">de telle sorte que</trd>
  <trd lng="pl">w wyniku czego&sacute;</trd>
</drv>

<drv mrk="rezult.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rezult.0igi.KOMUNE">
    <dif>
      Sekvigi kiel <tld/>on:
      <ekz>
        inter la malgranda gastaro trovi&gcirc;is unu plej &ccirc;arma knabino, sed
        terure fiera &#x2010;
        tion <tld/>igis la servistoj per siaj kisoj kaj karesoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Infana babilado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu okulo ricevas unu el la du bildoj, iomete diferencaj
        <klr>[&#x2026;]</klr> la cerbo
        ilin kunfandas kaj <tld/>igas unusolan tridimensian bildon
        <fnt>
          <aut>Gianfranco Cazzaro</aut>
          <vrk><url ref="&Monato;/1995/A950101.php">La modo de fiksokuloj</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ergeben</trd>,
      <trd>hervorrufen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to effect</trd>,
    <trd>bring about</trd>
    <trd>yield</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>produire<klr> (avoir pour cons&eacute;quence)</klr></trd>,
    <trd>susciter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eredm&eacute;nyez</trd>,
    <trd>okoz</trd>,
    <trd>hoz <klr tip="ind">(eredm&eacute;ny&uuml;l)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#xFF5E;<ind>&#x306E;&#x7D50;&#x679C;&#x3092;&#x3082;&#x305F;&#x3089;&#x3059; 
    </ind><pr>&#x306E;&#x3051;&#x3063;&#x304B;&#x3092;&#x3082;&#x305F;&#x3089;&#x3059;</pr></trd>,
    <trd>&#x5F15;&#x304D;&#x8D77;&#x3053;&#x3059;  <pr>&#x3072;&#x304D;&#x304A;&#x3053;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>doprowadzi&cacute; do skutku</trd>,
    <trd>powodowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i;<klr> (&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_u;, &c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_ju;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_d;&#x43E; &c_r;&c_je;&c_z;&c_u;&#x43B;&#x44C;&c_t;&c_a;&c_t;&c_u;</trd>,
    <trd>&#x43D;&c_a;&c_s;&#x43B;&c_ib;&c_d;&#x43A;&c_u;</trd>,
