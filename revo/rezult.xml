<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rezult.xml,v 1.22 2021/05/28 17:06:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>rezult</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rezult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rezult.0i.KOMUNE">
    <dif>
      Logike a&ubreve; nature sekvi el ia ka&ubreve;zo;
      <ref tip="sin" cel="rezult1.0i">rezultati</ref>:
      <ekz>
        el tiu principo <tld/>as, ke...;
      </ekz>
      <ekz>
        alia simila aran&gcirc;o <klr>[&#x2026;]</klr> eble <tld/>os
        el via diskutado
        <fnt>
          <bib>Paroloj</bib>,
          <vrk>Parolado de Zamenhof en la sepa
          universala kongreso, 1911</vrk>
        </fnt>;
      </ekz> 
      <ekz>
        ne <tld/>is ar&gcirc;ento
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 48:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> atendis, kio <tld/>os el la propono de la
        mastrino
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        siajn pensojn oni devas kura&gcirc;e esprimi, &ccirc;ar alie nenio <tld/>as
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>

    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">resultar</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t v&yacute;sledkem</trd>,
    <trd>projevit se</trd>,
    <trd>rezultovat</trd>,
    <trd>vypl&yacute;vat</trd>,
    <trd>vyzn&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ergeben</trd>,
    <trd>resultieren</trd>
  </trdgrp>
  <trd lng="en">result</trd>
  <trdgrp lng="es">
      <trd>resultar</trd>,
      <trd>dar como <ind>resultado</ind></trd>
  </trdgrp>
  <trd lng="fr">r&eacute;sulter</trd>
  <trdgrp lng="hu">
      <trd>ad&oacute;dik</trd>,
      <trd>kij&ouml;n <klr tip="ind">(eredm&eacute;nyk&eacute;nt)</klr></trd>,
      <trd>k&ouml;vetkezik</trd>,
      <trd>sz&aacute;rmazik</trd>
  </trdgrp>
  <trdgrp lng="nl">
      <trd>opleveren</trd>,
      <trd>volgen</trd>
  </trdgrp>
  <trd lng="pt">resultar</trd>
  <trdgrp lng="ru">
      <trd>&c_ja;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_o;&c_m;</ind></trd>,
      <trd>&c_p;&c_r;&c_o;&c_i;&c_s;&c_t;&c_je;&c_ch;&c_mol;</trd>,
      <trd>&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_j;&c_t;&c_i; <klr>(&c_k;&c_a;&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;)</klr></trd>,
      <trd>&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_a;&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;)</klr></trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>rezultova&tcaron;</trd>,
  <trd>vypl&yacute;va&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="rezult.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="rezult.0o.KOMUNE">
    <dif>
      Logika a&ubreve; natura sekvo, konsekvenco;
      <ref tip="sin" cel="rezult1.0o">rezultato</ref>:
      <ekz>
        de elekto tro multa plej malbona <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <nom>Joab</nom> transdonis al la re&gcirc;o la <tld/>on de la
        kalkulado de la popolo, kaj
        montri&gcirc;is, ke da Izraelidoj estas okcent mil viroj militkapablaj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 24:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar la malvirtulo superfortas la virtulon, tial la <tld/>oj de
        ju&gcirc;o estas
        mal&gcirc;ustaj
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;abakuk 1:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la provoj <klr>[&#x2026;]</klr> donis <tld/>a&jcirc;ojn tiel bonajn, ke la
        franca ministro de
        maro difinis specialan komision, por esplori la efikadon
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de la enketo certe &scirc;okis kaj instruistojn kaj
        gepatrojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9501-seksaj-rilatoj.txt">Seksaj
          Rilatoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">resultat</trd>
  <trd lng="cs">v&yacute;sledek</trd>
  <trdgrp lng="de">
    <trd>Ergebnis</trd>,
    <trd>Resultat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>result</trd>,
    <trd>effect</trd>
  </trdgrp>
  <trd lng="es">resultado</trd>
  <trd lng="fr">r&eacute;sultat</trd>
  <trdgrp lng="hu">
      <trd>eredm&eacute;ny</trd>,
      <trd>k&ouml;vetkezm&eacute;ny</trd>
    </trdgrp>
  <trd lng="ne">&#x92A;&#x930;&#x93F;&#x923;&#x93E;&#x92E;</trd>
  <trdgrp lng="nl">
      <trd>resultaat</trd>,
      <trd>gevolg</trd>
    </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
  <trd>sk&oacute;re</trd>,
  <trd>v&yacute;sledok</trd>
  </trdgrp>
</drv>

<drv mrk="rezult.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Kiel <tld/>o:
      <ekz>
        oni faras finon al la mallumo kaj <tld/>e oni trovas la &scirc;tonojn
        el grandega
        mallumo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 28:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e de militoj kaj konfliktoj, 13 milionoj da infanoj estas
        senhejmaj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007072.html">Plej
          senhelpaj viktimoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">im <ind>Ergebnis</ind></trd>
</drv>

<drv mrk="rezult.0eke">
  <kap><tld/>e ke</kap>
  <gra><vspec>subordigilo</vspec></gra>
  <snc mrk="rezult.0eke.KOMUNE">
    <dif>
      Ka&ubreve;zante tion ke..., kun la <tld/>o ke...,
      <ref tip="vid" cel="tiel.0ke.maniere">tiel ke<sncref/></ref>:
      <ekz>
        la peza jako kaj plejdo ne nur &scirc;vitigas, sed
        anka&ubreve; malpermesas forvapori&gcirc;on, <tld/>e ke lia
        torso suferas efektivan vaporbanon
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">mit dem <ind>Ergebnis</ind>, dass</trd>
  </snc>
  <trd lng="fr">de telle sorte que</trd>
</drv>

<drv mrk="rezult.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rezult.0igi.KOMUNE">
    <dif>
      Sekvigi kiel <tld/>on:
      <ekz>
        inter la malgranda gastaro trovi&gcirc;is unu plej &ccirc;arma knabino, sed
        terure fiera &#x2010;
        tion <tld/>igis la servistoj per siaj kisoj kaj karesoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Infana babilado</lok>
        </fnt>;
      </ekz>  
      <ekz>
        &ccirc;iu okulo ricevas unu el la du bildoj, iomete diferencaj
        <klr>[&#x2026;]</klr> la cerbo
        ilin kunfandas kaj <tld/>igas unusolan tridimensian bildon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9501-sterogramoj.txt">Stereogramoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ergeben</trd>,
      <trd>hervorrufen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to effect</trd>,
    <trd>bring about</trd>
    <trd>yield</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>produire<klr> (avoir pour cons&eacute;quence)</klr></trd>,
    <trd>susciter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eredm&eacute;nyez</trd>,
    <trd>okoz</trd>,
    <trd>hoz <klr tip="ind">(eredm&eacute;ny&uuml;l)</klr></trd>
  </trdgrp>      
  <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i;<klr> (&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_u;, &c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_ju;)</klr></trd>
</drv>
</art>
