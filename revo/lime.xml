<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lime.xml,v 1.11 2024/01/18 15:26:36 revo Exp $">
<kap>
  <rad>lime</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="lime.0o">
  <kap><tld/>o</kap>
  <snc mrk="lime.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <tld/>arbo, kun verda &scirc;elo, acida:
      <ekz>
        la genro <klr>...</klr> citruso inkluzivas komerce gravajn
        fruktojn kiel oran&gcirc;on, citronon, grapfrukton, <tld/>on
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Citruso">
          citruso</url>, 2013-03-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_j;&c_m; <klr>(&c_c;&c_y;&c_t;&c_r;&c_u;&c_s;&c_a;&c_v;&c_y; &c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="cs">zelen&yacute; citr&oacute;n</trd>
  <trd lng="de">Limette</trd>
  <trd lng="en">lime</trd>
  <trd lng="es">lima</trd>
  <trd lng="fr">citron vert</trd>
  <trd lng="id">jeruk nipis</trd>
  <trdgrp lng="pl">
    <trd>limonka</trd>,
    <trd>limetka</trd>
  </trdgrp>
  <trd lng="sk">zelen&yacute; citr&oacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_i;&c_t;&c_r;&c_u;&c_s; &c_s;&#x43F;&c_r;&c_a;&c_v;&c_zh;&#x43D;&c_ib;&c_j; &#x43A;&c_i;&c_s;&#x43B;&c_i;&c_j;</trd>,
    <trd>&#x43B;&c_a;&c_j;&#x43C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38738;&#x67E0;&#x6AAC; <pr>q&#x12B;ngn&#xED;ngm&#xE9;ng</pr></trd>,
    <trd>&#38738;&#x6AB8;&#x6AAC; <pr>q&#x12B;ngn&#xED;ngm&#xE9;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lime.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="lime.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta <ref tip="super" cel="citrus.0o">citruso</ref>
      kultivata en tropikaj landoj en du sufi&ccirc;e similaj specioj
      (<trd lng="la">Citrus aurantiifolia</trd> kaj
      <trd lng="la">Citrus latifolia</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_j;&c_m; <klr>(&c_c;&c_y;&c_t;&c_r;&c_u;&c_s;&c_a;&c_v;&c_a;&c_ja; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="fr">limettier</trd>
  <trdgrp lng="pl">
    <trd>lima</trd>,
    <trd>limona</trd>,
    <trd>limonka</trd>,
    <trd>limetka</trd>,
    <trd>limetka kwa&sacute;na</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F86;&#x6AAC; <pr>l&#xE1;im&#xE9;ng</pr></trd>,
    <trd>&#26469;&#x6AAC; <pr>l&#xE1;im&#xE9;ng</pr></trd>
  </trdgrp>
</drv>
</art>

