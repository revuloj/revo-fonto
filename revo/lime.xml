<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lime.xml,v 1.13 2025/06/15 06:00:20 revo Exp $">
<kap>
  <rad>lime</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="lime.0o">
  <kap><tld/>o</kap>
  <snc mrk="lime.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <tld/>arbo, kun verda &gcirc;is flava &scirc;elo, kaj ronda acida frukto:
      <ekz>
        la genro <klr>[&#8230;]</klr> citruso inkluzivas komerce gravajn
        fruktojn kiel oran&gcirc;on, citronon, grapfrukton, <tld/>on
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Citruso">
          citruso</url>, 2013-03-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bergam.0o">bergamoto</ref>,
      <ref cel="citron.0o">citrono</ref>,
      <ref cel="limed.0o">limedo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_j;&c_m; <klr>(&c_c;&c_y;&c_t;&c_r;&c_u;&c_s;&c_a;&c_v;&c_y;
  &c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trdgrp lng="ca">
    <trd>llima</trd>,
    <trd>llima dol&ccedil;a</trd>
  </trdgrp>
  <trd lng="cs">zelen&yacute; citr&oacute;n</trd>
  <trd lng="de">Saure <ind>Limette</ind> <klr>(Frucht)</klr></trd>
  <trd lng="en">lime</trd>
  <trd lng="es">lima</trd>
  <trd lng="fr">citron vert</trd>
  <trd lng="id">jeruk nipis</trd>
  <trdgrp lng="pl">
    <trd>limonka</trd>,
    <trd>limetka</trd>
  </trdgrp>
  <trd lng="sk">zelen&yacute; citr&oacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_i;&c_t;&c_r;&c_u;&c_s; &c_s;&c_p;&c_r;&c_a;&c_v;&c_zh;&c_n;&c_ib;&c_j;
    &c_k;&c_i;&c_s;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_j;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38738;&#26592;&#27308; <pr>q&imacron;ngn&iacute;ngm&eacute;ng</pr></trd>,
    <trd>&#38738;&#27320;&#27308; <pr>q&imacron;ngn&iacute;ngm&eacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lime.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="lime.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta <ref tip="super" cel="citrus.0o">citruso</ref>
      kultivata en tropikaj landoj en du sufi&ccirc;e similaj specioj
      (<trd lng="la">Citrus aurantiifolia</trd> kaj
      <trd lng="la">Citrus latifolia</trd>).
    </dif>
    <bld
      lok="&WCU;/thumb/5/55/Citrus_aurantiifolia_in_Kadavoor.jpg/640px-Citrus_aurantiifolia_in_Kadavoor.jpg"
      prm="CC BY-SA 4.0"
      >bran&ccirc;o de <tld/>o (Citrus aurantifolia) kun fruktoj
        <fnt>
          <aut>Jee &amp; Rani Nature Photography, propra foto</aut>
          <lok><url ref="&WCW;/File:Citrus_aurantiifolia_in_Kadavoor.jpg"
              >https://commons.wikimedia.org/wiki/File:Citrus_aurantiifolia_in_Kadavoor.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_j;&c_m; <klr>(&c_c;&c_y;&c_t;&c_r;&c_u;&c_s;&c_a;&c_v;&c_a;&c_ja;
  &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>llimera</trd>,
    <trd>llimer</trd>,
    <trd>llimera dol&ccedil;a</trd>,
    <trd>llimer dol&ccedil;</trd>
  </trdgrp>
  <trd lng="de">Echte <ind>Limette</ind> <klr>(Pflanze)</klr></trd>
  <trd lng="fr">limettier</trd>
  <trdgrp lng="pl">
    <trd>lima</trd>,
    <trd>limona</trd>,
    <trd>limonka</trd>,
    <trd>limetka</trd>,
    <trd>limetka kwa&sacute;na</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20358;&#27308; <pr>l&aacute;im&eacute;ng</pr></trd>,
    <trd>&#26469;&#27308; <pr>l&aacute;im&eacute;ng</pr></trd>
  </trdgrp>
</drv>
</art>

