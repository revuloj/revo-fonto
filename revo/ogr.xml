<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ogr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ogr.0o">
  <kap><tld/>o</kap>
  <snc mrk="ogr.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Granda, kruda, man&gcirc;avida <ref tip="super" 
      cel="fe.0o.MIT">feo</ref>, iafoje homman&gcirc;ema:
      <ekz>
        &leftquot;pa&ccirc;jo, pa&ccirc;jo, <tld/>o &jcirc;us 
        ka&scirc;is sin en la vesto&scirc;ranko&rightquot;
        <fnt>
          <aut>Louis Beaucaire</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Kruko/index.html">
          Kruko kaj Baniko el Bervalo</url></vrk>,
          <lok>TK, 1970</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kompatinda Bilbo sidis en la mallumo kaj pensis pri 
        &ccirc;iuj nomoj de gigantoj kaj <tld/>oj, kiujn oni rakontis 
        en fabeloj
        <fnt>
          <aut>J. R. R. Tolkien, trad. C. Gledhill</aut>,
          <vrk>La hobito</vrk>,
          <lok><url ref="http://esperanto.org/Ondo/T-0-05.htm">
          &ccirc;apitro 5a</url>. - Sezonoj, 2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ogre</trd>
  <trd lng="pl">ogr <klr> (zazwyczaj: olbrzym po&zdot;eraj&aogonek;cy 
  dzieci)</klr></trd>
  <trdgrp lng="pt">
    <trd>ogro</trd>,
    <trd>pap&atilde;o <klr tip="amb">(mit.)</klr></trd>,
    <trd>trasgo</trd>    
  </trdgrp>
</drv>

<drv mrk="ogr.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="ogr.0ino.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Granda, kruda, man&gcirc;avida <ref tip="super" 
      cel="fe.0o.MIT">feino</ref>, iafoje homman&gcirc;ema.
    </dif>
  </snc>
  <trdgrp lng="pl">
    <trd>baba-jaga</trd>,
    <trd>baba-j&eogonek;dza</trd>,
    <trd>wied&zacute;ma</trd>    
  </trdgrp>
</drv>
</art>

