      por mikroskopa esploro:
      <ekz>
        en sep monatoj la kanceroj tute malaperis,
        kaj la <tld/>oj renormali&gcirc;is
        <fnt>
          <aut>W. Lee, trad. L. De Cooman</aut>,
          <vrk><url ref="&STEB;medicino/angiogenezo.htm">
          Angiogenezo</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6D3B;&#x68C0; <pr>hu&#xF3;ji&#x1CE;n</pr></trd>,
    <trd>&#x6D3B;&#x4F53;&#x68C0;&#x89C6; <pr>hu&#xF3;t&#x1D0;ji&#x1CE;nsh&#xEC;</pr></trd>,
    <trd>&#x6D3B;&#x7D44;&#x7E54;&#x6AA2;&#x67E5; <pr>hu&#xF3;z&#x1D4;zh&#x12B;ji&#x1CE;nch&#xE1;</pr></trd>
  </trdgrp>
