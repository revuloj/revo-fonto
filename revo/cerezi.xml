<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cerezi.xml,v 1.19 2023/08/31 10:10:26 revo Exp $">
  <kap>
    <rad>cerezin</rad>/o
    <fnt>Z</fnt>
  </kap>
  <drv mrk="cerezi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de minerala <ref cel="vaks.0o">vakso</ref>:
      <ekz>
        oni produktas <tld/>on varmigante tervakson (ozokeriton) aldonante 6 &gcirc;is 10&#x202F;% da
        sulfuracido;
      </ekz>
      <ekz>
        <tld/>o estas uzata anstata&ubreve; abelvakso por kandeloj.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_e;&c_r;&c_e;&c_z;&c_ib;&c_n;</trd>
    <trd lng="ca">ceresina</trd>
    <trd lng="de">Zeresin</trd>
    <trd lng="es">ceresina</trd>
    <trd lng="fr">c&eacute;r&eacute;sine<klr tip="ind"> (vx.,
    d&eacute;riv&eacute; de l'ozoc&eacute;rite)</klr></trd>
    <trd lng="hu">cerezin</trd>
    <trdgrp lng="it">
    <trd>ozokerite</trd>,
    <trd>ozocerite</trd>,
    <trd>ceresina</trd>
    </trdgrp>
    <trd lng="pl">cerezyna</trd>
    <trd lng="pt">ceresina</trd>
    <trd lng="ru">&c_c;&c_je;&c_r;&c_je;&c_z;&c_i;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>cerezin</trd>,
    <trd>ozokerit</trd>
  </trdgrp>
  <trd lng="he">&#x5E9;&#x5E2;&#x5D5;&#x5D5;&#x5D4; &#x5DE;&#x5D7;&#x5E6;&#x5D1;&#x5D9;&#x5EA;</trd>
  <trd lng="ja">&#x30BB;&#x30EC;&#x30B7;&#x30F3;</trd>
  <trd lng="sk">cerez&iacute;n</trd>
  <trd lng="uk">&#x446;&#x435;&#x440;&#x435;&#x437;&#x438;&#x43D;</trd>
</drv>
</art>

