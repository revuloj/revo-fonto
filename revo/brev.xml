    <kap>
      <rad>brev</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="brev.0o">
  <kap><tld/>o</kap>
  <snc mrk="brev.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Papa <ref tip="super" cel="leter.0o">letero</ref> pli mallonga kaj malpli solena ol <ref cel="bule.0o">buleo</ref>:
      <ekz>
        apostola <tld/>o, en 1854, rajtigis la episkopon <klr>[&#8230;]</klr> nomi sin
        Episkopo de <nom>Bayeux</nom> kaj <nom>Lisieux</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=715312"
              >Episkopujo de Bayeux-Lisieux</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Breve</trd>
  </snc>
</drv>

  </art>

