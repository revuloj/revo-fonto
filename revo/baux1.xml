<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: baux1.xml,v 1.7 2023/02/04 21:22:57 revo Exp $">
<kap>
<rad>ba&ubreve;</rad>
</kap>
<drv mrk="baux1.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      Sono de hunda <ref tip="lst" cel="blek.0o"
      lst="voko:blekoj">bleko</ref>:
      <ekz>
        Subite a&ubreve;di&gcirc;is &leftquot;<tld/>-<tld/>!&rightquot; &parsl; Anstata&ubreve; l&#8217; anta&ubreve;a &leftquot;mia&ubreve;!&rightquot; &parsl; Rikanis la muso:
        &leftquot;la hund&#8217; aminda! &parsl; Nun fu&gcirc;os la kato kompatinda!&rightquot;
        <fnt>
          <aut>Heinz Erhardt, trad. Markus(?)</aut>
          <vrk><url ref="https://www.e-mental.cz/archiv/5192"
              >La poliglota kato</url></vrk>
          <lok>E-mental'</lok>
        </fnt>
      </ekz>
    </dif>
    <ref tip="vid" cel="boj.0i">boji</ref>
    </snc>
    <trd lng="de">wau</trd>
    <trd lng="en">bow-wow</trd>
    <trd lng="fr">ouah</trd>
    <trd lng="pl">hau</trd>
    <trd lng="ru">&c_g;&c_a;&c_v;</trd>
    <trd lng="sv">vov</trd>
</drv>
</art>

