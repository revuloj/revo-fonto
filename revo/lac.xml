<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lac.xml,v 1.37 2022/09/29 07:50:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lac</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="lac.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="lac.0a.malvigla">
    <dif>
      Sentanta sin
      <ref tip="vid" cel="vigl.mal0a">malvigla</ref>,
      bezonanta ripozon pro eluzo de siaj
      fortoj per troa penado:
      <ekz>
        <nom>Esav</nom> venis de la kampo kaj estis <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estis <tld/>a kaj multelaborinta
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 25:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> estis tro <tld/>aj, por transiri la torenton
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 30:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estis <tld/>aj de la nokta dancado
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la stomako estis malplena, la korpo estis <tld/>a
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sentis sin tiel <tld/>a, lia kapo klini&gcirc;is, liaj okuloj fermi&gcirc;is por ripozo
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a pro longa voja&gcirc;o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dorm.0ema">dormema</ref>,
      <ref cel="konsum.0i">konsumita</ref>,
      <ref cel="fresx.mal0a">malfre&scirc;a</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>fatigat</trd>,
      <trd>rendit</trd>,
      <trd>esgotat</trd>
    </trdgrp>
    <trd lng="de">matt</trd>
    <trdgrp lng="en">
      <trd>tired</trd>,
      <trd>weary</trd>,
      <trd>fatigued</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zm&eogonek;czony</trd>,
      <trd>zmordowany</trd>
    </trdgrp>
  </snc>
  <snc mrk="lac.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="ted.0i">Tedita</ref>
      de ia okupo kaj deziranta &gcirc;in rezigni a&ubreve; &ccirc;esigi:
      <ekz>
        mi estas <tld/>a pro viaj sen&ccirc;esaj petoj;
      </ekz>
      <ekz>
        <tld/>a de la vanaj klarigoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a de la plezuroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a de la piaj okupoj en la templo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj de la sendanka laborado, ni kun malespero kaj apatio lasus fali la
        manojn
        <fnt>
          <bib>EE</bib>
          <lok>IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi estos <tld/>aj de ludado, atendos vin <klr>[&#8230;]</klr> scenejo de intima
        teatro
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>fart</trd>,
      <trd>tip</trd>
    </trdgrp>
    <trd lng="de">&uuml;berdr&uuml;ssig</trd>
    <trdgrp lng="en">
      <trd>tired of</trd>,
      <trd>fed up with</trd>,
      <trd>jaded</trd>,
    </trdgrp>
    <trdgrp lng="pl">
      <trd>znu&zdot;ony</trd>,
      <trd>wyczerpany</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_m;&c_l;&c_je;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_u;&c_m;&c_o;&c_r;&c_je;&c_n;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_o;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_nga;&t_tsheg;&t_-a;&t_cha;&t_da;&t_tsheg;</trd>
  <trd lng="br">skuizh</trd>
  <trd lng="ca">cansat</trd>
  <trd lng="cs">unaven&yacute;</trd>
  <trd lng="de">m&uuml;de</trd>
  <trd lng="el">&kappa;&omikron;&ypsilon;&rho;&alfa;&sigma;&my;&epsilon_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="es">cansado</trd>
  <trd lng="fr">fatigu&eacute;</trd>
  <trd lng="he">&ayin;&yod;&yod;&fpe;</trd>
  <trd lng="hu">f&aacute;radt</trd>
  <trd lng="it">stanco</trd>
  <trd lng="nl">moe</trd>
  <trd lng="pt">cansado</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_a;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_t;&c_o;&c_m;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>unaven&yacute;</trd>,
    <trd>ustat&yacute;</trd>
  </trdgrp>
  <trd lng="sv">tr&ouml;tt</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_t;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_m;&c_u;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="lac.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="lac.0ega.KOMUNE">
    <dif>
      Tre <tld/>a:
      <ekz>
        ekkaptitoj plorantaj, <klr>[&#8230;]</klr> virinoj <tld/>egaj kaj palaj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        fra&ubreve;lino de Prelongo &scirc;ajnas pli <tld/>ega ol dan&gcirc;ere vundita
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ega pro transfuzio
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxerp.el0ita.FIZL">el&ccirc;erpita<sncref/></ref>
    <ref tip="vid" cel="asteni.0a">astenia</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_ib; <ind>&c_s;&c_t;&c_o;&c_m;&c_l;&c_je;&c_n;&c_y;</ind></trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_s;&c_ib;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">ersch&ouml;pft</trd>
  <trdgrp lng="es">
    <trd>exhausto</trd>,
    <trd>baldado</trd>
  </trdgrp>
</drv>

<drv mrk="lac.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="lac.0iga.KOMUNE">
    <dif>
      Kiu konsumas la fortojn, malvigligas:
      <ekz>
        bone aran&gcirc;ita lito atendas lin post la <tld/>iga taga voja&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        komenci&gcirc;is <klr>[&#8230;]</klr> vivo tre <tld/>iga, kian li tute ne atendis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        laboro enuiga, <tld/>iga, ne permesanta libere ekspiri
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en siaj bienoj li facile trovos por vi oficon, malpli <tld/>igan ol la via kaj
        pli profitan
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_m;&c_l;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_t;&c_o;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&uacute;morn&yacute;</trd>,
    <trd>&uacute;navn&yacute;</trd>
  </trdgrp>
  <trd lng="de">erm&uuml;dend</trd>
  <trdgrp lng="es">
    <trd>fatigador</trd>,
    <trd>fatigante</trd>
  </trdgrp>
  <trd lng="fr">fatiguant</trd>
  <trd lng="sk">&uacute;navn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_t;&c_o;&c_m;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_m;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_m;&c_l;&c_ju;&c_ju;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="lac.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lac.0igi.KOMUNE">
    <dif>
      Konsumi la fortojn, la viglon:
      <ekz>
        la tro multa parolado <tld/>igas lin
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas, ke la unuaj tagoj de l' regado <tld/>igis vin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ku&scirc;i&gcirc;os, la vizito <tld/>igis min
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia penegado sin mem konsumadis en la febro, <tld/>igis la korpon kaj la spiriton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li bezonis mar&scirc;adi kaj <tld/>igi siajn muskolojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        laboro <tld/>igas, sed akiro &gcirc;ojigas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxerp.el0i.cxion">el&ccirc;erpi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">skuizha&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="ca">
    <trd>cansar</trd>,
    <trd>fatigar</trd>,
    <trd>esgotar</trd>,
    <trd>extenuar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nam&aacute;hat</trd>,
    <trd>unavit</trd>,
    <trd>unavovat</trd>
  </trdgrp>
  <trd lng="de"><ind>m&uuml;de</ind> machen</trd>
  <trd lng="es">fatigar</trd>
  <trd lng="fr">fatiguer</trd>
  <trd lng="nl"><ind>moe</ind> maken</trd>
  <trd lng="oc">alassar</trd>
  <trdgrp lng="pl">
    <trd>m&eogonek;czy&cacute;</trd>,
    <trd>nu&zdot;y&cacute;</trd>,
    <trd>wyczerpywa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">unavi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_m;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_t;&c_o;&c_m;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_m;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_m;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lac.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lac.0igxi.KOMUNE">
    <dif>
      Eksenti mankon de forto, bezonon de ripozo:
      <ekz>
        &ccirc;ar &scirc;i tedadis lin per siaj vortoj &ccirc;iutage kaj turmentadis lin, lia animo morte
        <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne volas forsendi ilin fastantaj, por ke ili ne <tld/>i&gcirc;u sur la vojo
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 15:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        fratoj, ne <tld/>i&gcirc;u en bonfarado
        <fnt>
          <bib>NT</bib>
          <lok>II. Tesalonikanoj 3:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        tridek-tri fojojn &gcirc;i kantis tiun saman pecon kaj tamen ne <tld/>i&gcirc;is
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kandid</nom> ne <tld/>i&gcirc;is admiri la motivon de sia liberi&gcirc;o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>i&gcirc;is irante al ni
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        volanta kruro ne <tld/>i&gcirc;as de kuro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_t;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">skuizha&ntilde;<klr tip="ind"> (v.g.)</klr></trd>
  <trdgrp lng="ca">
    <trd>cansar-se</trd>,
    <trd>esgotar-se</trd>
  </trdgrp>
  <trd lng="de"><ind>m&uuml;de</ind> werden</trd>
  <trd lng="es"><ind>fatigar</ind>se</trd>
  <trd lng="fr">se <ind>fatiguer</ind></trd>
  <trd lng="nl"><ind>moe</ind> worden</trd>
  <trd lng="oc">s'alassar</trd>
  <trdgrp lng="pl">
    <trd>m&eogonek;czy&cacute; si&eogonek;</trd>,
    <trd>nu&zdot;y&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_t;&c_o;&c_m;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_m;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_m;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="lac.sen0a">
  <kap>sen<tld/>a,
  <var><kap>ne<tld/>igebla</kap></var></kap>
  <snc>
    <dif>
      Ne sentanta <tld/>i&gcirc;on; ne &ccirc;esiganta sian aktivecon:
      <ekz>
        sen<tld/>a laborado
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista 
          en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        en sia profesio &scirc;i estis diligenta kaj sen<tld/>a
        <fnt><bib>Fab4</bib>, <lok>la vento rakontas pri Valdemaro Doe
        kaj pri liaj filinoj</lok></fnt>;
      </ekz>
      <ekz>
        la sen<tld/>a sibletado de cikadoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i komencis labori kun sama ne<tld/>igebla energio kiel iam <nom>Kajsa</nom>
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> estis nelacigebla en la rakontado kaj <klr>[&#8230;]</klr> komencis
        anka&ubreve; elmontradi al li certan sindonemon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_s;&c_t;&c_o;&c_m;&c_n;&c_y;</trd>
  <trd lng="br">diskuizhus<klr tip="ind"> (tud)</klr></trd>
  <trdgrp lng="ca">
    <trd>infatigable</trd>,
    <trd>incansable</trd>,
    <trd>resistent</trd>
  </trdgrp>
  <trd lng="de">unerm&uuml;dlich</trd>
  <trd lng="el">&alfa;&kappa;&omikron;&ypsilon_ton;&rho;&alfa;&sigma;&tau;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>tireless</trd>,
    <trd>indefatigable</trd>,
    <trd>untiring</trd>
  </trdgrp>
  <trd lng="es">incansable</trd>
  <trd lng="fr">infatigable</trd>
  <trd lng="he">&bet;&lamed;&tav;&yod;-&nun;&lamed;&alef;&he;</trd>
  <trd lng="hu">f&aacute;radhatatlan</trd>
  <trd lng="nl">onvermoeid</trd>
  <trd lng="pl">niezmordowany</trd>
  <trd lng="pt">incans&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_u;&c_s;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_t;&c_o;&c_m;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_v;&c_t;&c_o;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_t;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_d;&c_mol;&c_o;&c_r;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
