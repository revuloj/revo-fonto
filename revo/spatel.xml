<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spatel.xml,v 1.19 2023/09/04 05:14:56 revo Exp $">
<kap>
  <rad>spatel</rad>/o
</kap>
<drv mrk="spatel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Plata <ref tip="super" cel="kuler.0o">kulero</ref>,
      ekz. uzata por miksi a&ubreve; sterni duonlikvajn
      substancojn, a&ubreve; por meti farbojn sur paletron:
      <ekz>
        la artisto estis preninta <tld/>on kun iom da olefarboj kaj tiris &gcirc;in sur
        seninteresa foto
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011419.php">10 x 15</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nur en potoj argilaj mi kuiros, uzante lignajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2006/008903.php">Fornoj kaj formoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="trul.0o">trulo</ref>
    <bld lok="&WCU;/thumb/1/1a/Tropenmuseum_Royal_Tropical_Institute_Objectnumber_H-2352_Houten_spatel_met_snijwerk.jpg/640px-Tropenmuseum_Royal_Tropical_Institute_Objectnumber_H-2352_Houten_spatel_met_snijwerk.jpg" prm="CC BY-SA 3.0">skulptita ligna <tld/>o
        <fnt>
          <aut>Tropika muzeo</aut>
          <lok><url ref="&WCW;/File:Tropenmuseum_Royal_Tropical_Institute_Objectnumber_H-2352_Houten_spatel_met_snijwerk.jpg">&WCW;/File:Tropenmuseum_Royal_Tropical_Institute_Objectnumber_H-2352_Houten_spatel_met_snijwerk.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_sh;&c_p;&c_a;&c_t;&c_u;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>lopati&ccaron;ka</trd>,
    <trd>st&ecaron;rka</trd>,
    <trd>&scaron;pachtle</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spatel</trd>,
    <trd>Spachtel</trd>
  </trdgrp>
  <trd lng="en">spatula</trd>
  <trd lng="fr">spatule</trd>
  <trdgrp lng="hu">
    <trd>lapoc</trd>,
    <trd>spatula</trd>,
    <trd>sim&iacute;t&oacute;lap&aacute;t</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3078;&#x3089;</trd>,
    <trd>&#x30D1;&#x30EC;&#x30C3;&#x30C8;&#x30CA;&#x30A4;&#x30D5;</trd>,
    <trd>&#x3053;&#x3066;</trd>
  </trdgrp>
  <trd lng="nl">spatel</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">&scaron;pacht&lcaron;a</trd>
  <trdgrp lng="uk">
    <trd>&#x448;&#x43F;&#x430;&#x442;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x43B;&#x43E;&#x43F;&#x430;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x43E;&#x43F;&#x438;&#x441;&#x442;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
