<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>febr</rad>/o
</kap>
<drv mrk="febr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="febr.0o.MED">
    <dif>
      Malsana stato, karakterizata per plirapidi&gcirc;o de la pulso kaj
      plialti&gcirc;o de la korpa temperaturo. 
    </dif>
    <trd lng="de">Fieber</trd>
  </snc>
  <snc mrk="febr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forta eksciteco, pasia ardo: 
      <ekz>
        monluda, politika <tld/>a<fnt>B</fnt>;
      </ekz>
      <ekz>
        la militista <tld/>o infektis &ccirc;iujn landojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0eco">ardo</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="fajr.0o.fervoro">fajro</ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo</ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="varm.0o.fervoro">varmo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <trd lng="de">Wahn</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>fi&egrave;vre</trd>,
    <trd>fougue</trd>,
    <trd>fr&eacute;n&eacute;sie</trd>,
    <trd>nervosit&eacute;</trd></trdgrp> 
  <trd lng="ru">&c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_a;</trd>
  <trd lng="sv">feber</trd>
</drv>
<drv mrk="febr.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc num="1">
    <dif>
      Ka&ubreve;zata de <tld/>o: <ekz><tld/>aj signoj.</ekz>
    </dif>
    <trd lng="de">fiebrig</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Arda: <ekz><tld/>a malpacienco, laboro, deziro.</ekz>
    </dif>
    <trd lng="de">fiebernd</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>f&eacute;brile</trd>,
    <trd>f&eacute;vreux</trd>,
    <trd>fougueux</trd>,
    <trd>fr&eacute;n&eacute;tique</trd>,
    <trd>nerveux</trd></trdgrp> 
  <trd lng="ru">&c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">febrigt</trd>
</drv>
<drv mrk="febr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="febr.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Esti en malsana <tld/>a stato. 
    </dif>
    <trdgrp lng="de">
     <trd>fiebern</trd>,
     <trd>Fieber haben</trd></trdgrp>
  </snc>
  <snc mrk="febr.0i.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en maltrankvila arda stato de animo. 
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="bol.0i">boli</ref>,
      <ref cel="brul.0i">bruli</ref>,
      <ref cel="flagr.0i">flagri</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>fiebern</trd>,
     <trd>rasen</trd></trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>fi&eacute;vreux</ind></trd>,
    <trd>&ecirc;tre <ind>f&eacute;brile</ind></trd>,
    <trd>s'<ind>agiter</ind><klr tip="ind"> (s'exciter)</klr></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="febr.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsanulo je <tld/>o.  
    </dif>
  </snc>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>
</art>
