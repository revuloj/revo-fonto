<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: febr.xml,v 1.40 2023/09/01 17:28:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>febr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="febr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="febr.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Malsana stato, karakterizata per plirapidi&gcirc;o de la pulso kaj
      plivarmi&gcirc;o de la korpo:
      <ekz>
        &scirc;ia pulso kvieti&gcirc;is kaj <tld/>o falis
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, p. 391a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Fieber</trd>
    <trd lng="es">fiebre</trd>
    <trdgrp lng="fr">
      <trd>fi&egrave;vre</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;z</trd>
    <trd lng="pl">gor&aogonek;czka</trd>
    <trd lng="pt">febre</trd>
    <trd lng="tr">ate&scedil;</trd>
  </snc>
  <snc mrk="febr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forta eksciteco, pasia ardo:
      <ekz>
        monluda, politika <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        la militista <tld/>o infektis &ccirc;iujn landojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="fajr.0o.fervoro">fajro<sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="varm.0ego.fervoro">varmego<sncref/></ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <trd lng="de">Wahn</trd>
    <trdgrp lng="es">
      <trd>ardor pasional</trd>,
      <trd>frenes&iacute;</trd>,
      <trd>fogosidad</trd>,
      <trd>excitaci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>fi&egrave;vre<klr> (fig.)</klr></trd>,
      <trd>fougue</trd>,
      <trd>fr&eacute;n&eacute;sie</trd>,
      <trd>nervosit&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>l&aacute;z<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>h&eacute;v</trd>
    </trdgrp>
    <trd lng="pl">gor&aogonek;czka</trd>
    <trdgrp lng="pt">
      <trd>ardor passional</trd>,
      <trd>grande <ind>excita&ccedil;&atilde;o</ind></trd>
    </trdgrp>
    <trdgrp lng="tr">
      <trd>co&scedil;kunluk</trd>,
      <trd>hararet</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_s;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>hore&ccaron;ka</trd>,
    <trd>teplota</trd>
  </trdgrp>
  <trd lng="de">Fieber</trd>
  <trd lng="en">fever</trd>
  <trd lng="es">fiebre</trd>
  <trd lng="id">demam</trd>
  <trdgrp lng="ja">
    <trd>&#x9AD8;&#x71B1;  [<ind>&#12371;&#12358;&#x306D;&#12388;</ind>]</trd>,
    <trd>&#x71B1;&#x75C5;  [<ind>&#x306D;&#12388;&#12403;&#12423;&#12358;</ind>]</trd>,
    <trd>&#x71B1;&#x4E2D;  [<ind>&#x306D;&#12387;&#12385;&#12421;&#12358;</ind>]</trd>,
    <trd>&#xFF5E;&#x71B1;  [&#xFF5E;<ind>&#12354;&#12388;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">koorts</trd>
  <trd lng="ru">&c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_a;</trd>
  <trd lng="sk">hor&uacute;&ccaron;ka</trd>
  <trd lng="sv">feber</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&#x44F;&c_ch;&#x43A;&c_a;</trd>,
    <trd>&#x43B;&c_i;&c_h;&#x43E;&#x43C;&c_a;&#x43D;&#x43A;&c_a;</trd>,
    <trd><klr>(&#x43F;&c_r;&#x43E; &c_h;&c_v;&#x43E;&c_r;&#x43E;&c_b;&c_u; &c_shch;&c_je;)</klr>
    &#x43F;&c_r;&#x43E;&#x43F;&c_a;&c_s;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd><klr>(&#x43F;&c_r;&#x43E; &#x43F;&c_ib;&c_d;&c_v;&c_i;&c_shch;&c_je;&#x43D;&c_u; &c_t;&c_je;&#x43C;&#x43F;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_u; &c_shch;&c_je;)</klr> &c_zh;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.0a">
  <kap><tld/>a</kap>
  <snc mrk="febr.0a.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Ka&ubreve;zata de <tld/>o:
      <ekz>
        <tld/>aj signoj.
      </ekz>
    </dif>
    <trd lng="de">fiebrig</trd>
    <trd lng="es">febril</trd>
    <trdgrp lng="fr">
      <trd>f&eacute;brile</trd>,
      <trd>fi&eacute;vreux</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;zas</trd>
    <trd lng="nl">koortsig</trd>
    <trd lng="pl">gor&aogonek;czkowy</trd>
  </snc>
  <snc mrk="febr.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Arda:
      <ekz>
        <tld/>a malpacienco, laboro, deziro.
      </ekz>
    </dif>
    <trd lng="de">fiebernd</trd>
    <trdgrp lng="es">
      <trd>febril<klr> (fig.)</klr></trd>,
      <trd>nervioso</trd>,
      <trd>inquieto</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>f&eacute;brile<klr> (fig.)</klr></trd>,
      <trd>fi&eacute;vreux<klr> (fig.)</klr></trd>,
      <trd>fougueux</trd>,
      <trd>nerveux</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>l&aacute;zas<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>heves</trd>,
      <trd>izgatott</trd>
    </trdgrp>
    <trd lng="nl">koortsachtig</trd>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>febriln&iacute;</trd>,
    <trd>hore&ccaron;nat&yacute;</trd>,
    <trd>hore&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">feverish</trd>
  <trd lng="es">febril</trd>
  <trdgrp lng="ja">
    <trd>&#x9AD8;&#x71B1;&#x306E;  [<ind>&#12371;&#12358;&#x306D;&#12388;&#x306E;</ind>]</trd>,
    <trd>&#x71B1;&#x75C5;&#x306E;  [<ind>&#x306D;&#12388;&#12403;&#12423;&#12358;&#x306E;</ind>]</trd>,
    <trd>&#x71B1;&#x72C2;&#12375;&#x305F;  [<ind>&#x306D;&#12387;&#x304D;&#12423;&#12358;&#12375;&#x305F;</ind>]</trd>,
    <trd>&#x71B1;&#x4E2D;&#12375;&#x305F;  [<ind>&#x306D;&#12387;&#12385;&#12421;&#12358;&#12375;&#x305F;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">gor&aogonek;czkowy</trd>
  <trd lng="ru">&c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">hor&uacute;&ccaron;kovit&yacute;</trd>
  <trd lng="sv">febrigt</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&#x44F;&c_ch;&#x43A;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&#x43F;&c_a;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43B;&c_i;&c_h;&#x43E;&#x43C;&c_a;&#x43D;&#x43A;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43B;&c_i;&c_h;&#x43E;&#x43C;&c_a;&#x43D;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="febr.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Esti en malsana <tld/>a stato.
    </dif>
    <trdgrp lng="de">
      <trd>fiebern</trd>,
      <trd>Fieber haben</trd>
    </trdgrp>
    <trd lng="es">tener fiebre</trd>
    <trd lng="hu">l&aacute;zas</trd>
    <trd lng="nl">koorts hebben</trd>
    <trdgrp lng="pl">
      <trd>gor&aogonek;czkowa&cacute;</trd>,
      <trd>mie&cacute; gor&aogonek;czk&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="febr.0i.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en maltrankvila arda stato de animo.
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="brul.0i">bruli</ref>,
      <ref cel="flagr.0i">flagri</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>fiebern</trd>,
      <trd>rasen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>estar caliente <klr> (estado de &aacute;nimo)</klr></trd>,
      <trd>estar excitado</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;zban &eacute;g</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_r;&c_e;&c_c;&c_mol; &c_n;&c_a; <ind>&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_u;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_g;&c_a;&c_r;&c_a;&c_ch;&c_c;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="cs">m&iacute;t hore&ccaron;ku</trd>
  <trd lng="en">have a fever</trd>
  <trd lng="es">tener <ind>fiebre</ind></trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>fi&eacute;vreux</ind></trd>,
    <trd>&ecirc;tre <ind>f&eacute;brile</ind></trd>,
    <trd>s'<ind>agiter</ind><klr tip="ind"> (s'exciter)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x304C;&#12354;&#x308B;  [<ind>&#x306D;&#12388;&#x304C;&#12354;&#x308B;</ind>]</trd>,
    <trd>&#x71B1;&#x72C2;&#12375;&#12390;&#12356;&#x308B;  [<ind>&#x306D;&#12387;&#x304D;&#12423;&#12358;&#12375;&#12390;&#12356;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">gor&aogonek;czkowa&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; hor&uacute;&ccaron;ku</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&#x44F;&c_ch;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_g;&c_a;&c_r;&#x44F;&c_ch;&c_c;&c_ib;</trd>,
    <trd>&c_h;&c_v;&#x43E;&c_r;&c_ib;&c_t;&c_i; &#x43F;&c_r;&#x43E;&#x43F;&c_a;&c_s;&#x43D;&c_i;&c_c;&c_je;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="febr.0ulo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsanulo je <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_o;&c_r;&c_y; &c_n;&c_a; <ind>&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_u;</ind></trd>
  <trd lng="cs">nemocn&yacute; s hore&ccaron;kou</trd>
  <trd lng="es">enfermo con fiebre</trd>
  <trd lng="hu">l&aacute;zas beteg</trd>
  <trd lng="ja">&#x71B1;&#x75C5;&#x60A3;&#32773;  [<ind>&#x306D;&#12388;&#12403;&#12423;&#12358;&#x304B;&#12435;&#12376;&#12419;</ind>]</trd>
  <trd lng="nl">koortspati&euml;nt</trd>
  <trd lng="pl">gor&aogonek;czkuj&aogonek;cy</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">nemocn&yacute; s hor&uacute;&ccaron;kou</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&#x43E;&c_r;&c_i;&c_j; &#x43D;&c_a; &#x43F;&c_r;&#x43E;&#x43F;&c_a;&c_s;&#x43D;&c_i;&c_c;&#x44E;</trd>,
    <trd>&#x43B;&#x44E;&c_d;&c_i;&#x43D;&c_a; &c_z; &c_g;&c_a;&c_r;&#x44F;&c_ch;&#x43A;&#x43E;&#x44E;</trd>,
    <trd>&c_h;&c_v;&#x43E;&c_r;&c_i;&c_j; &c_z;
    &c_g;&c_a;&c_r;&#x44F;&c_ch;&#x43A;&#x43E;&#x44E;/&#x43B;&c_i;&c_h;&#x43E;&#x43C;&c_a;&#x43D;&#x43A;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.flava0o">
  <kap>flava <tld/>o</kap>
  <snc mrk="febr.flava0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Virusa
      <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">malsano</ref>
      de tropikaj landoj, konta&gcirc;ata de kuloj,
      ka&ubreve;zanta febron, sangellason en la digesta organaro
      kaj nigran vomadon, kelkfoje mortiga;
      <ref tip="sin" cel="vomit.0o.MED">vomito</ref>:
      <ekz>
        <klr>[en]</klr> Panama la flava <tld/>o
        regis en karaktero endemia &gcirc;is anta&ubreve; nelonga tempo
        <fnt>
          <aut>Internacia Naturkuraca Asocio</aut>,
          <vrk><url ref="http://www.ina-asocio.info/naturkuracado-naturismo.html">
          Naturkuracado</url></vrk>,
          <lok>[2012?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a; &c_zh;&c_o;&c_w;&c_t;&c_a;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>yelow fever</trd>,
    <trd>&zcaron;lut&aacute; zimnice</trd>
  </trdgrp>
  <trd lng="es">la fiebre amarilla</trd>
  <trd lng="fr">fi&egrave;vre jaune</trd>
  <trd lng="hu">s&aacute;rgal&aacute;z</trd>
  <trd lng="id"><ind>demam</ind> kuning</trd>
  <trd lng="sk">&zcaron;lt&aacute; zimnica</trd>
</drv>

<drv mrk="febr.fojno0o">
  <kap>fojno<tld/>o</kap>
  <snc mrk="febr.fojno0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="alergi.0o.FIZL" lst="voko:malsanoj">Alergia</ref>
      irito de nazaj kaj okulaj mukozoj ka&ubreve;zata de polvoj,
      polenoj, akaroj:
      <ekz>
        eventuale eblas per poleno malsentivigi suferantojn de
        fojno<tld/>o
        <fnt><aut>R. Roze</aut>, <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib>, <lok>1997:11, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a; &c_s;&c_ja;&c_n;&c_n;&c_a;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>senn&aacute; hore&ccaron;ka</trd>,
    <trd>senn&aacute; r&yacute;ma</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heuschnupfen</trd>,
    <trd>allergische <ind>Rhinitis</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>rinitis</ind> al&eacute;rgica</trd>,
    <trd>fiebre del heno</trd>
  </trdgrp>
  <trd lng="fr">rhume des foins</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;nal&aacute;z</trd>,
    <trd>sz&eacute;nan&aacute;tha</trd>
  </trdgrp>
  <trd lng="ja">&#x67AF;&#33609;&#x71B1;  [<ind>&#12371;&#x305D;&#12358;&#x306D;&#12388;</ind>]</trd>
  <trd lng="sk">senn&aacute; n&aacute;dcha</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&#x43D;&#x43D;&c_a; &#x43B;&c_i;&c_h;&#x43E;&#x43C;&c_a;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_s;&c_ib;&#x43D;&#x43D;&c_a; &c_a;&c_s;&c_t;&#x43C;&c_a;</trd>,
    <trd>&c_s;&c_ib;&#x43D;&#x43D;&c_i;&c_j; &#x43D;&c_je;&c_zh;&c_i;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.tifa0o">
  <kap>tifa <tld/>o, <var><kap>tifoida
  <tld/>o<fnt><bib>PIV1</bib></fnt></kap></var></kap>
  <snc mrk="febr.tifa0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Infekta
      <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">malsano</ref>
      ka&ubreve;zata de pluraj salmoneloj
      disvastigataj tra akvo a&ubreve; man&gcirc;a&jcirc;oj,
      ka&ubreve;zanta febron, senforti&gcirc;on, sangadon
      kaj &scirc;iri&gcirc;ojn en la digesta organaro:
      <ekz>
        estas &gcirc;ustaj nur la naski&gcirc;dato <klr>...</klr>
        la aludo pri la ampleksaj lingvaj konoj, la
        tifa <tld/>o
        <fnt>
          <aut>R. Jossinet</aut>,
          <vrk><url ref="http://www.aleph.com.br/kce/historio-12-beaufront.htm">
          La franca savinto de Esperanto: Louis de
          Beaufront</url></vrk>,
          <lok>Franca Esperantisto, 1998-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_sh;&c_n;&c_y; <ind>&c_t;&c_y;&c_f;</ind></trd>
  <trd lng="en">typhoid fever</trd>
