<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: febr.xml,v 1.46 2025/10/02 06:39:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>febr</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="febr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="febr.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Malsana stato, karakterizata per
      alti&gcirc;o de la korpotemperaturo kaj plirapidi&gcirc;o de la pulso:
      <ekz>
        vi havas <tld/>on de 39 gradoj
        <fnt>
          <bib>VRA</bib>
          <lok>Jan Frideg&aring;rd</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vidis lian bopatrinon ku&scirc;anta, malsana de <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 8:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi malvarmumis kaj dum tri tagoj ku&scirc;is en <tld/>o
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia pulso kvieti&gcirc;is kaj <tld/>o falis
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, p. 391a</lok></fnt>;
      </ekz>
      <ekz>
        okuloj senbrilaj a&ubreve; flamantaj de <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gubernatoro subite malsani&gcirc;is de dan&gcirc;era malaria <tld/>o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        virusaj hemoragiaj <tld/>oj (<nom>Lasa</nom>, <nom>Marburg</nom>, <nom>Ebola</nom>)
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html"
              >Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virusa malsano okcident-nila <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html"
              >Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="inflam.0o">inflamo</ref>
    <trd lng="de">Fieber</trd>
    <trd lng="es">fiebre</trd>
    <trdgrp lng="fr">
      <trd>fi&egrave;vre</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;z</trd>
    <trd lng="pl">gor&aogonek;czka</trd>
    <trd lng="pt">febre</trd>
    <trd lng="tr">ate&scedil;</trd>
  </snc>
  <snc mrk="febr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forta <ref tip="super" cel="ekscit.0o">eksciteco</ref>, <ref tip="sin" cel="pasi.0o">pasia</ref>
      <ref tip="sin" cel="ard.0o.pasio">ardo</ref>:
      <ekz>
        <nom>Bazarov</nom> tute sin izolis: <tld/>o de laboro ekregis lin
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la komandanto sin levis kun malama kaj sova&gcirc;a mieno: hommortiga <tld/>o brilis
        en liaj okuloj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        ia febro ekregis tiujn milojn da vaguloj <klr>[&#x2026;]</klr>, la homojn ekregadis
        konsterno a&ubreve; furiozo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u decus interrompi artiston, dum la krea <tld/>o lin tenas
        <fnt>
          <bib>ChV</bib>
          <lok>13</lok>
        </fnt>?
      </ekz>
      <ekz>
        en tiu loko originis la ora <tld/>o en 1849
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="fajr.0o.fervoro">fajro<sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="varm.0ego.fervoro">varmego<sncref/></ref>,
      <ref cel="vigl">vigleco</ref>
    </refgrp>
    <trd lng="de">Wahn</trd>
    <trdgrp lng="es">
      <trd>ardor pasional</trd>,
      <trd>frenes&iacute;</trd>,
      <trd>fogosidad</trd>,
      <trd>excitaci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>fi&egrave;vre<klr> (fig.)</klr></trd>,
      <trd>fougue</trd>,
      <trd>fr&eacute;n&eacute;sie</trd>,
      <trd>nervosit&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>l&aacute;z<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>h&eacute;v</trd>
    </trdgrp>
    <trd lng="pl">gor&aogonek;czka</trd>
    <trdgrp lng="pt">
      <trd>ardor passional</trd>,
      <trd>grande <ind>excita&ccedil;&atilde;o</ind></trd>
    </trdgrp>
    <trdgrp lng="tr">
      <trd>co&scedil;kunluk</trd>,
      <trd>hararet</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_s;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_tsha;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hore&ccaron;ka</trd>,
    <trd>teplota</trd>
  </trdgrp>
  <trd lng="de">Fieber</trd>
  <trd lng="en">fever</trd>
  <trd lng="es">fiebre</trd>
  <trd lng="id">demam</trd>
  <trdgrp lng="ja">
    <trd>&#x9AD8;&#x71B1;  <pr>&#x3053;&#x3046;&#x306D;&#x3064;</pr></trd>,
    <trd>&#x71B1;&#x75C5;  <pr>&#x306D;&#x3064;&#x3073;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x71B1;&#x4E2D;  <pr>&#x306D;&#x3063;&#x3061;&#x3085;&#x3046;</pr></trd>,
    <trd>&#xFF5E;<ind>&#x71B1;</ind>  <pr>&#x3042;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">koorts</trd>
  <trd lng="ru">&c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_a;</trd>
  <trd lng="sk">hor&uacute;&ccaron;ka</trd>
  <trd lng="sv">feber</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_ja;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_h;&c_o;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd><klr>(&c_p;&c_r;&c_o; &c_h;&c_v;&c_o;&c_r;&c_o;&c_b;&c_u; &c_shch;&c_je;)</klr>
    &c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd><klr>(&c_p;&c_r;&c_o; &c_p;&c_ib;&c_d;&c_v;&c_i;&c_shch;&c_je;&c_n;&c_u; &c_t;&c_je;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_u; &c_shch;&c_je;)</klr> &c_zh;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x89F1; <pr>b&igrave;</pr></trd>,
    <trd>&#x70ED;&#x75C5; <pr>r&egrave;b&igrave;ng</pr></trd>,
    <trd>&#x70ED; <pr>r&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="febr.0a">
  <kap><tld/>a</kap>
  <snc mrk="febr.0a.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Ka&ubreve;zata de <tld/>o:
      <ekz>
        <tld/>aj son&gcirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj makuloj aperis sur liaj flavaj vangoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a paroksismo kun tintado en la oreloj proksimi&gcirc;as
        <fnt>
          <bib>IKr</bib>
          <lok>Rakonto Pri Spiritoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">fiebrig</trd>
    <trd lng="es">febril</trd>
    <trdgrp lng="fr">
      <trd>f&eacute;brile</trd>,
      <trd>fi&eacute;vreux</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;zas</trd>
    <trd lng="nl">koortsig</trd>
    <trd lng="pl">gor&aogonek;czkowy</trd>
  </snc>
  <snc mrk="febr.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="ard.0a.fervora">Arda</ref>:
      <ekz>
        <tld/>a malpacienco, laboro, deziro.
      </ekz>
      <ekz>
        <tld/>a maltrankvilo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a scivoleco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        forte stre&ccirc;ita <tld/>a penegado
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a dezirego
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">fiebernd</trd>
    <trdgrp lng="es">
      <trd>febril<klr> (fig.)</klr></trd>,
      <trd>nervioso</trd>,
      <trd>inquieto</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>f&eacute;brile<klr> (fig.)</klr></trd>,
      <trd>fi&eacute;vreux<klr> (fig.)</klr></trd>,
      <trd>fougueux</trd>,
      <trd>nerveux</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>l&aacute;zas<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>heves</trd>,
      <trd>izgatott</trd>
    </trdgrp>
    <trd lng="nl">koortsachtig</trd>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>febriln&iacute;</trd>,
    <trd>hore&ccaron;nat&yacute;</trd>,
    <trd>hore&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">feverish</trd>
  <trd lng="es">febril</trd>
  <trdgrp lng="ja">
    <trd>&#x9AD8;&#x71B1;&#x306E;  <pr>&#x3053;&#x3046;&#x306D;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x71B1;&#x75C5;&#x306E;  <pr>&#x306D;&#x3064;&#x3073;&#x3087;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x71B1;&#x72C2;&#x3057;&#x305F;  <pr>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x71B1;&#x4E2D;&#x3057;&#x305F;  <pr>&#x306D;&#x3063;&#x3061;&#x3085;&#x3046;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="pl">gor&aogonek;czkowy</trd>
  <trd lng="ru">&c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">hor&uacute;&ccaron;kovit&yacute;</trd>
  <trd lng="sv">febrigt</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_ja;&c_ch;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_i;&c_h;&c_o;&c_m;&c_a;&c_n;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_i;&c_h;&c_o;&c_m;&c_a;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="febr.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Esti en malsana <tld/>a stato:
      <ekz>
        la mano de la junulo tremis <klr>[&#x2026;]</klr> li &scirc;ajnis <tld/>i
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>3. La e&ubreve;rope edukita homo.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>fiebern</trd>,
      <trd>Fieber haben</trd>
    </trdgrp>
    <trd lng="es">tener fiebre</trd>
    <trd lng="hu">l&aacute;zas</trd>
    <trd lng="nl">koorts hebben</trd>
    <trdgrp lng="pl">
      <trd>gor&aogonek;czkowa&cacute;</trd>,
      <trd>mie&cacute; gor&aogonek;czk&eogonek;</trd>
    </trdgrp>
    <trd lng="zh">&#x70ED;&#x6027; <pr>r&egrave;x&igrave;ng</pr></trd>
  </snc>
  <snc mrk="febr.0i.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en maltrankvila arda stato:
      <ekz>
        mi <tld/>as pro scivolo
        <fnt>
          <bib>ChV</bib>
          <lok>29</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia ekonomio <tld/>as kaj tute dependas de eksportado de nafto kaj gaso
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="brul.0i">bruli</ref>,
      <ref cel="flagr.0i">flagri</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>fiebern</trd>,
      <trd>rasen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>estar caliente <klr> (estado de &aacute;nimo)</klr></trd>,
      <trd>estar excitado</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;zban &eacute;g</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_r;&c_e;&c_c;&c_mol; &c_n;&c_a; <ind>&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_u;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_g;&c_a;&c_r;&c_a;&c_ch;&c_c;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="cs">m&iacute;t hore&ccaron;ku</trd>
  <trd lng="en">have a <ind>fever</ind></trd>
  <trd lng="es">tener <ind>fiebre</ind></trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>fi&eacute;vreux</ind></trd>,
    <trd>&ecirc;tre <ind>f&eacute;brile</ind></trd>,
    <trd>s'<ind>agiter</ind><klr tip="ind"> (s'exciter)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x304C;&#x3042;&#x308B;  <pr>&#x306D;&#x3064;&#x304C;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x71B1;&#x72C2;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">gor&aogonek;czkowa&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; hor&uacute;&ccaron;ku</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&c_ja;&c_ch;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_g;&c_a;&c_r;&c_ja;&c_ch;&c_c;&c_ib;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_r;&c_ib;&c_t;&c_i; &c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_n;&c_i;&c_c;&c_je;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="febr.0ulo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsanulo je <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_o;&c_r;&c_y; &c_n;&c_a; <ind>&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_u;</ind></trd>
  <trd lng="cs">nemocn&yacute; s hore&ccaron;kou</trd>
  <trd lng="de">Fieberkranker</trd>
  <trd lng="en">person with a <ind>fever</ind></trd>
  <trd lng="es">enfermo con fiebre</trd>
  <trd lng="hu">l&aacute;zas beteg</trd>
  <trd lng="ja">&#x71B1;&#x75C5;&#x60A3;&#x8005;  <pr>&#x306D;&#x3064;&#x3073;&#x3087;&#x3046;&#x304B;&#x3093;&#x3058;&#x3083;</pr></trd>
  <trd lng="nl">koortspati&euml;nt</trd>
  <trd lng="pl">gor&aogonek;czkuj&aogonek;cy</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j; &c_l;&c_i;&c_h;&c_o;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">nemocn&yacute; s hor&uacute;&ccaron;kou</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_o;&c_r;&c_i;&c_j; &c_n;&c_a; &c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_n;&c_i;&c_c;&c_ju;</trd>,
    <trd>&c_l;&c_ju;&c_d;&c_i;&c_n;&c_a; &c_z; &c_g;&c_a;&c_r;&c_ja;&c_ch;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_r;&c_i;&c_j; &c_z;
    &c_g;&c_a;&c_r;&c_ja;&c_ch;&c_k;&c_o;&c_ju;/&c_l;&c_i;&c_h;&c_o;&c_m;&c_a;&c_n;&c_k;&c_o;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="febr.flava0o">
  <kap>flava <tld/>o</kap>
  <snc mrk="febr.flava0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Virusa
      <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">malsano</ref>
      de tropikaj landoj, konta&gcirc;ata de kuloj,
      ka&ubreve;zanta febron, sangellason en la digesta organaro
      kaj nigran vomadon, kelkfoje mortiga;
      <ref tip="sin" cel="vomit.0o.MED">vomito</ref>:
      <ekz>
        <klr>[en]</klr> Panama la flava <tld/>o
        regis en karaktero endemia &gcirc;is anta&ubreve; nelonga tempo
        <fnt>
          <aut>Internacia Naturkuraca Asocio</aut>,
          <vrk><url ref="http://www.ina-asocio.info/naturkuracado-naturismo.html">
          Naturkuracado</url></vrk>,
          <lok>[2012?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_a;&c_m;&c_a;&c_n;&c_k;&c_a; &c_zh;&c_o;&c_w;&c_t;&c_a;&c_ja;</trd>
  <trdgrp lng="cs">
