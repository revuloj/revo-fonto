<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ka&gcirc;</rad>/o
</kap>
<drv mrk="kagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Portebla lo&gcirc;ejo, el metalfadenoj a&ubreve; lignaj vergetoj,
      en kiu oni konservas vivajn birdojn a&ubreve; malgrandajn bestetojn. 
    </dif>
    <trdgrp lng="de">
      <trd>K&auml;fig</trd>,
      <trd>Bauer</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Portebla a&ubreve; fiksa granda lo&gcirc;ejo, el feraj stangoj kaj kradoj,
      por enfermi sova&gcirc;ajn bestojn a&ubreve; homojn.  
    </dif>
    <trd lng="de">K&auml;fig</trd>
  </snc>
  <trd lng="en">cage</trd>
  <trd lng="nl">kooi</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="sv">bur</trd>
</drv>
</art>
