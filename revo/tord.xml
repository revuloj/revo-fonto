<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tord</rad>/i
</kap>
<drv mrk="tord.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Turni teksa&jcirc;on a&ubreve; flekseblan objekton per &gcirc;iaj ekstremoj la&ubreve;
      reciproke kontra&ubreve;a direkto:
      <ekz>
        <tld/>i malsekan tukon;
      </ekz>
      <ekz>
        <tld/>i lanon, fadenon;
      </ekz>
      <ekz>
        pinte supren<tld/>itaj lipharoj;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i kiel serpento;
      </ekz>
      <ekz>
        la vento <tld/>as (turnas, kirlas) la ondojn<fnt>B</fnt>,
      </ekz>
      <ekz>
        la akvon en akvoturnojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        el<tld/>i&gcirc;ema kaj kurbi&gcirc;anta serpento<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="volv.0i">volvi</ref>,
      <ref cel="sxpin.0i">&scirc;pini</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>csavar</trd>,
      <trd>facsar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_ch;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_i;&c_t;&c_mol;)</klr></trd>,
      <trd>&c_v;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Misformi a&ubreve; difekti <tld/>ante:
      <ekz>
        <tld/>i la bu&scirc;on
        (<refgrp tip="sin">
          <ref cel="grimac.0i">grimaci</ref>,
          <ref cel="karika.0i">karikaturi</ref>
        </refgrp>);
      </ekz>
      <ekz>
        li <tld/>is al si la piedon;
      </ekz>
      <ekz>
        arbo <tld/>ita de vento;
      </ekz>
      <ekz>
        lia viza&gcirc;o rigidi&gcirc;is en terura
        <tld/>i&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malsato <tld/>as la ventrojn;
      </ekz>
      <ekz>
        <ind>ne unu hundo lin mordis, ne unu vento lin <tld/>is</ind>
        <klr>(li estas multe spertinta)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">&aacute;tesett a t&udblac;zkereszts&eacute;gen</trd>
      </ekz>
      <ekz>
        for<tld/>i<fnt>Z</fnt>, <tld/>orompi la kapon al birdo<fnt>Z</fnt>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>torz&iacute;t</trd>,
      <trd>eltorz&iacute;</trd>,
      <trd>elh&uacute;z</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_v;&c_i;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>wringen</trd>,
    <trd>winden</trd>
  </trdgrp>
  <trd lng="en">twist</trd>
  <trd lng="nl">wringen</trd>
</drv>

<drv mrk="tord.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Spiralforma, formanta plurajn kurbojn:
      <ekz>
        <tld/>a strato;
      </ekz>
      <ekz>
        <tld/>(it)a <klr>(embarasite kaj implike malklara)</klr> frazo;
      </ekz>
      <ekz>
        prezenti la faktojn false kaj dis<tld/>e
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="serpen.0a">serpenta</ref>,
      <ref cel="zigzag">zigzaga</ref>,
      <ref cel="ond.0olinia">ondolinia</ref>,
      <ref cel="turn">turni&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="hu">
    <trd>csavaros</trd>,
    <trd>girbe-gurba</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwrongen</trd>,
    <trd>krom</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_u;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_i;&c_je;&c_v;&c_a;&c_t;&c_y;&c_j; <klr>(f)</klr></trd>
  </trdgrp>
</drv>
</art>
