<kap>
  <ofc>*</ofc>
  <rad>pretend</rad>/i 
</kap>

<drv mrk="preten.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="preten.0i.postuli">
    <dif>
      <ref tip="super" cel="postul.0i">Postuli</ref> ion, pri kio oni 
      <ref tip="super" cel="asert.0i">asertas</ref>, ke oni havas rajton super &gcirc;i:
      <ekz>
        <tld/>i re&gcirc;econ pro hereda rajto;
      </ekz>
      <ekz>
        <tld/>ante esti sa&gcirc;aj, ili malsa&gcirc;i&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 1:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        postulo <klr>(sen aserto)</klr> ne <tld/>as, rifuzo ne ofendas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tio certe estas sufi&ccirc;a, por ke mi estu &gcirc;ia venkanto &#8208; sed mi
        ne <tld/>as pri
        la premio
        <fnt>
          <bib>Fab4</bib>
          <lok>Kuristoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        ili ne volis rebonigi kanalon, <tld/>ante, ke por ilia laboro
        devas pagi al ili la &scirc;tata trezorejo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="arog.0i">arogi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="col.0o">colo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beanspruchen</trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ig&eacute;nyt tart r&aacute;</trd>,
      <trd>mag&aacute;nak k&ouml;vetel</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>aanspraak maken</trd>,
      <trd>claimen</trd>,
      <trd>aanspraak maken op</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pretendowa&cacute; <klr>(do czego&sacute;)</klr></trd>,
      <trd>ubiega&cacute; si&eogonek; <klr>(o co&sacute;)</klr></trd>,
      <trd>ro&sacute;ci&cacute; sobie prawo <klr>(do czego&sacute;)</klr></trd>,
      <trd>mie&cacute; pretensj&eogonek; <klr>(do czego&sacute;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="preten.0i.parole">
    <dif>
      <ref tip="super" cel="fanfar1.0i.sxajnigi">Fanfaroni</ref> pri
      kvalitoj, kiujn oni kredas havi; flati al si, ke
      oni havas iajn kvalitojn:
      <ekz>
        li <tld/>is, ke li estas granda konanto de la le&gcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[Jesuo]</klr> devas morti tial, ke li <tld/>is esti Filo de Dio
        <fnt>
          <bib>NT</bib>
          <lok>Johano 19:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni volas &scirc;tonmortigi vin <klr>[&#8230;]</klr> &ccirc;ar vi, estante homo,
        <tld/>as esti Dio
        <fnt>
          <bib>NT</bib>
          <lok>Johano 10:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Esperantistoj tute ne <tld/>as, ke ilia lingvo prezentas ion
        tiom perfektan, ke nenio pli alta jam povus ekzisti
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>os ne koni la metodon
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko"
              >La Stranga Butiko</url></vrk>
          <lok>Al Paul Verlaine</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>as je tia sciado, por ne egaligi min kun li en
        perfekteco
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxajn.0igi">&scirc;ajnigi</ref>,
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="blag.0i">blagi</ref>,
      <ref cel="mensog.0i">mensogi</ref>
    </refgrp>
    <trd lng="de">vorgeben</trd>
    <trdgrp lng="hu">
      <trd>azt <ind>hiszi</ind> mag&aacute;r&oacute;l</trd>,
      <trd>felv&aacute;g</trd>,
    </trdgrp>     
    <trdgrp lng="nl">
      <trd><ind>pretentie</ind> hebben</trd>,
      <trd>beweren</trd>,
      <trd>doen alsof</trd>
    </trdgrp>
    <trd lng="pl">by&cacute; <ind>pretensjonalnym</ind></trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_t;&c_e;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ra;&t_dzata;&t_shabchu; &t_ba;&t_ra;&t_dzata;&t_shabchu;&t_sa; &t_ba;&t_ra;&t_dzata;&t_shabchu; &t_ra;&t_dzata;&t_shabchu;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>chlubit se</trd>,
    <trd>n&aacute;rokovat</trd>,
    <trd>osobovat si</trd>,
    <trd>po&zcaron;adovat</trd>,
    <trd>p&rcaron;edst&iacute;rat</trd>,
    <trd>&ccaron;init si n&aacute;rok</trd>,
    <trd>&zcaron;&aacute;dat</trd>
  </trdgrp>
  <trd lng="en">claim</trd>
  <trd lng="fr">pr&eacute;tendre</trd>
  <trd lng="ne">&#2342;&#2366;&#2348;&#2368;</trd>
  <trdgrp lng="pt">
    <trd>pretender</trd>,
    <trd>alegar</trd>,
    <trd>ter a <ind>pretens&atilde;o</ind> de</trd>    
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_t;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;rokova&tcaron;</trd>,
    <trd>po&zcaron;adova&tcaron;</trd>,
    <trd>robi&tcaron; si n&aacute;rok</trd>,
    <trd>&zcaron;iada&tcaron;</trd>
  </trdgrp>
</drv>


<drv mrk="preten.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="P"/>a postulo, aserto, fanfarono:
      <ekz>
        mi ne havas <tld/>on je indulga agado de via
        flanko
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni devas povoscii porti kaj gardi sian personan dignon, a&ubreve; oni devas rifuzi al
        si &ccirc;iun <tld/>on pri &gcirc;i
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu havas egalan rajton <klr>[&#8230;]</klr> <tld/>o disbati&gcirc;as kontra&ubreve; <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Anspruch</trd>
    <trd lng="fr">pr&eacute;tention</trd>
  </snc>
  <trd lng="af">eis</trd>
  <trd lng="am">&#4845;&#4872;&#4707;&#4763;&#4621; &#4901;&#4843;&#4676;</trd>
  <trd lng="ar">&a_m;&a_T;&a_A;&a_l;&a_b;&a_t_marbuta;</trd>
