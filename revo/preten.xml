<drv mrk="preten.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc>
    <dif>
      Inklina <tld/>i, montranta <tld/>on:
      <ekz>
        salamandro ne havas grandajn <tld/>ojn malgra&ubreve; la <tld/>ema ekstera&jcirc;o
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ema instruistino
        <fnt>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="&Monato;/2015/011401.php">&Gcirc;emelaj desegna&jcirc;oj</url></vrk>
          <bib>Monato</bib>
          <lok>2015/05, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> &scirc;atus aldoni kelkajn pensojn miajn al la menciita eseo, se tio
        estas tro <tld/>ema
        <fnt>
        <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200105.pdf#page=22">Hilelaj influoj sur
          Esperanto</url></vrk>
          <bib>Monato</bib>
          <lok>2001/05, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ema stilo</ind>.
        <trd lng="hu">felv&aacute;g&oacute;s st&iacute;lus</trd>
        <trd lng="pl">styl <ind>pretensjonalny</ind></trd>
        <trd lng="ru">&c_p;&c_r;&c_je;&c_t;&c_je;&c_n;&c_c;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;
        &c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ambici.0a">ambicia</ref>,
       <ref cel="moder.mal0a">malmodera</ref>,
       <ref cel="modest.mal0a">malmodesta</ref>,
       <ref cel="postul.0ema">postulema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_t;&c_e;&c_n;&c_c;&c_y;&c_jo;&c_z;&c_n;&c_y;</trd>
  <trd lng="de">anspruchsvoll</trd>
  <trdgrp lng="ja">
    <trd>&#x62BC;&#x3057;&#x306E;&#x5F37;&#x3044;  [<ind>&#x304A;&#x3057;&#x306E;&#x3064;&#x3088;&#x3044;</ind>]</trd>,
    <trd>&#x539A;&#x304B;&#x307E;&#x3057;&#x3044;  [<ind>&#x3042;&#x3064;&#x304B;&#x307E;&#x3057;&#x3044;</ind>]</trd>,
    <trd>&#x3046;&#x306C;&#x307C;&#x308C;&#x306E;&#x5F37;&#x3044;  [<ind>&#x3046;&#x306C;&#x307C;&#x308C;&#x306E;&#x3064;&#x3088;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">pretensjonalny</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43C;&#x43E;&c_g;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_b;&c_a;&c_g;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_je;&c_t;&c_je;&#x43D;&c_z;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="preten.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      <ref tip="sin" cel="rezign.0i">Rezigni</ref> je <tld/>o:
      <ekz>
        la proletaro ne nur mal<tld/>as defendi la nacian
        disvolvi&gcirc;on de &ccirc;iu nacio, sed male, avertadas la
        masojn kontra&ubreve; tiaj iluzioj
        <fnt>
          <aut>Lenin</aut>: <vrk>Notoj pri la nacia
          problemo</vrk>
          <lok>Cito la&ubreve;
            <url ref="http://satesperanto.free.fr/frakcioj/sen_abc.html#b45">A.B.C.
            de sennaciismo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        okulfrape ili manifestas siajn &ccirc;astecon, animpurecon, mal<tld/>emon kaj
        martiremon
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; &c_p;&c_r;&c_e;&c_t;&c_e;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_a;&c_d; <ind>&c_p;&c_r;&c_e;&c_t;&c_e;&c_n;&c_z;&c_ib;&c_ja;&c_w;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verzichten</trd>,
    <trd><ind>Abstand</ind> nehmen</trd>
  </trdgrp>
  <trd lng="fr">renoncer &agrave;</trd>
  <trd lng="hu">nem tart r&aacute; <ind>ig&eacute;nyt</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x68C4;&#x3059;&#x308B;  [<ind>&#x307B;&#x3046;&#x304D;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x68C4;&#x6A29;&#x3059;&#x308B;  [<ind>&#x304D;&#x3051;&#x3093;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl"><ind>afstand</ind> doen</trd>
  <trdgrp lng="pl">
    <trd>rezygnowa&cacute;</trd>,
    <trd>zrzeka&cacute; si&eogonek; <ind>pretensji</ind> <klr>(do
    czego&sacute;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr>
  (&c_o;&c_t; &c_p;&c_r;&c_je;&c_t;&c_je;&c_n;&c_z;&c_i;&c_j;,
  &c_p;&c_r;&c_a;&c_v;)</klr></trd>
  <trdgrp lng="sk">
    <trd>nen&aacute;rokova&tcaron; si</trd>,
    <trd>vzda&tcaron; sa n&aacute;roku</trd>
  </trdgrp>
</drv>

<drv mrk="preten.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="sin" cel="rezign.0o">Rezigno</ref> je eventualaj
      <tld/>oj:
      <ekz>
