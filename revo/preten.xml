  <trd lng="co">sustennu</trd>
  <trd lng="da">fordring</trd>
  <trd lng="et">n&#xF5;ue</trd>
  <trd lng="eu">erreklamazioa</trd>
  <trd lng="fy">eask</trd>
  <trd lng="gl">reivindicaci&#xF3;n</trd>
  <trd lng="gu">&#xAA6;&#xABE;&#xAB5;&#xACB;</trd>
