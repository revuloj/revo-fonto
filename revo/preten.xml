    <trd>alegar</trd>,
    <trd>ter a <ind>pretens&atilde;o</ind> de</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_t;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
