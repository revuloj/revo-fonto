  </trdgrp>
  <trd lng="en">claim</trd>
  <trd lng="fr">pr&eacute;tendre</trd>
  <trdgrp lng="ja">
    <trd>&#35201;&#27714;&#12377;&#12427;  <pr>&#12424;&#12358;&#12365;&#12421;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#20027;&#24373;&#12377;&#12427;  <pr>&#12375;&#12421;&#12385;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#35328;&#12356;&#24373;&#12427;  <pr>&#12356;&#12356;&#12399;&#12427;</pr></trd>,
    <trd>&#33258;&#31216;&#12377;&#12427;  <pr>&#12376;&#12375;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#33258;&#24930;&#12377;&#12427;  <pr>&#12376;&#12414;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#20844;&#35328;&#12377;&#12427;  <pr>&#12371;&#12358;&#12370;&#12435;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#2342;&#2366;&#2348;&#2368;</trd>
  <trdgrp lng="pt">
