<drv mrk="kur.trans0i">
  <kap>trans<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kur.trans0i.KOMUNE">
    <dif>
      Trairi rapide kaj poste forlasi:
      <ekz>
        ni trans<tld/>u al alia temo;
      </ekz><ekz>
        sur &scirc;ia viza&gcirc;o trans<tld/>is dum unu minuto milo da
        <klr>[&#8230;]</klr> malestimaj ridetoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
        &scirc;i rapidis trans<tld/>i la straton, kiam du kale&scirc;oj kun furioza
        rapideco pretergalopis
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kur.trans0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Transiri kaj submeti&gcirc;i al la ordono de la malamiko:
      <ekz>
        mi timas la Judojn, kiuj trans<tld/>is al la &Hcirc;aldeoj, ke oni ne
        transdonu min en iliajn manojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 38:19</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berlaufen</trd>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_b;&c_je;&c_g;&c_ch;&c_y; <klr tip="amb">(&c_p;&c_r;&c_a;&c_z;
  &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_o;&c_b;&c_ja;&c_g;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_t;&c_i;&c_ch;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_m;&c_i;&c_n;&c_a;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;eb&iacute;hat</trd>,
    <trd>p&rcaron;eb&ecaron;hnout</trd>
  </trdgrp>
  <trd lng="fr">traverser en courant</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tfut <klr tip="amb">(vhov&aacute;)</klr></trd>,
    <trd>t&uacute;lfut</trd>
  </trdgrp>
  <trd lng="it">passare<klr> (rapidamente)</klr></trd>
  <trdgrp lng="nl">
    <trd>lopen over</trd>,
    <trd>overgaan</trd>
  </trdgrp>
  <trd lng="pl">przebiega&cacute;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol;<klr>
  (&c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  <trd lng="sk">prebieha&tcaron; <klr>(r&yacute;chle
  prech&aacute;dza&tcaron;)</klr></trd>
</drv>

<drv mrk="kur.cxas0i">
  <kap>&ccirc;as<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kur.cxas0i.KOMUNE">
    <dif>
     <ref tip="super" cel="kur.post0i">Post<tld/>i</ref> 
     por kapti; <ref tip="dif" cel="cxas.0i">&ccirc;asi</ref>:
      <ekz>
        &ccirc;as<tld/>i leporon;
      </ekz><ekz>
        la &ccirc;as<tld/>o por la plej grandaj eksplodoj en la universo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=280836"
              >Gamo-radia ekbrilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_g;&c_ch;&c_y; <klr tip="amb">(&c_z;&c_a;
