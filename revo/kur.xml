  </trdgrp>
  <trd lng="nl">achtervolgen</trd>
  <trdgrp lng="pl">
    <trd>goni&cacute;</trd>,
    <trd>&sacute;ciga&cacute; <klr>(kogo&sacute;,
    co&sacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_n;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kur.vet0o">
  <kap>vet<tld/>o</kap>
  <snc mrk="kur.vet0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <tld lit="K"/>o inter sportaj konkurantoj:
      <ekz>
        jen la botelo mobilizas / kaj via glaso improvizas / vet<tld/>on
        al najbara glas'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Vagonrestoracio</lok>
        </fnt>;
      </ekz><ekz>
        li trovis anoncon pri la sabataj &ccirc;evalaj vet<tld/>oj, do ni kune
        iris al la kurejo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
        </fnt>;
      </ekz><ekz>
        subite arego da virinoj interrompas prezentotan &ccirc;arvet<tld/>on
        per sia propra 
        &ccirc;irka&ubreve;<tld/>ado en la cirkejo, tiel malebligante la prezenta&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008885.php">Rakontoj
          neniam perdontaj sian legindecon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_g;
    &c_n;&c_a;&c_v;&c_y;&c_p;&c_je;&c_r;&c_a;&c_d;&c_k;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_je;&c_g;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;
    &c_w; &c_b;&c_je;&c_g;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_malmol;&c_s;&c_t;&c_je;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_ja;&c_g;&c_a;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">redadeg</trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;h o z&aacute;vod</trd>,
    <trd>z&aacute;vod v b&ecaron;hu</trd>
  </trdgrp>
  <trd lng="fr">course<klr> (sportive)</klr></trd>
  <trd lng="hu">versenyfut&aacute;s</trd>
  <trdgrp lng="id">
    <trd><ind>balap</ind>an</trd>,
    <trd><ind>lomba</ind> lari</trd>,
    <trd><ind>pacu</ind>an</trd>
  </trdgrp>
  <trd lng="it">corsa<klr> (gara)</klr></trd>
  <trd lng="nl">wedloop</trd>
  <trdgrp lng="pl">
    <trd>wy&sacute;cig</trd>,
    <trd>gonitwa</trd>,
    <trd>rajd</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_g;
    &c_n;&c_a;&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_k;&c_i;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_n;&c_i;&c_je; &c_v;
    &c_b;&c_je;&c_g;&c_je;</trd>,
