<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>cikad</rad>/o
</kap>
<drv mrk="cikad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="super" cel="cikad.0oj"><tld/>oj</ref>.
    </dif>
  </snc>
  <trd lng="be">цыкада</trd>
  <trd lng="br">skrilh-wez</trd>
  <trdgrp lng="de">
    <trd>Zikade</trd>,
    <trd>Zirpe</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>τζιτζίκι</trd>,
    <trd>τζίτζικας</trd>
  </trdgrp>
  <trd lng="en">cicada</trd>
  <trd lng="fr">cigale</trd>
  <trd lng="hu">kabóca</trd>
  <trd lng="pt">cigarra</trd>
  <trd lng="ru">цикада</trd>
</drv>
<drv mrk="cikad.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subordo de la ordo <ref tip="super" cel="homopt.0oj">homopteroj</ref>
      (<trd lng="la">Auchenrhyncha</trd>), kun triparta rostro, saltkapablaj
      malantaŭaj kruroj kaj mallongaj tentakloj. La maskloj ofte
      kapabla laŭte ĉirpi per abdomenaj membranoj.
    </dif>
  </snc>
  <trd lng="hu">színkabócák</trd>
  <trd lng="pt">cicanídeos</trd>
</drv>
</art>
