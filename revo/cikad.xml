<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>cikad</rad>/o
</kap>
<drv mrk="cikad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="super" cel="cikad.0oj"><tld/>oj</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zikade</trd>,
    <trd>Zirpe</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&tau;&zeta;&jota;&tau;&zeta;&jota_ton;&kappa;&jota;</trd>,
    <trd>&tau;&zeta;&jota_ton;&tau;&zeta;&jota;&kappa;&alfa;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="fr">cigale</trd>
  <trd lng="pt">cigarra</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
</drv>
<drv mrk="cikad.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subordo de la ordo <ref tip="super" cel="homopt.0oj">homopteroj</ref>
      (<trd lng="la">Auchenrhyncha</trd>), kun triparta rostro, saltkapablaj
      malanta&ubreve;aj kruroj kaj mallongaj tentakloj. La maskloj ofte
      kapabla la&ubreve;te &ccirc;irpi per abdomenaj membranoj.
    </dif>
  </snc>
  <trd lng="pt">cican&iacute;deos</trd>
</drv>
</art>
