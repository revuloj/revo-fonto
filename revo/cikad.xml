<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cikad.xml,v 1.33 2024/11/06 18:37:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>cikad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cikad.0o">
  <kap><tld/>o</kap>
  <snc mrk="cikad.0o.ZOO">
    <dif>
      Iu el <ref tip="super" cel="cikad.0oj"><tld/>oj</ref>:
      <ekz>
        <nom>Fabre</nom> sidis foje dum 12 horoj kaj a&ubreve;skultis
        la senfinan kanton de unusola <tld/>o,
        poste li ne povis sidi pli longe,
        sed la cikado da&ubreve;rigis
        <fnt>
          <aut>R. Franc&eacute;, trad. N. G.</aut>,
          <vrk><url ref="&DonH;Revuoj/np/np5802/bestolingvo.html">
          Bestolingvo</url></vrk>,
          <lok>Norda Prismo, 58:2, p. 86a-88a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/78/Cacama_Albuquerque_NM.JPG/136px-Cacama_Albuquerque_NM.JPG"
    prm="CC-BY-SA-3.0"><tld/>o el la genro Cacama, Elena Gallegos parko en Albuquerque
        <fnt>
          <aut>I. Skoch3</aut>
          <lok><url
          ref="&WCW;/File:Cacama_Albuquerque_NM.JPG">&WCW;/File:Cacama_Albuquerque_NM.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_c;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="br">skrilh-wez</trd>
  <trd lng="ca">cigala</trd>
  <trd lng="cs">cik&aacute;da</trd>
  <trdgrp lng="de">
    <trd>Zikade</trd>,
    <trd>Zirpe</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&tau;&zeta;&jota;&tau;&zeta;&jota_ton;&kappa;&jota;</trd>,
    <trd>&tau;&zeta;&jota_ton;&tau;&zeta;&jota;&kappa;&alfa;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">cicada</trd>
  <trd lng="fr">cigale</trd>
  <trdgrp lng="he">
    <trd>&#x5E6;&#x5B4;&#x5D9;&#x5E7;&#x5B8;&#x5D3;&#x5B8;&#x5D4;</trd>,
    <trd>&#x5E1;&#x5D5;&#x5D2; &#x5E6;&#x5E8;&#x5E6;&#x5E8;</trd>
  </trdgrp>
  <trd lng="hu">kab&oacute;ca</trd>
  <trd lng="it">cicala</trd>
  <trd lng="ja">&#x30BB;&#x30DF;</trd>
  <trd lng="nl">cicade</trd>
  <trdgrp lng="pl">
    <trd>cykada</trd>,
    <trd>piewnik</trd>
  </trdgrp>
  <trd lng="pt">cigarra</trd>
  <trd lng="ro">greier</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">cik&aacute;da</trd>
  <trd lng="uk">&c_c;&c_i;&#x43A;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#34633;&#x5A1F; <pr>ch&#xE1;nju&amacron;n</pr></trd>,
    <trd>&#34601; <pr>ti&#xE1;o</pr></trd>,
    <trd>&#x77E5;&#x4E86; <pr>zh&#x12B;li&#x1CE;o</pr></trd>,
    <trd>&#34633; <pr>ch&#xE1;n</pr></trd>,
    <trd>&#21799;&#x9CE5; <pr>j&#xEC;niao</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cikad.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="cikad.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Subordo de la ordo <ref tip="malprt" cel="hemipt.0oj">hemipteroj</ref>
      (<trd lng="la">Auchenorrhyncha</trd>), kun triparta rostro,
      saltkapablaj
      malanta&ubreve;aj kruroj kaj mallongaj antenoj; la maskloj ofte
      kapabla la&ubreve;te &ccirc;irpi per abdomenaj membranoj.
    </dif>
  </snc>
   <trd lng="be">&c_c;&c_y;&c_k;&c_a;&c_d;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_p;&c_a;&c_d;&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="fr">Cicadid&eacute;s</trd>
  <trd lng="hu">sz&iacute;nkab&oacute;c&aacute;k</trd>
  <trdgrp lng="it">
    <trd>Auchenorrinchi</trd>,
    <trd>cicale</trd>,
    <trd>cicaline</trd>
  </trdgrp>
  <trd lng="nl">cicaden</trd>
  <trdgrp lng="pl">
    <trd>cykadowate</trd>,
    <trd>piewnikowate</trd>
  </trdgrp>
  <trd lng="pt">cican&iacute;deos</trd>
</drv>
</art>
