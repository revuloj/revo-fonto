<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>

<art mrk="$Id$">

<kap>

<ofc>*</ofc>

<rad>blek</rad>/i

</kap>

<drv mrk="blek.0i">

<kap><ofc>*</ofc><tld/>i</kap>

<gra><vspec>ntr</vspec></gra>

<snc mrk="blek.0i.beste">

<dif>

Krii (p. p. bestoj):

<ekz>

<ind>&scirc;af<tld/>i</ind>,

<trd lng="br">begeliat</trd>

<trd lng="fr">b&ecirc;ler</trd>

<trd lng="hu">b&eacute;get</trd>

</ekz>

<ekz>

<ind>&ccirc;eval<tld/>i</ind>,

<trd lng="br">c'hwirinat</trd>

<trd lng="fr">hennir</trd>

<trd lng="hu">nyer&iacute;t</trd>

</ekz>

<ekz>

<ind>lup<tld/>i</ind>.

<trd lng="br">yudal</trd>

<trd lng="fr">hurler</trd>

<trd lng="hu">von&iacute;t</trd>

</ekz>

<ekz>

<ind>ran<tld/>i</ind>,

<trd lng="br">rakal</trd>

<trd lng="fr">coasser</trd>

<trd lng="hu">brekeg</trd>

</ekz>

<ekz>

<ind>kokin<tld/>i</ind>.

<trd lng="br">klochal</trd>

<trd lng="fr">glousser</trd>

<trd lng="hu">kotkod&aacute;csol</trd>

</ekz>

</dif>

<refgrp tip="sub">

<ref cel="grumbl.0i">grumbli</ref>,

<ref cel="pep.0i">pepi</ref>,

<ref cel="boj.0i">boji</ref>,

<ref cel="hojl.0i">hojli</ref>,

<ref cel="hurl.0i.lupo">hurli</ref>,

<ref cel="mugx.0i">mu&gcirc;i</ref>,

<ref cel="miaux.0i">mia&ubreve;i</ref>,

<ref cel="tril.0i">trili</ref>,

<ref cel="hen.0i">heni</ref>,

<ref cel="grunt.0i">grunti</ref>,

<ref cel="kluk.0i">kluki</ref>,

<ref cel="ulul.0i">ululi</ref>.

</refgrp>

<trdgrp lng="be">

<trd>&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_u;)</klr></trd>,

<trd>&c_b;&c_l;&c_ja;&c_ja;&c_c;&c_mol;</trd>,

<trd>&c_ib;&c_r;&c_zh;&c_a;&c_c;&c_mol;</trd>,

<trd>&c_m;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,

<trd>&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,

<trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,

<trd>&c_k;&c_v;&c_a;&c_h;&c_t;&c_a;&c_c;&c_mol;</trd>

</trdgrp>

<trd lng="fr">crier <klr>(en parlant d'un animal)</klr></trd>

<trdgrp lng="ru">

<trd>&c_k;&c_r;&c_i;&c_ch;&c_a;&c_t;&c_mol; <klr>(&c_o; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_h;)</klr></trd>,

<trd>&c_b;&c_l;&c_je;&c_ja;&c_t;&c_mol;</trd>,

<trd>&c_r;&c_zh;&c_a;&c_t;&c_mol;</trd>,

<trd>&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>,

<trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>,

<trd>&c_k;&c_v;&c_o;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>

</trdgrp>

</snc>

<snc mrk="blek.0i.FIG">

<uzo tip="stl">FIG</uzo>

<dif>

Kolere a&ubreve; plore kriegi.

</dif>

<trd lng="be">&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>

<trd lng="br">blejal<klr tip="ind"> (gant an dud)</klr></trd>

<trd lng="fr">brailler</trd>

</snc>

<trd lng="en">bleat</trd>

<trd lng="hu">b&odblac;g</trd>

<trd lng="ru">&c_v;&c_y;&c_t;&c_mol;</trd>

</drv>

<drv mrk="blek.0o">

<kap><tld/>o</kap>

<snc>

<dif>

Krio de besto.

</dif>

</snc>

<trdgrp lng="be">

<trd>&c_k;&c_r;&c_y;&c_k; <klr tip="amb">(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_y;)</klr></trd>,

<trd>&c_v;&c_o;&c_j;</trd>,

<trd>&c_r;&c_y;&c_k;</trd>

</trdgrp>

<trd lng="fr">cri <klr>(d'un animal)</klr></trd>

<trdgrp lng="ru">

<trd>&c_k;&c_r;&c_i;&c_k; <klr>(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_g;&c_o;)</klr></trd>,

<trd>&c_v;&c_o;&c_j;</trd>

</trdgrp>

</drv>

</art>

