<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blek.xml,v 1.30 2023/10/29 19:11:02 revo Exp $">

<kap>
  <ofc>*</ofc>
  <rad>blek</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="blek.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="blek.0i.beste">
    <dif>
      <ref tip="super" cel="kri.0i.vocxi">Krii</ref> (p. p. bestoj):
      <ekz>
        animaloj povas <ref tip="sub" cel="boj.0i">boji</ref>, 
        <ref tip="sub" cel="miaux.0i">mia&ubreve;i</ref>, <ref tip="sub" cel="ia1.0i">iai</ref>, 
        <ref tip="sub" cel="hen.0i">heni</ref>, <ref tip="sub" cel="tril.0i">trili</ref>, 
        <ref tip="sub" cel="kluk.0i.ZOO">kluki</ref>, <ref tip="sub" cel="kluk.0i">kluki</ref>, 
        <tld/>i diversmaniere
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bovinoj <klr>[&#x2026;]</klr> <tld/>is, sed ne forflanki&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili fari&gcirc;is kiel voluptemaj pasiaj &ccirc;evaloj, &ccirc;iu <tld/>as al la edzino de sia
        proksimulo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 5:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekkriis per granda vo&ccirc;o, kiel leono <tld/>as
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 10:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>is ranoj en la mar&ccirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lupoj <tld/>is, kaj la korvoj kriis
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la azeno <tld/>is, la hundo bojis, la kato mia&ubreve;is kaj la koko kantis sian
        kikeriki
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&scirc;af<tld/>i</ind>,
        <trd lng="br">begeliat</trd>
        <trdgrp lng="de">
          <trd>m&auml;hen</trd>,
          <trd>bl&ouml;ken</trd>
        </trdgrp>
        <trd lng="fr">b&ecirc;ler</trd>
        <trd lng="hu">b&eacute;get</trd>
        <trd lng="pl">becze&cacute;</trd>
      </ekz>
      <ekz>
        <ind>&ccirc;eval<tld/>i</ind>,
        <trd lng="br">c'hwirinat</trd>
        <trd lng="de">wiehern</trd>
        <trd lng="fr">hennir</trd>
        <trd lng="hu">nyer&iacute;t</trd>
        <trd lng="pl">r&zdot;e&cacute;</trd>
      </ekz>
      <ekz>
        <ind>lup<tld/>i</ind>.
        <trd lng="br">yudal</trd>
        <trd lng="de">heulen</trd>
        <trd lng="fr">hurler</trd>
        <trd lng="hu">von&iacute;t</trd>
        <trd lng="pl">wy&cacute;</trd>
      </ekz>
      <ekz>
        <ind>ran<tld/>i</ind>,
        <trd lng="br">rakal</trd>
        <trd lng="de">quaken</trd>
        <trd lng="fr">coasser</trd>
        <trd lng="hu">brekeg</trd>
        <trd lng="pl">rechota&cacute;</trd>
      </ekz>      
      <ekz>
        <ind>kokin<tld/>i</ind>.
        <trd lng="br">klochal</trd>
        <trd lng="de">gackern</trd>
        <trd lng="fr">glousser</trd>
        <trd lng="hu">kotkod&aacute;csol</trd>
        <trd lng="pl">gdaka&cacute;</trd>
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="grumbl.0i">grumbli</ref>,
      <ref cel="pep.0i">pepi</ref>,      
      <ref cel="hojl.0i">hojli</ref>,
      <ref cel="hurl.0i.lupo">hurli</ref>,
      <ref cel="mugx.0i">mu&gcirc;i</ref>,      
      <ref cel="grunt.0i">grunti</ref>,
      <ref cel="ulul.0i">ululi</ref>
    </refgrp>
    
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a;
      &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_u;)</klr></trd>,
      <trd>&c_b;&c_l;&c_ja;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_ib;&c_r;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_m;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_a;&c_h;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">crier <klr>(en parlant d'un animal)</klr></trd>
    <trd lng="pl">wydawa&cacute; odg&lstroke;osy<klr> (o
    zwierz&eogonek;tach)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_i;&c_ch;&c_a;&c_t;&c_mol; <klr>(&c_o;
      &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_h;)</klr></trd>,
      <trd>&c_b;&c_l;&c_je;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_o;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="blek.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="kri.0i">Krii</ref> simile al besto:
      <ekz>
        li <tld/>is kiel frenezulo kaj batis kiel furiozanta diablo
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi volas defali de ni, &ccirc;ar ia senvalora knabino plore <tld/>as
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>?
      </ekz>
      <ekz>
        la a&ubreve;tomobilo ek<tld/>is, saltis, levi&gcirc;is
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi estas tiel malprudenta, li <tld/>is, ke vi tu&scirc;as min <klr>[&#x2026;]</klr> mi
        mortigas vin kiel hundon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la kolumo &scirc;i &ccirc;iufoje malkovras fremdajn harojn, kaj &scirc;i pri<tld/>as la
        malfidelulon
        <fnt>
          <bib>KrB</bib>
          <lok>...tial mi prenis broson kaj purigis la veston</lok>
        </fnt>;
      </ekz>
      <ekz>
        ha! tiamaniere okazas aferoj, &scirc;i <tld/>is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mu&gcirc;is kaj &gcirc;em<tld/>is <klr>[&#x2026;]</klr> batate
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Korpogvardia veterano</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trd lng="br">blejal<klr tip="ind"> (gant an dud)</klr></trd>
    <trd lng="fr">brailler</trd>
    <trdgrp lng="pl">
      <trd>rycze&cacute;</trd>,
      <trd>wy&cacute;</trd>,
      <trd>zawodzi&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_i;&c_z;&c_d;&c_a;&c_v;&c_a;&c_m;
    &c_zh;&c_i;&c_v;&c_o;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;
    &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_shch;&c_ja;</trd>,
    <trd>&c_b;&c_l;&c_je;&c_ja;</trd>,
    <trd>&c_r;&c_malmol;&c_m;&c_zh;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_malmol;&c_r;&c_k;&c_a;&c_m;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>be&ccaron;et <klr>(zv&iacute;&rcaron;e)</klr></trd>,
    <trd>me&ccaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bl&ouml;ken</trd>,
    <trd>heulen</trd>,
    <trd>kreischen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bleat</trd>,
    <trd>bellow</trd>,
    <trd>bray</trd>,
    <trd>low</trd>,
    <trd>moo</trd>,
    <trd>neigh</trd>,
    <trd>roar</trd>,
    <trd>squeal</trd>,
    <trd>whinny</trd>
  </trdgrp>
  <trd lng="es">gritar<klr tip="amb"> (un animal)</klr></trd>
  <trdgrp lng="he">
    <trd>&lamed;&gimel;&ayin;&vav;&tav;</trd>,
    <trd>&lamed;&pe;&ayin;&vav;&tav;</trd>
  </trdgrp>
  <trd lng="hu">b&odblac;g</trd>
   <trdgrp lng="ja">
    <trd>&#x9CF4;&#x304F;  <pr>&#x306A;&#x304F;</pr></trd>,
    <trd>&#x3044;&#x306A;&#x306A;&#x304F;</trd>,
    <trd>&#x307B;&#x3048;&#x308B;</trd>,
    <trd>&#x308F;&#x3081;&#x304F;</trd>
  </trdgrp>
  <trd lng="pt">gritar<klr tip="amb"> (falando-se de
   animais)</klr></trd>
  <trd lng="ru">&c_v;&c_y;&c_t;&c_mol;</trd>
  <trd lng="sk">b&lcaron;a&ccaron;a&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_k;&c_r;&c_i;&c_k;&c_i;&nbsp;(&c_p;&c_r;&c_o; &c_t;&c_v;&c_a;&c_r;&c_i;&c_n;)</trd>,
    <trd>&c_g;&c_a;&c_v;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_u;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ib;&c_r;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x6012;&#x543C; <pr>n&#xF9;h&#x1D2;u</pr></trd>
</drv>

<drv mrk="blek.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="sub" cel="kri.0o">Krio</ref> de besto:
      <ekz>
        en la fundo de la valo <klr>[&#x2026;]</klr> oni povis distingi <klr>[&#x2026;]</klr> &ccirc;evalajn
        <tld/>ojn kaj kriojn de la &ccirc;efoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksonis grandega vo&ccirc;o, pli potenca ol leona <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonis nokte &gcirc;emantaj <tld/>oj de l' hienoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="blek.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Krio simila al tiu de besto:
      <ekz>
        la junulo volis paroli, protesti, sed li povis respondi nur per malsonora <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la gor&gcirc;oj gargaris senartikaj, simiosimilaj <tld/>oj
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ventfantomo skuas kaj &gcirc;emigas la fenestrojn, morto<tld/>o
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_zh;&c_i;&c_v;&c_o;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;
  &c_z;&c_v;&c_u;&c_k;</trd>
  <trdgrp lng="en">
    <trd>animal sound</trd>,
    <trd>bleat</trd>, <trd>moo</trd>, <trd>whinny<klr>, etc</klr></trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_k; <klr tip="amb">(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_y;)</klr></trd>,
    <trd>&c_v;&c_o;&c_j;</trd>,
    <trd>&c_r;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bekot</trd>,
    <trd>jek</trd>,
    <trd>jekot</trd>,
    <trd>mekot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bl&ouml;ken</trd>,
    <trd>Heulen</trd>,
    <trd>Kreischen</trd>
  </trdgrp>
  <trd lng="es">grito<klr tip="amb"> (de un animal)</klr></trd>
  <trd lng="fr">cri <klr>(d'un animal)</klr></trd>
  <trdgrp lng="he">
    <trd>&gimel;&ayin;&yod;&he;</trd>,
    <trd>&pe;&ayin;&yod;&he;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9CF4;&#x304D;&#x58F0;  <pr>&#x306A;&#x304D;&#x3054;&#x3048;</pr></trd>,
    <trd>&#x3044;&#x306A;&#x306A;&#x304D;</trd>,
    <trd>&#x308F;&#x3081;&#x304D;&#x58F0;  <pr>&#x308F;&#x3081;&#x304D;&#x3054;&#x3048;</pr></trd>
  </trdgrp>
  <trd lng="pl">odg&lstroke;o&sacute;<klr>
  (zwierz&eogonek;cia)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_i;&c_k;
    <klr>(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_g;&c_o;)</klr></trd>,
    <trd>&c_v;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&lcaron;a&ccaron;anie</trd>,
    <trd>mekot</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_r;&c_i;&c_k;&nbsp;(&c_t;&c_v;&c_a;&c_r;&c_i;&c_n;&c_i;)</trd>
</drv>
</art>

