<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ciril.xml,v 1.19 2023/10/31 13:37:03 revo Exp $">
<kap>
    <rad>Ciril</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
  <drv mrk="ciril.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">NOM</uzo>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
          grekdevena:
          <ekz>
            ruslandanoj kaj anka&ubreve; aliaj &ccirc;iujare gloras la inventintojn
            de la slava, tiel nomata cirila, alfabeto kaj ties
            inventintojn la fratojn <tld/>on kaj Metodion
            <fnt>
              <aut>Aleksandras MASIUKAS</aut>:
              <vrk><url ref="&Monato;/2002/007850.html">Celebradi pri alfabeto</url></vrk>,
              <bib>Monato</bib>, 
              <lok>2002/7, p.7</lok>            
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_K;&c_ib;&c_r;&c_y;&c_l;</trd>
      <trd lng="bg">&c_K;&c_i;&c_r;&c_i;&c_l;</trd>
      <trd lng="ca">Ciril</trd>
      <trdgrp lng="de">
        <trd>Kyrillus</trd>,
        <trd>Cyrill</trd>
      </trdgrp>
      <trd lng="el">&Kappa;&ypsilon_ton;&rho;&jota;&lambda;&lambda;&omikron;&sigma_fina;</trd>
      <trd lng="en">Cyril</trd>
      <trd lng="fr">Cyril</trd>
      <trd lng="hu">Cirill</trd>
      <trd lng="it">Cirillo</trd>
      <trdgrp lng="ja">
    <trd>&#x30AD;&#x30E5;&#x30EA;&#x30ED;&#x30B9;</trd>,
    <trd>&#x8056;&#x30AD;&#x30E5;&#x30EA;&#x30ED;&#x30B9;  <pr>&#x3072;&#x3058;&#x308A;&#x30AD;&#x30E5;&#x30EA;&#x30ED;&#x30B9;</pr></trd>
  </trdgrp>
  <trd lng="nl">Cyrillus</trd>
      <trd lng="pl">Cyryl</trd>
      <trdgrp lng="ro">
        <trd>Cyril</trd>,
        <trd>Chiril</trd>
      </trdgrp>
      <trd lng="ru">&c_K;&c_i;&c_r;&c_i;&c_l;&c_l;</trd>
      <trd lng="sv">Kyrillos</trd>
    <trd lng="uk">&#x41A;&c_i;&c_r;&c_i;&#x43B;&#x43E;</trd>
  <trd lng="zh">&#x897F;&#x88E1;&#x723E; <pr>x&#x12B;l&#x1D0;&#x11B;r</pr></trd>
</drv>

  <drv mrk="ciril.0a">
      <kap><tld lit="c"/>a</kap>
      <snc mrk="ciril.0a.LIN">
      <uzo tip="fak">LIN</uzo>
        <dif>
          Rilata al la <ref cel="slavon.0a">Slavona</ref>
          <ref cel="alfabe.0o">alfabeto</ref> uzata de slavaj ortodoksaj
          eklezioj kaj tradicie atribuata al sankta <tld/>o, a&ubreve;
          al iuj el diversaj naciaj alfabetoj de &gcirc;i devenintaj:
          <ekz>
            <klr>Unikodo</klr> kovras la <tld lit="c"/>an, grekan kaj
            armenan alfabetojn
            <fnt>
              <aut>J.C.Wells</aut>:
              <vrk>Supersignoj, trasignoj kaj subsignoj</vrk>.
              <lok><url ref="http:/ww.uea.org/cgi/m.cgi?m=ghu;d=kongresoj/1996/supersignoj.html">Internacia
              Kongresa Universitato, Prago 1996 07
                  26</url>
              </lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_k;&c_ib;&c_r;&c_y;&c_l;&c_a;&c_w;</trd>,
        <trd>&c_k;&c_ib;&c_r;&c_y;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="bg">&c_k;&c_i;&c_r;&c_i;&c_l;&c_i;&c_c;&c_a;</trd>
      <trd lng="ca">cir&iacute;l&middot;lic</trd>
      <trd lng="de">kyrillisch</trd>
      <trd lng="en">Cyrillic</trd>
      <trd lng="fr">cyrillique<klr> (alphabet)</klr></trd>
      <trd lng="hu">cirill</trd>
      <trd lng="nl">cyrillisch</trd>
      <trd lng="pl">cyrylicki</trd>
      <trd lng="ro">chirilic</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_i;&c_r;&c_i;&c_l;&c_l;&c_o;&c_v;</trd>,
        <trd>&c_k;&c_i;&c_r;&c_i;&c_l;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
      </trdgrp>
  </drv>
</art>

