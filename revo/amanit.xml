<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>amanit</rad>/o
</kap>
<drv mrk="amanit.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="agarik1.0oj">agarikacoj</ref>
      (<trd lng="la">Amanita</trd>), blanksporaj fungoj, el kiuj kelkaj
      estas tre venenaj, ekz. la verda <tld/>o
      (<trd lng="la">Amanita phalloides</trd>), la pantero<tld/>o
      (<trd lng="la">Amanita pantherina</trd>) kaj la
      <ref tip="sub" cel="fung.musx0o">mu&scirc;fungo</ref>
      (<trd lng="la">Amanita muscaria</trd>), aliaj bongustaj
      (<trd lng="la">Amanita rubescens</trd>)
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_h;&c_a;&c_m;&c_o;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Knollenbl&auml;tterpilz</trd>,
    <trd>Pantherpilz</trd>,
    <trd>Fliegenpilz</trd>,
    <trd>Perlpilz</trd>
  </trdgrp>
  <trd lng="fr">amanite</trd>
  <trd lng="hu">gal&oacute;ca</trd>
  <trd lng="nl">amaniet</trd>
  <trd lng="ru">&c_m;&c_u;&c_h;&c_o;&c_m;&c_o;&c_r;</trd>
</drv>
</art>
