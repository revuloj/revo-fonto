<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balkasx.xml,v 1.11 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>Balka&scirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="balkasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="balkasx.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Neprofunda senelflua lago en sudorienta parto de Kaza&hcirc;ujo,
      salakva oriente kaj nesalakva okcidente:
      <ekz>
        provinco Almato <klr>[&#x2026;]</klr> &dash; la lago <tld/>o situas nordokcidente
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=367833">Provinco Almato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_l;&c_h;&c_a;&c_sh;</trd>
  <trd lng="bg">&c_B;&c_a;&c_l;&c_h;&c_a;&c_sh;</trd>
  <trd lng="de">Balkasch-See</trd>
  <trdgrp lng="en">
    <trd>Balkhash</trd>,
    <trd>Balqash</trd>
  </trdgrp>
  <trd lng="fr">Balkhach</trd>
  <trd lng="hu">Balkas-t&oacute;</trd> 
  <trd lng="ja">&#x30D0;&#x30EB;&#x30CF;&#x30B7;&#x6E56;  <pr>&#x30D0;&#x30EB;&#x30CF;&#x30B7;&#x3053;</pr></trd>
  <trd lng="ru">&c_B;&c_a;&c_l;&c_h;&c_a;&c_sh;</trd>
  <trd lng="uk">&#x411;&#x430;&#x43B;&#x445;&#x430;&#x448;</trd>
</drv>
</art>

