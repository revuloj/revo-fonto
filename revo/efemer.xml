<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>efemer</rad>/o
</kap>
<drv mrk="efemer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="efemer.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Ephemera</trd>).
    </dif>
  </snc>
  <trd lng="de">Eintagsfliege</trd>
</drv>
<drv mrk="efemer.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Unutaga, neda&ubreve;ra, rapide pasanta:
      <ekz>
        beleco de rozoj estas <tld/>a;
      </ekz>
      <ekz>
        la proponataj sistemoj estas nur nepripensitaj <tld/>aj provoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verg&auml;nglich</trd>
</drv>
<drv mrk="efemer.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="efemer.0oformaj"><tld/>oformaj
      insektoj</ref> (<trd lng="la">Ephemeridae</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="efemer.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Ephemeroptera</trd>),
      kies anoj vivas en matura stato nur plurajn horojn.
    </dif>
  </snc>
  <trd lng="de">Eintagsfliegen <klr tip="ind">(Ordnung)</klr></trd>
</drv>
</art>
