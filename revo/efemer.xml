<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efemer.xml,v 1.29 2021/03/09 15:43:50 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>efemer</rad>/a
</kap>

<drv mrk="efemer.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Unutaga, neda&ubreve;ra, rapide pasanta:
      <ekz>
        beleco de rozoj estas <tld/>a;
      </ekz>
      <ekz>
        la proponataj sistemoj estas nur nepripensitaj <tld/>aj provoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_f;&c_e;&c_m;&c_e;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_u;&c_t;&c_k;&c_a;&c_b;&c_je;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_ib;&c_n;&c_u;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_je;&c_f;&c_i;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_k;&c_o;&c_t;&c_r;&c_a;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ef&iacute;mer</trd>,
    <trd>fuga&ccedil;</trd>
  </trdgrp>
  <trd lng="de">verg&auml;nglich</trd>
  <trd lng="en">ephemeral</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_w;&a_d;&f_g;&a_dh;&a_r;</trd>,
    <trd>&f_g;&a_dh;&a_r;&a_A;</trd>,
    <trd>&a_n;&a_A;&f_p;&a_A;&f_y;&a_d;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;ph&eacute;m&egrave;re</trd>,
    <trd>fugace</trd>,
    <trd>passager<klr tip="ind">
    (&eacute;ph&eacute;m&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&uacute;l&eacute;kony</trd>,
    <trd>k&eacute;r&eacute;sz&eacute;let&udblac;</trd>
  </trdgrp>
  <trd lng="nl">kortstondig</trd>
  <trdgrp lng="pl">
    <trd>efemeryczny</trd>,
    <trd>przelotny</trd>,
    <trd>ulotny</trd>,
    <trd>znikomy</trd>
  </trdgrp>
  <trd lng="ro">efemer</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_f;&c_je;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_y;&c_s;&c_t;&c_r;&c_o;&c_t;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">kortvarig</trd>
</drv>

<drv mrk="efemer.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="efemer.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <tld/>oformaj <ref tip="lst" cel="insekt.0o" lst="voko:insektoj">insektoj</ref>, kiuj en plenkreska stadio
      ne plu nutras sin, vivantaj nur inter kelkaj minutoj kaj kvar
      tagoj.
    </dif>
  </snc>
  <snc mrk="efemer.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="efemer2.0oj"><tld/>edoj</ref>
      (<trd lng="la">Ephemera</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_w;&c_s;&c_ja;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_e;&c_f;&c_e;&c_m;&c_e;&c_r;&c_y;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_d;&c_n;&c_o;&c_d;&c_n;&c_je;&c_v;&c_k;&c_a;</trd>
  <trd lng="ca">ef&egrave;mera</trd>
  <trd lng="de">Eintagsfliege</trd>
  <trd lng="en">mayfly</trd>
  <trd lng="fr">&eacute;ph&eacute;m&egrave;re<klr tip="ind">
  (insecte)</klr></trd>
  <trdgrp lng="he">
    <trd>&#x5D7;&#x5B2;&#x5DC;&#x5D5;&#x5B9;&#x5E3;</trd>,
    <trd>&#x5D0;&#x5E8;&#x5E2;&#x5D9;&#x5D5;&#x5EA;</trd>
  </trdgrp>
  <trd lng="nl">eendagsvlieg</trd>
  <trd lng="hu">k&eacute;r&eacute;sz</trd>
  <trdgrp lng="pl">
    <trd>j&eogonek;tka</trd>,
    <trd>jednodni&oacute;wka<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Ephemera</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_jo;&c_n;&c_k;&c_a; <klr tip="amb">(&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_o;&c_je;)</klr></trd>
</drv>

<drv mrk="efemer.0oformaj">
  <kap>
    <tld/>oformaj
  </kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Ephemeroptera</trd>),
      kies anoj vivas en matura stato nur plurajn horojn.
    </dif>
  </snc>
  <trd lng="ca">efemer&ograve;pters</trd>
  <trd lng="de">Eintagsfliegen<klr tip="ind"> (Ordnung)</klr></trd>
  <trd lng="nl">eendagsvliegen<klr tip="ind"> (orde)</klr></trd>
  <trd lng="fr">&eacute;ph&eacute;m&eacute;ropt&egrave;res</trd>
  <trd lng="hu">k&eacute;r&eacute;szek<klr tip="ind"> (rend)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_jo;&c_n;&c_k;&c_i; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;
  &c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_h;)</klr></trd>
</drv>
</art>
