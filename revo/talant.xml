<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: talant.xml,v 1.15 2023/09/04 17:49:39 revo Exp $">
<kap>
  <rad>talant</rad>/o
</kap>

<drv mrk="talant.0o">
<kap><tld/>o</kap>
  <snc mrk="talant.0o.pezunuo">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Granda antikva pez<ref tip="lst" cel="mezur.0o" lst="voko:kvantoj">mezuro</ref>:
      <ekz>
        granda hajlo, peza kvaza&ubreve; po <tld/>o, falis el la &ccirc;ielo sur la homojn
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 16:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        aleksandria <tld/>o <klr>(35&#x202F;kg, poste 45&#x202F;kg)</klr>;
      </ekz>
      <ekz>
        atika <tld/>o <klr>(26,2&#x202F;kg)</klr>;
      </ekz>
      <ekz>
        babilona <tld/>o <klr>(30,5&#x202F;kg)</klr>;
      </ekz>
      <ekz>
        biblia <tld/>o <klr>(&ccirc;. 34&#x202F;kg)</klr>
        <fnt><bib>PIV2</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="talant.0o.monunuo">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva grandvalora <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        Atena <tld/>o valore egalis 6000 <ref tip="prt" cel="drahxm.0o">dra&hcirc;mo</ref>jn.
      </ekz>
      <ekz>
        ora, ar&gcirc;enta <tld/>o;
      </ekz>
      <ekz>
        sinjoro, vi komisiis al mi kvin <tld/>ojn; jen mi gajnis
        pluajn kvin <tld/>ojn
        <fnt><bib>NT</bib>, <lok>&Mat; 25:20.</lok></fnt>;
      </ekz>
      <ekz>
        Via sankteco <klr>[&#x2026;]</klr> povas en la da&ubreve;ro de unu jaro akiri pli ol tridek
        mil <tld/>ojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">talent</trd>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="talent.0o.kapablo">talento<sncref/></ref>.
  </rim>
  <trd lng="be">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  <trd lng="bg">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  <trd lng="ca">talent</trd>
  <trdgrp lng="cs">
    <trd>h&rcaron;ivna at&eacute;nsk&aacute;</trd>,
    <trd>talent</trd>
  </trdgrp>
  <trd lng="de">Talent</trd>
  <trd lng="en">talent</trd><trd lng="en">talent</trd>
  <trd lng="es">talento</trd>
  <trd lng="fr">talant</trd>
  <trd lng="hu">talentum</trd>
  <trd lng="ja">&#x30BF;&#x30E9;&#x30F3;&#x30C8;</trd>
  <trd lng="nl">talent</trd>
  <trdgrp lng="pt">
      <trd>talento</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>at&eacute;nska hrivna</trd>,
    <trd>talent</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x430;&#x43B;&#x430;&#x43D;&#x442; (&#x43E;&#x434;&#x438;&#x43D;&#x438;&#x446;&#x44F;)</trd>
</drv>

</art>

