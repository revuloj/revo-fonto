<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anton.xml,v 1.20 2023/08/30 18:23:35 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>Anton</rad>/o
</kap>

<drv mrk="anton.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        el &ccirc;iuj siaj fratoj <tld/>o estas la malplej sa&gcirc;a
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="br">Anton</trd>
  <trd lng="ca">Antoni</trd>
  <trd lng="de">Anton</trd>
  <trd lng="en">Anthony</trd>
  <trd lng="es">Antonio</trd>
  <trd lng="fr">Antoine</trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C8;&#x30F3;</trd>
  <trd lng="nl">Anton</trd>
  <trd lng="pl">Antoni</trd>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="it">Antonio</trd>
  <trd lng="ms">Anthony</trd>
  <trd lng="uk">&c_A;&#x43D;&c_t;&#x43E;&#x43D;</trd>
</drv>

<drv mrk="anton.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Ina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        a&ubreve;skultu, <tld/>ino, diru al mi malka&scirc;e, kial
        vi ne prenas oficon de nutristino?
	<fnt>
          <bib>FK</bib>. <lok>Parto 3a, &#xAB;Anekdotoj&#xBB;</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="antonen.0o">Antoneno</ref>
  <trd lng="be">&c_A;&c_n;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="en">Antonia</trd>
  <trd lng="fr">Antoinette</trd>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_o;&c_n;&c_i;&c_n;&c_a;</trd>
</drv>

</art>

