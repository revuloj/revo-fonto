<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>Anton</rad>/o
</kap>

<drv mrk="anton.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        el &ccirc;iuj siaj fratoj <tld/>o estas la malplej sa&gcirc;a
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="br">Anton</trd>
  <trd lng="ca">Antoni</trd>
  <trd lng="de">Anton</trd>
  <trd lng="es">Antonio</trd>
  <trd lng="fr">Antoine</trd>
  <trd lng="pl">Antoni</trd>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="it">Antonio</trd>
</drv>

<drv mrk="anton.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Ina anta&ubreve;nomo:
      <ekz>
        a&ubreve;skultu, <tld/>ino, diru al mi malka&scirc;e, kial
        vi ne prenas oficon de nutristino?
	<fnt>
          <bib>FK</bib>. <lok>Parto 3a, &#171;Anekdotoj&#187;</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_o;&c_n;&c_i;&c_n;&c_a;</trd>
</drv>

</art>

