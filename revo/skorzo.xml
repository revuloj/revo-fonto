<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skorzo.xml,v 1.17 2023/09/03 18:39:55 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>skorzoner</rad>/o
</kap>
<drv mrk="skorzo.0o">
  <kap><tld/>o</kap>
  <snc mrk="skorzo.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Scorzonera</trd>),
      legomo, kies dika rekta radiko kaj folioj estas man&gcirc;eblaj.
    </dif>
    <refgrp tip="vid">
       <ref cel="cikori.0o">cikorio</ref>
    </refgrp>
  </snc>
  <snc mrk="skorzo.0o.legomo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La man&gcirc;ebla <ref tip="super" cel="radik.0o">radiko</ref> uzata kiel 
      <ref tip="super" cel="legom.0o">legomo</ref>:
      <ekz>
        <tld/>o estas tipa vintra legomo, <klr>[&#x2026;]</klr>
        post la sen&scirc;eligado oni tran&ccirc;as la skorzonerojn en pecetojn kaj kuiras 
        ilin kun iom da vinagro por eviti senkolori&gcirc;on, oni povas eviti
        la gluecan plantolakton per la portado de kuirejaj gantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=140802">Skorzonero</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_a;&c_k;&c_o;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_n; &c_k;&c_o;&c_r;&c_je;&c_n;</trd>
  <trd lng="en">black salsify</trd>
  <trd lng="de">Schwarzwurzel</trd>
  <trdgrp lng="fr">
    <trd>salsifis noir</trd>,
    <trd>scorson&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">pozdor</trd>
  <trd lng="ja">&#x30D5;&#x30BF;&#x30CA;&#x30DF;&#x30BD;&#x30A6;</trd>
  <trd lng="nl">schorseneer</trd>
  <trd lng="pl">w&eogonek;&zdot;ymord</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_z;&c_je;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_c;&c_o;&c_n;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;
&c_k;&c_o;&c_r;&c_je;&c_n;&c_mol;<klr tip="ind">
(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x43E;&#x440;&#x437;&#x43E;&#x43D;&#x435;&#x440;&#x430;</trd>,
    <trd>&#x437;&#x43C;&#x456;&#x44F;&#x447;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
