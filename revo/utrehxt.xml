<art mrk="$Id$">
<kap>
<rad>Utre&hcirc;t</rad>/o
</kap>
<drv mrk="utrehxt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="utrehxt.0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la samnoma nederlanda provinco
      <ref tip="malprt" cel="utrehxt.0o.prov">Utre&hcirc;to</ref> (5&ring;7'E, 52&ring;5'N): <ekz>la ideo tamen jam ekestis je la 12a de septembro 1998 en kafejo en <tld/>o. 
<fnt>
<vrk>Hejmpa&gcirc;o de la Zamenhofa Kolegaro 'Parkero'</vrk> 
</fnt>.
</ekz>
    </dif>
  </snc>
  <snc mrk="utrehxt.0o.prov">
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en 
      <ref tip="malprt" cel="nederl.0">Nederlando</ref> kies &ccirc;efurbo anka&ubreve; nomi&gcirc;as 
      <ref tip="prt" cel="utrehxt.0o.urb">Utre&hcirc;to</ref>.
    </dif>
  </snc>
  <trdgrp lng="lat"><trd>Trajectum ad Rhenam</trd>
     <trd>Trajectum Batavorum</trd></trdgrp>
  <trd lng="nl">Utrecht</trd>
  <trd lng="pt">Utreque</trd>
  </drv>
