<vortaro>
<art mrk="$Id: promen.xml,v 1.39 2024/02/17 07:52:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>promen</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="promen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="promen.0i.KOMUNE">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="ir.0i.deAl">Iri</ref> de loko al loko por plezuro a&ubreve; sano:
      <ekz>
        li <tld/>as kun tri hundoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i du amikoj <tld/>as &ccirc;iam duope
        <fnt>
          <bib>F</bib>
          <lok>&FE; 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        en varmega tago mi amas <tld/>i en arbaro
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ante apud la reloj de fervojo, li falis
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jesuo <tld/>is en la templo
        <fnt>
          <bib>NT</bib>
          <lok>Johano 10:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        vespere <nom>David</nom> levi&gcirc;is de sia ku&scirc;ejo kaj ek<tld/>is sur la tegmento
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 11:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        studento <tld/>adis en la aleo tien kaj reen
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;idino <klr>[&#x2026;]</klr> preter<tld/>is, kaj kiam &scirc;i eka&ubreve;dis la melodion, &scirc;i
        haltis
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is mano en mano
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        blankaj anseroj <tld/>is sur la bordo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[cikonio]</klr> <tld/>adas en la &scirc;limo kaj man&gcirc;as ranojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam kato <tld/>as, la musoj festenas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun urso <tld/>u, sed pafilon prete tenu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi sekretos al edzino, &scirc;i sekretos al fratino, 
        kaj tiel la sekreto <tld/>ados sen fino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>lia cerbo iras <tld/>i</ind>
        <klr>(li estas frenezeta, <ref tip="vid" cel="musx.0o">mu&scirc;o</ref>)</klr>.
        <trdgrp lng="de">
          <trd>bei ihm ist eine <ind>Schraube</ind> locker</trd>,
          <trd> er hat nicht mehr alle <ind>Tasse</ind>n im Schrank</trd>
        </trdgrp>
        <trd lng="hu">elment az esze</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekskur">ekskursi</ref>,
      <ref cel="migr.0i">migri</ref>,
      <ref cel="vag.0i">vagi</ref>,
      <ref cel="vojagx.0i">voja&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF46;&#xF58;&#xF0B;&#xF60;&#xF46;&#xF58;&#xF0B;&#xF60;&#xF42;&#xFB2;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>korzovat</trd>,
    <trd>proch&aacute;zet se</trd>,
    <trd>promenovat</trd>,
    <trd>promenovat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spazieren</trd>,
    <trd>spazieren <ind>gehen</ind></trd>,
    <trd>lustwandeln</trd>
  </trdgrp>
  <trd lng="en">promenade</trd>
  <trdgrp lng="es">
    <trd>pasear</trd>
  </trdgrp>
  <trd lng="fr">se promener</trd>
  <trd lng="hu">s&eacute;t&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x6563;&#x6B69;&#x3059;&#x308B;  <pr>&#x3055;&#x3093;&#x307D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6563;&#x7B56;&#x3059;&#x308B;  <pr>&#x3055;&#x3093;&#x3055;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">wandelen</trd>
  <trd lng="pl">spacerowa&cacute;</trd>
  <trd lng="pt">passear</trd>
  <trd lng="ro">a se plimba</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_g;&c_u;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">prech&aacute;dza&tcaron; sa</trd>
  <trd lng="sv">promenera</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_g;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6563;&#x6B65; <pr>s&#xE0;nb&#xF9;</pr></trd>,
    <trd>&#x6E38;&#x901B; <pr>y&#xF3;ugu&#xE0;ng</pr></trd>,
    <trd>&#x904A;&#x901B; <pr>y&#xF3;ugu&#xE0;ng</pr></trd>,
    <trd>&#x6E9C;&#x6E9C;&#x8FBE;&#x8FBE; <pr>li&#x16B;liud&#x101;d&#x101;</pr></trd>,
    <trd>&#x6E9C;&#x6E9C;&#x9054;&#x9054; <pr>li&#x16B;liud&#x101;d&#x101;</pr></trd>,
    <trd>&#x901B;&#x901B; <pr>gu&#xE0;ngguang</pr></trd>,
    <trd>&#x6E38;&#x73A9; <pr>y&#xF3;uw&#xE1;n</pr></trd>,
    <trd>&#x904A;&#x73A9; <pr>y&#xF3;uw&#xE1;n</pr></trd>,
    <trd>&#x6F2B;&#x6B65; <pr>m&#xE0;nb&#xF9;</pr></trd>,
    <trd>&#x8FF6; <pr>y&#xF2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promen.0ado">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="promen.0ado.IRI">
    <dif>
      Ago <tld/>i; irado pro distri&gcirc;o, edifo a&ubreve; plezuro:
      <ekz>
        en la nokto fari <tld/>on en la libera aero
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari rajdan <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        belega estas la <tld/>ado sur la remparo
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havos <klr>[&#x2026;]</klr> &ccirc;evalojn por vetur<tld/>ado, kiam mi volas
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ado sur Nilo <klr>(per &scirc;ipo)</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_u;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_c;&c_y;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_m;&c_e;&c_n;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_h;&c_o;&c_d;&c_k;&c_a;</trd>
  <trd lng="bo">&#xF46;&#xF58;&#xF0B;&#xF60;&#xF46;&#xF58;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>proch&aacute;zen&iacute; se</trd>,
    <trd>proch&aacute;zka</trd>
  </trdgrp>
  <trd lng="de">Spaziergang</trd>
  <trd lng="es">paseo<klr> (acci&oacute;n)</klr></trd>
  <trd lng="fr">promenade<klr> (action)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6563;&#x6B69;  <pr>&#x3055;&#x3093;&#x307D;</pr></trd>,
    <trd>&#x6563;&#x7B56;  <pr>&#x3055;&#x3093;&#x3055;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">wandeling</trd>
  <trd lng="pl">spacer</trd>
  <trd lng="ro">plimbare</trd>
  <trd lng="sk">prech&aacute;dzka</trd>
  <trd lng="zh">&#x884C;&#x8D70; <pr>x&#xED;ngz&#x1D2;u</pr></trd>
</drv>

<drv mrk="promen.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc>
    <dif>
      Iu, kiu <tld/>as:
      <ekz>
        la <tld/>antoj vidis la vesperan &ccirc;ielon ankora&ubreve; pli bele
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        parko bela kaj zorge konservata, sed sen <tld/>antoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj ektimis: unue estis fulmo sen bruo, poste movi&gcirc;is okcidenten luma
        fenomeno
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010926.php">Spaco por a&jcirc;oj el la spaco</url></vrk>
          <bib>Monato</bib>
          <lok>2013/02, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_c;&c_y;&c_r;&c_a;&c_n;&c_t;</trd>
  <trd lng="de">Spazierg&auml;nger</trd>
  <trdgrp lng="ja">
    <trd>&#x6563;&#x6B69;&#x3059;&#x308B;&#x4EBA;  <pr>&#x3055;&#x3093;&#x307D;&#x3059;&#x308B;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x6563;&#x7B56;&#x8005;  <pr>&#x3055;&#x3093;&#x3055;&#x304F;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">wandelaar</trd>
  <trdgrp lng="zh">
    <trd>&#x6563;&#x6B65;&#x7684;&#x4EBA; <pr>s&#xE0;nb&#xF9;der&#xE9;n</pr></trd>,
    <trd>&#x6F2B;&#x6B65;&#x8005; <pr>m&#xE1;nb&#xF9;zh&#x11B;</pr></trd>,
    <trd>&#x52A9;&#x884C;&#x5668; <pr>zh&#xF9;x&#xED;ngq&#xEC;</pr></trd>
  </trdgrp>
</drv>


<drv mrk="promen.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="promen.0ejo.KOMUNE">
    <dif>
      <ref tip="super" cel="lok.0o.spaca">Loko</ref> oportuna a&ubreve; aran&gcirc;ita por <tld/>ado,
      en kiu homoj kutime <tld/>as:
      <ekz>
        &scirc;i estis tre scivola, kaj tial &scirc;i sur la <tld/>ejo tuj komencis paroladon
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        longa kaj belega <tld/>ejo la&ubreve;longe de la marbordo
        <fnt>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2004/008097.html">Adriatika marbordo</url></vrk>
          <bib>Monato</bib>
          <lok>2004/01, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo &ccirc;e Bistrico svarmas pro gejunuloj &ccirc;iun vesperon
        <fnt><aut>Bardhyl Selimi</aut>: 
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kors.0o.TRA">korso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_e;&c_n;&c_a;&c_d; <klr>(&c_m;&c_je;&c_s;&c_c;&c_a; &c_d;&c_l;&c_ja; &c_sh;&c_p;&c_a;&c_c;&c_y;&c_r;&c_u;)</klr></trd>
  <trd lng="de">Promenade</trd>
  <trd lng="es">paseo<klr> (lugar)</klr></trd>
  <trd lng="fr">promenade<klr> (lieu)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6563;&#x6B69;&#x9053;  <pr>&#x3055;&#x3093;&#x307D;&#x307F;&#x3061;</pr></trd>,
    <trd>&#x904A;&#x6B69;&#x9053;  <pr>&#x3086;&#x3046;&#x307B;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x904A;&#x6B69;&#x5834;  <pr>&#x3042;&#x305D;&#x3075;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x30D7;&#x30ED;&#x30E0;&#x30CA;&#x30FC;&#x30C9;</trd>
  </trdgrp>
  <trd lng="nl">wandelweg</trd>
  <trd lng="pl">promenada</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_ib;&c_s;&c_c;&c_je; &c_d;&#x43B;&#x44F; &#x43F;&c_r;&#x43E;&c_g;&c_u;&#x43B;&#x44F;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_f;&#x43E;&c_j;&c_jeu; (&c_u; &c_t;&c_je;&c_a;&c_t;&c_r;&c_ib;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x677F;&#x8DEF; <pr>m&#xF9;b&#x1CE;nl&#xF9;</pr></trd>,
    <trd>&#x8D70;&#x9053; <pr>z&#x1D2;ud&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promen.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="promen.0igi.KOMUNE">
    <dif>
      Irigi kun si <tld/>e:
      <ekz>
        <tld/>igi amikon, hundon;
      </ekz>
      <ekz>
        <klr>[li]</klr> ek<tld/>igas &scirc;in el inter la popolamaso
        <fnt>
          <aut>G. O. &Ograve;sh&oacute;-Davies</aut>
          <vrk><url ref="&Monato;/2012/010385.php">La perfekta edzino</url></vrk>
          <bib>Monato</bib>
          <lok>2012/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        bonsajon oni povus kompari kun dorlotbesto, tamen &gcirc;in oni ne bezonas <tld/>igi
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2007/009228.php">Kultivi bonsajon</url></vrk>
          <bib>Monato</bib>
          <lok>2007/08, p. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili decidas procesie <tld/>igi la korpajn postresta&jcirc;ojn de sankta <nom>Waudru</nom>
        <fnt>
          <aut>Dominique Vallet</aut>
          <vrk><url ref="&Monato;/2007/008996.php">Impona popolfesto en Monso</url></vrk>
          <bib>Monato</bib>
          <lok>2007/05, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ausf&uuml;hren</trd>,
    <trd>spazieren <ind>f&uuml;hren</ind></trd>
  </trdgrp>
  <trd lng="hu">s&eacute;t&aacute;ltat</trd>
  <trd lng="ja">&#x6563;&#x6B69;&#x3055;&#x305B;&#x308B;  <pr>&#x3055;&#x3093;&#x307D;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">uitlaten</trd>
  <trd lng="es">pasear<klr> (a alguien o algo)</klr></trd>
  <trd lng="fr">promener</trd>
  <trd lng="pl">wyprowadza&cacute; na spacer <klr>(np. psa)</klr></trd>
  <trd lng="ru">&c_v;&c_y;&c_g;&c_u;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(pp hundo)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_g;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_g;&c_u;&#x43B;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
