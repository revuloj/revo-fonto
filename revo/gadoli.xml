  <kap><rad>gadolini</rad>/o</kap>
<drv mrk="gadoli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Gd</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="64">elemento</ref> kun
      atomnumero 64, atompezo 157,25; 
      malofta ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>, lantanoido:
      <ekz>
        <tld/>o estas ar&gcirc;ent-blanka, martelebla kaj dratigebla lantanoida metalo,
        malsama al <klr>[multaj aliaj]</klr> lantanoidoj, <tld/>o estas iomete stabila &ccirc;e doma
        temperaturo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28689">Gadolinio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_d;&c_a;&c_l;&c_ib;&c_n;&c_ib;&c_j;</trd>
