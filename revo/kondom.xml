<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kondom</rad>/o
</kap>
<drv mrk="kondom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ka&ubreve;&ccirc;uka protektilo de la <ref tip="vid" cel="penis.0o">peniso</ref>,
      uzata dum <ref tip="vid" cel="koit.0o">koito</ref> por preventi
      infekti&gcirc;on a&ubreve; gravedi&gcirc;on.
    </dif>
  </snc>
    <trdgrp lng="de">
     <trd>Kondom</trd>,
     <trd>Pariser</trd>
    </trdgrp>
  <trd lng="en">condom</trd>
  <trd lng="nl">kondoom</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_d;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_a;&c_t;&c_i;&c_v;</trd>
  </trdgrp>
</drv>
</art>

