<drv mrk="kondom.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Fleksebla protektilo de la <ref tip="vid" cel="penis.0o">peniso</ref>, uzata dum <ref tip="vid" cel="kun.seks0igxo">sekskuni&gcirc;o</ref> por preventi infekti&gcirc;on a&ubreve;
      gravedi&gcirc;on:
      <ekz>
        multjara uzado de <tld/>oj fare de la prostituitinoj, kondukis
        al redukti&gcirc;o de aidoso
        <fnt>
          <aut>Vladimir Lemelev</aut>: <vrk>Grava bazo por preventi
          aidoson</vrk>,
          <bib>Monato</bib>
          <lok>jaro 2001a, numero 5a, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="koncip.kontraux0ilo">kontra&ubreve;koncipilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_d;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_e;&c_z;&c_e;&c_r;&c_v;&c_a;&c_t;&c_y;&c_w;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xFB3;&#xF72;&#xF42;&#xF0B;&#xF64;&#xF74;&#xF56;&#xF66;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>preservatiu</trd>,
    <trd>condom</trd>,
    <trd>cond&oacute;</trd>,
    <trd>profil&agrave;ctic</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kondom</trd>,
    <trd>Pariser</trd>
  </trdgrp>
  <trd lng="en">condom</trd>
  <trdgrp lng="es">
    <trd>preservativo</trd>,
    <trd>cond&oacute;n</trd>,
    <trd>profil&aacute;ctico</trd>,
    <trd>goma<klr> (coloquial)</klr></trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_A;&a_n;&a_d;&a_w;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;servatif</trd>,
    <trd>capote<klr tip="ind"> (pr&eacute;servatif)</klr></trd>,
    <trd>capote anglaise</trd>,
    <trd>condom</trd>
  </trdgrp>
  <trd lng="fy">kondoom</trd>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30C9;&#x30FC;&#x30E0;</trd>
  <trd lng="nl">condoom</trd>
  <trdgrp lng="pl">
    <trd>kondom</trd>,
    <trd>prezerwatywa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>preservativo</trd>,
    <trd>camisa-de-v&ecirc;nus</trd>,
    <trd>camisinha</trd>,
    <trd>preventivo</trd>,        
    <trd>condom</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>prezervativ</trd>,
    <trd>condom</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_d;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_a;&c_t;&c_i;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5B89;&#x5168;&#x5957; <pr>&#x101;nqu&#xE1;nt&#xE0;o</pr></trd>,
    <trd>&#x907F;&#x5B55;&#x5957; <pr>b&#xEC;y&#xF9;nt&#xE0;o</pr></trd>,
    <trd>&#x4FDD;&#x9669;&#x5957; <pr>b&#x1CE;oxi&#x1CE;nt&#xE0;o</pr></trd>,
    <trd>&#x536B;&#x751F;&#x5957; <pr>w&#xE8;ish&#x113;ngt&#xE0;o</pr></trd>,
    <trd>&#x5982;&#x610F;&#x5957; <pr>r&#xFA;y&#xEC;t&#xE0;o</pr></trd>,
    <trd>&#x9634;&#x830E;&#x5957; <pr>y&#x12B;nj&#x12B;ngt&#xE0;o</pr></trd>
