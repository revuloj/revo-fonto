<art mrk="$Id: meteorolog.xml,v 1.12 2024/01/25 20:10:36 revo Exp $">
<kap>
  <ofc>7</ofc>
  <rad>meteorolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="meteorolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="meteorolog.0o.SCI">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="dif" cel="meteor2.0isto">Meteorologiisto</ref>:
      <ekz>
        <nom>Vilh&ouml;</nom>, <nom>Yrj&ouml;</nom> kaj <nom>Kalle V&auml;is&auml;l&auml;</nom> estis tri fratoj, respektive
        <tld/>o, astronomo kaj matematikisto
        <fnt>
        <aut>A. Lagrange</aut>,
        <vrk><url ref="http://www.ipernity.com/blog/arno.lagrange/archive/2009/01/22">
        La fratoj V&auml;is&auml;l&auml;</url></vrk>,
        <lok>2009-01-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la germana <tld/>o <nom>Wladimir Peter K&ouml;ppen</nom> malvarmaj someroj pli gravas ol
        malvarmegaj vintroj por glacikreskado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51530">Glaciepoko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Meteorologe</trd>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">meteor&ograve;leg</trd>
  <trd lng="en">meteorologist</trd>
  <trd lng="fr">m&eacute;t&eacute;orologue</trd>
  <trd lng="ja">&#x6C17;&#x8C61;&#x5B66;&#x8005;  <pr>&#x304D;&#x3057;&#x3087;&#x3046;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">meteoroloog</trd>
  <trd lng="no">meteorolog</trd>
  <trd lng="pl">meteorolog</trd>
  <trd lng="uk">&#x43C;&c_je;&c_t;&c_je;&#x43E;&c_r;&#x43E;&#x43B;&#x43E;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C14;&#x50CF;&#x4EBA;&#x5458; <pr>q&#xEC;xi&#xE0;ngr&#xE9;nyu&#xE1;n</pr></trd>,
    <trd>&#x6C23;&#x50CF;&#x4EBA;&#x54E1; <pr>q&#xEC;xi&#xE0;ngr&#xE9;nyu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

