<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>legom</rad>/o
</kap>
<drv mrk="legom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu &gcirc;ardena man&gcirc;ebla vegeta&jcirc;o, escepte de fruktoj (brasiko, laktuko,
      karoto, pizo, ktp):
      <ekz>
        japanoj montras al &ccirc;inoj kiel kultivi bonkvalitajn <tld/>ojn
        ta&ubreve;gajn por japana merkato kaj rezulte grandkvante importas el
        &Ccirc;inio<fnt><aut>Hori Yasuo</aut>: <vrk>Malkaraj kostoj, malkaraj
        homoj</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 12a, p.
        10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gem&uuml;se</trd>
  <trd lng="en">vegetable</trd>
  <trd lng="es">legumbre</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gume</trd>
  </trdgrp>
  <trd lng="nl">groente</trd>
  <trd lng="pt">legume</trd>
  <trd lng="ru">&c_o;&c_v;&c_o;&c_shch;</trd>
</drv>

<drv mrk="legom.0ekologia">
  <kap>ekologia <tld/>o</kap>
  <snc>
    <dif>
      <tld lit="L"/>o kultivita sen uzo de kemia sterko a&ubreve; insekticidoj.
    </dif>
  </snc>
  <trd lng="en">organic vegetable</trd>
  <trd lng="nl">biologische groente</trd>
</drv>
</art>
