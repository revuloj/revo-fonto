<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>domen</rad>/o
</kap>
<drv mrk="domen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="domen.0o.TEKS">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Speco de vasta <ref tip="super" cel="mantel.0o">mantelo</ref> kun
      kapu&ccirc;o, uzata precipe en maskobaloj:
      <ekz>
        mi <klr>...</klr> ekvidis gracian virinan figuron en nigra
        <tld/>o
        kun nigra masko anta&ubreve; la viza&gcirc;o
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da
        glacia&jcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Domino</trd>,
      <trd>Maskenmantel</trd>,
      <trd>Maskenkost&uuml;m</trd>
    </trdgrp>
    <trd lng="fr">domino<klr tip="ind"> (v&ecirc;tement)</klr></trd>
    <trd lng="hu">domin&oacute;<klr tip="ind"> (ruha)</klr></trd>   
    <trd lng="pt">domin&oacute;</trd>
    <trd lng="sv">domino<klr tip="ind"> (cape med
    kapuschong)</klr></trd>
  </snc>
  <snc mrk="domen.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Surtabla ludo konsistanta el 28 tabuletoj, kiujn la ludantoj devas
      kunigi la&ubreve; la nombro de punktoj skribitaj sur ili.
    </dif>
    <trdgrp lng="de">
      <trd>Domino</trd>,
      <trd>Dominospiel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>domino</trd>,
      <trd>dominoes</trd>
    </trdgrp>
    <trd lng="es">domin&oacute;</trd>
    <trd lng="fr">domino<klr tip="ind"> (jeu)</klr></trd>
    <trd lng="hu">domin&oacute;<klr tip="ind">
    (j&aacute;t&eacute;k)</klr></trd>    
    <trd lng="pl">domino</trd>
    <trd lng="pt">domin&oacute;</trd>
    <trd lng="sv">domino<klr tip="ind"> (spel)"</klr></trd>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_m;&c_ib;&c_n;&c_o;</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_i;&c_n;&c_o;</trd>
  
</drv>
</art>
