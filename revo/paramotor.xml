<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>paramotor</rad>/o
</kap>
<drv mrk="paramotor.0o">
  <kap><tld/>o</kap>
  <snc mrk="paramotor.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Flugaparato simila al
      <ref tip="vid" cel="parasx.glis0o.SPO">glispara&scirc;uto</ref>
      sed provizita je motoro kaj pel-helico dorse de la sidanta
      kondukanto:
      <ekz>
        mi pilotas <tld/>on <klr>...</klr>
        kaj superflugis la bardenasan dezerton
        <fnt>
          <aut>B. Jouault</aut>,
          <vrk><url ref="http://www.ipernity.com/user/43962">
          Pri Raneto</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>...</klr> vaste populari&gcirc;as
        en tuta mondo depost <klr>la</klr> 1980-aj jaroj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Paramotoro">
          paramotoro</url>, 2011-06-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">paramotor</trd>
  <trd lng="fr">paramoteur</trd>
</drv>
</art>

