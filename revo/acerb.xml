<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>acerb</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="acerb.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="acerb.0a.sento">
    <dif>
      <ref tip="super" cel="acid.0a.akra">Acideta</ref> simile al nematura
      frukto:
      <ekz>
        &scirc;i sentis nokte, ke la kapo pezas, okuloj <tld/>as,
        nazo &scirc;topi&gcirc;as
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        391a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">&acirc;pre<klr> (gout)</klr></trd>
  </snc>
  <snc mrk="acerb.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Incita, moka, pika.
    </dif>
  </snc>
  <trd lng="fr">acerbe</trd>
</drv>
</art>

