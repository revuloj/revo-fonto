<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: acerb.xml,v 1.12 2023/03/24 12:44:40 revo Exp $">
<kap>
  <rad>acerb</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="acerb.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="acerb.0a.sento">
    <dif>
      <ref tip="super" cel="acid.0a.akra">Acideta</ref> simile al
      nematura
      frukto:
      <ekz>
        &scirc;i sentis nokte, ke la kapo pezas, okuloj <tld/>as,
        nazo &scirc;topi&gcirc;as
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p..
        391a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_w;&c_k;&c_ib;</trd>,
      <trd>&c_a;&c_s;&c_k;&c_o;&c_m;&c_ib;&c_s;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>astringent</trd>,
      <trd>biting</trd>, <trd>sour</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&acirc;cre<klr tip="ind"> (go&ucirc;t)</klr></trd>,
      <trd>&acirc;pre<klr tip="ind"> (go&ucirc;t)</klr></trd>,
      <trd>aigre</trd>
    </trdgrp>
    <trd lng="it">acerbo</trd>
  </snc>
  <snc mrk="acerb.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ofende incita, moka, pika.
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_je;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>
    <trdgrp lng="en">
      <trd>ascerbic</trd>,
      <trd>sardonic</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>acerbe</trd>,
      <trd>ac&eacute;r&eacute;<klr tip="ind"> (fig.)</klr></trd>,
      <trd>aigre<klr tip="ind"> (fig.)</klr></trd>,
      <trd>&acirc;pre<klr tip="ind"> (fig.)</klr></trd>,
      <trd>caustique<klr tip="ind"> (fig.)</klr></trd>,
      <trd>cinglant<klr tip="ind"> (fig.)</klr></trd>,
      <trd>corrosif<klr tip="ind"> (fig.)</klr></trd>,
      <trd>dur<klr tip="ind"> (fig.)</klr></trd>,
      <trd>mordant<klr tip="ind"> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fanyar</trd>,
      <trd>cs&iacute;p&odblac;s</trd>
    </trdgrp>
    <trd lng="it">immaturo</trd>
    <trd lng="pl">cierpki</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>acerb</trd>,
    <trd>agrenc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>j&iacute;zliv&yacute;</trd>,
    <trd>sv&iacute;rav&yacute;</trd>,
    <trd>trpk&yacute;</trd>,
    <trd>&scaron;kodolib&yacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5E8;&#x5D9;&#x5E8;</trd>
  <trd lng="pl">cierpki</trd>
  <trd lng="ro">acid</trd>
  <trdgrp lng="sk">
    <trd>hork&yacute;</trd>,
    <trd>trpk&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&c_r;&#x43F;&#x43A;&c_i;&c_j;</trd>
</drv>
</art>

