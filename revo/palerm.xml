  <art mrk="$Id: palerm.xml,v 1.4 2022/05/01 17:51:18 revo Exp $">
    <kap>
      <rad>Palerm</rad>/o
    </kap>

<drv mrk="palerm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="palerm.0o.urbo">
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      &ccirc;e la norda bordo de <ref tip="malprt" cel="sicili.0o">Sicilio</ref>:
      <ekz>
        kiu pasigas unu tagon en la centro de Milano, <tld/>o a&ubreve; Triesto, inhalas tiom
        da veneno, kiom egalas al fumado de 15 cigaredoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008474.html">Koincido</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni surprizi&gcirc;os pro la beleco de la subtera&jcirc;oj de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011265.php">En Palermo subtere</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Palermo</trd>
    <trd lng="en">Palermo</trd>
    <trd lng="fr">Palerme</trd>
    <trd lng="pl">Palermo</trd>
    <trd lng="ro">Palermo</trd>
  </snc>
  <trd lng="uk">&#x41F;&#x430;&#x43B;&#x435;&#x440;&#x43C;&#x43E;</trd>
</drv>

