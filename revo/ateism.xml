<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>ateism</rad>/o
</kap>
<drv mrk="ateism.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, kiu neas la ekziston de dioj a&ubreve; Dio.
    </dif>
  </snc>
  <trd lng="ca">ateisme</trd>
  <trd lng="es">ateismo</trd>
  <trd lng="fr">ath&eacute;isme</trd>
  <trd lng="hu">ateizmus</trd>
  <trd lng="ru">&c_a;&c_t;&c_je;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">ateizmus</trd>
  <trd lng="sv">ateism</trd>
</drv>

<drv mrk="ateism.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Ano de <tld/>o.
    </dif>
    <ref tip="sin" cel="ateist.0o">ateisto.</ref>
  </snc>
  <trd lng="ca">ateu</trd>
  <trd lng="es">ateo</trd>
  <trd lng="hu">ateista</trd>
  <trd lng="ru">&c_a;&c_t;&c_je;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">ateista</trd>
  <trd lng="sv">ateist</trd>
</drv>
</art>
