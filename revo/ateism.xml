<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>ateism</rad>/o
</kap>
<drv mrk="ateism.0o">
  <kap><tld/>o</kap>
  <snc mrk="ateism.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Doktrino, kiu neas la ekziston de dioj a&ubreve; Dio:
      <ekz>
        &ccirc;i tie ludas rolon anka&ubreve; la &scirc;tata <tld/>o, altrudata al la popolo
        dum pli ol 50 jaroj<fnt><aut>Volodimir Hurtovenko</aut>, <vrk>Kial
        la rusa ortodoksa eklezio malamas la papon?</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 2a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_e;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">ateisme</trd>
  <trd lng="es">ateismo</trd>
  <trd lng="fr">ath&eacute;isme</trd>
  <trd lng="hu">ateizmus</trd>
  <trd lng="ru">&c_a;&c_t;&c_je;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">ateizmus</trd>
  <trd lng="sv">ateism</trd>
  <trdgrp lng="zh">
    <trd>&#26080;&#31070;&#35770;</trd>,
    <trd><klr>tr. </klr>&#28961;&#31070;&#35542;<klr> [w&uacute;sh&eacute;nl&ugrave;n]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="ateism.0a">
  <kap><tld/>a</kap>
  <snc mrk="ateism.0a.FIL">
    <dif>
      Sendiisma:
      <ekz>
        &scirc;i devenas el <tld/>a medio kaj ankora&ubreve; neniam serioze okupi&gcirc;is
        pri la kristana kredo<fnt><aut>Ulrich Matthias</aut>,
        <vrk>Profanaj plezuroj</vrk>, <bib>Monato</bib>, <lok>jaro 1996a,
        numero 3a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_e;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>ath&eacute;e<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_t;&c_je;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="ateism.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="ateism.0ano.FIL">
    <dif>
      Ano de <tld/>o.
    </dif>
    <ref tip="sin" cel="ateist.0o">ateisto</ref>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_e;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">ateu</trd>
  <trdgrp lng="de">
    <trd>Atheist</trd>,
    <trd>Gottloser</trd>
  </trdgrp>
  <trd lng="es">ateo</trd>
  <trdgrp lng="fr">
    <trd>ath&eacute;e<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ateista</trd>,
    <trd>istentagad&oacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_t;&c_je;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>ateista</trd>,
    <trd>neznaboh</trd>
  </trdgrp>
  <trd lng="sv">ateist</trd>
</drv>
</art>
