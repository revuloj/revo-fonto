<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rotaci.xml,v 1.41 2024/04/14 05:47:07 revo Exp $">
<kap>
  <ofc>9</ofc><rad>rotaci</rad>/i
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rotaci.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Turni&gcirc;i &ccirc;irka&ubreve; interna <ref cel="aks.0o">akso</ref>:
      <ekz>
        turboj staras kaj tamen movi&gcirc;as kiam ili <tld/>as dum la pintoj restas fiksitaj
        &ccirc;e unu punkto
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>;
      </ekz>
      <ekz>
        tamburo <tld/>is rapidege
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ra&ubreve;blinio <tld/>is jen supren, jen malsupren
        <fnt>
          <bib>VaK</bib>
          <lok>Dekoka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        impulsa sudlandano <tld/>as en la lito dum la tuta nokto kaj sekve la lina&jcirc;oj
        kaj lana&jcirc;oj parte kuntordi&gcirc;as la&ubreve; tute komplika maniero
        <fnt>
          <bib>KKr</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        moveblaj motoroj povas <tld/>i je 360 gradoj kaj tiel la &scirc;ipo povas surloke
        turni&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Christian BERTIN/pg</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rivolu.0i">rivolui</ref>,
      <ref cel="ir.rond0i">rondiri</ref>,
      <ref cel="rul.0igxi">ruli&gcirc;i</ref>,
      <ref cel="turn.0igxi">turni&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_u;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_c;&c_je;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_a;&c_r;&c_a;&c_ch;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_a; &c_v;&c_o;&c_s;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">rotieren</trd>
  <trd lng="en">rotate</trd>
  <trd lng="es">rotar</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>rotation</ind></trd>,
    <trd>tourner<klr tip="ind"> (sur soi-m&ecirc;me)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>forog</trd>,
    <trd>p&ouml;r&ouml;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x56DE;&#x8EE2;&#x3059;&#x308B;  <pr>&#x304B;&#x3044;&#x3066;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x81EA;&#x8EE2;&#x3059;&#x308B;  <pr>&#x3058;&#x3066;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>roteren</trd>,
    <trd>draaien</trd>
  </trdgrp>
  <trd lng="pl">obraca&cacute; si&eogonek;<klr>  (dooko&lstroke;a osi)</klr></trd>
  <trdgrp lng="pt">
    <trd>rodar</trd>,
    <trd>rotar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_je;&c_r;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_t;&c_ib;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x8F49; <pr>d&#x1CE;zhu&#xE0;n</pr></trd>,
    <trd>&#x6253;&#x8F6C; <pr>d&#x1CE;zhu&#xE0;n</pr></trd>,
    <trd>&#x626D; <pr>ni&#x1D4;</pr></trd>,
    <trd>&#x8F49;&#x5F4E; <pr>zhu&#x1CE;nw&#x101;n</pr></trd>,
    <trd>&#x8F6C;&#x5F2F; <pr>zhu&#x1CE;nw&#x101;n</pr></trd>,
    <trd>&#x56DE;&#x8F6C; <pr>hu&#xED;zhu&#x1CE;n</pr></trd>,
    <trd>&#x8FF4;&#x8F49; <pr>hu&#xED;zhu&#x1CE;n</pr></trd>,
    <trd>&#x5FAA;&#x73AF; <pr>x&#xFA;nhu&#xE1;n</pr></trd>,
    <trd>&#x5FAA;&#x74B0; <pr>x&#xFA;nhu&#xE1;n</pr></trd>,
    <trd>&#x8F2A; <pr>l&#xFA;n</pr></trd>,
    <trd>&#x8F6E; <pr>l&#xFA;n</pr></trd>,
    <trd>&#x96DF; <pr>su&#xED;</pr></trd>,
    <trd>&#x8F2A;&#x8F49; <pr>l&#xFA;nzhu&#xE0;n</pr></trd>,
    <trd>&#x8F6E;&#x8F6C; <pr>l&#xFA;nzhu&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rotaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="rotaci.0o.TEKAST">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">AST</uzo>
    <dif>
      Ago <tld/>i; movo de io, kio <tld/>as:
      <ekz>
        la <tld/>o de la Tero estas ne tute konstanta
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007024.php">Pli ekzakte ne eblas: Radi-regata
            horlo&gcirc;o</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 13</lok>
          </fnt>;
      </ekz>
      <ekz>
        la pilko do kutime faras proksimume 20 <tld/>ojn dum sia vojo
        <fnt>
          <bib>Monato</bib>
          <aut>Vus-Tit Zik</aut>
          <vrk><url ref="&Monato;/2009/009769.php">Iom da historio kaj iom da fiziko</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>rapido</ind>.
        <trdgrp lng="de">
         <trd>Drehgeschwindigkeit</trd>,
         <trd>Umlaufgeschwindigkeit</trd>,
         <trd>Drehzahl</trd>
        </trdgrp>
        <trd lng="fr">vitesse de rotation</trd>
        <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_o;&c_s;&c_t;&c_mol;
        &c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_ja;</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_v;&c_ja;&c_r;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_v;&c_a;&c_r;&c_o;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Rotation</trd>
    <trd lng="hu">forg&aacute;s</trd>
    <trd lng="ru">&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
    <trdgrp lng="zh">
    <trd>&#x81EA;&#x8F49; <pr>z&#xEC;zhu&#xE0;n</pr></trd>,
    <trd>&#x81EA;&#x8F6C; <pr>z&#xEC;zhu&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="rotaci.0o.sinsekvo">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Regula &scirc;an&gcirc;o, sinsekvo:
      <ekz>
        la novico okupas &ccirc;iujn postenojn en <tld/>o;
      </ekz>
      <ekz>
        politika <tld/>o: jen la rezulto de &gcirc;eneralaj balotoj okazintaj en Albanio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2005/008751.php">Post ok jaroj: &scirc;an&gcirc;o</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">AGR</uzo>
        <ind><tld/>o de kulturoj <klr>(por eviti el&ccirc;erpi la
        grundon)</klr></ind>.
        <trd lng="de">Fruchtfolge</trd>
        <trd lng="en">crop rotation</trd>
        <trd lng="hu">vet&eacute;sforg&oacute;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="hu">rot&aacute;ci&oacute;</trd>
    <trd lng="ru">&c_r;&c_o;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
    <adm>
      Mi enkondukis, &ccirc;i tiun PIVan sencon, sed ne certas, &ccirc;u
      &gcirc;i estas internacie uzata. Se tiel ne estas, indus
      precizigi EVI kaj proponi alternativon. <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="rotaci.0o.kinematiko">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(kinematiko)</uzo>
    <dif>
      <klr>(de <ref cel="solid.0o.kinematiko">solido</ref>)</klr>
      Tia movo, ke almena&ubreve; du punktoj de la solido restas
      senmovaj:
      <ekz>
        &ccirc;iuj punktoj senmovaj dum <tld/>o apartenas al unu sama
        rekto <klr>(la akso de la <tld/>o)</klr>;
      </ekz>
      <ekz>
        dum <tld/>o la trajektorio de &ccirc;iu punkto ekster la akso
        estas cirklo a&ubreve; parto de cirklo.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_ja;&c_r;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="hu">k&ouml;rforg&aacute;s</trd>
    <trd lng="ru">&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="rotaci.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="rotaci.0o.vektora">
      <fnt><bib>EKV</bib><lok>&para;383</lok></fnt>
      <dif>
        <klr>(en <ref cel="dimens.n0a">finidimensia</ref>
        <ref cel="spac.euxklida0o">e&ubreve;klida vektora
        spaco</ref>)</klr>
        <ref cel="izomet.0a.bildigo">Izometria</ref>
        <ref cel="auxtom2.0o">a&ubreve;tomorfio</ref> kun
        <ref cel="determ.0anto.endomorfio">determinanto</ref>
        egala al <frm>1</frm>.
      </dif>
      <ref tip="sin" cel="rotaci.vektora0o">vektora <tld/>o.</ref>
    </subsnc>
    <subsnc mrk="rotaci.0o.afina">
      <fnt><bib>PIV2</bib></fnt>
      <dif>
        <klr>(en <ref cel="dimens.n0a">finidimensia</ref>
        <ref cel="spac.euxklida0o">e&ubreve;klida afina
        spaco</ref>)</klr>
        <ref cel="afin.0a.bildigo">Afina</ref>
        <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>, asociita al
        <ref cel="rotaci.0o.vektora"><tld/>o
        <sncref ref="rotaci.0o.vektora"/></ref>:
        <ekz>
          en ebeno, <tld/>o estas
          <ref cel="lig.kun0ajxo.rilatoj">kunliga&jcirc;o</ref> de
          du <ref cel="simetr.0o.orta">ortaj simetrioj</ref> rilate al
          rektoj, kies intersekco estas la
          <ref cel="centr.0o.rotacio">centro</ref> de la <tld/>o;
        </ekz>
        <ekz>
          se <frm am="O">O</frm> estas la centro de <tld/>o kaj
          <frm am="M&minute;">M'</frm> estas
          la bildo per &gcirc;i de punkto <frm am="M">M</frm>, la mezuro de la
          <ref cel="angul.0o.afina">angulo</ref> randata de duonrektoj
          <frm am="OM">OM</frm> kaj <frm am="OM&minute;">OM'</frm>
          ne dependas de <frm am="M">M</frm>
          kaj estas nomata la angulo de la <tld/>o;
        </ekz>
        <ekz>
          en tridimensia spaco, <tld/>o estas kunliga&jcirc;o de du
          ortaj
          simetrioj rilate al ebenoj, kies intersekco estas
          la <ref cel="aks.0o.rotacio">akso</ref> de la <tld/>o.
        </ekz>
      </dif>
      <ref tip="sin" cel="rotaci.afina0o">afina <tld/>o.</ref>
    </subsnc>
    <trd lng="be">&c_v;&c_ja;&c_r;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Drehung</trd>,
      <trd>Rotation</trd>
    </trdgrp>
    <trd lng="en">rotation</trd>
    <trd lng="fr">rotation</trd>
    <trd lng="pl">obr&oacute;t</trd>
    <trd lng="ru">&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
    <trdgrp lng="zh">
    <trd>&#x81EA;&#x8F49; <pr>z&#xEC;zhu&#xE0;n</pr></trd>,
    <trd>&#x81EA;&#x8F6C; <pr>z&#xEC;zhu&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>krou&zcaron;en&iacute;</trd>,
    <trd>ob&iacute;h&aacute;n&iacute;</trd>,
    <trd>ob&ecaron;h</trd>,
    <trd>ot&aacute;&ccaron;en&iacute;</trd>,
    <trd>rotace</trd>,
    <trd>st&aacute;&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>giration<klr tip="ind"> (mouvement de rotation)</klr></trd>,
    <trd>rotation<klr tip="ind"> (mouvement, alternance,
    math.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x56DE;&#x8EE2;&#x904B;&#x52D5;  <pr>&#x304B;&#x3044;&#x3066;&#x3093;&#x3046;&#x3093;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x81EA;&#x8EE2;  <pr>&#x3058;&#x3066;&#x3093;</pr></trd>,
    <trd>&#x56DE;&#x8EE2;  <pr>&#x304B;&#x3044;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">rotatie</trd>
  <trdgrp lng="pl">
    <trd>rotacja</trd>,
    <trd>obr&oacute;t</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obeh</trd>,
    <trd>obiehanie</trd>,
    <trd>ot&aacute;&ccaron;anie</trd>,
    <trd>rot&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_je;&c_r;&c_t;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_r;&c_u;&c_h;</trd>,
    <trd>&c_z;&c_m;&c_ib;&c_n;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rotaci.0a">
  <kap><tld/>a</kap>
  <snc mrk="rotaci.0a.gxenerale">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="R"/>anta; rilata al <tld/>o:
      <ekz>
        <ind><tld/>a presilo</ind><fnt><bib>PIV1</bib></fnt>;
        <trd lng="de">Rotationsdruckmaschine</trd>
        <trd lng="fr">rotative<klr tip="ind"> (machine &agrave;
        imprimer)</klr></trd>
        <trd lng="pl">rotacyjna drukarka</trd>
        <trd lng="ru">&c_r;&c_o;&c_t;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_a;&c_ja;
        &c_p;&c_je;&c_ch;&c_a;&c_t;&c_n;&c_a;&c_ja;
        &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
      </ekz>
      <ekz>
        bonvolu skribi la formulon de la <tld/>a parasfero
        <fnt>
          <bib>SatirRak</bib>
          <lok>Honorigo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a akso de rado.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>giratoire<klr tip="ind"> (adj.)</klr></trd>,
      <trd>rotatif</trd>,
      <trd>de <ind>rotation</ind></trd>,
      <trd>rotatoire</trd>
    </trdgrp>
    <trd lng="pl">rotacyjny</trd>
  </snc>
  <snc mrk="rotaci.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="klr">(p.p. ma&scirc;ino)</uzo>
    <dif>
      Kies turni&gcirc;a movo estas senpere produktata:
      <ekz>
        <tld/>a ma&scirc;ino.
      </ekz>
    </dif>
    <adm>
      Indus precizigi la difinon kaj trovi malpli ta&ubreve;tologian
      ekzemplon. Eble ~a motoro ta&ubreve;gus? <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>rotierend</trd>,
      <trd>drehbar</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>roterend</trd>,
      <trd>draaiend</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wirowy</trd>,
      <trd>obrotowy</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x53EF;&#x65CB;&#x8F49; <pr>k&#x11B;xu&#xE1;nzhu&#x1CE;n</pr></trd>,
    <trd>&#x53EF;&#x65CB;&#x8F6C; <pr>k&#x11B;xu&#xE1;nzhu&#x1CE;n</pr></trd>,
    <trd>&#x5FAA;&#x73AF; <pr>x&#xFA;nhu&#xE1;n</pr></trd>,
    <trd>&#x5FAA;&#x74B0; <pr>x&#xFA;nhu&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="rotaci.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="rotaci.0a.rivolua">
      <fnt><bib>PV</bib><lok>hiperboloido</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="solid.0o.geometrio">solido</ref> a&ubreve;
        <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
        <ref tip="dif" cel="rivolu.0a.figuro">Rivolua</ref>:
        <ekz>
          <tld/>a konuso.
        </ekz>
      </dif>
      <trd lng="de">Dreh-<klr tip="amb">(k&ouml;rper,
      fl&auml;che)</klr></trd>
      <trd lng="en"><klr tip="amb">(body, surface) </klr>of
<ind>revolution</ind></trd>
      <trdgrp lng="fr">
        <trd><klr tip="amb">(solide) </klr>de
        <ind>r&eacute;volution</ind></trd>,
        <trd><klr tip="amb">(surface) </klr>de
        <ind>r&eacute;volution</ind></trd>
      </trdgrp>
      <trd lng="nl">omwentelings-</trd>
      <trdgrp lng="pl">
        <trd><klr tip="amb">(bry&lstroke;a) </klr>obrotowa</trd>,
        <trd><klr tip="amb">(powierzchnia) </klr>obrotowa</trd>
      </trdgrp>
      <trd lng="ru"><klr tip="amb">(&c_t;&c_je;&c_l;&c_o;,
      &c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;)
</klr><ind>&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_ja;</ind></trd>
    </subsnc>
    <subsnc mrk="rotaci.0a.diversaj">
      <refgrp tip="vid">
        <ref cel="aks.rotacia0o"><tld/>a akso</ref>,
        <ref cel="centr.rotacia0o"><tld/>a centro</ref>.
      </refgrp>
    </subsnc>
