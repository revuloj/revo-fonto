<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: ibeks.xml,v 1.6 2024/06/20 13:29:02 afido Exp $">
    <kap>
      <rad>ibeks</rad>/o <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="ibeks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="ibeks.0o.alpa">
    <dif>
      Alpa <ref tip="super" cel="kapr.0o">kapro</ref>
      (<trd lng="la">Capra ibex</trd>):
      <ekz>
        &ccirc;u vi scias la tempon, en kiu naskas la <tld/>oj sur la rokoj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 39:1</lok>
        </fnt>?
      </ekz>
      <ekz>
        aminda kiel <tld/>ino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        iberia fa&ubreve;no kiel de cervoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=684046">Masegoso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kaprik.0o">Kaprikorno</ref>
    <bld lok="&WCU;/thumb/e/e6/Ibizea.jpg/640px-Ibizea.jpg" prm="FAL"><tld/>o (Capra ibex), en Valo
    Aosta, Italujo
        <fnt>
          <aut>Joxerra Aihartza, propra foto</aut>
          <lok><url ref="&WCW;/File:Ibizea.jpg">&WCW;/File:Ibizea.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Steinbock</trd>,
      <trd>Alpensteinbock</trd>
    </trdgrp>
    <trd lng="en">Alpine ibex</trd>
    <trd lng="fr">bouquetin des Alpes</trd>
    <trdgrp lng="zh">
    <trd>&#x7FB1;&#x7F8A; <pr>yu&#xE1;ny&#xE1;ng</pr></trd>,
    <trd>&#21271;&#x5C71;&#x7F8A; <pr>b&#x11B;ish&amacron;ny&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_p;&c_ib;&c_s;&c_k;&c_ib; &c_g;&c_o;&c_r;&c_n;&c_y; <ind>&c_k;&c_a;&c_z;&c_jo;&c_l;</ind></trd>
  <trd lng="ja">&#x30A2;&#x30A4;&#x30D9;&#x30C3;&#x30AF;&#x30B9;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_z;&c_a; &c_g;&c_ib;&c_r;&c_s;&#x44C;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&c_z;&c_a; &c_a;&#x43B;&#x44C;&#x43F;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

  </art>

