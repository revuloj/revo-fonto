<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: ibeks.xml,v 1.3 2022/04/30 08:42:42 afido Exp $">
    <kap>
      <rad>ibeks</rad>/o <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="ibeks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="ibeks.0o.alpa">
    <dif>
      Alpa <ref tip="super" cel="kapr.0o">kapro</ref> 
      (<trd lng="la">Capra ibex</trd>):
      <ekz>
        &ccirc;u vi scias la tempon, en kiu naskas la <tld/>oj sur la rokoj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 39:1</lok>
        </fnt>?
      </ekz>
      <ekz>
        aminda kiel <tld/>ino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        iberia fa&ubreve;no kiel de cervoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=684046">Masegoso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kaprik.0o">Kaprikorno</ref>
    <bld lok="&WCU;/thumb/e/e6/Ibizea.jpg/640px-Ibizea.jpg" prm="FAL"><tld/>o (Capra ibex), en Valo Aosta, Italujo
        <fnt>
          <aut>Joxerra Aihartza, propra foto</aut>
          <lok><url ref="&WCW;/File:Ibizea.jpg">&WCW;/File:Ibizea.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Steinbock</trd>,
      <trd>Alpensteinbock</trd>
    </trdgrp>
    <trd lng="en">Alpine ibex</trd>
    <trd lng="fr">bouquetin des Alpes</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x437;&#x430; &#x433;&#x456;&#x440;&#x441;&#x44C;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x43E;&#x437;&#x430; &#x430;&#x43B;&#x44C;&#x43F;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

  </art>

