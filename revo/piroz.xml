<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>piroz</rad>/o
</kap>
<drv mrk="piroz.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Bruleca doloro en ezofago a&ubreve; stomako.
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_ja;&c_k;&c_o;&c_t;&c_k;&c_a;</trd>       
   <trdgrp lng="br">
     <trd>kalondev</trd>,
     <trd>kalonlosk</trd>
   </trdgrp>
   <trd lng="ca">pirosi</trd>
   <trd lng="de">Sodbrennen</trd>
   <trdgrp lng="en">
     <trd>pyrosis</trd>,
     <trd>heart-burn</trd>,
     <trd>stomach acidity</trd>,
     <trd>acid stomach</trd>
   </trdgrp>
   <trd lng="es">pirosis</trd>
   <trd lng="hu">gyomor&eacute;g&eacute;s</trd>
   <trd lng="fr">pyrosis</trd>
   <trdgrp lng="nl">
     <trd>zuur<klr tip="ind"> (v.maag)</klr></trd>,
     <trd>maagbrand</trd>
   </trdgrp>
   <trd lng="ru">&c_i;&c_z;&c_zh;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
