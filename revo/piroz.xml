<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>piroz</rad>/o
</kap>
<drv mrk="piroz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Bruleca doloro en ezofago a&ubreve; stomako.
    </dif>
  </snc>
  <trd lng="de">Sodbrennen</trd>
  <trdgrp lng="en">
    <trd>pyrosis</trd>,
    <trd>heart-burn</trd>,
    <trd>stomach acidity</trd>,
    <trd>acid stomach</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zuur<klr tip="ind"> (v.maag)</klr></trd>,
    <trd>maagbrand</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_zh;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
