<art mrk="$Id: piroz.xml,v 1.23 2023/09/03 12:45:08 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>piroz</rad>/o
</kap>
<drv mrk="piroz.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Bruleca <ref tip="super" cel="dolor.0o">doloro</ref> 
      en <ref cel="ezofag.0o">ezofago</ref> a&ubreve; stomako 
      (<trd lng="la">pyrosis</trd>) pro refluanta stomakacido:
      <ekz>
        olivoleo <klr>[&#x2026;]</klr> efikas en la digesta procezo, 
        malhelpante <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26428">Oliv-oleo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rukt.0o">rukto</ref>,
      <ref cel="singul1.0o">singulto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_k;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_i;&c_n;&c_i;</trd>
  <trdgrp lng="br">
     <trd>kalondev</trd>,
     <trd>kalonlosk</trd>
   </trdgrp>
  <trd lng="ca">pirosi</trd>
  <trd lng="cs">p&aacute;len&iacute; &zcaron;&aacute;hy</trd>
  <trdgrp lng="de">
    <trd>Sodbrennen</trd>,
    <trd>Reflux</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>pyrosis</trd>,
     <trd>heart-burn</trd>,
     <trd>stomach acidity</trd>,
     <trd>acid stomach</trd>
   </trdgrp>
  <trd lng="es">pirosis</trd>
  <trd lng="fr">pyrosis</trd>
  <trd lng="hu">gyomor&eacute;g&eacute;s</trd>
  <trd lng="ja">&#x80F8;&#x3084;&#x3051;  [<ind>&#x3080;&#x306D;&#x3084;&#x3051;</ind>]</trd>
  <trdgrp lng="nl">
     <trd>zuur<klr tip="ind"> (v.maag)</klr></trd>,
     <trd>maagbrand</trd>
   </trdgrp>
   <trd lng="pl">zgaga</trd>
  <trdgrp lng="pt">
    <trd>azia</trd>,
    <trd>pirose</trd>
  </trdgrp>
  <trd lng="ro">pirozis</trd>
  <trd lng="ru">&c_i;&c_z;&c_zh;&c_o;&c_g;&c_a;</trd>
  <trd lng="sk">p&aacute;lenie z&aacute;hy</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_ch;&c_ib;&#x44F;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_g;&c_a;</trd>
  </trdgrp>
</drv>
</art>
