<vortaro>
  <art mrk="$Id: upsal.xml,v 1.3 2022/08/19 15:37:38 afido Exp $">
    <kap>
      <rad>Upsal</rad>/o
    </kap>

<drv mrk="upsal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Universitata <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="super" cel="sved.S0ujo">Svedujo</ref>, &ccirc;. 60&#x202F;km norde de Stokholmo:
      <ekz>
        en 1895 du studentoj &ccirc;e la universitato de <tld/>o faris longan voja&gcirc;on tra
        E&ubreve;ropo
        <fnt>
          <aut>Valdemar Langlet</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=vojaghimpresoj">Voja&gcirc;impresoj</url></vrk>
          <lok>Enkonduko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni man&gcirc;adis en la sama pensiono en <tld/>o, kie mi studis
        <fnt>
          <aut>Hjalmar S&ouml;derberg, trad. Sten Johansson </aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=la-kiso">La Kiso</url></vrk>
          <lok>La Bovinoj de la Paro&hcirc;estro</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna <tld/>a studento
        <fnt>
          <bib>Fab3</bib>
          <lok>Muta libro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Klubo Esperanta eldonis la gazeton
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_U;&c_p;&c_s;&c_a;&c_l;&c_a;</trd>
    <trd lng="de">Uppsala</trd>
    <trd lng="en">Uppsala</trd>
    <trd lng="fr">Uppsala</trd>
  </snc>
  <trd lng="uk">&c_U;&#x43F;&c_s;&c_a;&#x43B;&c_a;</trd>
</drv>

  </art>

