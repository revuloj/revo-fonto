<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: olimpik.xml,v 1.11 2021/05/13 07:54:18 revo Exp $">
  <kap>
    <rad>Olimpik</rad>/o
    <fnt><bib>SPV</bib></fnt>
  </kap>

<drv mrk="olimpik.0o">
  <kap><tld/>o</kap>
  <snc mrk="olimpik.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Granda internacia sportoturniro, kiu imitas samnoman
      ceremonion religian kaj sportan en antikva Grekujo:
      <ekz>
        la <tld lit="o"/>oj de la jaro 2000 en Sidneo
        anonci&gcirc;as luksaj
        <fnt>
          <aut>C. Rigollet, trad. C. Bertin</aut>,
          <vrk>
          <url ref="http://bertin.ifrance.com/artikol/jo/2000.htm">
          ... la olimpikoj de la jarmilo</url></vrk>,
          <lok>2001-07</lok>
        </fnt>;
      </ekz><ekz>
        &ccirc;e la lagobordo funkcias la trejnejo de la altkvalitaj
        sportistoj preti&gcirc;antaj al la <tld lit="o"/>o
        <fnt>
          <aut>-</aut>,
          <vrk>
          <url ref="http://www.hungario.hu/">Hungario</url></vrk>,
          <lok>Regiono 2 [2006]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="olimpik.0o.RIM_nombro">
    En iuj lingvoj la vorto aperas nur plurnombre.  Ununombraj 
    ekzemploj en Esperanto ne abundas, sed ekzistas, nombrante 2 
    elcentojn de interretaj aperoj en 2007-05.
    <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="bg">
    <trd>&c_O;&c_l;&c_i;&c_m;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i; &c_i;&c_g;&c_r;&c_i;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Olympiade</trd>,
    <trd>Olympische Spiele</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Olympic games</trd>,
    <trd>Olympics</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_l;&a_m;&f_p;&f_y;&a_A;&a_d;</trd>,
    <trd>&a_b;&a_A;&a_z;&f_y;&zwnj;&a_h;&a_A;&f_y; <ind>&a_A;&a_l;&a_m;&f_p;&f_y;&f_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Jeux olympiques</trd>,
    <trd>JO</trd>
  </trdgrp>
  <trd lng="id">Olimpiade</trd>
  <trd lng="nl">Olympische Spelen</trd>
  <trd lng="pl">olipiada</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;&c_je;
    &c_i;&c_g;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="sk">olympijsk&eacute; hry</trd>
  <trd lng="cs">olympijsk&eacute; hry</trd>
</drv>

<drv mrk="olimpik.somera0o">
  <kap>Somera <tld/>o</kap>
  <snc mrk="olimpik.somera0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="olimpik.0o.SPO"><tld lit="O"/>o</ref> de
      sportoj arenaj a&ubreve; en&ccirc;ambraj okazanta &ccirc;iun
      kvaran jaron ekde 1896. 
    </dif>
  </snc>
  <trd lng="bg">&c_L;&c_je;&c_t;&c_n;&c_i; <ind>&c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;</ind> &c_i;&c_g;&c_r;&c_i;</trd>
  <trd lng="de">Olympiade<klr tip="amb"> (Sommerspiele)</klr></trd>
  <trd lng="en">Summer <ind>Olympic</ind> Games</trd>
  <trd lng="fa">&a_b;&a_A;&a_z;&f_y;&zwnj;&a_h;&a_A;&f_y; <ind>&a_A;&a_l;&a_m;&f_p;&f_y;&f_k;</ind>
  &a_t;&a_A;&a_b;&a_s;&a_t;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">Jeux olympiques d'&eacute;t&eacute;</trd>
  <trd lng="id"><ind>Olimpiade</ind> musim panas</trd>
  <trd lng="nl">Olympische zomerspelen</trd>
  <trd lng="pl">olimpiada letnia</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_t;&c_n;&c_ja;&c_ja;
    &c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_t;&c_n;&c_i;&c_je;
    &c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;&c_je;
    &c_i;&c_g;&c_r;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="olimpik.vintra0o">
  <kap>Vintra <tld/>o</kap>
  <snc mrk="olimpik.vintra0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="olimpik.0o.SPO"><tld lit="O"/>o</ref> de
      sportoj surne&gcirc;aj, surglaciaj, okazanta &ccirc;iun kvaran
      jaron ekde 1921:
      <ekz>
        En Canmore oni konstruis instala&jcirc;ojn por la Vintra
        <tld/>o de 1988
        <fnt><bib>Viki</bib>,
          <lok>artikolo &leftquot;Canmore (Alberto)&rightquot;,
          2006-12-12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_Z;&c_i;&c_m;&c_n;&c_i; <ind>&c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;</ind> &c_i;&c_g;&c_r;&c_i;</trd>
  <trd lng="de">Olympische Winterspiele</trd>
  <trd lng="en">Winter <ind>Olympic</ind> Games</trd>
  <trd lng="fa">&a_b;&a_A;&a_z;&f_y;&zwnj;&a_h;&a_A;&f_y; <ind>&a_A;&a_l;&a_m;&f_p;&f_y;&f_k;</ind>
  &a_z;&a_m;&a_s;&a_t;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">Jeux olympiques d'hiver</trd>
  <trd lng="id"><ind>Olimpiade</ind> musim dingin</trd>
  <trd lng="nl">Olympische winterspelen</trd>
  <trd lng="pl">olimpiada zimowa</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_i;&c_m;&c_n;&c_ja;&c_ja;
    &c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_i;&c_m;&c_n;&c_i;&c_je;
    &c_o;&c_l;&c_i;&c_m;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;&c_je;
    &c_i;&c_g;&c_r;&c_y;</trd>
  </trdgrp>
</drv>
</art>

