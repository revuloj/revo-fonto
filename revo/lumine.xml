<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lumine.xml,v 1.13 2023/10/19 14:32:17 revo Exp $">
<kap>
  <rad>luminesk</rad>/i
</kap>

<drv mrk="lumine.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Lumi ne pro varmigo kaj ne pro reflekto (pro
      ekscito per lumo, per radiado, per elektro, pro
      kemiaj reakcioj ktp).
    </dif>
    <refgrp tip="sub">
      <ref cel="fosfor1.0i">fosforeski</ref>,
      <ref cel="fluore.0i">fluoreski</ref>
    </refgrp>
    <ref tip="vid" cel="inkand.0i">inkandeski</ref>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_c;&c_ib;&c_c;&c_c;&c_a; &c_l;&c_ju;&c_m;&c_ib;&c_n;&c_e;&c_s;&c_c;&c_e;&c_n;&c_t;&c_n;&c_y;&c_m; &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_o;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>luminiskovat</trd>,
    <trd>sv&ecaron;t&eacute;lkovat</trd>
  </trdgrp>
  <trd lng="de">lumineszieren</trd>
  <trd lng="en">luminesce</trd>
  <trd lng="ja">&#x51B7;&#20809;&#12434;&#x767A;&#12377;&#x308B; 
  <pr>&#12402;&#12420;&#12402;&#x304B;&#x308A;&#12434;&#x306F;&#12387;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>oplichten<klr tip="ind"> (luminisceren)</klr></trd>,
    <trd>luminisceren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;wieci&cacute;</trd>,
    <trd>emanowa&cacute; <ind>&sacute;wiat&lstroke;em</ind></trd>
  </trdgrp>
  <trd lng="pt">apresentar <ind>luminesc&ecirc;ncia</ind></trd>
  <trdgrp lng="ro">
    <trd>str&abreve;luci</trd>,
    <trd>s&abreve; radieze <ind>lumin&abreve;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_m;&c_i;&c_n;&c_je;&c_s;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">svetielkova&tcaron;</trd>
  <trd lng="uk">&c_s;&c_v;&c_ib;&c_t;&c_i;&c_t;&c_i;&c_s;&#x44F; &c_h;&#x43E;&#x43B;&#x43E;&c_d;&#x43D;&c_i;&#x43C;
  &c_s;&c_v;&c_ib;&c_t;&#x43B;&#x43E;&#x43C;</trd>
</drv>

<drv mrk="lumine.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Fenomeno <tld/>i.
    </dif>
    <refgrp tip="sub">
      <ref cel="fosfor1.0o">fosforesko</ref>,
      <ref cel="fluore.0o">fluoresko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ju;&c_m;&c_ib;&c_n;&c_e;&c_s;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>luminiscence</trd>,
    <trd>sv&ecaron;t&eacute;lkov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lumineszenz</trd>,
    <trd>kaltes <ind>Leuchten</ind></trd>
  </trdgrp>
  <trd lng="en">luminescence</trd>
  <trd lng="fr">luminescence</trd>
  <trdgrp lng="ja">
    <trd>&#x30EB;&#x30DF;&#x30CD;&#x30BB;&#x30F3;&#x30B9;</trd>,
    <trd>&#x51B7;&#20809;  <pr>&#x308C;&#12356;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">luminiscentie</trd>
  <trdgrp lng="pl">
    <trd>luminescencja</trd>,
    <trd>jarzenie</trd>,
    <trd>emanacja <ind>&sacute;wiat&lstroke;a</ind></trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>luminiscen&#x21B;&abreve;</trd>,
    <trd>lumina</trd>,
    <trd>emana&#x21B;ie de <ind>lumin&abreve;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_m;&c_i;&c_n;&c_je;&c_s;&c_c;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">svetielkovanie</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44E;&#x43C;&c_ib;&#x43D;&c_je;&c_s;&c_c;&c_je;&#x43D;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_ib;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="lumine.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Havanta econ <tld/>i:
      <ekz>
        <tld/>aj putroligna&jcirc;oj, fiksitaj anta&ubreve; la muzikistoj sur la salikaj vergoj,
        prilumis la notojn
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_m;&c_ib;&c_n;&c_e;&c_s;&c_c;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>luminiscen&ccaron;n&iacute;</trd>,
    <trd>sv&ecaron;t&eacute;lkuj&iacute;c&iacute;</trd>
  </trdgrp>
  <trd lng="de">lumineszierend</trd>
  <trd lng="en">luminescent</trd>
  <trd lng="fr">luminescent</trd>
  <trdgrp lng="nl">
    <trd>oplichtend<klr tip="ind">(luminiscerend)</klr></trd>,
    <trd>luminiscerend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>luminescencyjny</trd>,
    <trd>&sacute;wiec&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>luminos</trd>,
    <trd>luminescent</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ju;&c_m;&c_i;&c_n;&c_je;&c_s;&c_c;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_m;&c_i;&c_n;&c_je;&c_s;&c_c;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">svetielkuj&uacute;ci</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44E;&#x43C;&c_ib;&#x43D;&c_je;&c_s;&c_c;&c_je;&#x43D;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="lumine.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Substanco, kapabla <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_m;&c_ib;&c_n;&c_a;&c_f;&c_o;&c_r;</trd>
  <trdgrp lng="fr">
    <trd>luminophore</trd>,
    <trd>substance <ind>luminescente</ind></trd>
  </trdgrp>
  <trd lng="nl">luminiscerende stof</trd>
  <trd lng="pl">luminescencja</trd>
  <trd lng="ro">luminiscen&#x21B;&abreve;</trd>
  <trd lng="ru">&c_l;&c_ju;&c_m;&c_i;&c_n;&c_o;&c_f;&c_o;&c_r;</trd>
</drv>

</art>

