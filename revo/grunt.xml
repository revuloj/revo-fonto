<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grunt.xml,v 1.24 2024/06/18 09:28:45 revo Exp $">
<kap>
  <rad>grunt</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="grunt.0">
  <kap><tld/></kap>
  <snc mrk="grunt.0.KOMUNE">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref> de porko a&ubreve; simila:
      <ekz>
        &leftquot;<tld/>, <tld/>&rightquot; diris porko &leftquot;&ccirc;u vi volas veni ruli&gcirc;i en &scirc;limo kun mi?&rightquot;
        <fnt>
           <aut>E. Carle, trad. Nikola kaj Martin Markarian</aut>
          <vrk><url ref="http://www.phys.ens.fr/~jacobsen/infanlibroj/Araneo.pdf">
          La tre okupita araneo</url></vrk>
          <lok>[vidita en 2023]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_h;&c_k;!</trd>
  <trdgrp lng="fr">
    <trd>groumf<klr> (grognement)</klr></trd>,
    <trd>grou&iuml;c<klr> (porc)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="grunt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grunt.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref>
      obtuze, kiel porko:
      <ekz>
        kiam malheli&gcirc;is la lumo, ni a&ubreve;dis strangajn sonojn,
        bojojn, kriegojn, <tld/>ojn
        <fnt>
          <aut>D. Defo, trad. A. Krafft</aut>,
          <vrk><url ref="&Gutenberg;files/11511/11511-0.txt">
          Robinsono Kruso</url></vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grumbl.0i">grumbli</ref>,
      <ref cel="ronk.0i">ronki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_g;&c_r;&c_u;&c_h;&c_t;&c_ja;</trd>
  <trd lng="ca">grunyir</trd>
  <trd lng="cs">chrochtat</trd>
  <trd lng="de">grunzen</trd>
  <trd lng="en">grunt</trd>
  <trd lng="es">gru&ntilde;ir</trd>
  <trd lng="fr">grogner<klr tip="ind"> (en p. d'un porc)</klr></trd>
  <trd lng="hu">r&ouml;f&ouml;g</trd>
  <trd lng="ja">&#x30D6;&#x30FC;&#x30D6;&#x30FC;&#x9CF4;&#x304F;
  <pr>&#x30D6;&#x30FC;&#x30D6;&#x30FC;&#x306A;&#x304F;</pr></trd>
  <trd lng="nl">knorren</trd>
  <trd lng="pt">grunhir</trd>
  <trd lng="ru">&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">krochka&tcaron;</trd>
  <trd lng="uk">&c_h;&c_r;&#x44E;&#x43A;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x547C;&#x565C;&#x58F0; <pr>h&#x16B;l&#x16B;sh&emacron;ng</pr></trd>,
    <trd>&#21653;&#x565C; <pr>g&#x16B;lu</pr></trd>
  </trdgrp>
</drv>
</art>
