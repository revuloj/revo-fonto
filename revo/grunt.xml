<art mrk="$Id$">
<kap><rad>grunt</rad>/i
<fnt>Z</fnt></kap>
<drv mrk="grunt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel porko.
    </dif>
    <refgrp tip="vid">
      <ref cel="grumbl.0i">grumbli</ref>,
      <ref cel="ronk.0i">ronki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">grunyir</trd>
  <trd lng="de">grunzen</trd>
  <trd lng="en">grunt</trd>
  <trd lng="es">gru&ntilde;ir</trd>
  <trd lng="fr">grogner<klr tip="ind"> (en p. d'un porc)</klr></trd>
  <trd lng="hu">r&ouml;f&ouml;g</trd>
  <trd lng="nl">knorren</trd>
  <trd lng="pt">grunhir</trd>
  <trd lng="ru">&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
