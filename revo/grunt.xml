<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grunt</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="grunt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grunt.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref>
      obtuze, kiel porko:
      <ekz>
        kiam malheli&gcirc;is la lumo, ni a&ubreve;dis strangajn sonojn,
        bojojn, kriegojn, <tld/>ojn
        <fnt>
          <aut>D. Defo, trad. A. Krafft</aut>,
          <vrk><url
          ref="http://www.gutenberg.org/files/11511/11511-0.txt">
          Robinsono Kruso</url></vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grumbl.0i">grumbli</ref>,
      <ref cel="ronk.0i">ronki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">grunyir</trd>
  <trd lng="cs">chrochtat</trd>
  <trd lng="de">grunzen</trd>
  <trd lng="en">grunt</trd>
  <trd lng="es">gru&ntilde;ir</trd>
  <trd lng="fr">grogner<klr tip="ind"> (en p. d'un porc)</klr></trd>
  <trd lng="hu">r&ouml;f&ouml;g</trd>
  <trd lng="nl">knorren</trd>
  <trd lng="pt">grunhir</trd>
  <trd lng="ru">&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">krochka≈•</trd>
</drv>
</art>
