      &c_i;&c_n;&c_s;&c_t;&c_a;&c_l;&c_i;&c_r;&c_a;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="cs">ladi&ccaron;ka</trd>    
    <trdgrp lng="de">
