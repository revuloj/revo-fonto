<drv mrk="agord.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="agord.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ilo (tonforko, <tld/>a forko, a&ubreve; <tld/>a tubo) por
      a&ubreve;digi sen&scirc;an&gcirc;an tonon, la&ubreve; kiu oni <tld/>as
      muzikilojn.
    </dif>
    <ref tip="sub" cel="diapaz.0o">diapazono</ref>
    <trd lng="br">tonreizher</trd>
    <trd lng="de">Stimmgabel</trd>
    <trdgrp lng="en">
      <trd>tuning fork</trd>,
      <trd>pitch pipe</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>diapas&oacute;n</trd>,
      <trd>martillo <klr>(templador)</klr></trd>
    </trdgrp>
