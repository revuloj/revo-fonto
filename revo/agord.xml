</drv>

<drv mrk="agord.anim0o">
  <kap>anim<tld/>o</kap>
  <snc mrk="agord.anim0o.PSI">
    <dif>
      &Gcirc;enerala, komuna humoro;
      emocia flanko de 
      <ref tip="malprt" cel="karakt.0o.PSI">karaktero</ref>:
      <ekz>
        mi trovi&gcirc;as en ege ekscitita anim<tld/>o
        <fnt>
          <aut>P. Lagerkvist, trad. F. Szil&aacute;gyi</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5606/gasto.html">
          La pretendema gasto</url></vrk>,
          <lok>Norda Prismo, 1956:6, p. 221a-225a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;tat d'&acirc;me</trd>,
    <trd>tournure d'<ind>esprit</ind></trd>
  </trdgrp>
