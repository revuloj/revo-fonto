<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>graf</rad>/o
</kap>
<drv mrk="graf1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="graf1.0o.grafeo">
    <ref tip="dif" cel="grafe.0o.strukturo">grafeo</ref>
    <trd lng="de">Graph</trd>
    <trd lng="en">graph</trd>
    <trd lng="fr">graphe<klr tip="ind"> (sommets et ar&ecirc;tes)</klr></trd>
    <trd lng="hu">gr&aacute;f</trd>
    <trd lng="nl">graaf<klr tip="ind"> (wisk.)</klr></trd>
    <trd lng="pl">graf</trd>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_f;</trd>
  </snc>
  <snc mrk="graf1.0o.grafikajxo">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="grafik.0ajxo.MAT">grafika&jcirc;o</ref>
  </snc>
  <ref tip="hom" cel="graf.0o">grafo (nobelo)</ref>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;</trd>
</drv>
</art>

