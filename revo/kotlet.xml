<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kotlet.xml,v 1.21 2023/12/03 09:58:59 revo Exp $">
<kap><ofc>1</ofc>
<rad>kotlet</rad>/o</kap>
<drv mrk="kotlet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Rostita peco el rip<ref tip="super" cel="viand.0o">viando</ref> de &scirc;afo, bovido, porko:
      <ekz>
        mi kaj <nom>Craggs</nom> (benita memoro) ofte man&gcirc;adis tie &ccirc;i <tld/>on kaj trovadis &gcirc;in
        &ccirc;iam bona
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        salmo, kaj fi&scirc;o, kaj <tld/>oj? &dash; ili estas por personoj pli gravaj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas &scirc;atanto de: <klr>[&#x2026;]</klr> <tld/>oj kaj picoj
        <fnt>
          <bib>Monato</bib>
          <aut>Grigorij Arosev</aut>
          <vrk><url ref="&Monato;/2003/008100.html">Grigorij Arosev</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_l;&c_je;&c_t;&c_a;</trd>
  <trd lng="cs">kotleta</trd>
  <trdgrp lng="de">
    <trd>Kotelett <klr>(kun ripo)</klr></trd>,
    <trd>Schnitzel <klr>(sen ripo)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chop</trd>,
    <trd>cutlet</trd>
  </trdgrp>
  <trd lng="es">chuleta</trd>
  <trd lng="fr">c&ocirc;telette</trd>
  <trdgrp lng="hu">
    <trd>bordaszelet</trd>,
    <trd>karaj</trd>,
    <trd>kotlett</trd>
  </trdgrp>
  <trd lng="ja">&#x30AB;&#x30C4;&#x30EC;&#x30C4;</trd>
  <trdgrp lng="nl">
    <trd>kotelet</trd>,
    <trd>ribkarbonade</trd>
  </trdgrp>
  <trd lng="pl">kotlet</trd>
  <trdgrp lng="pt">
    <trd>costeleta</trd>,
    <trd>costelinha</trd>
  </trdgrp>
  <trd lng="ro">cotlet</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_t;&c_l;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_i;&c_v;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">kotleta <klr>(kuch)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x6392;&#x9AA8; <pr>p&#xE1;ig&#x1D4;</pr></trd>,
    <trd>&#x732A;&#x6392; <pr>zh&#x16B;p&#xE1;i</pr></trd>
  </trdgrp>
</drv>
</art>
