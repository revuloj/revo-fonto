<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>imunologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="imunologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="imunologi.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Scienco pri la <ref tip="vid"
      cel="sistem.imun0o.FIZL">imunsistemo</ref>, pri &gcirc;ia funkciado
      kontra&ubreve; korpofremdaj substancoj:
      <ekz>
        estis 1967, kiam li komencis labori en <tld/>o kaj virusologio
        <fnt>
          <aut>Liga Colombiana de Esperanto</aut>,
          <vrk><url 
ref="http://www.geocities.com/esperantokolombio/patarroyo.html">Manuel
Elkin
Patarroyo</url></vrk>,
          <lok>[vidita je 2006-05-26]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fa">
    <trd>&a_A;&f_y;&a_m;&a_n;&f_y;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_A;&f_y;&a_m;&a_w;&a_n;&a_w;&a_l;&a_w;&f_zh;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">immunologie</trd>
  <trd lng="hu">immunol&oacute;gia</trd>
  <trdgrp lng="nl">
    <trd>immunologie</trd>,
    <trd>immuniteitsleer</trd>
  </trdgrp>
  <trd lng="pl">immunologia</trd>
  <trd lng="pt">imunologia</trd>
  <trd
lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="imunologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="imunologi.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="fa">&a_A;&f_y;&a_m;&a_n;&f_y;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">immunologiste</trd>
  <trd lng="hu">immunol&oacute;gus</trd>
  <trd lng="nl">immunoloog</trd>
  <trd lng="pl">immunolog</trd>
  <trd lng="pt">imunologista</trd>
  <trd lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>

