<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vestibl</rad>/o
</kap>
<drv mrk="vestib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Enira parto de domo, konstruo kc, tra kiu oni atingas la &ccirc;ambrojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="cxambr.antaux0o">anta&ubreve;&ccirc;ambro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Diele</trd>,
    <trd>Vorhalle</trd>,
    <trd>Flur</trd>
  </trdgrp>
  <trd lng="el">&pi;&rho;&omikron;&theta;&alfa_ton;&lambda;&alfa;&my;&omikron;&sigma_fina;</trd>
  <trdgrp lng="nl">
    <trd>hal</trd>,
    <trd>vestibule</trd>
  </trdgrp>
  <trd lng="pt">vest&iacute;bulo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i;&c_b;&c_ju;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_zh;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
