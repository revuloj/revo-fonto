<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vestibl</rad>/o
</kap>
<drv mrk="vestib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Enira parto de domo, tra kiu oni atingas la &ccirc;ambrojn:
      <ekz>
        li <klr>...</klr> elpu&scirc;igas sin kune kun li en la <tld/>on
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 11a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxambr.antaux0o.porMantelo">anta&ubreve;&ccirc;ambro <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_e;&c_s;&c_t;&c_y;&c_b;&c_ju;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib; &c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Diele</trd>,
    <trd>Vorhalle</trd>,
    <trd>Flur</trd>
  </trdgrp>
  <trd lng="el">&pi;&rho;&omikron;&theta;&alfa_ton;&lambda;&alfa;&my;&omikron;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>entr&eacute;e<klr tip="amb"> (vestibule)</klr></trd>,
    <trd>vestibule</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hal</trd>,
    <trd>vestibule</trd>
  </trdgrp>
  <trd lng="pt">vest&iacute;bulo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i;&c_b;&c_ju;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_zh;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
