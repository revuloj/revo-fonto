<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: meteostaci.xml,v 1.6 2024/01/25 20:10:36 revo Exp $">
<kap>
  <rad>meteostaci</rad>/o <fnt><aut>M. Lewin</aut> <vrk>Meteologia
  terminaro</vrk> <lok>1961</lok></fnt>
</kap>
<drv mrk="meteostaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="meteostaci.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ke><ref tip="dif" cel="staci.veter0o">Veterstacio</ref></ke>:
      <ekz>
        la dekan de julio la <tld lit="M"/>o de <nom>Zhejiang</nom>
        publikigis ur&gcirc;an alarmon pri superpotenca tajfuno
        &leftquot;<nom>Canhong</nom>&rightquot;
        <fnt>
          <aut>Xiong Linping</aut>
          <vrk><url ref="&CRI;581/2015/07/11/177s178160.htm">
          Superpotenca tajfuno "Canhong" balda&ubreve; surteri&gcirc;os</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2015-07-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Wetterstation</trd>
  <trd lng="en">weather station</trd>
  <trd lng="fr">station m&eacute;t&eacute;o</trd>
  <trdgrp lng="zh">
    <trd>&#x6C14;&#x8C61;&#x53F0; <pr>q&#xEC;xi&#xE0;ngt&#xE1;i</pr></trd>,
    <trd>&#x6C23;&#x8C61;&#x53F0; <pr>q&#xEC;xi&#xE0;ngt&#xE1;i</pr></trd>
  </trdgrp>
</drv>
</art>

