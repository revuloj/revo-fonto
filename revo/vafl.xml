<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vafl</rad>/o
</kap>
<drv mrk="vafl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Plata <ref tip="super" cel="kuk.0o">kuko</ref> bakita inter du
      feraj ladoj:
      <ekz>
        la fundamenta plato de sveda bombo estas cirkloforma maldika <tld/>o,
        sur tiu levi&gcirc;as &scirc;a&ubreve;ma korpo, <klr>[&#8230;]</klr> la tuto estas kovrita de &ccirc;okolado
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        reklami la de li fabrikatajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Jean Pierre VandenDaele</aut>
          <vrk><url ref="&Monato;/2012/010697.php">De analogo al cifereco: evoluo de persona historio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="oblat.0o">oblato</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_f;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_v;&c_a;&c_f;&c_l;a</trd>
  <trd lng="br">galfrezenn</trd>
  <trd lng="ca">gofra</trd>
  <trdgrp lng="cs">
    <trd>oplatka</trd>,
    <trd>vafle</trd>
  </trdgrp>
  <trd lng="de">Waffel</trd>
  <trd lng="en">waffle</trd>
  <trd lng="es">barquillo</trd>
  <trd lng="fa">&a_w;&a_A;&a_f;&a_l;</trd>
  <trd lng="fr">gaufre</trd>
  <trdgrp lng="he">
    <trd>&alef;&pe;&yod;&pe;&yod;&tav;</trd>,
    <trd>&resh;&qof;&yod;&qof;</trd>
  </trdgrp>
  <trd lng="hu">ostya</trd>
  <trd lng="nl">wafel</trd>
  <trd lng="pl">gofr</trd>
  <trd lng="pt">folhado<klr tip="ind"> (culin&aacute;ria)</klr></trd>
  <trd lng="ru">&c_v;&c_a;&c_f;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>obl&aacute;tka</trd>,
    <trd>opl&aacute;tka</trd>
  </trdgrp>
  <trd lng="sv">v&aring;ffla</trd>
</drv>
</art>
