<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>filantrop</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="filant1.0o">
  <kap><tld/>o</kap>
  <snc mrk="filant1.0o.KOMUNE">
    <dif>
      Humanulo, homaramanto, bonfarema homo:
      <ekz>
        ni komprenas la emocion de la <tld/>o, kiu ne povas rezigni je 
        la espero en finan venkon de bono
        <fnt>
          <aut>J. Nov&aacute;k, trad. Rondeto de esperantistinoj</aut>, 
          <vrk><url ref="http://www.gutenberg.org/files/21951/21951-0.txt">
          Jan Amos Komensk&yacute;</url></vrk>,
          <lok>1921</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="filant2.0o">filantropio</ref>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_n;&c_t;&c_r;&c_o;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Philanthrop</trd>,
    <trd>Menschenfreund</trd>
  </trdgrp>
  <trd lng="es">fil&aacute;ntropo</trd>
  <trdgrp lng="fr">
    <trd>bienfaiteur<klr> (subst.)</klr></trd>,
    <trd>philanthrope</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>filantr&oacute;p</trd>,
    <trd>emberbar&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>filantroop</trd>,
    <trd>mensenvriend</trd>
  </trdgrp>
  <trd lng="pt">filantropo</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;</trd>
</drv>
</art>
