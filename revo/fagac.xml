<vortaro>
<art mrk="$Id$">
<kap><rad>fagac</rad>/oj</kap>
<drv mrk="fagac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Fagaceae</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="fag.0o">fago</ref>,
        <ref cel="kverk.0o">kverko</ref>,
        <ref cel="kasxta.0o">ka&scirc;tano</ref>
      </refgrp>
      k.a.
    </dif>
  </snc>
  <trd lng="de">Buchengew&auml;chse</trd>
  <trd lng="fr">fagac&eacute;es</trd>
  <trd lng="nl">napjesdragers</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
