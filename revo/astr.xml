<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>astr</rad>/o
</kap>
<drv mrk="astr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc num="1" mrk="astr.0o.1">
    <dif>
      &Ccirc;iela korpo:
      <refgrp tip="sub">
        <ref cel="stel.0o.AST">stelo</ref>,
        <ref cel="planed.0o">planedo</ref>,
        <ref cel="komet.0o">kometo</ref>
      </refgrp>.
    </dif>
    <trd lng="fr">astre</trd>
    <trd lng="nl">hemellichaam</trd>
    <trd lng="ru">&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;&c_je; &c_t;&c_je;&c_l;&c_o;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="stel.0o">Stelo.</ref>
    </dif>
    <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  </snc>
</drv>
</art>
