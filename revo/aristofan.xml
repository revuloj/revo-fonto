<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aristofan.xml,v 1.14 2023/08/30 18:23:36 revo Exp $">
<kap>
   <rad>Aristofan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="aristofan.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">PERS</uzo>
   <uzo tip="fak">TEA</uzo>
   <snc>
     <dif>
       Helena verkinto de komedioj (&ccirc;. 445 a. K. &ndash; &ccirc;. 385 a. K.),
       spritaj farsoj kun aludoj al la politika, socia kaj intelekta
       vivo
       en tiama Ateno:
       <ekz>
         greka komedio de <tld/>o, kiun li &gcirc;uis en la originalo
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 15a, p.
         44a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_A;&c_r;&c_y;&c_s;&c_t;&c_a;&c_f;&c_a;&c_n;</trd>
  <trd lng="bg">&c_A;&c_r;&c_i;&c_s;&c_t;&c_o;&c_f;&c_a;&c_n;</trd>
  <trd lng="cs">Aristofan&#xE9;s</trd>
  <trd lng="de">Aristophanes</trd>
  <trd lng="fa">&a_A_madda;&a_r;&f_y;&a_s;&a_t;&a_w;&a_f;&a_A;&a_n;</trd>
  <trd lng="fr">Aristophane</trd>
  <trd lng="hu">Arisztophan&eacute;sz</trd>
  <trd lng="it">Aristofane</trd>
  <trd lng="ja">&#x30A2;&#x30EA;&#x30B9;&#x30C8;&#x30D5;&#x30A1;&#x30CD;&#x30B9;</trd>
  <trd lng="nl">Aristofanes</trd>
  <trd lng="ru">&c_A;&c_r;&c_i;&c_s;&c_t;&c_o;&c_f;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>Aristofanes</trd>,
    <trd>Aristofan&#xE9;s</trd>
  </trdgrp>
  <trd lng="uk">&#x410;&#x440;&#x456;&#x441;&#x442;&#x43E;&#x444;&#x430;&#x43D;</trd>
</drv>
</art>

