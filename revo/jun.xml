  </trdgrp>
</drv>

<drv mrk="jun.mal0ulo">
  <kap><ofc>*</ofc>mal<tld/>ulo
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -ul'</lok></fnt></kap>
  <snc mrk="jun.mal0ulo.KOMUNE">
    <dif>
      Mal<tld/>a homo:
      <ekz>
        tiu &ccirc;i mal<tld/>ulo tute malsa&gcirc;i&gcirc;is kaj 
        infani&gcirc;is
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Abraham</nom> kaj <nom>Sara</nom> estis mal<tld/>uloj de
        profunda a&gcirc;o
        <fnt><bib>MT</bib>, <lok>&Gen; 18:11</lok></fnt>;
      </ekz>
      <ekz>
        sur <klr>[&scirc;ia viza&gcirc;o]</klr> aperis multaj mal<tld/>ulaj sulkoj
        <fnt>
          <bib>FK</bib>
          <vrk>Arturo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>mal<tld/>ulino</ind> staris anta&ubreve; la pordo
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>.
        <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_u;&c_h;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF62;&#xF92;&#xF53;&#xF0B;&#xF60;&#xF41;&#xF7C;&#xF42;&#xF66;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>kozhiad</trd>,
    <trd>kozh<klr tip="ind"> (ak.)</klr></trd>
  </trdgrp>
  <trd lng="cs">sta&rcaron;ec</trd>
  <trd lng="de">Alter</trd>
  <trd lng="en">old person</trd>
  <trdgrp lng="es">
    <trd>mayor<klr> (persona)</klr></trd>,
    <trd>anciano</trd>,
    <trd>viejo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vieillard</trd>,
    <trd>vieil homme</trd>,
    <trd>vieux<klr tip="amb"> (subst., vieil homme)</klr></trd>
  </trdgrp>
  <trd lng="id"><ind>orang</ind> tua</trd>
  <trd lng="io">oldo</trd>
  <trd lng="it">vecchio<klr> (sost.)</klr></trd>
  <trdgrp lng="nl">
    <trd>oud persoon</trd>,
    <trd>oudere</trd>,
    <trd>grijsaard</trd>,
    <trd><ind>oude</ind> man</trd>,
    <trd><ind>oude</ind> vrouw</trd>
  </trdgrp>
  <trd lng="pl">starzec</trd>
  <trd lng="ro">b&abreve;tr&icirc;n</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_i;&c_k;</trd>
  <trd lng="sk">starec</trd>
</drv>
