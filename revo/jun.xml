<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>jun</rad>/a
</kap>
<drv mrk="jun.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="jun.0a.junagxa">
    <dif>
      Ne ankora&ubreve; granda&gcirc;a; ekzistanta de relative nelonga tempo:
      <ekz>
        la pli <tld/>a filino
        <fnt><bib>F</bib>, <lok>&FE; 11</lok></fnt>;
      </ekz>
      <ekz>
        mi gratulis <klr>...</klr> la junajn geedzojn
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a virino, knabo, &ccirc;evalo, folio;
      </ekz>
      <ekz>
        lernu <tld/>a, vi scios mal<tld/>a
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la &ccirc;itiea Esperanto-movado estas tre <tld/>a;
      </ekz>
      <ekz>
        <tld/>ularo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="jun.0a.juneska">
    <dif>
      Havanta la kvalitajn de la <tld/>eco, viglecon, ardon, fre&scirc;econ ktp:
      <ekz>
        oni povas esti <tld/>a &ccirc;e la kvardeka jaro, kaj mal<tld/>a &ccirc;e la
        dudeka;
      </ekz>
      <ekz>
        &scirc;i <tld/>i&gcirc;as depost sia edzi&gcirc;o;
      </ekz>
      <ekz>
        <tld/>aj pizoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="jun.0a.nematura">
    <dif>
      Ne havanta ankora&ubreve; la kvalitojn de matura a&gcirc;o:
      <ekz>
        <tld/>a vino, bovido
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lakt.0o">lakto</ref>,
      <ref cel="sxel.0o">&scirc;elo</ref>
    </refgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="nov.0a">nova</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_ju;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_a;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">mlad&yacute;</trd>
  <trd lng="de">jung</trd>
  <trd lng="en">young</trd>
  <trd lng="es">joven</trd>
  <trd lng="fr">jeune<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="he">&tsadi;&ayin;&yod;&resh;</trd>
  <trdgrp lng="hu">
    <trd>fiatal</trd>,
    <trd>ifj&uacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>giovane</trd>,
    <trd>nuovo</trd>
  </trdgrp>
  <trd lng="nl">jong</trd>
  <trdgrp lng="pl">
    <trd>m&lstroke;ody</trd>,
    <trd>m&lstroke;odociany</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>jovem</trd>,
    <trd>novo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_o;&c_j;</trd>,
    <trd>&c_ju;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">ung</trd>
  <trd lng="vo">yunik</trd>
</drv>

<drv mrk="jun.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Tempo de la vivo inter la infaneco kaj matura a&gcirc;o:
      <ekz>
        en sia plej delikata <tld/>eco;
      </ekz>
      <ekz>
        afekti la <tld/>econ;
      </ekz>
      <ekz>
        la <tld/>eco rapide forpasas;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio estis ankora&ubreve; la <tld/>eco de la mondo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ju;&c_n;&c_a;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_a;&c_d;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Jugend</trd>
  <trd lng="es">juventud</trd>
  <trd lng="fr">jeunesse<klr tip="amb"> (temps de la vie)</klr></trd>
  <trd lng="he">&nun;&ayin;&vav;&resh;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>fiatals&aacute;g</trd>,
    <trd>ifj&uacute;s&aacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>giovent&ugrave;</trd>,
    <trd>giovanezza</trd>,
    <trd>giovinezza</trd>
  </trdgrp>
  <trd lng="nl">jeugd</trd>
  <trd lng="pl">m&lstroke;odo&sacute;&cacute;</trd>
  <trd lng="pt">juventude</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ju;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">ungdom</trd>
</drv>

<drv mrk="jun.0ulo">
  <kap><ofc>*</ofc><tld/>ulo</kap>
  <snc>
    <dif>
      <tld lit="J"/>a viro:
      <ekz>
        la <tld/>ulo ali&gcirc;is al nia militistaro
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ulaj jaroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o aran&gcirc;is festenon kaj invitis &ccirc;iujn belajn <tld/>ulinojn de
        la tuta lando
        <fnt><bib>ElektFab</bib>, <lok>Cindrulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>jeune <klr tip="amb"> (subst., jeune homme)</klr></trd>,
    <trd>jeune homme</trd>
  </trdgrp>
</drv>

<drv mrk="jun.0ularo">
  <kap><tld/>ularo</kap>
  <snc>
    <dif>
      &Ccirc;iuj <tld/>uloj a&ubreve; <tld/>ulinoj:
      <ekz>
        pli bone estos, se tion faros Luka Luki&ccirc;, kiel kleriganto de la
        <tld/>ularo
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">jeunesse<klr tip="amb"> (ensemble des jeunes)</klr></trd>
</drv>

<drv mrk="jun.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc>
    <dif>
      Granda&gcirc;a; ekzistanta jam de relative longa tempo:
      <ekz>
        tiu &ccirc;i patrino varmege amis sian pli mal<tld/>an filinon
        <fnt><bib>F</bib>, <lok>&FE; 13</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a avo, amiko, vino, amikeco, hundo, arbo, monumento;
      </ekz>
      <ekz>
        aspekti kiel mal<tld/>a pomo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>ulaj faltoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eco ne scias, mal<tld/>eco ne povas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la &ccirc;agrenoj mal<tld/>igas<fnt>B</fnt>;
      </ekz>
      <ekz>
        la nacioj mal<tld/>i&gcirc;as kiel homoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arkaik.0a">arkaika</ref>,
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="nov.mal0a">malnova</ref>,
      <ref cel="ruin.0a">ruina</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y;</trd>
  <trd lng="de">alt</trd>
  <trd lng="es">viejo</trd>
  <trd lng="fr">vieux</trd>
  <trd lng="he">&zayin;&qof;&fnun;</trd>
  <trd lng="it">vecchio</trd>
  <trd lng="nl">oud</trd>
  <trd lng="pl">stary</trd>
  <trd lng="pt">velho</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_i;&c_l;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">gammal</trd>
  <trd lng="vo">b&auml;ledik</trd>
</drv>

<drv mrk="jun.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc>
    <dif>
      Stato de pli a&gcirc;a ulo:
      <ekz>
        &gcirc;is la mal<tld/>eco kaj grizeco ne forlasu min
        <fnt><bib>MT</bib>, <lok>&Psa; 71:18</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vieillesse</trd>
</drv>

<drv mrk="jun.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      La&ubreve; la tempo i&gcirc;i mal<tld/>a:
      <ekz>
        kiam vi mal<tld/>i&gcirc;os, vi etendos viajn manojn, kaj alia vin zonos
        <fnt><bib>NT</bib>, <lok>&Joh; 21:18</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0igxi">kaduki&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="fr">vieillir</trd>
</drv>

<drv mrk="jun.mal0ulo">
  <kap><ofc>*</ofc>mal<tld/>ulo</kap>
  <snc>
    <dif>
      Mal<tld/>a, granda&gcirc;a viro:
      <ekz>
        tiu &ccirc;i mal<tld/>ulo tute malsa&gcirc;i&gcirc;is kaj infani&gcirc;is
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        la mal<tld/>ulino staris anta&ubreve; la pordo
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>vieil homme</trd>,
    <trd>vieux<klr tip="amb"> (subst., vieil homme)</klr></trd>
  </trdgrp>
</drv>
</art>
