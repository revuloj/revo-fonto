<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abdomen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="abdome.0o">
  <kap><tld/>o</kap>
  <snc mrk="abdome.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>, 
      inter la torako kaj la pelvo, kiu entenas 
      la intestojn.
    </dif>
  <trdgrp lng="pl">
  	<trd>abdomen</trd>,
    <trd>brzuch</trd>
  </trdgrp>
  </snc>
  <snc mrk="abdome.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La malanta&ubreve;a parto de la korpo de diversaj 
      senvertebruloj.
    </dif>
    <ref tip="vid" cel="torak.0o">torako</ref>
    <trd lng="pl">odw&lstroke;ok</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_sh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_sh;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_r;&c_je;&c_m;</trd>
    <trd>&c_k;&c_o;&c_r;&c_je;&c_m;&c_ch;&c_je;</trd>
  </trdgrp>
  <trd lng="br">abdomen</trd>
  <trdgrp lng="de">
    <trd>Unterleib</trd>,
    <trd>Bauch</trd>,
    <trd>Abdomen</trd>
  </trdgrp>
  <trd lng="ca">abdomen</trd>
  <trd lng="en">abdomen</trd>
  <trd lng="es">abdomen</trd>
  <trd lng="fr">abdomen</trd>
  <trd lng="hu">has</trd>
  <trd lng="it">addome</trd>
  <trd lng="nl">abdomen</trd>
  <trdgrp lng="pt">
    <trd>abd&ocirc;men</trd>,
    <trd>abdome</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_ju;&c_sh;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">brucho</trd>
  <trdgrp lng="sv">
    <trd>mage</trd>,
    <trd>buk</trd>,
    <trd>abdomen</trd>
  </trdgrp>
</drv>
</art>
