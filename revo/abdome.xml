<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abdomen</rad>/o
</kap>
<drv mrk="abdome.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>, 
      inter la <ref tip="vid" cel="torak.0o">torako</ref> 
      kaj la pelvo, kiu entenas 
      la <ref tip="vid" cel="intest.0o">intestojn</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_sh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_sh;&c_n;&c_a;&c_ja;
&c_p;&c_o;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">abdomen</trd>
  <trdgrp lng="de">
    <trd>Unterleib</trd>,
    <trd>Bauch</trd>,
    <trd>Abdomen</trd>
  </trdgrp>
  <trd lng="ca">abdomen</trd>
  <trd lng="en">abdomen</trd>
  <trd lng="es">abdomen</trd>
  <trd lng="fr">abdomen</trd>
  <trd lng="hu">has</trd>
  <trd lng="it">addome</trd>
  <trd lng="nl">abdomen</trd>
  <trdgrp lng="pl">
  	<trd>abdomen</trd>,
    <trd>brzuch</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abd&ocirc;men</trd>,
    <trd>abdome</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_ju;&c_sh;&c_n;&c_a;&c_ja;
&c_p;&c_o;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">brucho</trd>
  <trdgrp lng="sv">
    <trd>mage</trd>,
    <trd>buk</trd>,
    <trd>abdomen</trd>
  </trdgrp>
</drv>
<drv mrk="abdome.0o.ZOO">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      La malanta&ubreve;a parto de la korpo de diversaj 
      <ref tip="super" cel="">senvertebruloj</ref>.
    </dif>
    <ref tip="vid" cel="torak.0o">torako</ref>
  </snc>
  <trd lng="pl">odw&lstroke;ok</trd>
</drv>
</art>
