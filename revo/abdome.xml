<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abdomen</rad>/o
</kap>
<drv mrk="abdome.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la korpo, inter la torako kaj la pelvo, kiu entenas la
      intestojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Unterleib</trd>,
    <trd>Bauch</trd>,
    <trd>Abdomen</trd>
  </trdgrp>
  <trd lng="ca">abdomen</trd>
  <trd lng="en">abdomen</trd>
  <trd lng="es">abdomen</trd>
  <trd lng="fr">abdomen</trd>
  <trd lng="hu">has</trd>
  <trd lng="it">addome</trd>
  <trd lng="nl">abdomen</trd>
  <trd lng="pl">brzuch</trd>
  <trdgrp lng="pt">
    <trd>abd&ocirc;men</trd>,
    <trd>abdome</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_ju;&c_sh;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">brucho</trd>
  <trdgrp lng="sv">
    <trd>mage</trd>,
    <trd>buk</trd>,
    <trd>abdomen</trd>
  </trdgrp>
</drv>
</art>
