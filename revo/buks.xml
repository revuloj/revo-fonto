<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>buks</rad>/o
</kap>
<drv mrk="buks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="buksac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Buxus</trd>), kun flave blankaj floroj, ofte uzata
      por borderado de florbedoj, kaj kies flavkolora, malmolega ligno
      estas uzata por torna&jcirc;oj, inkrusta&jcirc;oj k.s.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_sh;&c_y;&c_t;</trd>
  <trd lng="de">Buchsbaum</trd>
  <trd lng="en">boxwood</trd>
  <trd lng="fr">buis</trd>
  <trdgrp lng="hu">
    <trd>puszp&aacute;ng</trd>,
    <trd>bukszus</trd>
  </trdgrp>
  <trd lng="pl">bukszpan</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_sh;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;
    &c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>

</art>
