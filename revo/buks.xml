<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buks.xml,v 1.23 2023/08/31 08:48:54 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>buks</rad>/o
</kap>
<drv mrk="buks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="buksac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Buxus</trd>), kun flave blankaj floroj, ofte uzata
      por borderado de florbedoj, kaj kies flavkolora, malmolega ligno
      estas uzata por torna&jcirc;oj, inkrusta&jcirc;oj k.s.:
      <ekz>
        cipreso, abio, kaj <tld/>o kuni&gcirc;os, por ornami la lokon de Mia sanktejo
        <fnt>
          <bib>MT</bib>
          <lok><url ref="&Steloj;biblio/">Jesaja 60:13</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        viajn benkojn ili faris el eburo kaj el <tld/>o de la insuloj de la Kitidoj
        <fnt>
          <bib>MT</bib>
          <lok><url ref="&Steloj;biblio/">Je&hcirc;ezkel 27:6</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kresko povas esti tre malrapida kaj la arbedo povas vivi dum pluraj jarcentoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=140602">Bukso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/12/Gew%C3%B6hnlicher_Buchsbaum_mit_Bl%C3%BCtekn%C3%A4uel.JPG/360px-Gew%C3%B6hnlicher_Buchsbaum_mit_Bl%C3%BCtekn%C3%A4uel.JPG" prm="CC BY-SA 3.0">ordinara <tld/>o (Buxus sempervirens) kun floraroj
        <fnt>
          <aut>3268zauber, propra verko</aut>
          <lok><url ref="&WCW;/File:Gew%C3%B6hnlicher_Buchsbaum_mit_Bl%C3%BCtekn%C3%A4uel.JPG">&WCW;/File:Gew%C3%B6hnlicher_Buchsbaum_mit_Bl%C3%BCtekn%C3%A4uel.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_sh;&c_y;&c_t;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_m;&c_sh;&c_i;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>buxus</trd>,
    <trd>zimostr&aacute;z <klr>(pu&scaron;pan kru&scaron;p&aacute;nek)</klr></trd>
  </trdgrp>
  <trd lng="de">Buchsbaum</trd>
  <trdgrp lng="en">
    <trd>box<klr tip="amb"> (tree)</klr></trd>,
    <trd>boxwood</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_w;&a_b; &a_sh;&a_m;&a_sh;&a_A;&a_d;</trd>
  <trd lng="fr">buis</trd>
  <trd lng="he">&#x5E1;&#x5D5;&#x5D2; &#x5E9;&#x5D9;&#x5D7;</trd>
  <trdgrp lng="hu">
    <trd>puszp&aacute;ng</trd>,
    <trd>bukszus</trd>
  </trdgrp>
  <trd lng="ja">&#x30C4;&#x30B2;</trd>
  <trd lng="nl">palmhout</trd>
  <trd lng="pl">bukszpan</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_sh;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;
    &c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">boler&aacute;z</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x430;&#x43C;&#x448;&#x438;&#x442;</trd>,
    <trd>&#x431;&#x443;&#x43A;&#x441;&#x443;&#x441;</trd>,
    <trd>&#x431;&#x443;&#x43A;&#x448;&#x43F;&#x430;&#x43D;</trd>
  </trdgrp>
</drv>

</art>
