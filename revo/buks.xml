<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>buks</rad>/o
</kap>
<drv mrk="buks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="buks.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Buxus</trd>), kun flave blankaj floroj, ofte uzata
      por borderado de florbedoj, kaj kies flavkolora, malmolega ligno
      estas uzata por torna&jcirc;oj, inkrusta&jcirc;oj k.s.
    </dif>
  </snc>
  <trd lng="de">Buchsbaum</trd>
  <trd lng="fr">buis</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_sh;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;
    &c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="buks.0acoj"> 
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Buxaceae</trd>), &ccirc;iamverdaj arbetoj.
    </dif>
  </snc>
  <trd lng="de">Buchsbaumgew&auml;chse</trd>
  <trd lng="fr">buxac&eacute;es</trd>
</drv>
</art>
