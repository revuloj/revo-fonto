<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>taban</rad>/o
</kap>
<drv mrk="taban.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="taban.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Tabanus</trd>), granda mu&scirc;simila insekto, kies      
      inoj pikas prefere la brutojn.
    </dif>
  </snc>
  <trd lng="de">Bremse</trd>
</drv>
<drv mrk="taban.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Tabanidae</trd>), kies inoj su&ccirc;as sangon.
    </dif>
  </snc>
  <trd lng="de">Bremsen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
