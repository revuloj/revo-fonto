<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>taban</rad>/o
</kap>
<drv mrk="taban.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="tabane.0oj"><tld/>edoj</ref>
      (<trd lng="la">Tabanus</trd>), granda mu&scirc;simila insekto, kies
      inoj pikas prefere la brutojn:
      <ekz>
        &gcirc;ena&jcirc;oj en somerdoma feli&ccirc;o estas kuloj, mu&scirc;oj, formikoj kaj foje
        <tld/>oj<fnt><aut>Saliko</aut>: <vrk>&Gcirc;ojoj kaj &gcirc;uoj de finna
        somero</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 12a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Bremse</trd>
  <trdgrp lng="fr">
    <trd>taon</trd>
  </trdgrp>
  <trd lng="hu">b&ouml;g&ouml;ly</trd>
  <trd lng="nl">horzel</trd>
  <trdgrp lng="pt">
   <trd>tav&atilde;o</trd>,
    <trd>mutuca</trd>
    <trd>butuca</trd>,
    <trd>moscardo</trd>,
    <trd>motuca</trd>
   </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">broms</trd>
</drv>
</art>
