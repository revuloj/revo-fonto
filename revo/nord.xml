<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nord.xml,v 1.74 2021/06/19 06:38:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nord</rad>/o
</kap>

<drv mrk="nord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="nord.0o.horizonta">
    <dif>
      Unu el la kvar geografiaj &ccirc;efpunktoj, parto de la mondo, kie
      trovi&gcirc;as la polusa stelo.
    </dif>
  </snc>
  <snc mrk="nord.0o.absoluta">
    <dif>
      Parto de la terglobo ku&scirc;anta en direkto al la polusa stelo:
      <ekz>
        rigardi <ind><tld/>en</ind><fnt>Z</fnt>.
        <trd lng="ca">cap al nord</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="af">noord</trd>
  <trd lng="am">&#x1230;&#x121C;&#x1295;</trd>
  <trd lng="ar">&#x634;&#x645;&#x627;&#x644;</trd>
  <trd lng="az">&#x15F;imal</trd>
  <trd lng="be">&c_p;&c_o;&c_w;&c_n;&c_a;&c_ch;</trd>
  <trd lng="bn">&#x989;&#x9A4;&#x9CD;&#x9A4;&#x9B0;</trd>
  <trd lng="bo">&#xF56;&#xFB1;&#xF44;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>hanternoz<klr tip="ind"> (norzh)</klr></trd>,
    <trd>norzh</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>nord</trd>,
    <trd>septentri&oacute;</trd>
  </trdgrp>
  <trd lng="co">nordu</trd>
  <trd lng="cs">sever</trd>
  <trd lng="da">nord</trd>
  <trd lng="de">Norden</trd>
  <trd lng="el">&beta;&omikron;&rho;&rho;&alfa_ton;&sigma_fina;</trd>
  <trd lng="en">north</trd>
  <trd lng="es">norte</trd>
  <trd lng="et">p&#xF5;hja</trd>
  <trd lng="eu">iparraldean</trd>
  <trd lng="fr">nord</trd>
  <trd lng="fy">noard</trd>
  <trd lng="gl">norte</trd>
  <trd lng="gu">&#xA89;&#xAA4;&#xACD;&#xAA4;&#xAB0;</trd>
  <trd lng="hi">&#x909;&#x924;&#x94D;&#x924;&#x930;</trd>
  <trd lng="hr">sjeverno</trd>
  <trd lng="hu">&eacute;szak</trd>
  <trd lng="hy">&#x570;&#x575;&#x578;&#x582;&#x57D;&#x56B;&#x57D;</trd>
  <trd lng="id">utara</trd>
  <trd lng="is">nor&#xF0;ur</trd>
  <trd lng="ja">&#x5317;</trd>
  <trd lng="jw">lor</trd>
  <trd lng="ka">&#x10E9;&#x10E0;&#x10D3;&#x10D8;&#x10DA;&#x10DD;&#x10D4;&#x10D7;</trd>
  <trd lng="km">&#x1797;&#x17B6;&#x1782;&#x1781;&#x17B6;&#x1784;&#x1787;&#x17BE;&#x1784;</trd>
  <trd lng="ko">&#xBD81;</trd>
  <trd lng="lo">&#xE9E;&#xEB2;&#xE81;&#xEC0;&#xEAB;&#xE99;&#xEB7;&#xEAD;</trd>
  <trd lng="ml">&#xD35;&#xD1F;&#xD15;&#xD4D;&#xD15;&#xD4D;</trd>
  <trd lng="mr">&#x909;&#x924;&#x94D;&#x924;&#x930;&#x947;&#x91A;&#x93E;</trd>
  <trd lng="my">&#x1019;&#x103C;&#x1031;&#x102C;&#x1000;&#x103A;&#x1018;&#x1000;&#x103A;&#x101E;&#x102D;&#x102F;&#x1037;</trd>
  <trd lng="ne">&#x909;&#x924;&#x94D;&#x924;&#x930;</trd>
  <trd lng="nl">noorden</trd>
  <trd lng="ny">kumpoto</trd>
  <trd lng="pa">&#xA09;&#xA71;&#xA24;&#xA30;</trd>
  <trd lng="pl">p&oacute;&lstroke;noc</trd>
  <trd lng="ps">&#x634;&#x645;&#x627;&#x644;</trd>
  <trd lng="pt">norte</trd>
  <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;</trd>
  <trd lng="sd">&#x627;&#x62A;&#x631;</trd>
  <trd lng="si">&#xD8B;&#xDAD;&#xDD4;&#xDBB;&#xDD4;</trd>
  <trd lng="sk">sever</trd>
  <trd lng="sq">veri</trd>
  <trd lng="st">ka leboea</trd>
  <trd lng="ta">&#xBB5;&#xB9F;&#xB95;&#xBCD;&#xB95;&#xBBF;&#xBB2;&#xBCD;</trd>
