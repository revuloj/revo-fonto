<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nord</rad>/o
</kap>
<drv mrk="nord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="nord.0o.horizonta">
    <dif>
      Unu el la kvar geografiaj &ccirc;efpunktoj, parto de la mondo, kie
      trovi&gcirc;as la polusa stelo.
    </dif>
  </snc>
  <snc mrk="nord.0o.absoluta">
    <dif>
      Parto de la terglobo ku&scirc;anta en direkto al la polusa stelo:
      <ekz>
        rigardi <tld/>en<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_w;&c_n;&c_a;&c_ch;</trd>
  <trd lng="cs">sever</trd>
  <trd lng="de">Norden</trd>
  <trd lng="el">&beta;&omikron;&rho;&rho;&alfa_ton;&sigma_fina;</trd>
  <trd lng="en">north</trd>
  <trd lng="es">norte</trd>
  <trd lng="fr">nord</trd>
  <trd lng="hu">&eacute;szak</trd>
  <trd lng="nl">noorden</trd>
  <trd lng="pl">p&oacute;&lstroke;noc</trd>
  <trd lng="pt">norte</trd>
  <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;</trd>
  <trd lng="vo">nol&uuml;d</trd>
  <trdgrp lng="zh">
    <trd>&#21271;<klr> [b&ecaron;i]</klr></trd>,
    <trd>&#21271;&#26041;<klr> [b&ecaron;if&#257;ng]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="nord.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Situanta en <tld/>o a&ubreve; de tie vena:
      <ekz>
        la monto Cion, en la <tld/>a regiono, La urbo de la granda Re&gcirc;o
        <fnt><bib>MT</bib>, <lok>&Psa; 48:2</lok></fnt>;
      </ekz>
      <ekz>
        <ctl>Norda naturo</ctl>
        <klr>(poemaro de Hilda Dresen)</klr>
        <fnt><aut>Hilda Dresen</aut>, <vrk>Norda naturo. - La Laguna:
        Stafeto, 1967.</vrk></fnt>;
      </ekz>
      <ekz>
        <tld/>a vento;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bor&eacute;al</trd>,
    <trd>du <ind>nord</ind></trd>,
    <trd>septentrional</trd>
  </trdgrp>
  <trd lng="hu">&eacute;szaki</trd>
  <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="nord.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="nord.0ede.en">
    <dif>
      <ref tip="super" cel="en.0.kie">En</ref> la <tld/>a parto de.
    </dif>
    <trd lng="hu">&eacute;szakon</trd>
    <trd lng="ru">&c_n;&c_a; <ind>&c_s;&c_je;&c_v;&c_je;&c_r;&c_je;</ind><klr tip="amb"> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </snc>
  <snc mrk="nord.0ede.ekster">
    <dif>
      <ref tip="super" cel="ekster.0.kie">Ekster</ref>, pli <tld/>e ol:
      <ekz>
        Belgujo ku&scirc;as <ind><tld/>e de Francujo</ind>;
        <trd lng="hu">Franciaorsz&aacute;gt&oacute;l <ind>&eacute;szakra</ind></trd>
      </ekz>
      <ekz>
        la Orkadoj &ndash; insuloj, situantaj norde de Skotlando
        <fnt>
        <aut>W. H. Simcock</aut>,
        <vrk>Vizito al la nordaj insuloj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 9a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&eacute;szakra</trd>
    <trdgrp lng="ru">
      <trd>&c_k; <ind>&c_s;&c_je;&c_v;&c_je;&c_r;&c_u;</ind> &c_o;&c_t;</trd>,
      <trd>&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_je;&c_je;<klr tip="amb"> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="fr">au nord de</trd>
</drv>

<drv mrk="nord.N0aMaro">
  <kap><tld lit="N"/>a Maro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="atlant.0o">Atlantiko</ref> limanta
      al nordokcidenta <ref tip="vid" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj orienta bordo de <ref tip="vid" cel="brit.B0ujo">Britujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Nordsee</trd>
  <trd lng="fr">Mer du nord</trd>
  <trd lng="hu">&Eacute;szaki-tenger</trd>
  <trd lng="ru">&c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
</drv>
</art>
