<drv mrk="nord.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Situanta en <tld/>o, tien montranta a&ubreve; de tie vena:
      <ekz>
        la monto Cion, en la <tld/>a regiono, la urbo de la granda Re&gcirc;o
        <fnt><bib>MT</bib>, <lok>&Psa; 48:2</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a vento</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:23</lok>
        </fnt>.
        <trdgrp lng="ca">
          <trd>tramuntana</trd>,
          <trd>aquil&oacute;</trd>,
          <trd>b&ograve;rees</trd>
        </trdgrp>
        <trd lng="de">Nordwindy</trd>
        <trd lng="nl">noorderwind</trd>
      </ekz>
      <ekz>
        la limo <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 34:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a pordego
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 40:44</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>a flanko la&ubreve;longe estu kurtenoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 27:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>Norda naturo</ctl>
        <klr>(poemaro de <nom>Hilda Dresen</nom>)</klr>
        <fnt><aut>Hilda Dresen</aut>, <vrk>Norda naturo. - La Laguna:
        Stafeto, 1967.</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>hanternoz<klr tip="ind"> (norzh)</klr></trd>,
    <trd>norzh</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>septentrional</trd>,
    <trd>boreal</trd>
  </trdgrp>
  <trd lng="cs">seversk&yacute;</trd>
  <trd lng="de">n&ouml;rdlich</trd>
  <trdgrp lng="fr">
    <trd>bor&eacute;al</trd>,
    <trd>du <ind>nord</ind></trd>,
    <trd>septentrional</trd>
  </trdgrp>
  <trd lng="hu">&eacute;szaki</trd>
  <trd lng="nl">noordelijk</trd>
  <trd lng="pl">p&oacute;&lstroke;nocny</trd>
  <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">seversk&yacute;</trd>
  <trd lng="uk">&c_p;&c_ib;&c_v;&c_n;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="nord.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="nord.0ede.ekster">
    <dif>
      <ref tip="super" cel="ekster.0.kie">Ekster</ref>, pli <tld/>e ol:
      <ekz>
        Belgujo ku&scirc;as <ind><tld/>e de Francujo</ind>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> starigis la tutan tendaron <klr>[&#8230;]</klr> <tld/>e de la urbo
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 8:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Orkadoj &ndash; insuloj, situantaj norde de Skotlando
        <fnt>
        <aut>W. H. Simcock</aut>,
        <vrk>Vizito al la nordaj insuloj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 9a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>                                                  
    <trdgrp lng="br">
      <trd>en <ind>hanternoz</ind> da</trd>,            
      <trd>en <ind>norzh</ind> da</trd>
    </trdgrp>
    <trd lng="de">n&ouml;rdlich von</trd>
    <trd lng="hu"><klr tip="amb">(vmit&odblac;l)</klr> &eacute;szakra</trd>
    <trd lng="nl">ten het <ind>noorden</ind> van</trd>
    <trd lng="pl">na <ind>p&oacute;&lstroke;noc</ind> od</trd>
    <trdgrp lng="ru">
      <trd>&c_k; <ind>&c_s;&c_je;&c_v;&c_je;&c_r;&c_u;</ind> &c_o;&c_t;</trd>,
      <trd>&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_je;&c_je;<klr tip="amb"> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="nord.0ede.en">
    <dif>
      <ref tip="super" cel="en.0.kie">En</ref> la <tld/>a parto de:
      <ekz>
        preska&ubreve; okcent kilometroj la&ubreve; malnovaj romiaj vojoj <tld/>e de Hispanio
        <klr>[&#8230;]</klr> tra la aragonaj kaj tra la navaraj Pirineoj
        <fnt>
          <bib>Monato</bib>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006809.html"
              >Hispanio: Vojoj al la sankta urbo</url></vrk>
        </fnt>.
      </ekz>
    </dif>                                         
    <trdgrp lng="br">
      <trd>e <ind>hanternoz</ind></trd>,            
      <trd>e <ind>norzh</ind></trd>
    </trdgrp>
    <trd lng="de">im <ind>Norden</ind> von</trd>
    <trd lng="hu"><klr tip="amb">(vmi)</klr> &eacute;szaki r&eacute;sz&eacute;n</trd>
    <trd lng="nl">in het <ind>noorden</ind> van</trd>
    <trd lng="pl">na <ind>p&oacute;&lstroke;nocy</ind></trd>
    <trd lng="ru">&c_n;&c_a; <ind>&c_s;&c_je;&c_v;&c_je;&c_r;&c_je;</ind><klr tip="amb">
    (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a; <ind>&c_p;&c_o;&c_w;&c_n;&c_a;&c_ch;</ind> &c_a;&c_d;&#8230;</trd>,
    <trd>&c_n;&c_a; <ind>&c_p;&c_o;&c_w;&c_n;&c_a;&c_ch;&c_y;</ind> &c_a;&c_d;&#8230;</trd>
  </trdgrp>
  <trd lng="ca">al nord de</trd>
  <trd lng="fr">au nord de</trd>
</drv>

