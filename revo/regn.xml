<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>regn</rad>/o
</kap>
<drv mrk="regn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="regn.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> kondukanta sian
      propran, sendependan eksteran politikon, subjekto de la
      internacia juro:
      <ekz>
        Polujo, kiu sub la regado de la rusaj caroj estis nur lando kaj
        nacio, nun refari&gcirc;is <tld/>o;
      </ekz>
      <ekz>
        tute&ubreve;ropa organizo de la <tld/>oj;
      </ekz>
      <ekz>
        la ligo de la <tld/>oj;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naci.0o">nacio</ref>,
      <ref cel="land.0o">lando</ref>,
      <ref cel="sxtat.0o">&scirc;tato</ref>.
    </refgrp>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_v;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>&aacute;llam</trd>,
      <trd>birodalom</trd></trdgrp>
    <trd lng="ru">&c_d;&c_je;&c_r;&c_zh;&c_a;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="regn.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Loko en la spaco a&ubreve; en hierarkio de ideoj kie regas iu
      specio, principo ktp:
      <ekz>
        la <tld/>o de belo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne en la akvo estas la <tld/>o de la leono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KRI</uzo>
        en tiuj tagoj venis Johano, la Baptisto, predikante en la
        dezerto de Judujo, kaj dirante: Pentu, &ccirc;ar alproksimi&gcirc;is
        la <ind><tld/>o de la &ccirc;ielo</ind>
        <fnt><bib>NT</bib>, <lok>&Mat; 3:1-2.</lok></fnt>.
        <trd lng="ru">&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o; &c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;&c_je;</trd>
        la regno de la mortintoj
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
        regno de son&gcirc;oj
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>birodalom</trd>,
      <trd>orsz&aacute;g</trd></trdgrp>
    <trd lng="ru">&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="regn.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Iu el la plej &gcirc;eneralaj grupoj en la biologia klasifiko, ekz.
      animaloj, fungoj, plantoj.
    </dif>
    <refgrp tip="prt">
      <ref cel="filum.0o">filumo</ref>,
      <ref cel="divizi.0o.BOT">divizio</ref>.
    </refgrp>
    <trd lng="be">&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a; <klr>(&c_zh;&c_y;&c_v;&c_y;&c_h; &c_ib;&c_s;&c_t;&c_o;&c_t;&c_a;&c_w;)</klr></trd>
    <trd lng="hu">orsz&aacute;g<klr tip="ind"> (rendszertani)</klr></trd>
    <trd lng="ru">&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o; <klr>(&c_b;&c_i;&c_o;&c_l;.)</klr></trd>
  </snc>
  <trd lng="de">Reich</trd>
  <trd lng="en">kingdom</trd>
  <trd lng="es">reino</trd>
  <trd lng="nl">rijk</trd>
 </drv>

<drv mrk="regn.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Apartenanta al la <tld/>o, koncernanta la <tld/>on:
      <ekz>
        la <tld/>a kaso.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>&aacute;llami</trd>,
    <trd>birodalmi</trd>
  </trdgrp>
  <trd lng="nl">rijks-</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="regn.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="regn.0ano.POL">
    <ref tip="dif" cel="sxtat.0ano">&scirc;tatano</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>citoyen</trd>,
    <trd>habitant<klr> (d'un pays)</klr></trd>
  </trdgrp>
  <trd lng="hu">&aacute;llampolg&aacute;r</trd>
  <trd lng="nl">staatsburger</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n;</trd>
</drv>

<drv mrk="regn.0estro">
  <kap><ofc>*</ofc><tld/>estro</kap>
  <snc mrk="regn.0estro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tiu, kiu &ccirc;efe gvidas la politikon de <tld/>o:
      <ekz>
        la <tld/>estro de nia lando estas bona kaj sa&gcirc;a re&gcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>chef d'&eacute;tat</trd>,
    <trd>souverain</trd>
  </trdgrp>
</drv>
</art>
