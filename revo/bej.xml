<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bej.xml,v 1.8 2023/06/09 13:29:42 revo Exp $">
<kap>
  <rad>bej</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bej.0o">
  <kap><tld/>o</kap>
  <snc mrk="bej.0o.RANGO">
    <uzo tip="fak">POL</uzo>
    <dif>
      Turkdevena titolo de landestro,
      poste uzata kiel honora alnomo
      malpli alta ol tiu de
      <ref tip="vid" cel="pasxa.0o.POL">pa&scirc;ao</ref>:
      <ekz>
        la <tld/>o estis gardokaptita sur la insulo Santa Cruz
        <fnt>
          <aut>J. Rejt&otilde;, trad. L. Bal&aacute;zs</aut>,
          <vrk><url ref="http://www.freeweb.hu/verkoj/13.htm">
          Mar&scirc;i a&ubreve; morti</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_e;&c_j;</trd>
  <trd lng="ca">bei</trd>
  <trd lng="fr">bey</trd>
  <trd lng="pl">bej</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bej</trd>,
    <trd>bey</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5D5;&#x5E9;&#x5DC; &#x5EA;&#x5D5;&#x5E8;&#x5DB;&#x5D9;</trd>
  <trd lng="sk">bej</trd>
  <trd lng="uk">&c_b;&c_je;&c_j;</trd>
</drv>
</art>

