<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kurkuli</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kurkul.0o">
  <kap><tld/>o</kap>
  <snc mrk="kurkul.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda insekto kun longa rostro,
      kiu ovas en nematurajn nuksojn, grenojn...
      genro el familio <ref tip="super"
      cel="kurkul1.0oj"><tld/>edoj</ref>,
      (<trd lng="la">Curculio</trd>):
      <ekz>
        riz- kaj maiz-sakoj kun <tld/>oj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_w;&c_g;&c_a;&c_n;&c_o;&c_s;&c_ib;&c_k;</trd>
  <trd lng="de">Haselnussbohrer</trd>
  <trd lng="en">curculio</trd>
  <trd lng="es">gorgojo</trd>
  <trd lng="fr">charen&ccedil;on</trd>
  <trd lng="hu">mogyor&oacute;zsuzs&oacute;k</trd>
  <trd lng="nl">snuitkever</trd>
  <trd lng="pt">gorgulho</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;</trd>
</drv>
</art>
