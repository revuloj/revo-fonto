<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kurkuli</rad>/o
</kap>
<drv mrk="kurkul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kurkul.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Curculio</trd>) kun longa rostro kaj
      kiu metas la ovojn en nematurajn nuksojn.
    </dif>
  </snc>
  <trd lng="de">Haselnu&szlig;bohrer</trd>
</drv>
<drv mrk="kurkul.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Curculionidae</trd>), kun rostre longigita kapo.
    </dif>
  </snc>
  <trd lng="de">R&uuml;sselk&auml;fer</trd>
</drv>
</art>
