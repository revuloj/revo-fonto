<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konekt</rad>/i
</kap>
<drv mrk="konekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Kunligi aparatojn, ma&scirc;inojn a&ubreve; elektrajn pecojn per drata
      kondukilo:
      <ekz>
        <tld/>i a&ubreve; mal<tld/>i kuplilon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komut.0i">komuti</ref>,
      <ref cel="kupl.0i">kupli</ref>,
      <ref cel="munt.0i">munti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>koppeln</trd>,
    <trd>kuppeln</trd>,
    <trd>verbinden</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatlakoztat</trd>,
    <trd>&ouml;sszekapcsol<klr tip="ind"> (villamosan)</klr></trd>
  </trdgrp>
  <trd lng="nl">verbinden</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; la rezulto de tiu ago:
      <ekz>
        a&ubreve;tomata <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>csatlakoz&aacute;s<klr tip="ind"> (villamos)</klr></trd>,
    <trd>&ouml;sszekapcsol&aacute;s<klr tip="ind"> (villamos)</klr></trd>,
    <trd>kapcsolat<klr tip="ind"> (villamos)</klr></trd>
  </trdgrp>
  <trd lng="nl">verbinding</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">TEK</uzo>
  <fnt><bib>PV</bib><lok>&jcirc;ako</lok></fnt>
  <snc>
    <dif>
      Ilo por <tld/>i. 
    </dif>
  </snc>
  <trd lng="en">connector</trd>
  <trd lng="fr">connecteur</trd>
</drv>

<drv mrk="konekt.0ite">
  <kap><tld/>ite</kap>
  <snc mrk="konekt.0ite.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="regxim.0o.TEK">Re&gcirc;imo</ref>
      de komputa sistemo a&ubreve; ties parto (ekz-e desegnilo)
      &ccirc;e kiu &gcirc;ian funkciadon stiras &ccirc;efa komputilo
      (= mala&ubreve;tonome).
    </dif>
    <ref tip="ant" cel="konekt.sen0e">senkonekte</ref>
    <rim>
      Oni anka&ubreve; aplikas tiun vorton al homo uzanta komputilon
      <tld/>itan al la <ref cel="ret.inter0o.la">Interreto</ref>:
      <ekz>
        hiera&ubreve; &scirc;i pasigis la tutan tagon <tld/>ite
        (a&ubreve; sen<tld/>e).
      </ekz>
    </rim>
  </snc>
  <trd lng="en">on-line</trd>
  <trd lng="fr"><klr>(mode)</klr> <ind>connect&eacute;</ind></trd>
  <trdgrp lng="hu">
    <trd>csatlakozva</trd>,
    <trd>on-line</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_p;&c_je;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; <klr>(&c_r;&c_je;&c_zh;&c_i;&c_m;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="konekt.sen0e">
  <kap>sen<tld/>e</kap>
  <snc mrk="konekt.sen0e.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="regxim.0o.TEK">Re&gcirc;imo</ref>
      de aparato funkcianta sendepende je la &ccirc;efa
      komputilo.
    </dif>
    <ref tip="sin" cel="auxton.0a.TEK">a&ubreve;tonome</ref>
  </snc>
  <trd lng="en">off-line</trd>
  <trdgrp lng="hu">
    <trd>kapcsolat n&eacute;lk&uuml;l</trd>,
    <trd>off-line</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_n;&c_o;&c_m;&c_n;&c_o;</trd>
</drv>
</art>
