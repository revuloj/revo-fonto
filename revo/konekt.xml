    <trd>&c_o;&c_n;-&c_l;&c_a;&c_j;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5FC3;&#x6000;&#x611F;&#x6FC0; <pr>x&#x12B;nhu&#xE1;ig&#x1CE;nj&#x12B;</pr></trd>,
