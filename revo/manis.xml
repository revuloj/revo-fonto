<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: manis.xml,v 1.8 2022/09/23 07:49:08 revo Exp $">
<kap>
  <rad>manis</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="manis.0o">
  <kap><tld/>o</kap>
  <snc mrk="manis.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Skvama <ref tip="super" cel="mangx.formik0ulo.ZOO">formikman&gcirc;ulo</ref>,
      mamulo el tropikaj Afriko a&ubreve; Azio,
      genro el familio manisedoj (<trd lng="la">Manis</trd>):
      <ekz>
        la termitoj <klr>...</klr> reprodukti&gcirc;as rapide kaj volas 
        dama&gcirc;i per mordado &ccirc;iujn arbojn en unu nokto, tamen la 
        sendentaj <tld/>oj ne lasas ilin tion fari
        <fnt>
          <aut>Pang N.</aut>,
          <vrk>En Zhangjiajie</vrk>,
          <lok><url ref="http://english.china.org.cn/EL/EL/ElPopolaCxinio/">
          El popola &Ccirc;inio, 2000-01</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj unuope a&ubreve; duope vivas sur montetoj a&ubreve; ebena&jcirc;oj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;maniso&rightquot;, 
        2007-03-17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_g;&c_a;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_ja;&c_sh;&c_ch;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_v;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="ca">pangol&iacute;</trd>
  <trd lng="de">Schuppentier</trd>
  <trd lng="en">pangolins</trd>
  <trd lng="fr">pangolin</trd>
  <trd lng="it">pangolino</trd>
  <trd lng="nl">schubbendier</trd>
  <trd lng="pl">&lstroke;uskowiec</trd>
  <trd lng="ro">pangolin</trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&c_shch;&c_ib;&c_r;</trd>,
    <trd>&#x43F;&c_a;&#x43D;&c_g;&#x43E;&#x43B;&c_ib;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

