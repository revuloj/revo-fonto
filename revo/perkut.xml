<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perkut.xml,v 1.15 2024/11/25 16:36:13 revo Exp $">
<kap>
  <rad>perkut</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="perkut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="perkut.0i.ekzameni">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Frape ekzameni:
      <ekz>
          la apartamenton <klr>[&#8230;]</klr> oni zorge <klr>[&#8230;]</klr> traser&ccirc;is,
          <tld/>is &gcirc;iajn murojn, esploris la kamentubojn<fnt><bib>MkM</bib>,
          <lok>&ccirc;apitro 27a, p. 288a</lok></fnt>.
       </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>perkutieren</trd>,
      <trd>abklopfen <klr>(Medizin)</klr></trd>
    </trdgrp>
    <trd lng="pl">opuka&cacute;</trd>
  </snc>

  <snc mrk="perkut.0i.muziki">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Frape, ritme muziki; <ref tip="dif" cel="frap.0i.produktiSonon">frapi<sncref/></ref>:
       <ekz>
        la du rakontistinoj <klr>[&#8230;]</klr> <tld/>is jen rapide, jen lante,
        tute ritme
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 54a, volumo 2a, p.
        213a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">percudir</trd>
    <trd lng="de">trommeln <klr>(allgemein)</klr></trd>
    <trd lng="en">percuss</trd>
    <trd lng="fr">percuter</trd>
    <trdgrp lng="nl">
      <trd>slaan<klr tip="ind"> (drummen)</klr></trd>,
      <trd>drummen</trd>
    </trdgrp>
    <trd lng="pl">puka&cacute;</trd>
    <trdgrp lng="pt">
      <trd>percutir</trd>,
      <trd>bater</trd>
    </trdgrp>
    <trd lng="ro">bate</trd>
    <trdgrp lng="sk">
      <trd>poklepa&tcaron;</trd>,
      <trd>preklepa&tcaron;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>oklepat</trd>,
      <trd>poklepat</trd>,
      <trd>proklepat</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_u;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25171;&#25731;&#12434;&#x4E0E;&#12360;&#x308B;  <pr>&#12384;&#12370;&#x304D;&#12434;&#12354;&#x305F;&#12360;&#x308B;</pr></trd>,
    <trd>&#x885D;&#25731;&#12434;&#x4E0E;&#12360;&#x308B;  <pr>&#12375;&#12423;&#12358;&#12370;&#x304D;&#12434;&#12354;&#x305F;&#12360;&#x308B;</pr></trd>,
    <trd>&#25171;&#x8A3A;&#12377;&#x308B;  <pr>&#12384;&#12375;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&#x43A;&c_u;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="perkut.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="instru2.frap0o">Frapinstrumento</ref>:
      <ekz>
        la disko Civilizacio enhavas junularan rokmuzikon <klr>[&#8230;]</klr> akompanatan de <tld/>iloj,
        gitaro, piano, orgeno, saksofono
        <fnt>
          <bib>Monato</bib>
          <aut>pp/cs</aut>
          <vrk><url ref="&Monato;/2005/008543.php">Tiel ili protestas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_r;&c_n;&c_y; <ind>&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>Schlagzeug</trd>,
    <trd>Rhytmusinstrument</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25731;&#37444;  <pr>&#12370;&#x304D;&#12390;&#12388;</pr></trd>,
    <trd>&#25171;&#x8A3A;&#22120;  <pr>&#12384;&#12375;&#12435;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pl">instrument perkusyjny</trd>
  <trd lng="ro">instrument de percu&#x21B;ie</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_d;&c_a;&c_r;&#x43D;&c_i;&#x43A; (&c_u; &c_v;&#x43E;&c_g;&#x43D;&c_je;&#x43F;&c_a;&#x43B;&#x44C;&#x43D;&c_ib;&c_j; &c_z;&c_b;&c_r;&#x43E;&c_ji;)</trd>,
    <trd>&#x43F;&c_je;&c_r;&#x43A;&c_u;&c_s;&c_ib;&c_j;&#x43D;&c_i;&c_j; &#x43C;&#x43E;&#x43B;&#x43E;&c_t;&#x43E;&c_ch;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

</art>

