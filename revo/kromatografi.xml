<art mrk="$Id: kromatografi.xml,v 1.9 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>kromatografi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kromatografi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kromatografi.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Analizo de miksa&jcirc;o per konduko tra sinsekvaj
      substancoj, kiuj adsorbas a&ubreve; solvas &gcirc;iajn konsista&jcirc;ojn:
      <ekz>
        maldikatavola <tld/>o
        <fnt>
          <aut>J. Hradil</aut>,
          <vrk>Esperanta Medicina Terminaro</vrk>,
          <lok>1979</lok>
        </fnt>;
      </ekz>
      <ekz>
        la analizo de la likenaj substancoj estis farita per <tld/>o 
        sur silica &jcirc;elea&jcirc;o
        <fnt>
          <aut>C. Roux</aut>,
          <vrk><url ref="http://lichenologue.org/fichiers/docs/1987rouxprecipajlikensubstanc.pdf">
          Precipaj Likenaj Substancoj...</url></vrk>,
          <lok>Bull. Soc. linn. Provence, t. 38, 1986</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spektrometri.0o.FIZ">spektrometrio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_m;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="ca">cromatografia</trd>
  <trd lng="cs">chromatografie</trd>
  <trd lng="fr">chromatographie</trd>
  <trd lng="nl">chromatografie</trd>
  <trd lng="pl">chromatografia</trd>     
  <trd lng="ro">cromatografie</trd>
  <trd lng="sk">chromatografia</trd>
  <trd lng="uk">&#x445;&#x440;&#x43E;&#x43C;&#x430;&#x442;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x8272;&#x6790;&#x6CD5; <pr>s&#xE8;x&#x12B;f&#x1CE;</pr></trd>,
    <trd>&#x8272;&#x8B5C;&#x5206;&#x6790;&#x6CD5; <pr>s&#xE8;p&#x1D4;f&#x113;nx&#x12B;f&#x1CE;</pr></trd>,
    <trd>&#x8272;&#x8C31;&#x5206;&#x6790;&#x6CD5; <pr>s&#xE8;p&#x1D4;f&#x113;nx&#x12B;f&#x1CE;</pr></trd>,
    <trd>&#x8272;&#x5C42;&#x5206;&#x6790;&#x6CD5; <pr>s&#xE8;c&#xE9;ngf&#x113;nx&#x12B;f&#x1CE;</pr></trd>,
    <trd>&#x8272;&#x5C64;&#x5206;&#x6790;&#x6CD5; <pr>s&#xE8;c&#xE9;ngf&#x113;nx&#x12B;f&#x1CE;</pr></trd>
  </trdgrp>
</drv>
