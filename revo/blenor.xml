<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blenor.xml,v 1.14 2019/01/04 14:10:14 revo Exp $">
<kap>
  <rad>blenoragi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="blenor.0o">
  <kap><tld/>o</kap>
  <snc mrk="blenor.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Pusa <ref tip="super" cel="inflam.0o">inflamo</ref>
      elfluo de <ref cel="mukoz.0o">mukozo</ref>:
      <ekz>
        irisito ofte dependas de &gcirc;enerala malsano de la organismo: sifiliso, re&ubreve;matismo,
        <tld/>o, podagro, tuberkulozo, akutaj infektaj malsanoj, diabeto ktp
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227931">Irisito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="blenor1.0o">blenoreo</ref>,
       <ref cel="gonore.0o">gonoreo</ref>
    </refgrp>
    <trd lng="de">Blennorrhagie</trd>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_je;&c_n;&c_a;&c_r;&c_a;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">blennorr&agrave;gia</trd>
  <trd lng="cs">b&ecaron;lotok</trd>
  <trd lng="es">blenorragia</trd>
  <trd lng="fr">blennorragie</trd>
  <trd lng="pl">ropotok</trd>
  <trd lng="ru">&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_r;&c_r;&c_a;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>hnisav&yacute; v&yacute;tok</trd>,
    <trd>kvapavka</trd>
  </trdgrp>
</drv>
</art>
