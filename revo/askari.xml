<vortaro>
<art mrk="$Id$">
<kap>
<rad>askarid</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="askari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="askari.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Ascaris</trd>) de intestaj parazitaj vermoj.
    </dif>
  </snc>
  <trd lng="de">Spulwurm</trd>
  <trd lng="ru">&#1072;&#1089;&#1082;&#1072;&#1088;&#1080;&#1076;&#1072;</trd>
</drv>
<drv mrk="askari.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="verm.faden0oj">fadenvermoj</ref>
      (<trd lng="la">Ascarididae</trd>).
    </dif>
  </snc>
  <trd lng="de">Spulw&uuml;rmer</trd>
</drv>
</art>
