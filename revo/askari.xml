<art mrk="$Id: askari.xml,v 1.21 2023/10/13 17:08:38 revo Exp $">
<kap>
  <rad>askarid</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="askari.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref> 
      el familio <ref tip="malprt" cel="askari1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Ascaris</trd>) de intestaj parazitaj vermoj:
      <ekz>
        askaridozo estas parazita infekto ka&ubreve;zita de <tld/>o, kiu similas al lumbriko
        <klr>[&#x2026;]</klr>, en kelkaj partoj de Brazilo, 90&#x202F;% de la lo&gcirc;antaro estas
        atakita de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227439">Askaridozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/7e/Ascaris_lumbricoides.jpeg/300px-Ascaris_lumbricoides.jpeg" prm="PD">Lumbrikeca <tld/>o
        <fnt>
          <aut>CDC Division of Parasitic Diseases</aut>
          <lok><url ref="&WCW;/File:Ascaris_lumbricoides.jpeg">&WCW;/File:Ascaris_lumbricoides.jpeg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_k;&c_a;&c_r;&c_y;&c_d;&c_a;</trd>
  <trd lng="bg">&c_d;&c_je;&c_t;&c_s;&c_k;&c_i; &c_g;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="en">intestinal <ind>roundworm</ind></trd>
  <trd lng="de">Spulwurm</trd>
  <trd lng="fa">&a_A_madda;&a_s;&f_k;&a_A;&a_r;&f_y;&a_s;</trd>
  <trd lng="fr">ascaris</trd>
  <trd lng="he">&#x5E9;&#x5E8;&#x5E9;&#x5D5;&#x5E8;</trd>
  <trd lng="hu">ors&oacute;f&eacute;reg</trd>
  <trd lng="it">ascaris</trd>
  <trd lng="ja">&#x56DE;&#x866B;  <pr>&#x304B;&#x3044;&#x3061;&#x3085;&#x3046;</pr></trd>
  <trd lng="ru">&c_a;&c_s;&c_k;&c_a;&c_r;&c_i;&c_d;&c_a;</trd>
  <trd lng="uk">&#x430;&#x441;&#x43A;&#x430;&#x440;&#x438;&#x434;&#x430;</trd>
</drv>
</art>
