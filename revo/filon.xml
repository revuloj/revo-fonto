<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filon.xml,v 1.7 2025/08/13 18:43:07 revo Exp $">
<kap>
  <rad>filon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="filon.0o">
  <kap><tld/>o</kap>
  <snc mrk="filon.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="filum.0o.BIO">Filumo</ref>:
      <ekz>
        nur du <tld/>oj de animaloj &dash; Artropodoj kaj Vertebruloj &dash;
        plene adapti&gcirc;is al vivo surtera
        <fnt>
          <aut>C. St&oslash;p-Bowitz, M. B&eacute;la</aut>,
          <vrk><url ref="http://www.fw.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm">
          Enkonduko en la biologion</url></vrk>,
          <lok>Fokuso, 1987-04</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_y;&c_p; <klr tip="amb">(&c_b;&c_ib;&c_ja;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
    <trd lng="de">Phylum</trd>
    <trdgrp lng="pl">
      <trd>typ</trd>,
      <trd>gromada</trd>
    </trdgrp>
    <trd lng="zh">&#38272; <pr>m&eacute;n</pr></trd>
</snc>
  <snc mrk="filon.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      &Scirc;tupo en la klasifiko de lingvofamilioj grupiganta plurajn familiojn:
      <ekz>
        diskutata propono en lingvistiko estas la reklasifiko de dekoj da mondaj lingvaj
        familioj inter nur dek du <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2634">Lingva filono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_ib;&c_l;</trd>,
      <trd>&c_m;&c_a;&c_k;&c_r;&c_a;&c_s;&c_ja;&c_m;&#8217;&c_ja; <klr>(&c_l;&c_ib;&c_n;&c_g;&c_v;.)</klr></trd>
    </trdgrp>
    <trd lng="de">Makrofamilie <klr>(Sprachen)</klr></trd>
    <trd lng="pl">makrorodzina <klr>(j&eogonek;zyki)</klr></trd>
  </snc>
  <trd lng="en">phylum</trd>
  <trd lng="uk">&c_v;&c_ib;&c_d;&c_d;&c_ib;&c_l; (&c_t;&c_a;&c_k;&c_s;&c_o;&c_n;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_i;&c_j; &c_t;&c_je;&c_r;&c_m;&c_ib;&c_n;)</trd>
</drv>
</art>

