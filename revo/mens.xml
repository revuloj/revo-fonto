<drv mrk="mens.0o">
  <kap><tld/>o</kap>
  <snc mrk="mens.0o.PSI">
    <dif>
      Spirita elemento de bestoj kaj homoj,
      kiu ebligas celhavan konduton kaj inkluzivas
      <ref tip="prt" cel="emoci.0o.PSI">emociojn</ref>,
      <ref tip="prt" cel="memor.0o.kapablo">memoron</ref>,
      <ref tip="prt" cel="vol.0o.PSI">volon</ref>,
      <ref tip="prt" cel="imag.0o.PSI">imagon</ref>,
      <ref tip="prt" cel="intele.0o.MENS">intelekton</ref>:
      <ekz>
        sana <tld/>o en sana korpo;
      </ekz>
      <ekz>
        junece gaja <tld/>o
        <fnt><bib>Fab2</bib>, <lok>El fenestro en Vartou</lok></fnt>;
      </ekz>
      <ekz>
        klerigi la <tld/>on de iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        homoj malkapablaj morale kaj <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la rolo, kiun la juda demando ludis en la <tld/>o de
        Zamenhof
        <fnt>
        <aut>N. Z. Maimon</aut>,
        <vrk>La cionista periodo en la vivo de Zamenhof</vrk>.
        <lok>Nica Literatura Revuo, 3:5</lok>.
        </fnt>;
      </ekz>
      <ekz>
        mia naiva juna <tld/>o, pro la instinktaj premoj, pleni&gcirc;is
        per malbelaj, teruraj imagoj
        <fnt>
        <aut>M. Boulton</aut>,
        <vrk>Kelkaj pensoj pri literaturo kaj moralo</vrk>.
        <lok>Nica Literatura Revuo, 3:5</lok>.
        </fnt>;
      </ekz>
      <ekz>
        tiuj poetoj jam trovis, kaj akceptis per sia subkonscia
        <tld/>o, solvon respektive fidon, kiu kontentigis
        ilin kaj povis solvi por ili la konflikton
        <fnt>
        <aut>W. Auld</aut>,
        <vrk>Pasio-Pasivo</vrk>.
        <lok>Nica Literatura Revuo, 1:1</lok>.
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens">penso</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_m;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_je; &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_s;&c_i;&c_h;&c_i;&c_k;&c_a;</trd>,
      <trd>&c_d;&c_u;&c_h;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="anim.0o">animo</ref>,
    <ref cel="spirit.0o.menso">spirito</ref>,
    <ref cel="psik.0o">psi&hcirc;o</ref>.
  </refgrp>
  <trd lng="af">gedagte</trd>
  <trd lng="am">&#x12A0;&#x12A5;&#x121D;&#x122E;</trd>
  <trd lng="ar">&#x639;&#x642;&#x644;</trd>
  <trd lng="az">a&#x11F;&#x131;l</trd>
  <trd lng="be">&c_d;&c_u;&c_h;</trd>
  <trd lng="bn">&#x9AE;&#x9A8;</trd>
  <trd lng="bs">uma</trd>
  <trd lng="co">cuntata</trd>
  <trdgrp lng="cs">
    <trd>duch</trd>,
    <trd>du&scaron;evno</trd>,
    <trd>mysl</trd>,
    <trd>rozum</trd>
  </trdgrp>
  <trd lng="da">sind</trd>
  <trdgrp lng="de">
    <trd>Geist</trd>,
    <trd>Verstand</trd>
  </trdgrp>
  <trd lng="en">mind</trd>
  <trd lng="es">mente</trd>
  <trd lng="et">meeles</trd>
  <trdgrp lng="fr">
    <trd>esprit</trd>
    <trd>mentalit&eacute;</trd>
  </trdgrp>
  <trd lng="fy">geast</trd>
  <trd lng="gl">mente</trd>
  <trd lng="gu">&#xAAE;&#xAA8;</trd>
  <trd lng="ha">hankali</trd>
  <trd lng="hi">&#x92E;&#x928;</trd>
  <trd lng="hr">um</trd>
  <trd lng="hu">&eacute;sz</trd>
  <trd lng="hy">&#x574;&#x56B;&#x57F;&#x584;&#x568;</trd>
  <trdgrp lng="id">
    <trd>akal</trd>,
    <trd>pikiran</trd>
  </trdgrp>
  <trd lng="is">hugur</trd>
  <trd lng="ja">&#x5FC3;</trd>
  <trd lng="jw">atine</trd>
  <trd lng="ka">&#x10D2;&#x10DD;&#x10DC;&#x10D4;&#x10D1;&#x10D0;</trd>
  <trd lng="km">&#x1785;&#x17B7;&#x178F;&#x17D2;&#x178F;</trd>
  <trd lng="ko">&#xB9C8;&#xC74C;</trd>
  <trd lng="lo">&#xEC3;&#xE88;</trd>
  <trd lng="ml">&#xD2E;&#xD28;&#xD38;&#xD4D;&#xD38;&#xD4D;</trd>
  <trd lng="mr">&#x92E;&#x928;</trd>
  <trd lng="my">&#x1005;&#x102D;&#x1010;&#x103A;&#x1000;&#x102D;&#x102F;</trd>
  <trd lng="ne">&#x92E;&#x928;</trd>
  <trd lng="nl">geest</trd>
  <trd lng="ny">maganizo</trd>
  <trd lng="pa">&#xA2E;&#xA28;</trd>
  <trdgrp lng="pl">
    <trd>rozum</trd>,
    <trd>umys&lstroke;</trd>
  </trdgrp>
  <trd lng="ps">&#x630;&#x647;&#x646;</trd>
