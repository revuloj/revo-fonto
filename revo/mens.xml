<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>3</ofc>
      <rad>mens</rad>/o
    </kap>
    <drv mrk="mens.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Individua homa konscio, kiu inkludas intelekton, la volon,
          la emociojn kaj la memoron:
          <ekz>
            sana <tld/>o en sana korpo;
          </ekz><ekz>
            klerigi la <tld/>on de iu<fnt>Z</fnt>;
          </ekz><ekz>
            homoj malkapablaj morale kaj <tld/>e<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="anim.0o">animo</ref>,
          <ref cel="cerb.0o">cerbo</ref>,
          <ref cel="spirit.0o.menso">spirito</ref>,
          <ref cel="psik.0o">psi&hcirc;o</ref>.
        </refgrp>
        <refgrp tip="prt">
          <ref cel="imag.0o0ado0emo0povo">imago</ref>,
          <ref cel="pens">penso</ref>.
        </refgrp>
      </snc>
      <trd lng="en">mind</trd>
      <trd lng="nl">geest</trd>
      <trdgrp lng="ru">
        <trd>&c_u;&c_m;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_je; &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>,
        <trd>&c_p;&c_s;&c_i;&c_h;&c_i;&c_k;&c_a;</trd>,
        <trd>&c_d;&c_u;&c_h;</trd></trdgrp>
    </drv>
  </art>
<!--
