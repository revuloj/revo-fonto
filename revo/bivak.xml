<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bivak.xml,v 1.28 2023/10/29 19:11:02 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bivak</rad>/o
</kap>
<drv mrk="bivak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="bivak.0o.halto">
    <dif>
      Nokta halto de soldatoj a&ubreve; promenantoj por ripozi
      sub&ccirc;iele, sen tendoj:
      <ekz>
        <ref cel="fajr.bivak0o">bivakfajro</ref>;
      </ekz>
      <ekz>
        &ccirc;e iu montfendo inter du rokoj oni kondukis la etapulojn el la
        akvo kaj faris <tld/>on &ccirc;e la bordo
        <fnt><aut>Lena Karpunina</aut>, <vrk>La bato</vrk>, <lok>
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bivak.0o.loko">
    <dif>
      Loko de tia halto:
       <ekz>
        ni vivas &ccirc;i tie, kvaza&ubreve; en <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        118a</lok></fnt>;
      </ekz>
      <ekz>
        vane <nom>Claussen</nom> atendis la du kunulojn en <tld/>o
        <fnt>
          <aut>Werner Schad</aut>,
          <vrk><url ref="&Monato;/2003/007824.html">Tondrulo &ndash;
            montego de la Andoj</url></vrk>,
          <bib>Monato</bib>,
          <lok>(2003/2, p. 18)</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kamp.0adejo">kampadejo</ref>,
      <ref cel="tend.0aro.TUR">tendaro<sncref/></ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5BE8; <pr>zh&#xE0;i</pr></trd>,
    <trd>&#x5E37;&#x5E44; <pr>w&#xE9;iw&#xF2;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_v;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_i;&c_v;&c_a;&c_k;</trd>
  <trd lng="ca">bivac</trd>
  <trdgrp lng="cs">
    <trd>bivak</trd>,
    <trd>poln&iacute; t&aacute;bor</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Feldlager</trd>,
    <trd>Biwak</trd>
  </trdgrp>
  <trd lng="en">bivouac</trd>
  <trd lng="es">vivac, vivaque</trd>
  <trd lng="fr">bivouac</trd>
  <trd lng="he">&mem;&#x5B7;&alef;&#x5B2;&he;&#x5B8;&lamed;</trd>
  <trd lng="hu">&eacute;jszakai <ind>pihen&odblac;</ind></trd>
  <trd lng="it">bivacco</trd>
  <trdgrp lng="ja">
    <trd>&#x91CE;&#x55B6;  <pr>&#x3084;&#x3048;&#x3044;</pr></trd>,
    <trd>&#x91CE;&#x5BBF;  <pr>&#x306E;&#x3058;&#x3085;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">bivak</trd>
  <trd lng="pl">biwak</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_v;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_i;&c_v;&c_u;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bivak</trd>,
    <trd>t&aacute;bor pod hol&yacute;m nebom</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_j;&c_s;&c_mol;&c_k;.&nbsp;&c_B;&c_ib;&c_v;&c_u;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="bivak.0i">
  <kap><tld/>i</kap>
   <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">SPO</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pasigi nokton <tld/>e:
      <ekz>
        la servantoj de mia sinjoro <tld/>as sur la
        kampo <fnt><bib>MT</bib></fnt>;
      </ekz>
      <ekz>
        mi &scirc;vitas kiel &ccirc;evala&ccirc;o! &ccirc;u ni <tld/>u
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>?
      </ekz>
      <ekz>
        kiam venis vespero ili <tld/>is apud la Rivero de Senzorgo
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Dek</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_ja;&c_c;&c_mol;
    <ind>&c_b;&c_ib;&c_v;&c_a;&c_k;&c_o;&c_m;</ind></trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_p;&c_r;&c_y;&c_v;&c_a;&c_l;</ind></trd>
  </trdgrp>
  <trd lng="ca">bivaquejar</trd>
  <trd lng="cs">t&aacute;bo&rcaron;it <klr>(pod &scaron;ir&yacute;m nebem)</klr></trd>
  <trdgrp lng="de">
    <trd>im Freien <ind>lagern</ind></trd>,
    <trd>biwakieren</trd>
  </trdgrp>
  <trd lng="en">bivouac</trd>
  <trd lng="es">vivaquear</trd>
  <trd lng="fr">bivouaquer</trd>
  <trd lng="hu">&eacute;jszakai <ind>pihen&odblac;t</ind> tart</trd>
  <trd lng="it">bivaccare</trd>
  <trdgrp lng="ja">
    <trd>&#x91CE;&#x55B6;&#x3059;&#x308B;  <pr>&#x3084;&#x3048;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x91CE;&#x5BBF;&#x3059;&#x308B;  <pr>&#x306E;&#x3058;&#x3085;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bivakkeren</trd>
  <trdgrp lng="pl">
    <trd>biwakowa&cacute;</trd>,
    <trd>roz&lstroke;o&zdot;y&cacute; si&eogonek;
    <ind>biwakiem</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;
    &c_b;&c_i;&c_v;&c_a;&c_k;&c_o;&c_m;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_p;&c_r;&c_i;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bivakova&tcaron;</trd>,
    <trd>t&aacute;bori&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_ja;&c_t;&c_i; &c_b;&c_ib;&c_v;&c_a;&c_k;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_r;&c_i;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
</drv>
</art>
