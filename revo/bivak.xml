<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bivak</rad>/o
</kap>
<drv mrk="bivak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Nokta halto de soldatoj a&ubreve; promenantoj por ripozi sub&ccirc;iele,
      sen tendoj; loko de tia halto.
      <ekz>
        ni vivas &ccirc;i tie, kvaza&ubreve; en <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p. 118a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Feldlager</trd>,
    <trd>Biwak</trd>
  </trdgrp>
  <trd lng="en">bivouac</trd>
  <trd lng="fr">bivouac</trd>
  <trd lng="hu">t&aacute;borhely</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_v;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_i;&c_v;&c_u;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">bivak</trd>
</drv>

<drv mrk="bivak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pasigi nokton <tld/>e:
      <ekz>
        <tld/>i sur la kampo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>im Freien <ind>lagern</ind></trd>,
    <trd>biwakieren</trd>
  </trdgrp>
  <trd lng="en">bivouac</trd>
  <trd lng="fr">bivouaquer</trd>
  <trd lng="hu">t&aacute;boroz</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; &c_b;&c_i;&c_v;&c_a;&c_k;&c_o;&c_m;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_p;&c_r;&c_i;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">bivakova&tcaron;</trd>
</drv>
</art>
