<drv mrk="sid.lok0a">
  <kap>lok<tld/>a</kap>
  <snc mrk="sid.lok0a.MOR">
    <dif>
      <ref tip="dif" cel="hejm.fiks0a.MOR">Fikshejma</ref>:
      <ekz>
        la migraj triboj havis pli multe da bon&scirc;anco
        sed la lok<tld/>aj malaperis
        <fnt>
          <aut>luphundo</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/drevnikocur7/315908">
          Kial la e&ubreve;ropanoj konkeris la amerikan
          kontinenton</url></vrk>,
          <lok>2011-03-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        tradicia okupo de Burjatoj &mdash; agrokulturo kaj brutbredado
        lok<tld/>a a&ubreve; duonnomada
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=474">Burjatio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>sesshaft</trd>,
     <trd>bodenst&auml;ndig</trd>
  </trdgrp>
  <trd lng="es">sedentario<klr tip="amb"> (opuesto a
  n&oacute;mada)</klr></trd> 
  <trdgrp lng="fr">
    <trd>s&eacute;dentaire</trd>
  </trdgrp>
  <trd lng="hu">letelepedett</trd>
  <trd lng="it">stanziale</trd>
  <trd lng="nl">vasthuizig</trd>
  <trd lng="pl">osiad&lstroke;y</trd>
  <trd lng="pt">sedent&aacute;rio<klr tip="amb">
  (de resid&ecirc;ncia fixa)</klr></trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_d;&c_l;&c_y;&c_j;</trd>
</drv>

