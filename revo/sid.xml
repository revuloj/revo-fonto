    <trd>&c_sh;&c_t;&c_a;&c_b;-&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_f;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">s&iacute;dlo</trd>
  <trd lng="uk">&#x43C;&#x456;&#x441;&#x446;&#x435;&#x437;&#x43D;&#x430;&#x445;&#x43E;&#x434;&#x436;&#x435;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="sid.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <snc mrk="sid.0igi.MOV">
    <dif>
      Igi iun <tld/>i 1:
      <ekz>
        <ind><tld/>igu vin</ind> <klr>[&#x2026;]</klr>, sinjoro
        <trd lng="hu">&uuml;lj&ouml;n le</trd>
        <trd lng="ru">&c_s;&c_a;&c_d;&c_i;&c_t;&c_je;&c_s;&c_mol;</trd>
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;ar <tld/>igi sin mem li ne volas, se li ne povas esti
        <tld/>igata, submovu
        se&gcirc;on sub liajn piedojn, kaj li kontra&ubreve;vole <tld/>i&gcirc;os
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        Moseo prenis sian edzinon kaj siajn filojn kaj <tld/>igis ilin
        sur azeno
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 4:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu lin sur la trono
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 10:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi lin kun eminentuloj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 113:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
