<drv mrk="sid.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc mrk="sid.0anto.iu">
    <dif>
      Iu, kiu <tld/>as:
      <ekz>
        la <tld/>antoj &ccirc;e la pordego
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 69:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>anto sur la trono
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 36:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anto &ccirc;e man&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Luko 22:27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sitzender</trd>
</drv>

<drv mrk="sid.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="sid.0ejo.POL">
    <dif>
      Loko, kie <tld/>as iu a&ubreve; io:
      <ekz>
        la veturigisto desaltis de sia <tld/>ejo, stari&gcirc;is, kuntiris per
        la mano la
        longajn konduk&scirc;nurojn, kaj per vo&ccirc;o ra&ubreve;ka de teruro li kriis al
        la &ccirc;evaloj,
        ke ili haltu
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        portreto pendigita &ccirc;e muro de l' <tld/>ejo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia popolo lo&gcirc;os en lo&gcirc;ejo paca kaj en <tld/>ejoj sendan&gcirc;eraj
        kaj en ripozejoj
        senzorgaj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 32:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la UN-<tld/>ejo en Novjorko
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Steloj;monato2/007180.html">Ri&ccirc;a per .tv</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ef<tld/>ejo de NATO en Bruselo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Steloj;monato2/007190.html">Trompitaj dizertintoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        limigita fizika spaco en kun<tld/>ejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2003/008051.html">Bruselo-Babelo: lingvo-kulturo en E&ubreve;ropa Unio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="lok.sid0o">sidloko</ref>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_ja;&c_s;&c_t;&c_o; &c_z;&c_a; &c_s;&c_ja;&c_d;&c_a;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">s&iacute;dlo</trd>
  <trdgrp lng="de">
    <trd>Sitz</trd>,
    <trd>Sitzplatz</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>emplazamiento</trd>,
    <trd>domicilio</trd>,
    <trd>sede</trd>
  </trdgrp>
  <trd lng="fr">si&egrave;ge<klr tip="amb"> (emplacement)</klr></trd>
  <trd lng="hu">sz&eacute;khely</trd>
  <trd lng="pl">siedziba</trd>
  <trdgrp lng="ru">
