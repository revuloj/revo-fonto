<drv mrk="posten.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Akiri <ref cel="posten.0o.ofico"><tld/>on <sncref/></ref>:
      <ekz>
        jam en<tld/>i&gcirc;is aparta komisiono por identigi &ccirc;iujn eblecojn
        <fnt>
          <aut>Pa&ubreve;lo Gubbins</aut>
          <vrk><url ref="&Monato;/1994/A940601.php">Revivi&gcirc;as la fantomoj -- de 1944</url></vrk>
          <bib>Monato</bib>
          <lok>1994/06, p. 11</lok>
        </fnt>.
      </ekz><ekz>
        &ccirc;e la muzeo de Si&ccirc;uano pasintjare en<tld/>i&gcirc;is ok, sed elposteni&gcirc;is dek
        muzeologoj
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008682.php">Senmonaj muzeoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz><ekz>
        &scirc;i en<tld/>i&gcirc;is kiel unua ina respublikestro de Brazilo
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2011/010516.php">La jarcento de la virino</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_n;&c_y;&c_m;</ind> &c_n;&c_a; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>colocarse<klr> (conseguir trabajo)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>collocarsi<klr> (ottenere lavoro)</klr></trd>,
    <trd>sistemarsi<klr> (ottenere lavoro)</klr></trd>
  </trdgrp>
  <trd lng="pl">zatrudni&cacute; si&eogonek; na <ind>stanowisku</ind></trd>
  <trdgrp lng="ro">
    <trd>fi numit &icirc;n <ind>func&#x21B;ie</ind></trd>,
    <trd>fi angajat &icirc;n <ind>pozi&#x21B;ie</ind></trd>
  </trdgrp>
</drv>

