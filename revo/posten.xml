<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: posten.xml,v 1.20 2021/06/16 06:38:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>posten</rad>/o
</kap>
<drv mrk="posten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="posten.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Loko okupita per soldataro por milita celo:
      <ekz>
        la anta&ubreve;aj kolonoj trovas anta&ubreve; si a&ubreve;strajn <tld/>ojn;
      </ekz>
      <ekz>
        morti &ccirc;e, sur sia <tld/>o;
      </ekz>
      <ekz>
        anta&ubreve;<tld/>o.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_o;&c_s;&c_t;</trd>
    <trdgrp lng="es">
      <trd>puesto<klr> (mil.)</klr></trd>,
      <trd>posici&oacute;n<klr> (mil.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;llom&aacute;shely</trd>,
      <trd>&odblac;rhely</trd>,
      <trd>poszt<klr tip="ind"> (&odblac;rhely)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>posto<klr> (mil.)</klr></trd>,
      <trd>postazione<klr> (mil.)</klr></trd>,
      <trd>posizione<klr> (mil.)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>post<klr tip="ind"> (wachtpost)</klr></trd>,
      <trd>wachtpost</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_t;</trd>
  </snc>
  <snc mrk="posten.0o.ofico">
    <dif>
      Estimata ofico asignita al iu:
      <ekz>
        okupi eminentan, gravan <tld/>on;
      </ekz>
      <ekz>
        ne resti &ccirc;e sia <tld/>o de prezidanto.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_o;&c_s;&c_t;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_a;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>puesto<klr> (ocupaci&oacute;n, trabajo, funci&oacute;n)</klr></trd>,
      <trd>ocupaci&oacute;n<klr> (trabajo)</klr></trd>,
      <trd>colocaci&oacute;n<klr> (trabajo)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;ll&aacute;s</trd>,
      <trd>poz&iacute;ci&oacute;</trd>,
      <trd>poszt<klr tip="ind"> (&aacute;ll&aacute;s)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>posto<klr> (impiego, lavoro, funzione)</klr></trd>,
      <trd>occupazione<klr> (lavoro)</klr></trd>,
      <trd>collocazione<klr> (lavoro)</klr></trd>,
      <trd>impiego<klr> (lavoro)</klr></trd>
    </trdgrp>
    <trd lng="nl">ambt</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_t;</trd>,
      <trd>&c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>post</trd>,
    <trd>postaven&iacute;</trd>,
    <trd>pozice</trd>,
    <trd>pracovn&iacute; m&iacute;sto</trd>,
    <trd>stanovi&scaron;t&ecaron;</trd>,
    <trd>zam&ecaron;stn&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Posten</trd>
  <trd lng="en">post</trd>
  <trd lng="fr">poste</trd>
  <trd lng="pt">posto<klr>(ocupa&ccedil;&atilde;o, fun&ccedil;&atilde;o)</klr></trd>
  <trdgrp lng="sk">
    <trd>post</trd>,
    <trd>poz&iacute;cia</trd>,
    <trd>stanovisko</trd>
  </trdgrp>
</drv>

<drv mrk="posten.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>e stari; prigardi &ccirc;e <tld/>o:
      <ekz>
        tri korpusoj <tld/>is anta&ubreve; la armeo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        an&gcirc;elo <tld/>as &ccirc;irka&ubreve; tiuj, kiuj timas kaj li ilin
        savas<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hl&iacute;dkovat</trd>,
    <trd>p&uring;sobit</trd>
  </trdgrp>
  <trd lng="es">apostarse</trd>
  <trd lng="fr">poster</trd>
  <trdgrp lng="hu">
    <trd>&aacute;ll&aacute;sozik</trd>,
    <trd>posztol</trd>,
    <trd>&odblac;rk&ouml;dik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>essere <ind>appostato</ind></trd>,
    <trd>stare di <ind>posta</ind></trd>
  </trdgrp>
  <trd lng="nl">op <ind>post</ind> staan</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; &c_n;&c_a; &c_p;&c_o;&c_s;&c_t;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>hliadkova&tcaron;</trd>,
    <trd>p&ocirc;sobi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="posten.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>i:
      <ekz>
        kvar soldatoj <tld/>igitaj vidalvide de tiu homo, pafis al li en
        la kranion;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;telistoj <ind><tld/>i&gcirc;is</ind> post la muro
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="embusk">embuski</ref>.
    </refgrp>
  </snc>
  <ref tip="hom" cel="post.0enigi">post&middot;e&middot;n&middot;ig&middot;i</ref>
  <trdgrp lng="cs">
    <trd>odd&aacute;lit</trd>,
    <trd>p&rcaron;id&ecaron;lit stanovi&scaron;t&ecaron;</trd>,
    <trd>uv&eacute;st do funkce</trd>,
    <trd>zahnat</trd>
  </trdgrp>
  <trd lng="de">postieren</trd>
  <trd lng="es">apostar</trd>
  <trd lng="hu">fel&aacute;ll&iacute;t <klr tip="amb"> (&odblac;r&ouml;ket)</klr> zetten</trd>
  <trdgrp lng="it">
    <trd><klr>(far) </klr>appostare</trd>,
    <trd>sistemare<klr> (collocare)</klr></trd>
  </trdgrp>
  <trd lng="nl">op <ind>post</ind> zetten</trd>
  <trdgrp lng="sk">
    <trd>postavi&tcaron; na miesto</trd>,
    <trd>stanovi&scaron;te</trd>,
    <trd>stanovi&tcaron;</trd>,
    <trd>umiestni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="posten.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Asigni <ref cel="posten.0o.ofico"><tld/>on <sncref/></ref> al iu.
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.en0igi">enoficigi</ref>,
      <ref cel="instal.0i.enpostenigi">instali</ref>
    </refgrp>
  </snc>
  <trdgrp lng="es">
    <trd>colocar<klr> (dar trabajo)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>collocare<klr> (dare lavoro)</klr></trd>,
    <trd>sistemare<klr> (dare lavoro)</klr></trd>
  </trdgrp>
  <trd lng="sk">dosadi&tcaron; <klr>(niekoho niekde)</klr></trd>
</drv>

<drv mrk="posten.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Akiri <ref cel="posten.0o.ofico"><tld/>on <sncref/></ref>.
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>colocarse<klr> (conseguir trabajo)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>collocarsi<klr> (ottenere lavoro)</klr></trd>,
    <trd>sistemarsi<klr> (ottenere lavoro)</klr></trd>
  </trdgrp>
</drv>
</art>
