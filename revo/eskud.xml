<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eskud</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="eskud.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref> de diversaj landoj
      kaj epokoj (&Ccirc;ilio, Francujo, Portugalujo):
      <ekz>
        li trovis en kunnodita naztuko amaseton da oraj
        <tld/>oj<fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro
        23a, p. 165a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_ju;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_u;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">escut</trd>
  <trd lng="de">Escudo</trd>
  <trd lng="es">escudo</trd>
  <trdgrp lng="fr">
    <trd>&eacute;cu<klr> (monnaie)</klr></trd>,
    <trd>escudo</trd>
  </trdgrp>
  <trd lng="pl">escudo</trd>
  <trd lng="pt">escudo</trd>
</drv>
</art>
