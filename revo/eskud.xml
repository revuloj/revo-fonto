<art mrk="$Id: eskud.xml,v 1.16 2023/08/31 11:49:30 revo Exp $">
<kap>
  <rad>eskud</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="eskud.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de diversaj landoj
      kaj epokoj (&Ccirc;ilio, Francujo, Portugalujo):
      <ekz>
        li trovis en kunnodita naztuko amaseton da oraj
        <tld/>oj<fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro
        23a, p. 165a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_ju;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_u;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_u;&c_d;&c_o;</trd>
  <trd lng="ca">escut</trd>
  <trd lng="cs">star&aacute; francouzsk&aacute; st&rcaron;&iacute;brn&aacute; mince</trd>
  <trd lng="de">Escudo</trd>
  <trd lng="en">escudo</trd>
  <trd lng="es">escudo</trd>
  <trdgrp lng="fr">
    <trd>&eacute;cu<klr> (monnaie)</klr></trd>,
    <trd>escudo</trd>
  </trdgrp>
  <trd lng="id">escudo</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30AD;&#x30E5;</trd>,
    <trd>&#x30A8;&#x30B9;&#x30AF;&#x30FC;&#x30C9;</trd>
  </trdgrp>
  <trd lng="pl">escudo</trd>
  <trd lng="pt">escudo</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_ju;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_u;&c_d;&c_o;</trd>,
    <trd>&c_s;&c_k;&c_u;&c_d;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">star&aacute; franc&uacute;zska strieborn&aacute; minca</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x44E;</trd>,
    <trd>&#x435;&#x441;&#x43A;&#x443;&#x434;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
