<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>enu</rad>/i
</kap>
<drv mrk="enu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malplezuri pro senintereso a&ubreve; neokupiteco:
      <ekz>
        mi <tld/>is la tutan tagon, atendante vin.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="osced.0i">oscedi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">sich <ind>langweilen</ind></trd>
  <trd lng="en">weary</trd>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="enu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de <tld/>anto:
      <ekz>
        la laboro estas plej bona kuracilo kontra&ubreve; <tld/>o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ted">tedo</ref>,
      <ref cel="splen.0o">spleno</ref>,
      <ref cel="melank.0o">melankolio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Langweile</trd>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_k;&c_a;</trd>
</drv>
<drv mrk="enu.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o 
    </dif>
    <refgrp tip="sin">
      <ref cel="griz.0a">griza</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="enu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        lia parolodo <tld/>igis &ccirc;iujn;
      </ekz>
      <ekz>
        tiu sencela vagado balda&ubreve; lacigas k <tld/>igas
        ilin<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">langweilen</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_k;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_s;&c_t;&c_i; &c_s;&c_k;&c_u;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
<drv mrk="enu.0iga">
  <kap><tld/>(ig)a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <tld/>iga viro, libro, teatra&jcirc;o;
      </ekz>
      <ekz>
        sono, tiel monotona, tiel terure <tld/>iga
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Malgranda tuk</vrk>)</fnt>; 
      </ekz>
      <ekz>
        malfacila kaj <tld/>a laborado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        scienco havas semon <tld/>an sed frukton bon&gcirc;uan
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="de">langweilig</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j; &c_s;&c_k;&c_u;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
</art>
