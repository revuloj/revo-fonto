<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enu.xml,v 1.29 2023/11/07 20:34:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>enu</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="enu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="enu.0i.PSI">
    <dif>
      Mal<ref tip="ant" cel="plezur.0i">plezuri</ref> pro senintereso a&ubreve; neokupiteco:
      <ekz>
        la tempo pasas tiel malrapide, mi <tld/>as
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malliberulo gratis sur la muro versojn <klr>[&#x2026;]</klr>, mi pensas, ke li
        <tld/>is, mi estis bonveninta gasto
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[demandinte]</klr>, &ccirc;u ili tiom <tld/>is, kiom mi &ccirc;e tiu legado, &ccirc;iuj
        sinceruloj konfesis al mi, ke la libro falas el iliaj manoj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar li <tld/>is nenion laborante, ideo naski&gcirc;is en lia cerbo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;ha, &ccirc;u vi du neniam enuas la &scirc;akludon?&rightquot; &scirc;i diris konversacie
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ted.0igxi">tedi&gcirc;i</ref>
    <refgrp tip="vid">
      <ref cel="langvor.0i">langvori</ref>,
      <ref cel="osced.0i">oscedi</ref>      
    </refgrp>
    <rim>
      PIV2 rigardas transitivan uzon de <em>enui</em>
      evitinda, sed &gcirc;i estas literature trovebla, kiel
      atestas supra ekzemplo.
      <aut>&jcirc;v</aut>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">avorrir-se</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;t dlouhou chv&iacute;li</trd>,
    <trd>nudit se</trd>
  </trdgrp>
  <trd lng="de">sich <ind>langweilen</ind></trd>
  <trd lng="en">be bored</trd>
  <trd lng="es">aburrirse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>emb&ecirc;ter</ind><klr tip="ind"> (&eacute;prouver de
    l'ennui)</klr></trd>,
    <trd>s'<ind>ennuyer</ind><klr tip="ind"> (&eacute;prouver de
    l'ennui)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9000;&#x5C48;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x305F;&#x3044;&#x304F;&#x3064;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x3046;&#x3093;&#x3056;&#x308A;&#x3057;&#x3066;&#x3044;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>vervelen</ind></trd>
  <trd lng="pl">nudzi&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
    <trd>aborrecer-se</trd>,
    <trd>enfastiar-se</trd>,
    <trd>enfadar-se</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">nudi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_u;&c_d;&c_mol;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_u;&c_d;&c_i;&c_t;&c_i;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x89BA;&#x5F97;&#x5F88;&#x7121;&#x804A; <pr>ju&#xE9;deh&#x11B;nw&#xFA;li&#xE1;o</pr></trd>,
    <trd>&#x611F;&#x5230;&#x65E0;&#x804A; <pr>g&#x1CE;nd&#xE0;ow&#xFA;li&#xE1;o</pr></trd>
  </trdgrp>
</drv>



<drv mrk="enu.0a">
  <kap><tld/>a</kap>
  <snc mrk="enu.0a.PSI">
    <dif>
      Esprimanta a&ubreve; ka&ubreve;zanta <tld/>adon:
      <ekz>
        mi iom amas en <tld/>a tempo tralegi iom amuzan
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo oscedis kaj &ccirc;irka&ubreve;rigard<klr>[is]</klr>
        per <tld/>aj okuloj <klr>(montrantaj enuon)</klr>
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 15</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas tiel unutona kaj <tld/>a, ke oni kelkafoje preska&ubreve; povus pendigi sin
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgrandaj lipharoj ombris la maldikajn lipojn, 
        iom ironiajn, iom <tld/>ajn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        en lingvo natura li atingos tion &ccirc;i nur post tutaj jaroj
        da malfacila kaj <tld/>a laborado <klr>(<tld/>iga)</klr>
        <fnt><bib>EE</bib></fnt>;
      </ekz>
      <ekz>
        scienco havas semon <tld/>an, sed frukton bon&gcirc;uan
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="egal.0a.nevaria">egala<sncref/></ref>,
      <ref cel="griz.0a.sengaja">griza<sncref/></ref>,
      <ref cel="indife.0a.io">indiferenta<sncref/></ref>,
      <ref cel="monoto.0a.unutona">monotona<sncref/></ref>,
      <ref cel="ted.0a">teda</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_d;&c_n;&c_y;</trd>
  <trd lng="bo">&t_ra;&t_nata;&t_naro;&t_tsheg;&t_pa;&t_naro;&t_tsheg;&t_ma;&t_drengbu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ca">avorrit</trd>
  <trd lng="en">boring</trd>
  <trd lng="es">aburrido</trd>
  <trdgrp lng="fr">
    <trd>ennuy&eacute;<klr tip="ind"> (qui montre l'ennui)</klr></trd>,
    <trd>ennuyeux<klr tip="ind"> (lassant)</klr></trd>,
    <trd>lassant<klr tip="ind"> (ennuyeux)</klr></trd>
  </trdgrp>
  <trd lng="he">&mem;&#x5B0;&shin;&#x5C1;&#x5B7;&ayin;&#x5B2;&mem;&#x5B5;&fmem;</trd>
  <trd lng="ja">&#x9000;&#x5C48;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x305F;&#x3044;&#x304F;&#x3064;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>
  <trd lng="nl">saai</trd>
  <trd lng="pl">nudny</trd>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nez&aacute;&zcaron;ivn&yacute;</trd>,
    <trd>nudn&yacute;</trd>
  </trdgrp>
  <trd lng="cs">nez&aacute;&zcaron;ivn&yacute;</trd>
  <trdgrp lng="de">
    <trd>gelangweilt</trd>,
    <trd>langweilig</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_u;&c_d;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x65E0;&#x8DA3; <pr>w&#xFA;q&#xF9;</pr></trd>,
    <trd>&#x60B6;&#x4EBA; <pr>m&#xE8;nr&#xE9;n</pr></trd>,
    <trd>&#x67AF;&#x71E5;&#x7684; <pr>k&#x16B;z&#xE0;ode</pr></trd>,
    <trd>&#x70E6;&#x4EBA; <pr>f&#xE1;nr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="enu.0o">
  <kap><tld/>o</kap>
  <snc mrk="enu.0o.PSI">
    <dif>
      Stato de <tld/>anto:
      <ekz>
        timo <klr>[&#x2026;]</klr> por unu momento liberigis min de la <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi en <klr>[&#x2026;]</klr> letargio de <tld/>o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        per siaj <tld/>plenaj okuloj Pilato rigardis for, kun grimaco de abomeno
        kontemplante la parton de la urbo ku&scirc;antan &ccirc;e liaj piedoj
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        terura malamiko atendis la junulinon: &gcirc;i estis la <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekaj kaj monotonaj sonoj <klr>[&#x2026;]</klr> la &ccirc;ambrojn de la lernejoj plenigas per
        <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fratino ka&ubreve;ze de <tld/>o kaj de malboneco malpacas kun la tuta familio
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la brando estis al li nur rimedo forigi la <tld/>on
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni petas min dispeli la <tld/>on de senokupaj horoj per rakonto el mia vivo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        damne, <ind>kia enuo</ind>
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>!
        <trd lng="en">what a bore!</trd>
        <trd lng="es">qu&eacute; aburrimiento!</trd>
        <trdgrp lng="pl">
          <trd>ale <ind>nuda</ind>!</trd>,
          <trd>co za <ind>nuda</ind>!</trd>
        </trdgrp>
      </ekz>
      <ekz>
        kiu amas &gcirc;uon, amu anka&ubreve; <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apati.0o">apatio</ref>,
      <ref cel="farnient.0o">farniento</ref>,
      <ref cel="indife.0eco">indiferenteco</ref>,
      <ref cel="langvor.0o">langvoro</ref>,
      <ref cel="melank.0o">melankolio</ref>,
      <ref cel="far.neni0o">nenifaro</ref>,
      <ref cel="splen.0o">spleno</ref>,
      <ref cel="ted.0o.PSI">tedo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_n;&c_u;&c_d;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">avorriment</trd>
  <trd lng="cs">nuda</trd>
  <trd lng="de">Langweile</trd>
  <trd lng="en">boredom</trd>
  <trd lng="es">aburrimiento</trd>
  <trdgrp lng="fr">
    <trd>ennui<klr tip="ind"> (lassitude)</klr></trd>,
    <trd>lassitude<klr tip="ind"> (ennui)</klr></trd>
  </trdgrp>
  <trd lng="he">&shin;&ayin;&mem;&vav;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#x9000;&#x5C48;  <pr>&#x305F;&#x3044;&#x304F;&#x3064;</pr></trd>,
    <trd>&#x5026;&#x6020;  <pr>&#x3051;&#x3093;&#x305F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">verveling</trd>
  <trd lng="pl">nuda</trd>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dlh&aacute; chv&iacute;&lcaron;a</trd>,
    <trd>nuda</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_u;&c_d;&c_mol;&c_g;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7A92;&#x606F;&#x611F; <pr>zh&#xEC;x&#xED;g&#x1CE;n</pr></trd>,
    <trd>&#x538C;&#x70E6; <pr>y&#xE0;nf&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="enu.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="enu.0iga.PSI">
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <tld/>iga viro, libro, teatra&jcirc;o;
      </ekz>
      <ekz>
        sono, tiel monotona, tiel terure <tld/>iga
        <fnt><bib>Fab2</bib>, <lok>Malgranda Tuk</lok></fnt>;
      </ekz>
      <ekz>
        tiuj samaj solecaj promenadoj, kiuj estas nun tiel
        &gcirc;uoplenaj por mi, tiam estis al mi sengustaj
        kaj <tld/>igaj
        <fnt><bib>Revadoj</bib>, <lok>Oka promenado</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_d;&c_n;&c_y;</trd>
  <trd lng="ca">avorrit</trd>
  <trdgrp lng="cs">
    <trd>nudn&yacute;</trd>,
    <trd>nud&iacute;c&iacute;</trd>,
    <trd>otravn&yacute;</trd>
  </trdgrp>
  <trd lng="de">langweilig</trd>
  <trd lng="en">boring</trd>
  <trd lng="es">aburrido</trd>
  <trdgrp lng="fr">
    <trd>ennuyeux<klr tip="ind"> (lassant)</klr></trd>,
    <trd>lassant<klr tip="ind"> (ennuyeux)</klr></trd></trdgrp>
  <trd lng="he">&mem;&#x5B0;&shin;&#x5C1;&#x5B7;&ayin;&#x5B2;&mem;&#x5B5;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#x9000;&#x5C48;&#x306A;  <pr>&#x305F;&#x3044;&#x304F;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x3064;&#x307E;&#x3089;&#x306A;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">vervelend</trd>
  <trd lng="pl">nudny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;
    &c_s;&c_k;&c_u;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nez&aacute;&zcaron;ivn&yacute;</trd>,
    <trd>nudn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x60B6;&#x4EBA; <pr>m&#xE8;nr&#xE9;n</pr></trd>,
    <trd>&#x67AF;&#x71E5;&#x7684; <pr>k&#x16B;z&#xE0;ode</pr></trd>,
    <trd>&#x4EE4;&#x4EBA;&#x538C;&#x70E6; <pr>l&#xEC;ngr&#xE9;ny&#xE0;nf&#xE1;n</pr></trd>,
    <trd>&#x6C92;&#x6709;&#x610F;&#x601D; <pr>m&#xE9;iy&#x1D2;uy&#xEC;si</pr></trd>
  </trdgrp>
</drv>

<drv mrk="enu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="enu.0igi.PSI">
    <dif>
      Ka&ubreve;zi <tld/>on,
      <ref tip="sin" cel="ted.0i">tedi</ref>:
      <ekz>
        sencel<klr>[a&#x2026;]</klr> vagado balda&ubreve; lacigas kaj <tld/>igas
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la diservon <klr>[&#x2026;]</klr> <tld/>igis lin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj <tld/>igas lin <klr>[dum]</klr> li <tld/>igas la aliajn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj kristanaj virtoj <tld/>igus min <klr>[&#x2026;]</klr> en unu tago
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XL</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tenos &scirc;in tiel longe, &gcirc;is &scirc;i <tld/>igos min
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj aran&gcirc;as &ccirc;ion tiel diable stulte, kvaza&ubreve; nur por <tld/>igi kaj turmenti
        sin
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la disputoj pri la administrado de la bieno &ccirc;iam <tld/>igis lin
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frustr.0i">frustri</ref>,
      <ref cel="amuz.mal0i">malamuzi</ref>,
      <ref cel="komfor.mal0igi">malkomfortigi</ref>,
      <ref cel="nauxz.0i.FIG">na&ubreve;zi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_a;&c_k;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trd lng="ca">avorrir</trd>
  <trdgrp lng="cs">
    <trd>nudit <klr>(n&ecaron;koho)</klr></trd>,
    <trd>unudit</trd>
  </trdgrp>
  <trd lng="de">langweilen</trd>
  <trd lng="en">bore</trd>
  <trd lng="es">aburrir</trd>
  <trdgrp lng="fr">
    <trd>ennuyer<klr tip="ind"> (lasser)</klr></trd>,
    <trd>lasser<klr tip="ind"> (ennuyer)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&shin;&ayin;&mem;&fmem;</trd>
  <trd lng="ja">&#x9000;&#x5C48;&#x3055;&#x305B;&#x308B;  <pr>&#x305F;&#x3044;&#x304F;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">vervelen</trd>
  <trdgrp lng="pl">
    <trd>nudzi&cacute;</trd>,
    <trd>zanudza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_k;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_s;&c_t;&c_i;
    &c_s;&c_k;&c_u;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nudi&tcaron;</trd>,
    <trd>vzbudzova&tcaron; nudu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_u;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_n;&c_u;&c_d;&c_mol;&c_g;&c_u;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_o;&c_k;&c_u;&c_ch;&c_a;&c_t;&c_i;</trd>
