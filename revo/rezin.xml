<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rezin.xml,v 1.28 2025/12/31 22:33:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rezin</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="rezin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rezin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Glueca brulema suko de kelkaj arboj, precipe de koniferoj:
      <ekz>
        pecetoj da ligno <klr>[&#x2026;]</klr> gluitaj inter si kredeble per <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ricevis <klr>[&#x2026;]</klr> <tld/>e odoretantan vinon
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is <klr>(per <ref cel="kolofo.0o">kolofono</ref>)</klr> la malnovan ar&ccirc;on,
        metis la violonon sub la mentonon kaj dum interna
        tremo ektu&scirc;is la kordojn
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la juna picearo <klr>[&#x2026;]</klr> elspiris
        spican aromon de <tld/>o
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam li strie sen&scirc;eligis la trunkojn, la <tld/>o fluis kvaza&ubreve; maldensa suko kaj
        alglui&gcirc;is al la manoj
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        flami&gcirc;ema kiel ligno<ind> <tld/>a</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">harzig</trd>
      </ekz>
    </dif>
  <trd lng="en">resin <klr>(biology)</klr></trd>
  </snc>
  <snc mrk="rezin.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Glueca <ref tip="super" cel="plast.0o.TEK">plasto</ref> kapabla
      moli&gcirc;i kaj provizore a&ubreve; definitive malmoli&gcirc;i sub varmo&scirc;an&gcirc;oj:
      <ekz>
        silikona <tld/>o
        <fnt>
        <aut>E. Raus, trad. A. De Salvo</aut>,
        <vrk>Nova kuracmetodo...</vrk>,
        <lok><url ref="http://www.esperokatolika.org/ek19761980/ek1976_07.htm">
        Espero Katolika, 1976, numero 7a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">resin <klr>(chemical)</klr></trd>
  </snc>
  <refgrp tip="sub">
    <ref cel="kolofo.0o">kolofono</ref>,
    <ref cel="bakeli.0o">bakelito</ref>,
    <ref cel="sukcen.0o">sukceno</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_m;&c_a;&c_l;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_s;&c_m;&c_o;&c_l;&c_a;</trd>
  <trd lng="ca">resina</trd>
  <trdgrp lng="cs">
    <trd>prysky&rcaron;ice</trd>,
    <trd>smola</trd>,
    <trd>sm&uring;la</trd>
  </trdgrp>
  <trd lng="de">Harz</trd>
  <trd lng="es">resina</trd>
  <trd lng="fr">r&eacute;sine</trd>
  <trd lng="hu">gyanta</trd>
  <trd lng="ja">&#x6A39;&#x8102;  <pr>&#x3058;&#x3085;&#x3057;</pr></trd>
  <trd lng="nl">hars</trd>
  <trdgrp lng="no">
    <trd>harpiks</trd>,
    <trd>kvae</trd>
  </trdgrp>
  <trd lng="pl">&zdot;ywica</trd>
  <trd lng="pt">resina</trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_n;&c_a;&c_ja; <ind>&c_s;&c_m;&c_o;&c_l;&c_a;</ind></trd>
  <trdgrp lng="sk">
    <trd>smola</trd>,
    <trd>&zcaron;ivica</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_v;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_n;&c_a; &c_s;&c_m;&c_o;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6811;&#x8102; <pr>sh&ugrave;zh&imacron;</pr></trd>,
    <trd>&#x6A39;&#x8102; <pr>sh&ugrave;zh&imacron;</pr></trd>,
    <trd>&#x6811;&#x80F6; <pr>sh&ugrave;ji&amacron;o</pr></trd>,
    <trd>&#x6A39;&#x81A0; <pr>sh&ugrave;ji&amacron;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rezin.epoksi0o">
  <kap>epoksi<tld/>o, <var><kap>epoks<tld/>o</kap></var></kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="rezin.0o.KEM">Rezino <sncref/></ref>,
      konsistanta el homo- a&ubreve; hetero-polimero de epoksihava(j)
    molekulo(j), industrie uzata por gluoj, en <ref tip="dif" cel="elektronik.0o.ELE">elektroniko</ref> kaj en
      elektrotekniko:
      <ekz>
        la impregnaj materioj povas esti produkta&jcirc;oj de mineraloleo,
        vaksoj, epoksi-<tld/>o
        <fnt>
          <vrk><url ref="http://espleono.uw.hu/kondensiloj.pdf">Kondensiloj</url>
          </vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kart.cirkvit0o">cirkvitkarto</ref>
  <trd lng="en">epoxy <ind>resin</ind></trd>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_a;&c_k;&c_s;&c_y;&c_d;&c_n;&c_a;&c_ja; <ind>&c_s;&c_m;&c_a;&c_l;&c_a;</ind></trd>
  <trd lng="de">Epoxidharz</trd>
  <trd lng="fr">r&eacute;sine d'&eacute;poxy</trd>
<trd lng="pl">&zdot;ywica epoksydowa</trd>
  <trdgrp lng="zh">
    <trd>&#x73AF;&#x6C27;&#x6811;&#x8102; <pr>hu&aacute;ny&#x1CE;ngsh&ugrave;zh&imacron;</pr></trd>,
    <trd>&#x74B0;&#x6C27;&#x6A39;&#x8102; <pr>hu&aacute;ny&#x1CE;ngsh&ugrave;zh&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rezin.arta0o">
  <kap>arta <tld/>o, <var><kap>sinteza <tld/>o</kap></var></kap>
  <snc mrk="rezin.arta0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="rezin.0o.KEM"><tld lit="R"/>o<sncref/></ref>:
      <ekz>
        bakelito kaj aliaj sintezaj <tld/>oj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;amoniako&rightquot;,
        2007-06-30</lok></fnt>.
      </ekz>
    </dif>
  <trd lng="en">synthetic <ind>resin</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_n;&c_t;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_s;&c_m;&c_a;&c_l;&c_a;</ind></trd>,
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_s;&c_m;&c_a;&c_l;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>syntetick&aacute; prysky&rcaron;ice</trd>,
    <trd>um&ecaron;l&aacute; prysky&rcaron;ice</trd>
  </trdgrp>
  <trd lng="de">Kunstharz</trd>
  <trd lng="fr">r&eacute;sine synth&eacute;tique</trd>
  <trd lng="ja">&#x5408;&#x6210;&#x6A39;&#x8102;  <pr>&#x3054;&#x3046;&#x305B;&#x3044;&#x3058;&#x3085;&#x3057;</pr></trd>
  <trd lng="pl">&zdot;ywica syntetyczna</trd>
  <trd lng="sk">umel&aacute; &zcaron;ivica</trd>
  <trdgrp lng="zh">
    <trd>&#x4EBA;&#x9020;&#x677E;&#x9999; <pr>r&eacute;nz&agrave;os&omacron;ngxi&amacron;ng</pr></trd>,
    <trd>&#x5408;&#x6210;&#x6811;&#x8102; <pr>h&eacute;ch&eacute;ngsh&ugrave;zh&imacron;</pr></trd>,
    <trd>&#x5408;&#x6210;&#x6A39;&#x8102; <pr>h&eacute;ch&eacute;ngsh&ugrave;zh&imacron;</pr></trd>
  </trdgrp>
</drv>

</art>
