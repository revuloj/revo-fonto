<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rezin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rezin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rezin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Glueca brulema suko de kelkaj arboj, precipe de koniferoj.
    </dif>
  </snc>
  <snc mrk="rezin.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Glueca <ref tip="super" cel="plast.0o.TEK">plasto</ref> kapabla
      moli&gcirc;i kaj provizore a&ubreve; definitive malmoli&gcirc;i sub varmo&scirc;an&gcirc;oj:
      <ekz>
        oni trempas la &scirc;tonon <klr>...</klr> per silikona <tld/>o, kiu
        enpenetras 10 centimetrojn
        <fnt>
        <aut>E. Raus, trad. A. De Salvo</aut>,
        <vrk>Nova kuracmetodo...</vrk>,
        <lok><url
        ref="http://www.esperokatolika.org/ek19761980/ek1976_07.htm">
        Espero Katolika, 1976, numero 7a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_l;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_s;&c_m;&c_o;&c_l;&c_a;</trd>
  <trd lng="ca">resina</trd>
  <trd lng="de">Harz</trd>
  <trd lng="en">resin</trd>
  <trd lng="es">resina</trd>
  <trd lng="fr">r&eacute;sine</trd>
  <trd lng="hu">gyanta</trd>
  <trd lng="nl">hars</trd>
  <trd lng="pt">resina</trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_n;&c_a;&c_ja; <ind>&c_s;&c_m;&c_o;&c_l;&c_a;</ind></trd>
</drv>

<drv mrk="rezin.arta0o">
  <kap>arta <tld/>o, <var><kap>sinteza <tld/>o</kap></var></kap>
  <snc mrk="rezin.arta0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="rezin.0o.KEM">rezino <sncref/></ref>
    <ekz>
      produktado de bakelito kaj aliaj sintezaj <tld/>oj
      <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;amoniako&rightquot;,
      2007-06-30</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Kunstharz</trd>
  <trd lng="fr">r&eacute;sine synth&eacute;tique</trd>
</drv>
</art>
