<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kavern.xml,v 1.22 2022/03/21 17:51:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kavern</rad>/o
</kap>
<drv mrk="kavern.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kavern.0o.KOMUNE">
    <dif>
      Natura profunda subtera <ref tip="super" cel="kav.0o">kavo</ref>:
      <ekz>
        <nom>Lot</nom> <klr>[&#x2026;]</klr> lo&gcirc;is en <tld/>o, li kaj liaj du filinoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        leonido a&ubreve;digas sian vo&ccirc;on el sia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Amos 3:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis subterajn riverojn kaj lagojn, kristalajn konstrua&jcirc;ojn, tute mallumajn
        <tld/>ojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li priskribis al ili la inferon kiel malbonodoran <tld/>on, en kiun enfluas la
        tuta malpura&jcirc;o kaj maldelikata&jcirc;o de la tuta mondo
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galeri.0o.mineja">galerio<sncref/></ref>,
      <ref cel="grot.0o">groto</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_ja;&c_ch;&c_o;&c_r;&c_a;</trd>  
    <trdgrp lng="br">
      <trd>kev<klr tip="ind"> (mougev)</klr></trd>,
      <trd>mougev</trd>
    </trdgrp>
    <trd lng="de">H&ouml;hle</trd>
    <trdgrp lng="en">
      <trd>cavern</trd>,
      <trd>cave</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>caverne<klr tip="ind"> (grotte)</klr></trd>,
      <trd>grotte<klr tip="ind"> (naturelle)</klr></trd>
    </trdgrp>
    <trd lng="hu">barlang</trd>
    <trd lng="id">gua</trd>
    <trdgrp lng="nl">
      <trd>hol <klr tip="ind">(z.nw.)</klr></trd>,
      <trd>spelonk</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>jaskinia</trd>,
      <trd>pieczara</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>pe&scedil;ter&abreve;</trd>,
      <trd>vizuin&abreve;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_shch;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="kavern.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsaneca kavo en organo:
      <ekz>
        <tld/>oj de la pulmoj &ccirc;e tuberkulozo;
      </ekz>
      <ekz>
        <tld/>o de <klr>[&#x2026;]</klr> arbo
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_u;&c_p;&c_l;&c_o;</trd>,
      <trd>&c_k;&c_a;&c_v;&c_e;&c_r;&c_n;&c_a;</trd>
    </trdgrp>             
    <trd lng="br">kleuzadenn<klr tip="ind"> (kle&ntilde;ved)</klr></trd>
    <trd lng="de">Kaverne</trd>
    <trd lng="en">cavern</trd>
    <trd lng="hu">kaverna</trd>
    <trd lng="fr">caverne<klr tip="ind"> (m&eacute;d.)</klr></trd>
    <trd lng="nl">holte</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_d;&c_u;&c_p;&c_l;&c_o;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_ba;&t_rata;&t_ga;&t_tsheg;&t_pha;&t_shabchu;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>dutina</trd>,
    <trd>jeskyn&ecaron;</trd>,
    <trd>kaverna</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dutina</trd>,
    <trd>jasky&ncaron;a</trd>,
    <trd>kaverna</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x430;&#x43B;&#x430;&#x43A;&#x442;&#x438;&#x442;&#x43E;&#x432;&#x430; &#x43F;&#x435;&#x447;&#x435;&#x440;&#x430;</trd>,
    <trd>&#x434;&#x443;&#x43F;&#x43B;&#x43E; (&#x443; &#x437;&#x443;&#x431;&#x456;)</trd>,
    <trd>&#x43A;&#x430;&#x432;&#x435;&#x440;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="kavern.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Prezentanta <tld/>on:
      <ekz>
        <tld/>a arbo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &scirc;tonego, dento, pulmo.
      </ekz>
    </dif>
    <trd lng="hu">barlangi</trd>
    <trd lng="pl">jaskiniowy</trd>
  </snc>
  <snc>
    <dif>
      Obtuze sonora kiel <tld/>o:
      <ekz>
        <tld/>a blovo, stertoro, vo&ccirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tomb">tomba</ref>,
      <ref cel="cxerk.0a">&ccirc;erka</ref>.
    </refgrp>
    <trd lng="hu">&ouml;bl&ouml;s</trd>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_ch;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="br">kleuz</trd>
  <trdgrp lng="cs">
    <trd>dut&yacute;</trd>,
    <trd>kavernov&yacute;</trd>
  </trdgrp>
  <trd lng="en">cavernous</trd>
  <trdgrp lng="fr">
    <trd>creux<klr tip="ind"> (pr&eacute;sentant des cavernes)</klr></trd>,
    <trd>pr&eacute;sentant des <ind>cavernes</ind></trd>,
    <trd>de <ind>caverne</ind></trd>,
    <trd>caverneux</trd>
  </trdgrp>
  <trd lng="nl">hol</trd>
  <trd lng="ru">&c_p;&c_je;&c_shch;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dut&yacute;</trd>,
    <trd>kavernov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x447;&#x435;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x434;&#x443;&#x43F;&#x43B;&#x430;&#x432;&#x438;&#x439; (&#x43F;&#x440;&#x43E; &#x437;&#x443;&#x431;)</trd>
  </trdgrp>
</drv>
</art>
