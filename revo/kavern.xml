<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kavern</rad>/o
</kap>
<drv mrk="kavern.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Natura profunda subtera kavo:
      <ekz>la anta&ubreve;historioj homoj lo&gcirc;is en <tld/>oj.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grot.0o">groto</ref>.
    </refgrp>
    <trd lng="de">H&ouml;hle</trd>
    <trdgrp lng="nl">
      <trd>hol <klr tip="ind">(z.nw.)</klr></trd>,
      <trd>spelonk</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_shch;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Malsaneca kavo en organo:
      <ekz><tld/>oj de la pulmoj &ccirc;e tuberkulozo;</ekz>
      <ekz><tld/>o en arbo<fnt>Z</fnt>.</ekz> 
    </dif>
    <trd lng="de">Kaverne</trd>
    <trd lng="nl">holte</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_d;&c_u;&c_p;&c_l;&c_o;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="kavern.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Prezentanta <tld/>on:
      <ekz><tld/>a arbo<fnt>Z</fnt>,</ekz>
      <ekz>&scirc;tonego, dento, pulmo.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Obtuze sonora kiel <tld/>o:
      <ekz><tld/>a blovo, stertoro, vo&ccirc;o</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tomb">tomba</ref>,
      <ref cel="cxerk.0a">&ccirc;erka</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">hol</trd>
  <trd lng="ru">&c_p;&c_je;&c_shch;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
