<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>algebr</rad>/o
</kap>
<drv mrk="algebr.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Scienco de kalkulado pri la kvantoj, reprezentataj  per literoj k ne per
ciferoj.
</dif>
<trd lng="fr">alg&egrave;bre (discipline)</trd>
<trd lng="ru">&#1072;&#1083;&#1075;&#1077;&#1073;&#1088;&#1072;</trd>
</snc>
</drv>

     <drv mrk="algebr.0o.lineara">
       <kap>lineara <tld/>o</kap>
       <snc mrk="algebr.0o.lin-fako">
         <uzo tip="fak">MAT</uzo>
         <dif>
           <tld lit="A"/>o kadre de
           <ref cel="modul.0o.magmo">modulo</ref> a&ubreve;
           <ref cel="spac.vektora0o">vektora spaco</ref>.
         </dif>
         <trd lng="fr">alg&egrave;bre lin&eacute;aire (discipline)</trd>
         <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja; 
<ind>&c_a;&c_l;&c_g;&c_je;&c_b;&c_r;&c_a; 
(&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t;)</ind></trd>
       </snc>

       <snc mrk="algebr.0o.lin-magmo">
         <uzo tip="fak">MAT</uzo>
         <dif>
           <klr>(super ringo <em>R</em>)</klr>
           Tia <ref cel="magm.0o.MAT">magmo</ref>
           (<em>A</em>,+,x,*), ke (<em>A</em>,+,x) estas
           <ref tip="super" cel="ring.0o.MAT">ringo</ref> kaj
           (<em>A</em>,+,*) estas
           <ref tip="super" cel="modul.0o.magmo">modulo</ref> super <em>R</em>:
           <ekz>
             la polinomringo de ringo <em>R</em> estas <tld/>o super &gcirc;i.
           </ekz>
         </dif>
         <trd lng="fr">alg&egrave;bre lin&eacute;aire (structure)</trd>
         <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja; 
<ind>&c_a;&c_l;&c_g;&c_je;&c_b;&c_r;&c_a;</ind> 
(&c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;)</trd>
       </snc>
       <rim>
         Por eviti beda&ubreve;rindan kolizion inter sencoj
         <sncref ref="algebr.0o.lin-fako"/>
         kaj <sncref ref="algebr.0o.lin-magmo"/>
         kelkaj proponas uzi la paronimon <ctl>al&gcirc;ebro</ctl>
         por la dua nocio.
       </rim>
     </drv>

</art>
