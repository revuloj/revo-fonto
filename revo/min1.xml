  <trd lng="ml">&#xD0E;&#xD28;&#xD4D;&#xD31;&#xD46;</trd>
  <trd lng="mr">&#x916;&#x93E;&#x923;</trd>
  <trd lng="my">&#x101E;&#x1010;&#x1039;&#x1010;&#x102F;&#x1010;&#x103D;&#x1004;&#x103A;&#x1038;</trd>
  <trd lng="ny">wanga</trd>
  <trd lng="pa">&#xA2E;&#xA47;&#xA30;&#xA47; &#xA32;&#xA08;</trd>
