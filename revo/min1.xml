<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: min1.xml,v 1.82 2022/09/15 08:45:25 revo Exp $">
<kap>
  <rad>min</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="min1.0o">
  <kap><tld/>o</kap>
  <snc mrk="min1.0o.MEZ_PEZ">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> de
      pezo, 60-ono de <ref tip="malprt" cel="talant.0o.pezunuo">talanto</ref>,
      &ccirc; 500 gramoj:
      <ekz>
        se ekzistus nacio, kiu povus &scirc;an&gcirc;i al ni &ccirc;ion &ccirc;i je oro, 
        la faraono havus &ccirc;iujare cent tridek tri mil <tld/>ojn da oro
        kaj &ccirc;ar unu soldato povas porti sur la dorso 26 <tld/>ojn da &scirc;ar&gcirc;o, 
        do por transporti tiun &ccirc;i oron oni devus alvoki &ccirc;irka&ubreve; kvin mil soldatojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="de">Mine<klr tip="ind"> (Gewichtseinheit)</klr></trd>
    <trd lng="el">&my;&ny;&alfa;</trd>
    <trd lng="en">mina (unit)</trd>
    <trd lng="et">mann</trd>
    <trd lng="fr">mine<klr tip="amb"> (unit&eacute; de mesure)</klr></trd>
    <trd lng="he">&mem;&yod;&nun;&he;</trd>
    <trd lng="hr">mina</trd>
    <trd lng="it">mina</trd>
    <trd lng="lt">mina</trd>
    <trd lng="pt">mina <klr tip="amb">(antiga medida de
    peso</klr></trd>
    <trd lng="nl">mina <klr>(eenheid)</klr></trd>
    <trd lng="no">Eininga mine</trd>
    <trd lng="ru">&c_m;&c_i;&c_n;&c_a; <klr>(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
    <trd lng="uk">&c_m;&c_ib;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="min1.0o.MEZ_MON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref> 60-ono de
      <ref tip="malprt" cel="talant.0o.monunuo">talanto</ref>.
    </dif>
    <trd lng="de">Mine<klr tip="ind"> (M&uuml;nze)</klr></trd>
    <trd lng="pt">mina <klr tip="amb">(antiga moeda)</klr></trd>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
  </snc>
  <rim mrk="min1.0o.RIM_hom">
    Samforma fundamenta vorto
    <ref tip="hom" cel="min.0o">mino</ref>
    signifas lokon de elfosado de &scirc;tonoj a&ubreve; mineraloj.
  </rim>
  <trd lng="pl">mina</trd>
</drv>
</art>

