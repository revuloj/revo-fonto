<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>min</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="min1.0o">
  <kap><tld/>o</kap>
  <snc mrk="min1.0o.MEZ_PEZ">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">Mezurunuo</ref> de
      pezo,
      60-ono de <ref tip="malprt" cel="talant.0o.pezunuo">talanto</ref>,
      ordinare &ccirc;irka&ubreve; 400 a&ubreve; 500 gramojn valora:
      <ekz>
        unu soldato povas porti sur la dorso 26 <tld/>ojn da &scirc;ar&gcirc;o
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mine<klr tip="ind">(Gewichtseinheit)</klr></trd>
    <trd lng="pt">mina <klr tip="amb">(antiga medida de
    peso</klr></trd>
  </snc>
  <snc mrk="min1.0o.MEZ_MON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref> 60-ono de
      <ref tip="malprt" cel="talant.0o.monunuo">talanto</ref>.
    </dif>
    <trd lng="de">Mine<klr tip="ind">(M&uuml;nze)</klr></trd>
    <trd lng="pt">mina <klr tip="amb">(antiga moeda)</klr></trd>
  </snc>
  <rim mrk="min1.0o.RIM_hom">
    Samforma fundamenta vorto
    <ref tip="hom" cel="min.0o">mino</ref>
    signifas lokon de elfosado de &scirc;tonoj a&ubreve; mineraloj.
  </rim>
  <trd lng="fr">mine<klr tip="amb"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="pl">mina</trd>
</drv>
</art>

