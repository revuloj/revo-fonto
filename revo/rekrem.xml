<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>rekrement</rad>/o
</kap>
<drv mrk="rekrem.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Resta&jcirc;o el fruktoj, grenoj, herboj a&ubreve; aliaj vegeta&jcirc;aj substancoj, el
      kiuj oni ekstraktis sukon, esencon a&ubreve; alian utiligebla&jcirc;on:
      <ekz>
        bier<tld/>o, vinber<tld/>o, brand<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fecx.0o">fe&ccirc;o</ref>,
      <ref cel="skori.0o">skorio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Treber</trd>
  <trd lng="nl">pulp</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_zh;&c_i;&c_m;&c_k;&c_i;</trd>,
    <trd>&c_zh;&c_m;&c_y;&c_h;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>
