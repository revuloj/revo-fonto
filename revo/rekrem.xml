<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rekrem.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>rekrement</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rekrem.0o">
  <kap><tld/>o</kap>
  <snc mrk="rekrem.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Resta&jcirc;o el fruktoj, grenoj, herboj a&ubreve; aliaj vegeta&jcirc;aj partoj,
      el kiuj oni elpremis sukon, esencon a&ubreve; alian utiligebla&jcirc;on:
      <ekz>
        bier<tld/>o, vinber<tld/>o, brand<tld/>o;
      </ekz>
      <ekz>
        a&ccirc;e odoris <tld/>o kaj mortintaj fi&scirc;oj
        <fnt>
        <aut>L. Lukina, E. Lukin, trad. J. Finkel</aut>,
        <vrk>La misiistoj</vrk>,
        <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis2.html">
        parto 2a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fecx.0o">fe&ccirc;o</ref>,
      <ref cel="skori.0o">skorio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_m;&c_je;&c_r;&c_y;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mláto</trd>,
    <trd>rostlinný odpad</trd>,
    <trd>sedlina po vyluhování</trd>,
    <trd>zbytek po lisování</trd>
  </trdgrp>
  <trd lng="de">Treber</trd>
  <trdgrp lng="en">
    <trd>draff</trd>,
    <trd>dregs</trd>,
    <trd>dross</trd>,
    <trd>grounds</trd>,
    <trd>husks</trd>,
    <trd>lees</trd>,
    <trd>refuse</trd>,
    <trd>residue</trd>,
    <trd>waste</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&egrave;ne<klr> (r&eacute;sidu apr&egrave;s pressurage)</klr></trd>,
    <trd>marc<klr> (r&eacute;sidu apr&egrave;s pressurage)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&ouml;rk&ouml;ly</trd>
  <trd lng="nl">pulp</trd>
  <trdgrp lng="pl">
    <trd>odpadki</trd>,
    <trd>wywar <klr>(gorzelniany)</klr></trd>,
    <trd>zyny <klr>(winne)</klr></trd>,
    <trd>s&lstroke;odziny <klr>(browarniane)</klr></trd>,
    <trd>wyt&lstroke;oki <klr>(cukrownicze)</klr></trd>,
    <trd>wys&lstroke;odki</trd>
  </trdgrp>
  <trd lng="pt">res&iacute;duo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_zh;&c_i;&c_m;&c_k;&c_i;</trd>,
    <trd>&c_zh;&c_m;&c_y;&c_h;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kal <klr>(rastlinného pôvodu)</klr></trd>,
    <trd>rastlinné výlisky</trd>,
    <trd>usadenina</trd>
  </trdgrp>
</drv>
</art>
