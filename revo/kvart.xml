<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kvart</rad>/o
</kap>
<drv mrk="kvart.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="kvart.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el kvar sinsekvaj tonoj de gamo.
    </dif>
  </snc>
  <snc mrk="kvart.0o.de.toniko">
    <dif>
      Kvara tono de gamo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a; <klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="de">Quart</trd>
  <trdgrp lng="fr">
    <trd>quarte<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">kvart</trd>
  <trd lng="nl">kwart</trd>
  <trd lng="pt">quarta <klr tip="ind">(m&uacute;s.)</klr></trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;</trd>
</drv>
</art>
