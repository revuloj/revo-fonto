<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvart</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvart.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvart.0o.intervalo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el kvar sinsekvaj tonoj de gamo:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> kompletigas kvinton al oktavo
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=27349">Piano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; en la 13-a jarcento la duonduton oni spertis kiel &gcirc;enan
        intervalon 
        ne facile kompreneblan, kiel la neracian resta&jcirc;on inter la
        perfekta <tld/>o kaj la dutono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35924">Kadenco
          (muziko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kvart.0o.de_toniko">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kvara tono de gamo.
    </dif>
  </snc>
  <rim>
    Nur la mezurunuo
    <ref tip="hom" cel="kvart1.0o.MEZ">kvarto</ref>
    estas fundamenta.
  </rim>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a; <klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;
  &c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="de">Quarte</trd>
  <trd lng="en">fourth<klr> (musical)</klr></trd>
  <trdgrp lng="fr">
    <trd>quarte<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">kvart</trd>
  <trd lng="nl">kwart</trd>
  <trd lng="pl">kwarta <klr>(interwa&lstroke;)</klr></trd>
  <trd lng="pt">quarta <klr tip="ind">(m&uacute;s.)</klr></trd>
  <trd lng="ro">cvart&abreve;</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;</trd>
  <trd lng="sk">kvarta</trd>
  <trdgrp lng="cs">
    <trd>kvart</trd>,
    <trd>v&ecaron;rtel <klr>(dut&aacute; m&iacute;ra)</klr></trd>
  </trdgrp>
</drv>
</art>
