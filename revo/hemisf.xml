<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hemisfer</rad>/o
</kap>
<drv mrk="hemisf.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="hemisf.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <ref tip="dif" cel="sfer.duon0o">duonsfero</ref>
    <trd lng="de">Halbkugel</trd>
    <trd lng="nl">halve <ind>bol</ind></trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu el la du partoj de la tero dividitaj per la ekvatoro.
    </dif>
    <trd lng="de">Hemisph&auml;re</trd>
    <trd lng="nl">halfrond</trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu el la du partoj de la &ccirc;iela sfero dividita per la ekvatora ebeno.
    </dif>
    <trd lng="de">Hemisph&auml;re</trd>
    <trd lng="nl">halfrond</trd>
  </snc>
  <trd lng="fr">h&eacute;misph&egrave;re</trd>
  <trd lng="nl">hemisfeer</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
</drv>

<drv mrk="hemisf.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Havanta formon de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>halbkugelig</trd>,
    <trd>hemisph&auml;risch</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;misph&eacute;rique</trd>
  <trd lng="nl">half <ind>rond</ind></trd>
</drv>
</art>
