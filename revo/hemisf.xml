<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemisf.xml,v 1.34 2026/02/09 06:00:21 revo Exp $">
<kap>
  <rad>hemisfer</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="hemisf.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="dif" cel="sfer.duon0o.AST">Duonsfero</ref>:
      <ekz>
        la veraj organizantoj de la ekspluatado estas personoj kaj kompanioj kiuj havas
        sian bazon en la norda <tld/>o (Ameriko, E&ubreve;ropo kaj Azio)
        <fnt>
          <bib>Monato</bib>
          <aut>Serge Zandandu Ntomono Zola</aut>
          <vrk><url ref="&Monato;/2003/007982.html">Disrabado de naturaj ri&ccirc;a&jcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la suda <tld/>o nun ja estas vintro, evidente tute normala
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010105.php">Varmo kaj frosto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
    <trd lng="de">Hemisph&auml;re</trd>
    <trdgrp lng="hu">
      <trd>hemiszf&eacute;ra</trd>,
      <trd>&eacute;g&ouml;mb&eacute;l</trd>
    </trdgrp>
    <trd lng="nl">halfrond</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="hemisf.0o.de_cerbo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Maldekstra a&ubreve; dekstra parto de la granda
      <ref tip="malprt" cel="cerb.0o.ANA">cerbo<sncref/></ref>:
      <ekz>
        oni trovis malpli elvolvi&gcirc;intan blankan substancon en la maldekstra cerbo<tld/>o
        proporcie kun la kvanto de aermalpura&jcirc;oj ensorbitaj de la patrinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo S&eacute;rgio Viana</aut>
          <vrk><url ref="&Monato;/2015/011485.php">Malpura aero dama&gcirc;as infanan cerbon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_w;&c_k;&c_u;&c_l;&c_ja; &c_m;&c_a;&c_z;&c_g;&c_o;&c_w;</trd>,
      <trd>&c_p;&c_a;&c_w;&c_g;&c_a;&c_l;&c_a; &c_m;&c_a;&c_z;&c_g;&c_o;&c_w;</trd>
    </trdgrp>
  </snc>
  <snc mrk="hemisf.0o.MAT">
    <uzo tip="stl">EVI</uzo>
    <uzo tip="fak">MAT</uzo>
    <ref tip="dif" cel="sfer.duon0o">duonsfero</ref>
    <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
    <trd lng="de">Halbkugel</trd>
    <trd lng="hu">f&eacute;lg&ouml;mb</trd>
    <trd lng="nl">halve <ind>bol</ind></trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <refgrp tip="dif">
      <ref cel="glob.duon0o">duonglobo</ref>,
      <ref cel="sfer.duon0o.GEOG">duonsfero<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_w;&c_sh;&c_a;&c_r;'&c_je;</trd>,
      <trd>&c_p;&c_a;&c_w;&c_k;&c_u;&c_l;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Hemisph&auml;re</trd>
    <trd lng="nl">halfrond</trd>
    <trdgrp lng="hu">
      <trd>f&eacute;lteke</trd>,
      <trd>f&eacute;lg&ouml;mb</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
  </snc>
  <trd lng="ca">hemisferi</trd>
  <trdgrp lng="cs">
    <trd>hemisf&eacute;ra</trd>,
    <trd>polokoule mozku</trd>,
    <trd>p&uring;lkulov&aacute; plocha</trd>
  </trdgrp>
  <trd lng="el">&eta;&my;&jota;&sigma;&phi;&alfa;&jota_ton;&rho;&jota;&omikron;</trd>
  <trd lng="es">hemisferio</trd>
  <trd lng="fr">h&eacute;misph&egrave;re</trd>
  <trd lng="it">emisfero</trd>
  <trd lng="ja">&#21322;&#29699;  <pr>&#12399;&#12435;&#12365;&#12421;&#12358;</pr></trd>
  <trd lng="nl">hemisfeer</trd>
  <trd lng="pl">hemisfera</trd>
  <trd lng="pt">hemisf&eacute;rio</trd>
  <trdgrp lng="sk">
    <trd>hemisf&eacute;ra</trd>,
    <trd>pologu&lcaron;a mozgu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_v;&c_k;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_g;&c_je;&c_m;&c_ib;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21322;&#29699; <pr>b&agrave;nqi&uacute;</pr></trd>,
    <trd>&#21322;&#29699;&#20307; <pr>b&agrave;nqi&uacute;t&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hemisf.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="sfer.duon0a">duonsfera</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">hemisf&egrave;ric</trd>
  <trdgrp lng="de">
    <trd>halbkugelig</trd>,
    <trd>hemisph&auml;risch</trd>
  </trdgrp>
  <trd lng="el">&eta;&my;&jota;&sigma;&phi;&alfa;&jota;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">h&eacute;misph&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;lg&ouml;mb-</trd>,
    <trd>f&eacute;lteke-</trd>,
    <trd>hemiszf&eacute;rikus</trd>
  </trdgrp>
  <trd lng="it">emisferico</trd>
  <trd lng="ja">&#21322;&#29699;&#12398;  <pr>&#12399;&#12435;&#12365;&#12421;&#12358;&#12398;</pr></trd>
  <trd lng="nl">half <ind>rond</ind></trd>
  <trd lng="pt">hemisf&eacute;rico</trd>
  <trd lng="pl">p&oacute;&lstroke;kulisty</trd>
  <trd lng="zh">&#21322;&#29699;&#24418; <pr>b&agrave;nqi&uacute;x&iacute;ng</pr></trd>
</drv>
</art>
