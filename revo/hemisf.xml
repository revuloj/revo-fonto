<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hemisfer</rad>/o
</kap>
<drv mrk="hemisf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <dif>
    plej ofte AST 
  </dif>
  <snc num="1">
    <dif>
      &Ccirc;iu el la duonpartoj de sfero (= duonsfero). 
    </dif>
    <trd lng="de">Halbkugel</trd>
  </snc>
  <snc num="2">
    <dif>
      &Ccirc;iu el la du partoj de la tero dividitaj per la ekvatoro. 
    </dif>
    <trd lng="de">Hemisph&auml;re</trd>
  </snc>
  <snc num="3">
    <dif>
      &Ccirc;iu el la du partoj de la &ccirc;iela sfero dividita per la ekvatora ebeno. 
    </dif>
    <trd lng="de">Hemisph&auml;re</trd>
  </snc>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
</drv>
<drv mrk="hemisf.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta formon de <tld/>o.  
    </dif>
  </snc>
    <trdgrp lng="de">
     <trd>halbkugelig</trd>,
     <trd>hemisph&auml;risch</trd>
    </trdgrp>
</drv>
</art>
