<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>hemisfer</rad>/o
    </kap>

    <drv mrk="hemisf.0o">
      <kap><tld/>o</kap>
      <fnt><bib>PV</bib></fnt>
      <snc mrk="hemisf.0o.MAT">
        <uzo tip="fak">MAT</uzo>
        <uzo tip="stl">EVI</uzo>
        <dif>
          <ref tip="dif" cel="sfer.duon0o">Duonsfero</ref>.
        </dif>
        <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
        <trd lng="de">Halbkugel</trd>
        <trd lng="hu">f&eacute;lg&ouml;mb</trd>
        <trd lng="nl">halve <ind>bol</ind></trd>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
      </snc>
      <snc>
        <refgrp tip="dif">
          <ref cel="glob.duon0o">duonglobo</ref>,
          <ref cel="sfer.duon0o.GEOG">duonsfero<sncref/></ref>
        </refgrp>
        <trdgrp lng="be">
          <trd>&c_p;&c_a;&c_w;&c_sh;&c_a;&c_r;'&c_je;</trd>,
          <trd>&c_p;&c_a;&c_w;&c_k;&c_u;&c_l;&c_ja;</trd>
        </trdgrp>
        <trd lng="de">Hemisph&auml;re</trd>
        <trd lng="nl">halfrond</trd>
        <trdgrp lng="hu">
          <trd>f&eacute;lteke</trd>,
          <trd>f&eacute;lg&ouml;mb</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
      </snc>
      <snc>
        <uzo tip="fak">AST</uzo>
        <ref tip="dif" cel="sfer.duon0o.AST">duonsfero</ref>
        <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
        <trd lng="de">Hemisph&auml;re</trd>
        <trdgrp lng="hu">
          <trd>hemiszf&eacute;ra</trd>,
          <trd>&eacute;g&ouml;mb&eacute;l</trd>
        </trdgrp>
        <trd lng="nl">halfrond</trd>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
      </snc>
      <trd lng="el">&eta;&my;&jota;&sigma;&phi;&alfa;&jota_ton;&rho;&jota;&omikron;</trd>
      <trd lng="es">hemisferio</trd>
      <trd lng="fr">h&eacute;misph&egrave;re</trd>
      <trd lng="nl">hemisfeer</trd>
      <trd lng="pl">hemisfera</trd>
      <trd lng="pt">hemisf&eacute;rio</trd>
    </drv>

    <drv mrk="hemisf.0a">
      <kap><tld/>a</kap>
      <snc>
        <ref tip="dif" cel="sfer.duon0a">duonsfera</ref>
        <dif>
          Havanta formon de <tld/>o.
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
      <trdgrp lng="de">
        <trd>halbkugelig</trd>,
        <trd>hemisph&auml;risch</trd>
      </trdgrp>
      <trd lng="el">&eta;&my;&jota;&sigma;&phi;&alfa;&jota;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
      <trd lng="fr">h&eacute;misph&eacute;rique</trd>
      <trdgrp lng="hu">
        <trd>f&eacute;lg&ouml;mb-</trd>,
        <trd>f&eacute;lteke-</trd>,
        <trd>hemiszf&eacute;rikus</trd>
      </trdgrp>
      <trd lng="nl">half <ind>rond</ind></trd>
      <trd lng="pt">hemisf&eacute;rico</trd>
      <trd lng="pl">p&oacute;&lstroke;kulisty</trd>
    </drv>
  </art>
