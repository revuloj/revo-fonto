<art mrk="$Id: minijup.xml,v 1.8 2024/01/26 17:40:06 revo Exp $">
<kap>
  <rad>minijup</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="minijup.0o">
  <kap><tld/>o</kap>
  <snc mrk="minijup.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tre mallonga <ref tip="super" cel="jup.0o.TEKS">jupo</ref> 
      kovranta de talio &gcirc;is supro de femuroj:
      <ekz>
        estus tute false kredi, ke mondumaj modistoj volonte
        dekretas <tld/>ojn
        <fnt>
          <aut>P. Thorsen</aut>,
          <vrk><url ref="http://www.poulthorsen.dk/jupo.htm">
          Jupoj kaj konjunkturoj</url></vrk>,
          <lok>1989</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;-&c_s;&c_p;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_m;&c_i;&c_n;&c_i;&c_zh;&c_u;&c_p;</trd>
  <trdgrp lng="cs">
    <trd>minisukn&ecaron;</trd>,
    <trd>minisukn&#x11B;</trd>
  </trdgrp>
  <trd lng="de">Minirock</trd>
  <trd lng="en">miniskirt</trd>
  <trd lng="es">minifalda</trd>
  <trd lng="fr">minijupe</trd>
  <trd lng="hu">miniszoknya</trd>
  <trd lng="it">minigonna</trd>
  <trd lng="ja">&#x30DF;&#x30CB;&#x30B9;&#x30AB;&#x30FC;&#x30C8;</trd>
  <trd lng="nl">minirok</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_i;-&c_ju;&c_b;&c_k;&c_a;</trd>
  <trd lng="sk">minisuk&ncaron;a</trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43D;&c_ib;-&c_s;&#x43F;&c_ib;&c_d;&#x43D;&c_i;&c_c;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x8D85;&#x77ED;&#x88D9; <pr>ch&#x101;odu&#x1CE;nq&#xFA;n</pr></trd>,
    <trd>&#x8FF7;&#x4F60;&#x88D9; <pr>m&#xED;n&#x1D0;q&#xFA;n</pr></trd>,
    <trd>&#x8D85;&#x77ED; <pr>ch&#x101;odu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
