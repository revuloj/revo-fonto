<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>variabl</rad>/o
    </kap>
    <drv mrk="variab.0o">
      <kap><tld/>o</kap>
      <snc mrk="variab.0o.mat">
        <uzo tip="fak">MAT</uzo>
        <dif>
          Simbolo uzata por indiki ajnan elementon de koncerna aro
          <klr>(el &gcirc;ia <em>variejo</em>)</klr>:
          <ekz>
            nedependa <tld/>o
            <klr>(= <ref cel="argume.0o.donitajxo">argumento</ref>)</klr>;
          </ekz><ekz>
            dependa <tld/>o
            <klr>(= <ref cel="funkci.0o.mat">funkcio</ref>)</klr>;
          </ekz><ekz>
            ligita, libera <tld/>oj <klr>(en logiko)</klr>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="variab.0o.komp">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Programa objekto posedanta
          <ref tip="prt" cel="nom.0o.de.io">nomon</ref> kaj
          <ref tip="prt" cel="valor.0o.mat">valoron</ref>, kiun lastan
          la programo povas ekzameni kaj eble e&ccirc; &scirc;an&gcirc;i.
          <ekz>
            deklari, uzi <tld/>on
          </ekz><ekz>
            loka, malloka <tld/>o.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">variable</trd>
      <trd lng="ru">&#x43f;&#x435;&#x440;&#x435;&#x43c;&#x435;&#x43d;&#x43d;&#x430;&#x44f;</trd>
    </drv>
  </art>

<!--
$Log$
-->
</vortaro>