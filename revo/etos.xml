<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>etos</rad>/o
</kap>
<drv mrk="etos.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <dif>
      Reganta humoro a&ubreve; spirita atmosfero kreata de &ccirc;eestantaj homoj,
      &ccirc;irka&ubreve;a&jcirc;o, cirkonstancoj a&ubreve; arta&jcirc;o:
      <!-- Reganta tono, humoro, sinteno a&ubreve; &gcirc;enerala impreso tipa
      por iu homgrupo, situacio, pejza&gcirc;o a&ubreve; arta verko: -->
      <ekz>
        la entuziasma <tld/>o de la Bulonja Kongreso;
      </ekz>
      <ekz>
        la suspektema <tld/>o de doganejo;
      </ekz>
      <ekz>
        la kortu&scirc;a <tld/>o de Zamenhofa verko;
      </ekz>
      <ekz>
        la <tld/>o en la oficejo estas stre&ccirc;a;
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        la muziko kreis agrablan <tld/>n
        <fnt><bib>9OA</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atmosf.0o">atmosfero</ref>,
      <ref cel="agord.0o">agordo</ref>,
      <ref cel="ton.0o">tono</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_t;&c_m;&c_a;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>,
      <trd>&c_d;&c_u;&c_h;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Stimmung</trd>,
      <trd>Atmosph&auml;re</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>atmosphere</trd>,
      <trd>ethos</trd>
    </trdgrp>
    <trd lng="es">ambiente</trd>
    <trdgrp lng="fr">
      <trd>ambiance</trd>,
      <trd>atmosph&egrave;re<klr tip="ind"> (ambiance)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hangulat</trd>,
      <trd>atmoszf&eacute;ra</trd>,
      <trd>hangolts&aacute;g</trd>,
      <trd>ethhosz</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ambi&#234;ncia</trd>,
      <trd>atmosfera espiritual</trd>,
      <trd>&#234;thos</trd>,
      <trd>etos</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_t;&c_m;&c_o;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_d;&c_u;&c_h;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>st&auml;mning</trd>,
      <trd>atmosf&auml;r</trd>
    </trdgrp>
  </snc>
</drv>
</art>
