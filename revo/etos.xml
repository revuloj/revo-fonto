<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>etos</rad>/o
    </kap>
    <drv mrk="etos.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Reganta tono, humoro, sinteno a&ubreve; &gcirc;enerala impreso tipa
          por iu homgrupo, situacio, pejza&gcirc;o a&ubreve; arta verko:
          <ekz>la entuziasma <tld/>o de la Bulonja Kongreso;</ekz>
          <ekz>la suspektema <tld/>o de doganejo;</ekz>
          <ekz>la kortu&scirc;a <tld/>o de Zamenhofa verko</ekz>.
        </dif>
        <refgrp tip="sin">
          <ref cel="atmosf.0o">atmosfero</ref>,
          <ref cel="agord.0o">agordo</ref>,
          <ref cel="ton.0o">tono</ref>.
        </refgrp>
        <trdgrp lng="de">
          <trd>Stimmung</trd>,
          <trd>Atmosph&auml;re</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>atmosphere</trd>,
          <trd>ethos</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&#x430;&#x442;&#x43c;&#x43e;&#x441;&#x444;&#x435;&#x440;&#x430;</trd>,
          <trd>&#x43d;&#x430;&#x441;&#x442;&#x440;&#x43e;&#x435;&#x43d;&#x438;&#x435;</trd>,
          <trd>&#x434;&#x443;&#x445;</trd>
        </trdgrp>
      </snc>
    </drv>
  </art>

