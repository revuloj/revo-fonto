<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: paleoklimatologi.xml,v 1.4 2024/02/16 19:24:10 revo Exp $">
<kap>
  <rad>paleoklimatologi</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="paleoklimatologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="paleoklimatologi.0o.MET">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      Scienco pri
      <ref tip="vid" cel="klimat.pra0o.MET">praklimato</ref>
      a&ubreve; <ref tip="vid" cel="paleoklimat.0o.MET">paleoklimato</ref>:
      <ekz>
        la plej grava faktoro <klr>...</klr> estas la 
        klimato, kies esploradon por la fora estinteco
        la nova scienco <tld/>o <klr>...</klr> starigis al 
        si kiel taskon
        <fnt>
          <aut>W. von Soden, trad. M. Wolf</aut>,
          <vrk><url ref="http://www.sorbzilla.de/aorient/aorient.pdf">
          Enkonduko en la Antikvan Orienton</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_je;&c_a;&c_k;&c_l;&c_ib;&c_m;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="en">
    <trd>palaeoclimatology</trd>,
    <trd>paleoclimatology</trd>
  </trdgrp>
  <trd lng="fr">pal&eacute;oclimatologie</trd>
  <trd lng="it">paleoclimatologia</trd>
  <trd lng="pl">paleoklimatologia</trd>
  <trd lng="ro">paleoclimatologie</trd>
  <trdgrp lng="zh">
    <trd>&#x53E4;&#x6C14;&#x5019;&#x5B66; <pr>g&#x1D4;q&#xEC;h&#xF2;uxu&#xE9;</pr></trd>,
    <trd>&#x53E4;&#x6C23;&#x5019;&#x5B78; <pr>g&#x1D4;q&#xEC;h&#xF2;uxu&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

