<vortaro>
<art mrk="$Id: fakocx.xml,v 1.18 2023/11/09 19:13:15 revo Exp $">
<kap>
  <rad>fako&ccirc;er</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad>fako&hcirc;er</rad>/o
        <fnt>
          <aut>Wouter F. Pilger</aut>
          <vrk><url ref="&STEB;vortaroj/mamulu8.htm">Provizora Privata Listo De Nomoj De Bestoj &dash; Mamuloj</url></vrk>
        </fnt></kap></var>
</kap>

<drv mrk="fakocx.0o">
  <kap><tld/>o,
    <var><kap>fako&hcirc;ero</kap></var>
  </kap>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="apr.veruk0o">Verukapro</ref>:
      <ekz>
        li estis kovrita per densa ursa felo kaj &scirc;ajnis &ccirc;iam ridetanta pro du longaj
        dentoj kiuj eliris de la bu&scirc;anguloj kaj similigis lin al <tld/>o
        <fnt>
          <aut>Sen Rodin</aut>
          <vrk><url ref="https://www.scribd.com/book/200213093/Libazar-kaj-Tero-Originala-romano-en-Esperanto">Libazar' kaj Tero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_r;&c_o;&c_d;&c_a;&c_v;&c_a;&c_ch;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_b;&c_a;&c_r;&c_o;&c_d;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_d;&c_a;&c_v;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_a; &c_s;&c_v;&c_i;&c_n;&c_ja;</trd>
  <trd lng="ca">facoquer</trd>
  <trd lng="de">Warzenschwein</trd>
  <trd lng="en">warthog</trd>
  <trd lng="fr">phacoch&egrave;re</trd>
  <trd lng="hu">varacskos <ind>diszn&oacute;</ind></trd>
  <trd lng="nl">wrattenzwijn</trd>
  <trdgrp lng="pl">
    <trd>guziec</trd>,
    <trd>&sacute;winia afryka&nacute;ska</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>javali-africano</trd>,
    <trd>facochero</trd>,
    <trd>porco-de-verrugas</trd>
  </trdgrp>
  <trd lng="zh">&#x75A3;&#x732A; <pr>y&#xF3;uzh&#x16B;</pr></trd>
</drv>

</art>
