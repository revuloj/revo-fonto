<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>artikl</rad>/o
    </kap>
    <drv mrk="artikl.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">EKON</uzo>
      <snc>
        <dif>
          Speco de varo:
          <ekz>
            la&ubreve; Elza &gcirc;in <klr>(la prezalti&gcirc;on de la ovoj)</klr>
            ka&ubreve;zas, ke la ovo estas sezona <tld/>o kaj nun la
            kokinoj ne ovumas
            <fnt>
              <aut>Szathmari</aut>: <vrk>Enciclopeditis</vrk>.
              <lok><url ref="http://www.esperanto.mv.ru/Szathmari/"
                  >Satiraj rakontoj</url>.</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>Artikel</trd>,
        <trd>Ware</trd></trdgrp>
      <trd lng="en">article</trd>
      <trd lng="es">art&iacute;culo</trd>
      <trd lng="fr">article (marchandise)</trd>
      <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;</trd>
      <trd lng="sv">artikel (handel)</trd>
    </drv>
  </art>
