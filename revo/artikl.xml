<vortaro>
  <art mrk="$Id: artikl.xml,v 1.24 2025/10/08 16:37:52 revo Exp $">
    <kap>
      <rad>artikl</rad>/o
    <fnt><bib>PVEF</bib></fnt></kap>
    <drv mrk="artikl.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">EKON</uzo>
      <snc>
        <dif>
          Speco de <ref tip="super" cel="var.0o">varo</ref>:
          <ekz>
            la&ubreve; Elza &gcirc;in <klr>(la prezalti&gcirc;on de la ovoj)</klr>
            ka&ubreve;zas, ke la ovo estas sezona <tld/>o kaj nun la
            kokinoj ne ovumas
            <fnt>
              <aut>Szathmari</aut>: <vrk>Enciclopeditis</vrk>.
              <lok><url ref="http://www.esperanto.mv.ru/Szathmari/">Satiraj rakontoj</url>.</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>varo</ke>, <ke>komerca&jcirc;o</ke>.
  </rim>

  <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l; <klr>(&c_t;&c_a;&c_v;&c_a;&c_r;)</klr></trd>
  <trd lng="bg">&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;</trd>
  <trd lng="ca">article</trd>
  <trdgrp lng="cs">
    <trd>druh zbo&zcaron;&iacute;</trd>,
    <trd>zbo&zcaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Artikel</trd>,
        <trd>Ware</trd></trdgrp>
  <trd lng="en">article</trd>
  <trd lng="es">art&iacute;culo</trd>
  <trd lng="fr">article (marchandise)</trd>
  <trdgrp lng="he">
    <trd>&pe;&resh;&yod;&tet;</trd>,
    <trd>&mem;&tsadi;&resh;&fkaf;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>barang dagangan</trd>,
    <trd>komoditas</trd>
  </trdgrp>
  <trd lng="it">articolo<klr> (merce)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#21697;&#29289;  <pr>&#12375;&#12394;&#12418;&#12398;</pr></trd>,
    <trd>&#21830;&#21697;  <pr>&#12375;&#12423;&#12358;&#12402;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">artikel</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>artikel</trd>,
    <trd>druh tovaru</trd>
  </trdgrp>
  <trd lng="sv">artikel (handel)</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t;&nbsp;(&c_t;&c_o;&c_r;&c_g;&c_ib;&c_v;&c_l;&c_ib;)</trd>,
    <trd>&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_ib;&c_ja; &c_t;&c_o;&c_v;&c_a;&c_r;&c_u;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;</trd>
  </trdgrp>
</drv>
