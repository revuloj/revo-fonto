<art mrk="$Id: sistr.xml,v 1.16 2023/09/03 18:39:30 revo Exp $">
<kap>
   <rad>sistr</rad>/o
   <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="sistr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MUZ</uzo>
   <snc>
     <dif>
       Antikva egipta <ref tip="lst" lst="voko:frapinstrumentoj" cel="muzik.0ilo">muzikilo</ref>
       konsistanta el metalaj vergetoj sonantaj per skui&gcirc;o:
      <ekz>
        la tuta domo de <nom>Izrael</nom> ludis anta&ubreve; la Eternulo per &ccirc;iaj instrumentoj el cipreso,
        per harpoj, psalteroj, tamburinoj, <tld/>oj, kaj cimbaloj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o en la antikva Egiptujo estas taksata kiel sankta instrumento, kiun oni
        uzis ofte dum templaj diservoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=483084">Sistro</url></lok>
        </fnt>.
      </ekz>
     </dif>
    <bld lok="&WCU;/thumb/5/51/Abu_Simbel_Nefartari_Sistrum-2.jpg/244px-Abu_Simbel_Nefartari_Sistrum-2.jpg" prm="PD">Nefartari, edzino de Ramses II kun <tld/>o
    
        <fnt>
          <aut>Abu_Simbel_Nefartari_Sistrum.jpg: Laban66derivative work: Oltau (talk), Abu_Simbel_Nefartari_Sistrum.jpg</aut>
          <lok><url ref="&WCW;/File:Abu_Simbel_Nefartari_Sistrum-2.jpg">&WCW;/File:Abu_Simbel_Nefartari_Sistrum-2.jpg</url></lok>
        </fnt>
    </bld>
   </snc>
  <trd lng="ca">sistre</trd>
  <trdgrp lng="cs">
    <trd>sistr</trd>,
    <trd>sistrum</trd>
  </trdgrp>
  <trd lng="de">Sistrum</trd>
  <trd lng="es">sistro</trd>
  <trd lng="fr">sistre</trd>
  <trd lng="hu">szisztrum</trd>
  <trd lng="ja">&#x30B7;&#x30B9;&#x30C8;&#x30EB;&#x30E0;</trd>
  <trd lng="nl">sistrum</trd>
  <trd lng="pl">sistrum</trd>
  <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">sistrum</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x438;&#x441;&#x442;&#x440;</trd>,
    <trd>&#x442;&#x440;&#x456;&#x441;&#x43A;&#x430;&#x447;&#x43A;&#x430; (&#x43C;&#x443;&#x437;. &#x456;&#x43D;&#x441;&#x442;&#x440;&#x443;&#x43C;&#x435;&#x43D;&#x442;)</trd>
  </trdgrp>
</drv>
</art>
