<kap>
  <ofc>1</ofc>
  <rad>pustul</rad>/o
</kap>
<drv mrk="pustul.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Pusa &scirc;vela&jcirc;eto sur la ha&ubreve;to:
      <ekz>
        se &ccirc;e iu homo sur la ha&ubreve;to de lia korpo aperos &scirc;vela&jcirc;o a&ubreve; <tld/>o a&ubreve; makulo
        tiamaniere, ke sur la ha&ubreve;to de lia korpo fari&gcirc;os kvaza&ubreve; infekta&jcirc;o de lepro,
        tiam oni alkonduku lin al la pastro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="akn.0o.sxvelajxo">akno<sncref/></ref>
  </snc>
  <trd lng="bo">&#xF50;&#xF7C;&#xF62;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>sporangie</trd>,
    <trd>v&yacute;trusnice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pustel</trd>,
    <trd>Eiterbl&auml;schen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pustule</trd>,
    <trd>pimple</trd>
  </trdgrp>
  <trd lng="fr">pustule</trd>
  <trd lng="hu">gennyh&oacute;lyag</trd>
  <trd lng="la">pustula</trd>
  <trd lng="nl">etterblaasje</trd>
  <trd lng="pl">krosta</trd>
  <trdgrp lng="pt">
   <trd>p&uacute;stula</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_g;&c_n;&c_o;&c_j;&c_n;&c_i;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">v&yacute;trusnica</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x443;&#x441;&#x442;&#x443;&#x43B;&#x430;</trd>,
    <trd>&#x433;&#x43D;&#x43E;&#x44F;&#x447;&#x43E;&#x43A;</trd>,
    <trd>&#x433;&#x43D;&#x456;&#x439;&#x43D;&#x438;&#x439; &#x43F;&#x440;&#x438;&#x449;</trd>
  </trdgrp>
</drv>
</art>
