<drv mrk="pluv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pluv.0i.MET">
    <dif>
      Fali <klr>(parolante pri <tld/>o)</klr>:
      <ekz>
        <ind><tld/>i kvaza&ubreve; la akvo estus ver&scirc;ata per
        trogoj, per siteloj</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z;
        &c_v;&c_je;&c_d;&c_r;&c_a;</trd>
      </ekz>
      <ekz>
        pri<tld/>ata lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iaj benoj <tld/>as sur ilin<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la insultoj, la monpunoj <tld/>as;
      </ekz>
      <ekz>
        mi <tld/>igos sur la teron dum 40 tagoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi batojn sur iun.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_a;&c_l;&c_i; &c_d;&c_malmol;&c_zh;&c_d;</trd>
  <trd lng="bo">&t_cha;&t_ra;&t_tsheg;&t_pa;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trd lng="ca">ploure</trd>
  <trd lng="cs">pr&scaron;et</trd>
  <trd lng="de">regnen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">llover</trd>
  <trd lng="fr">pleuvoir</trd>
  <trd lng="hu">esik<klr tip="ind"> (az es&odblac;)</klr></trd>
  <trd lng="nl">regenen</trd>
  <trd lng="pl">pada&cacute;<klr> (o deszczu)</klr></trd>
  <trd lng="pt">chover</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_b;&c_je;&c_z;&c_l;. &c_o;
    &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>,
    <trd>&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb">
    (&c_b;&c_je;&c_z;&c_l;. &c_o;
    &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pr&scaron;a&tcaron;</trd>
  <trd lng="sv">regna</trd>
  <trd lng="th">&#3613;&#3609;&#3605;&#3585;</trd>
  <trd lng="vo">rein&ouml;n</trd>
</drv>

<drv mrk="pluv.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="pluv.0egi.MET">
    <dif>
      Forte, dense <tld/>i:
      <ekz>
        <tld/>egis, sed la poeto sidis varme kaj komforte &ccirc;e
        sia kahela
        forno
        <fnt><bib>Fab1</bib>, <lok>malbonkonduta knabo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_m;&c_o;&c_c;&c_n;&c_a;, &c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_m;&c_o;&c_c;&c_n;&c_a;, &c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&iacute;t <klr>(d&eacute;&scaron;&tcaron;)</klr></trd>,
    <trd>siln&ecaron; pr&scaron;et</trd>
  </trdgrp>
  <trd lng="de">stark <ind>regnen</ind></trd>
  <trd lng="en">downpour</trd>
  <trd lng="es">diluviar</trd>
  <trd lng="fr">pleuvoir &agrave; verse</trd>
  <trdgrp lng="nl">
    <trd>stortregenen</trd>,
    <trd>pijpenstelen regenen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>la&cacute;<klr> (o deszczu, ulewie)</klr></trd>,
    <trd>la&cacute;<klr> jak z cebra</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cair um <ind>tor&oacute;</ind></trd>,
    <trd>chover a c&acirc;ntaros</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z;
  &c_v;&c_je;&c_d;&c_r;&c_a;<klr tip="ind">
  (&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  <trd lng="sk">silno pr&scaron;a&tcaron;</trd>
</drv>

<drv mrk="pluv.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="pluv.0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, densa <tld/>o:
      <ekz>
        rifu&gcirc;ejo kontra&ubreve; <tld/>ego
        <fnt><bib>MT</bib>, <lok>&Jes; 25:4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_l;&c_ib;&c_w;&c_n;&c_y; &c_d;&c_o;&c_zh;&c_d;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lijavec</trd>,
    <trd>lij&aacute;k</trd>,
    <trd>p&rcaron;&iacute;val de&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Wolkenbruch</trd>
  <trd lng="en">downpour</trd>
  <trdgrp lng="es">
    <trd>aguacero</trd>,
    <trd>chubasco</trd>,
    <trd>chaparr&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">pluie battante</trd>
  <trd lng="ja">&#22823;&#38632;
  [<ind>&#12362;&#12362;&#12354;&#12417;</ind>]</trd>
  <trd lng="nl">stortregen</trd>
  <trdgrp lng="pl">
    <trd>ulewa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chuvarada</trd>,
    <trd>aguaceiro</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>lejak</trd>,
    <trd>pr&iacute;val da&zcaron;&dcaron;a</trd>
  </trdgrp>
</drv>

<drv mrk="pluv.0ero">
  <kap>
    <tld/>ero
  </kap>
  <snc>
    <dif>
      Unuopa <ref tip="super" cel="gut.0o.fluero">guto</ref> de <tld/>o:
      <ekz>
        kiam la unuaj <tld/>eroj falas sur sekan teron, en la aero oni rekonas
        nekonfuzeblan odoron: tiun de la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2015/011480.php"
              >Pluveraj veroj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        pensojn malakrigas varmo sen <tld/>ero
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Regentropfen</trd>
  <trd lng="en">rain drop</trd>
</drv>






<drv mrk="pluv.0eti">
