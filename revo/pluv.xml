        <tld/>etis dum tagoj kaj semajnoj
        <fnt><bib>InfanTorent2</bib>,
        <lok>dua parto, &ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_d;&c_a;&c_zh;&c_d;&c_zh;&c_y;)</klr></trd>,
    <trd>&c_n;&c_a;&c_k;&c_r;&c_a;&c_p;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_d;&c_a;&c_zh;&c_d;&c_zh;&c_y;)</klr></trd>,
    <trd>&c_c;&c_je;&c_r;&c_u;&c_sh;&c_y;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_d;&c_a;&c_zh;&c_d;&c_zh;&c_y;)</klr></trd>,
    <trd>&c_c;&c_je;&c_r;&c_u;&c_sh;&c_y;&c_c;&c_c;&c_a; <klr>(&c_a;&c_b; &c_d;&c_a;&c_zh;&c_d;&c_zh;&c_y;)</klr></trd>,
