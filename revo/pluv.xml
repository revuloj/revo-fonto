<drv mrk="pluv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pluv.0i.MET">
    <dif>
      Fali <klr>(parolante pri <tld/>o)</klr>:
      <ekz>
        <ind><tld/>i kvaza&ubreve; la akvo estus ver&scirc;ata per
        trogoj, per siteloj</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z;
        &c_v;&c_je;&c_d;&c_r;&c_a;</trd>
      </ekz>
      <ekz>
        pri<tld/>ata lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iaj benoj <tld/>as sur ilin<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la insultoj, la monpunoj <tld/>as;
      </ekz>
      <ekz>
        mi <tld/>igos sur la teron dum 40 tagoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi batojn sur iun.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_a;&c_l;&c_i; &c_d;&c_malmol;&c_zh;&c_d;</trd>
  <trd lng="bo">&#xF46;&#xF62;&#xF0B;&#xF54;&#xF0B;&#xF42;&#xF4F;&#xF7C;&#xF44;&#xF0B;</trd>
  <trd lng="ca">ploure</trd>
  <trd lng="cs">pr&scaron;et</trd>
  <trd lng="de">regnen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">llover</trd>
  <trd lng="fr">pleuvoir</trd>
  <trd lng="hu">esik<klr tip="ind"> (az es&odblac;)</klr></trd>
  <trd lng="nl">regenen</trd>
  <trd lng="pl">pada&cacute;<klr> (o deszczu)</klr></trd>
  <trd lng="pt">chover</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_b;&c_je;&c_z;&c_l;. &c_o;
    &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>,
    <trd>&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb">
    (&c_b;&c_je;&c_z;&c_l;. &c_o;
    &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pr&scaron;a&tcaron;</trd>
  <trd lng="sv">regna</trd>
  <trd lng="th">&#xE1D;&#xE19;&#xE15;&#xE01;</trd>
  <trd lng="vo">rein&ouml;n</trd>
</drv>

<drv mrk="pluv.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="pluv.0egi.MET">
    <dif>
      Forte, dense <tld/>i:
      <ekz>
        <tld/>egis, sed la poeto sidis varme kaj komforte &ccirc;e
        sia kahela
        forno
        <fnt><bib>Fab1</bib>, <lok>malbonkonduta knabo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>l&iacute;t <klr>(d&eacute;&scaron;&tcaron;)</klr></trd>,
    <trd>siln&ecaron; pr&scaron;et</trd>
  </trdgrp>
  <trd lng="de">stark <ind>regnen</ind></trd>
  <trd lng="en">downpour</trd>
  <trd lng="es">diluviar</trd>
  <trd lng="fr">pleuvoir &agrave; verse</trd>
  <trdgrp lng="nl">
    <trd>stortregenen</trd>,
    <trd>pijpenstelen regenen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>la&cacute;<klr> (o deszczu, ulewie)</klr></trd>,
    <trd>la&cacute;<klr> jak z cebra</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cair um <ind>tor&oacute;</ind></trd>,
    <trd>chover a c&acirc;ntaros</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z;
  &c_v;&c_je;&c_d;&c_r;&c_a;<klr tip="ind">
  (&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  <trd lng="sk">silno pr&scaron;a&tcaron;</trd>
</drv>

<drv mrk="pluv.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="pluv.0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, densa <tld/>o:
      <ekz>
        rifu&gcirc;ejo kontra&ubreve; <tld/>ego
        <fnt><bib>MT</bib>, <lok>&Jes; 25:4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lijavec</trd>,
    <trd>lij&aacute;k</trd>,
    <trd>p&rcaron;&iacute;val de&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Wolkenbruch</trd>
  <trd lng="en">downpour</trd>
  <trdgrp lng="es">
    <trd>aguacero</trd>,
    <trd>chubasco</trd>,
    <trd>chaparr&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">pluie battante</trd>
  <trd lng="ja">&#x5927;&#x96E8;
  [<ind>&#x304A;&#x304A;&#x3042;&#x3081;</ind>]</trd>
  <trd lng="nl">stortregen</trd>
  <trdgrp lng="pl">
    <trd>ulewa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chuvarada</trd>,
    <trd>aguaceiro</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>lejak</trd>,
    <trd>pr&iacute;val da&zcaron;&dcaron;a</trd>
  </trdgrp>
</drv>

<drv mrk="pluv.0eti">
  <kap><tld/>eti</kap>
  <snc mrk="pluv.0eti.MET">
    <dif>
      Malforte, maldense <tld/>i:
      <ekz>
        <tld/>etis dum tagoj kaj semajnoj
        <fnt><bib>InfanTorent2</bib>,
        <lok>dua parto, &ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kr&aacute;pat</trd>,
    <trd>mrholit</trd>,
    <trd>poprch&aacute;vat</trd>
  </trdgrp>
  <trd lng="de">nieseln</trd>
  <trd lng="en">drizzle</trd>
  <trdgrp lng="es">
    <trd>lloviznar</trd>,
    <trd>chispear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bruiner</trd>,
    <trd>pleuviner</trd>,
    <trd>pleuvioter</trd>
  </trdgrp>
  <trd lng="hu">szit&aacute;l<klr tip="ind"> (az es&odblac;)</klr></trd>
  <trd lng="nl">motregenen</trd>
  <trdgrp lng="pl">
    <trd>si&aogonek;pi&cacute;</trd>,
    <trd>kapa&cacute;<klr> (o deszczu)</klr></trd>,
    <trd>m&zdot;y&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chuviscar</trd>,
    <trd>garoar</trd>
  </trdgrp>
  <trd lng="ru">
  &c_n;&c_a;&c_k;&c_r;&c_a;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;
  <klr tip="ind">
  (&c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  <trd lng="sk">pop&racute;cha</trd>
</drv>

<drv mrk="pluv.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="pluv.0eto.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Malforta, maldensa <tld/>o:
      <ekz>
        kiam vi vidas nubon levi&gcirc;antan en la okcidento, vi tuj
        diras: Venos <tld/>eto; kaj tiel fari&gcirc;as
        <fnt><bib>NT</bib>, <lok>&Luk; 12:54</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">d&eacute;&scaron;&tcaron; drobn&yacute;</trd>
  <trdgrp lng="de">
    <trd>Spr&uuml;hregen</trd>,
    <trd>Nieselregen</trd>
  </trdgrp>
  <trd lng="en">drizzle</trd>
  <trd lng="es">llovizna</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_r;&a_A;&a_n;&zwnj;&a_r;&f_y;&a_z;&a_h;</trd>,
    <trd>&a_n;&a_m;&zwnj;&a_n;&a_m; &a_b;&a_A;&a_r;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bruine</trd>,
    <trd>crachin</trd>
  </trdgrp>
  <trd lng="id">gerimis</trd>
  <trd lng="ja">&#x5C0F;&#x96E8; [<ind>&#x3053;&#x3055;&#x3081;</ind>]
  </trd>
  <trd lng="nl">motregen</trd>
  <trdgrp lng="pl">
    <trd>deszczyk</trd>,
    <trd>m&zdot;awka</trd>,
    <trd>kapu&sacute;niak<klr> (deszcz)</klr></trd>
    <trd>si&aogonek;pawica</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chuvisco</trd>,
    <trd>garoa</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_i;&c_ch;&c_je;&c_k;</trd>
  <trd lng="sk">drobn&yacute; d&aacute;&zcaron;&dcaron;</trd>
</drv>

<drv mrk="pluv.acida0o">
  <kap>acida <tld/>o</kap>
  <snc mrk="pluv.acida0o.MET">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      Pluvo a&ubreve; alia atmosfera precipita&jcirc;o pli acida
      ol normale pro enhavo de sulfuroksido,
      azotoksido,
      kiu havas detruan efikon al kalka&jcirc;oj kaj vegeta&jcirc;oj: 
      <ekz>
        la sulfuraj kombina&jcirc;oj <klr>...</klr> 
        estigis la acidajn <tld/>ojn
        <fnt>
          <aut>C. Bertin</aut>,
          <vrk><url ref="www.bertin.biz/index.php?titre=allegre-sciencaDubo.htm">
          La rajto pri scienca dubo</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kysel&yacute; d&eacute;&scaron;&tcaron;</trd>
  <trd lng="fr">pluie acide</trd>
  <trd lng="id"><ind>hujan</ind> asam</trd>
  <trd lng="sk">kysl&yacute; d&aacute;&zcaron;&dcaron;</trd>
  <trd lng="th">&#xE1D;&#xE19;&#xE01;&#xE23;&#xE14;</trd>
</drv>

<drv mrk="pluv.sen0eco">
  <kap>sen<tld/>eco</kap>
  <snc mrk="pluv.sen0eco.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Da&ubreve;ra manko de <tld/>o en iu loko:
      <ekz>
        la sen<tld/>eco plimalbonigis la situacion kaj
        man&gcirc;a&jcirc;oprezoj altegi&gcirc;is 330 procentojn
        <fnt>
          <aut>-</aut>,
          <vrk>Manko de nutra&jcirc;o minacas Etiopion</vrk>,
          <lok><url ref="http://www.eventeo.net/web/">eventeo</url>, 
          2008-09-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>D&uuml;rre</trd>
    <trd>Trockenheit</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&a_sh;&f_k;&a_s;&a_A;&a_l;&f_y;</trd>
  <trd lng="fr">s&eacute;cheresse<klr> (absence de pluie)</klr></trd>
  <trd lng="nl">langdurige <ind>droogte</ind></trd>
  <trd lng="pl">susza</trd>
  <trd lng="th">&#xE1D;&#xE19;&#xE41;&#xE25;&#xE49;&#xE07;</trd>
</drv>

<drv mrk="pluv.0oversxo">
  <kap><tld/>over&scirc;o<fnt><bib>KrDE</bib></fnt></kap>
  <snc mrk="pluv.0oversxo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, densa <tld/>o.
    </dif>
    <ekz>
      Dum <tld/>over&scirc;o tri viroj ... renkonti&gcirc;as &ccirc;e la
      ruinoj de
      Ra&scirc;omon...
      <fnt>
        <bib>Viki</bib>
        <lok><url ref="http://eo.wikipedia.org/w/index.php?title=Rash%C5%8Dmon_(filmo)&amp;oldid=3885320#Sinoptiko">Rash&omacron;mon
        (filmo), Sinoptiko</url></lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="de">Schauer</trd>
  <trd lng="en">rain shower</trd>
  <trd lng="fr">forte <ind>averse</ind> de pluie</trd>
  <trd lng="nl">regenbui</trd>
</drv>
</art>
