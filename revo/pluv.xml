<drv mrk="pluv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pluv.0i.MET">
    <dif>
      Fali kiel <tld/>o:
      <ekz>
        ek<tld/>is sur ilin el la &ccirc;ielo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 21:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as kaj <tld/>as kaj <tld/>as kaj <tld/>as sen&ccirc;ese, senfine, senhalte, el
        &ccirc;iel' al la ter' <klr>[&#x2026;]</klr> are gutoj frapi&gcirc;as resalte
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Pluvo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        lando ne purigata, ne pri<tld/>ata
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 22:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is regula monotona <tld/>o jam plurajn horojn
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi alvenis en Glasgovo, ne&gcirc;<tld/>is, kaj estis ka&ccirc;e kaj malagrable
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pluv.0i.FIG">
    <dif>
      Faladi are kvaza&ubreve; <tld/>o:
      <ekz>
        &ccirc;io mallumi&gcirc;is, de la sudo ek<tld/>is varmega sablo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> soldato <klr>[&#x2026;]</klr> estis &ccirc;iam unua, kie <tld/>is la kugloj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ripro&ccirc;oj, batoj, pu&scirc;egoj <tld/>is hajle
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_p;&c_r;&c_a;
    &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_a;&c_l;&c_i; &c_d;&c_malmol;&c_zh;&c_d;</trd>
  <trd lng="bo">&t_cha;&t_ra;&t_tsheg;&t_pa;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trd lng="ca">ploure</trd>
  <trd lng="cs">pr&scaron;et</trd>
  <trd lng="de">regnen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">llover</trd>
  <trd lng="fr">pleuvoir</trd>
  <trd lng="hu">esik<klr tip="ind"> (az es&odblac;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x96E8;&#x304C;&#x964D;&#x308B;  [<ind>&#x3042;&#x3081;&#x304C;&#x3075;&#x308B;</ind>]</trd>,
    <trd>&#x964D;&#x308A;&#x6CE8;&#x3050;  [<ind>&#x3075;&#x308A;&#x305D;&#x305D;&#x3050;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">regenen</trd>
  <trd lng="pl">pada&cacute;<klr> (o deszczu)</klr></trd>
  <trd lng="pt">chover</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_b;&c_je;&c_z;&c_l;. &c_o;
    &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>,
    <trd>&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb">
    (&c_b;&c_je;&c_z;&c_l;. &c_o;
    &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pr&scaron;a&tcaron;</trd>
  <trd lng="sv">regna</trd>
  <trd lng="th">&#xE1D;&#xE19;&#xE15;&#xE01;</trd>
  <trdgrp lng="uk">
    <trd>&#x456;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x430;&#x434;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x434;&#x43E;&#x449;)</trd>
  </trdgrp>
  <trd lng="vo">rein&ouml;n</trd>
</drv>

<drv mrk="pluv.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="pluv.0egi.MET">
    <dif>
      Forte, dense <tld/>i:
      <ekz>
        unu vesperon fari&gcirc;is granda uragano: fulmis kaj tondris, forte <tld/>egis
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egis, sed la poeto sidis varme kaj komforte &ccirc;e
        sia kahela forno
        <fnt><bib>Fab1</bib>, <lok>malbonkonduta knabo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_m;&c_o;&c_c;&c_n;&c_a;, &c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_m;&c_o;&c_c;&c_n;&c_a;, &c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&iacute;t <klr>(d&eacute;&scaron;&tcaron;)</klr></trd>,
    <trd>siln&ecaron; pr&scaron;et</trd>
  </trdgrp>
  <trd lng="de">stark <ind>regnen</ind></trd>
  <trd lng="en">downpour</trd>
  <trd lng="es">diluviar</trd>
  <trd lng="fr">pleuvoir &agrave; verse</trd>
  <trdgrp lng="nl">
    <trd>stortregenen</trd>,
    <trd>pijpenstelen regenen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>la&cacute;<klr> (o deszczu, ulewie)</klr></trd>,
    <trd>la&cacute;<klr> jak z cebra</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cair um <ind>tor&oacute;</ind></trd>,
    <trd>chover a c&acirc;ntaros</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z;
  &c_v;&c_je;&c_d;&c_r;&c_a;<klr tip="ind">
  (&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  <trd lng="sk">silno pr&scaron;a&tcaron;</trd>
</drv>

<drv mrk="pluv.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="pluv.0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, densa <tld/>o:
      <ekz>
        rifu&gcirc;ejo kontra&ubreve; <tld/>ego
        <fnt><bib>MT</bib>, <lok>&Jes; 25:4</lok></fnt>;
      </ekz>
      <ekz>
        la hajlo pasis <klr>[&#x2026;]</klr> tuj poste komencis mu&gcirc;i <tld/>ego
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ego tamburegis sur la tero
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu kis<tld/>ego estis iom kvietigita
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la varmaj vortoj <klr>[&#x2026;]</klr> estis granda, liberiga donaco kaj malfermis en
        mi la <tld/>egon de la vortoj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_l;&c_ib;&c_w;&c_n;&c_y; &c_d;&c_o;&c_zh;&c_d;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lijavec</trd>,
    <trd>lij&aacute;k</trd>,
    <trd>p&rcaron;&iacute;val de&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Wolkenbruch</trd>
  <trd lng="en">downpour</trd>
  <trdgrp lng="es">
    <trd>aguacero</trd>,
    <trd>chubasco</trd>,
    <trd>chaparr&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">pluie battante</trd>
  <trd lng="ja">&#x5927;&#x96E8;
  [<ind>&#x304A;&#x304A;&#x3042;&#x3081;</ind>]</trd>
  <trd lng="nl">stortregen</trd>
  <trdgrp lng="pl">
    <trd>ulewa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chuvarada</trd>,
    <trd>aguaceiro</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>lejak</trd>,
    <trd>pr&iacute;val da&zcaron;&dcaron;a</trd>
  </trdgrp>
  <trd lng="uk">&#x437;&#x43B;&#x438;&#x432;&#x430;</trd>
</drv>

<drv mrk="pluv.0ero">
  <kap>
    <tld/>ero
  </kap>
  <snc>
    <dif>
      Unuopa <ref tip="super" cel="gut.0o.fluero">guto</ref> de <tld/>o:
      <ekz>
        kiam la unuaj <tld/>eroj falas sur sekan teron, en la aero oni rekonas
        nekonfuzeblan odoron: tiun de la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011480.php">Pluveraj veroj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        pensojn malakrigas varmo sen <tld/>ero
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Regentropfen</trd>
  <trd lng="en">rain drop</trd>
  <trdgrp lng="ja">
    <trd>&#x96E8;&#x6EF4;  [<ind>&#x3046;&#x3066;&#x304D;</ind>]</trd>,
    <trd>&#x96E8;&#x7C92;  [<ind>&#x3042;&#x307E;&#x3064;&#x3076;</ind>]</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x440;&#x430;&#x43F;&#x43B;&#x44F; &#x434;&#x43E;&#x449;&#x443;</trd>
</drv>






<drv mrk="pluv.0eti">
  <kap><tld/>eti</kap>
  <snc mrk="pluv.0eti.MET">
    <dif>
      Malforte, maldense <tld/>i:
      <ekz>
        <tld/>etis dum tagoj kaj semajnoj
        <fnt><bib>InfanTorent2</bib>,
