<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pluv</rad>/o
</kap>
<drv mrk="pluv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pluv.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Akvo gute falanta de nuboj:
      <ekz>
        <tld/>o faladis
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        batanta <klr>(fortega)</klr> <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        griza, <tld/>a, kota <klr>...</klr> tago
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        trafi, fali el sub <tld/>o en riveron<fnt>Z</fnt>
        <klr>(en ankora&ubreve; pli malbonan situacion,
        <ref tip="vid" cel="flam.0o">flamo</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>&scirc;irmilo;
      </ekz>
      <ekz>
        <tld/>defluilo, <tld/>tubo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam komenci&gcirc;is <tld/>o <klr>(multo, amaso.
        <refgrp tip="sin">
          <ref cel="hajl.0o">hajlo</ref>,
          <ref cel="torent.0o">torento</ref>,
          <ref cel="river.0o">rivero</ref>,
          <ref cel="fask.0o">fasko</ref>,
          <ref cel="garb.0o">garbo</ref>
        </refgrp>)</klr> da sukcesoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o da kisoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_zh;&c_d;&c_zh;</trd>
  <trd lng="ca">pluja</trd>
  <trd lng="de">Regen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">lluvia</trd>
  <trd lng="fr">pluie</trd>
  <trd lng="hu">es&odblac;</trd>
  <trd lng="nl">regen</trd>
  <trd lng="pt">chuva</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_mol;</trd>
  <trd lng="sv">regn</trd>
</drv>

<drv mrk="pluv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pluv.0i.MET">
    <dif>
      Fali <klr>(parolante pri <tld/>o)</klr>:
      <ekz>
        <ind><tld/>i kvaza&ubreve; la akvo estus ver&scirc;ata per trogoj, per
        siteloj</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z; &c_v;&c_je;&c_d;&c_r;&c_a;</trd>
      </ekz>
      <ekz>
        pri<tld/>ata lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iaj benoj <tld/>as sur ilin<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la insultoj, la monpunoj <tld/>as;
      </ekz>
      <ekz>
        mi <tld/>igos sur la teron dum 40 tagoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi batojn sur iun.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; <klr>(&c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trd lng="ca">ploure</trd>
  <trd lng="cs">pr&scaron;et</trd>
  <trd lng="de">regnen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">llover</trd>
  <trd lng="fr">pleuvoir</trd>
  <trd lng="hu">esik<klr tip="ind"> (az es&odblac;)</klr></trd>
  <trd lng="nl">regenen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind"> (&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>,
    <trd>&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sv">regna</trd>
</drv>

<drv mrk="pluv.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="pluv.0egi.MET">
    <dif>
      Forte, dense <tld/>i:
      <ekz>
        <tld/>egis, sed la poeto sidis varme kaj komforte &ccirc;e sia kahela
        forno
        <fnt><bib>Fab1</bib>, <lok>malbonkonduta knabo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pleuvoir &agrave; verse</trd>
</drv>

<drv mrk="pluv.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="pluv.0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, densa <tld/>o:
      <ekz>
        rifu&gcirc;ejo kontra&ubreve; <tld/>ego
        <fnt><bib>MT</bib>, <lok>&Jes; 25:4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">orage</trd>
</drv>

<drv mrk="pluv.0eti">
  <kap><tld/>eti</kap>
  <snc mrk="pluv.0eti.MET">
    <dif>
      Malforte, maldense <tld/>i:
      <ekz>
        <tld/>etis dum tagoj kaj semajnoj
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bruiner</trd>,
    <trd>pleuviner</trd>,
    <trd>pleuvoter</trd>
  </trdgrp>
  <trd lng="hu">szit&aacute;l<klr tip="ind"> (az es&odblac;)</klr></trd>
  <trd lng="nl">motregenen</trd>
  <trd lng="ru">&c_n;&c_a;&c_k;&c_r;&c_a;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pluv.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="pluv.0eto.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Malforta, maldensa <tld/>o:
      <ekz>
        kiam vi vidas nubon levi&gcirc;antan en la okcidento, vi tuj diras:
        Venos <tld/>eto; kaj tiel fari&gcirc;as
        <fnt><bib>NT</bib>, <lok>&Luk; 12:54</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>averse</trd>,
    <trd>bruine</trd>
  </trdgrp>
</drv>
</art>
