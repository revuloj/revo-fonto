<drv mrk="pluv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pluv.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="akv.0o">Akvo</ref> gute falanta de nuboj:
      <ekz>
        <tld/>o faladis
        <fnt><bib>FK</bib>, <lok>4, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        estis <tld/>o sur la tero dum kvardek tagoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 7:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel la <tld/>o torentis
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>!
      </ekz>
      <ekz>
        la floroj kaj la greno staris en la trankvila, pura aero refre&scirc;igitaj de la
        <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        forta <tld/>o
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o batanta
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 28:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        griza, <tld/>a, kota <klr>[&#x2026;]</klr> tago
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la raketoj <klr>[&#x2026;]</klr> disvolvi&gcirc;is kvaza&ubreve; floroj kaj falis teren, kiel
        <tld/>o de diverskoloraj steloj
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur niajn kapojn &scirc;uti&gcirc;as <tld/>o de agitfolioj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en fumo de pulvo kaj en <tld/>o de kugloj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>li trafis el sub <tld/>o en riveron</ind>
        <klr>(en ankora&ubreve; pli malbonan situacion)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">er kam vom <ind>Regen</ind> in die Traufe</trd>
      </ekz>
      <ekz>
        granda nubo, eta <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
