<drv mrk="pluv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pluv.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Akvo gute falanta de nuboj:
      <ekz>
        <tld/>o faladis
        <fnt><bib>FK</bib>, <lok>4, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        batanta <klr>(fortega)</klr> <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        griza, <tld/>a, kota <klr>...</klr> tago
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        trafi, fali el sub <tld/>o en riveron<fnt>Z</fnt>
        <klr>(en ankora&ubreve; pli malbonan situacion,
        <ref tip="vid" cel="flam.0o">flamo</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>&scirc;irmilo;
      </ekz>
      <ekz>
        <tld/>defluilo, <tld/>tubo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam komenci&gcirc;is <tld/>o <klr>(multo, amaso.
        <refgrp tip="sin">
          <ref cel="hajl.0o">hajlo</ref>,
          <ref cel="torent.0o">torento</ref>,
          <ref cel="river.0o">rivero</ref>,
          <ref cel="fask.0o">fasko</ref>,
          <ref cel="garb.0o">garbo</ref>
        </refgrp>)</klr> da sukcesoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o da kisoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">re&#xEB;n</trd>
  <trd lng="am">&#x12DD;&#x1293;&#x1265;</trd>
  <trd lng="ar">&#x645;&#x637;&#x631;</trd>
