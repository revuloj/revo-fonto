<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pluv</rad>/o
</kap>
<drv mrk="pluv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Akvo gute teren defalanta de la densi&gcirc;intaj nubvaporoj:
      <ekz>
        forta, batanta (fortega) <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trafi, fali el sub <tld/>o en riveron<fnt>Z</fnt>
        <klr>(en ankora&ubreve; pli malbonan situacion. <ref tip="vid"
        cel="flam.0o">flamo</ref>)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam komencis <tld/>o <klr>(multo, amaso.
        <refgrp tip="sin">
          <ref cel="hajl.0o">hajlo</ref>,
          <ref cel="torent.0o">torento</ref>,
          <ref cel="river.0o">rivero</ref>,
          <ref cel="fask.0o">fasko</ref>,
          <ref cel="garb.0o">garbo</ref>
        </refgrp>)</klr> da sukcesoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o da kisoj;
      </ekz>
      <ekz>
        kota <tld/>a tago<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>mantelo;
      </ekz>
      <ekz>
        <tld/>&scirc;irmilo;
      </ekz>
      <ekz>
        <tld/>ombrelo;
      </ekz>
      <ekz>
        <tld/>defluilo, <tld/>tubo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_zh;&c_d;&c_zh;</trd>
  <trd lng="ca">pluja</trd>
  <trd lng="de">Regen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">lluvia</trd>
  <trd lng="fr">pluie</trd>
  <trd lng="hu">es&odblac;</trd>  
  <trd lng="nl">regen</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_mol;</trd>
  <trd lng="sv">regn</trd>
</drv>

<drv mrk="pluv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Fali (pp <tld/>o):
      <ekz>
        <ind><tld/>i kvaza&ubreve; la akvo estus ver&scirc;ata per trogoj, per
        siteloj</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_i;&c_z; &c_v;&c_je;&c_d;&c_r;&c_a;</trd>
      </ekz>
      <ekz>
        pri<tld/>ata lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iaj benoj <tld/>as sur ilin<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la insultoj, la monpunoj <tld/>as;
      </ekz>
      <ekz>
        mi <tld/>igos sur la teron dum 40 tagoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi batojn sur iun;
      </ekz>
      <ekz>
        <ind><tld/>eti</ind>.
        <trd lng="hu">szit&aacute;l<klr tip="ind"> (az es&odblac;)</klr></trd>        
        <trd lng="nl">motregenen</trd>
        <trd lng="ru">&c_n;&c_a;&c_k;&c_r;&c_a;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; <klr>(&c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_p;&c_r;&c_a; &c_d;&c_o;&c_zh;&c_d;&c_zh;)</klr></trd>
  </trdgrp>
  <trd lng="ca">ploure</trd>
  <trd lng="cs">pr&scaron;et</trd>
  <trd lng="de">regnen</trd>
  <trd lng="en">rain</trd>
  <trd lng="es">llover</trd>
  <trd lng="fr">pleuvoir</trd>
  <trd lng="hu">esik<klr tip="ind"> (az es&odblac;)</klr></trd>  
  <trd lng="nl">regenen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind"> (&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>,
    <trd>&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_b;&c_je;&c_z;&c_l;. &c_o; &c_d;&c_o;&c_zh;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sv">regna</trd>
</drv>
</art>
