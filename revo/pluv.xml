<drv mrk="pluv.0oversxo">
  <kap><tld/>over&scirc;o
  <fnt><bib>KrDE</bib></fnt></kap>
  <snc mrk="pluv.0oversxo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, densa <tld/>o:
      <ekz>
        dum <tld/>over&scirc;o tri viroj <klr>[&#x2026;]</klr> renkonti&gcirc;as &ccirc;e la
        ruinoj de <nom>Ra&scirc;omon</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Vikio;Rash%C5%8Dmon_(filmo)&amp;oldid=3885320#Sinoptiko">Rash&omacron;mon
          (filmo), Sinoptiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la grupo da lesboj <klr>[&#x2026;]</klr> dum tiu tre fre&scirc;a novembrotago malgra&ubreve; kelkaj
        <tld/>over&scirc;oj kura&gcirc;is manifestacii nudotorse
        <fnt>
          <bib>MD02</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_ib;&c_w;&c_n;&c_y; &c_d;&c_o;&c_zh;&c_d;&c_zh;</trd>
