<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sold</rad>/o
</kap>
<drv mrk="sold.0o">
  <kap><tld/>o</kap>
  <snc mrk="sold.0o.monunuo">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Malnova <ref tip="super" cel="unu.monasub0o">mona subunuo</ref> de
      Francujo, egala al kvin <ref cel="centim.0o">centimoj</ref>; tiomvalora
      kupra a&ubreve; nikela monero.
    </dif>
    <adm>
      La du sencoj estas etimologie ligitaj, sed apenaux rilatigeblaj
      ene de Esperanto. Indas konsideri, cxu aparta artikolo necesas.
      <aut>MB</aut>
    </adm>
    <trd lng="be">&c_s;&c_o;&c_l;&c_mol;&c_d;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>garas</trd>,
      <trd>solidus</trd>,
      <trd>sou</trd>
    </trdgrp>
    <trd lng="de">Sou</trd>
    <trd lng="fr">sou<klr tip="ind"> (ancienne monnaie)</klr></trd>
    <trdgrp lng="nl">
      <trd>sou</trd>,
      <trd>stuiver</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_l;&c_mol;&c_d;&c_o;</trd>
  </snc>
  <snc mrk="sold.0o.militista_salajro">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Militista <ref cel="salajr.0o">salajro</ref>:
      <ekz>
        la <tld/>o de la regimentoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_je; &c_z;&c_a;&c_b;&c_je;&c_s;&c_mol;&c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_w;&c_c;&c_a;)</klr></trd>,
      <trd>&c_p;&c_l;&c_a;&c_t;&c_a; <klr>(&c_z;&c_a; &c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_u;&c_ju; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_u;)</klr></trd>
    </trdgrp>
    <trd lng="de">Sold</trd>
    <trd lng="fr">solde<klr tip="ind"> (salaire)</klr></trd>
    <trd lng="hu">zsold</trd>
    <trd lng="nl">soldij</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_o;&c_je; &c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je; <klr>(&c_v;&c_o;&c_je;&c_n;&c_n;&c_o;&c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_je;&c_g;&c_o;)</klr></trd>,
      <trd>&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_n;&c_mol;&c_je; <klr>(&c_v; &c_n;&c_a;&c_jo;&c_m;&c_n;&c_o;&c_j; &c_a;&c_r;&c_m;&c_i;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="sold.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MIL</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="soldat.dungo0o">dungosoldato</ref>
    <ekz>
      la svedaj militistoj &dash; kiel iam en Bohemio &dash;
      ne kapablis reteni siajn <tld/>ulojn kaj ne malebligis al ili rabadon
      <fnt>
      <aut>F. Ko&zcaron;&iacute;k trad Z. Rus&iacute;n kaj M. Malovec</aut>
      <vrk>Lumo en tenebroj</vrk>
      <lok><url ref="http://www.volny.cz/malovec/tenebr3.htm">http://www.volny.cz/malovec/tenebr3.htm</url></lok>
      </fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      dum la <tld/>uloj de la Monda Organizo pri Komerco (MOK) certigas
      la cirkuladon de la komercaj fluoj, tiuj de la Monda Banko kaj
      de la Internacia Mon-Fonduso (IMF) okupi&gcirc;as pri la financa fluo
      <fnt>
      <aut>trad V. Lutermano</aut>
      <vrk>Le Monde Diplomatique en Esperanto</vrk>
      <lok><url ref="http://eo.mondediplo.com/article752.html">2002-10</url></lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="de">S&ouml;ldner</trd>
  <trd lng="fr">mercenaire<klr tip="ind"> (subst.)</klr></trd>
</drv>
</art>
