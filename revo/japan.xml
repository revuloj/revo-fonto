<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>japan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="japan.0o">
  <kap><ofc>9</ofc><tld/>o<fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="http://www.akademio-de-esperanto.org/decidoj/9oa.html">
9a Oficiala Aldono</url></vrk>, <lok>japan/o</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="japan.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref>, &scirc;tatanino de
      <ref tip="malprt" cel="japan.0ujo"><tld lit="J"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="japan.0o.ETN">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="bg">&c_ja;&c_p;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Japaner</trd>
  <trd lng="en">Japanese</trd>
  <trdgrp lng="fr"> 
    <trd>Japonais</trd>
    <trd>Japonaise<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">jap&aacute;n</trd>
  <trd lng="it">Giapponese</trd>
  <trd lng="nl">Japanner</trd>
  <trd lng="pl">Japo&nacute;czyk</trd>
  <trd lng="pt">Japon&ecirc;s</trd>
  <trd lng="sv">japan<klr> (person)</klr></trd>
</drv>

<drv mrk="japan.0a">
  <kap><tld/>a</kap>
  <snc mrk="japan.0a.KOMUNE">
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>a lingvo; <tld/>aj a&ubreve;toroj; <tld/>aj kutimoj.
      </ekz>
    </dif>
   <trd lng="en">Japanese<klr> (adj.)</klr></trd>
   <trdgrp lng="fr"> 
    <trd>japonais</trd>
    <trd>japonaise<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc>
    <dif>
    "la japana" = la nacia <ref tip="super" cel="lingv.0o">lingvo</ref>
    de  Japanujo
    <ekz><fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Japana lingvo">Japana
    lingvo</url></lok></fnt></ekz>.
    </dif>
   <trd lng="en">Japanese<klr> (language)</klr></trd>
   <trd lng="fr">japonais<klr> (langue)</klr></trd>
  </snc>
  <trd lng="bg">&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">japonsky</trd>
  <trd lng="de">japanisch</trd>
  <trd lng="en">Japanese</trd>
  <trd lng="hu">jap&aacute;n</trd>
  <trd lng="it">giapponese</trd>
  <trd lng="nl">Japans</trd>
  <trd lng="pl">japo&nacute;ski</trd>
  <trd lng="pt">Japon&ecirc;s</trd>
  <trd lng="sk">japonsk&yacute;</trd>
  <trd lng="sv">japansk</trd>
</drv>

<drv mrk="japan.0ujo">
  <kap><ofc>9</ofc><tld lit="J"/>ujo
  (<var><kap><tld lit="J"/>/i/o</kap></var>)
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Japanio">Japanio</url></lok></fnt>
  </kap>
  <snc mrk="japan.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insularo en <ref tip="malprt" cel="azi.OrientaA0o">Orienta
      Azio</ref> kun kvar &ccirc;efaj insuloj, konsistiganta
      &scirc;taton, kies &ccirc;efurbo estas Tokio:
      <ekz>
        sur unu el la insuloj de <tld lit="J"/>ujo oni okaze eltrovis
        novan nutran kreska&jcirc;on, al kiu oni donis la nomon
        <em>Polygonum Saghalae</em>
        <fnt>
          <bib>FK</bib>,
          <lok>Nova nutra kreska&jcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
   <ref tip="sin" cel="japani.0o">Japanio</ref>
  </snc>
<rim mrk="japan.0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan diferencon en la
  uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
  <vrk><url
  ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
  Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
  <trd lng="bg">&c_Ja;&c_p;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">Japan</trd>
  <trd lng="en">Japan</trd>
  <trdgrp lng="fr">
    <trd>Japon</trd>,
    <trd>Etat du Japon</trd>
  </trdgrp>
  <trd lng="hu">Jap&aacute;n</trd>
  <trd lng="it">Giappone</trd>
  <trd lng="nl">Japan</trd>
  <trdgrp lng="pl">
    <trd>Japonia</trd>,
    <trd>Kraj Kwitn&aogonek;cej Wi&sacute;ni<klr> (poet.)</klr></trd>
  </trdgrp>
  <trd lng="sv">Japan</trd>
  <trd lng="pt">Jap&atilde;o</trd>
</drv>
</art>

