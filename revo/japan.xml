<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: japan.xml,v 1.26 2023/10/18 05:25:55 revo Exp $">
<kap>
  <ofc>9</ofc><rad>japan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="japan.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="japan.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de <ref tip="malprt" cel="japan.0ujo"><tld lit="J"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="japan.0o.ETN">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_p;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_ja;&c_p;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="bo">&#xF49;&#xF72;&#xF0B;&#xF67;&#xF7C;&#xF44;&#xF0B;&#xF42;&#xF72;&#xF0B;&#xF58;&#xF72;&#xF0B;</trd>
  <trd lng="de">Japaner</trd>
  <trd lng="en">Japanese<klr tip="amb"> (person)</klr></trd>
  <trdgrp lng="fr"> 
    <trd>Japonais<klr tip="ind"> (subst.)</klr></trd>
    <trd>Japonaise<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">jap&aacute;n</trd>
  <trd lng="io">Japoniano</trd>
  <trd lng="it">Giapponese</trd>
  <trd lng="ja">&#x65E5;&#x672C;&#x4EBA; <pr>&#x306B;&#x307B;&#x3093;&#x3058;&#x3093;</pr></trd>
  <trd lng="ko">&#xC77C;&#xBCF8;&#xC778;</trd>
  <trd lng="nl">Japanner</trd>
  <trd lng="pl">Japo&nacute;czyk</trd>
  <trd lng="pt">Japon&ecirc;s</trd>
  <trd lng="sv">japan<klr> (person)</klr></trd>
  <trd lng="tok">jan pi ma <ind>Nijon</ind></trd>
  <trd lng="uk">&#x44F;&#x43F;&#x43E;&#x43D;&#x435;&#x446;&#x44C;</trd>
  <trd lng="vo">Yap&auml;nan</trd>
  <trd lng="zh">&#x65E5;&#x672C;&#x4EBA; <pr>r&igrave;b&ecaron;nr&eacute;n</pr></trd>
</drv>

<drv mrk="japan.0a">
  <kap><tld/>a</kap>
  <snc mrk="japan.0a.KOMUNE">
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>a lingvo; <tld/>aj a&ubreve;toroj; <tld/>aj kutimoj.
      </ekz>
    </dif>
    <trdgrp lng="fr"> 
      <trd>japonais<klr tip="ind"> (adj.)</klr></trd>
      <trd>japonaise<klr tip="amb"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#x65E5;&#x672C;&#x306E; <pr>&#x306B;&#x307B;&#x3093;&#x306E;</pr></trd>,
      <trd>&#x65E5;&#x672C;&#x4EBA;&#x306E; <pr>&#x306B;&#x307B;&#x3093;&#x3058;&#x3093;&#x306E;</pr></trd>,
      <trd>&#x65E5;&#x672C;&#x8A9E;&#x306E; <pr>&#x306B;&#x307B;&#x3093;&#x3054;&#x306E;</pr></trd>
    </trdgrp>
    <trdgrp lng="tok">
      <trd>pi ma <ind>Nijon</ind></trd>,
      <trd>pi toki <ind>Nijon</ind></trd>
    </trdgrp>
    <trd lng="vo">Yap&auml;nik</trd>
  </snc>
  <snc>
    <dif>
     La <tld/>a <ref tip="super" cel="lingv.0o">lingvo</ref>.
    </dif>
    <trd lng="fr">japonais<klr tip="amb"> (langue)</klr></trd>
    <trd lng="ja">&#x65E5;&#x672C;&#x8A9E; <pr>&#x306B;&#x307B;&#x3093;&#x3054;</pr></trd>
    <trd lng="ko">&#xC77C;&#xBCF8;&#xC5B4;</trd>
    <trd lng="tok">toki <ind>Nijon</ind></trd>
    <trd lng="vo">Yap&auml;nap&uuml;k</trd>
    <trdgrp lng="zh">
      <trd>&#x65E5;&#x8BED; <pr>r&igrave;y&#x1D4;</pr></trd>,
      <trd>&#x65E5;&#x6587; <pr>r&igrave;w&eacute;n</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="bo">&#xF49;&#xF72;&#xF0B;&#xF67;&#xF7C;&#xF44;&#xF0B;&#xF66;&#xF90;&#xF51;&#xF0B;</trd>
  <trd lng="cs">japonsky</trd>
  <trd lng="de">japanisch</trd>
  <trd lng="en">Japanese</trd>
  <trd lng="es">japon&eacute;s</trd>
  <trd lng="hu">jap&aacute;n</trd>
  <trd lng="io">Japoniana</trd>
  <trd lng="it">giapponese</trd>
  <trd lng="nl">Japans</trd>
  <trd lng="pl">japo&nacute;ski</trd>
  <trd lng="pt">Japon&ecirc;s</trd>
  <trd lng="sk">japonsk&yacute;</trd>
  <trd lng="sv">japansk</trd>
  <trd lng="uk">&#x44F;&#x43F;&#x43E;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
</drv>

<drv mrk="japan.0ujo">
  <kap><ofc>9</ofc><tld lit="J"/>ujo <fnt><bib>PIV1</bib></fnt></kap>
  <mlg kod="ISO-3166">JP</mlg>
  <snc mrk="japan.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insularo en <ref tip="malprt" cel="azi.OrientaA0o">Orienta
      Azio</ref> kun kvar &ccirc;efaj insuloj, konsistiganta &scirc;taton, kies
      &ccirc;efurbo estas Tokio:
      <ekz>
        sur unu el la insuloj de <tld lit="J"/>ujo oni okaze eltrovis
        novan nutran kreska&jcirc;on, al kiu oni donis la nomon
        <em>Polygonum Saghalae</em>
        <fnt>
          <bib>FK</bib>,
          <lok>Nova nutra kreska&jcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
   <ref tip="sin" cel="japani.0o">Japanio</ref>
  </snc>
  <trd lng="be">&c_Ja;&c_p;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_Ja;&c_p;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="cs">Japonsko</trd>
  <trd lng="de">Japan</trd>
  <trd lng="en">Japan</trd>
  <trd lng="es">Jap&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>Japon</trd>,
    <trd>Etat du Japon</trd>
  </trdgrp>
  <trd lng="hi">&#x91C;&#x93E;&#x92A;&#x93E;&#x928;</trd>
  <trd lng="hu">Jap&aacute;n</trd>
  <trd lng="ia">Japon</trd>
  <trd lng="id">Jepang</trd>
  <trd lng="ie">Japan</trd>
  <trd lng="io">Japonia</trd>
  <trd lng="it">Giappone</trd>
  <trd lng="ja">&#x65E5;&#x672C; [<ind>&#x306B;&#x307B;&#x3093;</ind>, <ind>&#x306B;&#x3063;&#x307D;&#x3093;</ind>]</trd>
  <trd lng="ko">&#xC77C;&#xBCF8;</trd>
  <trd lng="nl">Japan</trd>
  <trdgrp lng="pl">
    <trd>Japonia</trd>,
    <trd>Kraj Kwitn&aogonek;cej Wi&sacute;ni<klr> (poet.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Jap&atilde;o</trd>
  <trd lng="sk">Japonsko</trd>
  <trd lng="sv">Japan</trd>
  <trd lng="tok">ma <ind>Nijon</ind></trd>
  <trd lng="tr">Japonya</trd>
  <trd lng="vo">Yap&auml;n</trd>
  <trd lng="zh">&#x65E5;&#x672C; <pr>R&igrave;b&ecaron;n</pr></trd>
</drv>
</art>

