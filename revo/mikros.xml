<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mikroskop</rad>/o
</kap>
<drv mrk="mikros.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> por pligrandigi
      kaj vidigi objektojn nevideblajn per nuda okulo:
      <ekz>
        Bazarov alportis kun si <tld/>on kaj dum longaj horoj estis
        okupata per &gcirc;i
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 10a, p. 
46a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="de">Mikroskop</trd>
  <trd lng="en">microscope</trd>
  <trd lng="es">microscopio</trd>
  <trdgrp lng="fr">
    <trd>microscope</trd>
  </trdgrp>
  <trd lng="hu">mikroszk&oacute;p</trd>
  <trd lng="nl">microscoop</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="sv">mikroskop</trd>
</drv>

<drv mrk="mikros.0a">
  <kap><tld/>a</kap>
  <snc mrk="mikros.0a.ila">
    <dif>
      Farata per <tld/>o:
      <ekz>
        <tld/>a esploro.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mikros.0a.mikra">
    <dif>
      Videbla nur per <tld/>o:
      <ekz>
        <tld/>aj esta&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">mikroskopisch</trd>
  <trd lng="en">microscopic</trd>
  <trd lng="es">microsc&oacute;pico</trd>
  <trdgrp lng="fr">
    <trd>microscopique</trd>
  </trdgrp>
  <trd lng="hu">mikroszkopikus</trd>
  <trd lng="nl">microscopisch</trd>
  <trd 
lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">mikroskopisk</trd>
</drv>

<drv mrk="mikros.M0o">
  <kap><tld lit="M"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Mic</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="super" 
cel="stel.0figuro">stelfiguro</ref> (<trd lng="la">Microscopium</trd>).
    </dif>
  </snc>
  <trd lng="de">Mikroskop</trd>
  <trd lng="en">Microscopium</trd>
  <trd lng="es">Microsc&oacute;pio</trd>
</drv>

</art>
