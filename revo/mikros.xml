<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mikroskop</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="mikros.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikros.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> por
      pligrandigi kaj vidigi objektojn nevideblajn per nuda
      okulo:
      <ekz>
        Bazarov alportis kun si <tld/>on kaj dum longaj horoj estis
        okupata per &gcirc;i
        <fnt>
          <bib>PatrojFiloj</bib>,
          <lok>&ccirc;apitro 10a, p. 46a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="ca">microscopi</trd>
  <trd lng="de">Mikroskop</trd>
  <trd lng="en">microscope</trd>
  <trd lng="es">microscopio</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&f_k;&a_r;&a_w;&a_s;&f_k;&f_p;</trd>,
    <trd>&a_r;&f_y;&a_z;&a_b;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">microscope</trd>
  <trd lng="hu">mikroszk&oacute;p</trd>
  <trd lng="nl">microscoop</trd>
  <trd lng="pt">microsc&oacute;pio</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="sv">mikroskop</trd>
</drv>

<drv mrk="mikros.0a">
  <kap><tld/>a</kap>
  <snc mrk="mikros.0a.ila">
    <dif>
      Farata per <tld/>o:
      <ekz>
        <tld/>a esploro.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mikros.0a.mikra">
    <dif>
      Videbla nur per <tld/>o:
      <ekz>
        <tld/>aj esta&jcirc;oj.
      </ekz>
    </dif>
    <trd lng="fa">&a_dh;&a_r;&a_h;&zwnj;&a_b;&f_y;&a_n;&f_y;</trd>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">microsc&ograve;pic</trd>
  <trd lng="de">mikroskopisch</trd>
  <trd lng="en">microscopic</trd>
  <trd lng="es">microsc&oacute;pico</trd>
  <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_s;&f_k;&f_p;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>microscopique</trd>
  </trdgrp>
  <trd lng="hu">mikroszkopikus</trd>
  <trd lng="nl">microscopisch</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">mikroskopisk</trd>
</drv>

<drv mrk="mikros.M0o">
  <kap><tld lit="M"/>o</kap>
  <mlg>Mic</mlg>
  <snc mrk="mikros.M0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera
      <ref tip="super" cel="stel.0figuro">stelfiguro</ref>
      (<trd lng="la">Microscopium</trd>).
    </dif>
  </snc>
  <trd lng="ca">el <ind>Microscopi</ind></trd>
  <trd lng="de">Mikroskop</trd>
  <trd lng="en">Microscopium</trd>
  <trd lng="es">Microsc&oacute;pio</trd>
  <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_s;&f_k;&f_p;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t;
  &a_f;&a_l;&f_k;&f_y;)</klr></trd>
</drv>

<drv mrk="mikros.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="mikros.0ado.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Procedoj kaj plenumo de esploroj per <tld/>o:
      <ekz>
        sekigo de delikataj materialoj <klr>...</klr> por elektrona 
        <tld/>ado
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Superkrita_flua%C4%B5o">
          superkrita flua&jcirc;o</url>, 2010-07-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mikroskopi.0o.FOT">mikroskopio</ref>
    </refgrp>
  </snc>
  <trd lng="ca">microsc&ograve;pia</trd>
  <trd lng="fr">microscopie</trd>
</drv>

<drv mrk="mikros.elektron0o">
  <kap>elektronmikroskopo <fnt><bib>PIV2</bib></fnt>, 
  <var><kap>elektrona <tld/>o</kap></var></kap>
  <snc mrk="mikros.elektron0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="mikros.0o.FOT"><tld lit="M"/>o</ref>
      en kiu elektron&jcirc;eto esploras la observata&jcirc;on kaj ekrano
      prezentas la grandigitan bildon:
      <ekz>
        la unua elektrona <tld/>o estis konstruita en 1931
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/elektrona%20mikroskopo">
          elektrona mikroskopo</url>, 2010-12-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca"><ind>microscopi</ind> electr&ograve;nic</trd>
  <trd lng="fr">microscope &eacute;lectronique</trd>
</drv>

<drv mrk="mikros.lum0o">
  <kap>lum<tld/>o <fnt><bib>PIV2</bib></fnt>,
  <var><kap>optika <tld/>o</kap></var></kap>
  <snc mrk="mikros.lum0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="mikros.0o.FOT"><tld lit="M"/>o</ref>
      funkcianta per optikaj lensoj, vidiganta la observata&jcirc;on
      per ordinara lumo:
      <ekz>
        oni elprenis specimenojn je specife 
        fiksitaj tempoj por determini la distribuon de la 
        kristalgrandeco per la optika <tld/>o OLYMPUS BH-2
        <fnt>
          <aut>A. Toledo k.a.</aut>,
          <vrk><url
          ref="http://www.eventoj.hu/steb/medicino/insulina-kristaligxo/kristal.htm">
          Kinetiko de kristalizado de insulino</url></vrk>,
          <lok>Scienca Revuo, 1997:3 (48)</lok>
        </fnt>;
      </ekz>
      <ekz>
        La plej malgrandaj strukturoj &ndash; ekzameneblaj per lum<tld/>o &ndash; 
        grandas &ccirc;<klr>irka&ubreve;</klr> 500.000 <klr>pikometrojn</klr>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Pikometro">
          pikometro</url>, 2010-12-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca"><ind>microscopi</ind> &ograve;ptic</trd>
  <trd lng="fr">microscope optique</trd>
</drv>
</art>
