<kap>
  <ofc>2</ofc>
  <rad>mikroskop</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mikros.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikros.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> por
      pligrandigi kaj vidigi objektojn nevideblajn per nuda
      okulo:
      <ekz>
        Bazarov alportis kun si <tld/>on kaj dum longaj horoj estis
        okupata per &gcirc;i
        <fnt>
          <bib>PatrojFiloj</bib>,
          <lok>&ccirc;apitro 10a, p. 46a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="ca">microscopi</trd>
  <trdgrp lng="cs">
    <trd>drobnohled</trd>,
    <trd>mikroskop</trd>
  </trdgrp>
  <trd lng="de">Mikroskop</trd>
  <trd lng="en">microscope</trd>
  <trd lng="es">microscopio</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&f_k;&a_r;&a_w;&a_s;&f_k;&f_p;</trd>,
    <trd>&a_r;&f_y;&a_z;&a_b;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">microscope</trd>
  <trd lng="hu">mikroszk&oacute;p</trd>
  <trd lng="nl">microscoop</trd>
  <trd lng="pl">mikroskop</trd>
  <trd lng="pt">microsc&oacute;pio</trd>
  <trd lng="ro">microscop</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>drobnoh&lcaron;ad</trd>,
    <trd>mikroskop</trd>
  </trdgrp>
  <trd lng="sv">mikroskop</trd>
  <trdgrp lng="zh">
    <trd>&#x663E;&#x5FAE;&#x955C; <pr>xi&#x1CE;nw&#x113;ij&#xEC;ng</pr></trd>,
    <trd>&#x986F;&#x5FAE;&#x93E1; <pr>xi&#x1CE;nw&#x113;ij&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mikros.0a">
  <kap><tld/>a</kap>
  <snc mrk="mikros.0a.ila">
    <dif>
      Farata per <tld/>o:
      <ekz>
        <tld/>a esploro;
      </ekz>
      <ekz>Sido levis &gcirc;in el la interno de la esta&jcirc;o por porti &gcirc;in en
      Faremido cele de <tld/>a ekzameno
        <fnt>
          <aut>Frigyes Karinthy, trad. L. Totsche</aut>
          <vrk>Voja&gcirc;o al Faremido</vrk>
          <lok>&ccirc;apitro 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mikros.0a.mikra">
    <dif>
      Videbla nur per <tld/>o:
      <ekz>
        <tld/>aj esta&jcirc;oj;
      </ekz>
      <ekz>en sia korpo la homo <ctl>scias</ctl> kontra&ubreve; &ccirc;iu <tld/>a
      atakanto tuj produkti adekvatajn kontra&ubreve;korpetojn
        <fnt>
          <aut>Vinko O&scaron;lak</aut>
          <vrk>Filozofii subtende</vrk>
          <lok>&ccirc;apitro 3a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_dh;&a_r;&a_h;&#x200C;&a_b;&f_y;&a_n;&f_y;</trd>
    <trd lng="pl">mikroskopijny</trd>
    <trd lng="ro">microscopic</trd>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">microsc&ograve;pic</trd>
  <trdgrp lng="cs">
    <trd>drobnohledn&yacute;</trd>,
    <trd>mikroskopick&yacute;</trd>
  </trdgrp>
  <trd lng="de">mikroskopisch</trd>
  <trd lng="en">microscopic</trd>
  <trd lng="es">microsc&oacute;pico</trd>
  <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_s;&f_k;&f_p;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>microscopique</trd>
  </trdgrp>
  <trd lng="hu">mikroszkopikus</trd>
  <trdgrp lng="ja">
    <trd>&#x9855;&#x5FAE;&#x93E1;&#x306E;  <pr>&#x3051;&#x3093;&#x3073;&#x304B;&#x304C;&#x307F;&#x306E;</pr></trd>,
    <trd>&#x9855;&#x5FAE;&#x93E1;&#x3092;&#x4F7F;&#x3063;&#x305F; 
    <pr>&#x3051;&#x3093;&#x3073;&#x304B;&#x304C;&#x307F;&#x3092;&#x3064;&#x304B;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x5FAE;&#x8996;&#x7684;&#x306A;  <pr>&#x3073;&#x3057;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5FAE;&#x7D30;&#x306A;  <pr>&#x3073;&#x3055;&#x3044;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">microscopisch</trd>
  <trd lng="pl">mikroskopowy</trd>
  <trd lng="ro">microscopic&abreve;</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">mikroskopick&yacute;</trd>
  <trd lng="sv">mikroskopisk</trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43A;&c_r;&#x43E;&c_s;&#x43A;&#x43E;&#x43F;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x663E;&#x5FAE; <pr>xi&#x1CE;nw&#x113;i</pr></trd>,
    <trd>&#x986F;&#x5FAE; <pr>xi&#x1CE;nw&#x113;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mikros.M0o">
  <kap><tld lit="M"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Mic</mlg>
  <snc mrk="mikros.M0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera
      <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Microscopium</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p; <klr>(&c_s;&c_u;&c_z;&c_r;&c_o;&#x2019;&c_je;)</klr></trd>
  <trd lng="ca">el <ind>Microscopi</ind></trd>
  <trdgrp lng="cs">
    <trd>drobnohled</trd>,
    <trd>mikroskop</trd>
  </trdgrp>
  <trd lng="de">Mikroskop</trd>
  <trd lng="en">Microscopium</trd>
  <trd lng="es">Microsc&oacute;pio</trd>
  <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_s;&f_k;&f_p;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t;
  &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trd lng="fr">Microscope<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="ja">&#x9855;&#x5FAE;&#x93E1;  <pr>&#x3051;&#x3093;&#x3073;&#x304D;&#x3087;&#x3046;</pr></trd>
  <trd lng="pl">Mikroskop  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Microscopul  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="sk">
    <trd>drobnoh&lcaron;ad</trd>,
    <trd>mikroskop</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_ib;&#x43A;&c_r;&#x43E;&c_s;&#x43A;&#x43E;&#x43F;</trd>
  <trdgrp lng="zh">
    <trd>&#x663E;&#x5FAE;&#x955C;&#x5EA7; <pr>Xi&#x1CE;nw&#x113;ij&#xEC;ngzu&#xF2;</pr></trd>,
    <trd>&#x986F;&#x5FAE;&#x93E1;&#x5EA7; <pr>Xi&#x1CE;nw&#x113;ij&#xEC;ngzu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mikros.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="mikros.0ado.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Procedoj kaj plenumo de esploroj per <tld/>o:
      <ekz>
        sekigo de delikataj materialoj <klr>...</klr> por elektrona
        <tld/>ado
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Superkrita_flua%C4%B5o">
          superkrita flua&jcirc;o</url>, 2010-07-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mikroskopi.0o.FOT">mikroskopio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="de">Mikroskopie</trd>
  <trd lng="en">microscopy</trd>
  <trd lng="ca">microsc&ograve;pia</trd>
  <trd lng="fr">microscopie</trd>
  <trd lng="pl">mikroskopia</trd>
  <trd lng="ro">microscopie</trd>
  <trdgrp lng="zh">
    <trd>&#x663E;&#x5FAE;&#x672F; <pr>xi&#x1CE;nw&#xE9;ish&#xF9;</pr></trd>,
    <trd>&#x986F;&#x5FAE;&#x8853; <pr>xi&#x1CE;nw&#xE9;ish&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mikros.elektron0o">
  <kap>elektronmikroskopo <fnt><bib>PIV2</bib></fnt>,
  <var><kap>elektrona <tld/>o</kap></var></kap>
  <snc mrk="mikros.elektron0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="mikros.0o.FOT"><tld lit="M"/>o</ref>
      en kiu elektron&jcirc;eto esploras la observata&jcirc;on kaj ekrano
      prezentas la grandigitan bildon:
      <ekz>
        la unua elektrona <tld/>o estis konstruita en 1931
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/elektrona%20mikroskopo">
          elektrona mikroskopo</url>, 2010-12-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_y; <ind>&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</ind></trd>
  <trd lng="de">Elektronenmikroskop</trd>
  <trd lng="en">electron microscope</trd>
  <trd lng="ca"><ind>microscopi</ind> electr&ograve;nic</trd>
  <trd lng="cs">elektronov&yacute; mikroskop</trd>
  <trd lng="fr">microscope &eacute;lectronique</trd>
  <trd lng="ja">&#x96FB;&#x5B50;&#x9855;&#x5FAE;&#x93E1;  <pr>&#x3067;&#x3093;&#x3057;&#x3051;&#x3093;&#x3073;&#x304D;&#x3087;&#x3046;</pr></trd>
  <trd lng="pl">mikroskop elektronowy</trd>
  <trd lng="ro">microscop electronic</trd>
  <trd lng="sk">elektr&oacute;nov&yacute; mikroskop</trd>
  <trd lng="uk">&c_je;&#x43B;&c_je;&#x43A;&c_t;&c_r;&#x43E;&#x43D;&#x43D;&c_i;&c_j;
  &#x43C;&c_ib;&#x43A;&c_r;&#x43E;&c_s;&#x43A;&#x43E;&#x43F;</trd>
  <trdgrp lng="zh">
    <trd>&#x7535;&#x5B50;&#x663E;&#x5FAE;&#x955C; <pr>di&#xE0;nz&#x1D0;xi&#x1CE;nw&#x113;ij&#xEC;ng</pr></trd>,
    <trd>&#x96FB;&#x5B50;&#x986F;&#x5FAE;&#x93E1; <pr>di&#xE0;nz&#x1D0;xi&#x1CE;nw&#x113;ij&#xEC;ng</pr></trd>,
    <trd>&#x7535;&#x5B50;&#x663E;&#x5FAE;&#x955C; <pr>di&#xE0;nz&#x1D0;xi&#x1CE;nw&#xE9;ij&#xEC;ng</pr></trd>,
    <trd>&#x96FB;&#x5B50;&#x986F;&#x5FAE;&#x93E1; <pr>di&#xE0;nz&#x1D0;xi&#x1CE;nw&#xE9;ij&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mikros.lum0o">
  <kap>lum<tld/>o <fnt><bib>PIV2</bib></fnt>,
  <var><kap>optika <tld/>o</kap></var></kap>
  <snc mrk="mikros.lum0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="mikros.0o.FOT"><tld lit="M"/>o</ref>
      funkcianta per optikaj lensoj, vidiganta la observata&jcirc;on
      per ordinara lumo:
      <ekz>
        oni elprenis specimenojn je specife
        fiksitaj tempoj por determini la distribuon de la
        kristalgrandeco per la optika <tld/>o OLYMPUS BH-2
        <fnt>
          <aut>A. Toledo k.a.</aut>,
          <vrk><url ref="&STEB;medicino/insulina-kristaligxo/kristal.htm">
          Kinetiko de kristalizado de insulino</url></vrk>,
          <lok>Scienca Revuo, 1997:3 (48)</lok>
        </fnt>;
      </ekz>
      <ekz>
        La plej malgrandaj strukturoj &ndash; ekzameneblaj per lum<tld/>o &ndash;
        grandas &ccirc;<klr>irka&ubreve;</klr> 500.000 <klr>pikometrojn</klr>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Pikometro">
          pikometro</url>, 2010-12-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y; <ind>&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</ind></trd>
  <trdgrp lng="de">
    <trd>Lichtmikroskop</trd>,
    <trd>optisches <ind>Mikroskop</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>light microscope</trd>,
    <trd>optical microscope</trd>
  </trdgrp>
  <trd lng="ca"><ind>microscopi</ind> &ograve;ptic</trd>
  <trd lng="cs">mikroskop</trd>
  <trd lng="fr">microscope optique</trd>
  <trd lng="ja">&#x5149;&#x5B66;&#x9855;&#x5FAE;&#x93E1;  <pr>&#x3053;&#x3046;&#x304C;&#x304F;&#x3051;&#x3093;&#x3073;&#x304D;&#x3087;&#x3046;</pr></trd>
  <trd lng="pl">mikroskop <klr tip="amb">optyczny</klr></trd>
  <trd lng="ro">microscop  <klr tip="amb">optic</klr></trd>
  <trd lng="sk">mikroskop</trd>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x5B66;&#x663E;&#x5FAE;&#x955C; <pr>gu&#x101;ngxu&#xE9;xi&#x1CE;nw&#x113;ij&#xEC;ng</pr></trd>,
    <trd>&#x5149;&#x5B78;&#x986F;&#x5FAE;&#x93E1; <pr>gu&#x101;ngxu&#xE9;xi&#x1CE;nw&#x113;ij&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
