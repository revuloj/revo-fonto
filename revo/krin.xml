<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krin.xml,v 1.11 2023/12/01 18:29:32 revo Exp $">
<kap>
  <rad>krin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="krin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="krin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Solida, longa <ref tip="super" cel="har.0o.mamulo">haro</ref> de
      granda besto, precipe el la <ref tip="vid" cel="har.kol0aro">kolhararo</ref> a&ubreve; vosto:
      <ekz>
        la alia duelanto devas kro&ccirc;i&gcirc;i al la <tld/>oj de sia
        rajdbesto
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 13a, p.
        93a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fibr.0o">fibro</ref>,
      <ref cel="har.0ego">harego</ref>,
      <ref cel="lan.0o">lano</ref>,
      <ref cel="lanug.0o">lanugo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_l;&c_a;&c_s; <klr tip="amb">(&c_z; &c_g;&c_r;&c_y;&c_v;&c_y; &c_a;&c_b;&c_o;
  &c_h;&c_v;&c_a;&c_s;&c_t;&c_a; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_y;)</klr></trd>
  <trd lng="ca">crin</trd>
  <trd lng="cs">&zcaron;&iacute;n&ecaron;</trd>
  <trdgrp lng="de">
    <trd>Langhaar</trd>,
    <trd>Rosshaar</trd>
  </trdgrp>
  <trd lng="es">crin</trd>
  <trd lng="fr">crin</trd>
  <trdgrp lng="nl">
    <trd>crin</trd>,
    <trd>paardenhaar</trd>
  </trdgrp>
  <trd lng="pl">w&lstroke;osie</trd>
  <trd lng="pt">crina</trd>
  <trd lng="ro">p&abreve;r</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_s; <klr tip="amb">(&c_i;&c_z; &c_g;&c_r;&c_i;&c_v;&c_y; &c_i;&c_l;&c_i;
  &c_h;&c_v;&c_o;&c_s;&c_t;&c_a; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_g;&c_o;)</klr></trd>
  <trd lng="sk">srs&tcaron;</trd>
  <trd lng="uk">&c_k;&c_ib;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_v;&c_o;&c_l;&c_o;&c_s;</trd>
</drv>
</art>

