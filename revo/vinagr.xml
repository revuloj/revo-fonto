<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vinagr.xml,v 1.27 2022/03/28 11:08:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vinagr</rad>/o
</kap>

<drv mrk="vinagr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Alkohola&jcirc;o, precipe vino, acidi&gcirc;inta per fermentado, uzata kiel spica&jcirc;o:
      <ekz>
        en tiuj &ccirc;i boteletoj sin trovas diversaj acidoj: <tld/>o, sulfuracido, azotacido
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        alproksimi&gcirc;u kaj man&gcirc;u kune, vi anka&ubreve; povas trempi vian panon en la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        li donis al la maljunulo akvon, frotis al li la frunton kaj tempiojn per <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la dommastrino atendas min por <tld/>o-dol&ccirc;e aludi pri la mankanta lupago
        <fnt>
          <bib>KrM</bib>
          <lok>4. Konstante Frapante</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli allogas kulero da mielo ol da <tld/>o barelo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fluida&jcirc;o sen difino, nek <tld/>o nek vino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        havi <tld/>on en la mieno
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="acet.0o">aceto</ref>
    <refgrp tip="vid">
      <ref cel="mild.mal0a">malmilda</ref>,
      <ref cel="dolcx">maldol&ccirc;a</ref>,
      <ref cel="afabl">malafabla</ref>,
      <ref cel="acid.0a">acida</ref>,
      <ref cel="incit">incitita</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_c;&c_a;&c_t;</trd>
  <trd lng="bg">&c_o;&c_c;&c_je;&c_t;</trd>
  <trd lng="ca">vinagre</trd>
  <trd lng="cs">ocet</trd>
  <trd lng="de">Essig</trd>
  <trd lng="el">&xi;&jota_ton;&delta;&jota;</trd>
  <trd lng="en">vinegar</trd>
  <trdgrp lng="es">
    <trd>vinagre</trd>,
    <trd>vinagre</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_r;&f_k;&a_h;</trd>
  <trd lng="fr">vinaigre</trd>
  <trd lng="he">&het;&vav;&mem;&ftsadi;</trd>
  <trd lng="hu">ecet</trd>
  <trd lng="id">cuka</trd>
  <trd lng="nl">azijn</trd>
  <trd lng="pl">ocet</trd>
  <trd lng="pt">vinagre</trd>
  <trd lng="ro">o&tcedil;et</trd>
  <trd lng="ru">&c_u;&c_k;&c_s;&c_u;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>ocot</trd>,
    <trd>ocot <klr>(v&iacute;nny)</klr></trd>
  </trdgrp>
  <trd lng="sv">vin&auml;ger</trd>
  <trd lng="tr">sirke</trd>
  <trd lng="uk">&#x43E;&#x446;&#x435;&#x442;</trd>
</drv>
</art>
