<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vinagr</rad>/o
</kap>

<drv mrk="vinagr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Acidigita vino uzata kiel spica&jcirc;o:
      <ekz>
        &ccirc;esis esti vino kaj <tld/>o ne fari&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pli allogas kulero da mielo ol da <tld/>o barelo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        havi <tld/>on en la mieno
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>a humoro</ind><fnt>Z</fnt>, tono
        <trd lng="hu">savany&uacute; hangulat</trd>
        <trd lng="ru">&c_k;&c_i;&c_s;&c_l;&c_o;&c_je;
&c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mild.mal0a">malmilda</ref>,
      <ref cel="dolcx">maldol&ccirc;a</ref>,
      <ref cel="afabl">malafabla</ref>,
      <ref cel="acid.0a">acida</ref>,
      <ref cel="incit">incitita</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_c;&c_a;&c_t;</trd>
  <trd lng="bg">&c_o;&c_c;&c_je;&c_t;</trd>
  <trd lng="de">Essig</trd>
  <trd lng="el">&xi;&jota_ton;&delta;&jota;</trd>
  <trd lng="en">vinegar</trd>
  <trd lng="fa">&a_s;&a_r;&f_k;&a_h;</trd>
  <trd lng="fr">vinaigre</trd>
  <trd lng="he">&het;&vav;&mem;&ftsadi;</trd>
  <trd lng="hu">ecet</trd>
  <trd lng="nl">azijn</trd>
  <trd lng="pl">ocet</trd>
  <trd lng="pt">vinagre</trd>
  <trd lng="ru">&c_u;&c_k;&c_s;&c_u;&c_s;</trd>
  <trd lng="sk">ocot</trd>
  <trd lng="sv">vin&auml;ger</trd>
  <trd lng="tr">sirke</trd>
</drv>
</art>
