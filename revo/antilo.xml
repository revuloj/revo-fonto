<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>antilop</rad>/o
</kap>
<drv mrk="antilo.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc mrk="antilo.0o.subfamiliano">
     <dif>
       Iu el subfamilio de familio
       <ref tip="malprt" cel="bov.0edoj">bovedoj</ref>
       (<trd lng="la">Antilopinae</trd>), al kiu apartenas
       la genroj
       <refgrp tip="sub">
         <ref cel="antilo.0o.genro">antilopo</ref>,
         <ref cel="gazel.0o">gazelo</ref> kaj.a.
       </refgrp>
     </dif>
   </snc>
   <snc mrk="antilo.0o.genro">
     <dif>
       Genro el familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>
       (<trd lng="la">Antilope</trd>).
     </dif>
   </snc>
   <trd lng="de">Antilope</trd>
   <trd lng="fr">antilope</trd>
   <trd lng="ru">&#1072;&#1085;&#1090;&#1080;&#1083;&#1086;&#1087;&#1072;</trd>
</drv>
</art>
