<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>antilop</rad>/o
</kap>
<drv mrk="antilo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="antilo.0o.1" num="1">
    <dif>
      Iu el subfamilio de familio 
      <ref tip="super" cel="bov.0edoj">bovedoj</ref>       
      (<trd lng="la">Antilopinae</trd>), al kiu apartenas ekzemple      
      <refgrp tip="sub">         
        <ref cel="antilo.0o.2">antilopo 2</ref>,
        <ref cel="gazel.0o">gazelo</ref> k.a.
      </refgrp>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Genro el familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>
      (<trd lng="la">Antilope</trd>).
    </dif>
  </snc>
  <trd lng="de">Antilope</trd>
</drv>
</art>
