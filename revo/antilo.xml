<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>antilop</rad>/o
</kap>
<drv mrk="antilo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="antilo.0o.subfamiliano">
    <dif>
      Iu el subfamilio de familio
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Antilopinae</trd>), al kiu apartenas
      la genroj
      <refgrp tip="sub">
        <ref cel="antilo.0o.genro">antilopo</ref>,
        <ref cel="gazel.0o">gazelo</ref> k.a.
      </refgrp>
    </dif>
  </snc>
  <snc mrk="antilo.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Antilope</trd>).
    </dif>
  </snc>
  <trd lng="de">Antilope</trd>
  <trd lng="fr">antilope</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_l;&c_o;&c_p;&c_a;</trd>
  <trd lng="sv">antilop</trd>
</drv>
</art>
