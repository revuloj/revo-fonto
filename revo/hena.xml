<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hena</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hena.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxmink.0o">&Scirc;minko</ref> farita el pulvoro de
      sekigitaj folioj de lavsonio, uzata por kuprokolore tinkturi harojn,
      ungojn, ha&ubreve;ton, populara en Nordafriko:
      <ekz>
        muelitaj kiel <tld/>o a&ubreve; tritiko
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 28a, p.
        553a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_n;&c_a;</trd>
  <trd lng="fr">henn&eacute;</trd>
  <trd lng="pt">hena</trd>
  <trd lng="ru">&c_h;&c_n;&c_a;</trd>
</drv>
</art>

