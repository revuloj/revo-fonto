<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bunt.xml,v 1.24 2022/11/28 14:28:04 revo Exp $">
<kap>
  <rad>bunt</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bunt.0a">
  <kap><tld/>a</kap>
  <snc mrk="bunt.0a.KOL">
    <dif>
      <ref tip="dif" cel="kolor.divers0a">Diverskolora</ref>, varikolora:
      <ekz>
        <klr>[li]</klr> rigardas <tld/>an sensencan bildon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9501-sterogramoj.txt">Stereogramoj</url></vrk>
          <lok>95/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;festenega&rightquot; diman&ccirc;o estas tre <tld/>a pro la centoj da partoprenantoj,
        surhavantaj maskojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006955.html">Belgio: Fama karnavalo en Binche</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la kolon li ligis al si <tld/>an tukon
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bunt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Variaspekta, <ref tip="vid" cel="divers.0a">diversa</ref>:
      <ekz>
        la arkitekturo <klr>[&#8230;]</klr> indikas <tld/>an, plurkulturan miksa&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007095.html">Pollando: Landlimoj sen limoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li originis el <tld/>a familio: inter liaj prauloj estis kaj pastroj, kaj
        militistoj, kaj caraj oficistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2003/008014.html">Ukrainaj kantoj kaj peterburgaj
          groteskoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        nuntempe mia urbo havas <tld/>an grupon de esperantistoj
        <fnt>
          <aut>D. Obando</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_s93wXk7FkJE/">Mia Asocio, Mia Urbo &ndash; Cali,
          Kolombio</url></vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">vielf&auml;ltig</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_t;&c_y;</trd>, 
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_s;&c_t;&c_y;</trd>, 
    <trd>&c_r;&c_a;&c_b;&c_y;</trd>, 
    <trd>&c_r;&c_o;&c_z;&c_n;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_r;&c_o;&c_v;&c_y;</trd>,   
    <trd>&c_k;&c_v;&c_ja;&c_c;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">pestr&yacute;</trd>
  <trd lng="de">bunt</trd>
  <trdgrp lng="es">
    <trd>variopinto <klr tip="amb">(sobre colores)</klr></trd>,
    <trd>variopinta <klr tip="amb">(sobre colores)</klr></trd>,
    <trd>multicolor</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_r;&a_n;&f_g;&a_A;&a_r;&a_n;&f_g;</trd>,
    <trd>&a_A;&a_l;&a_w;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bariol&eacute;</trd>,
    <trd>bigarr&eacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5E1;&#x5E1;&#x5D2;&#x5D5;&#x5E0;&#x5D9;</trd>
  <trd lng="hu">tarka</trd>
  <trd lng="it">variopinto</trd>
  <trdgrp lng="nl">
   <trd>bont<klr tip="ind"> (veelkleurig)</klr></trd>,
   <trd>veelkleurig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowy</trd>,
    <trd>r&oacute;&zdot;nokolorowy</trd>,
    <trd>barwny</trd>,
    <trd>wielobarwny</trd>,
    <trd>r&oacute;&zdot;nobarwny</trd>,
    <trd>pstry</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_jo;&c_s;&c_t;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_g;&c_i;&c_j; <klr>(&c_o;
    &c_l;&c_o;&c_sh;&c_a;&c_d;&c_i;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jarab&yacute;</trd>,
    <trd>pestr&yacute;</trd>,
    <trd>strakat&yacute;</trd>
  </trdgrp>
  <trd lng="sv">f&auml;rgglad</trd>
  <trd lng="tok">kule</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&#x43E;&#x43A;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_ib;&c_s;&c_t;&c_r;&#x44F;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_z;&#x43D;&#x43E;&c_b;&c_a;&c_r;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x44F;&c_b;&c_i;&c_j;&#xA0;(&#x43F;&c_r;&#x43E; &#x43C;&c_a;&c_s;&c_t;&#x44C;)</trd>
  </trdgrp>
</drv>

<drv mrk="bunt.0eco">
  <kap><tld/>eco,
  <var><kap><tld/>o</kap></var></kap>
  <snc mrk="bunt.0eco.KOL">
    <dif>
      Plurkoloreco:
      <ekz>
        Pandiono vidis <klr>[&#8230;]</klr> fajencajn vazojn kun mirakla 
        <tld/>eco de ornama&jcirc;oj a&ubreve; kun bildoj de maraj animaloj
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>, 
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/arko.html">
          La Granda Arko (&Ccirc;e la rando de la Ekumeno)</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bunt.0eco.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="divers.0eco.KOMUNE">Diverseco</ref>,
      <ref tip="vid" cel="homoge.mal0eco.KOMUNE">malhomogeneco</ref>,
      <ref tip="vid" cel="unu.mal0eco.KOMUNE">malunueco</ref>:
      <ekz>
        por percepti la mondon en tuta &gcirc;ia <tld/>eco kaj profundeco, 
        necesas havi akrajn sentojn
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/horo.html">
          Horo de Bovo</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        diverseco ofte pli &gcirc;uste estas nur <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008168.html">EU-reformeto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        vikipedie, foirbude, volumege, bro&scirc;urete <klr>[&#8230;]</klr> en la 
        tuta <tld/>o de nia repertuaro
        <fnt>
          <aut>P. Dasgupta</aut>,
          <vrk><url ref="http://www.uea.org/dokumentoj/komunikoj/gk.php?no=355">
          Novjara Tosto</url></vrk>,
          <lok>UEA - Gazetaraj Komunikoj, 2010-01-01</lok>
        </fnt>.        
      </ekz>
    </dif>
    <trd lng="de">Vielf&auml;ltigkeit</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_e;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">pestrost</trd>
  <trd lng="de">Buntheit</trd>
  <trdgrp lng="es">
    <trd>mezcolanza</trd>,
    <trd>abigarramiento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bariolure</trd>,
    <trd>bigarrure</trd>,
    <trd>chamarrure</trd>
  </trdgrp>
  <trd
  lng="he">&#x5E6;&#x5B4;&#x5D1;&#x5E2;&#x5D5;&#x5B9;&#x5E0;&#x5B4;&#x5D9;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trd lng="sk">pestros&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&#x43E;&#x43A;&c_a;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_ib;&c_s;&c_t;&c_r;&#x44F;&c_v;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
