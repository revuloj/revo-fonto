<art mrk="$Id: embrio1.xml,v 1.17 2020/03/22 19:10:29 revo Exp $">
<kap>
  <rad>embriolog</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="embrio1.0o">
  <kap><tld/>o</kap>
  <snc mrk="embrio1.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="embrio2.0isto">Embriologiisto</ref>:
      <ekz>
        la &ccirc;efa rolanto de la verko estas David Kurio, kuracisto-<tld/>o
        kun firma volo
        kaj ne malpli firma kristana etiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=555834"
              >La Akvariinfanoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Koherigi kun aliaj nomoj de fakuloj en "-logo" koncerne fakindikon.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_e;&c_m;&c_b;&c_r;&c_y;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Embryologe</trd>
  <trd lng="en">embryologist</trd>
  <trd lng="es">embri&oacute;logo</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&f_y;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>,
    <trd>&a_j;&a_n;&f_y;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">embryologue</trd>
  <trd lng="hu">embriol&oacute;gus</trd>
  <trd lng="nl">embryoloog</trd>
  <trd lng="pl">embriolog</trd>
  <trd lng="pt">embriologista</trd>
  <trd lng="ru">&c_e;&c_m;&c_b;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
