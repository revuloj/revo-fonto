<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gak.xml,v 1.21 2025/06/27 02:21:52 revo Exp $">
<kap>
  <rad>gak</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gak.0i">
  <kap><tld/>i</kap>
  <snc mrk="gak.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel <ref tip="vid"
      cel="anas.0o.ZOO">anaso</ref>
      a&ubreve; <ref tip="vid" cel="anser.0o.genro">ansero</ref>:
      <ekz>
        sova&gcirc;aj anseroj <tld/>as dum nokta flugo
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>ansere bleki</ke>.
  </rim>

  <trd lng="be">&c_k;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_k;&c_a;&c_ch;&c_k;&c_u;)</klr></trd>
  <trd lng="bg">&c_k;&c_u;&c_d;&c_k;&c_u;&c_d;&c_ja;&c_k;&c_a;&c_m;</trd>
  <trd lng="ca">clacar</trd>
  <trd lng="cs">kejhat</trd>
  <trd lng="de">gackern</trd>
  <trd lng="en">cackle</trd>
  <trd lng="es">graznar</trd>
  <trd lng="fr">cancaner</trd>
  <trd lng="hu">g&aacute;gog</trd>
  <trdgrp lng="nl">
    <trd>gakken</trd>,
    <trd>kakelen</trd>
  </trdgrp>
  <trd lng="pl">kwaka&cacute;</trd>
  <trd lng="pt">grasnar</trd>
  <trd lng="sk">g&aacute;ga&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_je;&c_l;&c_gu;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_gu;&c_je;&c_l;&c_gu;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_gu;&c_je;&c_r;&c_gu;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_gu;&c_o;&c_gu;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="gak.gik0">
  <kap>gik <tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="gak.gik0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de anaso a&ubreve; simila akvobirdo:
      <ekz>
        la anasoj kriis &leftquot;gik-<tld/>, gik-<tld/>!&rightquot;
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, merkredo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Mi dubas, &ccirc;u tio povas esti konsiderata deriva&jcirc;o
    de radiko "gak". &Ccirc;u ekzistas radiko "gik"?
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_r;&c_a;-&c_k;&c_r;&c_a;!</trd>
  <trdgrp lng="ca">
    <trd>coac-coac</trd>,
    <trd>q&uuml;ec-q&uuml;ec</trd>
  </trdgrp>
  <trd lng="cs">citoslovce hlasu kachny gagaga</trd>
  <trd lng="de">Gak-gak</trd>
  <trd lng="es">graznido</trd>
  <trd lng="fr">coin-coin</trd>
  <trd lng="hu">g&aacute;-g&aacute;</trd>
  <trd lng="pl">kwa-kwa</trd>
  <trd lng="sk">citoslovce hlasu ka&ccaron;ice</trd>
  <trd lng="zh">&#22030; <pr>g&aacute;</pr></trd>
</drv>
</art>

