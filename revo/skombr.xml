<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skombr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="skombr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="skombr1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Scombrus</trd>), ofta bongusta mara fi&scirc;o,
      kies
      verda ha&ubreve;to estas diverskolore makulita.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_u;&c_m;&c_b;&c_r;&c_y;&c_ja;</trd>, 
    <trd>&c_m;&c_a;&c_k;&c_r;&c_e;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_u;&c_m;&c_r;&c_i;&c_ja;</trd>
  <trd lng="de">Makrele</trd>
  <trd lng="en">mackerel</trd>
  <trd lng="es">caballa</trd>
  <trd lng="fr">maquereau</trd>
  <trd lng="hu">makr&eacute;la</trd>
  <trdgrp lng="it">
    <trd>macarello</trd>,
    <trd>sgombro</trd>
  </trdgrp>
  <trd lng="nl">makreel</trd>
  <trd lng="pl">makrela</trd>
  <trdgrp lng="pt">
    <trd>makrela</trd>,
    <trd>cavala</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_u;&c_m;&c_b;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_r;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
