<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>skombr</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="skombr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skombr.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Scombrus</trd>), ofta bongusta mara fi&scirc;o, kies      
      verda ha&ubreve;to estas diverskolore makulita.
    </dif>
  </snc>
  <trd lng="de">Makrele</trd>
  <trd lng="en">mackerel</trd>
  <trd lng="es">caballa</trd>
  <trd lng="fr">maquereau</trd>
  <trdgrp lng="it">
    <trd>macarello</trd>,
    <trd>sgombro</trd>
  </trdgrp>
  <trd lng="nl">makreel</trd>
  <trdgrp lng="pt">
    <trd>makrela</trd>,
    <trd>cavala</trd>
  </trdgrp>
</drv>
<drv mrk="skombr.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="perk.0oformaj">perkoformaj
      fi&scirc;oj</ref> (<trd lng="la">Scombridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Makrelen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
