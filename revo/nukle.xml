<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nukle</rad>/o
</kap>
<drv mrk="nukle.0o">
  <kap><tld/>o</kap>
  <snc mrk="nukle.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      La centra parto de
      <ref tip="malprt" cel="protop.0o">protoplasmo</ref>,
      la &ccirc;elkerno, kiu entenas la
      <ref tip="prt" cel="kromos.0o">kromosomojn</ref>:
      <ekz>
        oni elprenas la blankajn &ccirc;elojn (le&ubreve;kocitojn) kaj analizas la
        kromosomojn en la <tld/>oj de tiuj &ccirc;eloj<fnt><aut>Stevens
        Norvell</aut>: <vrk>&Ccirc;u anka&ubreve; genoj responsas pri melanomo?</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 7a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="cxel.0o.BIO">&ccirc;elo 2</ref>
    <trdgrp lng="de">
      <trd>Kern</trd>,
      <trd>Zellkern</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>noyau<klr> (cellulaire)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sejtmag</trd>,
      <trd>nukleusz</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kern</trd>,
      <trd>nucleus</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_d;&c_r;&c_o;
    <klr>(&c_k;&c_l;&c_je;&c_t;&c_k;&c_i;)</klr></trd>
  </snc>
  <snc mrk="nukle.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="dif" cel="atom.0kerno">Atomkerno</ref>.
    </dif>
    <ref tip="prt" cel="nukleon.0o">nukleono</ref>
    <ref tip="malprt" cel="atom.0o">atomo</ref>
    <trdgrp lng="de">
      <trd>Kern</trd>,
      <trd>Atomkern</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>noyau<klr> (atomique)</klr></trd>
    </trdgrp>
    <trd lng="hu">atommag</trd>
    <trdgrp lng="nl">
      <trd>kern<klr tip="ind"> (atoomkern)</klr></trd>,
      <trd>atoomkern</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_d;&c_r;&c_o;
    <klr>(&c_a;&c_t;&c_o;&c_m;&c_a;)</klr></trd>
  </snc>
  <trd lng="el">&pi;&ypsilon;&rho;&eta_ton;&ny;&alfa;&sigma_fina;</trd>
  <trd lng="es">n&iacute;cleo</trd>
  <trd lng="pt">n&uacute;cleo</trd>
</drv>

<drv mrk="nukle.0a">
  <kap><tld/>a</kap>
  <snc mrk="nukle.0a.BIO">
    <dif>
      Rilata al <ref tip="vid" cel="nukle.0o.BIO">nukleoj <sncref/></ref>.
    </dif>
    <trd lng="de">atomar</trd>
  </snc>
  <snc mrk="nukle.0a.FIZ">
    <dif>
      Rilata al <ref tip="vid" cel="nukle.0o.FIZ">nukleoj <sncref/></ref>,
      al ties uzo por elektroproduktado a&ubreve; armoj:
      <ekz>
        la E&ubreve;ropa Centro de <tld lit="N"/>a Esploro
        CERN<fnt><aut>OH</aut>: <vrk>&Ccirc;u antimaterio en la universo?</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 23a</lok></fnt>;
      </ekz>
      <ekz>
        &Ccirc;irka&ubreve; 20 000 policanoj kaj 4000 manifestaciantoj interbatalis
        mezmarte apud la kutime trankvila urbeto Ahaus, en la
        nord-okcidenta Germanio, okaze de la plej nova provo de la germana
        registaro deponi sian <tld/>an ruba&jcirc;on<fnt><aut>Rudolf
        Fischer</aut>: <vrk>Nia ribelo sukcesis</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 5a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">nuklear</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>nucl&eacute;aire</trd>
  </trdgrp>
</drv>
</art>
