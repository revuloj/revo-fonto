<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nukle</rad>/o
</kap>
<drv mrk="nukle.0o">
  <kap><tld/>o</kap>
  <snc mrk="nukle.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      La centra parto de
      <ref tip="malprt" cel="protop.0o">protoplasmo</ref>,
      la &ccirc;elkerno, kiu entenas la
      <ref tip="prt" cel="kromos.0o">kromosomojn</ref>.
    </dif>
    <ref tip="malprt" cel="cxel.0o.BIO">&ccirc;elo 2</ref>
    <trdgrp lng="de">
      <trd>Kern</trd>,
      <trd>Zellkern</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kern</trd>,
      <trd>nucleus</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_d;&c_r;&c_o; <klr>(&c_k;&c_l;&c_je;&c_t;&c_k;&c_i;)</klr></trd>
  </snc>
  <snc mrk="nukle.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Atomkerno.
    </dif>
    <ref tip="prt" cel="nukleon.0o">nukleono</ref>
    <ref tip="malprt" cel="atom.0o">atomo</ref>
    <trdgrp lng="de">
      <trd>Kern</trd>,
      <trd>Atomkern</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kern<klr tip="ind"> (atoomkern)</klr></trd>,
      <trd>atoomkern</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_d;&c_r;&c_o; <klr>(&c_a;&c_t;&c_o;&c_m;&c_a;)</klr></trd>
  </snc>
</drv>
</art>
