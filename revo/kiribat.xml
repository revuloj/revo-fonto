<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kiribat</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="kiribat.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="ISO-3166">KI</mlg>
  <snc mrk="kiribat.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insulara <ref tip="lst" lst="voko:&scirc;tatoj"
      cel="sxtat.0o.POL">&scirc;tato</ref>
      en <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
      kun &ccirc;efurbo Taravo, Respubliko <tld/>o:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> konsistas el multaj insuloj de Mikronezio
        kaj Polinezio dislokitaj en granda areo norde kaj sude de la
        ekvatoro
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=2288">Kiribato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>Kiribati</trd>,
      <trd>Republika <ind>Kiribati</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_K;&c_i;&c_r;&c_i;&c_b;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>Kiribati</trd>,
    <trd>Rep&uacute;blica de <ind>Kiribati</ind></trd>
  </trdgrp>
  <trd lng="de">Kiribati</trd>
  <trd lng="en">Kiribati</trd>
  <trdgrp lng="fr">
    <trd>Kiribati</trd>,
    <trd>R&eacute;publique des Kiribati</trd>
  </trdgrp>
  <trd lng="id">Kiribati</trd>
  <trd lng="nl">Kiribati</trd>
  <trd lng="pt">Kiribati</trd>
</drv>

</art>

