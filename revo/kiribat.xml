<kap>
  <rad>Kiribat</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="kiribat.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="ISO-3166">KI</mlg>
  <snc mrk="kiribat.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insulara <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&scirc;tato</ref>
      en <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
      kun &ccirc;efurbo Taravo, Respubliko <tld/>o:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> konsistas el multaj insuloj de Mikronezio
        kaj Polinezio dislokitaj en granda areo norde kaj sude de la
        ekvatoro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2288">Kiribato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_ib;&c_r;&c_y;&c_b;&c_a;&c_t;&c_y;</trd>
    <trdgrp lng="pl">
      <trd>Kiribati</trd>,
      <trd>Republika <ind>Kiribati</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_K;&c_i;&c_r;&c_i;&c_b;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>Kiribati</trd>,
    <trd>Rep&uacute;blica de <ind>Kiribati</ind></trd>
  </trdgrp>
  <trd lng="de">Kiribati</trd>
  <trd lng="en">Kiribati</trd>
  <trdgrp lng="fr">
    <trd>Kiribati</trd>,
    <trd>R&eacute;publique des Kiribati</trd>
  </trdgrp>
  <trd lng="id">Kiribati</trd>
  <trd lng="ja">&#x30AD;&#x30EA;&#x30D0;&#x30B9;</trd>
  <trd lng="nl">Kiribati</trd>
  <trd lng="pt">Kiribati</trd>
  <trd lng="uk">&#x41A;&c_ib;&c_r;&c_i;&c_b;&c_a;&c_t;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x57FA;&#x91CC;&#x5DF4;&#x65AF;&#x5171;&#x548C;&#x56FD; <pr>J&#x12B;l&#x1D0;b&#x101;s&#x12B;G&#xF2;ngh&#xE9;gu&#xF3;</pr></trd>,
    <trd>&#x57FA;&#x91CC;&#x5DF4;&#x65AF; <pr>j&#x12B;l&#x1D0;b&#x101;s&#x12B;</pr></trd>,
    <trd>&#x57FA;&#x91CC;&#x5DF4;&#x65AF; <pr>J&#x12B;l&#x1D0;b&#x101;s&#x12B;</pr></trd>,
    <trd>&#x5409;&#x91CC;&#x5DF4;&#x65AF; <pr>J&#xED;l&#x1D0;b&#x101;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

