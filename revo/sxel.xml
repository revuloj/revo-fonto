<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;el</rad>/o
</kap>
<drv mrk="sxel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ekstera krusto de arba trunko.
    </dif>
    <refgrp tip="vid">
      <ref cel="bast.0o">basto</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_r;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Rinde</trd>,
      <trd>Borke</trd>
    </trdgrp>
    <trd lng="fr">&eacute;corce</trd>
    <trd lng="hu">fak&eacute;reg</trd>    
    <trdgrp lng="nl">
      <trd>boomschors</trd>,
      <trd>schors</trd>
    </trdgrp>
    <trd lng="pt">casca</trd>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Ekstera krusteto a&ubreve; membrano teganta la frukton.
    </dif>
    <refgrp tip="sin">
      <ref cel="perika1.0o">perikarpo</ref>,
      <ref cel="gusx.0o">gu&scirc;o</ref>,
      <ref cel="hauxt.0o">ha&ubreve;to</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_l;&c_u;&c_p;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_sh;&c_a;&c_l;&c_u;&c_p;&c_ib;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Schale</trd>
    <trdgrp lng="fr">
      <trd>peau (d'un fruit)</trd>,
      <trd>pelure</trd>
    </trdgrp>
    <trd lng="hu">h&eacute;j</trd>
    <trd lng="nl">schil</trd>
    <trd lng="pt">casca</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_zh;&c_u;&c_r;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_sh;&c_je;&c_l;&c_u;&c_h;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Ekstera kalkeca krusteto entenanta la ovon:
      <ekz>
        li &jcirc;us elrampis el la ova <tld/>o
        <klr>(estas juna kaj sensperta.
        <ref tip="vid" cel="lakt.0o">lakto</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_k;&c_a;&c_r;&c_l;&c_u;&c_p;&c_ib;&c_n;&c_a;</trd>
    <trd lng="de">Schale</trd>
    <trd lng="fr">coquille</trd>
    <trd lng="hu">h&eacute;j</trd>
    <trdgrp lng="nl">
      <trd>eierdop</trd>,
      <trd>eierschaal</trd>,
      <trd>schaal</trd>
    </trdgrp>
    <trd lng="pt">casca</trd>
    <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_l;&c_u;&c_p;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Ekstera korneca a&ubreve; kalkeca kiraso de testudo, kankro,
      omaro ks:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lepro kovris mian tutan korpon per abomeninda
        <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karapa.0o">karapaco</ref>,
      <ref cel="krust.0o">krusto</ref>,
      <ref cel="kiras.0o">kiraso</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_l;&c_u;&c_s;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
    </trdgrp>
    <trd lng="de">Panzer</trd>
    <trd lng="fr">carapace</trd>
    <trd lng="he">&shin;&yod;&resh;&yod;&vav;&fnun;<klr tip="ind"> (&shin;&lamed; &bet;&ayin;"&het;)</klr></trd>
    <trd lng="hu">p&aacute;nc&eacute;l<klr tip="ind"> (&aacute;llati)</klr></trd>    
    <trd lng="nl">huis</trd>
    <trdgrp lng="pt">
      <trd>casca</trd>,
      <trd>crosta</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_ch;&c_je;&c_sh;&c_u;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxel.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Interfaca programo peranta pri atingo al risurco, servilo,
      <ref tip="malprt" cel="operac.0umo">operaciumo</ref> ktp,
      por plia oportuno, rendimento a&ubreve; sekuro;
      precipe, interpretilo transmetanta operaciumajn komandojn
      al la operaciuma kerno.
    </dif>
    <trd lng="he">&mem;&ayin;&tet;&pe;&tav;<klr tip="ind"> (&bet;&mem;&het;&shin;&bet;&yod;&fmem;)</klr></trd>
    <trdgrp lng="hu">
      <trd>h&eacute;jprogram</trd>,
      <trd>keretprogram</trd>
    </trdgrp>
    <trd lng="nl">schil</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">shell</trd>
  <trd lng="he">&qof;&lamed;&yod;&pe;&he;</trd>
</drv>

<drv mrk="sxel.long0o">
  <kap>long<tld/>o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="gusx.0o">gu&scirc;o</ref>.
    </dif>
  </snc>
  <trd lng="hu">h&uuml;vely <klr tip="ind">(n&ouml;v&eacute;nyi)</klr></trd>  
</drv>

<drv mrk="sxel.el0igi">
  <kap>el<tld/>igi</kap>
  <snc>
    <dif>
      Eltiri el la <tld/>o:
      <ekz>
        el<tld/>igi pizojn, ka&scirc;tanojn.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_a;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_u;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_u;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">&eacute;cosser</trd>
    <trdgrp lng="he">
      <trd>&lamed;<ind>&qof;&lamed;&fpe;</ind></trd>,
      <trd>&lamed;&qof;&lamed;&vav;&fpe; \<ind>&qof;&lamed;&fpe;</ind>\</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>h&aacute;moz</trd>,
      <trd>fejt</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pellen</trd>,
      <trd>doppen</trd>
    </trdgrp>
    <trd lng="pt">descascar</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_o;&c_t; &c_k;&c_o;&c_zh;&c_u;&c_r;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>,
      <trd>&c_l;&c_u;&c_shch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Eltrovi kaj klare aperigi:
      <ekz>
        el la reciproke kontra&ubreve;diraj raportoj estas malfacile
        el<tld/>igi la veron.
      </ekz>
    </dif>
    <trd lng="hu">kih&aacute;moz</trd>    
    <trd lng="nl">distilleren<klr tip="ind"> (v.waarheid bijv.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol; &c_o;&c_t; &c_sh;&c_je;&c_l;&c_u;&c_h;&c_i;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="sxel.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      El<tld/>igi 1.
    </dif>
  </snc>
  <trd lng="nl">schillen</trd>
   <trdgrp lng="fr">
     <trd>peler (fruit)</trd>,
     <trd>d&eacute;cortiquer (noix)</trd>,
     <trd>&eacute;corcer (arbre)</trd>,
     <trd>&eacute;plucher (l&eacute;gume)</trd>,
     <trd>&eacute;caler (oeuf dur)</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_o;&c_t; &c_k;&c_o;&c_zh;&c_u;&c_r;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>,
     <trd>&c_l;&c_u;&c_shch;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>

<drv mrk="sxel.sen0igilo">
  <kap>sen<tld/>igilo</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Specialforma tran&ccirc;ilo por sen<tld/>igi:
      <ekz>
        sen<tld/>igilo por legomoj, por ostroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="he">&qof;&vav;&lamed;&pe;&fnun;</trd>
  <trd lng="nl">schilmesje</trd>
</drv>
</art>
