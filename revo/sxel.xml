  </trdgrp>
  <trdgrp lng="fr">
    <trd>peler (fruit)</trd>,
    <trd>d&eacute;cortiquer (noix)</trd>,
    <trd>&eacute;corcer (arbre)</trd>,
    <trd>&eacute;plucher (l&eacute;gume)</trd>,
    <trd>&eacute;caler (oeuf dur)</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>sgusciare<klr> (degusciare)</klr></trd>,
      <trd>sbucciare</trd>, 
      <trd>pelare</trd>,
      <trd>scortecciare</trd>, 
      <trd>scorzare</trd>
    </trdgrp>
  <trd lng="nl">schillen</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_o;&c_t; 
&c_k;&c_o;&c_zh;&c_u;&c_r;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_l;&c_u;&c_shch;&c_i;&c_t;&c_mol;</trd>
