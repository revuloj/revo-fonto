<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>&scirc;el</rad>/o
    </kap>
    <drv mrk="sxel.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Ekstera krusto de arba trunko. 
        </dif>
        <refgrp tip="vid">
          <ref cel="bast.0o">basto</ref>.
        </refgrp>
        <trdgrp lng="nl">
          <trd>boomschors</trd>,
          <trd>schors</trd>
        </trdgrp>
        <trd lng="ru">&c_k;&c_o;&c_r;&c_a;</trd>
      </snc>
      <snc>
        <dif>
          Ekstera krusteto a&ubreve; membrano teganta la frukton. 
        </dif>
        <refgrp tip="sin">
          <ref cel="perika1.0o">perikarpo</ref>,
          <ref cel="gusx.0o">gu&scirc;o</ref>,
          <ref cel="hauxt.0o">ha&ubreve;to</ref>.
        </refgrp>
        <trd lng="nl">schil</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_o;&c_zh;&c_u;&c_r;&c_a;</trd>,
          <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
          <trd>&c_sh;&c_je;&c_l;&c_u;&c_h;&c_a;</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Ekstera kalkeca krusteto entenanta la ovon:
          <ekz>
            li &jcirc;us elrampis el la ova <tld/>o<fnt>Z</fnt>
            <klr>(estas juna kaj sensperta. 
              <ref tip="vid" cel="lakt.0o">lakto</ref>)</klr>.
          </ekz>
        </dif>
        <trdgrp lng="nl">
          <trd>eierdop</trd>,
          <trd>eierschaal</trd>,
          <trd>schaal</trd>
        </trdgrp>
        <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_l;&c_u;&c_p;&c_a;</trd>
      </snc>
      <snc>
        <dif>
          Ekstera korneca a&ubreve; kalkeca kiraso de testudo, kankro,
          omaro ks:
          <ekz>
            (f) lepro kovris mian tutan korpon per abomeninda
            <tld/>o<fnt>Z</fnt>.
          </ekz> 
        </dif>
        <refgrp tip="vid">
          <ref cel="karapa.0o">karapaco</ref>,
          <ref cel="krust.0o">krusto</ref>,
          <ref cel="kiras.0o">kiraso</ref>.
        </refgrp>
        <trd lng="nl">huis</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>,
          <trd>&c_ch;&c_je;&c_sh;&c_u;&c_ja;</trd>
        </trdgrp>
      </snc>
      <snc mrk="sxel.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Interfaca programo peranta pri atingo al risurco, servilo,
          <ref tip="malprt" cel="operac.0umo">operaciumo</ref> ktp,
          por plia oportuno, rendimento a&ubreve; sekuro;
          precipe, interpretilo transmetanta operaciumajn komandojn
          al la operaciuma kerno.
        </dif>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
          <trd>&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>
        </trdgrp>
      </snc>
      <trd lng="en">shell</trd>
    </drv>
    <drv mrk="sxel.long0o">
      <kap>long<tld/>o<fnt>Z</fnt></kap>
      <snc>
        <dif>
          <ref tip="dif" cel="gusx.0o">gu&scirc;o</ref>. 
        </dif>
      </snc>
    </drv>
    <drv mrk="sxel.el0igi">
      <kap>el<tld/>igi</kap>
      <snc>
        <dif>
          Eltiri el la <tld/>o: <ekz>el<tld/>igi pizojn, ka&scirc;tanojn.
          </ekz>
        </dif>
        <trdgrp lng="nl">
          <trd>pellen</trd>,
          <trd>doppen</trd>
        </trdgrp>
        <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_o;&c_t; &c_k;&c_o;&c_zh;&c_u;&c_r;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo><dif>
          Eltrovi kaj klare aperigi:
          <ekz>
            el la reciproke kontra&ubreve;diraj raportoj estas malfacile
            el<tld/>igi la veron.
          </ekz>
        </dif>
      </snc>
    </drv>
    <drv mrk="sxel.sen0igi">
      <kap>sen<tld/>igi</kap>
      <snc>
        <dif>
          El<tld/>igi 1.  
        </dif>
      </snc>
    </drv>
    <drv mrk="sxel.sen0igilo">
      <kap>sen<tld/>igilo</kap>
      <uzo tip="fak">KUI</uzo>
      <snc>
        <dif>
          Specialforma tran&ccirc;ilo por sen<tld/>igi:
          <ekz>sen<tld/>igilo por legomoj, por ostroj
          </ekz>.
        </dif>
      </snc>
    </drv>
  </art>

