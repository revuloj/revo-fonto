      <trd>pelare</trd>,
      <trd>scortecciare</trd>,
      <trd>scorzare</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x76AE;&#12434;&#x306F;&#12368;  <pr>&#x304B;&#x308F;&#12434;&#x306F;&#12368;</pr></trd>,
    <trd>&#x6BBB;&#12434;&#12416;&#x304F;  <pr>&#x304B;&#12425;&#12434;&#12416;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">schillen</trd>
  <trdgrp lng="pl">
    <trd>okorowa&cacute;</trd>,
    <trd>obiera&cacute;</trd>,
    <trd>wy&lstroke;uska&cacute;</trd>,
    <trd>wy&lstroke;uszczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_o;&c_t;
&c_k;&c_o;&c_zh;&c_u;&c_r;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_l;&c_u;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">skala</trd>
