    <trd>&c_a;&c_b;&c_l;&c_u;&c_z;&c_g;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_t;&c_o;&c_w;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_d;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF54;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF0B;&#xF64;&#xF74;&#xF66;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>sch&auml;len</trd>,
