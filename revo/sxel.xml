<drv mrk="sxel.sen0igilo">
  <kap>sen<tld/>igilo</kap>
  <snc mrk="sxel.sen0igilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Specialforma 
      <ref tip="super" cel="trancx.0ilo.KOMUNE">tran&ccirc;ilo</ref> 
      por sen<tld/>igi:
      <ekz>
        sen<tld/>igilo por legomoj;
      </ekz>
      <ekz>
        sen<tld/>igilo estas tran&ccirc;ilo per kiu oni povas sen&scirc;eligi terpomojn, pomojn, kukumojn ktp
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=280276">Sen&scirc;eligilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Sch&auml;lmesser</trd>,
    <trd>Sch&auml;ler</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pelador <klr>(de fruta)</klr></trd>,
    <trd>abridor <klr>(de ostras)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;conome</trd>,
    <trd>&eacute;pluche-l&eacute;gume</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sbucciatore<klr> (coltello)</klr></trd>,
    <trd>pelapatate</trd>, 
    <trd>pelaverdure</trd>
  </trdgrp>
  <trd lng="he">&qof;&vav;&lamed;&pe;&fnun;</trd>
  <trd lng="nl">schilmesje</trd>
  <trdgrp lng="sv">
    <trd>potatisskalare</trd>,
    <trd>skalare</trd>
  </trdgrp>
</drv>

<drv mrk="sxel.long0o">
  <kap>long<tld/>o<fnt>Z</fnt></kap>
  <snc mrk="sxel.long0o.BOT">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="gusx.0o">gu&scirc;o</ref>
    <trd lng="de">H&uuml;lse</trd>
  </snc>
  <trd lng="fr">gousse</trd>
  <trd lng="hu">h&uuml;vely <klr tip="ind">(n&ouml;v&eacute;nyi)</klr></trd>
  <trd lng="it">baccello</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_o;&c_k;</trd>
</drv>

<drv mrk="sxel.sxip0o">
  <kap>&scirc;ip<tld/>o</kap>
  <snc mrk="sxel.sxip0o.MAR">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Ekstera tavolo de <ref tip="malprt" cel="sxip.0o.MAR">&scirc;ipo</ref>,
      kiu izolas &gcirc;in de akvo:
      <ekz>
        la ondoj &jcirc;etis la boaton per forta pu&scirc;o kontra&ubreve; la 
        &scirc;ip<tld/>on
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>tria libro, &ccirc;apitro
        46a</lok></fnt>;
      </ekz>
      <ekz>
        Oni fermas la fendojn inter la tabuloj de &scirc;ip<tld/>o (kareno) a&ubreve; ferdeko per stupo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=331440">Stupo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schiffschale</trd>,
    <trd>Schiffsrumpf</trd>
  </trdgrp>
  <trd lng="fr">coque de navire</trd>
  <trd lng="it">scafo</trd>
  <trd lng="sv">skrov</trd>
</drv>

<drv mrk="sxel.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="sxel.ter0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Solida supera parto de la terglobo,
      &ccirc;irka&ubreve;anta la centran varmegan kernon;
      <ref tip="sin" cel="krust.ter0o.GEOL">terkrusto</ref>:
      <ekz>
        du urboj nomitaj <nom>Cambridge</nom> ku&scirc;as sur la ter<tld/>o
        <fnt>
          <aut>Mozano</aut>,
          <vrk>Tra la Mondo Esperantista</vrk>,
          <lok><url ref="http://www.esperanto.es/jspui/bitstream/11013/1291/1/BelgaSonorilo_067_1907aug10_cambr.pdf">
          La Belga Sonorilo, 1907-08-10 (67)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivera akvo skulptis la akvofalon la&ubreve; frakcizono en la ter<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=116071">Viktoria Akvofalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Erdkruste</trd>,
    <trd>Erdmantel</trd>
  </trdgrp>
  <trd lng="es">corteza terrestre</trd>
  <trdgrp lng="fr">
    <trd>croute terrestre</trd>,
    <trd>&eacute;corce terrestre</trd>
  </trdgrp>
  <trd lng="hu">f&ouml;ldk&eacute;reg</trd>
  <trd lng="nl">aardkorst</trd>
  <trd lng="pl">skorupa <ind>ziemska</ind></trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_r;&c_a;</trd>
</drv>
