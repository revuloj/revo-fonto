    <trd>&#x43E;&c_b;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i; &#x43A;&#x43E;&c_r;&c_u;</trd>,
    <trd>&#x43E;&c_b;&#x43B;&c_u;&#x43F;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
