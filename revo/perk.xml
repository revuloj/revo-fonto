<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>perk</rad>/o
</kap>
<drv mrk="perk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="perked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Perca</trd>), man&gcirc;ebla riverfi&scirc;o:
      <ekz>
        pacienco estas bezonata, &ccirc;ar ofte oni kaptas nenion a&ubreve; nur
        malgrandajn <tld/>ojn<fnt><aut>Saliko</aut>: <vrk>Fi&scirc;kapti
        ekscite</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 10a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="percx.0o">per&ccirc;o</ref>
  </snc>
  <trd lng="de">Flussbarsch</trd>
  <trdgrp lng="fr">
    <trd>perche<klr> (poisson)</klr></trd>
  </trdgrp>
  <trd lng="hu">s&uuml;g&eacute;r</trd>
  <trd lng="nl">baars</trd>
  <trd lng="ru">&c_o;&c_k;&c_u;&c_n;&c_mol;</trd>
</drv>

<drv mrk="perk.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Perciformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Barschartige Fische</trd>
  <trd lng="hu">s&uuml;g&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="ru">&c_o;&c_k;&c_u;&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr
tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
