<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perk.xml,v 1.34 2024/02/17 06:43:06 revo Exp $">
<kap>
  <ofc>*</ofc><rad>perk</rad>/o
</kap>
<drv mrk="perk.0o">
  <kap><tld/>o</kap>
  <snc mrk="perk.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="lst" cel="fisx.river0o" lst="voko:fi&scirc;oj">riverfi&scirc;o</ref> kun dividita
      dorsna&gcirc;ilo, ru&gcirc;etaj
      malsupraj na&gcirc;iloj, ofte
      kun malhelaj vertikalaj strioj sur la korpo;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="perked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Perca</trd>):
      <ekz>
        pacienco estas bezonata, &ccirc;ar ofte oni kaptas nenion
        a&ubreve; nur
        malgrandajn <tld/>ojn<fnt><aut>Saliko</aut>: <vrk>Fi&scirc;kapti
        ekscite</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 10a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="percx.0o">per&ccirc;o</ref>
    <trd lng="bg">&c_k;&c_o;&c_s;&c_t;&c_u;&c_r;<klr tip="amb"> (&c_r;&c_i;&c_b;&c_a;)</klr></trd>   
    <trdgrp lng="br">                    
      <trd>brell<klr tip="ind"> (perch)</klr></trd>
      <trd>perch</trd>
    </trdgrp>
    <trd lng="ca">perca</trd>
    <trd lng="de">Flussbarsch</trd>
    <trd lng="en">perch<klr> (fish)</klr></trd>
    <trd lng="fr">perche<klr> (poisson)</klr></trd>
    <trd lng="hu">s&uuml;g&eacute;r</trd>
    <trd lng="nl">baars</trd>
    <trd lng="pl">oko&nacute;</trd>
    <trd lng="pt">perca</trd>
    <trd lng="ru">&c_o;&c_k;&c_u;&c_n;&c_mol;</trd>
    <trd lng="sv">abborre</trd>
  </snc>
  <snc mrk="perk.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="perked.0oj">perkedoj</ref>.
    </dif>
    <trd lng="de">Barsch</trd>
    <trdgrp lng="zh">
    <trd>&#x9B63; <pr>y&#xFA;</pr></trd>,
    <trd>&#x9B8A; <pr>b&#xF3;</pr></trd>,
    <trd>&#x9C8C; <pr>b&#xF3;</pr></trd>,
    <trd>&#x9C78; <pr>l&#xFA;</pr></trd>,
    <trd>&#x9C88; <pr>l&#xFA;</pr></trd>,
    <trd>&#x9C78;&#x9B5A; <pr>l&#xFA;y&#xFA;</pr></trd>,
    <trd>&#x9C88;&#x9C7C; <pr>l&#xFA;y&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_n;&c_mol;</trd>
  <trd lng="cs">okoun</trd>
  <trdgrp lng="ja">
    <trd>&#x30DA;&#x30EB;&#x30AB;</trd>,
    <trd>&#x30D1;&#x30FC;&#x30C1;</trd>
  </trdgrp>
  <trd lng="sk">ostrie&#x17E;</trd>
  <trd lng="uk">&#x43E;&#x43A;&c_u;&#x43D;&#x44C;</trd>
</drv>

<drv mrk="perk.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de
      <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Perciformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_n;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">perciformes</trd>
  <trd lng="de">Barschartige Fische</trd>
  <trd lng="hu">s&uuml;g&eacute;ralak&uacute;ak</trd>
  <trd lng="pl">okoniokszta&lstroke;tne</trd>
  <trd lng="ru">&c_o;&c_k;&c_u;&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sv">abborrartade fiskar</trd>
  <trd lng="uk">&#x43E;&#x43A;&c_u;&#x43D;&c_je;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x9C78;&#x5F62;&#x76EE; <pr>l&#xFA;x&#xED;ngm&#xF9;</pr></trd>,
    <trd>&#x9C88;&#x5F62;&#x76EE; <pr>l&#xFA;x&#xED;ngm&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
