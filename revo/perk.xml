<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>perk</rad>/o</kap>
<drv mrk="perk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="perk.0edoj"><tld/>edoj</ref> 
      (<trd lng="la">Perca</trd>), man&gcirc;ebla riverfi&scirc;o.  
    </dif>
    <ref tip="vid" cel="percx.0o">per&ccirc;o</ref>
  </snc>
  <trd lng="de">Flu&szlig;barsch</trd>
</drv>
<drv mrk="perk.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="perk.0oformaj">perkoformaj</ref>
      fi&scirc;oj (<trd lng="la">Percidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Barsche</trd>
</drv>
<drv mrk="perk.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso 
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Perciformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Barschartige Fische</trd>
</drv>
</art>
