<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kobaj.xml,v 1.26 2023/09/01 15:46:40 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kobaj</rad>/o
</kap>
<drv mrk="kobaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="kobaj.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">Ron&gcirc;ulo</ref> 
      kun mallongaj kruroj, ronda kapo kaj mallongaj oreloj, 
      anta&ubreve;e kvar, malanta&ubreve;e tri piedfingroj, bredata en 
      landoj &ccirc;e Andoj pro la
      karno,
      cetere tenata kiel <ref cel="kares.0i">kares</ref>besto
      kaj uzata por medicinaj <ref cel="eksper.0o">eksperimentoj</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="kobaje.0oj">kobajedoj</ref>
      (<trd lng="la">Cavia</trd>):
      <ekz>
        sova&gcirc;aj <tld/>oj vivas en la centra parto de Sud-Ameriko, en la
        Andaj montoj, la <ref cel="pamp.0o.GEOG">pampoj</ref>
        <fnt>
        <aut>K. MacGill</aut>,
        <vrk><url ref="http://www.lernu.net/grafikajhoj/ja/pdf/interesa_bestoj.pdf"/>&ccirc;io
        interesa pri kobajoj</vrk>,
        <lok>Juna amiko, 2002-04</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o; &c_s;&c_v;&c_i;&c_n;&c_ch;&c_je;</trd>
  <trd lng="de">Meerschweinchen</trd>
  <trd lng="en">Guinea pig</trd>
  <trdgrp lng="es">
    <trd>cobaya</trd>,
    <trd>conejillo de indias</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cobaye</trd>,
    <trd>cochon d'Inde</trd>
  </trdgrp>
  <trd lng="hu">tengerimalac</trd>
  <trd lng="id"><ind>tikus</ind> belanda</trd>
  <trd lng="nl">Guinees <ind>biggetje</ind></trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja;
  &c_s;&c_v;&c_i;&c_n;&c_k;&c_a;</trd>
  </snc> 
   <snc mrk="kobaj.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="hom.0o">Homo</ref>, &ccirc;e kiu okazas la 
      fina elprovo de nova <ref cel="medika1.0o.MED">medikamento</ref>:
      <ekz>
        &ccirc;iun ajn medikamenton la fabrikantoj devas prove injekcii a&ubreve;
        glutigi &ccirc;e homoj, la <tld/>oj
        <fnt>
        <aut>Will Green</aut>,
        <vrk>&Ccirc;u esti kobajo?</vrk>,
        <bib>Monato</bib>,
        <lok>1994-02, p. 13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Versuchskaninchen</trd>
    <trd lng="en">guinea pig</trd>
    <trd lng="fr">cobaye</trd>
    <trd lng="nl">proefkonijn</trd>
  </snc>
  <trd lng="cs">mor&ccaron;e peru&aacute;nsk&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30EB;&#x30E2;&#x30C3;&#x30C8;</trd>,
    <trd>&#x30C6;&#x30F3;&#x30B8;&#x30AF;&#x30CD;&#x30BA;&#x30DF;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;winka morska</trd>,
    <trd>kawia</trd>
  </trdgrp>
  <trd lng="ro">cobai</trd>
  <trd lng="sk">mor&ccaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_v;&c_ib;&c_ja; (&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_a;)</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_a; &c_s;&c_v;&c_i;&c_n;&c_k;&c_a; (&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_a;)</trd>
  </trdgrp>
</drv>
</art>
