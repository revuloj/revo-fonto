<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kobaj</rad>/o
</kap>
<drv mrk="kobaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="kobaj.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="kobaje.0oj">kobajedoj</ref>
      (<trd lng="la">Cavia</trd>), kun mallongaj kruroj, ronda kapo kaj
      mallongaj oreloj, tenata kiel karesbesto kaj uzata por medicinaj
      eksperimentoj:
      <ekz>
        sova&gcirc;aj <tld/>oj vivas en la centra parto de Sud-Ameriko, en la
        Andaj montoj, la pampoj
        <fnt>
        <aut>K. MacGill</aut>,
        <vrk><url ref="http://www.lernu.net/grafikajhoj/ja/pdf/interesa_bestoj.pdf"></url>&ccirc;io interesa pri kobajoj</vrk>,
        <lok>Juna amiko, 2002-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Meerschweinchen</trd>
  <trd lng="en">Guinea pig</trd>
  <trdgrp lng="fr">
    <trd>cobaye</trd>,
    <trd>cochon d'Inde</trd>
  </trdgrp>
  <trd lng="hu">tengerimalac</trd>
  <trd lng="nl">Guinees <ind>biggetje</ind></trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja;
  &c_s;&c_v;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
