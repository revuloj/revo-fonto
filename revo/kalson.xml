<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kalson</rad>/o
</kap>
<drv mrk="kalson.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Tola a&ubreve; kalikota pantaloneto, portata kiel <ref tip="super" cel="vest.sub0o">subvesto</ref> sub pantalono a&ubreve; jupo. 
    </dif>
  </snc>
  <trd lng="de">Unterhose</trd>
  <trdgrp lng="en">
    <trd>bathing suit</trd>,
    <trd>swimsuit</trd>
  </trdgrp>
  <trdgrp lng="fr">
   <trd>slip</trd>,
   <trd>cale&ccedil;on</trd>
  </trdgrp>
  <trd lng="nl">onderbroek</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_s;&c_o;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_t;&c_a;&c_l;&c_o;&c_n;&c_y; <klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_sh;&c_t;&c_a;&c_n;&c_n;&c_i;&c_k;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_y;</trd>
  </trdgrp>
</drv>
<drv mrk="kalson.ban0o">
  <kap>ban<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Pli mallonga <tld/>o, uzata en kelkaj landoj por sin bani.  
    </dif>
  </snc>
  <trd lng="de">Badehose</trd>
  <trdgrp lng="fr">
   <trd>maillot de bain</trd>
   <trd>cale&ccedil;on de bain</trd>,
  </trdgrp>
  <trd lng="nl">zwembroek</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_v;&c_k;&c_i;</trd>
</drv>
</art>
