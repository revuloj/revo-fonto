<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ojstr</rad>/o
</kap>
<drv mrk="ojstr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ojstr.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Oestrus</trd>), kies larvoj
      parazitas en la kapo de &scirc;afoj.
    </dif>
  </snc>
  <trd lng="de">Schafbremse</trd>
</drv>
<drv mrk="ojstr.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Oestridae</trd>), grandaj haraj insektoj, kies larvoj
      parazitas en hufuloj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dasselfliegen</trd>,
    <trd>Biesfliegen</trd>
  </trdgrp>
</drv>
</art>
