<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: denotaci.xml,v 1.4 2023/11/08 09:26:38 revo Exp $">
<kap>
  <rad>denotaci</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="denotaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="denotaci.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="senc.cxef0o.LIN">&Ccirc;efsenco</ref>:
      <ekz>
        fidinda traduko respegulas ne nur la <tld/>ojn
        de la fontotekstaj vortoj, sed anka&ubreve; la konotaciojn
        <fnt>
          <aut>R. Haveman</aut>
          <vrk><url ref="http://www.tekstoj.nl/esperanto/artikoloj/terminaro.htm">
          Terminareto pri tradukado</url></vrk>
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y; <ind>&c_s;&c_e;&c_n;&c_s;</ind></trd>,
    <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_a;&c_je; <ind>&c_z;&c_n;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="pl">denotacja</trd>
  <trd lng="uk">&c_d;&c_je;&#x43D;&#x43E;&c_t;&c_a;&c_c;&c_ib;&#x44F;</trd>
</drv>
</art>

