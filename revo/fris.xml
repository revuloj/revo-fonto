<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fris.xml,v 1.21 2022/04/27 17:19:11 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fris</rad>/o
</kap>
<drv mrk="fris.0o">
  <kap><tld/>o</kap>
  <snc mrk="fris.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Horizontala ornama <ref tip="super" cel="stri.0o">strio</ref> &ccirc;e 
      la <ref cel="supr.0o">supro</ref> de objekto.
    </dif>
    <subsnc mrk="fris.0o.ARKI">
      <uzo tip="fak">ARKI</uzo>
      <dif>
        Arkitektura ornama&jcirc;o inter la <ref cel="arkitr.0o">arkitravo</ref> kaj la <ref cel="kornic.0o.ARKI">kornico</ref>:
        <ekz>
          la glataj, poluritaj kolonoj kun ri&ccirc;e ornamitaj kapiteloj
          kaj altaj <tld/>oj estis el kalk&scirc;tono
          <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 4a, Ijob akceptas
          fremdlandajn vizitantojn</lok></fnt>.
        </ekz>
      </dif>
      <bld lok="&WCU;/thumb/1/1b/Temple_hephaistion_Agora-Athens.jpg/512px-Temple_hephaistion_Agora-Athens.jpg" prm="CC-BY-SA-3.0"><tld/>o &ccirc;e la templo de &Eta_densa;&phi;&alfa;&jota;&sigma;&tau;&jota_ton;&omega;&ny;, antikva agoro de Ateno
          <fnt>
            <aut>Ifernyen, prilaborita de ulrichstill</aut>
            <lok><url ref="&WCW;/File:Temple_hephaistion_Agora-Athens.jpg">&WCW;/File:Temple_hephaistion_Agora-Athens.jpg</url></lok>
          </fnt>
      </bld>
      <trd lng="de">Fries</trd>
      <trdgrp lng="hu">
        <trd>fr&iacute;z<klr tip="amb"> (p&aacute;rk&aacute;nymez&odblac;)</klr></trd>,
        <trd>k&eacute;psz&eacute;k</trd>
      </trdgrp>
      <trd lng="pl">fryz</trd>
      <trd lng="pt">friso</trd>
    </subsnc>
    <subsnc mrk="fris.0o.MEB">
      <dif>
        Ornama aran&gcirc;a&jcirc;o &ccirc;irka&ubreve; vazo, a&ubreve; supre de pordo, kameno; <ref tip="sin" cel="listel.0o">listelo</ref>:
        <ekz>
          gisa&jcirc;oj havis pli malaltan valoron, iusabate ili alvenis trenante malnovan
          kamen<tld/>on
          <fnt>
            <bib>VRA</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">Zierleiste</trd>
      <trd lng="hu">fr&iacute;z</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="feston.0o">festono</ref>,
      <ref cel="lambrekin.0o">lambrekino</ref>,
      <ref cel="modlur.0o">modluro</ref>,
      <ref cel="cikl.0o.peri">ciklo</ref>
    </refgrp>    
    <trd lng="nl">fries</trd>
    <trd lng="pl">fryz</trd>
    <trd lng="pt">sanefa</trd>
  </snc>
  <snc mrk="fris.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Pentritaj strioj pendantaj &ccirc;e la supro de la scenejo por
      reprezenti plafonon a&ubreve; &ccirc;ielon
    </dif>
    <refgrp tip="vid">
      <ref cel="dekora.0o">dekoracio</ref>,
      <ref cel="kulis.0o">kuliso</ref>
    </refgrp>
    <trd lng="de">B&uuml;hnenhimmel</trd>
    <trd lng="hu">&eacute;g<klr tip="amb"> (sz&iacute;nh&aacute;zi
    d&iacute;szlet)</klr></trd>
    <trd lng="pt">bambolina</trd>
  </snc>
  <refgrp tip="hom">
    <ref cel="fris1.0o">friso <klr>(homo)</klr></ref>,
    <ref cel="fris2.0o">friso <klr>(drapo)</klr></ref>
  </refgrp>
  <trd lng="be">&c_f;&c_r;&c_y;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>obrubnice, malovan&eacute; prou&zcaron;ky p&rcaron;edstavuj&iacute;c&iacute; na sc&eacute;n&ecaron; strop nebo nebe</trd>,
    <trd>vlys</trd>
  </trdgrp>
  <trd lng="en">frieze</trd>
  <trd lng="fr">frise</trd>
  <trd lng="pl">kulisy</trd>
  <trd lng="ru">&c_f;&c_r;&c_i;&c_z;</trd>
  <trd lng="sk">vlys</trd>
  <trd lng="uk">&#x444;&#x440;&#x438;&#x437;</trd>
</drv>
</art>
