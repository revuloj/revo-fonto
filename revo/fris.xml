<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>fris</rad>/o
</kap>
<drv mrk="fris.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Arkitektura ornama&jcirc;o inter la arkitravo kaj la kornico. 
    </dif>
    <trd lng="de">Fries</trd>
    <trd lng="fr">frise</trd>
    <trdgrp lng="hu">
      <trd>fr&iacute;z<klr tip="amb">
(p&aacute;rk&aacute;nymez&odblac;)</klr></trd>,
      <trd>k&eacute;psz&eacute;k</trd></trdgrp>
    <trd lng="nl">fries</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ornama aran&gcirc;o &ccirc;irka&ubreve; vazo, a&ubreve; supre de
pordo, kameno. 
    </dif>
    <trd lng="de">Fries</trd>
    <trd lng="fr">frise</trd>
    <trd lng="hu">fr&iacute;z</trd>
    <trd lng="nl">fries</trd>
  </snc>
  <snc num="3">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Pentritaj strioj pendantaj &ccirc;e la supro de la scenejo por
      reprezenti plafonon a&ubreve; &ccirc;ielon. 
    </dif>
    <refgrp tip="vid">
      <ref cel="dekora.0o">dekoracio</ref>,
      <ref cel="kulis.0o">kuliso</ref>.
    </refgrp>
    <trd lng="de">B&uuml;hnenhimmel</trd>
    <trd lng="hu">&eacute;g<klr tip="amb"> (sz&iacute;nh&aacute;zi
d&iacute;szlet)</klr></trd>
  </snc>
  <ref tip="hom" cel="fris1.0o">friso</ref>
  <trd lng="be">&c_f;&c_r;&c_y;&c_z;</trd>
  <trd lng="ru">&c_f;&c_r;&c_i;&c_z;</trd>
</drv>
</art>
