<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fris.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fris</rad>/o
</kap>
<drv mrk="fris.0o">
  <kap><tld/>o</kap>
  <snc mrk="fris.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Horizontala ornama strio &ccirc;e la supro de objekto.
    </dif>
    <subsnc mrk="fris.0o.ARKI">
      <uzo tip="fak">ARKI</uzo>
      <dif>
        Arkitektura ornama&jcirc;o inter la arkitravo kaj la kornico:
        <ekz>
          la glataj, poluritaj kolonoj kun ri&ccirc;e ornamitaj kapiteloj
kaj
          altaj <tld/>oj estis el kalk&scirc;tono
          <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 4a, Ijob akceptas
          fremdlandajn vizitantojn</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="hu">
        <trd>fr&iacute;z<klr tip="amb">
(p&aacute;rk&aacute;nymez&odblac;)</klr></trd>,
        <trd>k&eacute;psz&eacute;k</trd>
      </trdgrp>
 <trd lng="pl">fryz</trd>
  <trd lng="pt">friso</trd>
    </subsnc>
    <subsnc mrk="fris.0o.MEB">
      <dif>
        Ornama aran&gcirc;a&jcirc;o &ccirc;irka&ubreve; vazo, a&ubreve;
supre de
        pordo, kameno.
      </dif>
      <trd lng="hu">fr&iacute;z</trd>
    </subsnc>
    <trd lng="de">Fries</trd>
    <trd lng="nl">fries</trd>
 <trd lng="pl">fryz</trd>
  <trd lng="pt">sanefa</trd>
  </snc>
  <snc mrk="fris.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Pentritaj strioj pendantaj &ccirc;e la supro de la scenejo por
      reprezenti plafonon a&ubreve; &ccirc;ielon.
    </dif>
    <refgrp tip="vid">
      <ref cel="dekora.0o">dekoracio</ref>,
      <ref cel="kulis.0o">kuliso</ref>.
    </refgrp>
    <trd lng="de">B&uuml;hnenhimmel</trd>
    <trd lng="hu">&eacute;g<klr tip="amb"> (sz&iacute;nh&aacute;zi
    d&iacute;szlet)</klr></trd>
    <trd lng="pt">bambolina</trd>
  </snc>
  <ref tip="hom" cel="fris1.0o">friso</ref>
  <trd lng="be">&c_f;&c_r;&c_y;&c_z;</trd>
  <trd lng="en">frieze</trd>
  <trd lng="fr">frise</trd>
  <trd lng="pl">kulisy</trd>
  <trd lng="ru">&c_f;&c_r;&c_i;&c_z;</trd>
  <trd lng="sk">vlys</trd>
  <trdgrp lng="cs">
    <trd>obrubnice ,, malované proužky představující na scéně strop nebo nebe</trd>,
    <trd>vlys</trd>
  </trdgrp>
</drv>
</art>
