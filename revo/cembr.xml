<art mrk="$Id$">
<kap>
<rad>cembr</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="cembr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pin.0o">pino</ref>
      (<trd lng="la">Pinus cembra</trd>), kvinpingla arbo kun
      nukssimilaj migdale gustantaj fruktoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zirbelkiefer</trd>,
    <trd>Arve</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arol</trd>,
    <trd>arolle</trd>,
    <trd>pin cembro</trd></trdgrp> 
  <trd lng="ru">&c_k;&c_je;&c_d;&c_r;
  &c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
