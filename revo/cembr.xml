<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>cembr</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="cembr.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">BOT</uzo>
      <snc mrk="cembr.0o.supernocio">
        <dif>
          Iu el kelkaj specioj de <ref tip="super" cel="pin.0o">pino</ref>,
          konifero kun kvinopaj pinglofaskoj kaj man&gcirc;eblaj
          nukssimilaj semoj (cembraj nuksoj).
        </dif>
        <trd lng="ru">&c_k;&c_je;&c_d;&c_r;&c_o;&c_v;&c_a;&c_ja;
&c_s;&c_o;&c_s;&c_n;&c_a;</trd>
        <refgrp tip="sub">
          <ref cel="cembr.0oeuxropa"><tld/>o e&ubreve;ropa</ref>,
          <ref cel="cembr.0osiberia"><tld/>o siberia</ref>
        </refgrp>
        <ref tip="vid" cel="pini.0o">pinio</ref>
      </snc>
      <snc mrk="cembr.0o.euxropa">
        <ref tip="dif" cel="cembr.0oeuxropa"><tld/>o e&ubreve;ropa</ref>
      </snc>
      <trdgrp lng="de">
        <trd>Zirbelkiefer</trd>,
        <trd>Arve</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>arol</trd>,
        <trd>arolle</trd>,
        <trd>pin cembro</trd>
      </trdgrp> 
      <trd lng="hu">cirbolyafeny&odblac;</trd>
    </drv>

    <drv mrk="cembr.0oeuxropa">
      <kap><tld/>o e&ubreve;ropa</kap>
      <uzo tip="fak">BOT</uzo>
      <snc>
        <dif>
          <tld lit="C"/>o, la specio de <ref tip="super"
            cel="pin.0o">pino</ref>
          (<trd lng="la">Pinus cembra</trd>) kreskanta en la centra
          kaj okcidenta E&ubreve;ropo.
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_je;&c_d;&c_r;
&c_e;&c_w;&c_r;&c_a;&c_p;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_je;&c_d;&c_r;
&c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_s;&c_o;&c_s;&c_n;&c_a;
&c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_a;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="cembr.0osiberia">
      <kap><tld/>o siberia</kap>
      <uzo tip="fak">BOT</uzo>
      <snc>
        <dif>
          <tld lit="C"/>o, la specio de
          <ref tip="super" cel="pin.0o">pino</ref>
          (<trd lng="la">Pinus sibirica</trd>) ofta
          tra la tuta Siberio.
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_je;&c_d;&c_r;
&c_s;&c_i;&c_b;&c_i;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>

    <drv mrk="cembr.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="fak">BOT</uzo>
      <snc>
        <dif>
          Parto de la tajgo a&ubreve; arbaro kie kreskas <tld/>oj.
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_je;&c_d;&c_r;&c_a;&c_ch;</trd>
    </drv>

  </art>
