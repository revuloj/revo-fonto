<art mrk="$Id: skarab1.xml,v 1.16 2025/04/06 03:09:26 revo Exp $">
<kap>
  <rad>skarabed</rad>/oj
</kap>
<drv mrk="skarab1.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Scarabaeidae</trd>):
      <ekz>
        la <ref tip="prt" cel="cetoni.0o">cetonio</ref>
        estas genro de insektoj el la ordo de koleopteroj en la familio de la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=286854">Cetonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_v;&c_u;&c_s;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_o;&c_r;&c_o;&c_g;&c_i; <ind>&c_b;&c_r;&c_malmol;&c_m;&c_b;&c_a;&c_r;&c_i;</ind></trd>
  <trd lng="ca">escarabeids</trd>
  <trd lng="de"> Blatthornk&auml;fer</trd>
  <trdgrp lng="en">
    <trd>Scarabaeidae</trd>,
    <trd><ind>scarab</ind> family</trd>,
    <trd><ind>scarab</ind> beetle family</trd>
  </trdgrp>
  <trd lng="fr">Scarab&eacute;id&eacute;s</trd>
  <trd lng="hu">lemezescs&aacute;p&uacute;ak<klr tip="amb"> (csal&aacute;d)</klr></trd>
  <trd lng="ja">&#12467;&#12460;&#12493;&#12512;&#12471;&#31185;
  <pr>&#12467;&#12460;&#12493;&#12512;&#12471;&#12363;</pr></trd>
  <trdgrp lng="nl">
    <trd>bladsprietkevers<klr tip="ind"> (fam.)</klr></trd>,
    <trd>bladsprietigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>po&sacute;wi&eogonek;tnikowate</trd>,
    <trd>&zdot;ukowate</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_u;&c_s;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_v;&c_u;&c_s;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#37329;&#40860;&#23376; <pr>j&imacron;ngu&imacron;z&#464;</pr></trd>,
    <trd>&#37329;&#40863;&#23376; <pr>j&imacron;ngu&imacron;z&#464;</pr></trd>
  </trdgrp>
</drv>
</art>
