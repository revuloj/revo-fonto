<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blekn.xml,v 1.2 2020/10/09 06:35:12 revo Exp $">
<kap>
    <rad>blekn</rad>/o
</kap>
<drv mrk="blekn.0o">
  <kap><tld/>o</kap>
  <snc mrk="blekn.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
       <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
       de <ref tip="vid" cel="filik.0oplantoj">filikoplantoj</ref> (<trd lng="la">Blechnum</trd>) kun ripecaj folioj:
      <ekz>
        la genro de filikoj <tld/>o apartenas al la familio <tld/>acoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/wiki/Blekno">Blekno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
      <tld/>o-filiko
        <fnt>
          <aut>Bernhard Eichkorn</aut>,
          <vrk>Plant-vortaro REF</vrk>,
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1c/Illustration_Blechnum_spicant0.jpg/446px-Illustration_Blechnum_spicant0.jpg" prm="PD"
      >botanika tabulo de <tld/>o (Blechnum spicant)
        <fnt>
          <lok><url ref="&WCW;/File:Illustration_Blechnum_spicant0.jpg"
          >https://commons.wikimedia.org/wiki/File:Illustration_Blechnum_spicant0.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Rippenfarn</trd>
  <trd lng="en">Blechnum</trd>
  <trd lng="fr">Blechnon</trd>
<trd lng="ru">&c_D;&c_je;&c_r;&c_b;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>
</art>

