<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kupul.xml,v 1.15 2024/12/11 08:35:39 revo Exp $">
<kap>
  <rad>kupul</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kupul.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="kupul.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Malgranda folieca a&ubreve; skvameca kaliko,
      kiu formas la bazon de kelkaj fruktoj,
      ekz. <ref tip="super" cel="glan.0o.BOT">glano<sncref/></ref>:
      <ekz>
        karpeno: <klr>[&#8230;]</klr> la frukto estas glano kun triparta
        flugila <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=41110">Karpeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="brakte.0o">brakteo</ref>
  </snc>
  <trdgrp lng="de">
     <trd>Becher <klr>(Frucht)</klr></trd>,
     <trd>Fruchtbecher</trd>,
     <trd>Cupula</trd>
  </trdgrp>
  <trd lng="fr">cupule</trd>
  <trd lng="hu">kupacs</trd>
  <trdgrp lng="ja">
    <trd>&#27579;&#26007;  <pr>&#12363;&#12367;&#12392;</pr></trd>,
    <trd>&#12408;&#12383;</trd>
  </trdgrp>
  <trd lng="nl">dop<klr tip="ind"> (van eikels enz.)</klr></trd>
  <trdgrp lng="pl">
    <trd>kupula</trd>,
    <trd>miseczka <klr>(owocu)</klr></trd>
  </trdgrp>
  <trd lng="ro">calybium &scomma;i cupule</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_ju;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_sh;&c_je;&c_ch;&c_k;&c_a;
<klr>(&c_zh;&c_jo;&c_l;&c_u;&c_d;&c_ja; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_s;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_n;&c_ib;&c_z;&c_d;&c_je;&c_ch;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_sh;&c_je;&c_ch;&c_k;&c_a; ( &c_zh;&c_o;&c_l;&c_u;&c_d;&c_ja;)</trd>
  </trdgrp>
</drv>
</art>
