        <trd>Shinobi</trd>,
        <trd>Ninja</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>shinobi</trd>, 
        <trd>ninja</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_i;&c_n;&c_d;&c_z;&c_ja;</trd>
    </drv>
  </art>

