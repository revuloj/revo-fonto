<vortaro>
<art mrk="$Id: novosib.xml,v 1.15 2024/01/29 15:59:31 revo Exp $">
<kap>
  <rad>Novosibirsk</rad>/o <fnt><bib>BoER</bib></fnt>
</kap>
<drv mrk="novosib.0o">
  <kap><tld/>o</kap>
  <snc mrk="novosib.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      de <ref tip="malprt" cel="siberi.S0o">Siberio</ref>, kaj la
      tria plej granda urbo de Ruslando (55&#x2DA;04'N, 82&#x2DA;93'E):
      <ekz>
        la ekskursantoj vizitos <tld/>on, la faman Akademian urbeton,
        muzeon de Suno kaj unikan infanan kinostudion
        <fnt>
        <lok>
          <url ref="http://www.chinareport.com.cn/04e/2003-1.htm">
            Al EoLA-15 invitas REU, REJM kaj la Novosibirska E-klubo
          </url>
        </lok>
        </fnt>;
      </ekz>
      <ekz>
        mi lo&gcirc;as en <tld/>o, en la centro de la kontinento, mezvoje
        inter Moskvo kaj Pekino: 2810 km de Moskvo; 2975 km &gcirc;is Pekino
        <fnt>
        <aut>S. Pokrovskij</aut>, <vrk>La granda stepo</vrk>.
        <bib>LOdE</bib>,
        <lok><url ref="&LOdE;Ondo/100-lode.htm#100-25">
        2003, num. 2 (100).</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la areo, observata per la sputniko, trovi&gcirc;as spaco
        de <tld/>o &gcirc;is Madrido
        <fnt>
        <lok><url ref="http://universitato.narod.ru/novajxoj/novajxoj_01.htm">Fiero de la universitato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_o;&c_v;&c_a;&c_s;&c_ib;&c_b;&c_ib;&c_r;&c_s;&c_k;</trd>
  <trd lng="bg">&c_N;&c_o;&c_v;&c_o;&c_s;&c_i;&c_b;&c_i;&c_r;&c_s;&c_k;</trd>
  <trd lng="de">Nowosibirsk</trd>
  <trd lng="en">Novosibirsk</trd>
  <trd lng="fa">&a_n;&a_w;&a_w;&a_w;&a_s;&f_y;&a_b;&f_y;&a_r;&a_s;&f_k;</trd>
  <trd lng="fr">Novosibirsk</trd>
  <trd lng="ja">&#x30CE;&#x30DC;&#x30B7;&#x30D3;&#x30EB;&#x30B9;&#x30AF;</trd>
  <trd lng="nl">Novosibirsk</trd>
  <trd lng="pl">Nowosybirsk</trd>
  <trd lng="ro">Novosibirsk</trd>
  <trd lng="ru">&c_N;&c_o;&c_v;&c_o;&c_s;&c_i;&c_b;&c_i;&c_r;&c_s;&c_k;</trd>
  <trd lng="uk">&#x41D;&#x43E;&#x432;&#x43E;&#x441;&#x438;&#x431;&#x456;&#x440;&#x441;&#x44C;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x65B0;&#x897F;&#x4F2F;&#x5229;&#x4E9A;&#x5E02; <pr>X&#x12B;nx&#x12B;b&#xF3;l&#xEC;y&#xE0;sh&#xEC;</pr></trd>,
    <trd>&#x65B0;&#x897F;&#x4F2F;&#x5229;&#x4E9E;&#x5E02; <pr>X&#x12B;nx&#x12B;b&#xF3;l&#xEC;y&#xE0;sh&#xEC;</pr></trd>,
    <trd>&#x65B0;&#x897F;&#x4F2F;&#x5229;&#x4E9A; <pr>X&#x12B;nx&#x12B;b&#xF3;l&#xEC;y&#xE0;</pr></trd>,
    <trd>&#x65B0;&#x897F;&#x4F2F;&#x5229;&#x4E9E; <pr>X&#x12B;nx&#x12B;b&#xF3;l&#xEC;y&#xE0;</pr></trd>
  </trdgrp>
</drv>
