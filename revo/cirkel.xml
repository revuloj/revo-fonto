<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cirkel.xml,v 1.36 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cirkel</rad>/o
</kap>
<drv mrk="cirkel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="il.0o">Ilo</ref>
      kun du moveblaj, &ccirc;arnire kunigitaj brakoj, uzata por
      desegni rondojn, a&ubreve; mezuri liniojn:
      <ekz>
        alia hakas lignon, mezuras &gcirc;in per &scirc;nuro, faras konturojn per
        grifelo, rabotas per rabotilo, &ccirc;irka&ubreve;signas per <tld/>o, kaj faras el &gcirc;i bildon de homo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 44:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la vicoj, &ccirc;iun kelkdekon da pa&scirc;oj, staris la majstroj: &ccirc;iu havis
        en la mano dikan bastonon a&ubreve; lignan <tld/>on, a&ubreve; ortilon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Carl Friedrich Gauss</nom> <klr>[&#x2026;]</klr>
        montris ke regula plurlatero povas esti konstruita pere de rektilo kaj 
        <tld/>o se kaj nur se la nombro de lateroj estas la produto de diferencaj Fermat-primoj kaj
        potenco de 2
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28739">Carl Friedrich Gauss</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_k;&c_u;&c_l;&c_mol;</trd>
  <trd lng="br">kelc'hier</trd>
  <trd lng="ca">comp&agrave;s</trd>
  <trdgrp lng="cs">
    <trd>kru&zcaron;idlo</trd>,
    <trd>kru&zcaron;&iacute;tko</trd>
  </trdgrp>
  <trd lng="de">Zirkel</trd>
  <trd lng="en">(pair of) <ind>compasses</ind></trd>
  <trd lng="es">comp&aacute;s</trd>
  <trd lng="fr">compas<klr tip="ind"> (pour dessiner)</klr></trd>
  <trd lng="hu">k&ouml;rz&odblac;</trd>
  <trd lng="it">compasso</trd>
  <trd lng="nl">passer</trd>
  <trd lng="pl">cyrkiel</trd>
  <trd lng="pt">compasso</trd>
  <trd lng="ro">compas</trd>
  <trd lng="ru">&c_c;&c_i;&c_r;&c_k;&c_u;&c_l;&c_mol;</trd>
  <trd lng="sk">kru&zcaron;idlo</trd>
  <trd lng="sv">passare</trd>
  <trd lng="zh">&#x5706;&#x89C4; <pr>yu&#xE1;ngu&#x12B;</pr></trd>
</drv>

<drv mrk="cirkel.C0o">
  <kap><tld lit="C"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
    <mlg>Cir</mlg>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Circinus</trd>):
      <ekz>
        NGC 5288 estas malfermita stelamaso en konstelacio <tld lit="C"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=118313">NGC 5288</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_C;&c_y;&c_r;&c_k;&c_u;&c_l;&c_mol; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="ca">el <ind>Comp&agrave;s</ind></trd>
  <trdgrp lng="cs">
    <trd>kru&zcaron;idlo</trd>,
    <trd>kru&zcaron;&iacute;tko</trd>
  </trdgrp>
  <trd lng="de">Zirkel</trd>
  <trd lng="en">Circinus</trd>
  <trd lng="es">Circinus</trd>
  <trd lng="fr">Compas<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&#x5DE;&#x5D7;&#x5D5;&#x5D2;&#x5D4;</trd>
  <trd lng="it">Compasso</trd>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30D1;&#x30B9;</trd>
  <trd lng="nl">Passer<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Cyrkiel  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Compasul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="sk">kru&zcaron;idlo</trd>
  <trd lng="sv">Cirkelpassaren</trd>
  <trd lng="uk">&c_c;&c_i;&c_r;&#x43A;&c_u;&#x43B;&#x44C;</trd>
</drv>

</art>
