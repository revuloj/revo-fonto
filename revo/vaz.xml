<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vaz</rad>/o
</kap>
<drv mrk="vaz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vaz.0o.VAZ">
    <dif>
      Ujo el diversaj materioj, de diversa formo kaj grando, destinita
      por enteni &ccirc;iajn fluecajn a&ubreve; solidajn substancojn:
      <ekz>
        &scirc;i prenis la plej belan ar&gcirc;entan <tld/>on, kiu estis
        en la lo&gcirc;ejo 
        <fnt><bib>F</bib>,
        <lok>&FE; 19, la feino (da&ubreve;rigo)</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;erpi akvon per <tld/>o;
      </ekz>
      <ekz>
        meti florojn en <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Vase</trd>
  <trd lng="el">&beta;&alfa_ton;&zeta;&omikron;</trd>
  <trd lng="en">vase</trd>
  <trd lng="fr">r&eacute;cipient</trd>
  <trdgrp lng="hu">
    <trd>ed&eacute;ny</trd>,
    <trd>v&aacute;za</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vaas</trd>,
    <trd>pot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>naczynie</trd>,
    <trd>waza</trd>
  </trdgrp>
  <trd lng="pt">vaso</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_u;&c_d;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_a;</trd> &c_i; &c_t;.&c_d;.
  </trdgrp>
</drv>

<drv mrk="vaz.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="vaz.0aro.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sortimento de vazoj, precipe tia uzata por man&gcirc;o:
      <ekz>
        <ind>lavi la <tld/>aron</ind>
        <trd lng="fr">faire la vaiselle</trd>
        <trd lng="hu">elmossa az ed&eacute;nyt</trd>
        <trdgrp lng="nl">
          <trd>afwassen</trd>,
          <trd>de <ind>vaat</ind> doen</trd>
        </trdgrp>
      </ekz>
      <ekz>
        en la dometo estis <klr>...</klr> kuirejo kaj provizejo,
        <tld/>aro kupra kaj stana, &ccirc;io pura, brilanta
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>service de table</trd>,
    <trd>vaisselle</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ed&eacute;nyek</trd>,
    <trd>(asztali) <ind>k&eacute;szlet</ind></trd>,
    <trd>ed&eacute;nygarnit&uacute;ra</trd>
  </trdgrp>
  <trd lng="nl">vaat</trd>
  <trdgrp lng="pl">
    <trd>zastawa</trd>,
    <trd>garnitur</trd>,
    <trd>komplet<klr> (np. obiadowy)</klr></trd>,
    <trd>naczynia</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_u;&c_d;&c_a;</trd>
</drv>

<drv mrk="vaz.0lavilo">
  <kap><tld/>lavilo, <var><kap><tld/>lavma&scirc;ino</kap></var></kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="vaz.0lavilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ma&scirc;ino por lavi la vazojn kaj aliajn man&gcirc;ilojn post
      man&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Geschirrsp&uuml;lmaschine</trd>
  <trd lng="en">dishwasher</trd>
  <trd lng="fr">lave-vaisselle</trd>
  <trdgrp lng="hu">
    <trd>ed&eacute;nymosogat&oacute;</trd>,
    <trd>mosogat&oacute;g&eacute;p</trd>
  </trdgrp>
  <trd lng="nl">vaatwasmachine</trd>
  <trd lng="pl">zmywarka<klr> do naczy&nacute;</klr></trd>
  <trd lng="pt">m&aacute;quina de lavar lou&ccedil;a</trd>
  <trd lng="ru">
  &c_p;&c_o;&c_s;&c_u;&c_d;&c_o;&c_m;&c_o;&c_je;&c_ch;&c_n;&c_a;&c_ja;
  &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;
  </trd>
</drv>

<drv mrk="vaz.fand0o">
  <kap>fand<tld/>o</kap>
  <snc mrk="vaz.fand0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="krisol.0o">krisolo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Schmelztiegel</trd>
  <trd lng="en">crucible</trd>
  <trd lng="fr">creuset</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nt&odblac;&uuml;st</trd>,
    <trd>olvaszt&oacute;t&eacute;gely</trd>
  </trdgrp>
  <trd lng="nl">smeltkroes</trd>
  <trd lng="pl">tygiel</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_n;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>
