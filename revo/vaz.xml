<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vaz</rad>/o
</kap>
<drv mrk="vaz.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Ujo el diversaj materioj, de diversa formo kaj grando, destinita por
      enteni &ccirc;iajn fluecajn a&ubreve; solidajn substancojn:
      <ekz>
        &ccirc;erpi akvon per <tld/>o;
      </ekz>
      <ekz>
        meti florojn en <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pot.0o">poto</ref>,
      <ref cel="pelv.0o">pelvo</ref>,
      <ref cel="krucx.0o">kru&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Vase</trd>
  <trdgrp lng="fr">
    <trd>pot</trd>,
    <trd>r&eacute;cipient</trd>
  </trdgrp>
  <trd lng="en">vase</trd>
  <trdgrp lng="nl">
    <trd>vaas</trd>,
    <trd>pot</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_u;&c_d;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_a;</trd> &c_i; &c_t;.&c_d;.
  </trdgrp>
</drv>

<drv mrk="vaz.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc>
    <dif>
      Sortimento de vazoj, aparte de tiuj uzataj dum man&gcirc;o.
      <ekz>
        lavi la <tld/>aron;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>vaisselle</trd>
  </trdgrp>
  <trd lng="nl">vaat</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_u;&c_d;&c_a;</trd>
</drv>

<drv mrk="vaz.0lavilo">
  <kap>
    <tld/>lavilo, <tld/>lavma&scirc;ino
  </kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Ma&scirc;ino por lavi la vazojn kaj aliajn man&gcirc;ilojn post man&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Geschirrsp&uuml;lmaschine</trd>
  <trd lng="en">dishwasher</trd>
  <trdgrp lng="fr">
    <trd>lave-vaisselle</trd>
  </trdgrp>
  <trd lng="nl">vaatwasmachine</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_u;&c_d;&c_o;&c_m;&c_o;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="vaz.fand0o">
  <kap>
    fand<tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="krisol.0o">Krisolo</ref>.
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_n;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>
