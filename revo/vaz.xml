<vortaro>
<art mrk="$Id: vaz.xml,v 1.38 2024/06/24 23:28:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vaz</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="vaz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vaz.0o.VAZ">
    <dif>
      Profunda <ref tip="super" cel="uj.0o">ujo</ref> el diversaj materialoj, de diversa formo kaj
      grando, destinita
      por enteni &ccirc;iajn fluecajn a&ubreve; solidajn substancojn:
      <ekz>
        &scirc;i prenis la plej belan ar&gcirc;entan <tld/>on, kiu estis
        en la lo&gcirc;ejo
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        prenu el la plej bonaj fruktoj de la lando en viajn <tld/>ojn, kaj alportu al la
        homo donacon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne havante <tld/>on <klr>[li]</klr> submetis sian &ccirc;apon kaj komencis melki
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne havas <tld/>ojn por trinki
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tabletoj portis sur si kristalajn <tld/>ojn plenigitajn de floroj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la fenestraj platoj verde brilis vitraj <tld/>oj kun konfita&jcirc;oj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ma&scirc;inoj <klr>[&#8230;]</klr> pretaj por <klr>[&#8230;]</klr> la faron kaj pentron de
        porcelanaj <tld/>oj
        <fnt>
          <bib>KrM</bib>
          <lok>14. Sperto Sa&gcirc;on Akrigas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la man&gcirc;o mi helpis al &scirc;i lavi la <tld/>ojn
        <fnt>
          <bib>KrM</bib>
          <lok>10. Virina Rideto pli Kaptas ol Reto.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o rompita longe sin tenas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en fremdan <tld/>on ne &scirc;ovu la nazon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bokal.0o">bokalo</ref>,
      <ref cel="botel.0o">botelo</ref>,
      <ref cel="bovl.0o.KOMUNE">bovlo</ref>,
      <ref cel="glas.0o.ujo">glaso</ref>,
      <ref cel="kalik.0o.glaso">kaliko</ref>,
      <ref cel="krucx.0o">kru&ccirc;o</ref>,
      <ref cel="tub.prov0o">provtubo</ref>,
      <ref cel="retort.0o">retorto</ref>,
      <ref cel="urn.0o.VAZ">urno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_shabchu;&t_ma;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>atuell</trd>,
    <trd>vas</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;doba</trd>,
    <trd>v&aacute;za</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gef&auml;&szlig;</trd>,
    <trd>Vase</trd>
  </trdgrp>
  <trd lng="el">&beta;&alfa_ton;&zeta;&omikron;</trd>
  <trdgrp lng="en">
    <trd>vase</trd>,
    <trd>vessel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vaso</trd>,
    <trd>recipiente</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;cipient</trd>
  <trdgrp lng="hu">
    <trd>ed&eacute;ny</trd>,
    <trd>v&aacute;za</trd>
  </trdgrp>
  <trd lng="id">vas</trd>
  <trd lng="ja">&#33032;&#x7BA1;  <pr>&#x307F;&#12419;&#12387;&#x304B;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>vaas</trd>,
    <trd>pot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>naczynie</trd>,
    <trd>waza</trd>
  </trdgrp>
  <trd lng="pt">vaso</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_u;&c_d;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_a;</trd> &c_i; &c_t;.&c_d;.
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;doba</trd>,
    <trd>v&aacute;za</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_u;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#29970; <pr>w&#x1D4;</pr></trd>
</drv>

<drv mrk="vaz.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="vaz.0aro.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sortimento de <tld/>oj, precipe tia uzata por man&gcirc;o:
      <ekz>
        <ind>lavi la <tld/>aron</ind>
        <trd lng="fr">faire la vaiselle</trd>
        <trd lng="hu">elmossa az ed&eacute;nyt</trd>
        <trdgrp lng="nl">
          <trd>afwassen</trd>,
          <trd>de <ind>vaat</ind> doen</trd>
        </trdgrp>
      </ekz>
      <ekz>
        kiam vi soifos, iru al la <tld/>aro, kaj trinku
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aro brilis de lukso
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la dometo estis <klr>[&#8230;]</klr> kuirejo kaj provizejo,
        <tld/>aro kupra kaj stana, &ccirc;io pura, brilanta
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_s;&c_u;&c_d;</trd>,
    <trd>&c_s;&c_e;&c_r;&c_v;&c_ib;&c_z;</trd>
  </trdgrp>
  <trd lng="ca">vaixella</trd>
  <trd lng="cs">n&aacute;dob&iacute;</trd>
  <trd lng="de">Geschirr</trd>
  <trdgrp lng="en">
    <trd>dishes</trd>,
    <trd>crockery</trd>,
    <trd>plates</trd>,
    <trd>table service</trd>
  </trdgrp>
  <trd lng="es">vajilla</trd>
  <trdgrp lng="fr">
    <trd>service de table</trd>,
    <trd>vaisselle</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ed&eacute;nyek</trd>,
    <trd>(asztali) <ind>k&eacute;szlet</ind></trd>,
    <trd>ed&eacute;nygarnit&uacute;ra</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12358;&#12388;&#x308F;&#x985E;  <pr>&#12358;&#12388;&#x308F;&#x308B;&#12356;</pr></trd>,
    <trd>&#x98DF;&#22120;&#x985E;  <pr>&#12375;&#12423;&#12387;&#x304D;&#x308B;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">vaat</trd>
  <trdgrp lng="pl">
    <trd>zastawa</trd>,
    <trd>garnitur</trd>,
    <trd>komplet<klr> (np. obiadowy)</klr></trd>,
    <trd>naczynia</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_u;&c_d;&c_a;</trd>
  <trd lng="sk">riad</trd>
  <trd lng="uk">&c_s;&c_je;&c_r;&c_v;&c_ib;&c_z; (&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_i;&c_j;)</trd>
  <trdgrp lng="zh">
    <trd>&#x76D8;&#30871; <pr>p&#xE1;nw&#x1CE;n</pr></trd>,
    <trd>&#x76E4;&#30871; <pr>p&#xE1;nw&#x1CE;n</pr></trd>,
    <trd>&#38518;&#22120; <pr>t&#xE1;oq&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vaz.fand0o">
  <kap>fand<tld/>o</kap>
  <snc mrk="vaz.fand0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="krisol.0o">krisolo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">gresol</trd>
  <trdgrp lng="cs">
    <trd>kokila</trd>,
    <trd>tav&iacute;c&iacute; n&aacute;doba</trd>
  </trdgrp>
  <trd lng="de">Schmelztiegel</trd>
  <trd lng="en">crucible</trd>
  <trd lng="es">crisol</trd>
  <trd lng="fr">creuset</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nt&odblac;&uuml;st</trd>,
    <trd>olvaszt&oacute;t&eacute;gely</trd>
  </trdgrp>
  <trd lng="ja">&#x308B;&#12388;&#x307C;</trd>
  <trd lng="nl">smeltkroes</trd>
  <trd lng="pl">tygiel</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_n;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">taviaca n&aacute;doba</trd>
  <trd lng="uk">&c_t;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="vaz.nokto0o">
  <kap>nokto<tld/>o</kap>
  <snc mrk="vaz.nokto0o.xx">
    <dif>
      Vazo uzata por en&ccirc;ambra, aparte dumnokta urinado kaj fekado:
      <ekz>
        vespere la unua <klr>[migra komizo]</klr> ka&scirc;e ver&scirc;as botelon
        da biero en la nokto<tld/>on de sia amiko
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el
        Bervalo</vrk>, <lok>1974</lok></fnt>;
      </ekz>
      <ekz>
        sub divano videblis nokto<tld/>o
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pis.0poto">pispoto</ref>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_n;&c_y; <ind>&c_g;&c_a;&c_r;&c_sh;&c_ch;&c_o;&c_k;</ind></trd>
  <trdgrp lng="ca">
    <trd>orinal</trd>,
    <trd>gibrelleta</trd>
  </trdgrp>
  <trd lng="cs">no&ccaron;n&iacute;k</trd>
  <trd lng="de">Nachttopf</trd>
  <trd lng="en">chamber pot</trd>
  <trdgrp lng="fr">
    <trd>pot de chambre</trd>,
    <trd>vase de nuit</trd>
  </trdgrp>
  <trd lng="ja">&#12375;&#12403;&#12435;</trd>
  <trd lng="pl">nocnik</trd>
  <trd lng="sk">no&ccaron;n&iacute;k</trd>
  <trd lng="uk">&c_n;&c_ib;&c_ch;&c_n;&c_i;&c_j; &c_g;&c_o;&c_r;&c_shch;&c_o;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x591C;&#x58F6; <pr>y&#xE8;h&#xFA;</pr></trd>,
    <trd>&#x591C;&#x58FA; <pr>y&#xE8;h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vaz.trans0igi">
  <kap>trans<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vaz.trans0igi.KOMUNE">
    <dif>
      Ver&scirc;i a&ubreve; konduki de <tld/>o en alian <tld/>on:
      <ekz>
        iam mi trovis lin &ccirc;irka&ubreve;atan de grupo da sklavo
        apud la olivmuelilo, meze de diskuto ekstreme
        teknika la&ubreve; mia impreso pri la plej bona momento
        por trans<tld/>igi la novan oleon
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>Morto de Artisto</vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_y; &c_w; &c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_u;)</klr></trd>
  <trdgrp lng="de">
    <trd>umf&uuml;llen</trd>,
    <trd>umgie&szlig;en</trd>
  </trdgrp>
  <trd lng="fr">transvaser</trd>
  <trdgrp lng="pl">
    <trd>przelewa&cacute;</trd>,
    <trd>przetacza&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="vaz.0lavilo">
  <kap><tld/>lavilo, <var><kap><tld/>lavma&scirc;ino</kap></var></kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="vaz.0lavilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ma&scirc;ino por lavi la vazojn kaj aliajn man&gcirc;ilojn post
      man&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_u;&c_d;&c_a;&c_m;&c_y;&c_j;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trd lng="ca">rentaplats</trd>
  <trd lng="de">Geschirrsp&uuml;lmaschine</trd>
  <trd lng="en">dishwasher</trd>
  <trd lng="fr">lave-vaisselle</trd>
  <trdgrp lng="hu">
    <trd>ed&eacute;nymosogat&oacute;</trd>,
    <trd>mosogat&oacute;g&eacute;p</trd>
  </trdgrp>
  <trd lng="ja">&#x98DF;&#22120;&#x6D17;&#12356;&#x6A5F;  <pr>&#12375;&#12423;&#12387;&#x304D;&#12354;&#12425;&#12356;&#x304D;</pr></trd>
  <trd lng="nl">vaatwasmachine</trd>
  <trd lng="pl">zmywarka<klr> do naczy&nacute;</klr></trd>
  <trd lng="pt">m&aacute;quina de lavar lou&ccedil;a</trd>
  <trd lng="es">lavaplatos</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_u;&c_d;&c_o;&c_m;&c_o;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
  <trd lng="uk">&c_p;&c_o;&c_s;&c_u;&c_d;&c_o;&c_m;&c_i;&c_j;&c_n;&c_a; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D17;&#30871;&#x673A; <pr>x&#x1D0;w&#x1CE;nj&#x12B;</pr></trd>,
    <trd>&#x6D17;&#30871;&#x6A5F; <pr>x&#x1D0;w&#x1CE;nj&#x12B;</pr></trd>
  </trdgrp>
</drv>
