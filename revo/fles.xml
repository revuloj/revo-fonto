<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fles.xml,v 1.21 2023/11/09 20:48:33 revo Exp $">
<kap>
<rad>fles</rad>/o
</kap>
<drv mrk="fles.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref>
      vivanta &ccirc;e marbordoj,
      kun ovoforma nesimetria plata korpo kaj okuloj sur unu flanko,
      faciligante enfosadon en la sablon de la marfundo lasante
      nur la okulojn nekovritaj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="pleuxr3.0oj">ple&ubreve;ronektedoj</ref>
      (<trd lng="la">Platichtys</trd>):
      <ekz>
        la <tld/>o havas pli dikan korpon ol la plateso kaj la karno havas
        pli delikatan strukturon, <tld/>o povas atingi longecon de 60&nbsp;cm
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=102169">Fleso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/13/Platichthys_flesus_V%C3%A4%C3%A4na-J%C3%B5esuu_in_Estonia.jpg/640px-Platichthys_flesus_V%C3%A4%C3%A4na-J%C3%B5esuu_in_Estonia.jpg" prm="CC BY-SA 3.0"><tld/>o (Platichthys flesus), &ccirc;e V&auml;&auml;na-J&otilde;esuu en Estonujo
         <fnt>
           <aut>Tiit Hunt, propra verko</aut>
           <lok><url ref="&WCW;/File:Platichthys_flesus_V%C3%A4%C3%A4na-J%C3%B5esuu_in_Estonia.jpg">&WCW;/File:Platichthys_flesus_V%C3%A4%C3%A4na-J%C3%B5esuu_in_Estonia.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_a; &c_r;&c_a;&c_ch;&c_n;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_i;&c_s;&c_i;&c_ja;</trd>
  <trd lng="de">Flunder</trd>
  <trd lng="en">flounder</trd>
  <trd lng="fr">flet</trd>
  <trd lng="hu">&eacute;rdes<ind> lep&eacute;nyhal</ind></trd>
  <trd lng="ja">&#x30CB;&#x30B7;&#x30AB;&#x30EF;&#x30AC;&#x30EC;&#x30A4;</trd>
  <trd lng="nl">bot<klr tip="ind"> (vis)</klr></trd>
  <trdgrp lng="pt">
    <trd>linguado</trd>,
    <trd>solha</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_a; &c_r;&c_je;&c_ch;&c_n;&c_a;&c_ja;</trd>
  <trd lng="uk">&c_r;&c_ib;&c_ch;&#x43A;&#x43E;&c_v;&c_a; &#x43A;&c_a;&#x43C;&c_b;&c_a;&#x43B;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5E73;&#x9B5A; <pr>p&#xED;ngy&#xFA;</pr></trd>,
    <trd>&#x9C1C; <pr>ji&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>

