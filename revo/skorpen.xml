<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>skorpen</rad>/o
</kap>
<drv mrk="skorpen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skorpe.0oj"><tld/>edoj</ref>
      (<trd lng="la">Scorpaena</trd>).
    </dif>
  </snc>
  <trd lng="de">Drachenkopf</trd>
  <trd lng="nl">zeeschorpioen</trd>
</drv>

<drv mrk="skorpen.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Scorpaeniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Panzerwangen</trd>
  <trd lng="nl">schorpioenvisachtigen</trd>
</drv>
</art>

