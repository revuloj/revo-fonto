<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>skorpen</rad>/o
</kap>
<drv mrk="skorpen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skorpen.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Scorpaena</trd>).
    </dif>
  </snc>
  <trd lng="de">Drachenkopf</trd>
</drv>
<drv mrk="skorpen.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo 
      <ref tip="super" cel="skorpen.0oformaj"><tld/>oformaj fi&scirc;oj</ref>
      (<trd lng="la">Scorpaenidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Drachenk&ouml;pfe <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="skorpen.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Scorpaeniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Panzerwangen</trd>
</drv>
</art>

