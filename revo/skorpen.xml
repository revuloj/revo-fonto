<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skorpen.xml,v 1.14 2022/03/27 17:06:35 revo Exp $">
<kap>
  <rad>skorpen</rad>/o
</kap>
<drv mrk="skorpen.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>,
      grandkapa kun venenaj na&gcirc;iloj, ofte vivanta en <ref tip="malprt" cel="rif.0o">rifoj</ref>,
      kamufli&gcirc;anta per ru&gcirc;eta, marmora kolora&jcirc;o kaj ha&ubreve;taj elkreska&jcirc;oj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="skorpe.0oj"><tld/>edoj</ref>
      (<trd lng="la">Scorpaena</trd>):
      <ekz>
        la Grandan <tld/>on oni ofte renkontas sur roka, algokovrita grundo, kie &gcirc;i embuskas predon,
        &gcirc;i &scirc;an&gcirc;as sian ha&ubreve;ton regule, e&ccirc; plurfoje monate.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/7f/Scorpaena_scrofa_02.JPG/640px-Scorpaena_scrofa_02.JPG" prm="CC BY-SA 3.0"><tld/>o (Scorpaena scrofa), &Scirc;tata Muzeo de Naturscienco, Karlsruhe, Germanujo
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Scorpaena_scrofa_02.JPG">&WCW;/File:Scorpaena_scrofa_02.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">esc&oacute;rpora</trd>
  <trd lng="de">Drachenkopf</trd>
  <trd lng="fr">rascasse</trd>
  <trd lng="hu">sziklahal</trd>
  <trd lng="nl">zeeschorpioen</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; <ind>&c_jo;&c_r;&c_sh;</ind></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x43E;&#x440;&#x43F;&#x435;&#x43D;&#x430;</trd>,
    <trd>&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x439;&#x43E;&#x440;&#x436;</trd>
  </trdgrp>
</drv>

<drv mrk="skorpen.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Scorpaeniformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">escorpeniformes</trd>
  <trd lng="de">Drachenkopfartige</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;l&ouml;ntealak&uacute;ak</trd>,
    <trd>p&aacute;nc&eacute;losarc&uacute;ak</trd></trdgrp>
  <trd lng="nl">schorpioenvisachtigen</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_p;&c_je;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="uk">&#x441;&#x43A;&#x43E;&#x440;&#x43F;&#x435;&#x43D;&#x43E;&#x43F;&#x43E;&#x434;&#x456;&#x431;&#x43D;&#x456;</trd>
</drv>
</art>
