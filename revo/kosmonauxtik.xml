  </snc>
  <trd lng="fr">astronautique<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="pt">cosmon&aacute;utica</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_n;&c_a;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
</art>

