  <art mrk="$Id: katmand.xml,v 1.8 2023/12/03 08:00:25 afido Exp $">
    <kap>
      <rad>Katmand</rad>/o, <var><kap><rad var="u">Katmandu</rad>/o</kap></var>
    </kap>

<drv mrk="katmand.0o">
  <kap><tld/>o, <var><kap><tld var="u"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La  <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="nepal.N0o">Nepalo</ref>:
      <ekz>
        &scirc;i piediris sola en montara regiono 4300&#x202F;m alta norde de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Shrestha/pg</aut>
          <vrk><url ref="&Monato;/2012/010435.php">S-vage</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri nepalaninoj el <tld/>o en urbaj vesta&jcirc;oj kaj molaj &scirc;uetoj kura&gcirc;e grimpis en
        la montaro
        <fnt>
          <bib>Monato</bib>
          <aut>Vera Barandovsk&aacute;-Frank</aut>
          <vrk><url ref="&Monato;/2004/008388.html">Nepalo tra e&ubreve;ropaj okuloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        15-minuta filmo gvidos vin al <tld var="u"/>o
        <fnt>
          <vrk><url ref="https://uea.facila.org/filmetoj/nepalo-katmanduo-turismaj-filmoj-en-esperanto/">Nepalo, Katmanduo. Turismaj filmoj en Esperanto - Filmetoj ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_a;&c_t;&c_m;&c_a;&c_n;&c_d;&c_u;</trd>
    <trd lng="de">Kathmandu</trd>
    <trd lng="en">Kathmandu</trd>
    <trd lng="fr">Katmandou</trd>
  </snc>
  <trd lng="bo">&#xF40;&#xF0B;&#xF4F;&#xF0B;&#xF58;&#xF53;&#xF0B;&#xF4F;&#xF74;&#xF0B;</trd>
  <trd lng="ja">&#x30AB;&#x30C8;&#x30DE;&#x30F3;&#x30BA;</trd>
  <trd lng="uk">&#x41A;&#x430;&#x442;&#x43C;&#x430;&#x43D;&#x434;&#x443;</trd>
  <trdgrp lng="zh">
    <trd>&#x52A0;&#x5FB7;&#x6EE1;&#x90FD; <pr>Ji&#x101;d&#xE9;m&#x1CE;nd&#x16B;</pr></trd>,
    <trd>&#x52A0;&#x5FB7;&#x6EE1;&#x90FD; <pr>ji&#x101;d&#xE9;m&#x1CE;nd&#x16B;</pr></trd>
  </trdgrp>
