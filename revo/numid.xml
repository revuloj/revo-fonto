<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: numid.xml,v 1.21 2024/01/29 15:59:31 revo Exp $">
<kap>
  <rad>numid</rad>/o
</kap>
<drv mrk="numid.0o">
  <kap><tld/>o</kap>
  <snc mrk="numid.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="bird.korto0o.AGR">Kortobirdo</ref>
      kun griza nigre punktita plumaro kaj delikata viando; 
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      el la ordo de <ref tip="malprt" cel="kok.0oformaj">kokoformaj</ref>:
      <ekz>
        sur herbo promenis <klr>[&#x2026;]</klr> &scirc;prucmakulaj mukokulaj 
        <tld/>oj
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/V.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>;
      </ekz>
      <ekz>
        akacioj kaj jujubarboj <klr>[&#x2026;]</klr> donas ombron al birdokorto, kie vivas
        dudeko da <tld/>oj
        <fnt>
          <bib>MD08</bib>
          <vrk>Novaj spertoj pri agro-forstado &dash; Kiel Sahelo reverdi&gcirc;as</vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1a/Numida_meleagris_-_Heidelberg.jpg/640px-Numida_meleagris_-_Heidelberg.jpg" prm="CC BY-SA 3.0"><tld/>o (Numida meleagris), Zoo Heidelberg, Germanujo
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Numida_meleagris_-_Heidelberg.jpg">&WCW;/File:Numida_meleagris_-_Heidelberg.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="numid1.0o">numido (nordafrikano)</ref>
  <trd lng="be">&c_c;&c_e;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>pintada</trd>,
    <trd>gallina de Guinea</trd>    
  </trdgrp>
  <trd lng="cs">perli&ccaron;ka</trd>
  <trd lng="de">Perlhuhn</trd>
  <trd lng="fr">pintade</trd>
  <trd lng="hu">gy&ouml;ngyty&uacute;k</trd>
  <trd lng="ja">&#x30DB;&#x30ED;&#x30DB;&#x30ED;&#x30C1;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">parelhoen</trd>
  <trd lng="pl">perliczka</trd>
  <trd lng="pt">galinha d'angola</trd>
  <trd lng="ru">&c_c;&c_je;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">perli&ccaron;ka oby&ccaron;ajn&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x446;&#x435;&#x441;&#x430;&#x440;&#x43A;&#x430;</trd>,
    <trd>&#x43D;&#x443;&#x43C;&#x456;&#x434;&#x456;&#x454;&#x446;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x73E0;&#x96DE; <pr>zh&#x16B;j&#x12B;</pr></trd>,
    <trd>&#x73E0;&#x9E21; <pr>zh&#x16B;j&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
