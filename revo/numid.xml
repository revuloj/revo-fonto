<vortaro>
<art mrk="$Id$">
<kap>
  <rad>numid</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="numid.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Korta birdo el fam. de kokoj, kun griza nigre punktita plumaro kaj
      delikata viando.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>pintada</trd>,
    <trd>gallina de Guinea</trd>    
  </trdgrp>
  <trd lng="cs">perlička</trd>
  <trd lng="de">Perlhuhn</trd>
  <trd lng="fr">pintade</trd>
  <trd lng="hu">gy&ouml;ngyty&uacute;k</trd>
  <trd lng="nl">parelhoen</trd>
  <trd lng="pl">perliczka</trd>
  <trd lng="pt">galinha d'angola</trd>
  <trd lng="ru">&c_c;&c_je;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">perlička obyčajná</trd>
</drv>
</art>
