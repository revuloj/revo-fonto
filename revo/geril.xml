<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>geril</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="geril.0o">
  <kap><tld/>o</kap>
  <snc mrk="geril.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Partizana <ref tip="super" cel="milit.0i.MIL">milito</ref> el
      malgrandaj bataloj kaj embuskoj:
      <ekz>
        liaj gamboj estas harditaj per montmigrado en la tempo de
        <tld/>o
        kontra&ubreve; Japanio
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;tragedio de man&gcirc;o en
        la
        insulo Mindanao&rightquot;, p. 33a</lok></fnt>;
      </ekz>
      <ekz><ind><tld/>milito</ind>
      <trd lng="de">Guerillakrieg</trd>
      <trd lng="es">Guerra de Guerrillas</trd>;
      <trd lng="hu">gerillah&aacute;bor&uacute;</trd>
      <trd lng="nl">guerrillaoorlog</trd>
      </ekz>
      <ekz>
        mar&scirc;i <tld/>e;
      </ekz>
      <ekz><uzo tip="stl">FIG</uzo> politikaj <tld/>oj
       <fnt><aut>Paul Gubbins</aut>, <vrk>BRITIO: Politikaj 
       geriloj</vrk>, <bib>Monato</bib>, <lok>1989/06, p.
      10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Guerilla</trd>
  <trd lng="en">guerilla war</trd>
  <trd lng="es">guerrilla</trd>
  <trd lng="fr">gu&eacute;rilla</trd>
  <trd lng="hu">gerillaharc</trd>
  <trd lng="it">guerriglia</trd>
  <trd lng="nl">guerrilla</trd>
  <trd lng="pt">guerrilha</trd>
  <trd lng="sk">partizánske boje</trd>
</drv>

<drv mrk="geril.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="geril.0ano.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ano de grupo, praktikanta <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Guerrillero</trd>
  <trd lng="en">guerilla</trd>
  <trd lng="es">guerrillero</trd>
  <trd lng="hu">gerilla</trd>
  <trd lng="it">guerrigliero</trd>
  <trd lng="nl">guerrillastrijder</trd>
  <trd lng="pt">guerrilheiro</trd>
  <trd lng="sk">partizánsky bojovník</trd>
</drv>
</art>

