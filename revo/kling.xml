<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kling</rad>/o
</kap>
<drv mrk="kling.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metala plata parto de instrumento a&ubreve; armilo, destinita por
      tran&ccirc;i a&ubreve; piki:
      <ekz><tld/>o de tran&ccirc;ilo, glavo, &ccirc;izilo, hakilo.</ekz>
    </dif>
  </snc>
  <trd lng="de">Klinge</trd>
  <trdgrp lng="en">
    <trd>blade</trd>,
    <trd>cutting edge</trd>
  </trdgrp>
  <trd lng="nl">lemmet</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_z;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
