<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kooper1.xml,v 1.20 2023/12/03 09:58:58 revo Exp $">
<kap>
  <ofc>9</ofc><rad>kooperativ</rad>/o
  <fnt><bib>BEE</bib></fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kooper1.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <snc mrk="kooper1.0o.EKON">
  <uzo tip="fak">EKON</uzo>
  <dif>
      Koopera <ref tip="super" cel="societ.0o.institucio">societo</ref>:
      <ekz>
        oni distingas <tld/>ojn pri produktado kaj <tld/>ojn pri
        konsumado;
      </ekz>
      <ekz>
        <ref tip="sub" cel="kolhxo.0o">kol&hcirc;ozo</ref> <klr>[&#x2026;]</klr> estas <tld/>o 
        de vila&gcirc;anoj posedanta la grundon kaj la
        ekipa&jcirc;on, <ref tip="sub" cel="sovhxoz.0o">sov&hcirc;ozo</ref> apartenas al la &scirc;tato
        <fnt>
          <bib>CKv</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;parmono iom post iom malaperis el la kontoj en la <tld/>o, regis malgaja
        humoro, kiam man&gcirc;a&jcirc;o estis minimuma
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Olga</nom> <klr>[&#x2026;]</klr> malvarmetone demandis, &ccirc;u 
        li atentis mankojn de l&#x2019; <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        Japana Esperanta Libro-<tld lit="K"/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kultura <tld lit="K"/>o de Esperantistoj, kiu nun, interalie, subtenas propran
        eldonejon kaj organizas &ccirc;iujarajn seminariojn
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1934 oni planis fondon de <tld/>o por financado de Esperantaj sonfilmoj
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_p;&c_e;&c_r;&c_a;&c_t;&c_y;&c_w;</trd>
  <trd lng="ca">cooperativa</trd>
  <trdgrp lng="de">
     <trd>Kooperative<klr> [GDR]</klr></trd>,
     <trd>Genossenschaft<klr> [FRG]</klr></trd>
  </trdgrp>
  <trd lng="en">cooperative</trd>
  <trd lng="fr">coop&eacute;rative</trd>
  <trd lng="hu">sz&ouml;vetkezet</trd> 
  <trd lng="ja">&#x5354;&#x540C;&#x7D44;&#x5408;  <pr>&#x304D;&#x3087;&#x3046;&#x3069;&#x3046;&#x304F;&#x307F;&#x3042;&#x3044;</pr></trd>
  <trd lng="pl">sp&oacute;&lstroke;dzielnia</trd> 
  <trd lng="pt">cooperativa</trd>
  <trd lng="ro">cooperativ&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_o;&c_p;&c_je;&c_r;&c_a;&c_t;&c_i;&c_v;</trd>
  <trd lng="zh">&#x5408;&#x4F5C;&#x793E; <pr>h&#xE9;zu&#xF2;sh&#xE8;</pr></trd>
</drv>

<drv mrk="kooper1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a societo kun variigeblaj socia kapitalo kaj personaro
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a redaktejo de la renoma internacia kaj dumonata beletra revuo
        Literatura Verniso
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj gastoj trafantomis la salonon preter du ma&ubreve;zedongece grandaj fotoj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kooper.0a">koopera</ref>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_a;&c_p;&c_e;&c_r;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_k;&c_a;&c_a;&c_p;&c_e;&c_r;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x5354;&#x540C;&#x7D44;&#x5408;&#x306E;  <pr>&#x304D;&#x3087;&#x3046;&#x3069;&#x3046;&#x304F;&#x307F;&#x3042;&#x3044;&#x306E;</pr></trd>
</drv>





</art>
