<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>truf</rad>/o
</kap>
<drv mrk="truf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Subtere kreskanta fruktokorpo de iuj fungoj, precipe el
      la genro <trd lng="la">Tuber</trd>,
      tre &scirc;atata pro &gcirc;ia delikata aromo.
    </dif>
  </snc>
  <trd lng="de">Tr&uuml;ffel</trd>
  <trd lng="fr">truffe</trd>
</drv>
<drv mrk="truf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">truffer</trd>
</drv>
</art>
