<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>truf</rad>/o
</kap>
<drv mrk="truf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Subtere kreskanta fruktokorpo de iuj fungoj, precipe el
      la genro <trd lng="la">Tuber</trd>,
      tre &scirc;atata pro &gcirc;ia delikata aromo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_f;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>t&ograve;fona</trd>,
    <trd>trufa</trd>
  </trdgrp>
  <trd lng="cs">lan&yacute;&zcaron;</trd>
  <trd lng="de">Tr&uuml;ffel</trd>
  <trd lng="en">truffle</trd>
  <trd lng="fr">truffe</trd>
  <trd lng="hu">szarvasgomba</trd>
  <trd lng="nl">truffel</trd>
  <trd lng="pl">trufla</trd>
  <trd lng="pt">trufa</trd>
  <trd lng="ru">&c_t;&c_r;&c_ju;&c_f;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">h&lcaron;uzovka <klr>(huba)</klr></trd>
</drv>
<drv mrk="truf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj.
    </dif>
  </snc>
  <trd lng="ca">trufar</trd>
  <trd lng="cs">oblo&zcaron;it lan&yacute;&zcaron;i</trd>
  <trd lng="en">to garnish with truffles</trd>
  <trd lng="fr">truffer</trd>
  <trd lng="hu">szarvasgomb&aacute;val t&ouml;lt</trd>
  <trd lng="nl">met <ind>truffels</ind> garneren</trd>
  <trd lng="sk">oblo&zcaron;i&tcaron; h&lcaron;uzovkami</trd>
</drv>
</art>
