<drv mrk="nazi.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="nazi.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ekstrem-dekstra politika movado ekinta en E&ubreve;ropo en la 1930aj
      jaroj, naciisma, diktatorema kaj kruele rasisma aparte kontra&ubreve; judoj,
      unue sukcesinta en Germanujo, precipa ka&ubreve;zo de la dua mondmilito:
      <ekz>
        la negativa sinteno de <tld/>ismo al Esperanto &ndash; tute natura, &ccirc;ar tia plej
        ekstrema naciismo-rasismo, unuflanke, kaj internaciismo, kies lingva esprimo
        estas la Internacia Lingvo, aliflanke, neniel povas kunekzisti &ndash; ka&ubreve;zis
        detruon de la Esperanto-Movado en Germanio
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fasxis.0o">fa&scirc;ismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_c;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>nazisme</trd>,
    <trd>nacionalsocialisme</trd>
  </trdgrp>
  <trd lng="cs">nacismus</trd>
  <trd lng="de">Nationalsozialismus</trd>
  <trd lng="en">Nazism</trd>
  <trdgrp lng="es">
  	<trd>nazismo</trd>,
  	<trd>nacionalsocialismo</trd>
  </trdgrp>
  <trd lng="fr">nazisme</trd>
  <trd lng="he">&nun;&alef;&tsadi;&yod;&zayin;&fmem;</trd>
  <trd lng="hu">n&aacute;cizmus</trd>
  <trd lng="ja">&#12490;&#12481;&#12474;&#12512;</trd>
  <trd lng="nl">nazisme</trd><trd lng="nl">nazismo</trd>
  <trdgrp lng="pl">
    <trd>nazizm</trd>,
    <trd>hitleryzm</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>nazism</trd>,
    <trd>hitlerism</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_c;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">nacizmus</trd>
  <trd lng="sv">nazism</trd>
  <trd lng="uk">&c_n;&c_a;&c_c;&c_i;&c_z;&c_m;</trd>
