    <trd>&#32435;&#31929; <pr>N&agrave;cu&igrave;</pr></trd>,
    <trd>&#32013;&#31929;&#20221;&#23376; <pr>N&agrave;cu&igrave;f&egrave;nz&#464;</pr></trd>,
    <trd>&#32435;&#31929;&#20998;&#23376; <pr>N&agrave;cu&igrave;f&egrave;nz&#464;</pr></trd>,
    <trd>&#32013;&#31929; <pr>n&agrave;cu&igrave;</pr></trd>,
    <trd>&#32435;&#31929; <pr>n&agrave;cu&igrave;</pr></trd>,
    <trd>&#32013;&#31929;&#20998;&#23376; <pr>n&agrave;cu&igrave;f&#275;nz&#464;</pr></trd>,
    <trd>&#32435;&#31929;&#20998;&#23376; <pr>n&agrave;cu&igrave;f&#275;nz&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nazi.0a">
  <kap><tld/>a</kap>
  <snc mrk="nazi.0a.POL">
    <dif>
      Rilata al <tld/>oj, al ilia doktrino kaj agado:
      <ekz>
        <tld/>a studenta unui&gcirc;o
        <fnt><bib>Dan&gcirc;Lng</bib>, <lok>&ccirc;ap. 2.5, p. 117a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_c;&c_y;&c_s;&c_c;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>nazi</trd>,
    <trd>nacionalsocialista</trd>
  </trdgrp>
  <trd lng="de">nationalsozialistisch</trd>
  <trd lng="en">Nazi</trd>
  <trdgrp lng="es">
  	<trd>nazi</trd>,
  	<trd>nacionalsocialista</trd>
  </trdgrp>
  <trd lng="fr">nazi<klr> (adj.)</klr></trd>
  <trd lng="he">&nun;&alef;&tsadi;&yod;</trd>
  <trd lng="hu">n&aacute;ci</trd>
  <trdgrp lng="ja">
    <trd>&#12490;&#12481;&#12398;</trd>,
    <trd>&#12490;&#12481;&#12473;&#12398;  <pr>&#12394;&#12385;&#12377;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">nazistisch</trd>
