<drv mrk="nazi.0o">
  <kap><tld/>o</kap>
  <snc mrk="nazi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano, subtenanto de <tld/>ismo:
      <ekz>
        mi memoras la viza&gcirc;on de mia avo; li estis <tld/>o, konvinkita <tld/>o &gcirc;is sia
        morto anta&ubreve; tri jaroj
        <fnt>
          <bib>FSI</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_c;&c_y;&c_s;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>nazi</trd>,
    <trd>nacionalsocialista</trd>
  </trdgrp>
  <trd lng="cs">nacista</trd>
  <trd lng="de">Nazi</trd>
  <trd lng="en">Nazi</trd>
  <trd lng="es">nazi</trd>
  <trd lng="fr">nazi<klr> (subst.)</klr></trd>
  <trd lng="he">&nun;&alef;&tsadi;&yod;</trd>
  <trd lng="hu">n&aacute;ci</trd>
  <trd lng="ja">&#12490;&#12481;&#20826;&#21729;  <pr>&#12490;&#12481;&#12392;&#12358;&#12356;&#12435;</pr></trd>
  <trd lng="nl">nazi</trd>
  <trd lng="pl">nazista</trd>
  <trdgrp lng="pt">
    <trd>nazista <klr>(subst.)</klr></trd>,
    <trd>nazi <klr>(subst.)</klr></trd>,
    <trd>hitlerista <klr>(subst.)</klr></trd>,
    <trd>hitleriano <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="ro">nazist</trd>
  <trd lng="ru">&c_n;&c_a;&c_c;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">nacista</trd>
  <trd lng="sv">nazist</trd>
  <trd lng="uk">&c_n;&c_a;&c_c;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
