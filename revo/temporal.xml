<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: temporal.xml,v 1.17 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>temporal</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="temporal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la du <ref tip="super" cel="ost.0o">osto</ref>j de la homa
      <ref tip="malprt" cel="skelet.0o.ANA">skeleto</ref>, formantaj la
      flankojn kaj la malsupron de la <ref tip="malprt" cel="krani.0o">kranio</ref>, kaj entenantaj la internan orelon:
      <ekz>
        la&ubreve; ne&ubreve;ropsikolog<klr>[i]</klr>aj esploroj la difektitaj areoj estas la maldekstra <tld/>a lobo kaj tempora-parietala areoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=205207">Afazio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frunt.0osto">fruntosto</ref>,
      <ref cel="pariet.0osto">parietosto</ref>,
      <ref cel="okcipi.0osto">okcipitosto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_o;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">os <ind>temporal</ind></trd>
  <trd lng="cs">sp&aacute;nkov&aacute; kost</trd>
  <trd lng="de">Schl&auml;fenbein</trd>
  <trd lng="en">temporal bone</trd>
  <trd lng="es">hueso <ind>temporal</ind></trd>
  <trd lng="fr">os temporal</trd>
  <trd lng="hu">hal&aacute;nt&eacute;kcsont</trd>
  <trd lng="it">osso <ind>temporale</ind></trd>
  <trd lng="ja">&#x5074;&#x982D;&#x9AA8;  <pr>&#x305D;&#x304F;&#x3068;&#x3046;&#x3053;&#x3064;</pr></trd>
  <trd lng="nl">slaapbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; skroniowa</trd>
  <trdgrp lng="pt">
    <trd>temporal</trd>,
    <trd>osso temporal</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">sluchov&aacute; kos&tcaron;</trd>
  <trd lng="uk">&#x432;&#x438;&#x441;&#x43A;&#x43E;&#x432;&#x430; &#x43A;&#x456;&#x441;&#x442;&#x43A;&#x430;</trd>
</drv>
</art>
