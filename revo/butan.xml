<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: butan.xml,v 1.15 2022/07/08 08:39:40 revo Exp $">
<kap>
  <rad>Butan</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="butan.B0o">
  <kap>
    <tld lit="B"/>o
  </kap>
  <mlg kod="ISO-3166">BT</mlg>
  <snc mrk="butan.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tateto</ref>
      en la <ref tip="vid" cel="himalaj.H0o">Himalajo</ref>
      kun &ccirc;efurbo Timbuo; teritorio de Re&gcirc;ujo <tld/>o,
      limanta kun
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      kaj <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>:
      <ekz>
        la futbala asocio FIFA regule aktualigas sian rangliston kaj du lastajn lokojn
        okupas du etaj &scirc;tatoj <tld/>o (30&#x202F;000 lo&gcirc;antoj) kaj Montserato (5&#x202F;000
        lo&gcirc;antoj)
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki LINS</aut>
          <vrk><url ref="&Monato;/2004/008364.html">Plej malforta futbalnacio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pozitivaj evoluoj okazis la&ubreve; AI en <tld/>o, Grekio, Samoo, Senegalo kaj Turkio,
        kiuj abolis la mortpunon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008644.php">Ekzekutado ekzekuciata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        multaj nepalanoj eklo&gcirc;is en <tld/>o, a&ccirc;etante terpecojn kaj domojn
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Shrestha/pg</aut>
          <vrk><url ref="&Monato;/2008/009405.php">Timo senlima</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        eksterlandanoj rajtas viziti nur du el la dek regionoj en <tld/>o kaj ne facilas
        akiri vizojn
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Shrestha/pg</aut>
          <vrk><url ref="&Monato;/2008/009405.php">Timo senlima</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_B;&c_u;&c_t;&c_a;&c_n; <klr tip="amb">(&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_v;&c_a;)</klr></trd>
    <trd lng="bg">&c_B;&c_u;&c_t;&c_a;&c_n;</trd>
    <trd lng="bo">&#xF56;&#xFB2;&#xF74;&#xF42;&#xF0B;&#xF61;&#xF74;&#xF63;&#xF0B;</trd>
  <trd lng="de">Bhutan</trd>
    <trd lng="en">Bhutan</trd>
    <trdgrp lng="fr">
      <trd>Bhoutan</trd>,
      <trd>Royaume du Bhoutan</trd>
    </trdgrp>
    <trd lng="he">&#x5D2;&#x5B8;&#x5D6; &#x5D1;&#x5BC;&#x5D5;&#x5D8;&#x5B8;&#x5D0;&#x5DF;</trd>
  <trd lng="id">Bhutan</trd>
    <trd lng="it">Bhutan</trd>
    <trd lng="nl">Bhoetan</trd>    
    <trdgrp lng="pl">
      <trd>Bhutan</trd>,
      <trd>Kr&oacute;lestwo <ind>Bhutanu</ind></trd>
    </trdgrp>
    <trd lng="pt">But&atilde;o</trd>
  <trd lng="uk">&c_B;&c_u;&c_t;&c_a;&#x43D;</trd>
</drv>

</art>

