<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>butan</rad>/o
</kap>

<drv mrk="butan.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="butan.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la <ref tip="vid" cel="himalaj.0o">Himalajo</ref>
      kun &ccirc;efurbo Timbuo.
      Teritorio de Re&gcirc;ujo Butano.
      ISO-3166 kodo: BT.
      &Gcirc;i limas kun
      <ref tip="vid" cel="cxin.Cx0ujo">&Ccirc;inujo</ref>
      kaj <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>.
    </dif>
    <trd lng="bg">&c_B;&c_u;&c_t;&c_a;&c_n;</trd>
    <trd lng="de">Bhutan</trd>
    <trd lng="en">Bhutan</trd>
    <trd lng="nl">Bhoetan</trd>    
    <trdgrp lng="pl">
      <trd>Bhutan</trd>,
      <trd>Kr&oacute;lestwo <ind>Bhutanu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

