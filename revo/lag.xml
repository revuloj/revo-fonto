<drv mrk="lag.Asfalta_L0o">
  <kap>
    Asfalta <tld lit="L"/>o
  </kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva nomo de
      <ref tip="sin" cel="mar.MalvivaM0o">Malviva Maro</ref>:
      <ekz>
        super <nom>Jer&scirc;alaim</nom> nun &scirc;vebis <klr>[&#x2026;]</klr> griza nubo
        ariergarda, la fulmotondro
        estis forportata al la Asfalta <tld lit="L"/>o
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Asphaltsee</trd>
  <trd lng="id"><ind>Danau</ind> Asphaltite</trd>
</drv>

<drv mrk="lag.Garda_L0o">
  <kap>
    Garda <tld lit="L"/>o
  </kap>
  <snc>
    <dif>
      Plej granda lago de Italujo, &ccirc;. 50&#x202F;km sudokcidente de Trento:
      <ekz>
        ne malproksime de Verono <klr>[&#x2026;]</klr>, &ccirc;eborde de
        la Garda <tld lit="L"/>o,
        kun vizito de la lokoj, kie vivis
        la poeto <nom>Gabriele D&#x2019;Annunzio</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_r;&c_d;&c_a; <klr tip="amb">(&c_v;&c_o;&c_z;&c_je;&c_r;&c_a; &c_w; &c_Ib;&c_t;&c_a;&c_l;&c_ib;&c_ib;)</klr></trd>
  <trd lng="de">Gardasee</trd>
  <trd lng="en">Lake Garda</trd>
  <trd lng="fr">lac de Garde</trd>
  <trd lng="id"><ind>Danau</ind> Garda</trd>
  <trdgrp lng="it">
    <trd>Lago di Garda</trd>,
    <trd>Benaco</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52A0;&#x5C14;&#x8FBE;&#x6E56; <pr>ji&#x101;&#x11B;rd&#xE1;h&#xFA;</pr></trd>,
    <trd>&#x52A0;&#x723E;&#x9054;&#x6E56; <pr>ji&#x101;&#x11B;rd&#xE1;h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lag.Grandaj_0oj">
  <kap>
    Grandaj <tld lit="L"/>oj
  </kap>
  <snc mrk="lag.Grandaj_0oj.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La kvin grandaj lagoj &ccirc;e la kanada-usona limo, t.e.
      <refgrp tip="ekz">
         Supra Lago,
         Hurono,
         <ref cel="micxigan.0o.lago">Mi&ccirc;igano</ref>,
         <ref cel="eri.0o">Erio</ref>,
         <ref cel="ontari.O0o">Ontario</ref>
      </refgrp>:
      <ekz>
        la Grandaj <tld lit="L"/>oj <klr>[&#x2026;]</klr> formas la plej grandan nesalan
        akvosistemon de la tero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1393">Grandaj Lagoj
          (Nordameriko)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la MNP, liverita de tri usonaj grandaj
        urbaj regionoj (Nov-Jorko, la regiono de
        Los-An&gcirc;eleso kaj la regiono de la
        Grandaj <tld lit="L"/>oj), konsistigis 67&#x202F;%
        de la MNP de ilia lando
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2004/008325.html">Kvin defioj por &ccirc;inaj
          urboj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib;&c_ja; &c_a;&c_z;&c_jo;&c_r;&c_y;</trd>
  <trd lng="de">Gro&szlig;e Seen</trd>
  <trd lng="en">Great Lakes</trd>
  <trd lng="fr">Grands Lacs</trd>
  <trd lng="id"><ind>Danau</ind>-Danau Besar (Amerika)</trd>
  <trdgrp lng="zh">
    <trd>&#x4E94;&#x5927;&#x6E56; <pr>W&#x1D4;d&#xE0;h&#xFA;</pr></trd>,
    <trd>&#x4E94;&#x5927;&#x6E56; <pr>w&#x1D4;d&#xE0;h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lag.Gxeneva_0o">
  <kap>
    &Gcirc;eneva <tld lit="L"/>o
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="leman.0o">Lemano</ref>:
      <ekz>
        diris <nom>Kabe</nom> siatempe: &leftquot;la Esperantistoj, jes,
        <klr>[&#x2026;]</klr> ne scias sian
        lingvon! ili tiel malbone parolis! (ekzemple, sur
        &scirc;ipo sur &Gcirc;eneva <tld/>o),
        ke mi petis paroli al mi en nacia lingvo&rightquot;
        <fnt>
          <lok><url ref="https://www.liberafolio.org/2020/12/11/esperanto-aperas-duonkase-en-filmo-de-netflix/">Libera
          Folio, 2020-12-11</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Zh;&c_e;&c_n;&c_je;&c_w;&c_s;&c_k;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_L;&c_je;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Genfersee</trd>
  <trd lng="id"><ind>Danau</ind> Jenewa</trd>
  <trdgrp lng="zh">
    <trd>&#x65E5;&#x5167;&#x74E6;&#x6E56; <pr>r&#xEC;n&#xE8;iw&#x1CE;h&#xFA;</pr></trd>,
    <trd>&#x65E5;&#x5185;&#x74E6;&#x6E56; <pr>r&#xEC;n&#xE8;iw&#x1CE;h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lag.Konstanca_0o">
  <kap>
    Konstanca <tld lit="L"/>o, <var><kap>Boden<tld/>o</kap></var>
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <tld lit="L"/>o trafluata de <ref cel="rejn.R0o">Rejno</ref> en la trilanda regiono inter
      Germanujo, A&ubreve;strujo kaj
      Svislando:
      <ekz>
        la kampara lo&gcirc;antaro, sub la premo de mizeraj vivkondi&ccirc;oj, sendis <klr>[&#x2026;]</klr>
        lertajn masonistojn kaj konstrumajstrojn, pentristojn kaj aliajn metiistojn,
        kiuj konstruis la famajn barokajn katedralojn pli-malpli proksime de la
        Konstanca <tld lit="L"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj
          progreso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        subakve na&gcirc;i en la Boden<tld/>o estas samtempe alloge kaj postuleme
        <fnt>
          <vrk><url ref="https://eo.advisor.travel/poi/Bodenlago-425">Bodenlago in K&ouml;chlin -
          Advisor.Travel</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Rejn-Akvofalo estas natura obstaklo, kiu interrompas la &scirc;ipvojon inter la
        norda maro kaj la Boden<tld/>o
        <fnt>
          <vrk><url ref="https://eo.advisor.travel/poi/Rejn-Akvofalo-5350">Rejn-Akvofalo in Laufen -
          Advisor.Travel</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_o;&c_d;&c_e;&c_n;&c_s;&c_k;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>
  <trd lng="de">Bodensee</trd>
  <trd lng="en">Lake Constance</trd>
  <trd lng="fr">Lac de Constance</trd>
  <trd lng="id"><ind>Danau</ind> Konstanz</trd>
  <trd lng="zh">&#x535A;&#x767B;&#x6E56; <pr>B&#xF3;d&#x113;ngH&#xFA;</pr></trd>
</drv>

<drv mrk="lag.soda_0o">
  <kap>
    soda <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <tld lit="L"/>o sen elfluejo kun alta pH-valoro kaj
      granda kvanto de solvitaj mineraloj:
      <ekz>
        la armeo de la kronprinco, malgra&ubreve; la levi&gcirc;o de
        Nilo <klr>[&#x2026;]</klr> atingis la
        bordon de la dezerto kaj, provizita per akvo
        kaj nutra&jcirc;o, malaperis inter la
        montoj de Sodaj <tld lit="L"/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_o;&c_l;&c_a;&c_ch;&c_n;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_d;&c_a;&c_v;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Natronsee</trd>
  <trd lng="en">soda lake</trd>
  <trd lng="fr">lac de soude</trd>
  <trdgrp lng="id">
    <trd><ind>danau</ind> alkalin</trd>,
    <trd><ind>danau</ind> soda</trd>
  </trdgrp>
</drv>

<drv mrk="lag.Tanganjika_L0o">
  <kap>
    Tanganjika <tld lit="L"/>o
  </kap>
  <snc>
    <dif>
      Granda afrika lago lime de Kongo Kin&scirc;asa,
      Tanzanio, Zambio kaj Burundo:
      <ekz>
        la celebrado de &ccirc;i tiu festo i&gcirc;is tradicia en
        Burundo ekde la eniro de islamo
        <klr>[&#x2026;]</klr> trans la Tanganjika <tld lit="L"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="https://www.monato.be/2009/009701.php">Fino de ramadano: &ccirc;u festo de
          gejunuloj?</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_a;&c_n;&c_g;&c_a;&c_n;&c_mol;&c_ib;&c_k;&c_a; <klr tip="amb">(&c_v;&c_o;&c_z;&c_je;&c_r;&c_a;)</klr></trd>
  <trd lng="de">Tanganjikasee</trd>
  <trd lng="en">Lake Tanganyika</trd>
  <trd lng="fr">lac Tanganyika</trd>
  <trd lng="id"><ind>Danau</ind> Tanganyika</trd>
  <trd lng="zh">&#x5766;&#x5676;&#x5C3C;&#x5580;&#x6E56; <pr>T&#x1CE;ng&#xE1;n&#xED;k&#x101;H&#xFA;</pr></trd>
</drv>

<drv mrk="lag.Tiberiada_L0o">
  <kap>
    Tiberiada <tld lit="L"/>o
  </kap>
  <snc>
    <dif>
      Lago &ccirc;. 130&#x202F;km norde de Malviva Maro kaj kiun trafluas Jordano:
      <ekz>
        li bone montris al mi &ccirc;iujn sanktajn lokojn en Jerusalemo,
        min gvidis tra la
        tuta lando, &gcirc;is la Tiberiada <tld lit="L"/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 11 (61)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_a;&c_l;&c_ib;&c_l;&c_je;&c_j;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_T;&c_y;&c_v;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_z;&c_k;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_K;&c_ib;&c_n;&c_e;&c_r;&c_e;&c_t; <klr>(&c_v;&c_o;&c_z;&c_je;&c_r;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ar">&a_b;&a_H;&a_y;&a_r;&a_t_marbuta; &a_T;&a_b;&a_r;&a_y;&a_A;</trd>
  <trd lng="de">See Genezareth</trd>
  <trd lng="en">Sea of Galilee</trd>
  <trd lng="fr">lac de Tib&eacute;riade</trd>
  <trd lng="he">&yod;&fmem; &kaf;&nun;&resh;&tav;</trd>
  <trdgrp lng="id">
    <trd><ind>Danau</ind> Galilea</trd>,
