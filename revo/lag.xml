<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lag</rad>/o
</kap>
<drv mrk="lag.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Vasta etenda&jcirc;o da akvo, &ccirc;iuflanke limigita de tero:
      <ekz>
        &Gcirc;eneva <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>
  <trd lng="br">lenn<klr tip="ind"> (dour)</klr></trd>
  <trd lng="cs">jezero</trd>
  <trd lng="de">See</trd>
  <trd lng="el">&lambda;&jota_ton;&my;&ny;&eta;</trd>
  <trd lng="en">lake</trd>
  <trd lng="es">lago</trd>
  <trd lng="fr">lac</trd>
  <trd lng="hu">t&oacute;</trd>
  <trd lng="it">lago</trd>
  <trd lng="nl">meer<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="pl">jezioro</trd>
  <trd lng="pt">lago</trd>
  <trd lng="ru">&c_o;&c_z;&c_je;&c_r;&c_o;</trd>
  <trd lng="tr">g&ouml;l</trd>
  <trd lng="sv">sj&ouml;</trd>
</drv>
</art>
