<drv mrk="lag.0eto">
  <kap><ofc>*</ofc><tld/>eto <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5082">lag'et'</url></lok></fnt></kap>
  <snc mrk="lag.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malgranda, superrigardebla <tld/>o:
      <ekz>
        oni lavis la &ccirc;aron &ccirc;e la <tld/>eto de Samario
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 22:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili stari&gcirc;is &ccirc;e la akvotubo de la supra <tld/>eto, kiu estas &ccirc;e la vojo de la
        kampo de fulistoj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 18:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        iru, lavu vin en la <tld/>eto de <nom>&Scirc;iloa&hcirc;</nom>
        <fnt><bib>NT</bib>, <lok>&Joh; 9:7</lok></fnt>;
      </ekz>
      <ekz>
        en la vila&gcirc;a <tld/>eto okazis ia grava fari&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovis lian kadavron en la palaca lag<tld/>eto
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_z;&c_ja;&c_r;&c_c;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_je;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_zh;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_k;&c_o; <ind>&c_je;&c_z;&c_je;&c_r;&c_o;</ind></trd>
  <trd lng="bo">&t_cha;&t_shabchu;&t_tsheg;&t_-a;&t_dza;&t_gigu;&t_nga;&t_tsheg;</trd>
  <trd lng="cs">jez&iacute;rko</trd>
  <trd lng="de">Teich</trd>
  <trd lng="en">pond</trd>
  <trd lng="es">laguna</trd>
  <trd lng="fr">&eacute;tang</trd>
  <trd lng="id">kolam</trd>
  <trd lng="ja">&#x6C60;  <pr>&#x3044;&#x3051;</pr></trd>
  <trd lng="nl">vijver</trd>
  <trd lng="sk">jazierko</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_z;&c_je;&c_r;&c_c;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
