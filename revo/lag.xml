<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lag</rad>/o
</kap>

<drv mrk="lag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lag.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vasta etenda&jcirc;o da akvo, &ccirc;iuflanke limigita de tero:
      <ekz>
        la tuta supra&jcirc;o de la <tld/>o estis kovrita per na&gcirc;antaj
        folioj
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
      </ekz>
      <ekz>
        <ref tip="vid" cel="gxenev.0o">&Gcirc;eneva</ref> <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>
  <trd lng="bg">&c_je;&c_z;&c_je;&c_r;&c_o;</trd>
  <trd lng="br">lenn<klr tip="ind"> (dour)</klr></trd>
  <trd lng="cs">jezero</trd>
  <trd lng="de">See</trd>
  <trd lng="el">&lambda;&jota_ton;&my;&ny;&eta;</trd>
  <trd lng="en">lake</trd>
  <trd lng="es">lago</trd>
  <trd lng="fr">lac</trd>
  <trd lng="hu">t&oacute;</trd>
  <trd lng="id">danau</trd>
  <trd lng="it">lago</trd>
  <trd lng="nl">meer<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">jezioro</trd>
  <trd lng="pt">lago</trd>
  <trd lng="ro">lac</trd>
  <trd lng="ru">&c_o;&c_z;&c_je;&c_r;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>jazero</trd>,
    <trd>pleso</trd>
  </trdgrp>
  <trd lng="sv">sj&ouml;</trd>
  <trd lng="tr">g&ouml;l</trd>
  <trd lng="zh">&#28246;<klr> [h&uacute;]</klr></trd>
</drv>

<drv mrk="lag.0eto">
  <kap><ofc>*</ofc><tld/>eto <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5082">lag'et'</url></lok></fnt></kap>
  <snc mrk="lag.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malgranda, superrigardebla <tld/>o:
      <ekz>
        iru, lavu vin en la <tld/>eto de &Scirc;iloa&hcirc;
        <fnt><bib>NT</bib>, <lok>&Joh; 9:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_k;&c_o; <ind>&c_je;&c_z;&c_je;&c_r;&c_o;</ind></trd>
  <trd lng="cs">jez&iacute;rko</trd>
  <trd lng="de">Teich</trd>
  <trd lng="en">pond</trd>
  <trd lng="es">laguna</trd>
  <trd lng="fr">&eacute;tang</trd>
  <trd lng="id">kolam</trd>
  <trd lng="nl">vijver</trd>
  <trd lng="sk">jazierko</trd>
</drv>
</art>
