<drv mrk="lag.0eto">
  <kap><ofc>*</ofc><tld/>eto <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-5082">lag'et'</url></lok></fnt></kap>
  <snc mrk="lag.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malgranda, superrigardebla <tld/>o:
      <ekz>
        oni lavis la &ccirc;aron &ccirc;e la <tld/>eto de Samario
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 22:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili stari&gcirc;is &ccirc;e la akvotubo de la supra <tld/>eto, kiu estas &ccirc;e la vojo de la
        kampo de fulistoj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 18:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        iru, lavu vin en la <tld/>eto de <nom>&Scirc;iloa&hcirc;</nom>
        <fnt><bib>NT</bib>, <lok>&Joh; 9:7</lok></fnt>;
      </ekz>
      <ekz>
        en la vila&gcirc;a <tld/>eto okazis ia grava fari&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovis lian kadavron en la palaca lag<tld/>eto
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_z;&c_ja;&c_r;&c_c;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_je;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_zh;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_k;&c_o; <ind>&c_je;&c_z;&c_je;&c_r;&c_o;</ind></trd>
  <trd lng="bo">&#xF46;&#xF74;&#xF0B;&#xF60;&#xF5B;&#xF72;&#xF44;&#xF0B;</trd>
  <trd lng="cs">jez&iacute;rko</trd>
  <trd lng="de">Teich</trd>
  <trd lng="en">pond</trd>
  <trd lng="es">laguna</trd>
  <trd lng="fr">&eacute;tang</trd>
  <trd lng="id">kolam</trd>
  <trd lng="nl">vijver</trd>
  <trd lng="sk">jazierko</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_z;&c_je;&c_r;&c_c;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="lag.Grandaj_0oj">
  <kap>
    Grandaj <tld lit="L"/>oj
  </kap>
  <snc mrk="lag.Grandaj_0oj.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La kvin grandaj lagoj &ccirc;e la kanada-usona limo, t.e. 
      <refgrp tip="ekz">
         Supra Lago,
         Hurono,
         <ref cel="micxigan.0o.lago">Mi&ccirc;igano</ref>,
         <ref cel="eri.0o">Erio</ref>,
         <ref cel="ontari.O0o">Ontario</ref>
      </refgrp>:
      <ekz>
        la Grandaj <tld lit="L"/>oj <klr>[&#8230;]</klr> formas la plej grandan nesalan
        akvosistemon de la tero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1393">Grandaj Lagoj
          (Nordameriko)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la MNP, liverita de tri usonaj grandaj urbaj regionoj (Nov-Jorko, la regiono de
        Los-An&gcirc;eleso kaj la regiono de la Grandaj <tld lit="L"/>oj), konsistigis 67 %
        de la MNP de ilia lando
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2004/008325.html">Kvin defioj por &ccirc;inaj
          urboj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib;&c_ja; &c_a;&c_z;&c_jo;&c_r;&c_y;</trd>
  <trd lng="de">Gro&szlig;e Seen</trd>
  <trd lng="en">Great Lakes</trd>
  <trd lng="fr">Grands Lacs</trd>
</drv>




<drv mrk="lag.Konstanca_0o">
  <kap>
    Konstanca <tld lit="L"/>o, <var><kap>Boden<tld/>o</kap></var>
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <tld lit="L"/>o trafluata de <ref cel="rejn.R0o">Rejno</ref> en la trilanda regiono inter
      Germanujo, A&ubreve;strujo kaj
      Svislando:
      <ekz>
        la kampara lo&gcirc;antaro, sub la premo de mizeraj vivkondi&ccirc;oj, sendis <klr>[&#8230;]</klr>
        lertajn masonistojn kaj konstrumajstrojn, pentristojn kaj aliajn metiistojn,
        kiuj konstruis la famajn barokajn katedralojn pli-malpli proksime de la
        Konstanca <tld lit="L"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj
          progreso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        subakve na&gcirc;i en la Boden<tld/>o estas samtempe alloge kaj postuleme
        <fnt>
          <vrk><url ref="https://eo.advisor.travel/poi/Bodenlago-425">Bodenlago in K&ouml;chlin -
          Advisor.Travel</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Rejn-Akvofalo estas natura obstaklo, kiu interrompas la &scirc;ipvojon inter la
        norda maro kaj la Boden<tld/>o
        <fnt>
          <vrk><url ref="https://eo.advisor.travel/poi/Rejn-Akvofalo-5350">Rejn-Akvofalo in Laufen -
          Advisor.Travel</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_o;&c_d;&c_e;&c_n;&c_s;&c_k;&c_a;&c_je; &c_v;&c_o;&c_z;&c_je;&c_r;&c_a;</trd>
  <trd lng="de">Bodensee</trd>
  <trd lng="en">Lake Constance</trd>
  <trd lng="fr">Lac de Constance</trd>
</drv>


