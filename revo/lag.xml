  <trd lng="ru">&c_F;&c_i;&c_r;&c_v;&c_a;&c_l;&c_mol;&c_d;&c_sh;&c_t;&c_je;&c_t;&c_s;&c_k;&c_o;&c_je; &c_o;&c_z;&c_je;&c_r;&c_o;</trd>
