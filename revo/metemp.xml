    <trd>metempszich&oacute;zis</trd></trdgrp>
  <trd lng="nl">zielsverhuizing</trd>
  <trd lng="pt">metempsicose</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_je;&c_m;&c_p;&c_s;&c_i;&c_h;&c_o;&c_z;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;
&c_d;&c_u;&c_sh;</trd>
  </trdgrp>
</drv>
</art>
