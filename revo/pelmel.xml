<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pelmel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pelmel.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konfuz.0o">Konfuza</ref> renverso, mikso de
      malsamspecaj a&jcirc;oj:
      <ekz>
        la subita atako <klr>...</klr> &jcirc;etis en <tld/>on la &scirc;ar&gcirc;oselon, la
        armojn, la azenon, la &ccirc;evalon
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 68a, p.
        780a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_l;&c_a;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enchev&ecirc;trement</trd>,
    <trd>pagaille</trd>,
    <trd>p&ecirc;le-m&ecirc;le<klr> (subst..)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kavarod&aacute;s</trd>,
    <trd>z&udblac;rzavar</trd>,
    <trd>felfordul&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>balb&uacute;rdia</trd>,
    <trd>grande <ind>desordem</ind></trd>
  </trdgrp>
</drv>
</art>

