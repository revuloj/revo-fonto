<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pelmel.xml,v 1.12 2019/08/10 12:10:14 revo Exp $">
<kap>
  <rad>pelmel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pelmel.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pelmel.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="konfuz.0o">Konfuza</ref> renverso
      kaj mikso de a&jcirc;oj:
      <ekz>
        la subita atako <klr>[&#8230;]</klr> &jcirc;etis en <tld/>on la
        &scirc;ar&gcirc;oselon, la armojn, la azenon, la &ccirc;evalon
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 68a, p.
        780a</lok></fnt>;
      </ekz>
      <ekz>
        la de&ccirc;ifrado lamis pro grandega semantika <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007315.html">De&ccirc;ifrado de la paskinsula skribo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pot.miks0ajxo.FIG">mikspoto<sncref/></ref>,
      <ref cel="salat.0o.FIG">salato<sncref/></ref>,
      <ref cel="hxaos.0o.FIG">&hcirc;aoso<sncref/></ref>,
      <ref cel="tohuvabohu.0o">tohuvabohuo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_l;&c_a;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nepo&rcaron;&aacute;dek</trd>,
    <trd>p&aacute;t&eacute; p&rcaron;es dev&aacute;t&eacute;</trd>,
    <trd>zmatek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Durcheinander</trd>,
    <trd>Drunter und dr&uuml;ber</trd>
  </trdgrp>
  <trd lng="en">pell-mell</trd>
  <trdgrp lng="es">
    <trd>desbarajuste</trd>,
    <trd>mara&ntilde;a</trd>,
    <trd>confusi&oacute;n</trd>,
    <trd>algarab&iacute;a</trd>,
    <trd>batiburrillo</trd>,
    <trd>enredo</trd>,
    <trd>gatuperio</trd>,
    <trd>jaleo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enchev&ecirc;trement</trd>,
    <trd>pagaille</trd>,
    <trd>p&ecirc;le-m&ecirc;le<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kavarod&aacute;s</trd>,
    <trd>z&udblac;rzavar</trd>,
    <trd>felfordul&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>p&ecirc;le-m&ecirc;le</trd>,
    <trd>dooreen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>balb&uacute;rdia</trd>,
    <trd>grande <ind>desordem</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>galimati&aacute;&scaron;</trd>,
    <trd>zl&aacute;tanina</trd>
  </trdgrp>
</drv>
</art>

