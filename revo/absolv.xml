<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>absolv</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="absolv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="absolv.0i.KAT">
    <uzo tip="fak">KAT</uzo>
    <ref tip="dif" cel="pek.0liberigi">pekliberigi</ref>
    <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
&c_g;&c_r;&c_a;&c_h;&c_ib;</trd>
    <trd lng="en">absolve</trd>
    <trdgrp lng="pl">
      <trd>rozgrzesza&cacute;</trd>
      <trd>odpu&sacute;ci&cacute; grzech</trd>
      <trd>darowa&cacute; win&eogonek;</trd>
    </trdgrp>
    <trd lng="pt">absolver</trd>
    <trd lng="hu">feloldoz</trd>
    <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;
&c_g;&c_r;&c_je;&c_h;&c_i;</trd>
  </snc>
  <snc mrk="absolv.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="kondam.mal0i">malkondamni</ref>
    <rim num="1">
      Ne&gcirc;usta estas la signifo <ctl>plenumi</ctl> (p.p. kurso)
      a&ubreve; <ctl>fini</ctl> (p.p. ekzameno).
    </rim>
    <rim num="2">
      La du sencoj malkoheras: oni pekliberigas pekulon, oni
      malkondamnas senkulpulon. Kaj &gcirc;enerale la vorto aspektas
      kiel malnecesa latina barbara&jcirc;o.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_c;&c_mol;
&c_n;&c_je;&c_v;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_t;&c_y;&c_m;</trd>
    </trdgrp>
    <trd lng="de">freisprechen</trd>
    <trd lng="en">pardon</trd>
    <trd lng="hu">felment</trd>
    <trd lng="pl">u&lstroke;askawi&cacute;</trd>
    <trdgrp lng="pt">
      <trd>absolver</trd>,
      <trd>perdoar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_t;&c_mol;
&c_n;&c_je;&c_v;&c_i;&c_n;&c_o;&c_v;&c_n;&c_y;&c_m;</trd>,
      <trd>&c_o;&c_p;&c_r;&c_a;&c_v;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">oslobodi&tcaron;
<klr>(ob&zcaron;alovan&eacute;ho)</klr></trd>
  </snc>
  <trd lng="es">absolver</trd>
  <trd lng="fr">absoudre</trd>
  <trd lng="nl">vergeven</trd>
  <trd lng="de">vergeben</trd>
  <trd lng="sk">odpusti&tcaron;</trd>
</drv>
<drv mrk="absolv.0o">
  <kap><tld/>o</kap>
  <snc mrk="absolv.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <trdgrp lng="pl">
      <trd>u&lstroke;askawienie</trd>
      <trd>&lstroke;aska</trd>
      <trd>prawo <ind>&lstroke;aski</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="absolv.0o.KAT">
    <trdgrp lng="pl">
      <trd>absolucja</trd>
      <trd>rozgrzeszenie</trd>
      <trd>odpuszczenie<klr> (winy, grzechu)</klr></trd>
      <trd>darowanie<klr> (winy, grzechu)</klr></trd>
    </trdgrp>
  </snc>
</drv>
</art>
