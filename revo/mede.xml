    <trd>Medea</trd>,
    <trd>Medeia</trd>
  </trdgrp>
  <trd lng="sk">Medea</trd>
  <trd lng="uk">&#x41C;&#x435;&#x434;&#x435;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x7C73;&#x8482;&#x4E9A; <pr>m&#x1D0;d&#xEC;y&#xE0;</pr></trd>,
    <trd>&#x7C73;&#x8482;&#x4E9E; <pr>m&#x1D0;d&#xEC;y&#xE0;</pr></trd>
  </trdgrp>
</drv>
