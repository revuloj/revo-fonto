<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: palpeb.xml,v 1.34 2024/02/16 19:24:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>palpebr</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="palpeb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="palpeb.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du moveblaj membranoj, kiuj proksimi&gcirc;ante kovras la
      okulglobon:
      <ekz>
        froti sur la fermitaj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a ha&ubreve;to
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia viza&gcirc;o &scirc;velis de plorado, kaj sur miaj <tld/>oj estas morta ombro
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne ser&ccirc;u kun <tld/>oj mallevitaj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>                     
    <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_k;&c_a;</trd>
    <trd lng="bg">&c_k;&c_l;&c_je;&c_p;&c_a;&c_ch;</trd>
    <trd lng="br">malvenn<klr tip="ind"> (korf.)</klr></trd>
    <trd lng="de">Lid</trd>
    <trd lng="en">eyelid</trd>
    <trd lng="es">p&aacute;rpado</trd>
    <trd lng="fr">paupi&egrave;re</trd>
    <trd lng="hu">szemh&eacute;j</trd>
    <trd lng="it">palpebra</trd>
    <trd lng="nl">ooglid</trd>
    <trd lng="no">&oslash;yelokk</trd>
    <trd lng="pl">powieka</trd>
    <trd lng="pt">p&aacute;lpebra</trd>
    <trd lng="ru">&c_v;&c_je;&c_k;&c_o;</trd>
    <trd lng="sv">&ouml;gonlock</trd>
    <trdgrp lng="zh">
    <trd>&#x773C;&#x7751; <pr>y&#x1CE;nji&#x1CE;n</pr></trd>,
    <trd>&#x773C;&#x77BC; <pr>y&#x1CE;nji&#x1CE;n</pr></trd>,
    <trd>&#x773C;&#x76AE; <pr>y&#x1CE;np&#xED;</pr></trd>,
    <trd>&#x7751; <pr>ji&#x1CE;n</pr></trd>,
    <trd>&#x77BC; <pr>ji&#x1CE;n</pr></trd>,
    <trd>&#x773C;&#x76AE;&#x5B50; <pr>y&#x1CE;np&#xED;zi</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="palpeb.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Okulo:
      <ekz>
        viaj <tld/>oj direkti&gcirc;u rekte anta&ubreve;en
        <fnt><bib>MT</bib>, <lok>&Sen; 4:25</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_k;&c_a;</trd>
    <trd lng="nl">oog</trd>
    <trd lng="no">&oslash;ye</trd>
    <trd lng="pt">olho</trd>
  </snc>
  <trd lng="bo">&#xF41;&#xF0B;&#xF42;&#xF45;&#xF7C;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>o&ccaron;n&iacute; v&iacute;&ccaron;ko</trd>,
    <trd>palpebra</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x307E;&#x3076;&#x305F;</trd>,
    <trd>&#x773C;&#x77BC;  <pr>&#x304C;&#x3093;&#x3051;&#x3093;</pr></trd>,
    <trd>&#x76EE;  <pr>&#x3082;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mihalnica</trd>,
    <trd>vie&ccaron;ko</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x432;&#x456;&#x43A;&#x430;</trd>
</drv>

<drv mrk="palpeb.0umi">
  <kap><ofc>*</ofc><tld/>umi <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5139">palpebr'um'</url></lok></fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="palpeb.0umi.MIEN">
    <dif>
      Rapide fermi kaj malfermi la <tld/>ojn,
      &ccirc;u por ripozigi la okulon, &ccirc;u por interkonsente signi kun iu:
      <ekz>
        la Greko restis senmova, e&ccirc; ne <tld/>umis
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#x2026;]</klr> <tld/>umis kun ia filino de komercisto
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 7a</lok></fnt>;
      </ekz>
      <ekz>
        li <tld/>umas petole kaj montras per la okuloj la akompanatinon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> elpalpebrumis al si la akvon el la okuloj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sekvo de ofta <tld/>umado la larma fluida&jcirc;o kun la eliga&jcirc;o de la Meibomaj
        glandetoj per bati&gcirc;ado fari&gcirc;as speco de &scirc;a&ubreve;ma emulsio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="dauxr.enpalpebruma0o.KAL">en <tld/>uma
        da&ubreve;ro</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sign.okul0i.MIEN">okulsigni</ref>,
      <ref cel="okul.0i.KOMUNE">okulumi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_p;&c_a;&c_d;&c_m;&c_ib;&c_r;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_o;&c_r;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_i;&c_g;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_i;&c_g;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">gwilc'ha&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>mrknout</trd>,
    <trd>zamrkat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zwinkern</trd>,
    <trd>blinzeln</trd>
  </trdgrp>
  <trd lng="en">wink</trd>
  <trdgrp lng="es">
    <trd>parpadear</trd>,
    <trd>gui&ntilde;ar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cligner de l'&oelig;il</trd>,
    <trd>cligner des yeux</trd>
  </trdgrp>
  <trd lng="hu">pislog</trd>
  <trdgrp lng="it">
    <trd>ammiccare</trd>,
    <trd><ind>sbattere</ind> le palpebre</trd>,
    <trd><ind>strizzare</ind> gli occhi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x307E;&#x3070;&#x305F;&#x304D;&#x3059;&#x308B;</trd>,
    <trd>&#x76EE;&#x304F;&#x3070;&#x305B;&#x3059;&#x308B;  <pr>&#x3081;&#x304F;&#x3070;&#x305B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x70B9;&#x6EC5;&#x3059;&#x308B;  <pr>&#x3066;&#x3093;&#x3081;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">knipogen</trd>
  <trd lng="no">blinke <klr>(med &oslash;ynene)</klr></trd>
  <trd lng="pl">mru&zdot;y&cacute; <klr>(oczy)</klr></trd>
  <trdgrp lng="pt">
    <trd>piscar</trd>, 
    <trd>pestanejar</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_i;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&zcaron;murka&tcaron;</trd>
  <trd lng="sv">blinka<klr> (med &ouml;gonen)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x456;&#x43F;&#x430;&#x442;&#x438; &#x43E;&#x447;&#x438;&#x43C;&#x430;</trd>,
    <trd>&#x431;&#x43B;&#x438;&#x43C;&#x430;&#x442;&#x438; &#x43E;&#x447;&#x438;&#x43C;&#x430;</trd>,
    <trd>&#x43C;&#x43E;&#x440;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x43C;&#x43E;&#x440;&#x433;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7728;&#x773C; <pr>zh&#x1CE;y&#x1CE;n</pr></trd>,
    <trd>&#x772F; <pr>m&#x12B;</pr></trd>,
    <trd>&#x7787; <pr>m&#x12B;</pr></trd>,
    <trd>&#x6251;&#x95EA; <pr>p&#x16B;sh&#x1CE;n</pr></trd>,
    <trd>&#x64B2;&#x9583; <pr>p&#x16B;sh&#x1CE;n</pr></trd>,
    <trd>&#x7728; <pr>zh&#x1CE;</pr></trd>,
    <trd>&#x772F; <pr>m&#x1D0;</pr></trd>,
    <trd>&#x7787; <pr>m&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
