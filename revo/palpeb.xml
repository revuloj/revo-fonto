<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>palpebr</rad>/o</kap>
<drv mrk="palpeb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du moveblaj membranoj, kiuj proksimi&gcirc;ante kovras la
      okulglobon.
    </dif>                     
    <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_k;&c_a;</trd>
    <trd lng="br">malvenn<klr tip="ind"> (korf.)</klr></trd>
    <trd lng="de">Lid</trd>
    <trd lng="en">eyelid</trd>
    <trd lng="fr">paupi&egrave;re</trd>
    <trd lng="hu">szemh&eacute;j</trd>
    <trd lng="nl">ooglid</trd>
    <trd lng="pl">powieka</trd>
    <trd lng="ru">&c_v;&c_je;&c_k;&c_o;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Okulo:
      <ekz>
        viaj <tld/>oj direkti&gcirc;u rekte anta&ubreve;en<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_k;&c_a;</trd>
  </snc>
  <trd lng="nl">oog</trd>
</drv>

<drv mrk="palpeb.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Rapide fermi kaj malfermi la <tld/>ojn &ccirc;u por ripozigi, la okulon,
      &ccirc;u por interkonsente signi kun iu:
      <ekz>
        en <tld/>uma da&ubreve;ro
        <klr>(tre rapide)</klr>;
      </ekz>
      <ekz>
        mi <tld/>umis kun la filino de komercisto<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="okul">okulumi</ref>.
    </refgrp>
    <trd lng="en">wink</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_p;&c_a;&c_d;&c_m;&c_ib;&c_r;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_o;&c_r;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>   
  <trd lng="br">gwilc'ha&ntilde;</trd>
  <trdgrp lng="de">
    <trd>zwinkern</trd>,
    <trd>blinzeln</trd>
  </trdgrp>
  <trd lng="fr">cligner de l'&oelig;il</trd>
  <trd lng="hu">pislog</trd>
  <trd lng="nl">knipogen</trd>
  <trd lng="pl">mru&zdot;y&cacute; <klr>(oczy)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_i;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
