<drv mrk="kler.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="kler.0a.PED">
    <dif>
      Kapabla pro instruiteco kaj kulturo ju&gcirc;i klare kaj rekte:
      <ekz>
        &ccirc;iu <tld/>a homo ja anka&ubreve; nun devas ellernadi
        kelkajn fremdajn lingvojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        granda socia problemo, eble granda maljusta&jcirc;o
        <klr>[&#x2026;]</klr> aperis anta&ubreve; la pensoj de la bona
        kaj klera homo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        al neniu <tld/>a homo en la mondo la nomo de nia afero nun estas
        jam nekonata
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en
          Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>osv&iacute;cen&yacute;</trd>,
    <trd>vzd&ecaron;lan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gebildet</trd>,
    <trd>gelehrt</trd>,
    <trd>aufgekl&auml;rt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>educated</trd>,
    <trd>cultured</trd>,
    <trd>enlightened</trd>,
    <trd>learned</trd>,
    <trd>well <ind>informed</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>culto</trd>,
    <trd>instruido</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cultiv&eacute;<klr tip="ind"> (ayant de la
    culture)</klr></trd>,
    <trd>&eacute;clair&eacute;<klr tip="ind">
    (cultiv&eacute;)</klr></trd>,
    <trd>instruit<klr tip="ind"> (cultiv&eacute;)</klr></trd>,
    <trd>lettr&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>savant<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;velt</trd>,
    <trd>k&eacute;pzett</trd>,
    <trd>kultur&aacute;lt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5B66;&#x8B58;&#x306E;&#x3042;&#x308B;
    <pr>&#x304C;&#x304F;&#x3057;&#x304D;&#x306E;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x6559;&#x990A;&#x3042;&#x308B; <pr>&#x304D;&#x3087;&#x3046;&#x3088;&#x3046;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontwikkeld</trd>,
    <trd>geleerd</trd>,
    <trd>knap</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;wiat&lstroke;y</trd>,
    <trd>wykszta&lstroke;cony</trd>,
    <trd>o&sacute;wiecony</trd>
  </trdgrp>
