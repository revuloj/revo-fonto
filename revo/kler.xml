<drv mrk="kler.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kler.0igi.PED">
    <dif>
      <ref tip="super" cel="instru.0i">Instrui</ref> iun pri kulture
      utilaj temoj:
      <ekz>
        la <tld/>igantoj de la popolo
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 11:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu min, por ke mi vivu
        <fnt>
        <bib>MT</bib>,
        <lok>&Psa; 119:144</lok>
        </fnt>;
      </ekz>
      <ekz>
        talento ne sole natura, sed anka&ubreve; <tld/>igita
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>igi iun pri matematiko;
      </ekz>
      <ekz>
        ministro pri publika <ind><tld/>igado</ind>.
        <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
        <trdgrp lng="fr">
          <trd>instruction<klr tip="ind"> (fait
          d'instruire)</klr></trd>,
          <trd>&eacute;ducation</trd>,
          <trd>enseignement</trd>
        </trdgrp>
        <trd lng="nl">onderwijs</trd>
        <trd lng="hu">k&ouml;zm&udblac;vel&odblac;d&eacute;si
        miniszter</trd>
        <trd lng="pl">o&sacute;wiata</trd>
        <trdgrp lng="ro">
          <trd>educa&#x21B;ie</trd>,
          <trd>&icirc;nv&abreve;&#x21B;&abreve;m&acirc;nt</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vzd&ecaron;l&aacute;vat</trd>,
    <trd>&scaron;&iacute;&rcaron;it osv&ecaron;tu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bilden</trd>,
    <trd>ausbilden</trd>,
    <trd>belehren</trd>,
    <trd>aufkl&auml;ren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>educate</trd>,
    <trd>enlighten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cultiver<klr tip="ind"> (r&eacute;pandre la
    culture)</klr></trd>,
    <trd>&eacute;duquer</trd>,
    <trd>instruire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;vel</trd>,
    <trd>k&eacute;pez</trd>
  </trdgrp>
  <trd lng="ja">&#x5553;&#x767A;&#x3059;&#x308B;  <pr>&#x3051;&#x3044;&#x306F;&#x3064;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>scholen</trd>,
    <trd>onderwijzen</trd>
  </trdgrp>
  <trd lng="pl">kszta&lstroke;ci&cacute;</trd>
  <trd lng="ro">educa</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pou&ccaron;ova&tcaron;</trd>,
    <trd>vzdel&aacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_v;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43E;&c_s;&c_v;&c_ib;&c_ch;&c_je;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&c_v;&c_i;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6559;&#x5BFC; <pr>ji&#xE0;od&#x1CE;o</pr></trd>,
    <trd>&#x57F9;&#x8A13; <pr>p&#xE9;ix&#xF9;n</pr></trd>
  </trdgrp>
