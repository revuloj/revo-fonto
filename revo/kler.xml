<drv mrk="kler.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kler.0eco.PED">
    <dif>
      Kompetento pri kultura temo:
      <ekz>
        observi la &gcirc;ustan mezuron estas arto,
        kiu montras altan <tld/>econ<fnt>Z</fnt>;
      </ekz>
      <ekz>
        artisma <tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        artista <tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        muzika <tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun fini&gcirc;as mia <tld/>eco
        <klr>(<ref tip="vid" cel="fin.0o">fino</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sciencon oni ne mendas, <tld/>econ oni ne vendas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        malproksime de tiuj bonfaroj, rimedoj kaj iloj, kiujn al homoj
        donas <tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        modelo de la moroj kaj <tld/>eco
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kompet.0o">kompetento</ref>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>. vzd&ecaron;l&aacute;n&iacute;</trd>,
    <trd>osv&iacute;cenost</trd>,
    <trd>osv&ecaron;ta</trd>,
    <trd>vzd&ecaron;lanost</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bagage<klr tip="ind"> (culturel)</klr></trd>,
    <trd>culture<klr tip="ind"> (personnelle)</klr></trd>,
    <trd>instruction<klr tip="ind"> (fait d'&ecirc;tre
    instruit)</klr></trd>
  </trdgrp>
  <trd lng="hu">zenei m&udblac;velts&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x990A;  <pr>&#x304D;&#x3087;&#x3046;&#x3088;&#x3046;</pr></trd>,
    <trd>&#x5B66;&#x8B58;  <pr>&#x304C;&#x304F;&#x3057;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontwikkeling</trd>,
    <trd>scholing</trd>,
    <trd>kennis</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>jasno&sacute;&cacute;</trd>,
    <trd>o&sacute;wiecenie</trd>,
    <trd>wykszta&lstroke;cenie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>educa&#x21B;ie</trd>,
    <trd>iluminism</trd>,
    <trd>claritate</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_jo;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>osvietenos&tcaron;</trd>,
    <trd>vzdelanos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
