<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kler</rad>/a
</kap>
<drv mrk="kler.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Instruita; kapabla pro instruiteco ju&gcirc;i klare kaj rekte; kulturita:
      <ekz>akademie <tld/>a homo<fnt>Z</fnt>;</ekz>
      <ekz>
        observi la &gcirc;ustan mezuron estas arto,
        kiu montras altan <tld/>econ<fnt>Z</fnt>;
      </ekz>
      <ekz>artisma<fnt>Z</fnt>,</ekz>
      <ekz>artista<fnt>Z</fnt>,</ekz>
      <ekz>
        muzika <ind><tld/>eco</ind><fnt>Z</fnt>;
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
          <trd>&c_u;&c_ch;&c_jo;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        nun fini&gcirc;as mia <tld/>eco<fnt>Z</fnt>
        (<ref tip="vid" cel="fin.0o">fino</ref>);
      </ekz>
      <ekz>
        sciencon oni ne mendas, <tld/>econ oni ne 
        vendas<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>educated</trd>,
      <trd>cultured</trd>,
      <trd>enlighntened</trd>,
      <trd>learned</trd>,
      <trd>well <ind>informed</ind></trd>
    </trdgrp>
    <trd lng="nl">geleerd</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="kler.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun <tld/>a:
      <ekz>la <tld/>igantoj de la popolo<fnt>Z</fnt>;</ekz>
      <ekz>elemente <tld/>igita homo;</ekz>
      <ekz>talento ne sole natura, sed anka&ubreve; <tld/>igita<fnt>Z</fnt>;</ekz>
      <ekz>ek<tld/>igi iun pri matematiko;</ekz>
      <ekz>
        ministro pri publika <ind><tld/>igado</ind>.
        <trd lng="nl">onderwijs</trd>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>educate</trd>,
    <trd>enlighten</trd>
  </trdgrp>
  <trd lng="nl">onderwijzen</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="kler.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Tute kruda, ne instruita.
    </dif>
    <refgrp tip="sin">
      <ref cel="analfa.0o">analfabeto</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>barbarous</trd>,
      <trd>ignorant</trd>,
      <trd>illiterate</trd>,
      <trd>simple</trd>,
      <trd>uncultured</trd>,
      <trd>uneducated</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
