<drv mrk="lidi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>,
      devena de la antikva regiono <ref tip="hom" cel="lidi.0o">Lidio</ref>:
      <ekz>
        li ja estas konstante &ccirc;e <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        Horacio kaj <tld/>o
        <fnt>
          <aut>D-ro Vallienne</aut>
          <vrk><url ref="D-ro Vallienne"
              >Literatura Almanako 1910 de Lingvo Internacia</url></vrk>
          <lok>Horacio: Odo XIa, Libro IIIa.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lydia</trd>
    <trd lng="en">Lydia</trd>
    <trd lng="nl">Lydia</trd>
  </snc>
</drv>

  </art>

