<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: lidi1.xml,v 1.4 2023/09/01 19:10:59 afido Exp $">
    <kap>
      <rad>Lidi</rad>/o
    </kap>

<drv mrk="lidi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>,
      devena de la antikva regiono <ref tip="hom" cel="lidi.0o">Lidio</ref>:
      <ekz>
        li ja estas konstante &ccirc;e <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        Horacio kaj <tld/>o
        <fnt>
          <aut>D-ro Vallienne</aut>
          <vrk><url ref="D-ro Vallienne">Literatura Almanako 1910 de Lingvo Internacia</url></vrk>
          <lok>Horacio: Odo XIa, Libro IIIa.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_L;&c_ib;&c_d;&c_z;&c_ib;&c_ja;</trd>,
      <trd>&c_L;&c_ib;&c_d;&c_a; <klr tip="amb">(&c_ib;&c_m;&c_ja;)</klr></trd>
    </trdgrp>
    <trd lng="de">Lydia</trd>
    <trd lng="en">Lydia</trd>
    <trd lng="nl">Lydia</trd>
  </snc>
  <trd lng="ja">&#x30EA;&#x30C7;&#x30A3;&#x30A2;</trd>
</drv>

  </art>

