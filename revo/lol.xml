<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lol.xml,v 1.21 2023/09/02 05:33:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lol</rad>/o
</kap>
<drv mrk="lol.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="poac.0oj">poacoj</ref> (<trd lng="la">Lolium</trd>),
      multe kreskanta sur pa&scirc;tejoj kaj gazonoj kaj kies semoj
      estas venenaj pro mikroskopa fungo:
      <ekz>
        lia malamiko venis kaj semis <tld/>on meze de la <ref cel="tritik.0o">tritiko</ref>
        <fnt><bib>NT</bib>, <lok>&Mat; 13:25</lok></fnt>.
      </ekz>
      <ekz>
        en lia tritiko, sur kiu li fondis grandajn esperojn, aperis <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tritiko estas io kulturita, artefarita, dum la <tld/>o estas io tute natura
        <fnt>
          <bib>Lanti</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="festuk.0o">festuko</ref>
    <bld lok="&WCU;/thumb/f/fe/Lolium_perenne_flowers%2C_engels_raaigras_bloeiend_aartjes.jpg/360px-Lolium_perenne_flowers%2C_engels_raaigras_bloeiend_aartjes.jpg" prm="CC BY-SA 3.0">spiketoj de <tld/>o (Lolium perenne)
        <fnt>
          <aut>Rasbak, propra verko</aut>
          <lok><url ref="&WCW;/File:Lolium_perenne_flowers,_engels_raaigras_bloeiend_aartjes.jpg">&WCW;/File:Lolium_perenne_flowers,_engels_raaigras_bloeiend_aartjes.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_c;&c_ib;&c_k;</trd>   
  <trd lng="br">dreog(enn)</trd>
  <trdgrp lng="de">
    <trd>Weidelgras</trd>,
    <trd>Lolch</trd>,
    <trd>Raygras</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rye grass</trd>,
    <trd>way-bent</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ivraie</trd>,
    <trd>ray-grass</trd>
  </trdgrp>
  <trd lng="hu">vad&oacute;c<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30C9;&#x30AF;&#x30E0;&#x30AE;</trd>,
    <trd>&#x30E9;&#x30A4;&#x30B0;&#x30E9;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>raaigras</trd>,
    <trd>dolik</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&aogonek;kol</trd>,
    <trd>&zdot;ycica</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>neghin&abreve;</trd>,
    <trd>zizanie </trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_v;&c_je;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_j;&c_g;&c_r;&c_a;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x436;&#x438;&#x442;&#x43D;&#x438;&#x446;&#x44F; &#x43F;&#x2019;&#x44F;&#x43D;&#x43A;&#x430;</trd>,
    <trd>&#x434;&#x443;&#x440;&#x456;&#x439;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x443;&#x43A;&#x456;&#x43B;&#x44C;</trd>,
    <trd>&#x431;&#x443;&#x440;&#x2019;&#x44F;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
