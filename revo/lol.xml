<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lol</rad>/o
</kap>
<drv mrk="lol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Lolium</trd>),
      multe kreskas sur pa&scirc;tejoj kaj gazonoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Weidelgras</trd>,
    <trd>Lolch</trd>,
    <trd>Raygras</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>rye grass</trd>,
     <trd>way-bent</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>raaigras</trd>,
    <trd>dolik</trd></trdgrp>
</drv>
</art>
