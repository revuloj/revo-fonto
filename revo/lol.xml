<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lol.xml,v 1.17 2020/02/09 12:10:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lol</rad>/o
</kap>
<drv mrk="lol.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Lolium</trd>),
      multe kreskas sur pa&scirc;tejoj kaj gazonoj:
      <ekz>lia malamiko venis kaj semis <tld/>on meze de la tritiko
      <fnt><bib>NT</bib>, <lok>&Mat; 13:25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_c;&c_ib;&c_k;</trd>   
  <trd lng="br">dreog(enn)</trd>
  <trdgrp lng="de">
    <trd>Weidelgras</trd>,
    <trd>Lolch</trd>,
    <trd>Raygras</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rye grass</trd>,
    <trd>way-bent</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ivraie</trd>,
    <trd>ray-grass</trd>
  </trdgrp>
  <trd lng="hu">vad&oacute;c<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  <trdgrp lng="nl">
    <trd>raaigras</trd>,
    <trd>dolik</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&aogonek;kol</trd>,
    <trd>&zdot;ycica</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>neghin&abreve;</trd>,
    <trd>zizanie </trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_v;&c_je;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_j;&c_g;&c_r;&c_a;&c_s;</trd>
  </trdgrp>
</drv>
</art>
