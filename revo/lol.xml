<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lol</rad>/o
</kap>
<drv mrk="lol.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Lolium</trd>),
      multe kreskas sur pa&scirc;tejoj kaj gazonoj:
      <ekz>lia malamiko venis kaj semis <tld/>on meze de la tritiko
      <fnt><bib>NT</bib>, <lok>&Mat; 13:25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Weidelgras</trd>,
    <trd>Lolch</trd>,
    <trd>Raygras</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rye grass</trd>,
    <trd>way-bent</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ivraie</trd>,
    <trd>ray-grass</trd>
  </trdgrp>
  <trd lng="hu">vad&oacute;c<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  <trdgrp lng="nl">
    <trd>raaigras</trd>,
    <trd>dolik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_v;&c_je;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_j;&c_g;&c_r;&c_a;&c_s;</trd>
  </trdgrp>
</drv>
</art>
