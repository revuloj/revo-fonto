<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kotangent</rad>/o
</kap>
<drv mrk="kotang.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> &alfa;)</klr>
      <ref cel="tangen.0o">Tangento</ref> de &gcirc;ia
      <ref cel="komple1.0o.angulo">komplemento</ref>; simb. cotg&#xa0;&alfa;
      a&ubreve; cot&#xa0;&alfa; :
      <ekz>
        la <tld/>o de angulo estas anka&ubreve; la inverso de &gcirc;ia
        tangento.
      </ekz>
    </dif>
    <bld lok="../bld/trigo.jpg">
      la <tld/>o de angulo &alfa; egalas al BU/OM
    </bld>
    <trd lng="de">Kotangens</trd>
    <trd lng="en">cotangent</trd>
    <trd lng="fr">cotangente</trd>
    <trd lng="ru">&c_k;&c_o;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  </snc>
</drv>
</art>
