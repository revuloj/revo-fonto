    <trd>&#x6C34;&#x816B;  <pr>&#x3059;&#x3044;&#x3057;&#x3085;</pr></trd>,
    <trd>&#x3080;&#x304F;&#x307F;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>v&yacute;krojok</trd>,
    <trd>z&aacute;rez</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x43E;&#x434;&#x44F;&#x43D;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x6C34;&#x80BF; <pr>shu&#x1D0;zh&#x1D2;ng</pr></trd>
