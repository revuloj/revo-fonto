<kap>
  <rad>hidrops</rad>/o <fnt><bib>JED</bib></fnt>
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hidrops.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="hidrops.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="edem.0o.MED">Edemo</ref>:
      <ekz>
        jen anta&ubreve; li estis unu <tld/>ulo
        <fnt>
          <bib>NT</bib>
          <lok>Luko 14:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la kadavro]</klr> estis &scirc;velinta, pli ol &ccirc;e
        mortinto pro <tld/>o
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&Ouml;dem</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_y;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ed&eacute;m</trd>,
    <trd>hydrops</trd>,
    <trd>otok</trd>,
    <trd>vodnatelnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hydrops</trd>,
    <trd>Wassersucht</trd>
  </trdgrp>
  <trd lng="fr">hydropisie</trd>
  <trdgrp lng="ja">
    <trd>&#x6C34;&#x75C7;  <pr>&#12377;&#12356;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x6C34;&#x816B;  <pr>&#12377;&#12356;&#12375;&#12421;</pr></trd>,
    <trd>&#12416;&#x304F;&#x307F;</trd>
  </trdgrp>
  <trd lng="pl">puchlina wodna</trd>
  <trdgrp lng="sk">
    <trd>v&yacute;krojok</trd>,
    <trd>z&aacute;rez</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&#x43E;&c_d;&#x44F;&#x43D;&#x43A;&c_a;</trd>
  <trd lng="zh">&#x6C34;&#x80BF; <pr>shu&#x1D0;zh&#x1D2;ng</pr></trd>
