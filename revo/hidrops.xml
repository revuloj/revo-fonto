<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hidrops.xml,v 1.4 2019/10/07 19:10:18 revo Exp $">
<kap>
  <rad>hidrops</rad>/o <fnt><bib>NT</bib></fnt>
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hidrops.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="hidrops.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="edem.0o.MED">Edemo</ref>:
      <ekz>
        jen anta&ubreve; li estis unu <tld/>ulo
        <fnt>
          <bib>NT</bib>
          <lok>Luko 14:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la kadavro]</klr> estis &scirc;velinta, pli ol &ccirc;e 
        mortinto pro <tld/>o
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&Ouml;dem</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ed&eacute;m</trd>,
    <trd>hydrops</trd>,
    <trd>otok</trd>,
    <trd>vodnatelnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hydrops</trd>,
    <trd>Wassersucht</trd>
  </trdgrp>
  <trd lng="fr">hydropisie</trd>
  <trdgrp lng="sk">
    <trd>v&yacute;krojok</trd>,
    <trd>z&aacute;rez</trd>
  </trdgrp>
</drv>
</art>

