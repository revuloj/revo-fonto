<drv mrk="teg.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="teg.0ajxo.tego">
    <dif>
      Tio, kio tegas<sncref ref="teg.0i.alglui"/>; <ref tip="dif" cel="teg.0o">tego</ref>:
      <ekz>
        kolonojn li konstruis el ar&gcirc;ento <klr>[&#x2026;]</klr> la <tld/>a&jcirc;on el purpuro
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o de la malnova se&gcirc;o en la angulo krevis
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la modeloj de la <ind>tabul<tld/>a&jcirc;o</ind> 
        <klr>(vd <ref cel="panel.0o.ARKI">panelo<sncref/></ref>, 
        <ref cel="tabul.0ajxo">tabula&jcirc;o</ref>)</klr>
        kaj fenestroj malka&scirc;as la konstrutempon
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php">Tammisaari, urbeto &ccirc;e Balta Maro</url></vrk>
          <lok>2014</lok>
        </fnt>;
        <trd lng="de">T&auml;felung</trd>
      </ekz>
      <ekz>
        malgranda para&scirc;uto, belarte farita kaj <tld/>ita per kolora silko <klr>[&#x2026;]</klr>
        oni faldas la ombrelan <tld/>a&jcirc;on &ccirc;irka&ubreve; la stangeto kaj enmetas la tuton en
        tubeton
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stern.0ajxo">sterna&jcirc;o</ref>
    </refgrp>
    <trd lng="de">&Uuml;berzug</trd>
    <trdgrp lng="en">
      <trd>covering</trd>,
      <trd>surface</trd>,
      <trd>coating</trd>
    </trdgrp>
    <trd lng="fa">&a_r;&a_w;&f_k;&a_sh;</trd>
    <trdgrp lng="hu">
      <trd>huzat</trd>,
      <trd>bevonat</trd>,
      <trd>burkolat</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pokrycie</trd>,
      <trd>pow&lstroke;oka</trd>,
      <trd>obicie</trd>,
      <trd>p&lstroke;aszcz<klr> (tech., np. kabla)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="teg.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de subaro)</klr>
      La plej malgranda aro kun la dezirataj ecoj, kiu &gcirc;in
      inkluzivas.
    </dif>
    <subsnc mrk="teg.0ajxo.konveksa">
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(<ref cel="konvek.0a.subaro">konveksa</ref>
        <tld/>a&jcirc;o)</klr>
        <ekz>
          konveksa surfaco a&ubreve; solido estas konveksa
          <tld/>a&jcirc;o de &gcirc;ia
          <ref cel="rand.0o.figuro">rando</ref>;
        </ekz>
        <ekz>
          la konveksa <tld/>a&jcirc;o de subaro konsistas el &ccirc;iuj
          <ref cel="centr.pezo0o.MAT">pezocentroj</ref> de &gcirc;iaj
          elementoj kun pozitivaj koeficientoj.
        </ekz>
      </dif>
      <trd lng="de">konvexe <ind>H&uuml;lle</ind></trd>
      <trd lng="en">convex <ind>hull</ind></trd>
      <trd lng="fa">&a_gh;&a_l;&a_A;&a_f; &a_m;&a_H;&a_d;&a_b;</trd>
      <trd lng="fr">enveloppe convexe</trd>
      <trd lng="hu">konvex <ind>burok</ind></trd>      
      <trd lng="pl">pow&lstroke;oka wypuk&lstroke;a</trd>
      <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_a;&c_ja; <ind>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
    </subsnc>
    <subsnc mrk="teg.0ajxo.lineara">
      <dif>
        <klr>(<ref cel="linear.0a.gxenerala">lineara</ref> 
        <tld/>a&jcirc;o)</klr>
        <ekz>
          la lineara <tld/>a&jcirc;o de subaro konsistas el &ccirc;iuj
          <ref cel="kombin.lineara0ajxo">linearaj kombina&jcirc;oj</ref>
