    <trd>&c_f;&c_u;&c_t;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ib;&c_c;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
