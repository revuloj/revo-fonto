    <trd>&c_a;&c_b;&c_l;&c_ib;&c_c;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>  <trdgrp lng="cs">
    <trd>kryt</trd>,
    <trd>potah</trd>,
    <trd>povlak</trd>,
    <trd>povle&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bezug</trd>,
    <trd>&Uuml;berzug</trd>
  </trdgrp><trdgrp lng="en">
    <trd>cover</trd>,
    <trd>wrapping</trd>,
    <trd>coating</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_w;&f_k;&a_sh;</trd>
  <trdgrp lng="hu">
    <trd>huzat</trd>,
    <trd>bevonat</trd>,
    <trd>burkolat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35206;&#12356;  <pr>&#x304A;&#x304A;&#12356;</pr></trd>,
    <trd>&#x30AB;&#x30D0;&#x30FC;  <pr>&#x304B;&#12400;&#x30FC;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
