</snc>
  <snc mrk="teg.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de subaro)</klr>
      La plej malgranda aro kun la dezirataj ecoj, kiu &gcirc;in
      inkluzivas.
    </dif>
    <subsnc mrk="teg.0ajxo.konveksa">
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(<ref cel="konvek.0a.subaro">konveksa</ref>
        <tld/>a&jcirc;o)</klr>
        <ekz>
          konveksa surfaco a&ubreve; solido estas konveksa
          <tld/>a&jcirc;o de &gcirc;ia
          <ref cel="rand.0o.figuro">rando</ref>;
        </ekz>
        <ekz>
          la konveksa <tld/>a&jcirc;o de subaro konsistas el &ccirc;iuj
          <ref cel="centr.pezo0o.MAT">pezocentroj</ref> de &gcirc;iaj
          elementoj kun pozitivaj koeficientoj.
        </ekz>
      </dif>
      <trd lng="be">&c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a; &c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_a;&c_ja;</trd>
      <trd lng="de">konvexe <ind>H&uuml;lle</ind></trd>
      <trd lng="en">convex <ind>hull</ind></trd>
      <trd lng="fa">&a_gh;&a_l;&a_A;&a_f; &a_m;&a_H;&a_d;&a_b;</trd>
      <trd lng="fr">enveloppe convexe</trd>
      <trd lng="hu">konvex <ind>burok</ind></trd>
      <trd lng="pl">pow&lstroke;oka wypuk&lstroke;a</trd>
      <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_a;&c_ja; <ind>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
    </subsnc>
    <subsnc mrk="teg.0ajxo.lineara">
      <dif>
        <klr>(<ref cel="linear.0a.gxenerala">lineara</ref>
        <tld/>a&jcirc;o)</klr>
        <ekz>
          la lineara <tld/>a&jcirc;o de subaro konsistas el &ccirc;iuj
          <ref cel="kombin.lineara0ajxo">linearaj kombina&jcirc;oj</ref>
          de &gcirc;iaj elementoj;
        </ekz>
        <ekz>
          &gcirc;i estas la submodulo naskita de la koncerna subaro.
        </ekz>
      </dif>
      <trd lng="be">&c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a; &c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja;</trd>
      <trd lng="de">lineare <ind>H&uuml;lle</ind></trd>
      <trd lng="en">linear <ind>closure</ind></trd>
      <trdgrp lng="fa">
        <trd>&a_gh;&a_l;&a_A;&a_f; &a_kh;&a_T;&f_y;</trd>,
        <trd>&a_b;&a_s;&a_t;&a_A;&a_r; &a_kh;&a_T;&f_y;</trd>
      </trdgrp>
      <trd lng="fr">enveloppe lin&eacute;aire</trd>
      <trd lng="hu">line&aacute;ris <ind>burok</ind></trd>
      <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja; <ind>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
    </subsnc>
    <subsnc mrk="teg.0ajxo.algebra">
      <fnt><bib>EKV</bib><lok>&para;18</lok></fnt>
      <dif>
        <klr>
        (algebra <tld/>a&jcirc;o de <ref cel="korp.0o.MAT">korpo</ref>
        <frm><g>K</g></frm> en superkorpo <frm><g>K'</g></frm> de
        &gcirc;i)</klr>
        La aro de &ccirc;iuj
        <ref cel="algebr.0a.rilate_subkorpon">algebraj</ref>
        super <frm><g>K</g></frm> elementoj de <frm><g>K'</g></frm>:
        <ekz>
          la algebra <tld/>a&jcirc;o de <frm><g>K</g></frm> estas
          <ref cel="algebr.0a.superkorpo">algebra</ref> superkorpo de
          &gcirc;i;
        </ekz>
        <ekz>
          korpo, kiu egalas al sia algebra <tld/>a&jcirc;o, estas
          algebre fermita.
        </ekz>
      </dif>
      <trd lng="be">&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_a;&c_l;&c_mol;&c_g;&c_je;&c_b;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;</trd>
      <trd lng="de">algebraische <ind>H&uuml;lle</ind></trd>
      <trd lng="en">algebraic <ind>closure</ind></trd>
      <trd lng="fa">&a_b;&a_s;&a_t;&a_A;&a_r; &a_j;&a_b;&a_r;&f_y;</trd>
      <trdgrp lng="fr">
        <trd>cl&ocirc;ture alg&eacute;brique</trd>,
        <trd>fermeture alg&eacute;brique</trd></trdgrp>
      <trd lng="hu">algebrai <ind>lez&aacute;rt</ind></trd>
      <trd lng="pl">domkni&eogonek;cie algebraiczne</trd>
      <trd lng="ru">&c_a;&c_l;&c_g;&c_je;&c_b;&c_r;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
      <ind>&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_i;&c_je;</ind></trd>
    </subsnc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  </snc>

