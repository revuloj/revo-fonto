    <trd>potah</trd>,
    <trd>povlak</trd>,
    <trd>povle&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bezug</trd>,
    <trd>&Uuml;berzug</trd>
  </trdgrp><trdgrp lng="en">
    <trd>covering</trd>,
    <trd>surface</trd>,
    <trd>coating</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>recubrimiento</trd>,
