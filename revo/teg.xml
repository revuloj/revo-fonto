  </trdgrp>
</drv>

<drv mrk="teg.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        <tld/>o el antilopaj feloj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 4:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ar&gcirc;enta <tld/>o de viaj statuoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        sofoj kun silka <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la oficiala <tld/>o <klr>(trompa mieno, <ref cel="mask.0o">masko</ref>)</klr>,
        per kiu sin kovradis la mastrino de la informa oficejo,
        <klr>[&#8230;]</klr> fari&gcirc;is travidiga
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kovril<tld/>(il)o</ind> <fnt><bib>HejmVort</bib></fnt>.
        <trd lng="hu">takar&oacute;huzat</trd>
        <trd lng="nl">overtrek</trd>
        <trdgrp lng="pl">
          <trd>poszwa</trd>,
          <trd>pow&lstroke;oczka</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="teg.0ilo">tegilo</ref>
    <refgrp tip="vid">
      <ref cel="kovr.0o.komune">kovro<sncref/></ref>
    </refgrp>
