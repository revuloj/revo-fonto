<drv mrk="teg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="teg.0i.almeti">
    <dif>
      <ref tip="super" cel="kovr.0i.surmeti">Kovri</ref> &ccirc;iuflanke per tuko speciale destinata kaj
      al&gcirc;ustigita por tio:
      <ekz>
        sur la ora altaro ili sternu bluan tukon kaj <tld/>u &gcirc;in per <tld/>o el
        antilopaj feloj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 4:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la salono <klr>[&#x2026;]</klr> staris se&gcirc;oj kaj sofoj, <tld/>itaj per veluro kaj
        silko
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        brakse&gcirc;oj <tld/>itaj per punca lana damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mole <tld/>ita <klr>[&#x2026;]</klr> divano
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volv.en0i.tute">envolvi<sncref/></ref>,
      <ref cel="rembur.0i.KOMUNE">remburi<sncref/></ref>,
      <ref cel="met.sur0i.veston">surmeti<sncref/></ref>
    </refgrp>
    <trd lng="hu">beh&uacute;z <klr tip="ind">(huzattal)</klr></trd>    
    <trdgrp lng="pl">
      <trd>pokrywa&cacute;</trd>
      <trd>obleka&cacute;</trd>,
      <trd>powleka&cacute;</trd>,
      <trd>obija&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol; <klr>(&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="teg.0i.alglui">
    <dif>
      Da&ubreve;re <ref tip="super" cel="kovr.0i.surmeti">kovri</ref> ion per io tiamaniere, 
      ke la kovra&jcirc;o formas tuton kun la kovrita&jcirc;o kaj estas parto de &gcirc;i:
      <ekz>
        faru stangojn el akacia ligno kaj <tld/>u ilin per oro
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro prenis la kuprajn incensujojn <klr>[&#x2026;]</klr> kaj tavoligis ilin, por
        <tld/>i la altaron
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 16:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        kabineta skribtablo <tld/>ita per verda drapo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la muroj estis <tld/>itaj per buterflava, porcelansimila materialo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        muroj <klr>[&#x2026;]</klr> tegitaj per platoj el diversaj marmoroj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pordo de la notaria kabineto estis duobla, kun lede <tld/>ita remburo
        destinita sufoki la transpason de la sonoj
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        montoj <tld/>itaj de arboj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116; 8-9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxirka.0igi">&ccirc;irka&ubreve;igi</ref>,
      <ref cel="ferm.en0i">enfermi</ref>
    </refgrp>
    <trd lng="en">coat</trd>
    <trd lng="hu">bevon <klr tip="ind">(bevonattal)</klr></trd>    
    <trdgrp lng="pl">
      <trd>pokrywa&cacute;</trd>,
      <trd>powleka&cacute;</trd>,
      <trd>obija&cacute;</trd>,
      <trd>wylepia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_i;&c_c;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_kha;&t_drengbu;&t_ba;&t_sa;&t_tsheg;&t_ga;&t_ca;&t_naro;&t_da;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>pokr&yacute;t</trd>,
    <trd>pot&aacute;hnout</trd>,
    <trd>povl&eacute;ci</trd>
  </trdgrp>
  <trd lng="de">&uuml;berziehen</trd>
  <trdgrp lng="en">
    <trd>cover</trd>,
    <trd>overlay</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>recubrir</trd>,
    <trd>forrar</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_w;&f_k;&a_sh; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>envelopper</trd>,
    <trd>recouvrir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0A;&#x5F35;&#x308A;&#x3059;&#x308B;  [<ind>&#x3046;&#x3048;&#x306F;&#x308A;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x8986;&#x3044;&#x3092;&#x3059;&#x308B;  [<ind>&#x304A;&#x304A;&#x3044;&#x3092;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bekleden</trd>,
    <trd>overtrekken</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pokry&tcaron;</trd>,
    <trd>potiahnu&tcaron; <klr>(nie&ccaron;&iacute;m )</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43A;&#x440;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x442;&#x44F;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x432;&#x43E;&#x43B;&#x456;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x43F;&#x43E;&#x432;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="teg.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        <tld/>o el antilopaj feloj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 4:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ar&gcirc;enta <tld/>o de viaj statuoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        sofoj kun silka <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la oficiala <tld/>o <klr>(trompa mieno, <ref cel="mask.0o">masko</ref>)</klr>, 
        per kiu sin kovradis la mastrino de la informa oficejo,
        <klr>[&#x2026;]</klr> fari&gcirc;is travidiga
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kusen<tld/>(il)o</ind> <fnt><bib>HejmVort</bib></fnt>;
        <trdgrp lng="hu">
          <trd>p&aacute;rnahuzat</trd>,
          <trd>v&aacute;nkoshuzat</trd>
        </trdgrp>
        <trd lng="nl">sloop</trd>
        <trd lng="pl">poszewka</trd>
      </ekz>
      <ekz>
        <ind>kovril<tld/>(il)o</ind> <fnt><bib>HejmVort</bib></fnt>.
        <trd lng="hu">takar&oacute;huzat</trd>
        <trd lng="nl">overtrek</trd>
        <trdgrp lng="pl">
          <trd>poszwa</trd>,
          <trd>pow&lstroke;oczka</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="teg.0ilo">tegilo</ref>
    <refgrp tip="vid">
      <ref cel="kovr.0o.komune">kovro<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kryt</trd>,
    <trd>potah</trd>,
    <trd>povlak</trd>,
    <trd>povle&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bezug</trd>,
    <trd>&Uuml;berzug</trd>
  </trdgrp><trdgrp lng="en">
    <trd>covering</trd>,
    <trd>surface</trd>,
    <trd>coating</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>recubrimiento</trd>,
    <trd>forro</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_w;&f_k;&a_sh;</trd>
  <trdgrp lng="nl">
    <trd>bekleding</trd>,
    <trd>overtrek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pow&lstroke;oczka</trd>,
    <trd>obicie</trd>,
    <trd>podpinka</trd>,
    <trd>pokrycie</trd>,
    <trd>podbitka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_h;&c_o;&c_l;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_i;&c_c;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>1.povlak</trd>,
    <trd>2.obkladac&iacute; materi&aacute;l <klr>(kachli&ccaron;ky)</klr></trd>,
    <trd>oblie&ccaron;ka</trd>,
    <trd>po&tcaron;ah</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43A;&#x440;&#x438;&#x442;&#x442;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x43E;&#x43B;&#x43E;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x447;&#x43E;&#x445;&#x43E;&#x43B;</trd>,
    <trd>&#x43E;&#x431;&#x43B;&#x438;&#x446;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&#x440;&#x438;&#x442;&#x442;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="teg.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="teg.0ilo.tego">
    <dif>
      <ref tip="dif" cel="teg.0o">Tego</ref>:
      <ekz>
        la palacoj per bronzo, oro, speguloj, pargeto, <tld/>iloj, gobelenoj, treege
        similas
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        el unu el la du <tld/>iloj de sia zono la centuriestro eligis malpuran
        pantran&ccirc;ilon
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        plumo povas esti <klr>[&#x2026;]</klr> anka&ubreve; skribilo, iam la koncerna skribilo estis
        ordinare farita el la koncerna birdo<tld/>ilo, sed hodia&ubreve; ni uzas la saman
        vorton anka&ubreve; por skribiloj el metalo
        <fnt>
          <aut>Wells</aut>
          <vrk> Lingvistikaj aspektoj de Esperanto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> dormadis <klr>[&#x2026;]</klr> sur la malnova matraco, 
        la <tld/>ilo estis kudrita el malglata, truplena kanvaso, kaj la kuseno &scirc;ajnis
        malmola kiel &scirc;tono
        <fnt>
          <bib>LOdE</bib>
          <lok> 2003, &#x2116; 3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kryt</trd>,
    <trd>potah</trd>,
    <trd>povlak</trd>,
    <trd>povle&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bezug</trd>,
    <trd>&Uuml;berzug</trd>
  </trdgrp><trdgrp lng="en">
    <trd>cover</trd>,
    <trd>wrapping</trd>,
    <trd>coating</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_w;&f_k;&a_sh;</trd>
  <trdgrp lng="hu">
    <trd>huzat</trd>,
    <trd>bevonat</trd>,
    <trd>burkolat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8986;&#x3044;  [<ind>&#x304A;&#x304A;&#x3044;</ind>]</trd>,
    <trd>&#x30AB;&#x30D0;&#x30FC;  [<ind>&#x304B;&#x3070;&#x30FC;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bekleding</trd>,
    <trd>overtrek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pow&lstroke;oczka</trd>,
    <trd>obicie</trd>,
    <trd>podpinka</trd>,
    <trd>pokrycie</trd>,
    <trd>podbitka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_h;&c_o;&c_l;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_i;&c_c;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>1.povlak</trd>,
    <trd>2.obkladac&iacute; materi&aacute;l <klr>(kachli&ccaron;ky)</klr></trd>,
    <trd>oblie&ccaron;ka</trd>,
    <trd>po&tcaron;ah</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43A;&#x440;&#x438;&#x442;&#x442;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x43E;&#x43B;&#x43E;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x447;&#x43E;&#x445;&#x43E;&#x43B;</trd>,
    <trd>&#x43E;&#x431;&#x43B;&#x438;&#x446;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
