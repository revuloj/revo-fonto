<drv mrk="teg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="teg.0i.almeti">
    <dif>
      <ref tip="super" cel="kovr.0i.surmeti">Kovri</ref> &ccirc;iuflanke per tuko speciale destinata kaj
      al&gcirc;ustigita por tio:
      <ekz>
        sur la ora altaro ili sternu bluan tukon kaj <tld/>u &gcirc;in per <tld/>o el
        antilopaj feloj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 4:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la salono <klr>[&#8230;]</klr> staris se&gcirc;oj kaj sofoj, <tld/>itaj per veluro kaj
        silko
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        brakse&gcirc;oj <tld/>itaj per punca lana damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mole <tld/>ita <klr>[&#8230;]</klr> divano
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volv.en0i.tute">envolvi<sncref/></ref>,
      <ref cel="rembur.0i.KOMUNE">remburi<sncref/></ref>,
      <ref cel="met.sur0i.veston">surmeti<sncref/></ref>
    </refgrp>
    <trd lng="hu">beh&uacute;z <klr tip="ind">(huzattal)</klr></trd>
    <trdgrp lng="pl">
      <trd>pokrywa&cacute;</trd>
      <trd>obleka&cacute;</trd>,
      <trd>powleka&cacute;</trd>,
      <trd>obija&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_mol; <klr>(&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="teg.0i.alglui">
    <dif>
      Da&ubreve;re <ref tip="super" cel="kovr.0i.surmeti">kovri</ref> ion per io tiamaniere,
      ke la kovra&jcirc;o formas tuton kun la kovrita&jcirc;o kaj estas parto de &gcirc;i:
      <ekz>
        faru stangojn el akacia ligno kaj <tld/>u ilin per oro
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro prenis la kuprajn incensujojn <klr>[&#8230;]</klr> kaj tavoligis ilin, por
        <tld/>i la altaron
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 16:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        kabineta skribtablo <tld/>ita per verda drapo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la muroj estis <tld/>itaj per buterflava, porcelansimila materialo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        muroj <klr>[&#8230;]</klr> tegitaj per platoj el diversaj marmoroj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pordo de la notaria kabineto estis duobla, kun lede <tld/>ita remburo
        destinita sufoki la transpason de la sonoj
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        montoj <tld/>itaj de arboj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 8-9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxirka.0igi">&ccirc;irka&ubreve;igi</ref>,
      <ref cel="ferm.en0i">enfermi</ref>
    </refgrp>
    <trd lng="en">coat</trd>
    <trd lng="hu">bevon <klr tip="ind">(bevonattal)</klr></trd>
    <trdgrp lng="pl">
      <trd>pokrywa&cacute;</trd>,
      <trd>powleka&cacute;</trd>,
      <trd>obija&cacute;</trd>,
      <trd>wylepia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_i;&c_c;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_drengbu;&t_ba;&t_sa;&t_tsheg;&t_ga;&t_ca;&t_naro;&t_da;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>pokr&yacute;t</trd>,
    <trd>pot&aacute;hnout</trd>,
    <trd>povl&eacute;ci</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&uuml;berziehen</trd>,
    <trd>beziehen <klr>(mit Stoff usw.)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cover</trd>,
    <trd>overlay</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>recubrir</trd>,
    <trd>forrar</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_w;&f_k;&a_sh; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>envelopper</trd>,
    <trd>recouvrir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0A;&#x5F35;&#x308A;&#12377;&#x308B;  <pr>&#12358;&#12360;&#x306F;&#x308A;&#12377;&#x308B;</pr></trd>,
    <trd>&#35206;&#12356;&#12434;&#12377;&#x308B;  <pr>&#x304A;&#x304A;&#12356;&#12434;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bekleden</trd>,
    <trd>overtrekken</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pokry&tcaron;</trd>,
    <trd>potiahnu&tcaron; <klr>(nie&ccaron;&iacute;m )</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_t;&c_ja;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_o;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76D6; <pr>g&#xE0;i</pr></trd>,
    <trd>&#x84CB; <pr>g&#xE0;i</pr></trd>,
    <trd>&#28952; <pr>d&#xE0;o</pr></trd>,
    <trd>&#x71FE; <pr>d&#xE0;o</pr></trd>,
    <trd>&#x7B3C;&#x7F69; <pr>l&#x1D2;ngzh&#xE0;o</pr></trd>,
    <trd>&#x7C60;&#x7F69; <pr>l&#x1D2;ngzh&#xE0;o</pr></trd>,
    <trd>&#35206; <pr>f&#xF9;</pr></trd>,
    <trd>&#35206;&#x76D6; <pr>f&#xF9;g&#xE0;i</pr></trd>,
    <trd>&#35206;&#x84CB; <pr>f&#xF9;g&#xE0;i</pr></trd>,
