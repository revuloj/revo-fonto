<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;eriz</rad>/o
</kap>
<drv mrk="cxeriz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bongusta frukto de <tld/>ujo.
    </dif>
  </snc>
  <bld lok="../bld/cxeriz.jpg">
    bran&ccirc;o kun <tld/>oj
  </bld>
  <trd lng="cs">t&rcaron;e&scaron;n&ecaron;
  <klr>(vi&scaron;n&ecaron;)</klr></trd>
  <trd lng="de">Kirsche</trd>
  <trd lng="en">cherry</trd>
  <trd lng="fr">cerise</trd>
  <trdgrp lng="tr">
    <trd>kiraz <klr>(dol&ccirc;a)</klr></trd>,
    <trd>vi&scedil;ne <klr>(acida)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="cxeriz.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specioj kaj varioj de <ref tip="super" cel="prunus.0o">prunuso</ref>,
      devenantaj de la origina formo <trd lng="la">Prunus avium</trd>,
      kun globformaj ru&gcirc;aj fruktoj. La pli acida specio
      <trd lng="la">Prunus cerasus</trd> estas ver&scirc;ajne kruci&gcirc;o
      de la specioj <trd lng="la">Prunus avium</trd> kaj
      <trd lng="la">Prunus fruticosa</trd>.
    </dif>
  </snc>
  <trd lng="de">Kirschbaum</trd>
  <trd lng="fr">cerisier</trd>
</drv>
</art>
