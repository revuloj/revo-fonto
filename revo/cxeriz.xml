<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxeriz.xml,v 1.40 2024/01/31 18:21:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;eriz</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="cxeriz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Bongusta <ref tip="sub" cel="frukt.0o.BOT">frukto</ref> de
      <tld/>ujo:
      <ekz>
        sur la tablo trovi&gcirc;is plej bongustaj <tld/>oj, kaj
        <nom>Gerda</nom> man&gcirc;is de ili kiom &scirc;i
        volis
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiel certe, kiel <tld/>oj kreskas sur &ccirc;i tiuj kverkoj kaj &ccirc;i
        tiuj abioj portas
        persikojn, tiel certe vi povos vivaj forkuri
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        aldono de fre&scirc;aj <tld/>oj a&ubreve; framboj rezultigas fruktecan, pli
        dol&ccirc;an guston
       <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        feli&ccirc;aj ricevi pomon kun vermo, <tld/>on kun larvo, laktukon kun
        heliko
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011266.php">Vivi en mondo surogata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>la <tld/>o sur la torto</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2011/010160.php">Belaj figoj post Pasko</url></vrk>
        </fnt>.
        <trd lng="de">das T&uuml;pfelchen auf dem &leftquot;i&rightquot;</trd>
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/cxeriz.jpg">
    bran&ccirc;o kun <tld/>oj
  </bld>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_e;&c_sh;&c_n;&c_ja; <klr>(&c_p;&c_l;&c_o;&c_d;)
  </klr></trd>,
    <trd>&c_v;&c_ib;&c_sh;&c_n;&c_ja; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_a;</trd>
  <trd lng="ca">cirera</trd>
  <trd lng="cs">t&rcaron;e&scaron;n&ecaron;
  <klr>(vi&scaron;n&ecaron;)</klr></trd>
  <trd lng="de">Kirsche</trd>
  <trd lng="el">&kappa;&epsilon;&rho;&alfa_ton;&sigma;&jota;</trd>
  <trd lng="en">cherry</trd>
  <trd lng="es">cereza</trd>
  <trd lng="fr">cerise</trd>
  <trd lng="he">&dalet;&vav;&bet;&dalet;&bet;&fnun;</trd>
  <trd lng="hu">cseresznye</trd>
  <trd lng="id">ceri</trd>
  <trd lng="it">ciliegia</trd>
  <trd lng="ja">&#12469;&#12463;&#12521;&#12531;&#12508;</trd>
  <trdgrp lng="nl">
    <trd>kriek <klr>(dol&ccirc;a)</klr></trd>,
    <trd>kers <klr>(acida)</klr></trd>
  </trdgrp>
  <trd lng="pl">wi&sacute;nia</trd>
  <trd lng="pt">cereja</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_n;&c_ja; <klr>(dol&ccirc;a)</klr>
  </trd>,
    <trd>&c_v;&c_i;&c_sh;&c_n;&c_ja; <klr>(acida)</klr></trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>kiraz <klr>(dol&ccirc;a)</klr></trd>,
    <trd>vi&scedil;ne <klr>(acida)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_sh;&c_n;&c_ja; (&c_p;&c_l;&c_ib;&c_d;)</trd>
  <trdgrp lng="zh">
    <trd>&#27185; <pr>y&imacron;ng</pr></trd>,
    <trd>&#27185;&#26691; <pr>y&imacron;ngt&aacute;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cxeriz.0ujo">
  <kap><tld/>ujo,
    <var><kap><tld/>arbo</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specioj kaj varia&jcirc;oj de
      <ref tip="super" cel="prunus.0o">prunuso</ref>,
      devenantaj de la origina formo <trd lng="la">Prunus avium</trd>
      (<ref tip="sub" cel="meriz.0arbo">merizujo</ref>),
      kun globformaj ru&gcirc;aj fruktoj. La pli acida specio
      <trd lng="la">Prunus cerasus</trd> estas ver&scirc;ajne kruci&gcirc;o
      de la specioj <trd lng="la">Prunus avium</trd> kaj
      <trd lng="la">Prunus fruticosa</trd>:
      <ekz>
        fortimigi paserojn de <tld/>arboj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>arboj estis plene kovritaj de sukaj nigraj, preska&ubreve;
        varmegaj <tld/>oj
        <fnt>&ccirc;erizoj
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        La <tld/>ujoj jam similas &parsl; Al florpejza&gcirc;' dum son&gcirc;-aper'
        <fnt>
          <bib>LSB</bib>
          <lok>Japana Balado</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      &leftquot;Prunus cerasus&rightquot; (esperantlingve &leftquot;acida &ccirc;erizujo&rightquot; a&ubreve; &leftquot;merizujo&rightquot;)
      miascie ne estas kruci&gcirc;o, male &gcirc;i estas sova&gcirc;a arbo, kiu originis el
      Mezoriento.
      La&ubreve; historia fonto, ver&scirc;ajne &gcirc;i estis importita (eble fare de la
      fam-konata ri&ccirc;ulo Lucullo) kaj disvastigita de la Romianoj.
      <aut>A. Codazzi</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_e;&c_sh;&c_n;&c_ja;
    <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>,
    <trd>&c_v;&c_ib;&c_sh;&c_n;&c_ja; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr>
   </trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_a;</trd>
  <trd lng="ca">cirerer</trd>
  <trd lng="de">Kirschbaum</trd>
  <trd lng="el">&kappa;&epsilon;&rho;&alfa;&sigma;&jota;&alfa_ton;</trd>
  <trd lng="es">cerezo</trd>
  <trd lng="fr">cerisier</trd>
  <trd lng="he">&ayin;&ftsadi; &dalet;&vav;&bet;&dalet;&bet;&fnun;</trd>
  <trd lng="hu">cseresznyefa</trd>
  <trd lng="it">ciliegio</trd><trdgrp lng="ja">
    <trd>&#12469;&#12463;&#12521;&#12531;&#12508;&#12398;&#26408;  <pr>&#12373;&#12367;&#12425;&#12435;&#12412;&#12398;&#12365;</pr></trd>,
    <trd>&#12458;&#12454;&#12488;&#12454;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kersenboom</trd>,
    <trd>kerselaar</trd>
  </trdgrp>
  <trd lng="pl">wi&sacute;nia</trd>
  <trd lng="pt">cerejeira</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_sh;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_sh;&c_n;&c_jo;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_sh;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_sh;&c_n;&c_je;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="cxeriz.lauxro0o">
  <kap>
    la&ubreve;ro<tld/>ujo, <var><kap>la&ubreve;ro<tld/>arbo</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref> kun la&ubreve;roformaj
      folioj (<trd lng="la">Prunus laurocerasus</trd>), kies &ccirc;iuj partoj
      estas venenaj:
      <ekz>
        ekzemple <ref cel="padus.0o">paduso</ref>, la&ubreve;ro<tld/><klr>[arb]</klr>o <klr>[&#8230;]</klr>
        povas en laboratoriaj kondi&ccirc;oj pereigi je distanco muson
        <fnt>
          <aut>B. P. Tonkin, trad. A. Sidorov</aut>
          <vrk><url ref="https://scienca-revuo.info/files/427-1-565-1-10-20140426.pdf"
              >Fitoncidoj</url></vrk>
          <lok>Scienca Revuo, 07/1958, p.  63</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lorbeerkrische</trd>
  <trd lng="fr">laurier-cerise</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_o;&c_v;&c_i;&c_sh;&c_n;&c_ja; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
</drv>

</art>
