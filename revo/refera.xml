<vortaro>
<art mrk="$Id$">
<kap>
  <rad>referat</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="refera.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="raport.0o.dokumenta">Raporto</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Referat</trd>,
    <trd>Vortrag</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verslag</trd>,
    <trd>rapport</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;</trd>,
    <trd>&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
