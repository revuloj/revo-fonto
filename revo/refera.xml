<kap>
  <rad>referat</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="refera.0o">
  <kap><tld/>o</kap>
  <snc mrk="refera.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="raport.0o.dokumenta">Raporto<sncref/></ref> a&ubreve; prelego:
      <ekz>
        la a&ubreve;toro skribas pri la <tld/>o, prelegita en la jaro 1914 en Parizo, &ccirc;ar nur
        tie kaj nur tiam <nom>Lenin</nom> diris ion, se ne pri Esperanto, do almena&ubreve; pri la
        lingvo tutmonda
        <fnt>
          <vrk><url ref="http://miresperanto.com/historio/lenin_kaj_esperanto.htm">Lenin Kaj
          Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_f;&c_e;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="cs">refer&aacute;t</trd>
  <trdgrp lng="de">
    <trd>Referat</trd>,
    <trd>Vortrag</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compte rendu</trd>,
    <trd>rapport<klr tip="amb"> (compte rendu)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;ad&aacute;s</trd>,
    <trd>refer&aacute;tum</trd>,
    <trd>besz&aacute;mol&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verslag</trd>,
    <trd>rapport</trd>
  </trdgrp>
  <trd lng="pl">referat</trd>
  <trd lng="pt">relat&oacute;rio</trd>
  <trd lng="ro">referat</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;</trd>,
    <trd>&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>refer&aacute;t</trd>,
    <trd>spr&aacute;va</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5F59;&#x5831; <pr>hu&#xEC;b&#xE0;o</pr></trd>,
    <trd>&#x6C47;&#x62A5; <pr>hu&#xEC;b&#xE0;o</pr></trd>,
    <trd>&#x5831;&#x544A; <pr>b&#xE0;og&#xE0;o</pr></trd>,
    <trd>&#x62A5;&#x544A; <pr>b&#xE0;og&#xE0;o</pr></trd>
  </trdgrp>
</drv>
