<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>frakas</rad>/i</kap>
<drv mrk="frakas.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte kaj brue disrompi, dispecetigi:
      <ekz>
        <tld/>i vazon, &ccirc;aron<fnt>Z</fnt>, la dentojn de
        iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        unuj kurante en la mallumo sin <tld/>is je la muro
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 16</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polv.0igi">polvigi</ref>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_u;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_b;&c_n;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol; &c_v;&c_d;&c_r;&c_je;&c_b;&c_je;&c_z;&c_g;&c_i;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">hacer fracasar</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plenvenki malamikon, okazigante al li grandajn perdojn:
      <ekz>
        <tld/>ita armeo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_a;&c_m;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_k;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute detrui, ruinigi:
      <ekz>
        <tld/>i nacion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;ion tempo <tld/>as;
      </ekz>
      <ekz>
        la kruela realo ne <tld/>is tian kura&gcirc;on;
      </ekz>
      <ekz>
        &ccirc;iuj provoj de la reakcio <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        mia fido, mia espero tute <tld/>i&gcirc;is.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detru.0i">detrui</ref>,
      <ref cel="romp.0i">rompi</ref>,
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="neni">neniigi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_k;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Afliktegi:
      <ekz>
        <tld/>i la koron de iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu rememoro <tld/>as min<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="ru">&c_s;&c_o;&c_k;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plene refuti:
      <ekz>
        <tld/>i argumenton.
      </ekz>
    </dif>
    <ref tip="vid" cel="nul0.0igi">nuligi</ref>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_a;&c_m;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">hacer fracasar</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>zerschmettern</trd>,
    <trd>zertr&uuml;mmern</trd>
  </trdgrp>
  <trd lng="en">shatter</trd>
  <trdgrp lng="fr">
    <trd>fracasser</trd>,
    <trd>briser</trd>,
    <trd>casser</trd>,
    <trd>mettre en <ind>d&eacute;route</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vermorzelen</trd>,
    <trd>verbrijzelen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>quebrar</trd>,
    <trd>britar</trd>,
    <trd>fracassar</trd>,
    <trd>esmagar</trd>,
    <trd>partir</trd>
  </trdgrp>
</drv>

<drv mrk="frakas.0o0ado">
  <kap><tld/>o, <tld/>ado</kap>
  <snc>
    <dif>
      Disrompo, dispecetigo.
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="F"/>bruo:
      <ekz>
        granda <tld/>o en malgranda glaso
        <klr>(granda bruo por nenio)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        eliri sen <tld/>o el granda embaraso<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bruo de batalo estas en la lando kaj granda <tld/>ado
        <fnt><bib>MT</bib>, <lok>&Jer; 50:22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fracas</trd>,
    <trd>casse<klr tip="ind"> (action de casser)</klr></trd>,
    <trd>vacarme</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>vermorzelen</ind></trd>,
    <trd>het <ind>verbrijzelen</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
</art>
