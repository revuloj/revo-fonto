<drv mrk="frakas.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="frakas.0igxi.KOMUNE">
    <dif>
      Brue <ref tip="super" cel="romp.0igxi">rompi&gcirc;i</ref>; plene <ref tip="super" cel="detru.0igxi">detrui&gcirc;i</ref>:
      <ekz>
        subite falis <nom>Babel</nom> kaj <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 51:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io kune <tld/>i&gcirc;is: la fero, argilo, kupro, ar&gcirc;ento, kaj oro fari&gcirc;is kiel
        grenventuma&jcirc;o sur somera dra&scirc;ejo
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 2:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la rabistoj frapis per sia peza fera martelo lian frunton, tiel ke &gcirc;i
        <tld/>i&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj kurante en la mallumo sin <tld/>is je la muro
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        li &jcirc;etis la multekostan vazon sur la pargeton <klr>[kaj]</klr> &gcirc;i <tld/>i&gcirc;is en
        malgrandajn pecetojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bela idolo de mia koro <tld/>i&gcirc;is, la malindeco de miaj revoj staras anta&ubreve;
        miaj internaj okuloj en malbela nudeco
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kurante kiel blindaj, en la mallumo sin <tld/>is je la muroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="averi.0i">averii</ref>,
      <ref cel="romp.dis0igxi.KOMUNE">disrompi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_c;&c_a;</trd>,
