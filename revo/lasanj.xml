<drv mrk="lasanj.0o">
  <kap><tld/>o</kap>
  <snc mrk="lasanj.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="graten.0ajxo">Gratena&jcirc;o</ref> el pluraj tavoloj de tre lar&gcirc;aj nudeloj
      kun intera sa&ubreve;co;
      tre lar&gcirc;a <ref tip="super" cel="nudel.0o.rubanda">nudelo</ref> uzata por tiu plado:
      <ekz>
        legoma lasanjo, salma lasanjo
        <fnt>
          <vrk><url ref="https://www.esperanto.de/eo/gek2015/tagmangho"
              >Komuna tagman&gcirc;o </url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        okazis la mirakla trovo de alia manuskripto <klr>[&#8230;]</klr> en torfejo, io simila
        al bulo de <tld/>oj venis al la surfaco de la grundo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009180.php"
              >Ornamitaj manuskriptoj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lasagne</trd>
    <trd lng="en">lasagna</trd>
    <trd lng="fr">lasagnes</trd>
    <trd lng="ru">&c_l;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_ja;</trd>
  </snc>
