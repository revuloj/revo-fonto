<art mrk="$Id$">
<kap>
  <rad>americi</rad>/o
</kap>
<drv mrk="americi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 94, atompezo 243,0614, simbolo Am. <ref tip="super"
     cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
     estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Americium</trd>
  <trd lng="hu">amer&iacute;cium</trd>
  <trd lng="nl">americium</trd>
  <trd lng="ru">&c_a;&c_m;&c_je;&c_r;&c_i;&c_c;&c_i;&c_j;</trd>
</drv>
</art>

