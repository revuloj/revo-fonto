<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: americi.xml,v 1.22 2022/03/11 17:34:18 revo Exp $">
<kap>
  <rad>americi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="americi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Am</mlg>
  <snc mrk="americi.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="95">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 95, atompezo 243,0614, kies &ccirc;iuj 
      <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj:
      <ekz>
        <tld/>o estas ar&gcirc;ent-kolora radioaktiva metala elemento kiu
        estas farita per bombard<klr>[o de]</klr> plutonio per ne&ubreve;tronoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28863">Americio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_e;&c_r;&c_y;&c_c;&c_y;&c_j;</trd>
  <trd lng="bg">&c_a;&c_m;&c_je;&c_r;&c_i;&c_c;&c_i;&c_j;</trd>
  <trd lng="br">amerikiom</trd>
  <trd lng="ca">americi</trd>
  <trd lng="cs">americium</trd>
  <trd lng="de">Americium</trd>
  <trd lng="en">americium</trd>
  <trd lng="es">americio</trd>
  <trd lng="fr">americium</trd>
  <trd lng="hu">amer&iacute;cium</trd>
  <trd lng="id">amerisium</trd>
  <trd lng="it">americio</trd>
  <trd lng="la">Americum</trd>
  <trd lng="nl">americium</trd>
  <trd lng="pl">ameryk</trd>
  <trd lng="pt">amer&iacute;cio</trd>
  <trd lng="ru">&c_a;&c_m;&c_je;&c_r;&c_i;&c_c;&c_i;&c_j;</trd>
  <trd lng="sk">amer&iacute;cium</trd>
  <trd lng="sv">americium</trd>
  <trd lng="uk">&#x430;&#x43C;&#x435;&#x440;&#x438;&#x446;&#x456;&#x439;</trd>
</drv>
</art>
