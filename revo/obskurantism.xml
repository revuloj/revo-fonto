<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>obskurantism</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="obskurantism.0o">
  <kap><tld/>o</kap>
  <uzo tip="klr">(pliofte en klerisma a&ubreve; kontra&ubreve;religia
  kunteksto)</uzo>
  <snc mrk="obskurantism.0o.KOMUNE">
    <dif>
      Granda malklereco kaj malracieco:
      <ekz>
        prilaborataj formoj de <tld/>o manifesti&gcirc;as en la tuta mondo
        <fnt>
          <aut>H. Masson</aut>,
          <bib>PIV2</bib>,
          <lok>Interkonati&gcirc;a espero kun SAT, p. 1265a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="obskur.0a.malklara">obskura <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fa">&a_t;&a_A;&a_r;&f_y;&f_k;&zwnj;&a_A;&a_n;&a_d;&f_y;&a_sh;&f_y;</trd>
  <trd lng="fr">obscurantisme</trd>
  <trd lng="pt">obscurantismo</trd>
</drv>
</art>

