<vortaro>
<art mrk="$Id: obskurantism.xml,v 1.11 2022/06/13 08:38:11 revo Exp $">
<kap>
  <rad>obskurantism</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="obskurantism.0o">
  <kap><tld/>o</kap>
  <snc mrk="obskurantism.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="obskur.0ismo.KOMUNE">Obskurismo</ref>:
      <ekz>
        prilaborataj formoj de <tld/>o manifesti&gcirc;as en la tuta mondo
        <fnt>
          <aut>H. Masson</aut>,
          <bib>PIV2</bib>,
          <lok>Interkonati&gcirc;a espero kun SAT, p. 1265a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la religio <klr>[&#8230;]</klr> donis motivon por misa&jcirc;oj kiel <klr>[&#8230;]</klr> <tld/>o
        kaj opono al &ccirc;iuj socialaj reformoj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Steloj;monato2/006847.html">Opinio: Sobraj pensoj de
          sobrulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        hermetikaj dogmoj de arkaikaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <lok><url
          ref="&Steloj;monato2/007018.html">&Steloj;monato2/007018.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="kler.0ismo">klerismo</ref>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_k;&c_u;&c_r;&c_a;&c_n;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>obskurantizmus</trd>,
    <trd>obskurantstv&iacute;</trd>,
    <trd>tm&aacute;&rcaron;stv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Obskurantismus</trd>
  <trd lng="fa">&a_t;&a_A;&a_r;&f_y;&f_k;&zwnj;&a_A;&a_n;&a_d;&f_y;&a_sh;&f_y;</trd>
  <trd lng="fr">obscurantisme</trd>
  <trd lng="nl">obscurantisme</trd>
  <trdgrp lng="pl">
    <trd>obskurantyzm</trd>,
    <trd>wstecznictwo</trd>
  </trdgrp>
  <trd lng="pt">obscurantismo</trd>
  <trdgrp lng="ro">
    <trd>obscurantism</trd>,
    <trd>face un pas</trd>
  </trdgrp>
  <trd lng="sk">tm&aacute;rstvo</trd>
</drv>
</art>

