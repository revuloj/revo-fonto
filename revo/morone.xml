<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: morone.xml,v 1.7 2023/08/10 07:26:15 revo Exp $">
<kap>
    <rad>moroned</rad>/oj
</kap>
<drv mrk="morone.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="morone.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="perk.0oformaj">perkoformaj</ref>
      fi&scirc;oj (<trd lng="la">Moronidae</trd>):
      <ekz>
        <ref tip="prt" cel="labrak.0o.ZOO">labrako</ref> <klr>[el]</klr>
        la familio <tld/>oj <klr>[estas]</klr> indi&gcirc;ena en Mediteraneo kaj Atlantiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=353847">Labrako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">mor&ograve;nids</trd>
  <trd lng="de">Wolfsbarsche</trd>
  <trd lng="fr">Moronid&eacute;s</trd>
  <trd lng="pl">moronowate</trd>
</drv>
</art>

