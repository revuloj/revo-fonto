<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Burbon</rad>/oj
    </kap>

<drv mrk="burbon1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="burbon1.0oj.gento">
    <dif>
      Nobela <ref tip="super" cel="gent.0o.familio">gento</ref>, el kiu devenis sep francaj re&gcirc;oj kaj pliaj en aliaj &scirc;tatoj:
      <ekz>
        la 300a datreveno de la batalo de <nom>Almansa</nom>, kie la dinastioj de la <tld/>oj kaj
        la Habsburgoj luktis por akiri la hispanajn re&gcirc;landojn
        <fnt>
          <bib>Monato</bib>
          <aut>Xavier Margais i Basi</aut>
          <vrk><url ref="https://www.monato.be/2007/009341.php"
              >Decida batalo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Haus Bourbon</trd>
  </snc>
  <rim>
    Maizviskion oni anka&ubreve; nomas <ref tip="hom" cel="burbon.0o">burbono</ref>.
  </rim>
</drv>

<drv mrk="burbon1.0a">
  <kap>
    <tld lit="b"/>a
  </kap>
  <snc mrk="burbon1.0a.rilata">
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        <tld/>a monarkio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=201455"
              >Burbono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1707 trupoj de la <tld lit="b"/>a kandidato por la hispana trono Filipo la 5a
        de Kastilio venkis &ccirc;e <nom>Almansa</nom> kaj eniris la re&gcirc;landon Valencio
        <fnt>
          <bib>Monato</bib>
          <aut>Xavier Margais i Basi</aut>
          <vrk><url ref="https://www.monato.be/2007/009341.php"
              >Decida batalo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>


  </art>

