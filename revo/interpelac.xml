<art mrk="$Id$">
<kap>
  <rad>interpelac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="interpelac.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="interp.0o">interpelacio</ref>
  </snc>
