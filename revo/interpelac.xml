<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: interpelac.xml,v 1.5 2025/04/30 10:34:24 revo Exp $">
<kap>
  <rad>interpelac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="interpelac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="interp.0o">Interpelacio</ref>:
      <ekz>
        FIFA sendis respektivan <tld/>on al asocio, kaj tiu refutis la informon pri puno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287823">Kroniko de protestado en Irano
          (2009)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
 <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_y;&c_t; <klr>(&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y;)</klr></trd>,
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_r;&c_p;&c_e;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">offizielle <ind>Anfrage</ind></trd>
  <trd lng="pl">interpelacja</trd>
</drv>

<drv mrk="interpelac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="interp.0i">interpelacii</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_z;&c_a;&c_p;&c_y;&c_t;</ind> <klr>(&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y;)</klr></trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_ib;&c_n;&c_t;&c_e;&c_r;&c_p;&c_e;&c_l;&c_ja;&c_c;&c_y;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="de">anfragen <klr>(offiziell)</klr></trd>
  <trd lng="pt">interpela&ccedil;&atilde;o</trd>
  <trd lng="pl">interpelowa&cacute;</trd>
</drv>
</art>

