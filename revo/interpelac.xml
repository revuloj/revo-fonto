<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: interpelac.xml,v 1.4 2018/09/14 17:10:15 revo Exp $">
<kap>
  <rad>interpelac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="interpelac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="interp.0o">Interpelacio</ref>:
      <ekz>
        FIFA sendis respektivan <tld/>on al asocio, kaj tiu refutis la informon pri puno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287823">Kroniko de protestado en Irano (2009)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">offizielle <ind>Anfrage</ind></trd>
  <trd lng="pl">interpelacja</trd>
</drv>

<drv mrk="interpelac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="interp.0i">interpelacii</ref>
  </snc>
  <trd lng="de">anfragen <klr>(offiziell)</klr></trd>
  <trd lng="pt">interpela&ccedil;&atilde;o</trd>
  <trd lng="pl">interpelowa&cacute;</trd>
</drv>
</art>

