<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: descend.xml,v 1.8 2023/11/04 13:16:27 revo Exp $">
<kap>
  <rad>descend</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="descend.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="descend.0i.malsupreniri">
    <dif>
      <ref tip="dif" cel="ir.malsupren0i.KOMUNE">Malsupreniri</ref>,
      <ref tip="dif" cel="supr.de0i.KOMUNE">desupri</ref>:
      <ekz>
        ni faris kelkajn turni&gcirc;ojn kaj <tld/>is sur deklivon
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj ege malfacile <tld/>is, la koto faris la krutan deklivon glitiga
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="&Monato;/2015/011386.php">Cent fabeloj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> <tld/>is en puton, kiu neatendite aperis anta&ubreve; li
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        Klorinda <tld/>as la&ubreve; la jeta &scirc;tuparo rapide, sed ne tiel gaje-pimpe kiel kutime
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond kaj Sen Rodin</aut>
          <vrk><url ref="&Monato;/2010/009988.php">La mal&jcirc;aluzo de Klorinda</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>inte de la biciklo kun rideto, li karesis mian kapon, kaj levis min facile
        sur la supran framtubon, kaj pu&scirc;is la biciklon hejmen
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="&Monato;/2012/010573.php">La biciklo de mia patro</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="ascend.0i">ascendi</ref>
  </snc>
  <!-- formon maloftan traduku prefere per formo nacilingve malofta -->
  <trd lng="be">&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="bo">&#xF56;&#xFB1;&#xF7A;&#xF0B;&#xF50;&#xF44;&#xF0B;</trd>
  <trd lng="de">absteigen</trd>
  <trd lng="he">&#x5DC;&#x5B8;&#x5E8;&#x5B6;&#x5D3;&#x5B6;&#x5EA;</trd>
  <trd lng="uk">&c_s;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x800C;&#x4E0B; <pr>&#xE9;rxi&#xE0;</pr></trd>,
    <trd>&#x722C;&#x4E0B; <pr>p&#xE1;xia</pr></trd>
  </trdgrp>
</drv>
</art>

