<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalt.xml,v 1.5 2023/11/22 09:25:17 revo Exp $">
<kap>
  <rad>kalt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kalt.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara vegeta&jcirc;o vivanta en mar&ccirc;aj lokoj de
      E&ubreve;ropo kaj Nordameriko;
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref> el
      familio <ref tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref> (<trd lng="la">Caltha</trd>),
      kun brile flavaj a&ubreve; blankaj florfolioj:
      <ekz>
        sur la herbejo malanta&ubreve; la kluzo ekfloris <tld/>o
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/XIII.">
          Avineto</url></vrk>,
          <lok>1909-1914</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/5/5a/Gewone_dotterbloem_-_Caltha_palustris_subsp._palustris_-_overview.jpg/640px-Gewone_dotterbloem_-_Caltha_palustris_subsp._palustris_-_overview.jpg"
      prm="CC BY-SA 3.0"
      >Mar&ccirc;a <tld/>o (Caltha palustris), Gouda, Nederlando
        <fnt>
          <aut>TeunSpaans</aut>
          <lok><url ref="&WCW;/File:Gewone_dotterbloem_-_Caltha_palustris_subsp._palustris_-_overview.jpg"
              >https://commons.wikimedia.org/wiki/File:Gewone_dotterbloem_-_Caltha_palustris_subsp._palustris_-_overview.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_o;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">Dotterblume</trd>
  <trdgrp lng="fr">
    <trd>caltha</trd>,
    <trd>populage</trd>
  </trdgrp>
  <trd lng="kk">&#1179;&c_a;&c_l;&c_t;&c_a;&c_g;&#1199;&c_l;</trd>
  <trd lng="pl">knie&cacute;</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_u;&c_zh;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&c_k;&c_a;&c_l;&c_ju;&c_zh;&c_n;&c_i;&c_c;&c_ja;</trd>
</drv>
</art>

