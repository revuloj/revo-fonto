<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galion.xml,v 1.12 2025/03/30 08:31:12 revo Exp $">
<kap>
  <rad>galion</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="galion.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Granda &scirc;ar&gcirc;oporta <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>
      memdefenda, kiajn oni tipe uzis por veturoj inter klasika Hispanujo
      kaj Sudameriko:
      <ekz>
        graciaj karaveloj, rapidaj brigantinoj, pezaj mallertaj <tld/>oj kaj flugilaj
        galiotoj portis sur siaj buspritoj kaj flagmastoj tre diversajn flagojn k
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sur la &ccirc;emara roko entran&ccirc;ita figuro de
        <tld/>o<fnt><bib>IntArkeol</bib>, <lok>p. 54a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="galer.0o">galero</ref>
    <bld
      lok="&WCU;/thumb/a/a2/Abraham_Willaerts%2C_Galley_and_men_of_war.jpeg/640px-Abraham_Willaerts%2C_Galley_and_men_of_war.jpeg"
      prm="PD"
      >Abraham Willaerts, 17a jc: nederlanda <tld/>o kaj franca galero apud haveno
        <fnt>
          <aut>Abraham Willaerts, http://www.nmm.ac.uk/mag/images/700/BHC0824_700.jpg</aut>
          <lok><url ref="&WCW;/File:Abraham_Willaerts,_Galley_and_men_of_war.jpeg"
              >https://commons.wikimedia.org/wiki/File:Abraham_Willaerts,_Galley_and_men_of_war.jpeg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="ca">gali&oacute;</trd>
  <trd lng="cs">galion</trd>
  <trd lng="de">Galeone</trd>
  <trd lng="fr">galion</trd>
  <trd lng="hu">galeon</trd>
  <trd lng="pl">galeon</trd>
  <trd lng="pt">gale&atilde;o</trd>
  <trd lng="sk">gali&oacute;n</trd>
  <trd lng="uk">&c_g;&c_a;&c_l;&c_ib;&c_o;&c_n;</trd>
</drv>
</art>

