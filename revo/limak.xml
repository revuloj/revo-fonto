<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>limak</rad>/o
</kap>
<drv mrk="limak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="limak.0o.gastropodo">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      &Ccirc;iu el la ordo <ref tip="dif" cel="gastro2.0oj">gastropodoj</ref>:
      <ekz>
        oni havas kun si sian propran domon kiel la limako la sian
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Historio el la dunoj</vrk>)</fnt> 
      </ekz>
      <ekz>
        li pripalpu la malgrandan konkon de la limako
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Feli&ccirc;a familio</vrk>)</fnt>
      </ekz>
    </dif>
    <trd lng="de">Schnecke</trd>
    <trdgrp lng="nl">
      <trd>slak</trd>,
      <trd>naaktslak</trd>
    </trdgrp>
    </snc>
  <snc mrk="limak.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="limak.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Limax</trd>).
    </dif>
    <trd lng="de">Schnegel</trd>
  </snc>
</drv>

</art>
