<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>limak</rad>/o
</kap>
<drv mrk="limak.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="limak.0o.gastropodo">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      &Ccirc;iu el la ordo <ref tip="dif" cel="gastro2.0oj">gastropodoj</ref>:
      <ekz>
        oni havas kun si sian propran domon kiel la limako la sian
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Historio el la dunoj</vrk>)</fnt>
      </ekz>
      <ekz>
        li pripalpu la malgrandan konkon de la limako
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Feli&ccirc;a familio</vrk>)</fnt>
      </ekz>
    </dif>
    <trd lng="de">Schnecke</trd>
    <trd lng="en">slug</trd>
    <trdgrp lng="nl">
      <trd>slak<klr tip="ind"> (naakt-)</klr></trd>,
      <trd>naaktslak</trd>
    </trdgrp>
    <trd lng="pl">&sacute;limak</trd>
  </snc>
  <snc mrk="limak.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super"
      cel="limake.0oj"><tld/>edoj</ref>
      (<trd lng="la">Limax</trd>).
    </dif>
    <trd lng="de">Schnegel</trd>
    <trd lng="pl">&sacute;limaki</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_i;&c_z;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_i;&c_z;&c_n;&c_ja;&c_k;</trd>,
    <trd>&c_u;&c_l;&c_i;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
