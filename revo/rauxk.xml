<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ra&ubreve;k</rad>/a
</kap>
<drv mrk="rauxk.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Sonanta malklare, obtuze, kvaza&ubreve; skrapante:
      <ekz>
        <tld/>a vo&ccirc;o, krio;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i pro troa kriado, pro malvarmumo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rasp">raspa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>rau</trd>,
    <trd>heiser</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hees</trd>,
    <trd>rauw</trd>,
    <trd>schor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_r;&c_i;&c_p;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">hes</trd>
</drv>
</art>
