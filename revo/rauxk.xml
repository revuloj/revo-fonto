<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rauxk.xml,v 1.18 2023/04/22 13:24:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ra&ubreve;k</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rauxk.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="rauxk.0a.SON">
    <dif>
      Sonanta mal<ref tip="ant" cel="klar.0a.malrauxka">klare</ref>,
      obtuze, kvaza&ubreve; skrapante:
      <ekz>
        la infano <klr>[&#8230;]</klr> veki&gcirc;is tre <tld/>a kaj kun sufoka tusado
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        subite granda nigra korvo <klr>[&#8230;]</klr> kriis per <tld/>a vo&ccirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la anseroj revenis hejmen kaj kriis sian <tld/>an krion
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;for, for!&rightquot; bojis la maljuna &ccirc;enhundo, &gcirc;i estis iom <tld/>a <klr>[&#8230;]</klr>
        depost la tempo, kiam &gcirc;i &ccirc;esis esti &ccirc;ambra hundo
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kapridoj rekonis la lupon per la <tld/>a vo&ccirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>La sep kapridoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ellasu min!... &dash; kriis li per <tld/>a vo&ccirc;o, &dash; ellasu min, mi volas trinki
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.mal0a">malglata</ref>,
      <ref cel="klar.mal0a">malklara</ref>,
      <ref cel="pur.mal0a">malpura</ref>,
      <ref cel="obtuz.0a.MUZ">obtuza<sncref/></ref>,
      <ref cel="rasp">raspa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_r;&c_y;&c_p;&c_l;&c_y;</trd>,
    <trd>&c_h;&c_r;&c_y;&c_p;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_r;&c_k;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_tsata;&t_shabchu;&t_ba;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>rau</trd>,
    <trd>heiser</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hoarse</trd>,
    <trd>husky</trd>
  </trdgrp>
  <trd lng="fr">rauque</trd>
  <trd lng="hu">rekedt</trd>
  <trdgrp lng="nl">
    <trd>hees</trd>,
    <trd>rauw</trd>,
    <trd>schor</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chrypliwy</trd>,
    <trd>ochryp&lstroke;y</trd>,
    <trd>zachrypniety</trd>,
    <trd>chrypi&aogonek;cy</trd>
  </trdgrp>
  <trd lng="pt">rouco</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_r;&c_i;&c_p;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">hes</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_r;&c_i;&c_p;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_r;&c_i;&c_p;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_r;&c_i;&c_p;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="rauxk.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rauxk.0igxi.KOMUNE">
    <dif>
      Ricevi <tld/>an vo&ccirc;on:
      <ekz>
        mi alvenis en <nom>Jehupec</nom> al la ri&ccirc;ulo, montris mian komerca&jcirc;on,
        parolis, paroladis, e&ccirc; <tld/>i&gcirc;is
        <fnt>
          <aut>&Scirc;. Alej&hcirc;em, trad. I. Mu&ccirc;nik</aut>
          <vrk>Ne estas feli&ccirc;o</vrk>
          <lok>en: Hebreaj rakontoj, 1923</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">s'<ind>enrouer</ind></trd>
</drv>
</art>
