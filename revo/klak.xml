<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>3</ofc>
      <rad>klak</rad>/i
    </kap>
    <drv mrk="klak.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          A&ubreve;digi sekan mallongan sonon per rapida kruda movo de
          io kontra&ubreve; io:
          <ekz>
            <tld/>i per la manoj, per la
            fingroj<fnt>Z</fnt>, </ekz><ekz>
            per la lango<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>as vipo, vangofrapo, dentoj, pordo, beko
            de cikonio, ekpafo, bato, pluvo (sur vitroj), moneroj (sur
            metalplato);
          </ekz><ekz>
            tondro<tld/>egoj.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="knar.0i">knari</ref>,
          <ref cel="krak.0i">kraki</ref>,
          <ref cel="plauxd.0i">pla&ubreve;di</ref>,
          <ref cel="sxmac.0i">&scirc;maci</ref>.
        </refgrp>
        <trdgrp lng="de">
          <trd>klappern [i.a. dentoj]</trd>,
          <trd>schnipsen [fingroj]</trd>,
          <trd>knallen [vipo]</trd>,
          <trd>flattern [flugiloj]</trd>,
          <trd>klatschen [manoj, frapo sur ies vangon]</trd>,
          <trd>schmatzen [lipoj]</trd>,
          <trd>schnalzen [lango]</trd>,
          <trd>klicken [mekanika ilo]</trd></trdgrp>
        <trdgrp lng="en">
          <trd>chatter<klr> (dentoj)</klr></trd>,
          <trd>crack<klr> (glacio, fingroj, vipo)</klr></trd>,
          <trd>flap<klr> (flugiloj)</klr></trd>,
          <trd>slap<klr> (frapo sur ies vangon, kastorvosto sur akvon)</klr></trd>,
          <trd>smack<klr> (per la lipoj)</klr></trd>
        </trdgrp>
        <trdgrp lng="nl">
           <trd>knallen<klr tip="ind"> (v.zweep)</klr></trd>,
              <trd>kraken</trd>,
                 <trd>klappertanden</trd>
                 </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_shch;&c_jo;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>; <trd>&c_h;&c_l;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd></trdgrp>
      </snc>
      <snc mrk="klak.0i.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Sendi signalon al komputilo per tujsekvaj premo kaj kaj
          relaso de <ref tip="malprt" cel="mus.0o.KOMP">musa</ref>
          butono a&ubreve; klavo (kutime akompanataj de malla&ubreve;ta
          klaketo):
          <ekz>
            <tld/>i sur ligilo por elekti TTT-pa&gcirc;on.
          </ekz>
        </dif>
        <rim>
          La transitiva formo estas
          <ref tip="vid" cel="klak.al0i.KOMP">alklaki</ref>.
        </rim>
        <trd lng="en">click</trd>
        <trd lng="nl">klikken<klr tip="ind"> (muis v.computer)</klr></trd>
        <trd lng="ru">&c_shch;&c_jo;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
      </snc>
    </drv>
    <drv mrk="klak.al0i">
      <kap>al<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="klak.al0i.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <tld lit="K"/>i <sncref ref="klak.0i.KOMP"/> &ccirc;e iu ekrana objekto
          por aktivigi &gcirc;in (elekti menueron, la&ubreve;iri ligilon ktp).
        </dif>
      </snc>
      <trd lng="de">anklicken</trd>
      <trd lng="nl">klikken op</trd>
      <trd lng="en">click</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_shch;&c_jo;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol; &c_p;&c_o; <klr>&c_ch;&c_je;&c_m;&c_u;-&c_l;.</klr></trd>
      </trdgrp>
    </drv>
    <drv mrk="klak.0ileto">
      <kap><tld/>ileto</kap>
      <snc>
        <dif>
          <ref tip="sin" cel="kastan.0o">Kastanjeto</ref>. 
        </dif>
      </snc>
    <trd lng="nl">castagnet</trd>
    </drv>
    <drv mrk="klak.0cxapelo">
      <kap><tld/>&ccirc;apelo</kap>
      <snc>
        <dif>
          Alta &ccirc;apelo, povanta plati&gcirc;i, por ke oni
          &gcirc;in portu sub la brako.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>top hat</trd>,
        <trd>high hat</trd>
      </trdgrp>
      <trd lng="ru">&c_sh;&c_a;&c_p;&c_o;&c_k;&c_l;&c_ja;&c_k;</trd>
    </drv>
  </art>

