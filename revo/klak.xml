<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>klak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="klak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klak.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> per seka mallonga
      sono pro rapida kruda movo de io kontra&ubreve; io:
      <ekz>
        li <klr>[...]</klr> <tld/>is malpacience per la fingroj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i per la lango<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as vipo, vangofrapo, dentoj, pordo, beko
        de cikonio, ekpafo, bato, pluvo (sur vitroj), moneroj (sur
        metalplato);
      </ekz>
      <ekz>
        je certaj horoj estis malfacile a&ubreve;di super la 
        <ind><tld/>oj</ind>, sibloj kaj mu&gcirc;oj de la ma&scirc;inaro
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 15</lok></fnt>;
        <trd lng="fr">claquement</trd>
        <trd lng="nl">klepperen<klr tip="ind"> (zn.)</klr></trd>
      </ekz>
      <ekz>
        tondro<tld/>egoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_ch;&c_o;&c_w;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_a;&c_z;&c_g;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_ja;&c_p;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>klappern [i.a. dentoj]</trd>,
      <trd>schnipsen [fingroj]</trd>,
      <trd>knallen [vipo]</trd>,
      <trd>flattern [flugiloj]</trd>,
      <trd>klatschen [manoj, frapo sur ies vangon]</trd>,
      <trd>schmatzen [lipoj]</trd>,
      <trd>schnalzen [lango]</trd>,
      <trd>klicken [mekanika ilo]</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>chatter<klr> (dentoj)</klr></trd>,
      <trd>crack<klr> (glacio, fingroj, vipo)</klr></trd>,
      <trd>flap<klr> (flugiloj)</klr></trd>,
      <trd>slap<klr> (frapo sur ies vangon, kastorvosto sur
      akvon)</klr></trd>,
      <trd>smack<klr> (per la lipoj)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>claquer<klr tip="ind"> (intr., produire un
      claquement)</klr></trd>
      <trd>crever<klr tip="ind"> (intr., faire un bruit sec)</klr></trd>
      <trd>p&eacute;ter<klr tip="ind"> (intr., faire un bruit sec)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kattan</trd>,
      <trd>csattan</trd>,
      <trd>csetten</trd>,
      <trd>durran</trd>,
      <trd>koppan</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>knallen<klr tip="ind"> (v.zweep)</klr></trd>,
      <trd>kraken</trd>,
      <trd>klappertanden</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_jo;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>; <trd>&c_h;&c_l;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="klak.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Sendi signalon al komputilo per tujsekvaj premo kaj
      relaso de <ref tip="malprt" cel="mus.0o.KOMP">musa</ref>
      butono a&ubreve; klavo (kutime akompanataj de malla&ubreve;ta
      klaketo):
      <ekz>
        <tld/>i sur ligilo por elekti TTT-pa&gcirc;on.
      </ekz>
    </dif>
    <rim>
      La transitiva formo estas
      <ref tip="vid" cel="klak.al0i.KOMP">alklaki</ref>.
    </rim>
    <trd lng="be">&c_sh;&c_ch;&c_o;&c_w;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
    <trd lng="en">click</trd>
    <trd lng="de">klicken</trd>
    <trdgrp lng="fr">
      <trd>cliquer<klr tip="ind"> (intr., info.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kattint</trd>,
      <trd>klikkel</trd>
    </trdgrp>
    <trd lng="it">cliccare</trd>
    <trd lng="nl">klikken<klr tip="ind"> (muis v.computer)</klr></trd>
    <trd lng="ru">&c_shch;&c_jo;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="klak.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KOMP</uzo>
  <snc mrk="klak.al0i.KOMP">
    <dif>
      <tld lit="K"/>i <sncref ref="klak.0i.KOMP"/> &ccirc;e iu ekrana objekto
      por aktivigi &gcirc;in (elekti menueron, la&ubreve;iri ligilon ktp).
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_w;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="de">anklicken</trd>
  <trd lng="en">click</trd>
  <trd lng="fr">cliquer (sur)<klr tip="ind"> (info.)</klr></trd>
  <trdgrp lng="hu">
    <trd>r&aacute;kattint</trd>,
    <trd>r&aacute;klikkel</trd>
  </trdgrp>
  <trd lng="it">cliccare (su)</trd>
  <trd lng="nl">aanklikken</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_shch;&c_jo;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol; &c_p;&c_o; <klr>&c_ch;&c_je;&c_m;&c_u;-&c_l;.</klr></trd>
  </trdgrp>
</drv>

<drv mrk="klak.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="klak.0ilo.MUZ">
    <dif>
      Signalilo el du kun<tld/>ataj tabuletoj:
      <ekz>
        seka, mallonga rido, kiu nun similis <tld/>ilon de nokta
        gardisto
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        multaj knaboj postenis por fari gvatadon kaj anonci la horojn
        <tld/>ile
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 59a, volumo 2a, p.
        292a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>claquette</trd>,
    <trd>claquoir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kerepl&odblac;</trd>,
    <trd>csap&oacute;l&eacute;c</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ratel</trd>,
    <trd>klepper</trd>
  </trdgrp>
</drv>

<drv mrk="klak.0ileto">
  <kap><tld/>ileto</kap>
  <snc mrk="klak.0ileto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="kastan.0o">kastanjeto</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_je;&c_t;&c_a;</trd>
  <trd lng="de">Kastagnetten</trd>
  <trd lng="en">castanet</trd>
  <trd lng="fr">castagnettes</trd>
  <trd lng="hu">kasztanyetta</trd>
  <trd lng="nl">castagnet</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_je;&c_t;&c_a;</trd>
</drv>

<drv mrk="klak.man0i">
  <kap>man<tld/>i, <var><kap><tld/>i per la manoj</kap></var></kap>
  <snc mrk="klak.man0i.KOMUNE">
    <dif>
      <tld lit="K"/>e bruigi la manojn, ilin kunbatante:
      <ekz>
        damo Laji man<tld/>is kaj enkuris
        Lilio kaj Papilio
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplaux.0i.KOMUNE">apla&ubreve;di</ref>
    </refgrp>
  </snc>
  <trd lng="fr">claquer des mains</trd>
</drv>

<drv mrk="klak.0cxapelo">
  <kap><tld/>&ccirc;apelo</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="klak.0cxapelo.TEKS">
    <dif>
      Alta <ref tip="super" cel="cxapel.0o.kapvesto">&ccirc;apelo</ref>,
      povanta
      plati&gcirc;i, por ke oni &gcirc;in portu sub la brako.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_p;&c_a;&c_k;&c_l;&c_ja;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Klappzylinder</trd>,
    <trd>Chapeau Claque</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>top hat</trd>,
    <trd>high hat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>claque<klr tip="ind"> (chapeau claque)</klr></trd>,
    <trd>chapeau claque</trd>,
    <trd>gibus</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>klakk</trd>,
    <trd>rug&oacute;s <ind>cilinder</ind> <klr tip="ind">(kalap)</klr></trd>
  </trdgrp>
  <trd lng="nl">klakhoed</trd>
  <trd lng="ru">&c_sh;&c_a;&c_p;&c_o;&c_k;&c_l;&c_ja;&c_k;</trd>
</drv>
</art>
