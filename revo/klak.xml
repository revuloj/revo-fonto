<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klak.xml,v 1.36 2022/05/01 08:09:24 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>klak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="klak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klak.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> per seka mallonga
      sono pro rapida kruda movo de io kontra&ubreve; io:
      <ekz>
        se vi volos rajdi rapide, <tld/>u per la lango kaj kriu hop hop
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>!
      </ekz>
      <ekz>
        ekregis lin tiel terura timo, ke duan fojon li estingis la tor&ccirc;on kaj tremante,
        <tld/>ante per la dentoj li en&scirc;ovis sin
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam levi&gcirc;is vento, &ccirc;iuj skeletoj <tld/>adis tiel, ke la birdetoj timis
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la cikonio promenadis sur siaj longaj ru&gcirc;aj piedoj kaj <tld/>adis
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        Nero komencis <tld/>i per la manoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>os la vipo hodia&ubreve; sur viaj dorsoj
        <fnt>
          <bib>SatirRak</bib>
        </fnt>!
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;as gaja muziko <klr>[&#x2026;]</klr> kaj <tld/>ado <klr>(kp <ref cel="knal.0i">knali</ref>)</klr> de korkoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie <tld/>adis la maldikaj pordetoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kolere <tld/>is <klr>(kp <ref cel="tint.0i.SON">tinti</ref>)</klr> per la &scirc;losilo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksonis <tld/>ado de pantofloj
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la homoj <tld/>adas tra la rond&scirc;tonaj stratoj <klr>[&#x2026;]</klr> kun piedoj vestitaj
        per ligno-&scirc;uoj
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ado de la hufoj doloris la orelojn
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        apude ili ekvidis arbeton plenan de nuksoj, ili de&scirc;iris kaj <tld/>is 
        <klr>(kp <ref cel="krak.dis0i">diskrak</ref>igi, <ref cel="krev.0igi">krevigi</ref>)</klr>
        kaj man&gcirc;is la delikatajn kernojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        kla&ccirc;a&jcirc;o kaj &ccirc;ikana&jcirc;o <klr>[&#x2026;]</klr> zumis sur la langoj tiel, ke de ilia lerta
        movi&gcirc;ado a&ubreve;di&gcirc;is konstanta <tld/>ado
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;esu do, mi petas, vi, <tld/>istinoj
        <klr>(vd <ref cel="klacx.0i">kla&ccirc;i</ref>)</klr>! tie &ccirc;i estas afero grava
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[...]</klr> <tld/>is malpacience per la fingroj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        telefonoj sonoris, skribma&scirc;inoj <tld/>adis
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        funkciis kvin-ses miloj da strangaj, sed tute egalaj teksma&scirc;inoj, kies <tld/>ado
        kuni&gcirc;is en ia muzika sono
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        rapide turni&gcirc;antaj radoj, <tld/>antaj transmisioj
        <fnt>
          <bib>Tor</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="knal.0i">knali</ref>,
       <ref cel="tint.0i.SON">tinti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_ch;&c_o;&c_w;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_a;&c_z;&c_g;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_ja;&c_p;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>klappern <klr>(Storch, Z&auml;hne)</klr></trd>,
      <trd>schnipsen <klr>(Finger)</klr></trd>,
      <trd>knallen <klr>(Peitsche)</klr></trd>,
      <trd>flattern <klr>(Fl&uuml;gel)</klr></trd>,
      <trd>klatschen <klr>(Hand, Wange)</klr></trd>,
      <trd>schmatzen <klr>(Lippen)</klr></trd>,
      <trd>schnalzen <klr>(Zunge)</klr></trd>,
    </trdgrp>
    <trdgrp lng="en">
      <trd>chatter<klr> (dentoj)</klr></trd>,
      <trd>crack<klr> (glacio, fingroj, vipo)</klr></trd>,
      <trd>flap<klr> (flugiloj)</klr></trd>,
      <trd>slap<klr> (frapo sur ies vangon, kastorvosto sur
      akvon)</klr></trd>,
      <trd>smack<klr> (per la lipoj)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>chasquear<klr tip="ind"> (intr., producir un
      chasquido)</klr></trd>,
      <trd>hacer un ruido seco</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>claquer<klr tip="ind"> (intr., produire un
      claquement)</klr></trd>
      <trd>crever<klr tip="ind"> (intr., faire un bruit sec)</klr></trd>
      <trd>p&eacute;ter<klr tip="ind"> (intr., faire un bruit sec)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kattan</trd>,
      <trd>csattan</trd>,
      <trd>csetten</trd>,
      <trd>durran</trd>,
      <trd>koppan</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>knallen<klr tip="ind"> (v.zweep)</klr></trd>,
      <trd>kraken</trd>,
      <trd>klappertanden</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_jo;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>; <trd>&c_h;&c_l;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="klak.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Sendi signalon al komputilo per tujsekvaj premo kaj
      relaso de <ref tip="malprt" cel="mus.0o.KOMP">musa</ref>
      butono a&ubreve; klavo (kutime akompanataj de malla&ubreve;ta
      <tld/>eto):
      <ekz>
        por vidi la bildon <tld/>u &ccirc;i tie
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">Vorta futbalo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la internacia firmao <klr>[&#x2026;]</klr> proponas al konsumantoj a&ccirc;eti <tld/>ante en
        Interreto, kaj ricevi ene de du tagoj
        <fnt>
          <bib>MD11</bib>
          <vrk>Enketo en la staploj de la reta komerco</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la procedo estas simpla: <tld/>u sur <nom>www.meine-marke.at</nom>, ser&ccirc;u bildon, kontrolu,
        mendu
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2004/008373.html">Ri-mark-inde</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La transitiva formo estas
      <ref tip="vid" cel="klak.al0i.KOMP">al<tld/>i</ref>.
    </rim>
    <trd lng="be">&c_sh;&c_ch;&c_o;&c_w;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
    <trd lng="en">click</trd>
    <trd lng="de">klicken <klr>(Maus)</klr></trd>
    <trd lng="es">clic</trd>
    <trdgrp lng="fr">
      <trd>cliquer<klr tip="ind"> (intr., info.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kattint</trd>,
      <trd>klikkel</trd>
    </trdgrp>
    <trd lng="it">cliccare</trd>
    <trd lng="nl">klikken<klr tip="ind"> (muis v.computer)</klr></trd>
    <trd lng="ru">&c_shch;&c_jo;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="bo">&t_la;&t_ga;&t_tsheg;&t_pa;&t_tsheg;&t_ra;&t_data;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>cvakat</trd>,
    <trd>klapat</trd>,
    <trd>louskat</trd>,
    <trd>luskat</trd>,
    <trd>plesknout</trd>,
    <trd>pl&aacute;cat</trd>,
    <trd>pr&aacute;skat</trd>,
    <trd>tleskat</trd>
  </trdgrp>
  <trd lng="es">hacer clak<klr tip="amb"> (un ruido brusco)</klr></trd>
  <trdgrp lng="pl">
    <trd>klaska&cacute;</trd>,
    <trd>klikn&aogonek;&cacute;</trd>,
    <trd>stuka&cacute;</trd>,
    <trd>trzaska&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>aplauda</trd>,
    <trd>bate</trd>
  </trdgrp> 
  <trdgrp lng="sk">
    <trd>cvaka&tcaron;</trd>,
    <trd>drkota&tcaron;</trd>,
    <trd>klepota&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44F;&#x441;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x442;&#x443;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&c_s;&#x442;&#x443;&#x43A;&#x43E;&#x442;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x442;&#x443;&#x43A;&#x43E;&#x442;&#x456;&#x442;&#x438;</trd>,
    <trd>&#x43A;&#x43B;&#x430;&#x446;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x440;&#x44F;&#x441;&#x43A;&#x430;&#x442;&#x438; <klr>(&#x43D;-&#x434;, &#x43F;&#x440;&#x43E; &#x43F;&#x43E;&#x441;&#x442;&#x440;&#x456;&#x43B;, &#x431;&#x430;&#x442;&#x456;&#x433;, &#x434;&#x432;&#x435;&#x440;&#x456;)</klr></trd>,
    <trd>&#x43A;&#x43B;&#x435;&#x43A;&#x43E;&#x442;&#x430;&#x442;&#x438; <klr>(&#x43D;-&#x434;, &#x43F;&#x440;&#x43E; &#x43B;&#x435;&#x43B;&#x435;&#x43A;&#x443;)</klr></trd>,
    <trd>&#x434;&#x437;&#x432;&#x44F;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x440;&#x44F;&#x436;&#x447;&#x430;&#x442;&#x438; <klr>(&#x43F;&#x440;&#x43E; &#x449;&#x43E;&#x441;&#x44C; &#x43C;&#x435;&#x442;&#x430;&#x43B;&#x456;&#x447;&#x43D;&#x435;)</klr></trd>,
    <trd>&#x431;&#x440;&#x438;&#x43D;&#x456;&#x442;&#x438;</trd>,
    <trd>&#x434;&#x437;&#x432;&#x435;&#x43D;&#x456;&#x442;&#x438; <klr>(&#x43D;-&#x434;, &#x43F;&#x440;&#x43E; &#x448;&#x438;&#x431;&#x438;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="klak.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="klak.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de io <tld/>anta, subite fermi&gcirc;anta,
      fendi&gcirc;anta...:
      <ekz>
        iu surrampis <tld/> <tld/> la marmoran &scirc;tuparon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Re&gcirc;o rano a&ubreve; fera Henriko</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia po&scirc;o estis plena de nuksoj,
        oni a&ubreve;dis sen&ccirc;ese: <tld/>! <tld/>
        <fnt>
          <bib>Fab4</bib> <vrk>Dekdu per la po&scirc;to</vrk>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxapel.klak0o"><tld/>&ccirc;apelo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>klack</trd>,
      <trd>klick</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="es">
    <trd>&#xA1;clic!</trd>,
    <trd>&#xA1;clac!</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clic !</trd>,
    <trd>clac !</trd>
  </trdgrp>
</drv>

<drv mrk="klak.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="klak.0o.SON">
    <dif>
      Mallonga sono de kunbato de malmolaj a&jcirc;oj:
      <ekz>
        je certaj horoj estis malfacile a&ubreve;di super la 
        <tld/>oj, sibloj kaj mu&gcirc;oj de la ma&scirc;inaro
        <fnt>
          <bib>&Scirc;talrato</bib>
          <lok>&ccirc;ap. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>casta&ntilde;etear</trd>,
      <trd>chasquear los dedos</trd>
    </trdgrp>
    <trd lng="fr">claquement</trd>
    <trd lng="nl">klepperen<klr tip="ind"> (zn.)</klr></trd>
  </snc>
  <snc mrk="klak.0o.FON">
    <uzo tip="fak">FON</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>
      sonigata de la lango a&ubreve; lipo sen pulma blovo:
      <ekz>
        en la lingvo, kiun eble vi konas kiel hotentota, kiun oni nomas nun kojkoja, oni
        uzas tiujn kvaza&ubreve; matematikajn simbolojn por prezenti diversajn <tld/>ojn:
        <frm>&#x1C0; &#x1C1; &#x1C3; &#x1C2;</frm>
        <fnt>
          <aut>J. Wells</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_sQbwOdJCv-s/">Kiam la latina skribo ne sufi&ccirc;as</url></vrk>
          <lok>Londona Klubo, 2021-05-28</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">clic<klr> (fon.)</klr></trd>
  </snc>
</drv>

<drv mrk="klak.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc mrk="klak.0eto.FON">
    <uzo tip="fak">FON</uzo>
    <fnt><bib>Viki</bib></fnt>
    <dif>
      <ref tip="dif" cel="klak.0o.FON"><tld lit="K"/>o<sncref/></ref>:
      <ekz>
        la sud-amerika <nom>pirah&atilde;</nom> kaj la papua <nom>rotokas</nom> lingvoj <klr>[&#x2026;]</klr> kontenti&gcirc;as
        per 11 sonoj kaj aliflanke la <nom>!X&#x169;</nom> lingvo parolata en Sud-Afriko uzas 141
        sonojn, inter ili <tld/>etojn
        <fnt>
          <aut>I. Koutny</aut>
          <vrk><url ref="https://interl.home.amu.edu.pl/interlingvistiko/IlonaK_Tipologio2015.pdf">Tipologia karakterizo de esperanto kiel natura lingvo</url></vrk>
          <lok>[2015]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">clic<klr> (fon.)</klr></trd>
</drv>

<drv mrk="klak.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="klak.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on, ekzemple per kunbato:
      <ekz>
        la&ubreve; <nom>Li&ubreve;</nom> subite <klr>[&#x2026;]</klr> <tld/>igis la du 
        duonojn de sia kastanjeto
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        la du gardistoj kun<tld/>igis la kalkanojn kaj li supozis, ke ili denove soldate
        salutis
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Tagmeze en Tunizo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zusammen <ind>schlagen</ind> <klr>(H&auml;nde, Fersen usw.)</klr></trd>
  <trd lng="es">producir un <ind>chasquido</ind></trd>
  <trd lng="fr">faire <ind>claquer</ind></trd>
  <trd lng="pl">wytworzy&cacute; klikni&eogonek;cie</trd> 
</drv>

<drv mrk="klak.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="klak.0ilo.MUZ">
    <dif>
      Signalilo el du kun<tld/>igataj tabuletoj:
      <ekz>
        per la beko <klr>[la cikonio]</klr> <tld/>adis tiel, kvaza&ubreve; &gcirc;i estus <tld/>ilo
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        seka, mallonga rido, kiu nun similis <tld/>ilon de nokta
        gardisto
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        multaj knaboj postenis por fari gvatadon kaj anonci la horojn
        <tld/>ile
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 59a, volumo 2a, p.
        292a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Klapper</trd>,
    <trd>Klatsche</trd>
  </trdgrp>
  <trd lng="es">claqueta</trd>
  <trdgrp lng="fr">
    <trd>claquette</trd>,
    <trd>claquoir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kerepl&odblac;</trd>,
    <trd>csap&oacute;l&eacute;c</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ratel</trd>,
    <trd>klepper</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>grzechotka</trd>,
    <trd>klaps</trd>,
    <trd>ko&lstroke;atka</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ardezie</trd>,
    <trd>criticastru</trd>,
    <trd>mori&#x219;c&abreve;</trd>,
    <trd>tr&abreve;nc&abreve;neal&abreve;</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x440;&#x456;&#x441;&#x43A;&#x430;&#x447;&#x43A;&#x430;</trd>
</drv>

<drv mrk="klak.0ileto">
  <kap><tld/>ileto</kap>
  <snc mrk="klak.0ileto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="kastan.0o">kastanjeto</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_je;&c_t;&c_a;</trd>
