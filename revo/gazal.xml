<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gazal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gazal.0o">
  <kap><tld/>o</kap>
  <snc mrk="gazal.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Pers-devena lirika versa&jcirc;o kun amaj, subtone mistikaj temoj,
      kun memalvoko de la poeto en la lastaj versoj kaj rimskemo aabacada...
    </dif>
  </snc>
  <snc mrk="gazal.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Duonklasika <ref tip="super" cel="kant.0ajxo">kanta&jcirc;o</ref> la&ubreve;
      <tld/>-poemo tradicia de Irano &gcirc;is Nordhindujo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ghazal</trd>
  </trdgrp>
</drv>
</art>

