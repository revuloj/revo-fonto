<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: persimon.xml,v 1.6 2019/03/22 14:10:16 revo Exp $">
<kap>
  <rad>persimon</rad>/o<fnt>PIV1</fnt>
</kap>

<drv mrk="persimon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="persimon.0o.orienta">
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref> de <tld/>ujo
      orienta, ekstere simila al tomato, ru&gcirc;a
      a&ubreve; flava, sukplena, dol&ccirc;a, kun kvar restantaj
      kalikfolioj:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> estas ronda kaj simila al pomo a&ubreve; tomato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=266852"
              >Persimono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni faris &gcirc;in <klr>(la &ccirc;inan man&gcirc;on
          <nac>zongzi</nac>-o)</klr> el glueca rizo
        kun aldonitaj jujubo, ka&scirc;tano, sekigita <tld/>o,
        ginkofruktoj, ru&gcirc;aj faboj kaj volvis &gcirc;in per folioj
        <fnt>
          <vrk>Zongzi-o &dash; tradicia man&gcirc;a&jcirc;o de la Festo
            de Duobla Kvino</vrk>,
          <lok><url ref="http://es.chinabroadcast.cn/1/2004/08/16/1@13796.htm"
          >&Ccirc;ina Radio Internacia</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kaki<klr>(pflaume)</klr></trd>
  </snc>
  <snc mrk="persimon.0o.virginia">
    <dif>
      Frukto de <tld/>ujo virginia.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>persimmon</trd>,
    <trd>date-plum</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kakiszilva</trd>,
    <trd>datolyaszilva</trd>
  </trdgrp>
  <trd lng="nl">dadelpruim</trd>
  <trd lng="pt">caqui</trd>
  <trd lng="ru">&c_h;&c_u;&c_r;&c_m;&c_a;</trd>
</drv>

<drv mrk="persimon.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el la familio ebonujacoj (<trd lng="la">Diospyrus</trd>),
      de fruktaj arboj kaj arbetoj (proks. 200 specioj).
    </dif>
    <refgrp tip="sub">
      <ref cel="persimon.0ujoorienta"><tld/>ujo orienta</ref>,
      <ref cel="persimon.0ujovirginia"><tld/>ujo virginia</ref>
    </refgrp>
  </snc>
  <trd lng="de">Ebenholzbaum</trd>
  <trdgrp lng="hu">
    <trd>kakiszilvafa</trd>,
    <trd>datolyaszilvafa</trd>
  </trdgrp>
  <trd lng="nl">persimmon</trd>
  <trd lng="ru">&c_h;&c_u;&c_r;&c_m;&c_a;<klr>
      (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="persimon.0ujoorienta">
  <kap><tld/>ujo orienta</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      La plej disvastigita <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref>
      de <tld/>ujo (<trd lng="la">Diospyros kaki</trd>), arbo normale 5&#8239;&ndash;&#8239;8 (sed iam &gcirc;is 25) metrojn alta 
      (iam tamen arbeto), komuna en la sudorienta Azio, en Uzbekujo, Persujo, Ka&ubreve;kazo,
      Krimeo, Egiptujo kaj iom malpli en Italujo kaj Hispanujo:
      <ekz>
        en Ka&ubreve;kazio kaj en la Centra Azio la <tld/>ujo orienta
        kreskas kaj sova&gcirc;e, kaj kulture.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/67/Diospyros_kaki_fruit_01_by_Line1.JPG/360px-Diospyros_kaki_fruit_01_by_Line1.JPG"
      prm="CC BY-SA 3.0"
      >bran&ccirc;o de Orienta <tld/>o (Diospyros kaki), Serres d'Auteuil
        <fnt>
          <aut>Lin&eacute;1</aut>
          <lok><url ref="&WCW;/File:Diospyros_kaki_fruit_01_by_Line1.JPG"
              >https://commons.wikimedia.org/wiki/File:Diospyros_kaki_fruit_01_by_Line1.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Kakibaum</trd>
  <trdgrp lng="hu">
    <trd>&aacute;zsiai <ind>kakiszilva</ind></trd>,
    <trd>&aacute;zsiai <ind>datolyaszilva</ind></trd>
  </trdgrp>
  <trd lng="nl">kakivrucht</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_u;&c_r;&c_m;&c_a;
&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_h;&c_u;&c_r;&c_m;&c_a;
      &c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_a;&c_ja;</trd>,
    <trd>&c_h;&c_u;&c_r;&c_m;&c_a;
      &c_v;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="persimon.0ujojapana">
  <kap><tld/>ujo japana</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Japana <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref> de
      <tld/>ujo (<trd lng="la">Diospyros japonica</trd>).
    </dif>
    <trd lng="nl">Japanse <ind>persimmon</ind></trd>
  </snc>
  <snc mrk="persimon.0ujojapana.orienta">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Orienta <tld/>ujo.
    </dif>
  </snc>
</drv>

<drv mrk="persimon.0ujovirginia">
  <kap><tld/>ujo virginia</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref>
      de <tld/>ujo komuna en la sudo de Usono (<trd lng="la">Diospyros virginiana</trd>).
    </dif>
    <bld
      lok="&WCU;/thumb/5/51/Diospyros_virginiana_-_American_Persimmon.jpg/640px-Diospyros_virginiana_-_American_Persimmon.jpg"
      prm="CC BY-SA 3.0"
      >bran&ccirc;o de floranta Virginia <tld/>o (Diospyros virginiana), Maryland, Usono
        <fnt>
          <aut>Fritzflohrreynolds, propra verko</aut>
          <lok><url ref="&WCW;/File:Diospyros_virginiana_-_American_Persimmon.jpg"
              >https://commons.wikimedia.org/wiki/File:Diospyros_virginiana_-_American_Persimmon.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Amerikanische Persimone</trd>
  <trdgrp lng="hu">
    <trd>amerikai <ind>kakiszilva</ind></trd>,
    <trd>amerikai <ind>datolyaszilva</ind></trd>
  </trdgrp>
  <trd lng="nl">Amerikaanse <ind>persimmon</ind></trd>
  <trd lng="ru">&c_h;&c_u;&c_r;&c_m;&c_a;
    &c_v;&c_i;&c_r;&c_g;&c_i;&c_n;&c_s;&c_k;&c_a;&c_ja;</trd>
</drv>

</art>

