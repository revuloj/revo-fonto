<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rang</rad>/o
</kap>
<drv mrk="rang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rang.0o.hierarkia">
    <dif>
      Grado, &ccirc;e kiu staras iu persono en difinita <ref tip="malprt"
      cel="hierar.0o">hierarkio</ref>:
      <ekz>
        la diversaj <tld/>oj de la eklezia, militista,
        administracia oficistaro;
      </ekz>
      <ekz>
        akiri nobelan <tld/>on;
      </ekz>
      <ekz>
        okupi altan <tld/>on en la socio;
      </ekz>
      <ekz>
        niaj amikoj reprezentus per si &ccirc;iujn <tld/>ojn,
        statojn kaj klasojn de la homoj.
      </ekz>
    </dif>
    <subsnc mrk="rang.0o.militista">
      <ekz>
        oni ne konfuzu militistan <tld/>on kun la ofico;
      </ekz>
      <ekz>
        havi <tld/>on kolonelo.
      </ekz>
      <trd lng="ru">&c_z;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
    </subsnc>
    <subsnc mrk="rang.0o.eklezia">
      <ekz>
        pastra, episkopa <tld/>o.
      </ekz>
      <trd lng="ru">&c_s;&c_a;&c_n;</trd>
    </subsnc>
    <subsnc mrk="rang.0o.ludkarta">
      <ekz>
        &Ccirc;iu <ref tip="vid" cel="emblem.0o.karta">emblemo</ref>
        entenas 13 <ref tip="vid" cel="kart.lud0o">ludkartojn</ref>
        divers<tld/>ajn.
      </ekz>
      <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
    </subsnc>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_n;&c_g;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_r;&c_ja;&c_d;</trd>,
      <trd>&c_z;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rang.0o.gravo">
    <dif>
      Grado de graveco de iu a&ubreve; io:
      <ekz>
        multaj rigardas Esperanton kvaza&ubreve; ion ne seriozan,
        kvaza&ubreve; distron a&ubreve; ilon de dua <tld/>o;
      </ekz>
      <ekz>
        ka&ubreve;&ccirc;uko fari&gcirc;is nun unua<tld/>a materio, kiel la
        &scirc;talo a&ubreve; la karbo.
      </ekz>
      <trdgrp lng="ru">
        <trd>&c_s;&c_o;&c_r;&c_t;</trd>,
        <trd>&c_k;&c_l;&c_a;&c_s;&c_s;</trd>
      </trdgrp>
    </dif>
  </snc>
  <snc mrk="rang.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="rang.0o.vektoroj">
      <dif>
        <klr>(de <frm><k>n</k></frm> vektoroj de
        <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
        <ref cel="dimens.0o.vektoraspaco">Dimensio</ref> de la
        vektora subspaco, kiun ili naskas.
      </dif>
    </subsnc>
    <subsnc mrk="rang.0o.matrico">
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(de <ref cel="matric.n_p0o">(n,p)-matrico</ref>
        <frm>A</frm> super korpo <frm><g>K</g></frm>)</klr>
        <ref cel="dimens.0o.vektoraspaco">Dimensio</ref> de la
        vektora subspaco de <frm><g>K</g><sup><k>n</k></sup></frm>,
        kiun naskas &gcirc;iaj vertikaloj; simb. <frm>rang A</frm>.
      </dif>
      <trd lng="pl">rz&aogonek;d<klr tip="ind"> (macierzy)</klr></trd>
    </subsnc>
    <subsnc mrk="rang.0o.homomorfio">
      <dif>
        <klr>(de <ref cel="homomo.0o">homomorfio</ref> inter
        <ref cel="spac.vektora0o">vektoraj spacoj</ref>)</klr>
        <ref cel="dimens.0o.vektoraspaco">Dimensio</ref> de &gcirc;ia
        bildaro:
        <ekz>
          la <tld/>o de endomorfio de finidimensia spaco egalas al la
          diferenco inter la dimensio de la spaco kaj la dimensio de
          la <ref cel="kern.0o.homomorfio">kerno</ref> de la endomorfio.
        </ekz>
      </dif>
      <trd lng="pl">rz&aogonek;d<klr tip="ind"> (homomorfizmu)</klr></trd>
    </subsnc>
    <trd lng="ru">&c_r;&c_a;&c_n;&c_g;</trd>
  </snc>
  <trd lng="de">Rang</trd>
  <trd lng="en">rank</trd>
  <trd lng="fr">rang</trd>
  <trd lng="nl">rang</trd>
</drv>

<drv mrk="rang.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Okupi iun <tld/>on:
      <ekz>
        li <tld/>as kiel ambasadoro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">rangieren</trd>
  <trd lng="fr">avoir le <ind>rang</ind> de</trd>
</drv>

<drv mrk="rang.alt0a">
  <kap>alt<tld/>a</kap>
  <snc>
    <dif>
      Havanta en hierarkio a&ubreve; en socio gravan, influan a&ubreve;
      presti&gcirc;an funkcion.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;minent</trd>
  </trdgrp>
</drv>

<drv mrk="rang.alt0ulo">
  <kap>alt<tld/>ulo</kap>
  <snc>
    <dif>
      Homo kun grava posteno, funkcio a&ubreve; rolo:
      <ekz>
        estas pensige vidi, kiel alt<tld/>uloj miskomprenas sin
        reciproke<fnt><bib>BonaLingvo</bib>, <lok>p. 5a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="elit.0o">elito</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>huile<klr> (pers. importante)</klr></trd>,
    <trd>notable<klr> (pers.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rang.pli0igi">
  <kap><tld/>o</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="promoc.0i">promocii</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;lever<klr> (en dignit&eacute;)</klr></trd>,
    <trd>promouvoir<klr> (&agrave; un poste)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rang.sam0a">
  <kap>sam<tld/>a</kap>
  <snc>
    <dif>
      Kun sama funkcio, potenco a&ubreve; graveco en socio a&ubreve; societo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;gal<klr> (adj., de m&ecirc;me rang)</klr></trd>,
    <trd>pair<klr> (adj., de m&ecirc;me rang)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rang.sam0ulo">
  <kap>sam<tld/>ulo</kap>
  <snc>
    <dif>
      Homo egala al alia rilate oficon, gravecon, a&ubreve;toritaton.
    </dif>
  </snc>
  <ref tip="vid" cel="koleg.0o.samsocieta">kolego <sncref/></ref>
  <trdgrp lng="fr">
    <trd>&eacute;gal<klr> (subst., pers. de m&ecirc;me rang)</klr></trd>,
    <trd>pair<klr> (subst., pers. de m&ecirc;me rang)</klr></trd>
  </trdgrp>
</drv>
</art>
