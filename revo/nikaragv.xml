<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nikaragv</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="nikaragv.0o">
  <kap>
    <tld lit="N"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="nikaragv.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt"
      cel="amerik.A0o">Ameriko</ref> kun &ccirc;efurbo Managvo.
      Teritorio de Respubliko Nikaragvo.
      ISO-3166 kodo: NI.
      &Gcirc;i limas kun
      <ref tip="vid" cel="hondur.0o.GEOG">Honduro</ref>,
      <ref tip="vid" cel="kostarik.0o.GEOG">Kostariko</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Nikaragua</trd>,
      <trd>Republika <ind>Nikaragui</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
