<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Nikaragv</rad>/o
</kap>

<drv mrk="nikaragv.N0o">
  <kap>
    <ofc>8</ofc><tld/>o
     <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Nikaragvo">Nikaragvo</url></lok></fnt>
  </kap>
  <snc mrk="nikaragv.N0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la centra
      
      <ref tip="malprt"
      cel="amerik.A0o">Ameriko</ref> kun &ccirc;efurbo Managvo.
      Teritorio de Respubliko Nikaragvo.
      ISO-3166 kodo: NI.
      &Gcirc;i limas kun
      <ref tip="vid" cel="hondur.H0o.GEOG">Honduro</ref>,
      <ref tip="vid" cel="kostarik.0o.GEOG">Kostariko</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>.
    </dif>
    </snc>
    <trd lng="be">&c_N;&c_ib;&c_k;&c_a;&c_r;&c_a;&c_g;&c_u;&c_a;</trd>
    <trd lng="bg">&c_N;&c_i;&c_k;&c_a;&c_r;&c_a;&c_g;&c_u;&c_a;</trd>
    <trd lng="de">Nicaragua</trd>
    <trd lng="en">Nicaragua</trd>
    <trd lng="es">Nicaragua</trd>
  <trd lng="fa">&a_n;&f_y;&f_k;&a_A;&a_r;&a_A;&f_g;&a_w;&a_A;</trd>
    <trdgrp lng="fr">
      <trd>Nicaragua</trd>,
      <trd>R&eacute;publique du Nicaragua</trd>
    </trdgrp>
    <trd lng="id">Nikaragua</trd>
    <trd lng="nl">Nicaragua</trd>
    <trdgrp lng="pl">
      <trd>Nikaragua</trd>,
      <trd>Republika <ind>Nikaragui</ind></trd>
    </trdgrp>
    <trd lng="pt">Nicar&aacute;gua</trd>
</drv> 

<drv mrk="nikaragv.0ano">
  <kap><tld lit="n"/>ano,
  <var><kap><tld lit="n"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Nikaragvo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Nicaraguayen</trd>
    <trd>Nicaraguayenne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="nikaragv.0a">
  <kap><tld lit="n"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Nikaragvo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>nicaraguayen</trd>
    <trd>nicaraguayenne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

