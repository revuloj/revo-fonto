<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Nikaragv</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="nikaragv.N0o">
  <kap>
    <ofc>8</ofc><tld/>o
     <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">NI</mlg>
  <snc mrk="nikaragv.N0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref> en la centra      
      <ref tip="malprt" cel="amerik.A0o">Ameriko</ref> kun &ccirc;efurbo Managvo;
      teritorio de Respubliko Nikaragvo, limanta kun
      <ref tip="vid" cel="hondur.H0o.GEOG">Honduro</ref>,
      <ref tip="vid" cel="kostarik.0o.GEOG">Kostariko</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>:
      <ekz>
        li laboris kiel volontulo inter la malri&ccirc;uloj en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2010/009731.php">Tajg kaj Tomas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li rakontis pri la landoj, en kiuj li estis konsulo &ndash; Japanio, Germanio, <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2011/010260.php">Labirinto en &ccirc;ina lo&gcirc;kvartalo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la raporto indikis, ke Honduro, Kostariko kaj <tld/>o atingis la statuson
        de &ccirc;efaj trairo-landoj por la kontrabandaj narkota&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010671.php">Kreskanta Interret-minaco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_N;&c_ib;&c_k;&c_a;&c_r;&c_a;&c_g;&c_u;&c_a;</trd>
    <trd lng="bg">&c_N;&c_i;&c_k;&c_a;&c_r;&c_a;&c_g;&c_u;&c_a;</trd>
    <trd lng="de">Nicaragua</trd>
    <trd lng="en">Nicaragua</trd>
    <trd lng="es">Nicaragua</trd>
  <trd lng="fa">&a_n;&f_y;&f_k;&a_A;&a_r;&a_A;&f_g;&a_w;&a_A;</trd>
    <trdgrp lng="fr">
      <trd>Nicaragua</trd>,
      <trd>R&eacute;publique du Nicaragua</trd>
    </trdgrp>
    <trd lng="id">Nikaragua</trd>
    <trd lng="nl">Nicaragua</trd>
    <trdgrp lng="pl">
      <trd>Nikaragua</trd>,
      <trd>Republika <ind>Nikaragui</ind></trd>
    </trdgrp>
    <trd lng="pt">Nicar&aacute;gua</trd>
</drv> 

<drv mrk="nikaragv.0a">
  <kap><tld lit="n"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la Sandinista Fronto gvidis la lukton kontra&ubreve; la diktaturo de la familio 
        <nom>Somoza</nom> en <tld/>o de 1978, tiu lukto estis nomita <tld lit="n"/>a 
        revolucio kaj fini&gcirc;is per la eniro de la ribeluloj en Managvon en 1979
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40232">Sandinisto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nicaraguanisch</trd>
  <trdgrp lng="fr"> 
    <trd>nicaraguayen</trd>
    <trd>nicaraguayenne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="nikaragv.0ano">
  <kap><tld lit="n"/>ano,
  <var><kap><tld lit="n"/>anino</kap></var></kap>
  <snc>
    <dif>
      Civitano, civitanino de <tld/>o:
      <ekz>
        <nom>Mart&iacute;</nom> avertas klare pri tiu minaco kiu dek<klr>[ ]</klr>kvin jarojn poste estos
        ripetita de sia samstilano modernisma <tld lit="n"/>ano <nom>Rub&eacute;n Dar&iacute;o</nom> en 
        lia poemo &leftquot;Al <nom>Roosevelt</nom>&rightquot; 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40740">Jos&eacute; Mart&iacute;</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Nicaraguaner</trd>
  <trdgrp lng="fr"> 
    <trd>Nicaraguayen</trd>
    <trd>Nicaraguayenne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

</art>

