<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mordent.xml,v 1.10 2022/08/16 09:28:03 revo Exp $">
<kap>
  <rad>mordent</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>

<drv mrk="mordent.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="mordent.0o.MUZ">
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      kiu alternigas la tonon unu- a&ubreve; du-foje kun alia tono duon- a&ubreve;
      plendute diferenca:
      <ekz>
        en la 19a jarcento <klr>...</klr> la nomo <tld/>o &gcirc;enerale
        apliki&gcirc;is al tio, kio nun nomi&gcirc;as la <tld/>o supra
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Ornamo (muziko)&rightquot;,
        2006-06-12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_r;&c_d;&c_e;&c_n;&c_t;</trd>
  <trd lng="de">Mordent<klr tip="amb"> (Musik)</klr></trd>
  <trd lng="en">mordent<klr> (musical ornament)</klr></trd>
  <trd lng="es">mordente</trd>
  <trd lng="fr">mordant<klr> (ornement musical)</klr></trd>
  <trd lng="it">mordente <klr>(mus.)</klr></trd>
  <trd lng="pl">mordent</trd>
  <trd lng="pt">mordente <klr>(m&uacute;s.)</klr></trd>
  <trd lng="ro">mordent</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_d;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
