<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mordent</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="mordent.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu komencas la tonon kaj alternigas &gcirc;in unu- a&ubreve;
      du-foje kun alia tono duon- a&ubreve; plendute diferenca.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>mordant<klr> (ornement musical)</klr></trd>
  </trdgrp>
</drv>
</art>

