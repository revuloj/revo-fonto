<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hamelen.xml,v 1.4 2023/11/15 16:45:17 afido Exp $">
    <kap>
      <rad>Hamelen</rad>/o
    </kap>

<drv mrk="hamelen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref>
      &ccirc;e Vezero, &ccirc;. 40&#x202F;km sudokcidente de Hanovro:
      <ekz>
        la legendo pri la Ratkaptisto (a&ubreve; Flutisto)
        de <tld/>o <klr>[&#x2026;]</klr> bazi&gcirc;as
        sur rakonto el la jaro 1284
        <fnt>
          <vrk><url ref="https://www.esperanto.de/eo/gek2015/hameleno">Hameleno kaj la Vezera Montaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        92a Germana Esperanto-Kongreso en <tld/>o
        <fnt>
          <lok><url ref="https://www.esperanto.de/eo/enhavo/gek2015/92a-germana-esperanto-kongreso-en-hameleno-hameln-22a-25a-de-majo-2015?lingvo=eo">92a Germana Esperanto-Kongreso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_a;&c_m;&c_je;&c_l;&c_mol;&c_n;</trd>
    <trd lng="de">Hameln</trd>
    <trd lng="en">Hamelin</trd>
    <trd lng="fr">Hamelin <klr>(Allemagne)</klr></trd>
    <trd lng="nl">Hamelen</trd>
  </snc>
  <trd lng="zh">&#x54C8;&#x6885;&#x6069; <pr>h&#x101;m&#xE9;i&#x113;n</pr></trd>
</drv>

  </art>

