<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Vergili</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vergili.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
    </dif>
    <trd lng="be">&c_V;&c_e;&c_r;&c_g;&c_ib;&c_l;&c_ib;&c_j;</trd>
    <trd lng="bg">&c_V;&c_je;&c_r;&c_g;&c_i;&c_l;&c_i;&c_j;</trd>
    <trd lng="ca">Virgili</trd>
    <trd lng="de">Vergil</trd>
    <trd lng="en">Virgil</trd>
    <trd lng="hu">Virgil</trd>
    <trd lng="lat">Vergilius</trd>
    <trd lng="nl">Vergilius</trd>
  </snc>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      <ref tip="lst" cel="beletr.0isto"
      lst="voko:historiaj_personoj">Beletristo</ref> latina (proksimume
      70 a. K. &ndash; 19 a. K.) vivinta en
      Italio,
      a&ubreve;toro de pa&scirc;tistaj poemoj kaj de la eposo <ref tip="vid"
      cel="ene.0ado">Eneado</ref>.
    </dif>
    <trd lng="be">&c_P;&c_u;&c_b;&c_l;&c_ib;&c_j; <ind>&c_V;&c_je;&c_r;&c_g;&c_ib;&c_l;&c_ib;&c_j;</ind> &c_M;&c_a;&c_r;&c_o;&c_n;</trd>
    <trd lng="bg">&c_P;&c_u;&c_b;&c_l;&c_i;&c_j; <ind>&c_V;&c_je;&c_r;&c_g;&c_i;&c_l;&c_i;&c_j;</ind> &c_M;&c_a;&c_r;&c_o;&c_n;</trd>
    <trd lng="ca">Publi <ind>Virgili</ind> Mar&oacute;</trd>
    <trd lng="de">Vergilius</trd>
    <trd lng="en">Publius <ind>Vergilius</ind> Maro</trd>
    <trd lng="hu">Vergilius</trd>
    <trd lng="lat">Publius <ind>Vergilius Maro</ind></trd>
    <trd lng="nl">Publius <ind>Vergilius</ind> Maro</trd>
  </snc>                      
  <trd lng="br">Virgilius</trd>
  <trd lng="es">Virgilio</trd>
  <trd lng="fr">Virgile</trd>
  <trd lng="it">Virgilio</trd>
  <trd lng="ru">&c_V;&c_je;&c_r;&c_g;&c_i;&c_l;&c_i;&c_j;</trd>
</drv>
</art>

