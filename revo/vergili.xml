<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vergili.xml,v 1.20 2024/05/03 22:25:34 revo Exp $">
<kap>
  <rad>Vergili</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vergili.0o">
  <kap><tld/>o</kap>
  <snc mrk="vergili.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      <ref tip="lst" cel="beletr.0isto" lst="voko:historiaj_personoj">Beletristo</ref> latina
      (proksimume
      70 a. K. &ndash; 19 a. K.) vivinta en Romio,
      a&ubreve;toro de pa&scirc;tistaj poemoj kaj de la epopeo
      <ref tip="vid" cel="ene.0ado">Eneado</ref>:
      <ekz>
        en Eneado <klr>[&#8230;]</klr> post alvoko de la muzoj, <tld/>o prirakontas la malbonvolon de la
        diino Juno kontra&ubreve; Trojo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12183">Eneado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la titolo <nom>Myricae</nom> (latina vorto kun senco: tamarisko) fami&gcirc;is &ccirc;e <tld/>o
        &ndash; Eklogo 4, verso 2: &leftquot;Pla&ccirc;as ne &ccirc;iujn arbustoj kaj &ccirc;i tamarisk' humila&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2013/010730.php">Brile versform-fidela traduko &gcirc;uinda</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_u;&c_b;&c_l;&c_ib;&c_j; <ind>&c_V;&c_je;&c_r;&c_g;&c_ib;&c_l;&c_ib;&c_j;</ind> &c_M;&c_a;&c_r;&c_o;&c_n;</trd>
    <trd lng="bg">&c_P;&c_u;&c_b;&c_l;&c_i;&c_j; <ind>&c_V;&c_je;&c_r;&c_g;&c_i;&c_l;&c_i;&c_j;</ind> &c_M;&c_a;&c_r;&c_o;&c_n;</trd>
    <trd lng="ca">Publi <ind>Virgili</ind> Mar&oacute;</trd>
    <trd lng="de">Vergilius</trd>
    <trd lng="en">Publius <ind>Vergilius</ind> Maro</trd>
    <trd lng="hu">Vergilius</trd>
    <trd lng="lat">Publius <ind>Vergilius Maro</ind></trd>
    <trd lng="nl">Publius <ind>Vergilius</ind> Maro</trd>
    <trd lng="pl">Publiusz  <ind>Wergiliusz</ind>  Maro</trd>
  </snc>
  <snc mrk="vergili.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
    </dif>
    <trd lng="be">&c_V;&c_e;&c_r;&c_g;&c_ib;&c_l;&c_ib;&c_j;</trd>
    <trd lng="bg">&c_V;&c_je;&c_r;&c_g;&c_i;&c_l;&c_i;&c_j;</trd>
    <trd lng="ca">Virgili</trd>
    <trd lng="de">Vergil</trd>
    <trd lng="en">Virgil</trd>
    <trd lng="hu">Virgil</trd>
    <trd lng="lat">Vergilius</trd>
    <trd lng="nl">Vergilius</trd>
    <trd lng="pl">Wergiliusz</trd>
  </snc>
  <trd lng="br">Virgilius</trd>
  <trd lng="cs">Vergilius</trd>
  <trd lng="es">Virgilio</trd>
  <trd lng="fr">Virgile</trd>
  <trd lng="it">Virgilio</trd>
  <trd lng="ja">&#x30A6;&#x30A7;&#x30EB;&#x30AE;&#x30EA;&#x30A6;&#x30B9;</trd>
  <trd lng="ru">&c_V;&c_je;&c_r;&c_g;&c_i;&c_l;&c_i;&c_j;</trd>
  <trd lng="sk">Verg&iacute;lius</trd>
  <trd lng="uk">&c_V;&c_je;&c_r;&c_gu;&c_ib;&#x43B;&c_ib;&c_j; (&#x43F;&#x43E;&c_je;&c_t;)</trd>
</drv>
</art>

