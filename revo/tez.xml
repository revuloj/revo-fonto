<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>tez</rad>/o
    </kap>

    <drv mrk="tez.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="tez.0o.aserto">
        <dif>
          Aserto elmetata kaj defendata de iu persono:
          <ekz>
            tiu &ccirc;i konstato estas tre valora por mia <tld/>o;
          </ekz><ekz>
            temis pri <tld/>o, ke la katastrofaj naturfenomenoj
            <klr>...</klr> priskribitaj en la Biblio <klr>...</klr> ne estas
            nur fantazia&jcirc;oj
            <fnt><aut>Manuel Halvelik</aut>: <vrk>(Andr&eacute; Cherpillod,
                Kaj la suno haltis...)</vrk>, <bib>Monato</bib><lok>jaro 1993a,
                numero 11a, p. 23a</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_t;&c_e;&c_z;&c_ib;&c_s;</trd>
        <trd lng="de">These</trd>
        <trdgrp lng="hu">
          <trd>t&eacute;tel</trd>,
          <trd>t&eacute;zis</trd>
        </trdgrp>
        <trd lng="nl">stelling</trd>
        <trd lng="pl">teza</trd>
        <trdgrp lng="pt">
          <trd>tese</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_t;&c_je;&c_z;&c_i;&c_s;</trd>,
          <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;
            <klr tip="amb">(&c_n;&c_a;&c_u;&c_ch;&c_n;.)</klr></trd>
        </trdgrp>
      </snc>
      <snc mrk="tez.0o.PED">
        <uzo tip="fak">PED</uzo>
        <ref tip="dif" cel="disert.0ajxo.doktoriga">diserta&jcirc;o</ref>
        <ekz>
          en la supera nivelo la studentoj verkas <tld/>on pri
          <klr>...</klr> historia temo
          <fnt><aut>Jo Haazen</aut>:
            <vrk>Unika muziklernejo en Belgio</vrk>,
            <bib>Monato</bib><lok>jaro 1997a, numero 4a, p. 16a</lok>
          </fnt>.
        </ekz>
        <trd lng="be">&c_d;&c_y;&c_s;&c_e;&c_r;&c_t;&c_a;&c_c;&c_y;&c_ja;
        </trd>
        <trdgrp lng="de">
          <trd>Doktorarbeit</trd>,
          <trd>Dissertation</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>dissertation<klr> (univ.)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>disszert&aacute;ci&oacute;</trd>,
          <trd>doktori <ind>&eacute;rtekez&eacute;s</ind></trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>proefschrift</trd>,
          <trd>dissertatie</trd>,
          <trd>thesis</trd>
        </trdgrp>
        <trd lng="pl">dysertacja</trd>
        <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
      </snc>
      <trd lng="en">thesis</trd>
      <trdgrp lng="fr">
        <trd>th&egrave;se</trd>
      </trdgrp>
    </drv>
  </art>
