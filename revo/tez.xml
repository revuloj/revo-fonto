<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tez</rad>/o
</kap>
<drv mrk="tez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Aserto elmetata kaj defendata de iu persono:
      <ekz>
        tiu &ccirc;i konstato estas tre valora por mia <tld/>o;
      </ekz>
      <ekz>
        lia senpruva <tld/>o estas nur hipotezo.
      </ekz>
    </dif>
    <trd lng="nl">stelling</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_z;&c_i;&c_s;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_n;&c_a;&c_u;&c_ch;&c_n;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Scienca aserto prezentata kaj defendata de kandidato en iuj
      universitataj ekzamenoj.
    </dif>
    <trdgrp lng="nl">
      <trd>proefschrift</trd>,
      <trd>dissertatie</trd>,
      <trd>thesis</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_je;&c_r;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="de">These</trd>
</drv>
</art>
