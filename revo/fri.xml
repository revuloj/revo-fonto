<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fri</rad> <fnt><bib>PIV1</bib></fnt> <fnt><bib>PMEG</bib></fnt>
</kap>
<drv mrk="fri.0o">
  <kap><tld/></kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>prepozicio</vspec></gra>
  <snc mrk="fri.0.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
    Liverante
    </dif>
    <subsnc mrk="fri.0.EKON_pago">
      <dif>
        sen kroma pago de:
        <ekz>
          liveri varojn <tld/> dogano;
        </ekz>
        <ekz>
          <tld/> pakmaterialo;
        </ekz>
        <ekz>
          <tld/> &ccirc;iuj kostoj.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="fri.0.EKON_gxis">
      <dif>
        sen kroma pago &gcirc;is:
        <ekz>
          <tld/> Roterdamo.
        </ekz>
      </dif>
    </subsnc>
  <rim>
  Neoficiala prepozicio.
  </rim>
  </snc>
  <ref tip="vid" cel="frajt.0i">frajti</ref>
  <trd lng="fr">franco de port</trd>
</drv>
</art>

