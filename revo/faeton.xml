<vortaro>
<art mrk="$Id: faeton.xml,v 1.19 2019/01/12 16:10:14 revo Exp $">
<kap><rad>faeton</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="faeton.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malpeza malkovrita <ref tip="super" cel="kalesx.0o">kale&scirc;o</ref>
      sen pordetoj:
      <ekz>
        la memstiratan kale&scirc;on veturigas la posedanto, &gcirc;i rekoneblas per ekipa&jcirc;o de la
        veturigista sidejo, tipaj memstira&jcirc;oj: <tld lit="F"/>o, &ccirc;e kelkaj tipoj de
        Viktoria kale&scirc;o ekzistas la eblo transmunti ilin al memstiro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34140"
              >Kale&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiakr.0o">fiakro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_e;&c_t;&c_o;&c_n;</trd>
  <trd lng="bg">&c_f;&c_a;&c_j;&c_t;&c_o;&c_n;</trd>
  <trd lng="ca">faet&oacute;</trd>
  <trd lng="cs">Faet&oacute;n</trd>
  <trd lng="de">Phaeton</trd>
  <trd lng="en">phaeton</trd>
  <trd lng="es">faet&oacute;n</trd>
  <trd lng="fa">&a_f;&a_A;&f_y;&a_t;&a_w;&a_n;</trd>
  <trd lng="fr">pha&eacute;ton<klr tip="ind">
  (cal&egrave;che)</klr></trd>
  <trd lng="hu">faeton</trd>
  <trd lng="nl">fa&euml;ton</trd>
  <trd lng="pl">faeton</trd>
  <trdgrp lng="pt">
    <trd>faeton</trd>,
    <trd>faetonte</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_e;&c_t;&c_o;&c_n;
  <klr>(&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;)</klr></trd>
  <trdgrp lng="sk">
    <trd>faeton</trd>,
    <trd>otvoren&yacute; ko&ccaron;</trd>,
    <trd>&lcaron;ahk&yacute;</trd>
  </trdgrp>
</drv>
</art>
