<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>baga&gcirc;</rad>/o <fnt><bib>PIV2</bib></fnt>
    </kap>

    <drv mrk="bagagx.0o">
      <kap><tld/>o</kap>
      <snc mrk="bagagx.0o.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          Tuto de la
          <refgrp tip="sub">
            <ref cel="kofr.0o">kofroj</ref>,
            <ref cel="valiz.0o">valizoj</ref>,
            <ref cel="pak.0o">pakoj</ref>,
            <ref cel="sak.0o">sakoj</ref>
          </refgrp> ktp kiujn oni portas kun si &ccirc;e voja&gcirc;o
          a&ubreve; kiujn oni ekspedas per publika transportilo:
          <ekz>
            iu iris eksenti la pezon de la <tld/>o, kiun oni metis apud 
            la pordo
            <fnt><bib>Voja&gcirc;Imp</bib>,
              <lok>du blinduloj, sceno el la lando de Donaj kozakoj
              </lok></fnt>.
          </ekz>
        </dif>
        <ref tip="dif" cel="pak.0ajxo.bagagxo">paka&jcirc;o<sncref/>
        </ref>
      </snc>
      <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
      <trd lng="bg">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
      <trd lng="de">Gep&auml;ck</trd>
      <trd lng="es">equipaje</trd>
      <trd lng="hu">poggy&aacute;sz</trd>  
      <trd lng="it">bagaglio</trd>
      <trd lng="nl">bagage</trd>
      <trd lng="pl">baga&zdot;</trd>
      <trd lng="ru">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
      <trd lng="sv">bagage</trd>
      <trdgrp lng="tr">
        <trd>valiz</trd>,
        <trd>ba&gbreve;aj</trd>
      </trdgrp>
    </drv>
  </art>

