<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>moned</rad>/o
</kap>
<drv mrk="moned.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="korv.0o.genro">korvo</ref>
      (<trd lng="la">Corvus monedula</trd>), pli malgranda kun griza
      kolo, mallonga beko, konstruanta nestojn en klifoj a&ubreve; altaj
      konstrua&jcirc;oj:
      <ekz>
        paro da monedoj, kiuj sidis unu apud la alia sur la tegmento de
        la
        stalo
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 26a, p.
        189a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="korv.0eto">korveto</ref>
  </snc>
   <rim>
    individuoj maskla : <em>virmonedo</em> ; ina : <em>monedino</em> ;
    juna : <em>monedido</em> ; juna maskla : <em>virmonedido</em> ;
    juna ina : <em>monedidino</em>
    </rim> 
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">kavan-tour</trd>
  <trd lng="ca">gralla</trd>
  <trdgrp lng="cs">
    <trd>kavka</trd>,
    <trd>kavka obecn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Dohle</trd>
  <trd lng="en">jackdaw</trd>
  <trd lng="fa">&a_z;&a_A;&a_gh;&f_ch;&a_h;</trd>
  <trd lng="fr">choucas<klr> (des tours)</klr></trd>
  <trd lng="hu">cs&oacute;ka</trd>
  <trd lng="nl">torenkraai</trd>
  <trd lng="pl">kawka</trd>
  <trd lng="pt">chuca</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">kavka</trd>
</drv>
</art>
