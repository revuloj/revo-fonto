<drv mrk="januar.0a">
  <kap><tld lit="j"/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        mi estas tre gaja, &ccirc;ar mi ricevis la <tld lit="j"/>an numeron de Monato
        <fnt>
          <bib>Monato</bib>
          <aut>Andy Hern&aacute;ndez Mart&iacute;nez</aut>
          <vrk><url ref="&Monato;/2006/008902.php">Sova&gcirc;a kapitalismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; <tld lit="j"/>a enketo, inter oktobro kaj marto 124&nbsp;802 laboristoj estos perdintaj sian
        laboron
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009770.php">Vila&gcirc;o por maldungitoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        rivero <nom>Iskar</nom>, kiu tie malsupre ar&gcirc;entis kaj ie-tie estis glacii&gcirc;inta pro la <tld
        lit="j"/>a frosto
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2013/010962.php">La iluziisto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_u;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_s;&c_k;&c_ib;</trd>
