<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>januar</rad>/o
</kap>
<drv mrk="januar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Unua monato de la jaro, la&ubreve; la Gregoria kalendaro:
      <ekz>
        <tld/>o estas la unua monato de la jaro
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="januar.0o.RIM">
    Kvankam la monatnomoj en la Fundamento estas majusklaj, nuntempe
    ili estas ofte uzataj minuskle analoge al la semajnotagoj. Fakte
    la monatoj ne estas unikaj individuoj, kiel personoj a&ubreve;
    geografiaj objektoj kaj la minuskligo helpas distingi ilin de la
    personaj nomoj Julio kaj A&ubreve;gusto.
  </rim>
  <trd lng="be">&c_s;&c_t;&c_u;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ja;&c_n;&c_u;&c_a;&c_r;&c_i;</trd>
  <trd lng="br">Genver</trd>
  <trd lng="ca">gener</trd>
  <trd lng="cs">leden</trd>
  <trd lng="fr">janvier</trd>
  <trd lng="de">Januar</trd>
  <trd lng="el">&Jota;&alfa;&ny;&omikron;&ypsilon;&alfa_ton;&rho;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">January</trd>
  <trd lng="es">enero</trd>
  <trd lng="hu">janu&aacute;r</trd>
  <trd lng="id">Januari</trd>
  <trd lng="it">gennaio</trd>
  <trd lng="ja">&#19968;&#26376; [<ind>&#12356;&#12385;&#12364;&#12388;</ind>]</trd>
  <trd lng="nl">januari</trd>
  <trd lng="pl">stycze&nacute;</trd>
  <trd lng="pt">janeiro</trd>
  <trd lng="ro">ianuarie</trd>
  <trd lng="ru">&c_ja;&c_n;&c_v;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sv">januari</trd>
  <trd lng="tr">ocak</trd>
  <trd lng="vo">yanul</trd>
</drv>

<drv mrk="januar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">de gener</trd>
  <trd lng="fr">de <ind>janvier</ind></trd>
  <trd lng="ja">&#19968;&#26376;&#12398; [<ind>&#12356;&#12385;&#12364;&#12388;&#12398;</ind>]</trd>
  <trd lng="pl">styczniowy</trd>
</drv>
</art>
