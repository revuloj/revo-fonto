<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
 <art mrk="$Id: rodop.xml,v 1.2 2025/02/28 08:34:49 afido Exp $">
    <kap>
      <rad>Rodop</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="rodop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:montaroj" cel="mont.0aro">Montaro</ref> etendi&gcirc;anta la&ubreve; la
      bulgara-greka landlimo:
      <ekz>
        <klr>[okazas]</klr> konstruado de dekoj da moskeoj en la montaro <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2006/008957.php"
              >Religioj en Bulgario</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_R;&c_o;&c_d;&c_o;&c_p;&c_i;</trd>
    <trd lng="de">Rhodopen</trd>
    <trd lng="en">Rhodopes</trd>
    <trd lng="el">&Rho;&omikron;&delta;&omikron_ton;&pi;&eta;</trd>
    <trd lng="fr">Rhodopes</trd>
    <trd lng="tr">Rodoplar</trd>
    <trd lng="ru">&c_R;&c_o;&c_d;&c_o;&c_p;&c_y;</trd>
    <trd lng="uk">&c_R;&c_o;&c_d;&c_o;&c_p;&c_i;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_R;&c_a;&c_d;&c_o;&c_p;&c_y;</trd>,
  <trd>&c_R;&c_a;&c_d;&c_o;&c_p;&c_s;&c_k;&c_ib;&c_ja; &c_g;&c_o;&c_r;&c_y;</trd>
</trdgrp>
</drv>

  </art>

