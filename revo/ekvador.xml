<kap>
 <ofc>8</ofc><rad>Ekvador</rad>/o <fnt><bib>PIV1</bib></fnt></kap>

<drv mrk="ekvador.E0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="malprt" cel="amerik.Sud0o">Sudamerika</ref>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&scirc;tato</ref>,
      limanta kun 
      <ref tip="vid" cel="peru.P0o">Peruo</ref>,
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref> kaj
      <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>:
      <ekz>
        la bando de <nom>Simonelli</nom> <klr>[&#x2026;]</klr> planis enporti 1600&nbsp;kg da kokaino el Kolombio kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010475.php">El Paco al paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Venezuelo, Bolivio kaj  <tld/>o la kontraktoj estas nuntempe retraktataj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010686.php">Mizeriga petrolo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne &ccirc;iujn <tld lit="e"/>anojn oni konsideros hispanlingvaj, &ccirc;ar ili ofte estas ke&ccirc;uoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2004/008346.html">Lingva bunto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_k;&c_v;&c_a;&c_d;&c_o;&c_r;</trd>
  <trd lng="bg">&c_Je;&c_k;&c_v;&c_a;&c_d;&c_o;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>l'Equador</trd>,
    <trd>Rep&uacute;blica de l'Equador</trd>
  </trdgrp>
  <trd lng="cs">Ekv&aacute;dor</trd>
  <trd lng="de">Ecuador</trd>
  <trd lng="en">Ecuador</trd>
  <trd lng="es">Ecuador (el pa&iacute;s)</trd>
  <trdgrp lng="fr">
    <trd>&Eacute;quateur</trd>,
    <trd>R&eacute;publique d'&Eacute;quateur</trd>
  </trdgrp>
  <trd lng="id">Ekuador</trd>
  <trd lng="ja">&#x30A8;&#x30AF;&#x30A2;&#x30C9;&#x30EB;</trd>
  <trd lng="nl">Ecuador</trd>
  <trd lng="pl">Ekwador</trd>
  <trd lng="pt">Equador<klr tip="amb"> (pa&iacute;s)</klr> </trd>
  <trd lng="sk">Ekv&aacute;dor</trd>
  <trd lng="uk">&#x415;&#x43A;&#x432;&#x430;&#x434;&#x43E;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x5384;&#x74DC;&#x591A; <pr>&#xE8;gu&#x101;du&#x14D;</pr></trd>,
    <trd>&#x5384;&#x74DC;&#x591A;&#x5C14; <pr>&#xE8;gu&#x101;du&#x14D;&#x11B;r</pr></trd>
  </trdgrp>
</drv>
</art>


