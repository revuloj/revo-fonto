<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>azil</rad>/o
</kap>
<drv mrk="azil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rifu&gcirc;ejo, kie oni trovas sekurecon kaj flegadon:
      <ekz>
        <tld/>o por
        malri&ccirc;uloj, kadukuloj, frenezuloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_t;&c_u;&c_l;&c_a;&c_k;</trd>, 
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_a;&c_k;</trd>, 
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>, 
    <trd>&c_s;&c_h;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_b;&c_je;&c_zh;&c_i;&c_shch;&c_je;</trd>
  <trd lng="ca">asil</trd>
  <trdgrp lng="de">
    <trd>Asyl</trd>,
    <trd>Zufluchtsort</trd>
  </trdgrp>
  <trd lng="en">asylum</trd>
  <trdgrp lng="es">
    <trd>asilo</trd>,
    <trd>refugio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asile <klr tip="ind">(refuge)</klr></trd>,
    <trd>refuge</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>menhely</trd>,
    <trd>mened&eacute;k</trd>
  </trdgrp>
  <trd lng="it">asilo<klr> (rifugio)</klr></trd>
  <trd lng="nl">aziel</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_b;&c_je;&c_zh;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ju;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">azyl</trd>
</drv>
</art>
