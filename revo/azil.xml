<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>azil</rad>/o
</kap>
<drv mrk="azil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rifu&gcirc;ejo, kie oni trovas sekurecon kaj flegadon:
      <ekz>
        <tld/>o por
        malri&ccirc;uloj, kadukuloj, frenezuloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Asyl</trd>,
    <trd>Zufluchtsort</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>asilo</trd>,
    <trd>refugio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asile <klr tip="ind">(refuge)</klr></trd>,
    <trd>refuge</trd>
  </trdgrp>
  <trd lng="nl">aziel</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_b;&c_je;&c_zh;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ju;&c_t;</trd>
  </trdgrp>
</drv>
</art>
