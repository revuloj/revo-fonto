<drv mrk="pulvor.lav0o">
  <kap>lav<tld/>o <fnt><bib>HejmVort</bib></fnt></kap>
  <snc mrk="pulvor.lav0o.KEM">
    <dif>
      <ref cel="sap.0o">Sapo</ref> a&ubreve; alia lava &hcirc;emia&jcirc;o en <tld/>o, tipe uzata
      por lavado de vestoj:
      <ekz>
        &leftquot;mi bezonus iom pli da lav<tld/>o&rightquot;, li prononcis
        <fnt>
          <bib>CKv</bib>
          <lok>18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la enhavo de la paka&jcirc;o konsistas el <klr>[&#x2026;]</klr>
        vitaminoj (1 pako), lav<tld/>o (3 kg), sapo (1 kg)
        <fnt>
        <aut>R. Corsetti</aut>,
        <vrk>UEA - agado espero projekto 2a</vrk>,
        <lok>soc.culture.esperanto, 1999-05-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_h; &c_z;&c_a; &c_p;&c_r;&c_a;&c_n;&c_je;</trd>
  <trd lng="de">Waschpulver</trd>
  <trdgrp lng="en">
    <trd>detergent</trd>,
    <trd>soap powder</trd>
  </trdgrp>
  <trd lng="es">detergente en polvo</trd>
  <trd lng="fa">&f_p;&a_w;&a_d;&a_r; &a_l;&a_b;&a_A;&a_s;&a_sh;&a_w;&f_y;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>lessive en poudre</trd>,
    <trd>poudre &agrave; laver</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D17;&#x5264;  <pr>&#x305B;&#x3093;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x7C89;&#x77F3;&#x3051;&#x3093;  <pr>&#x3053;&#x306A;&#x305B;&#x3063;&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>waspoeder</trd>,
    <trd>zeeppoeder</trd>
