<drv mrk="pulvor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pulvor.0o.KOMUNE">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Solida substanco dispecigita en subtilajn erojn:
      <ekz>
        oni spicas per dol&ccirc;a paprik-<tld/>o kaj pipro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2008/009386.php">La kukurbo frukto multviza&gcirc;a</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru el tio incensa&jcirc;on <klr>[&#x2026;]</klr> kaj pistu el tio subtilan <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; liaj propraj, oni metis al li vitrajn okulojn, enkadrigitajn en bronzo,
        oni sur&scirc;utis la tutan korpon per soda <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;etante <tld/>on en okulojn de sia atakanto, &scirc;i esperis, ke &scirc;i iom paralizos
        liajn fortojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis tiun junulon &scirc;uti kuleron da mi-ne-scias-kia <tld/>o en mian sa&ubreve;con
        <fnt>
          <bib>ChC</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni protektas vesta&jcirc;ojn kaj tapi&scirc;ojn kontra&ubreve; insektoj per piretro-<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/010891.php">De praavo al arto</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="farun.0o">faruno</ref>,
      <ref cel="polv.0o">polvo</ref>,
      <ref cel="pudr.0o">pudro</ref>,
      <ref cel="pulv.0o">pulvo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_h;</trd>
  <trdgrp lng="cs">
    <trd>prach</trd>,
    <trd>pr&aacute;&scaron;ek</trd>,
    <trd>z&aacute;syp pudr</trd>
  </trdgrp>
  <trd lng="de">Pulver</trd>
  <trd lng="en">powder</trd>
  <trd lng="es">polvo</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_fatha;&a_r;&a_d;</trd>,
    <trd>&f_p;&a_w;&a_d;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">poudre</trd>
  <trd lng="hu">por</trd>
  <trd lng="io">pulvero</trd>
  <trdgrp lng="ja">
    <trd>&#x7C89; <pr>&#x3053;&#x306A;</pr></trd>,
    <trd>&#x7C89;&#x672B; <pr>&#x3075;&#x3093;&#x307E;&#x3064;</pr></trd>,
    <trd>&#x30D1;&#x30A6;&#x30C0;&#x30FC; <pr>&#x3071;&#x3046;&#x3060;&#x30FC;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>poeder</trd>,
    <trd>gruis</trd>
  </trdgrp>
  <trd lng="pl">proszek</trd>
  <trdgrp lng="pt">
    <trd>p&oacute;</trd>,
    <trd>moinha</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>prach</trd>,
    <trd>pr&aacute;&scaron;ok</trd>,
    <trd>p&uacute;der</trd>,
    <trd>z&aacute;syp</trd>
  </trdgrp>
  <trd lng="tok">ko</trd>
  <trd lng="tr">pudra</trd>
  <trd lng="uk">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x7C89; <pr>f&#x11B;n</pr></trd>,
    <trd>&#x9999;&#x7C89; <pr>xi&#x101;ngf&#x11B;n</pr></trd>,
    <trd>&#x7D30;&#x7C89; <pr>x&#xEC;f&#x11B;n</pr></trd>,
    <trd>&#x7EC6;&#x7C89; <pr>x&#xEC;f&#x11B;n</pr></trd>,
    <trd>&#x6563; <pr>s&#x1CE;n</pr></trd>,
    <trd>&#x4E39; <pr>d&#x101;n</pr></trd>,
    <trd>&#x7C89;&#x672B; <pr>f&#x11B;nm&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pulvor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pulvor.0igi.KOMUNE">
    <dif>
      Tre subtile <ref tip="vid" cel="pist.0i">pisti</ref>:
      <ekz>
        <tld/>igi kreton;
      </ekz>
      <ekz>
        per batoj de martelo li faligis kaj <tld/>igis, la&ubreve; unumetra longo, la vitrajn
        rompa&jcirc;ojn enmetitajn en la gipson
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        misteroj,kiuj konsternas e&ccirc; sa&gcirc;ulojn kaj <tld/>igas la
        animon de la popola&ccirc;o
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_w;&c_ch;&c_y; &c_w;
&c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>pulverizovat</trd>,
    <trd>rozdrtit</trd>,
    <trd>rozeml&iacute;t na prach</trd>,
    <trd>rozm&ecaron;lnit</trd>,
    <trd>rozm&ecaron;lnit na pr&aacute;&scaron;ek</trd>,
    <trd>rozm&ecaron;l&ncaron;ovat</trd>,
    <trd>rozpra&scaron;ovat</trd>
  </trdgrp>
  <trd lng="de">pulverisieren</trd>
  <trdgrp lng="en">
    <trd>pulverize</trd>,
    <trd>reduce to powder</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pulverizar<klr tip="ind"> (reducir a polvo)</klr></trd>,
    <trd>reducir a <ind>polvo</ind></trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_d;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>pulv&eacute;riser<klr tip="ind"> (r&eacute;duire en
poudre)</klr></trd>,
    <trd>r&eacute;duire en <ind>poudre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>por&iacute;t</trd>,
    <trd>porr&aacute; z&uacute;z</trd>
  </trdgrp>
  <trd lng="io">pulverigar</trd>
  <trdgrp lng="ja">
    <trd>&#x7C89;&#x672B;&#x306B;&#x3059;&#x308B; <pr>&#x3075;&#x3093;&#x307E;&#x3064;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7C89;&#x7815;&#x3059;&#x308B; <pr>&#x3075;&#x3093;&#x3055;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verpulveren</trd>,
    <trd>verpoederen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zproszkowa&cacute;</trd>,
    <trd>zetrze&cacute; na proszek</trd>,
    <trd>zmieli&cacute; na proszek</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_l;&c_o;&c_ch;&c_mol; &c_v;
&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>rozdrvi&tcaron;</trd>,
    <trd>rozomlie&tcaron; na prach</trd>
  </trdgrp>
  <trdgrp lng="tr">
  <trd>toz etmek</trd>,
  <trd>parampar&ccedil;a etmek</trd>,
  <trd>yok etmek</trd>,
  <trd>p&uuml;sk&uuml;rtmek</trd>,
</trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_t;&c_o;&c_v;&c_k;&c_t;&c_i;/&c_r;&c_o;&c_z;&c_t;&c_je;&c_r;&c_t;&c_i; &c_n;&c_a;
    &c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_r;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_n;&c_a; &c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x7838;&#x788E; <pr>z&#xE1;su&#xEC;</pr></trd>
</drv>

<drv mrk="pulvor.bak0o">
  <kap>
    bak<tld/>o
  </kap>
  <snc mrk="pulvor.bak0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Hcirc;emia&jcirc;o uzata por pasto anstata&ubreve; gisto a&ubreve; alia fermenta&jcirc;o, kiu eligas
      karbondioksidon dum varmi&gcirc;o kaj tiel donas spongecan konsiston al baka&jcirc;o:
      <ekz>
        kelkfoje mi opinias ke la miksbulkoj enhavas tro da bak<tld/>o
        <fnt>
          <aut>Baum Lyman Frank</aut>
          <vrk><url ref="https://www.litmir.me/br/?b=87754&amp;p=28">La Smeralda Urbo de
          Oz</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        faruno, sen aldono de gisto, bak<tld/>o a&ubreve; almena&ubreve; bulka&jcirc;o kaj ovo estus tre
        malfacile digestebla
        <fnt>
          <vrk><url ref="https://is.muni.cz/el/ped/podzim2008/SZ9BP_CJE1/um/KnedlikoWB.pdf">PDF
          Microsoft Word - Dokument2 | 2 Al kiu apartenas la &leftquot;nacia plado"?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="sod.bak0o">baksodo</ref>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_k;&c_a;&c_r;&c_n;&c_y; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="de">Backpulver</trd>
  <trd lng="en">baking powder</trd>
  <trd lng="fr">levure chimique</trd>
  <trdgrp lng="ja">
    <trd>&#x3075;&#x304F;&#x3089;&#x3057;&#x7C89;  <pr>&#x3075;&#x304F;&#x3089;&#x3057;&#x3053;</pr></trd>,
    <trd>&#x30D9;&#x30FC;&#x30AD;&#x30F3;&#x30B0;&#x30D1;&#x30A6;&#x30C0;&#x30FC;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k; &c_d;&c_l;&c_ja; &c_p;&c_je;&c_ch;&c_i;&c_v;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CE1;&#x6253;&#x7C89; <pr>p&#xE0;od&#x1CE;f&#x11B;n</pr></trd>,
    <trd>&#x7119;&#x7C89; <pr>b&#xE8;if&#x11B;n</pr></trd>,
    <trd>&#x53D1;&#x7C89; <pr>f&#x101;f&#x11B;n</pr></trd>,
    <trd>&#x767C;&#x7C89; <pr>f&#x101;f&#x11B;n</pr></trd>,
    <trd>&#x53D1;&#x9175;&#x7C89; <pr>f&#x101;ji&#xE0;of&#x11B;n</pr></trd>,
    <trd>&#x767C;&#x9175;&#x7C89; <pr>f&#x101;ji&#xE0;of&#x11B;n</pr></trd>,
    <trd>&#x9762;&#x5305;&#x7C89; <pr>mi&#xE0;nb&#x101;of&#x11B;n</pr></trd>,
    <trd>&#x9EB5;&#x5305;&#x7C89; <pr>mi&#xE0;nb&#x101;of&#x11B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pulvor.kaf0o">
  <kap>kaf<tld/>o</kap>
  <snc mrk="pulvor.kaf0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o preparita el <ref cel="kaf.0o.BOT">kafoj</ref>:
      <ekz>
        &jcirc;etu iomete da kaf<tld/>o en glason da akvo kaj la kafo fre&scirc;a flosos, la
        malfre&scirc;a male
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2011/010337.php">Konsiloj avinaj kaj neavinaj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la turka kafo <klr>[&#x2026;]</klr> estis nekredeble plena je sukero, kaj mi man&gcirc;is
        tiom da kaf<tld/>o, kiom mi trinkis da kafo
        <fnt>
          <bib>Gerda</bib>
          <lok>&Ccirc;apitro 16 (dek Ses)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiuj]</klr> kafoma&scirc;inoj <klr>[&#x2026;]</klr> funkcias per apartaj karto&ccirc;oj a&ubreve; aliaj
        kaf<tld/>ujoj por &ccirc;iu unuopa taso
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde kaj Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2014/011347.php">Konsumantoj agiti&gcirc;as kaj konscii&gcirc;as</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_y; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Kaffeepulver</trd>,
    <trd>Kaffeemehl</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>coffee grounds</trd>,
    <trd>ground coffee</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caf&eacute; molido</trd>
  </trdgrp>
  <trd lng="fa">&a_q;&a_h;&a_w;&a_h;&a_hamza_sure; &a_s;&a_A;&f_y;&f_y;&a_d;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>caf&eacute;<klr tip="ind"> (en poudre)</klr></trd>,
    <trd>poudre de caf&eacute;</trd>
  </trdgrp>
  <trd lng="nl">gemalen <ind>koffie</ind></trd>
  <trdgrp lng="pl">
    <trd>kawa w proszku</trd>,
    <trd>kawa mielona</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_j;
&c_k;&c_o;&c_f;&c_je;</trd>
<trd lng="tr">kahve<klr>(&ouml;g&uuml;t&uuml;lm&uuml;&scedil; hali)</klr></trd>
</drv>

