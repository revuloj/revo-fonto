<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pulvor.xml,v 1.36 2022/06/16 08:41:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pulvor</rad>/o
</kap>

<drv mrk="pulvor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pulvor.0o.KOMUNE">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Solida substanco dispecigita en subtilajn erojn:
      <ekz>
        <tld/>o de antipirino, oro, ar&gcirc;ento, pipro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_h;</trd>
  <trdgrp lng="cs">
    <trd>prach</trd>,
    <trd>pr&aacute;&scaron;ek</trd>,
    <trd>z&aacute;syp pudr</trd>
  </trdgrp>
  <trd lng="de">Pulver</trd>
  <trd lng="en">powder</trd>
  <trd lng="es">polvo</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_fatha;&a_r;&a_d;</trd>,
    <trd>&f_p;&a_w;&a_d;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">poudre</trd>
  <trd lng="hu">por</trd>
  <trd lng="io">pulvero</trd>
  <trdgrp lng="ja">
    <trd>&#x7C89; [<ind>&#12371;&#x306A;</ind>]</trd>,
    <trd>&#x7C89;&#x672B; [<ind>&#12405;&#12435;&#x307E;&#12388;</ind>]</trd>,
    <trd>&#x30D1;&#x30A6;&#x30C0;&#x30FC; [<ind>&#12401;&#12358;&#12384;&#x30FC;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>poeder</trd>,
    <trd>gruis</trd>
  </trdgrp>
  <trd lng="pl">proszek</trd>
  <trdgrp lng="pt">
    <trd>p&oacute;</trd>,
    <trd>moinha</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>prach</trd>,
    <trd>pr&aacute;&scaron;ok</trd>,
    <trd>p&uacute;der</trd>,
    <trd>z&aacute;syp</trd>
  </trdgrp>
  <trd lng="tok">ko</trd>
  <trd lng="tr">pudra</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="pulvor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pulvor.0igi.KOMUNE">
    <dif>
      Tre subtile <ref tip="vid" cel="pist.0i">pisti</ref>:
      <ekz>
        <tld/>igi kreton;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        misteroj,kiuj konsternas e&ccirc; sa&gcirc;ulojn kaj <tld/>igas la
animon de
        la popola&ccirc;o
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_w;&c_ch;&c_y; &c_w;
&c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>pulverizovat</trd>,
    <trd>rozdrtit</trd>,
    <trd>rozeml&iacute;t na prach</trd>,
    <trd>rozm&ecaron;lnit</trd>,
    <trd>rozm&ecaron;lnit na pr&aacute;&scaron;ek</trd>,
    <trd>rozm&ecaron;l&ncaron;ovat</trd>,
    <trd>rozpra&scaron;ovat</trd>
  </trdgrp>
  <trd lng="de">pulverisieren</trd>
  <trdgrp lng="en">
    <trd>pulverize</trd>,
    <trd>reduce to powder</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pulverizar<klr tip="ind"> (reducir a polvo)</klr></trd>,
    <trd>reducir a <ind>polvo</ind></trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_d;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>pulv&eacute;riser<klr tip="ind"> (r&eacute;duire en
poudre)</klr></trd>,
    <trd>r&eacute;duire en <ind>poudre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>por&iacute;t</trd>,
    <trd>porr&aacute; z&uacute;z</trd>
  </trdgrp>
  <trd lng="io">pulverigar</trd>
  <trdgrp lng="ja">
    <trd>&#x7C89;&#x672B;&#x306B;&#12377;&#x308B; [<ind>&#12405;&#12435;&#x307E;&#12388;&#x306B;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x7C89;&#30741;&#12377;&#x308B; [<ind>&#12405;&#12435;&#12373;&#12356;&#12377;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verpulveren</trd>,
    <trd>verpoederen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zproszkowa&cacute;</trd>,
    <trd>zetrze&cacute; na proszek</trd>,
    <trd>zmieli&cacute; na proszek</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_l;&c_o;&c_ch;&c_mol; &c_v;
&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>rozdrvi&tcaron;</trd>,
    <trd>rozomlie&tcaron; na prach</trd>
  </trdgrp>
  <trdgrp lng="tr">
  <trd>toz etmek</trd>,
  <trd>parampar&ccedil;a etmek</trd>,
  <trd>yok etmek</trd>,
  <trd>p&uuml;sk&uuml;rtmek</trd>,
</trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&c_t;&#x43E;&c_v;&#x43A;&c_t;&c_i;/&c_r;&#x43E;&c_z;&c_t;&c_je;&c_r;&c_t;&c_i; &#x43D;&c_a;
    &#x43F;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&#x43E;&c_r;&#x43E;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_t;&c_v;&#x43E;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a; &#x43F;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="pulvor.bak0o">
  <kap>
    bak<tld/>o
  </kap>
  <snc mrk="pulvor.bak0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Hcirc;emia&jcirc;o uzata por pasto anstata&ubreve; gisto a&ubreve; alia fermenta&jcirc;o, kiu eligas
      karbondioksidon dum varmi&gcirc;o kaj tiel donas spongecan konsiston al baka&jcirc;o:
      <ekz>
        kelkfoje mi opinias ke la miksbulkoj enhavas tro da bak<tld/>o
        <fnt>
          <aut>Baum Lyman Frank</aut>
          <vrk><url ref="https://www.litmir.me/br/?b=87754&amp;p=28">La Smeralda Urbo de
          Oz</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        faruno, sen aldono de gisto, bak<tld/>o a&ubreve; almena&ubreve; bulka&jcirc;o kaj ovo estus tre
        malfacile digestebla
        <fnt>
          <vrk><url ref="https://is.muni.cz/el/ped/podzim2008/SZ9BP_CJE1/um/KnedlikoWB.pdf">PDF
          Microsoft Word - Dokument2 | 2 Al kiu apartenas la &leftquot;nacia plado"?</url></vrk>
        </fnt>;
      </ekz>
    </dif>
    <ref tip="sub" cel="sod.bak0o">baksodo</ref>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_k;&c_a;&c_r;&c_n;&c_y; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="de">Backpulver</trd>
  <trd lng="en">baking powder</trd>
  <trd lng="fr">levure chimique</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A; &c_d;&#x43B;&#x44F; &#x43F;&c_je;&c_ch;&c_i;&c_v;&c_a;</trd>
</drv>




<drv mrk="pulvor.kaf0o">
  <kap>kaf<tld/>o</kap>
  <snc mrk="pulvor.kaf0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o preparita el <ref cel="kaf.0o.BOT">kafoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_v;&c_y; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trdgrp lng="en">
    <trd>coffee grounds</trd>,
    <trd>ground coffee</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caf&eacute; molido</trd>
  </trdgrp>
  <trd lng="fa">&a_q;&a_h;&a_w;&a_h;&a_hamza_sure; &a_s;&a_A;&f_y;&f_y;&a_d;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>caf&eacute;<klr tip="ind"> (en poudre)</klr></trd>,
    <trd>poudre de caf&eacute;</trd>
  </trdgrp>
  <trd lng="nl">gemalen <ind>koffie</ind></trd>
  <trdgrp lng="pl">
    <trd>kawa w proszku</trd>,
    <trd>kawa mielona</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_j;
&c_k;&c_o;&c_f;&c_je;</trd>
<trd lng="tr">kahve<klr>(&ouml;g&uuml;t&uuml;lm&uuml;&scedil; hali)</klr></trd>
</drv>

<drv mrk="pulvor.kaka0o">
  <kap>kaka<tld/>o, <var><kap>kakao<tld/>o</kap></var></kap>
  <snc mrk="pulvor.kaka0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o preparita el <ref cel="kaka.0o.BOT">kakaoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_v;&c_a;-&c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_k;&c_a;&c_o; (&c_n;&c_a;
&c_p;&c_r;&c_a;&c_h;)</trd>
  <trd lng="de">Kakaopulver</trd>
  <trd lng="en">cocoa powder</trd>
  <trdgrp lng="es">
    <trd>cacao<klr tip="ind"> (en polvo)</klr></trd>,
    <trd><ind>polvo</ind> de cacao</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_d;&a_r; &f_k;&a_A;&f_k;&a_A;&a_y_hamza;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>cacao<klr tip="ind"> (en poudre)</klr></trd>,
    <trd>poudre de cacao</trd>
  </trdgrp>
  <trd lng="nl">cacaopoeder</trd>
  <trd lng="pl">kakao w proszku</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_a;&c_o;-&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trd lng="tr">pudra kakao</trd>
  <trd lng="uk">&#x43A;&c_a;&#x43A;&c_a;&#x43E;-&#x43F;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="pulvor.lav0o">
  <kap>lav<tld/>o</kap> <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="pulvor.lav0o.KEM">
    <dif>
      Sapo a&ubreve; alia lava &hcirc;emia&jcirc;o en <tld/>o, tipe uzata
      por lavado de vestoj:
      <ekz>
        la enhavo de la paka&jcirc;o konsistas el <klr>[&#8230;]</klr> 
        vitaminoj (1 pako), lav<tld/>o (3 kg), sapo (1 kg)
        <fnt>
        <aut>R. Corsetti</aut>,
        <vrk>UEA - agado espero projekto 2a</vrk>,
        <lok>soc.culture.esperanto, 1999-05-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_h; &c_z;&c_a; &c_p;&c_r;&c_a;&c_n;&c_je;</trd>
  <trd lng="de">Waschpulver</trd>
  <trdgrp lng="en">
    <trd>detergent</trd>,
    <trd>soap powder</trd>
  </trdgrp>
  <trd lng="es">detergente en polvo</trd>
  <trd lng="fa">&f_p;&a_w;&a_d;&a_r; &a_l;&a_b;&a_A;&a_s;&a_sh;&a_w;&f_y;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>lessive en poudre</trd>,
    <trd>poudre &agrave; laver</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>waspoeder</trd>,
    <trd>zeeppoeder</trd>
  </trdgrp>
  <trd lng="pl">proszek do prania</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
  <trd lng="tr">&ccedil;ama&scedil;&inodot;r deterjan&inodot;<klr>(toz hali)</klr></trd>
  <trd lng="uk">&#x43F;&c_r;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &#x43F;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A;</trd>
</drv>
</art>
