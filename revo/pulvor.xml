<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pulvor</rad>/o
</kap>
<drv mrk="pulvor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Solida substanco dispecigita en subtilajn erojn:
      <ekz>
        <tld/>o de antipirino, oro, ar&gcirc;ento, pipro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="de">Pulver</trd>
  <trd lng="en">powder</trd>
  <trd lng="fr">poudre</trd>
  <trd lng="hu">por</trd>
  <trdgrp lng="nl">
    <trd>poeder</trd>,
    <trd>gruis</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
</drv>

<drv mrk="pulvor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Tre subtile pisti:
      <ekz>
        <tld/>igi kreton;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj misteroj konsternas e&ccirc; sa&gcirc;ulojn kaj <tld/>igas la animon de
        la popola&ccirc;o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_w;&c_ch;&c_y; &c_w; &c_p;&c_a;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
  <trd lng="de">pulverisieren</trd>
  <trd lng="en">pulvorize</trd>
  <trdgrp lng="fr">
    <trd>pulv&eacute;riser<klr tip="ind"> (r&eacute;duire en poudre)</klr></trd>,
    <trd>r&eacute;duire en <ind>poudre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>por&iacute;t</trd>,
    <trd>porr&aacute; z&uacute;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verpulveren</trd>,
    <trd>verpoederen</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_l;&c_o;&c_ch;&c_mol; &c_v; &c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
</drv>

<drv mrk="pulvor.kaf0o">
  <kap>kaf<tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="P"/>o preparita el <ref cel="kaf.0o.BOT">kafoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>caf&eacute;<klr tip="ind"> (en poudre)</klr></trd>,
    <trd>poudre de caf&eacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_j; &c_k;&c_o;&c_f;&c_je;</trd>
</drv>

<drv mrk="pulvor.kaka0o">
  <kap>kaka<tld/>o, kakao<tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="P"/>o preparita el <ref cel="kaka.0o.BOT">kakaoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cacao<klr tip="ind"> (en poudre)</klr></trd>,
    <trd>poudre de cacao</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_a;&c_o;-&c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>
</drv>
</art>
