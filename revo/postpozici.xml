<art mrk="$Id: postpozici.xml,v 1.11 2024/02/17 07:52:35 revo Exp $">
<kap>
  <rad>postpozici</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="postpozici.0o">
  <kap><tld/>o</kap>
  <snc mrk="postpozici.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Senfleksia vorteto, kiu postiras la
      substantivan grupon, kies rolon &gcirc;i indikas:
      <ekz>
        la japana kaj la urdua markas la objekton per <tld/>o
        <fnt>
          <aut>A. Val&eacute;n</aut>,
          <vrk><url ref="http://www.esperanto-es.net/lerno/lingva_angulo/04.php">Lingva
          angulo 4: Kio estas akuzativo?</url></vrk>,
          <lok>[vidita je 2006-02-18]</lok>
        </fnt>
      </ekz>.
    </dif>
    <refgrp tip="ant">
      <ref cel="prepoz.0o.GRA">prepozicio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_l;&c_o;&c_g;</trd>
  <trd lng="de">Postposition</trd>
  <trd lng="en">postposition</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_s;&#x200C;&a_n;&a_h;&a_sh;&a_t;</trd>,
    <trd>&f_p;&a_s;&a_A;&f_y;&a_n;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">postposition</trd>
  <trd lng="ja">&#x5F8C;&#x7F6E;&#x8A5E;  <pr>&#x3053;&#x3046;&#x3061;&#x3057;</pr></trd>
  <trd lng="nl">achterzetsel</trd>
  <trd lng="pl">poimek</trd>
  <trd lng="pt">posposi&ccedil;&atilde;o</trd>
  <trd lng="ro">postpozi&#x21B;ie</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_s;&c_t;&#x43F;&#x43E;&c_z;&c_i;&c_c;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x540E;&#x7F6E;&#x8BCD; <pr>h&#xF2;uzh&#xEC;c&#xED;</pr></trd>,
    <trd>&#x5F8C;&#x7F6E;&#x8A5E; <pr>h&#xF2;uzh&#xEC;c&#xED;</pr></trd>,
    <trd>&#x540E;&#x7F6E; <pr>h&#xF2;uzh&#xEC;</pr></trd>,
    <trd>&#x5F8C;&#x7F6E; <pr>h&#xF2;uzh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

