  <trd lng="fr">bourbon<klr tip="amb"> (boisson)</klr></trd>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_b;&c_o;&c_n;<klr tip="amb"> (&c_k;&c_u;&c_k;&c_u;&c_r;&c_u;&c_z;&c_n;&c_o;&c_je; &c_v;&c_i;&c_s;&c_k;&c_i;)</klr></trd>
