<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: burbon.xml,v 1.7 2022/11/30 18:35:17 revo Exp $">
<kap>
  <rad>burbon</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>

<drv mrk="burbon.0o">
  <kap><tld/>o</kap>
  <snc mrk="burbon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="viski.maiz0o.KUI">Maizviskio</ref>:
      <ekz>
        la nomo <tld/>o <klr>[&#8230;]</klr> devenas de la nomo de la graflando
        &leftquot;<nom>Bourbon</nom>&rightquot;, en Kentukio
        <fnt>
        <bib>Viki</bib>,
        <lok>burbono (viskio), 2005-06-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <rim>
      <ref tip="hom" cel="burbon1.0oj">Burbonoj</ref> estas anka&ubreve; franca-hispana nobela gento.
    </rim>
    <trd lng="be">&c_b;&c_u;&c_r;&c_b;&c_o;&c_n; <klr tip="amb">(&c_v;&c_ib;&c_s;&c_k;&c_ib;)</klr></trd>
  <trd lng="ca">bourbon</trd>
  <trd lng="de">Bourbon<klr tip="amb"> (Whiskey)</klr></trd>
  <trd lng="fr">bourbon<klr tip="amb"> (boisson)</klr></trd>
  <trd lng="pl">burbon</trd>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_b;&c_o;&c_n;<klr tip="amb">
  (&c_k;&c_u;&c_k;&c_u;&c_r;&c_u;&c_z;&c_n;&c_o;&c_je;
  &c_v;&c_i;&c_s;&c_k;&c_i;)</klr></trd>
</drv>
</art>

