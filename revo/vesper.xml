<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vesper</rad>/o
</kap>
<drv mrk="vesper.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Lasta parto de la tago:
      <ekz>
        anta&ubreve; <tld/>o ne estu fiera
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        atingi la <tld/>on <klr>(proksimi&gcirc;antan finon)</klr> de sia vivo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la&ubreve;du tagon nur <tld/>e
        (<ref tip="sin" cel="fru">tro frue</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        matenas, <tld/>as, kaj tago malaperas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_ch;&c_a;&c_r;</trd>
  <trd lng="de">Abend</trd>
  <trd lng="en">evening</trd>
  <trd lng="fr">soir</trd>
  <trd lng="nl">avond</trd>
  <trdgrp lng="pt">
    <trd>tarde</trd>,
    <trd>tardezinha</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_ch;&c_je;&c_r;</trd>
  <trd lng="sv">kv&auml;ll</trd>
</drv>
</art>
