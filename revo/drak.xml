<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>drak</rad>/o
</kap>
<drv mrk="drak.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc mrk="drak.0o.MIT">
   <uzo tip="fak">MIT</uzo>
   <dif>
     Mita bestego kun serpenta korpo, ungegoj kaj flugiloj;
     dan&gcirc;era fajro&scirc;pruca trezorgardanto en E&ubreve;ropo,
     reganto de riveroj kaj pluvoj kaj re&gcirc;a simbolo en Orientazio:
     <ekz>
       avara kaj teruriga fluganta <tld/>o<fnt><aut>Familio
       Macgill</aut>: <vrk>June, nejune, pretertempe (John Ronald Reuel
       Tolkien, La Hobito a&ubreve; tien kaj reen)</vrk>,
       <bib>Monato</bib><lok>jaro 2001a, numero 7a, p. 26a</lok></fnt>.
     </ekz>
   </dif>
   <trdgrp lng="de">
     <trd>Drache</trd>,
     <trd>Lindwurm</trd>
   </trdgrp>
   <trd lng="en">dragon</trd>
   <trd lng="es">drag&oacute;n</trd>
   <trd lng="fr">dragon<klr tip="ind"> (myth.)</klr></trd>
   <trd lng="pl">smok</trd>
   <trd lng="ru">&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;</trd>
 </snc>
 <snc mrk="drak.0o.kajto">
   <uzo tip="fak">SPO</uzo>
   <uzo tip="fak">TEK</uzo>
   <ref tip="dif" cel="kajt.0o">kajto</ref>
   <trd lng="de">Drache</trd>
   <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; 
&c_z;&c_m;&c_je;&c_j;</trd>
 </snc>
</drv>

<drv mrk="drak.D0o">
 <kap><tld lit="D"/>o</kap>
 <mlg>Dra</mlg>
 <snc>
   <dif>
     Nord-hemisfera <ref tip="super" cel="stel.0figuro">stelfiguro</ref> 
(<trd lng="la">Draco</trd>).
   </dif>
 </snc>
 <trd lng="de">Drache</trd>
 <trd lng="en">Draco</trd>
 <trd lng="es">Drag&oacute;n</trd>
 <trd lng="pl">Smok</trd>
</drv>

<drv mrk="drak.flug0o">
 <kap>flug<tld/>o</kap>
 <uzo tip="fak">SPO</uzo>
 <uzo tip="fak">TEK</uzo>
 <snc>
   <ref tip="dif" cel="kajt.0o">kajto</ref>
 </snc>
 <trd lng="de">Flugdrache</trd>
 <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; 
&c_z;&c_m;&c_je;&c_j;</trd>
</drv>
</art>
