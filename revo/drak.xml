<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drak.xml,v 1.40 2023/10/14 14:40:07 revo Exp $">
<kap>
 <ofc>*</ofc>
 <rad>drak</rad>/o
</kap>
<drv mrk="drak.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc mrk="drak.0o.MIT">
   <uzo tip="fak">MIT</uzo>
   <dif>
     Mita bestego kun serpenta korpo, ungegoj kaj flugiloj;
     dan&gcirc;era kaj fajro&scirc;pruca en E&ubreve;ropo,
     reganto de akvoj kaj re&gcirc;a simbolo en Orientazio:
      <ekz>
        galo de <tld/>oj estas ilia vino, kaj pereiga veneno de aspidoj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 32:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen granda ru&gcirc;a <tld/>o, havanta sep kapojn kaj dek kornojn, kaj sur siaj kapoj
        sep diademojn
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 12:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        fajrokra&ccirc;anta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la urbo estas granda monto, sur kiu <tld/>o lo&gcirc;as: &ccirc;iujare oni devas oferi
        al &gcirc;i virgulinon, alie &gcirc;i dezertigus la tutan landon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
       okaze de festoj <klr>[&#x2026;]</klr> &ccirc;inoj &scirc;atas prezenti
       <tld/>o-dancon
       <fnt>
         <aut>Yang Zhen</aut>,
         <vrk>Kiel la &ccirc;inoj rigardas drakon</vrk>,
         <lok><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/">
         El popola &Ccirc;inio, 2000-02</url></lok>
       </fnt>;
      </ekz>
      <ekz>
        la virinoj kune remas, uzante specimenon de pra&ccirc;ina tiel nomata <tld/>boato, kun
        20 sidlokoj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2015/011437.php">Remado-rimedo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
       trans la vila&gcirc;o sur la monteto al <nom>Slatina</nom> ku&scirc;as terurega besto,
       certe <tld/>o
       <fnt>
         <aut>M. Hru&scaron;ka</aut>,
         <vrk><url ref="http://www.medy.cz/Brno-Esperanto/Drako.htm">
         Kiel viandisto mortigis drakon</url></vrk>,
         <lok>2006-09</lok>
       </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &leftquot;<tld/>oj&rightquot; de Komodo: gigantaj bestoj sur izola insulo
       <fnt>
          <aut>Wim de Smet</aut>
          <vrk><url ref="&Monato;/2000/006900.php">La &leftquot;drakoj&#x201D; de Komodo:
          Gigantaj bestoj sur izola insulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 18</lok>
        </fnt>.
      </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="behemot.0o">Behemoto</ref>,
     <ref cel="levjat.0o">Levjatano</ref>,
     <ref cel="hximer.0o.MIT">&hcirc;imero <sncref/></ref>,
     <ref cel="bazili1.0o.MIT">bazilisko<sncref/></ref>
   </refgrp>
   <trdgrp lng="be">
     <trd>&c_c;&c_m;&c_o;&c_k;</trd>,
     <trd>&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;</trd>
   </trdgrp>
   <trd lng="bo">&t_-a;&t_ba;&t_rata;&t_shabchu;&t_ga;&t_tsheg;</trd>
   <trd lng="ca">drac</trd>
   <trdgrp lng="de">
     <trd>Lindwurm</trd>,
   </trdgrp>
   <trd lng="en">dragon</trd>
   <trd lng="es">drag&oacute;n</trd>
   <trd lng="fr">dragon<klr tip="ind"> (myth.)</klr></trd>
   <trd lng="id">naga</trd>
   <trdgrp lng="it">
     <trd>drago</trd>,
     <trd>dragone<klr> (mit. e arald.)</klr></trd>
   </trdgrp>
   <trd lng="nl">draak</trd>
   <trd lng="pl">smok</trd>
   <trd lng="pt">drag&atilde;o</trd>
   <trd lng="ro">dragon</trd>
   <trd lng="ru">&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;</trd>
   <trd lng="zh">&#x9F99; <pr>l&oacute;ng</pr></trd>
 </snc>
 <snc mrk="drak.0o.kajto">
   <uzo tip="fak">SPO</uzo>
   <dif>
      <ref tip="dif" cel="kajt.0o">Kajto</ref>:
      <ekz>
        de la plafono pendas paperaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010678.php">Bratislavo, urbo kun (&ccirc;ina)
          klaso</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
   </dif>
   <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y; <ind>&c_z;&c_mol;&c_m;&c_je;&c_j;</ind></trd>
   <trdgrp lng="ca">
     <trd>estel</trd>,
     <trd>milotxa</trd>
   </trdgrp>
   <trd lng="fr">cerf-volant</trd>
   <trd lng="it">aquilone</trd>
   <trd lng="nl">vlieger</trd>
   <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; &c_z;&c_m;&c_je;&c_j;</trd>
 </snc>
  <trdgrp lng="cs">
    <trd>drak</trd>,
    <trd>sa&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Drache</trd>
  <trdgrp lng="sk">
    <trd>drak</trd>,
    <trd>&scaron;arkan</trd>
  </trdgrp>
</drv>

<drv mrk="drak.D0o">
 <kap><tld lit="D"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
 </kap>
 <mlg>Dra</mlg>
 <snc mrk="drak.D0o.AST">
   <uzo tip="fak">AST</uzo>
   <dif>
     Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
     (<trd lng="la">Draco</trd>).
   </dif>
 </snc>
 <trd lng="be">&c_C;&c_m;&c_o;&c_k; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="bo">&t_-a;&t_ba;&t_rata;&t_shabchu;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">el Drag&oacute;</trd>
  <trdgrp lng="cs">
    <trd>drak</trd>,
    <trd>sa&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Drache <klr>(Sternbild)</klr></trd>
  <trd lng="en">Draco</trd>
  <trd lng="es">Drag&oacute;n</trd>
  <trd lng="fr">Dragon<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="it">Dragone<klr> (astr.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7ADC;  <pr>&#x308A;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x30C9;&#x30E9;&#x30B4;&#x30F3;</trd>,
    <trd>&#x30C8;&#x30D3;&#x30C8;&#x30AB;&#x30B2;</trd>
  </trdgrp>
  <trd lng="nl">Draak</trd>
  <trd lng="pl">Smok  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Dragonul  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="sk">
    <trd>drak</trd>,
    <trd>&scaron;arkan</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_z;&c_m;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="drak.flug0o">
 <kap>flug<tld/>o</kap>
 <snc mrk="drak.flug0o.SPO">
   <uzo tip="fak">SPO</uzo>
   <dif>
     <ref tip="dif" cel="kajt.0o">Kajto</ref>:
     <ekz>
      la ventoj <klr>[&#x2026;]</klr> portis iliajn flug<tld/>ojn al la altoj
      <fnt>
        <aut>Stellan Engholm</aut>:
        <vrk>Homoj sur la Tero</vrk>
      </fnt>.
     </ekz>
   </dif>
 </snc>
 <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y; <ind>&c_z;&c_mol;&c_m;&c_je;&c_j;</ind></trd>
  <trdgrp lng="ca">
   <trd>estel</trd>,
   <trd>milotxa</trd>
 </trdgrp>
  <trd lng="cs">drak <klr>(hra&ccaron;ka| letadlo)</klr></trd>
  <trd lng="de">Flugdrache</trd>
  <trd lng="en">kite</trd>
 <trd lng="fr">cerf-volant</trd>
  <trd lng="he">&dalet;&resh;&qof;&vav;&fnun; &mem;&ayin;&vav;&pe;&fpe;</trd>
  <trdgrp lng="id">
   <trd><ind>layang</ind>an</trd>,
   <trd><ind>layang</ind>-layang</trd>
 </trdgrp>
  <trd lng="it">aquilone</trd>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#x305F;&#x3053;</trd>
  </trdgrp>
  <trd lng="nl">vlieger</trd>
  <trd lng="pl">smok lataj&aogonek;cy</trd>
  <trd lng="ro">dragonul zburator</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; &c_z;&c_m;&c_je;&c_j;</trd>
  <trd lng="sk">&scaron;arkan</trd>
  <trd lng="uk">&c_p;&c_a;&c_p;&c_je;&c_r;&c_o;&c_v;&c_i;&c_j;/&c_p;&c_o;&c_v;&c_ib;&c_t;&c_r;&c_ja;&c_n;&c_i;&c_j; &c_z;&c_m;&c_ib;&c_j;</trd>
</drv>
</art>
