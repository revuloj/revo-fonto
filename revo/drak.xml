<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>drak</rad>/o
</kap>
<drv mrk="drak.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="drak.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Fabela besto, kiun antikvuloj imagis kun flugiloj, ungegoj kaj serpenta vosto.
    </dif>
    <trdgrp lng="de">
       <trd>Drache</trd>,
       <trd>Lindwurm</trd>
    </trdgrp>
    <trd lng="fr">dragon<klr tip="ind"> (myth.)</klr></trd>
    <trd lng="ru">&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;</trd>
  </snc>
  <snc mrk="drak.0o.kajto">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="kajt.0o">kajto</ref>
    <trd lng="de">Drache</trd>
  </snc>
</drv>

<drv mrk="drak.flug0o">
  <kap>
    flug<tld/>o
  </kap>
  <uzo tip="fak">SPO</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="kajt.0o">kajto</ref>
  </snc>
  <trd lng="de">Flugdrache</trd>
</drv>
</art>
