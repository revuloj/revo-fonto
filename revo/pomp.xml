<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pomp.xml,v 1.22 2024/02/17 07:52:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>pomp</rad>/o
</kap>
<drv mrk="pomp.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Impona graviga aspekto de lukse ornamita vidita&jcirc;o:
      <ekz>
        <tld/>o de la kulto, de re&gcirc;a kortego, de ceremonio;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la stilo ne sufi&ccirc;as por ka&scirc;i la senenhavecon de
        la
        verko;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en intensa bluo <tld/>as la &ccirc;ielo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_p;&c_e;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ok&aacute;zalost</trd>,
    <trd>pompa</trd>
  </trdgrp>
  <trd lng="de">Pomp</trd>
  <trdgrp lng="en">
    <trd>pomp</trd>,
    <trd>display</trd>,
    <trd>splendor</trd>
  </trdgrp>
  <trd lng="fr">pompeux</trd>
  <trd lng="hu">pompa</trd>
  <trdgrp lng="ja">
    <trd>&#x8C6A;&#x52E2;  <pr>&#x3054;&#x3046;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x58EE;&#x9E97;  <pr>&#x305D;&#x3046;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x8C6A;&#x83EF;&#x7D62;&#x721B;  <pr>&#x3054;&#x3046;&#x304B;&#x3051;&#x3093;&#x3089;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pracht</trd>,
    <trd>praal</trd>,
    <trd>luister</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ceremonia</trd>,
    <trd>pompa</trd>,
    <trd>przepych</trd>
  </trdgrp>
  <trd lng="pt">pompa</trd>
  <trdgrp lng="ro">
    <trd>ceremonie</trd>,
    <trd>pomp&abreve;</trd>,
    <trd>fast</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;dhera</trd>,
    <trd>pomp&eacute;znos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_i;&c_sh;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_i;&c_sh;&#x43D;&#x43E;&c_t;&c_a;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&c_ib;&c_sh;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&#x43F;&c_je;&c_z;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x534E; <pr>gu&#x101;nghu&#xE1;</pr></trd>,
    <trd>&#x5149;&#x83EF; <pr>gu&#x101;nghu&#xE1;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pomp.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Elmontranta imponan luksan belecon:
      <ekz>
        <tld/>a palaco, urbo, ceremonio;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a somero;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj arboj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a parolado.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="solen.0a">solena</ref>,
      <ref cel="bril">brila</ref>,
      <ref cel="fest">festa</ref>,
      <ref cel="grandi.0a">grandioza</ref>,
      <ref cel="parad">parada</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_p;&c_e;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>ok&aacute;zal&yacute;</trd>,
    <trd>pomp&eacute;zn&iacute;</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
  <trd lng="de">pomp&ouml;s</trd>
  <trdgrp lng="en">
    <trd>pompous</trd>,
    <trd>showy</trd>,
    <trd>splendid</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pompeux</trd>,
    <trd>sentencieux</trd>,
    <trd>majestueux</trd>
  </trdgrp>
  <trd lng="hu">pomp&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x8C6A;&#x52E2;&#x306A;  <pr>&#x3054;&#x3046;&#x305B;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x306F;&#x306A;&#x3084;&#x304B;&#x306A;</trd>,
    <trd>&#x58EE;&#x9E97;&#x306A;  <pr>&#x305D;&#x3046;&#x308C;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x83EF;&#x9E97;&#x306A;  <pr>&#x304B;&#x308C;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x7D62;&#x721B;&#x8C6A;&#x83EF;&#x306A;  <pr>&#x3051;&#x3093;&#x3089;&#x3093;&#x3054;&#x3046;&#x304B;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pompeus</trd>,
    <trd>prachtig</trd>,
    <trd>luisterrijk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pompatyczny</trd>,
    <trd>wystawny</trd>,
    <trd>zbytkowny</trd>
  </trdgrp>
  <trd lng="pt">pomposo</trd>
  <trdgrp lng="ro">
    <trd>pompos</trd>,
    <trd>somptuos</trd>,
    <trd>exagerat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ok&aacute;zal&yacute;</trd>,
    <trd>pomp&eacute;zny</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_i;&c_sh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&c_ib;&c_sh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&#x43F;&c_je;&c_z;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x865A;&#x5938; <pr>x&#x16B;ku&#x101;</pr></trd>,
    <trd>&#x865B;&#x8A87; <pr>x&#x16B;ku&#x101;</pr></trd>,
    <trd>&#x6D6E;&#x534E; <pr>f&#xFA;hu&#xE1;</pr></trd>,
    <trd>&#x6D6E;&#x83EF; <pr>f&#xFA;hu&#xE1;</pr></trd>,
    <trd>&#x5962;&#x534E; <pr>sh&#x113;hu&#xE1;</pr></trd>,
    <trd>&#x5962;&#x83EF; <pr>sh&#x113;hu&#xE1;</pr></trd>
  </trdgrp>
</drv>
