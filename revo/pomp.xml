<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pomp.xml,v 1.16 2020/03/20 23:11:13 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>pomp</rad>/o
</kap>
<drv mrk="pomp.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Impona graviga aspekto de lukse ornamita vidita&jcirc;o:
      <ekz>
        <tld/>o de la kulto, de re&gcirc;a kortego, de ceremonio;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la stilo ne sufi&ccirc;as por ka&scirc;i la senenhavecon de
        la
        verko;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en intensa bluo <tld/>as la &ccirc;ielo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_p;&c_e;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ok&aacute;zalost</trd>,
    <trd>pompa</trd>
  </trdgrp>
  <trd lng="de">Pomp</trd>
  <trdgrp lng="en">
    <trd>pomp</trd>,
    <trd>display</trd>,
    <trd>splendor</trd>
  </trdgrp>
  <trd lng="fr">pompeux</trd>
  <trd lng="hu">pompa</trd>
  <trdgrp lng="nl">
    <trd>pracht</trd>,
    <trd>praal</trd>,
    <trd>luister</trd>
  </trdgrp>
  <trd lng="pt">pompa</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;dhera</trd>,
    <trd>pomp&eacute;znos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pomp.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Elmontranta imponan luksan belecon:
      <ekz>
        <tld/>a palaco, urbo, ceremonio;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a somero;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj arboj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a parolado.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="solen.0a">solena</ref>,
      <ref cel="bril">brila</ref>,
      <ref cel="fest">festa</ref>,
      <ref cel="grandi.0a">grandioza</ref>,
      <ref cel="parad">parada</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_p;&c_e;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>ok&aacute;zal&yacute;</trd>,
    <trd>pomp&eacute;zn&iacute;</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
  <trd lng="de">pomp&ouml;s</trd>
  <trdgrp lng="en">
    <trd>pompous</trd>,
    <trd>showy</trd>,
    <trd>splendid</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pompeux</trd>,
    <trd>sentencieux</trd>,
    <trd>majestueux</trd>
  </trdgrp>
  <trd lng="hu">pomp&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>pompeus</trd>,
    <trd>prachtig</trd>,
    <trd>luisterrijk</trd>
  </trdgrp>
  <trd lng="pt">pomposo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ok&aacute;zal&yacute;</trd>,
    <trd>pomp&eacute;zny</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
</drv>
</art>
