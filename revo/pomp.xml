<kap>
  <ofc>2</ofc>
  <rad>pomp</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="pomp.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Impona graviga aspekto de lukse ornamita vidita&jcirc;o:
      <ekz>
        la edzi&gcirc;on oni festis kun granda <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis nepriskribebla <tld/>o; &gcirc;i estis festoj, karuseloj, sen&ccirc;esaj
        opero-burleskoj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        l' altara <tld/>o de via falsa dio
        <fnt>
          <bib>LSB</bib>
          <lok>Averto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e meblitaj &ccirc;ambregoj
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Konfeso de la Potifar-edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        reklamaj flugfolioj priskribis la donacojn per la plej <tld/>e la&ubreve;daj vortoj
        <fnt>
          <bib>KrM</bib>
          <lok>41. Aliajn gvidas kaj mem ne vidas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la jaro 1796 la glora instruisto de surdamutaj, <nom>Sicard</nom>, kun granda <tld/>o
        <klr>(<ref cel="tro.0igo">troigo</ref>)</klr> anta&ubreve;anoncis oportunan pazigrafion
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="luks.0o">lukso</ref>,
      <ref cel="superb.0o">superbo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_p;&c_e;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ok&aacute;zalost</trd>,
    <trd>pompa</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pomp</trd>,
    <trd>Prunk</trd>,
    <trd>Aufwand <klr>(Pomp)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pomp</trd>,
    <trd>display</trd>,
    <trd>splendor</trd>
  </trdgrp>
  <trd lng="fr">pompeux</trd>
  <trd lng="hu">pompa</trd>
  <trdgrp lng="ja">
    <trd>&#x8C6A;&#x52E2;  <pr>&#x3054;&#x3046;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x58EE;&#x9E97;  <pr>&#x305D;&#x3046;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x8C6A;&#x83EF;&#x7D62;&#x721B;  <pr>&#x3054;&#x3046;&#x304B;&#x3051;&#x3093;&#x3089;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pracht</trd>,
    <trd>praal</trd>,
    <trd>luister</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ceremonia</trd>,
    <trd>pompa</trd>,
    <trd>przepych</trd>
  </trdgrp>
  <trd lng="pt">pompa</trd>
  <trdgrp lng="ro">
    <trd>ceremonie</trd>,
    <trd>pomp&abreve;</trd>,
    <trd>fast</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;dhera</trd>,
    <trd>pomp&eacute;znos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_i;&c_sh;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_i;&c_sh;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_k;&c_ib;&c_sh;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x534E; <pr>gu&amacron;nghu&aacute;</pr></trd>,
    <trd>&#x5149;&#x83EF; <pr>gu&amacron;nghu&aacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pomp.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Elmontranta imponan luksan belecon:
      <ekz>
        sur la strato okazis <tld/>a enterigira ceremonio
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a parolado pri la graveco de l' sciencoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda domo kun verando kaj vico de fenestroj verdire aspektis <tld/>a
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vico da kortegaj oficiroj, mar&scirc;antaj kun <tld/>a parado
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>

      <ekz>
        <tld/>a palaco, urbo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a somero;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj arboj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a parolado.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="solen.0a">solena</ref>,
      <ref cel="bril.0a.elstara">brila<sncref/></ref>,
      <ref cel="fest">festa</ref>,
      <ref cel="grandi.0a">grandioza</ref>,
      <ref cel="luks.0a">luksa</ref>,
      <ref cel="majest.0a">majesta</ref>,
      <ref cel="parad.0a">parada</ref>,
      <ref cel="puc.0a">puca</ref>,
      <ref cel="splend.0a">splenda</ref>,
      <ref cel="superb.0a">superba</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_p;&c_e;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>ok&aacute;zal&yacute;</trd>,
    <trd>pomp&eacute;zn&iacute;</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pomp&ouml;s</trd>,
    <trd>prunkvoll</trd>,
    <trd>aufw&auml;ndig <klr>(pomp&ouml;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pompous</trd>,
    <trd>showy</trd>,
    <trd>splendid</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pompeux</trd>,
    <trd>sentencieux</trd>,
    <trd>majestueux</trd>
  </trdgrp>
  <trd lng="hu">pomp&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x8C6A;&#x52E2;&#x306A;  <pr>&#x3054;&#x3046;&#x305B;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x306F;&#x306A;&#x3084;&#x304B;&#x306A;</trd>,
    <trd>&#x58EE;&#x9E97;&#x306A;  <pr>&#x305D;&#x3046;&#x308C;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x83EF;&#x9E97;&#x306A;  <pr>&#x304B;&#x308C;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x7D62;&#x721B;&#x8C6A;&#x83EF;&#x306A;  <pr>&#x3051;&#x3093;&#x3089;&#x3093;&#x3054;&#x3046;&#x304B;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pompeus</trd>,
    <trd>prachtig</trd>,
    <trd>luisterrijk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pompatyczny</trd>,
    <trd>wystawny</trd>,
    <trd>zbytkowny</trd>
  </trdgrp>
  <trd lng="pt">pomposo</trd>
  <trdgrp lng="ro">
    <trd>pompos</trd>,
    <trd>somptuos</trd>,
    <trd>exagerat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ok&aacute;zal&yacute;</trd>,
    <trd>pomp&eacute;zny</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_i;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_k;&c_ib;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_p;&c_je;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x865A;&#x5938; <pr>x&umacron;ku&amacron;</pr></trd>,
    <trd>&#x865B;&#x8A87; <pr>x&umacron;ku&amacron;</pr></trd>,
    <trd>&#x6D6E;&#x534E; <pr>f&uacute;hu&aacute;</pr></trd>,
    <trd>&#x6D6E;&#x83EF; <pr>f&uacute;hu&aacute;</pr></trd>,
    <trd>&#x5962;&#x534E; <pr>sh&emacron;hu&aacute;</pr></trd>,
    <trd>&#x5962;&#x83EF; <pr>sh&emacron;hu&aacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pomp.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Prezenti&gcirc;i kun <tld/>o:
      <ekz>
        mi vidis la butikan fenestron, en kiu <tld/>is la bela kuirila elmontro
        <fnt>
          <bib>KrM</bib>
          <lok>38. Du botoj faras paron.</lok>
        </fnt>;
      </ekz>
      <ekz>
        nomada monumento en la tombejo <tld/>as el nigra marmoro
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2009/009638.php"
              >La tulamora tombejo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia najbaro, la silkokomercisto, <tld/>is kaj aplombis per mono
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2005/008555.php"
              >&Scirc;uisto kaj silkokomercisto</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fanfar1.0i">fanfaroni</ref>
    </refgrp>
  </snc>
</drv>

