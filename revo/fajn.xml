<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fajn</rad>/a
</kap>
<drv mrk="fajn.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Ekstrakvalita, pursubstanca:
      <ekz><tld/>a oro, &scirc;tono;</ekz>
      <ekz><ind><tld/>igi</ind> metalon.
      <trd lng="fr">raffiner</trd>
      </ekz>
    </dif>
   <trd lng="de">fein</trd>
   <trd lng="nl">zuiver</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_s;&c_sh;&c_je;&c_g;&c_o; &c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_p;&c_r;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Prezentanta a&ubreve; distinganta akre subtilajn diferencojn:
      <ekz><tld/>a tolo, man&gcirc;a&jcirc;o, orelo;</ekz>
      <ekz><tld/>e kanelita disko de gramofono;</ekz>
      <ekz>
      <uzo tip="stl">FIG</uzo>
      <tld/>a gusto</ekz>.
    </dif>
    <trd lng="nl">delicaat</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_u;&c_t;&c_o;&c_n;&c_ch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_i;&c_z;&c_y;&c_s;&c_k;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
<trdgrp lng="fr">
  <trd>fin<klr tip="ind"> (d'une grande puret&eacute;)</klr></trd>,
  <trd>raffin&eacute;</trd></trdgrp> 
</drv>
<drv mrk="fajn.pli0a">
  <kap>pli <tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="fajn.pli0a.topologio">
    <dif>
      <klr>(p.p. <ref cel="topolo.0o.strukturo">topologio</ref>
      rilate al alia topologio super la sama aro)</klr>
      &Gcirc;in inkluzivanta:
      <ekz>
        la maldiskreta topologio estas la malplej <tld/>a el &ccirc;iuj;
      </ekz>
      <ekz>
        la idento-bildigo de topologia spaco al topologia spaco kun pli 
        malfajna topologio estas &ccirc;iam
        <ref cel="kontin3.0a.cxie">kontinua</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">feinere<klr tip="amb"> (Topologie)</klr></trd>
  <trd lng="en">finer<klr tip="amb"> (topology)</klr></trd>
  <trd lng="fr"><klr tip="amb">(topologie) </klr>plus <ind>fine</ind></trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_je;&c_je; <ind>&c_t;&c_o;&c_n;&c_k;&c_a;&c_ja;</ind><klr tip="amb"> (&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;)</klr></trd>
</drv>

</art>

