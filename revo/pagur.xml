<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>pagur</rad>/o
</kap>
<drv mrk="pagur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="pagur.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Pagurus</trd>).
    </dif>
  </snc>
  <trd lng="de">Einsiedlerkrebs</trd>
</drv>
<drv mrk="pagur.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="krust.dekpiedaj0uloj">depiedaj
      krustuloj</ref> (<trd lng="la">Paguridae</trd>), vivantaj en malplenaj   
      konkoj.
    </dif>
  </snc>
  <trd lng="de">Meereseinsiedlerkrebse</trd>
</drv>
</art>
