<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>pagur</rad>/o
</kap>
<drv mrk="pagur.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:zoologiaj_genroj">Genro</ref> 
       el familio <ref tip="malprt"
       cel="pagure.0oj"><tld/>edoj</ref>
       (<trd lng="la">Pagurus</trd>)
       <ref tip="lst" cel="krust.0ulo"
       lst="voko:krustuloj">krustulo</ref>
       kun du diversgrandaj pin&ccirc;iloj, protektanta sin en 
       konko de marheliko.
     </dif>
   </snc>
<bld lok="../bld/pagur.jpg">
<tld/>o atakas litorinan konkon</bld>
   <trd lng="be">&c_r;&c_a;&c_k;-&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
   <trd lng="bg">&c_o;&c_t;&c_sh;&c_je;&c_l;&c_n;&c_i;&c_k; &c_r;&c_a;&c_c;&c_i;</trd>
   <trd lng="de">Einsiedlerkrebs</trd>
   <trd lng="en">hermit <ind>crab</ind></trd>
   <trdgrp lng="fr">
     <trd>pagure</trd>,
     <trd>bernard-l'<ind>ermite</ind></trd>
   </trdgrp>
   <trd lng="hu">remeter&aacute;k</trd>
   <trd lng="nl">heremietkreeft</trd>
   <trdgrp lng="pt">
     <trd>paguro</trd>,
     <trd>ermit&atilde;o</trd>
   </trdgrp>
   <trd 
lng="ru">&c_r;&c_a;&c_k;-&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>

</art>
