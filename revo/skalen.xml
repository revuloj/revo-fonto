<kap>
  <rad>skalen</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skalen.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="angul.tri0o">triangulo</ref>)</klr>
      Kies tri lateroj estas malsamlongaj:
      <ekz>
        egallatera triangulo estas tiu kiu havas la tri laterojn egalajn, izocela estas
        tiu kiu havas nur du laterojn egalajn, <tld/>a estas tiu kiu havas la tri
        laterojn malegalajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=80361">Elementoj de E&ubreve;klido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_z;&c_n;&c_a;&c_b;&c_a;&c_k;&c_o;&c_v;&c_y;</trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_n;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_je;&c_n;</trd>
  <trd lng="ca">escal&egrave;</trd>
  <trdgrp lng="cs">
    <trd>obecn&yacute;</trd>,
    <trd>r&uring;znostrann&yacute;</trd>
  </trdgrp>
  <trd lng="de">ungleichseitig</trd>
  <trd lng="en">scalene</trd>
  <trd lng="es">escaleno</trd>
  <trd lng="fr"><klr tip="ind">(triangle) </klr>scal&egrave;ne</trd>
  <trd lng="hu">szab&aacute;lytalan <klr tip="ind">(h&aacute;romsz&ouml;g)</klr></trd>
  <trd lng="nl">ongelijkzskolop2ijdig</trd>
  <trd lng="pl"><klr tip="ind">(tr&oacute;jk&aogonek;t) </klr>r&oacute;&zdot;noboczny</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">nerovnostrann&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x456;&#x437;&#x43D;&#x43E;&#x431;&#x456;&#x447;&#x43D;&#x438;&#x439; (&#x43F;&#x440;&#x43E; &#x442;&#x440;&#x438;&#x43A;&#x443;&#x442;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x43F;&#x435;&#x446;&#x456;&#x44E;)</trd>
  </trdgrp>
</drv>
</art>
