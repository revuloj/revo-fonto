<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: monoik.xml,v 1.2 2024/02/20 15:22:08 afido Exp $">
    <kap>
      <rad>monoik</rad>/a
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="monoik.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="diklin.0a">Diklina</ref> kun kaj masklaj kaj femalaj floroj sur sama
      individuo:
      <ekz>
        palmoj <klr>[&#8230;]</klr> estas <tld/>aj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=79123"
              >Areko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="dioik.0a">dioika</ref>
    <trdgrp lng="de">
      <trd>einh&auml;usig</trd>,
      <trd>mon&ouml;zisch</trd>
    </trdgrp>
    <trd lng="en">monoecious</trd>
    <trd lng="fr">mono&iuml;que</trd>
    <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_d;&c_o;&c_m;&c_n;&c_y;&c_j;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_a;&c_d;&c_n;&c_a;&c_p;&c_n;&c_jo;&c_v;&c_y;</trd>,
  <trd>&c_a;&c_d;&c_n;&c_a;&c_d;&c_o;&c_m;&c_n;&c_y;</trd>
</trdgrp>
</drv>

  </art>

