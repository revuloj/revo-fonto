<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pari</rad>/o
</kap>
<drv mrk="pari.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo el la lasta, plej mal&scirc;atata kasto &ccirc;e la Hindoj.
    </dif>
    <trd lng="de">Paria</trd>
    <trd lng="hu">p&aacute;ria</trd>
    <trd lng="nl">paria</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_ja;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mizerulo de &ccirc;iuj mal&scirc;atata.
    </dif>
    <trd lng="nl">paria</trd>
    <trd lng="hu">p&aacute;ria</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_r;&c_i;&c_ja;</trd>,
      <trd>&c_i;&c_z;&c_g;&c_o;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Interna surfaco de cilindro, vazo, tubo, a&ubreve; alispeca objekto.
    </dif>
    <trd lng="de">Innenfl&auml;che</trd>
    <trd lng="hu">bels&odblac; <ind>fel&uuml;let</ind></trd>
    <trd lng="nl">binnenwand</trd>
    <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja; &c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Interna surfaco de organa kava&jcirc;o:
      <ekz>
        <tld/>oj de kranio, de stomako.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pariet.0">parieto</ref>,
      <ref cel="vand.0o">vando</ref>.
    </refgrp>
    <trd lng="nl">binnenwand</trd>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_k;&c_a; <klr tip="amb">(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  </snc>
  <trd lng="de">Wandung</trd>
  <trdgrp lng="hu">
    <trd>paries</trd>,
    <trd>fal</trd>
  </trdgrp>
</drv>
</art>
