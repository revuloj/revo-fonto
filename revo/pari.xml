<art mrk="$Id: pari.xml,v 1.20 2022/10/19 20:37:12 revo Exp $">
<kap>
  <rad>pari</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pari.0o">
  <kap><tld/>o</kap>  
  <snc>
    <uzo tip="fak">REL</uzo><uzo tip="fak">POL</uzo>
    <dif>
      Homo el la lasta, plej mal&scirc;atata <ref tip="malprt" cel="kast.0o">kasto</ref> &ccirc;e la Hindoj.
    </dif>
    <trd lng="ca">p&agrave;ria</trd>
    <trd lng="en">untouchable</trd>
    <trd lng="es">paria</trd>
    <trd lng="fr">intouchable<klr> (subst.)</klr></trd>
    <trd lng="hu">p&aacute;ria</trd>
    <trdgrp lng="it">
      <trd>paria</trd>,
      <trd>intoccabile</trd>
    </trdgrp>
    <trd lng="nl">paria</trd>
    <trd lng="pl">parias</trd>   
    <trd lng="pt">p&aacute;ria</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_ja;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="mizer.0ulo">Mizerulo</ref> de &ccirc;iuj mal&scirc;atata:
      <ekz>
        mizeruloj de &ccirc;i tiu socio, <tld/>oj, kiuj rondiras sen favoro kaj sen protekto,
        kiuj malhavas hejmon
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluraj aidoso-suferantoj i&gcirc;is <tld/>oj inter siaj najbaroj, kiuj senbaze
        kulpigis ilin pri ilia malsano
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007075.html"
              >Pa&scirc;oj al purigado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni devis stari en longega vico de senkreditkartuloj por ricevi nian bileton, ni
        sentis nin <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2007/009152.php"
              >Malflugemo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="almoz.0ulo">almozulo</ref>,
      <ref cel="jxet.el0ulo">el&jcirc;etulo</ref>,
      <ref cel="fav.0ulo.FIG">favulo<sncref/></ref>,
      <ref cel="margxe.0ulo">mar&gcirc;enulo</ref>
    </refgrp>
    <trd lng="ca">p&agrave;ria</trd>
    <trdgrp lng="de">
      <trd>Au&szlig;enseiter</trd>,
      <trd>Auss&auml;tziger</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>pariah</trd>,
      <trd>outcaste</trd>
    </trdgrp>
    <trd lng="es">paria</trd>
    <trd lng="fr">paria</trd>
    <trd lng="hu">p&aacute;ria</trd>
    <trdgrp lng="it">
      <trd>paria<klr> (fig.)</klr></trd>,
      <trd>reietto</trd>
    </trdgrp>
    <trd lng="nl">paria</trd>
    <trd lng="pl">parias</trd>   
    <trd lng="pt">p&aacute;ria</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_r;&c_i;&c_ja;</trd>,
      <trd>&c_i;&c_z;&c_g;&c_o;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>

