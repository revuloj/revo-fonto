    <trd>vyvr&zcaron;enec</trd>
  </trdgrp>
  <trd lng="de">Paria</trd>
  <trdgrp lng="sk">
    <trd>vn&uacute;torn&yacute; povrch dut&yacute;ch telies (tech.)</trd>,
    <trd>vn&uacute;torn&yacute; povrch &uacute;strojn&yacute;ch dut&iacute;n <klr>(anat.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_p;&c_r;&c_a;&c_v;&c_n;&c_i;&c_j;</trd>,
