<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suprem.xml,v 1.29 2024/03/25 07:28:38 revo Exp $">
<kap>
  <rad>suprem</rad>/o
</kap>
<drv mrk="suprem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de subaro <frm am="bb A"><g>A</g></frm> de
      <ref cel="ar.orda0o">orda aro</ref> 
      <frm am="(bb E,le)">(<g>E</g>,&#x2264;)</frm>)</klr>
      La <ref tip="super" cel="minimu.0o.MAT">minimumo</ref> de la aro
      de &gcirc;iaj superaj
      <ref cel="bar.0o.MAT">baroj</ref>; alidire: la plej malgranda
      supera baro; simb. <frm am="&quot;sup&quot; bb A">sup <g>A</g></frm>:
      <ekz>
        se &gcirc;i ekzistas, la <tld/>o de <frm am="bb A"><g>A</g></frm>
        povas
        ne aparteni al <frm am="bb A"><g>A</g></frm>;
      </ekz>
      <ekz>
        &ccirc;iu reela subaro kun superaj baroj akceptas reelan <tld/>on,
        sed tio ne veras en la aro de racionalaj nombroj.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="infim.0o">infimo</ref>.
    </refgrp>
    <rim>
      Kelkaj preferas la pli skemisman <ctl>supera limo</ctl>
      kiel en <fnt><bib>EKV</bib><lok>&para;252</lok></fnt>.
    </rim>
  </snc>
  <trd lng="ca">suprem <klr>(mat.)</klr></trd>
  <trdgrp lng="cs">
    <trd>nadvl&aacute;da</trd>,
    <trd>supremace</trd>,
    <trd>svrchovanost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Supremum</trd>,
    <trd>obere <ind>Grenze</ind></trd></trdgrp>
  <trd lng="en">least upper <ind>bound</ind></trd>
  <trd lng="es">l&iacute;mite superior</trd>
  <trd lng="fr">borne sup&eacute;rieure</trd>
  <trd lng="hu">szupr&eacute;mum</trd>
  <trd lng="pl">kres g&oacute;rny</trd>
  <trd lng="pt">supremo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_je;&c_m;&c_u;&c_m;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;&c_ja; <ind>&c_g;&c_r;&c_a;&c_n;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadvl&aacute;da</trd>,
    <trd>zvrchovanos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x43F;&#x440;&#x435;&#x43C;&#x443;&#x43C;</trd>,
    <trd>&#x442;&#x43E;&#x447;&#x43D;&#x430; &#x432;&#x435;&#x440;&#x445;&#x43D;&#x44F; &#x43C;&#x435;&#x436;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0A;&#x9650; <pr>sh&#xE0;ngxi&#xE0;n</pr></trd>,
    <trd>&#x4E0A;&#x786E;&#x754C; <pr>sh&#xE0;ngqu&#xE8;ji&#xE8;</pr></trd>,
    <trd>&#x4E0A;&#x78BA;&#x754C; <pr>sh&#xE0;ngqu&#xE8;ji&#xE8;</pr></trd>,
    <trd>&#x6700;&#x5C0F;&#x4E0A;&#x754C; <pr>zu&#xEC;xi&#x1CE;osh&#xE0;ngji&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="suprem.limeso0o">
  <kap>limeso<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <subsnc mrk="suprem.limeso0o.subaro">
      <fnt><bib>EKV</bib><lok>&para;255</lok></fnt>
      <dif>
        <klr>(de subaro el reeloj)</klr>
        &Gcirc;ia plej granda
        <ref cel="akumul.0igxa.punkto">akumuli&gcirc;a</ref> punkto.
      </dif>
      <refgrp tip="ant">
        <ref cel="infim.limes0o.subaro">limesinfimo</ref>.
      </refgrp>
    </subsnc>
    <subsnc mrk="suprem.limeso0o.funkcio">
      <fnt><bib>PIV2</bib><lok>&leftquot;limesa supremo&rightquot;</lok></fnt>
      <dif>
        <klr>(de reela <ref cel="funkci.0o.MAT">funkcio</ref>,
        &ccirc;e punkto <frm am="a"><k>a</k></frm> de ties fonto-aro)</klr>
        La <ref cel="suprem.0o"><tld/>o</ref> de la aro de punktoj,
        <ref cel="adher.0a.bildigo">adheraj</ref> al <frm am="f"><k>f</k></frm>
        &ccirc;e <frm am="a"><k>a</k></frm>; simb. 
        <frm am="&quot;lim sup&quot;_(x to a)f(x)">lim sup<sub><k>x</k>&#x2192;<k>a</k></sub> <k>f</k>(<k>x</k>)</frm>.
      </dif>
      <refgrp tip="ant">
        <ref cel="infim.limes0o.funkcio">limesinfimo</ref>.
      </refgrp>
    </subsnc>
  </snc>
  <trd lng="ca">l&iacute;mit superior</trd>
  <trd lng="cs">horn&iacute; limita</trd>
  <trdgrp lng="de">
    <trd>oberer <ind>Limes</ind></trd>,
    <trd>limes superior</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>upper <ind>limit</ind></trd>,
    <trd>superior <ind>limit</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>limite sup&eacute;rieure</trd>
  </trdgrp>
  <trd lng="hu">limesz szuperior</trd>
  <trdgrp lng="pl">
    <trd>granica g&oacute;rna</trd>,
    <trd>limes superior</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_n;&c_i;&c_j; <ind>&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;</ind></trd>
  </trdgrp>
  <trd lng="sk">horn&aacute; limita</trd>
</drv>
</art>
