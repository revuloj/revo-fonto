<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>suprem</rad>/o
</kap>
<drv mrk="suprem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de subaro <em>A</em> de
      <ref cel="ord.0a.aro">orda aro</ref> (<em>E</em>, <em>R</em>))</klr>
      La <ref tip="super" cel="minimu.0o.MAT">minimumo</ref> de la aro
      de &gcirc;iaj superaj
      <ref cel="bar.0o.MAT">baroj</ref>; alidire: la plej malgranda
      supera baro:
      <ekz>
        se &gcirc;i ekzistas, la <tld/>o de <em>A</em> povas
        ne aparteni al <em>A</em>;
      </ekz>
      <ekz>
        &ccirc;iu reela subaro kun superaj baroj akceptas reelan <tld/>on,
        sed tio ne validas en la aro de racionalaj nombroj.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="infim.0o">infimo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Supremum</trd>
  <trd lng="fr">borne sup&eacute;rieure</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_je;&c_m;&c_u;&c_m;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;&c_ja; &c_g;&c_r;&c_a;&c_n;&c_mol; <klr tip="amb">&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_a;</klr></trd>
  </trdgrp>
</drv>
</art>
