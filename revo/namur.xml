<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Namur</rad>/o
</kap>
<drv mrk="namur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="namur.0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la samnoma belga provinco
      <ref tip="malprt" cel="namur.0o.prov">Namuro</ref>, &ccirc;e la bordo de la
      riveroj <ref tip="vid" cel="moz.0o">Mozo</ref> kaj
      <ref tip="vid" cel="sambr.0o">Sambro</ref> (4&ring;52'E, 50&ring;27'N):
      <ekz>
        post Varsovio mi sidis kaj dormis trankvile en alia pola vagono
        sur sidlokoj &gcirc;is <tld/>o.
        <fnt><aut>Bruno HENRY</aut></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="namur.0o.prov">
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en
      <ref tip="malprt" cel="valon.V0ujo">Valonujo</ref> (suda Belgujo)
      kies &ccirc;efurbo anka&ubreve; nomi&gcirc;as <ref tip="prt"
      cel="namur.0o.urb">Namuro</ref>.
    </dif>
  </snc>
  <trd lng="fr">Namur</trd>
  <trd lng="nl">Namen</trd>
  <trd lng="ru">&c_N;&c_a;&c_m;&c_ju;&c_r;</trd>
</drv>
</art>
