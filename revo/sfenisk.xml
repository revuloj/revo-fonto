<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>sfenisk</rad>/o
</kap>
<drv mrk="sfenisk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Plej norda genro el la ordo
      <ref tip="super" cel="sfenisk.0oformaj">sfeniskoformaj</ref>
      birdoj (<trd lng="la">Spheniscus</trd>),
      vivanta en sudokcidenta Afriko kaj sur Galapagaj insuloj.
    </dif>
  </snc>
</drv>
<drv mrk="sfenisk.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Sphenisci</trd>),
      vivantaj &ccirc;e la maroj de la suda hemisfero, kun malgrandaj
      flugiloj uzataj por na&gcirc;ado kaj kun na&gcirc;ha&ubreve;toj
      &ccirc;e la piedoj.
    </dif>
    <ref tip="prt" cel="pingvi.0o">pingvino</ref>
  </snc>
  <trd lng="de">Pinguine</trd>
</drv>
</art>
