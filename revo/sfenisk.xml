<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sfenisk.xml,v 1.15 2024/03/24 20:21:56 revo Exp $">
<kap>
  <rad>sfenisk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sfenisk.0o">
  <kap><tld/>o</kap>
  <snc mrk="sfenisk.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdoj vivantaj en sudokcidenta Afriko kaj sur Galapagaj insuloj;
      plej norda genro de <ref tip="super" cel="pingve.0o">pingvenoj</ref>
      (<trd lng="la">Spheniscus</trd>)
      el la ordo <ref tip="super" cel="sfenisk.0oformaj">sfeniskoformaj</ref>:
      <ekz>
        la Sudafrika pingveno a&ubreve; Okulvitra pingveno (<nom>Spheniscus demersus</nom>),
        konata anka&ubreve; kiel Afrika pingveno, Mergi&gcirc;a <tld/>o la&ubreve; la latina
        scienca nomo, a&ubreve; Nigrapieda pingveno estas specio de pingveno, limigita al sudafrikaj akvoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=425611">Sudafrika pingveno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cc/Magellanic_penguin%2C_Valdes_Peninsula%2C_e.jpg/640px-Magellanic_penguin%2C_Valdes_Peninsula%2C_e.jpg" prm="CC BY 2.0">Magelana <tld/>o (Spheniscus magellanicus), Duoninsulo Valdes, Argentino
        <fnt>
          <aut>David, Penguin Waddle</aut>
          <lok><url ref="&WCW;/File:Magellanic_penguin,_Valdes_Peninsula,_e.jpg">&WCW;/File:Magellanic_penguin,_Valdes_Peninsula,_e.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Brillenpinguin</trd>
  </snc>
  <trd lng="cs">tu&ccaron;&ncaron;&aacute;k galap&aacute;gsk&yacute;</trd>
  <trd lng="fr">manchot</trd>
  <trd lng="hu">pingvin<klr tip="amb"> (rend)</klr></trd>
  <trd lng="nl">pingu&iuml;n</trd>
  <trd lng="pl">pingwin magella&nacute;ski</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_a;&c_p;&c_a;&c_g;&c_o;&c_s;&c_s;&c_k;&c_i;&c_j; <ind>&c_p;&c_i;&c_n;&c_g;&c_v;&c_i;&c_n;</ind></trd>
  <trd lng="sk">tu&ccaron;niak galap&aacute;gsky</trd>
  <trd lng="uk">&#x43F;&c_ib;&#x43D;&c_gu;&c_v;&c_ib;&#x43D; (&#x43D;&c_a;&c_u;&#x43A;. &#x43D;&c_a;&c_z;&c_v;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x9ED1;&#x811A;&#x4F01;&#x9E45; <pr>h&#x113;iji&#x1CE;oq&#x1D0;&#xE9;</pr></trd>,
    <trd>&#x9ED1;&#x8173;&#x4F01;&#x9D5D; <pr>h&#x113;iji&#x1CE;oq&#x1D0;&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sfenisk.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="sfenisk.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="prt" cel="pingve.0o.ZOO">Pingvenoj</ref>;
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">ordo</ref> de
      <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      vivantaj &ccirc;e la maroj de la suda hemisfero, kun malgrandaj
      flugiloj uzataj por na&gcirc;ado kaj kun na&gcirc;ha&ubreve;toj
      &ccirc;e la piedoj (<trd lng="la">Sphenisciformes</trd>).
    </dif>

  </snc>
  <trd lng="cs">tu&ccaron;&ncaron;&aacute;ci <klr>(&rcaron;&aacute;d)</klr></trd>
  <trd lng="de">Pinguine</trd>
  <trd lng="fr">Sph&eacute;nisciformes</trd>
  <trd lng="hu">pingvinalak&uacute;ak</trd>
  <trd lng="nl">pingu&iuml;ns</trd>
  <trd lng="pl">pingwiny</trd>
  <trd lng="ru">&c_p;&c_i;&c_n;&c_g;&c_v;&c_i;&c_n;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">tu&ccaron;niaci</trd>
  <trd lng="uk">&#x43F;&c_ib;&#x43D;&c_gu;&c_v;&c_ib;&#x43D;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F01;&#x9D5D; <pr>q&#x1D0;&#xE9;</pr></trd>,
    <trd>&#x4F01;&#x9E45; <pr>q&#x1D0;&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
