<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>arkiv</rad>/o
</kap>
<drv mrk="arkiv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kolekto da malnovaj aktoj kaj dokumentoj:
      <ekz>
        historiaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Loko kie oni gardas dokumentojn, <tld/>ejo:
      <ekz>
        formeti ion en
        <tld/>on<fnt>Z</fnt> <klr>(f. flankenlasi.
        <refgrp tip="sin">
          <ref cel="kalend.0o">kalendo</ref>,
          <ref cel="forges.0i">forgesi</ref>,
          <ref cel="ter.en0igi">enterigi</ref>
        </refgrp>)</klr>;
      </ekz>
      <ekz>
        jam aparteni al la <tld/>o<fnt>B</fnt> <klr>(esti eksmoda)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">archief</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_v;</trd>
</drv>

<drv mrk="arkiv.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Oficisto gardanta, aran&gcirc;anta <tld/>ojn.
    </dif>
  </snc>
  <trd lng="nl">archivaris</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_h;&c_i;&c_v;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_i;&c_v;&c_a;&c_r;&c_i;&c_u;&c_s;</trd>
  </trdgrp>
</drv>
</art>
