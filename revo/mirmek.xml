<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mirmekob</rad>/o
</kap>
<drv mrk="mirmek.0o">
  <kap><tld/>o</kap>
  <snc mrk="mirmek.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la subklaso <ref tip="super"
      cel="marsup.0oj">marsupialoj</ref>
      de malgrandaj a&ubreve;straliaj mamuloj, kiuj man&gcirc;as
      preska&ubreve; nur
      <ref tip="vid" cel="termit.0oj">termitojn</ref>.
      <ekz>
        <tld lit="M"/>oj estas marsupiuloj, ru&gcirc;-brunaj
        &scirc;ultre kaj kape. Sur la plejparto
        de la korpo la koloro estas nigra kun blankaj strioj. Ilia vosto
        estas &ccirc;. 17cm
        longa.
        <tld lit="M"/>oj man&gcirc;as ekskluzive termitojn. Plenkreska
        <tld/>o bezonas
        man&gcirc;i &gcirc;is 20 mil termitojn &ccirc;iutage.
        <fnt>
          <aut>Donald Broadribb</aut>,
          <vrk>Kio do estas mirmekobo?
            <url
            ref="http://web.me.com/donbroadribb1/Mirmekobo/Mirmekobo_files/Mirmekobo%25204.pdf">Mirmekobo
            4</url>
          </vrk>,
          <lok>p. 6</lok>
       </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">mravencojed &zcaron;&iacute;han&yacute;</trd>
    <trd lng="de">Ameisenbeutler</trd>
    <trdgrp lng="en">
      <trd>numbat</trd>,
      <trd>banded <ind>anteater</ind></trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>pussimuurahaiskarhu</trd>,
      <trd>numbatti</trd>
    </trdgrp>
    <trd lng="hu">ersz&eacute;nyeshangy&aacute;sz</trd>
    <trd lng="la">Myrmecobius fasciatus</trd>
    <trd lng="pl">mr&oacute;wko&zdot;er workowaty</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_m;&c_ch;&c_a;&c_t;&c_y;&c_j;
      &c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_je;&c_d;</trd>,
      <trd>&c_n;&c_a;&c_m;&c_b;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>