<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mirmek.xml,v 1.7 2022/09/07 15:36:52 revo Exp $">
<kap>
  <rad>mirmekob</rad>/o
</kap>
<drv mrk="mirmek.0o">
  <kap><tld/>o</kap>
  <snc mrk="mirmek.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda a&ubreve;stralia 
      <ref tip="lst" cel="marsup.0o"
      lst="voko:marsupiuloj">marsupiulo</ref>,
      kiu man&gcirc;as preska&ubreve; nur
      <ref tip="vid" cel="termit.0oj">termitojn</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> 
      kun nur unu vivanta specio
      (<trd lng="la">Myrmecobius</trd>):
      <ekz>
        <tld/>oj estas marsupiuloj, ru&gcirc;-brunaj
        &scirc;ultre kaj kape; sur la plejparto
        de la korpo la koloro estas nigra kun blankaj strioj
        <fnt>
          <aut>D. Broadribb</aut>,
          <vrk>Kio do estas mirmekobo?</vrk>,
          <lok><url
          ref="http://web.me.com/donbroadribb1/Mirmekobo/Mirmekobo_files/Mirmekobo%25204.pdf">
          Mirmekobo, 2009-04 (4)</url>, p. 6a</lok>
       </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_u;&c_r;&c_a;&c_sh;&c_k;&c_a;&c_l;&c_ju;&c_b; &c_t;&c_o;&c_r;&c_b;&c_a;&c_ch;&c_k;&c_a;&c_v;&c_y;</trd>
    <trd lng="ca">numbat</trd>
    <trd lng="cs">mravencojed &zcaron;&iacute;han&yacute;</trd>
    <trdgrp lng="de">
      <trd>Ameisenbeutler</trd>,
      <trd>Numbat</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>numbat</trd>,
      <trd>banded <ind>anteater</ind></trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>pussimuurahaiskarhu</trd>,
      <trd>numbatti</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>myrm&eacute;cobie</trd>,
      <trd>numbat</trd>
    </trdgrp>
    <trd lng="hu">ersz&eacute;nyeshangy&aacute;sz</trd>
    <trd lng="id">numbat</trd>
    <trd lng="pl">mr&oacute;wko&zdot;er workowaty</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_m;&c_ch;&c_a;&c_t;&c_y;&c_j;
      &c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_je;&c_d;</trd>,
      <trd>&c_n;&c_a;&c_m;&c_b;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>
</art>

