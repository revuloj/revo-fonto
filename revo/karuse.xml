<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karusel</rad>/o
</kap>
<drv mrk="karuse.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="karuse.0o.rajda">
    <dif>
      Festa turniro, kie kavaliroj a&ubreve; rajdistoj turni&gcirc;e kaj artisme
      manovras.
    </dif>
    <trdgrp lng="de">
      <trd>Karussellreiten</trd>,
      <trd>Ritterspiel mit Ringstechen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>carrousel</trd>
    </trdgrp>
    <trd lng="en">carousel</trd>
    <trd lng="nl">caroussel</trd>
  </snc>
  <snc mrk="karuse.0o.infana">
    <dif>
      Porinfana amuzilo, konsistanta el lignaj rajdbestoj kaj se&gcirc;oj,
      metitaj sur granda ronda plata&jcirc;o, kiu turni&gcirc;as &ccirc;irka&ubreve; sia akso.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_u;&c_s;&c_e;&c_l;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>Karussell</trd>,
      <trd>Ringelspiel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>carousel</trd>,
      <trd>merry-go-round</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>man&egrave;ge<klr> (forain)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>caroussel</trd>,
      <trd>draaimolen</trd>,
      <trd>paardenmolen</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_u;&c_s;&c_je;&c_l;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="karuse.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">FIG</uzo>
  <snc mrk="karuse.0i.FIG">
    <dif>
      Alternadi, ade kaj senpa&ubreve;ze kirli&gcirc;i kaj &scirc;an&gcirc;i&gcirc;i:
      <ekz>
        dubo <tld/>is en lia kapo
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 251a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kalejd.0i">kalejdoskopi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="svarm.0i">svarmi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>bousculer</ind><klr> (fig.)</klr></trd>,
    <trd>s'<ind>embrouiller</ind><klr> (fig.)</klr></trd>
  </trdgrp>
</drv>
</art>
