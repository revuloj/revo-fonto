<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karusel</rad>/o
</kap>
<drv mrk="karuse.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Festa turniro, kie kavaliroj a&ubreve; rajdistoj turni&gcirc;e kaj artisme
      manovras:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &hcirc;aosaj pensoj <tld/>as <klr>(kirli&gcirc;as)</klr> en lia kapo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Karussellreiten</trd>,
      <trd>Ritterspiel mit Ringstechen</trd>
    </trdgrp>
    <trd lng="en">carousel</trd>
    <trd lng="nl">caroussel</trd>
  </snc>
  <snc>
    <dif>
      Porinfana amuzilo, konsistanta el lignaj rajdbestoj kaj se&gcirc;oj,
      metitaj sur granda ronda plata&jcirc;o, kiu turni&gcirc;as &ccirc;irka&ubreve; sia akso.
    </dif>
    <trdgrp lng="de">
      <trd>Karussell</trd>,
      <trd>Ringelspiel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>carousel</trd>,
      <trd>merry-go-round</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>caroussel</trd>,
      <trd>draaimolen</trd>,
      <trd>paardenmolen</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_u;&c_s;&c_je;&c_l;&c_mol;</trd>
  </snc>
</drv>
</art>
