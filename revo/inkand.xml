<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inkand.xml,v 1.24 2023/11/19 10:22:13 revo Exp $">
<kap>
  <rad>inkandesk</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="inkand.0a">
  <kap><tld/>a</kap>
  <snc mrk="inkand.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Blankarda:
      <ekz>
        <tld/>a korpo, lumo;
      </ekz>
      <ekz>
        <ind><tld/>a lampo</ind>;
        <trd lng="de">Gl&uuml;hlampe</trd>
      </ekz>
      <ekz>
        <tld/>aj ampoloj donas agrablan lumon, sed bezonas multe da elektro kaj
        produktas nenecesan varmon
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011486.php">Neleda LEDo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        majstreca kaj kelkfoje <tld/>a prozo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009181.php">Nekonata genio</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_y; &c_d;&c_a;&c_b;&c_ja;&c_l;&c_a;</trd>
  <trd lng="bg">&c_n;&c_a;&c_k;&c_a;&c_l;&c_je;&c_n; &c_d;&c_o; &c_b;&c_ja;&c_l;&c_o;</trd>
  <trd lng="cs">roz&zcaron;haven&yacute;</trd>
  <trdgrp lng="de">
    <trd>wei&szlig;gl&uuml;hend</trd>
  </trdgrp>
  <trd lng="en">incandescent</trd>
  <trd lng="es">incandescente</trd>
  <trd lng="fr">incandescent</trd>
  <trd lng="hu">izz&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x767D;&#x71B1;&#x306E;  <pr>&#x306F;&#x304F;&#x306D;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x767D;&#x71B1;&#x3057;&#x305F;  <pr>&#x306F;&#x304F;&#x306D;&#x3064;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">witgloeiend</trd>
  <trd lng="pt">incandescente</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_d;&c_o;&c_b;&c_je;&c_l;&c_a; <klr>(&c_k;&c_a;&c_k; &c_n;&c_i;&c_t;&c_mol; &c_v; &c_l;&c_a;&c_m;&c_p;&c_je;
  &c_n;&c_a;&c_k;&c_a;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>
  <trd lng="sk">roz&zcaron;eraven&yacute; do biela</trd>
  <trd lng="uk">&c_r;&#x43E;&c_z;&#x43F;&c_je;&c_ch;&c_je;&#x43D;&c_i;&c_j; &c_d;&#x43E; &c_b;&c_ib;&#x43B;&#x43E;&c_g;&#x43E;</trd>
  <trd lng="zh">&#x6781;&#x4EAE; <pr>j&#xED;li&#xE0;ng</pr></trd>
</drv>

<drv mrk="inkand.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="inkand.0i.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Blanke <ref tip="super" cel="ard.0i.brulegi">ardi</ref>:
      <ekz>
        la astronomoj esprimis surprizon, ke tiuj substancoj
        ne <tld/>is pro la rapida eniro en la atmosferon de
        la Tero
        <fnt>
          <aut>B. Golden</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/golden_kosma.html">
          Kosma Ruba&jcirc;o</url></vrk>
          <lok>Monda Karuselo, 1977:10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a; &c_d;&c_a;&c_b;&c_ja;&c_l;&c_a;</trd>
  <trd lng="cs">&zcaron;hnout</trd>
  <trdgrp lng="de">
    <trd>wei&szlig;gl&uuml;hen</trd>,
    <trd>wei&szlig;gl&uuml;hend werden</trd>
  </trdgrp>
  <trd lng="es">incandecer</trd>
  <trd lng="fr">&ecirc;tre <ind>incandescent</ind></trd>
  <trdgrp lng="hu">
    <trd>izzik</trd>,
    <trd>izz&aacute;sban van</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x767D;&#x71B1;&#x3059;&#x308B;  <pr>&#x306F;&#x304F;&#x306D;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x767D;&#x71B1;&#x5149;&#x3092;&#x767A;&#x3059;&#x308B; 
    <pr>&#x306F;&#x304F;&#x306D;&#x3064;&#x3072;&#x304B;&#x308A;&#x3092;&#x306F;&#x3063;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">witgloeiend worden</trd>
  <trd lng="pt">estar <ind>incandescente</ind><klr tip="amb"> (f&iacute;sica,
  tecnologia)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_d;&c_o;&c_b;&c_je;&c_l;&c_a;</trd>
  <trd lng="sk">roz&zcaron;eravi&tcaron; sa <klr>(do biela)</klr></trd>
  <trd lng="uk">&c_b;&c_u;&c_t;&c_i; &c_r;&#x43E;&c_z;&#x43F;&c_je;&c_ch;&c_je;&#x43D;&c_i;&#x43C;</trd>
</drv>

<drv mrk="inkand.0o">
  <kap><tld/>o</kap>
  <snc mrk="inkand.0o.FIZ">
    <dif>
      Blankardo:
      <ekz>
        la masojn ili dra&scirc;as al fina <tld/>o, kaj el mallum' ekbrilas la miriadaj steloj
        <fnt>
          <vrk><url ref="http://www.esperanto.mv.ru/Kolekto/Kosmo.html">La kosmo -
          Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la materio, kiu kolapsas sur la proto-stelon <klr>[&#x2026;]</klr> plivarmigas &gcirc;in &gcirc;is
        <tld/>o
        <fnt>
          <vrk><url ref="&Viki;/Proto-stelo">Proto-stelo - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_l;&c_y; <ind>&c_n;&c_a;&c_p;&c_a;&c_l;</ind></trd>,
    <trd>&c_b;&c_je;&c_l;&c_a;&c_je; <ind>&c_n;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="ca">incandesc&egrave;ncia</trd>
  <trdgrp lng="de">
    <trd>Wei&szlig;glut</trd>
  </trdgrp>
  <trd lng="es">incandescencia</trd>
  <trd lng="fr">incandescence</trd>
  <trd lng="hu">izz&aacute;s</trd>
  <trd lng="nl">gloed</trd>
  <trd lng="pt">incandesc&ecirc;ncia</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_k;&c_a;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_o;&c_je; <ind>&c_k;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </trdgrp>
</drv>

