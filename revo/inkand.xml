<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inkandesk</rad>/i
</kap>
<drv mrk="inkand.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Varmi&gcirc;i &gcirc;is blankarda stato. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>wei&szlig;gl&uuml;hen</trd>,
     <trd>wei&szlig;gl&uuml;hend werden</trd>
  </trdgrp>
  <trd lng="es">incandecer</trd>
  <trd lng="nl">witgloeiend worden</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_d;&c_o;&c_b;&c_je;&c_l;&c_a;</trd>
</drv>
<drv mrk="inkand.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Varmi&gcirc;inta &gcirc;is blankarda stato, estanta en blankarda stato:
      <ekz><tld/>a korpo, lampo, lumo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>wei&szlig;gl&uuml;hend</trd>
  </trdgrp>
  <trd lng="es">incandecente</trd>
  <trd lng="nl">witgloeiend</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_d;&c_o;&c_b;&c_je;&c_l;&c_a; <klr>(&c_k;&c_a;&c_k; &c_n;&c_i;&c_t;&c_mol; &c_v; &c_l;&c_a;&c_m;&c_p;&c_je; &c_n;&c_a;&c_k;&c_a;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>
</drv>
<drv mrk="inkand.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fenomeno <tld/>i. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Wei&szlig;glut</trd>
  </trdgrp>
  <trd lng="es">incandecencia</trd>
<trd lng="nl">gloed</trd>
</drv>
<drv mrk="inkand.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari <tld/>a.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>wei&szlig;gl&uuml;hen</trd>,
     <trd>zur <ind>Wei&szlig;glut</ind> bringen</trd>
  </trdgrp>
<trd lng="nl">witgloeiend maken</trd>
</drv>
</art>
