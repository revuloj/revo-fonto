<vortaro>
  <art mrk="$Id: sansalvador.xml,v 1.3 2022/03/26 20:43:10 afido Exp $">
    <kap>
      <rad>Sansalvador</rad>/o, 
      <var><kap><rad var="-">San-Salvador</rad>/o</kap></var>
    </kap>

<drv mrk="sansalvador.0o">
  <kap><tld/>o, <var><kap><tld var="-"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="sansalvador.0o.urbo">
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="salvador.S0o">Salvadoro</ref>:
      <ekz>
        la malmola sono de la sonoriloj de <tld/>o
        <fnt>
          <vrk><url ref="https://issuu.com/gabrielbriasbuendia/docs/profesoro_schleiden_kaj_la_luno._-e">Profesoro Schleiden kaj la luno esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la urbo <tld var="-"/>o situas &ccirc;e la piedo de la samnoma vulkano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=41090">San-Salvadoro</url></lok>
        </fnt>.
      </ekz>      
    </dif>
    <trd lng="de">San Salvador</trd>
    <trd lng="en">San Salvador</trd>
    <trd lng="fr">San Salvador</trd>
  </snc>
  <trd lng="uk">&#x421;&#x430;&#x43D;-&#x421;&#x430;&#x43B;&#x44C;&#x432;&#x430;&#x434;&#x43E;&#x440; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
</drv>

