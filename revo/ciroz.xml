<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ciroz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="ciroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="ciroz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kronika inflamo de la interlobeta histo de organo:
      <ekz>
        hepata, atrofia, vejna, disvasti&gcirc;anta <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_o;&c_z;</trd>
  <trd lng="bg">&c_c;&c_i;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="br">kaletadur<klr tip="ind"> (an avu)</klr></trd>
  <trd lng="de">Zirrhose</trd>
  <trd lng="el">&kappa;&jota_ton;&rho;&rho;&omega;&sigma;&eta;</trd>
  <trd lng="en">cirrhosis</trd>
  <trd lng="es">cirrosis</trd>
  <trd lng="fr">cirrhose</trd>
  <trd lng="hu">cirr&oacute;sis</trd>
  <trd lng="it">cirrosi</trd>
  <trd lng="la">cirrhosis</trd>
  <trd lng="pl">marsko&sacute;&cacute;</trd>
  <trd lng="pt">cirrose</trd>
  <trd lng="ru">&c_c;&c_i;&c_r;&c_r;&c_o;&c_z;</trd>
</drv>
</art>
