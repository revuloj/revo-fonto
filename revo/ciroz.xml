  <trd lng="ru">&c_c;&c_i;&c_r;&c_r;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>cirh&oacute;za</trd>,
    <trd>cir&oacute;za</trd>
  </trdgrp>
  <trd lng="uk">&#x446;&#x438;&#x440;&#x43E;&#x437;</trd>
  <trd lng="zh">&#x809D;&#x786C;&#x5316; <pr>g&#x101;ny&#xEC;nghu&#xE0;</pr></trd>
