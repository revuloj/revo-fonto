<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fojer.xml,v 1.9 2023/11/09 20:48:33 revo Exp $">
<kap>
    <rad>fojer</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fojer.0o">
  <kap><tld/>o</kap>
  <snc mrk="fojer.0o.SNC">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="salon.0o">Salono</ref> de
      <ref tip="malprt" cel="teatr.0o.ejo">teatro<sncref/></ref>, 
      <ref tip="malprt" cel="koncer1.0ejo">koncertejo</ref> ktp, 
      kie kunvenas la spektantoj dum la pa&ubreve;zoj:
      <ekz>
        la ekspozicio okazis en la Kultur-Centro de <nom>Dar&lstroke;owo</nom> en la fojero de kinejo &leftquot;<nom>Bajka</nom>&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=195951">Dar&lstroke;owo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Foyer</trd>,
      <trd>Lobby</trd>,
      <trd>Empfangshalle</trd>,
      <trd>Eingangshalle</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5165;&#x53E3;&#x5927;&#x5385; <pr>r&#xF9;k&#x1D2;ud&#xE0;t&#x12B;ng</pr></trd>,
    <trd>&#x9580;&#x5EF3; <pr>m&#xE9;nt&#x12B;ng</pr></trd>,
    <trd>&#x5927;&#x5802; <pr>d&#xE0;t&#xE1;ng</pr></trd>,
    <trd>&#x5267;&#x9662;&#x4F11;&#x606F;&#x5BA4; <pr>j&#xF9;yu&#xE0;nxi&#x16B;x&#x12B;sh&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ca">
    <trd>vest&iacute;bul</trd>,
    <trd>sala d'entreactes</trd>
  </trdgrp>
  <trd lng="cs">foyer</trd>
  <trd lng="el">&phi;&omikron;&ypsilon;&alfa;&gamma;&jota;&epsilon_ton;</trd>
  <trd lng="en">foyer</trd>
  <trd lng="fr">foyer</trd>
  <trd lng="nl">foyer</trd>
  <trd lng="pl">foyer</trd>
  <trd lng="no">foaj&eacute;</trd>
  <trd lng="sk">foyer</trd>
  <trd lng="uk">&#x444;&#x43E;&#x439;&#x454;</trd>
</drv>
</art>

