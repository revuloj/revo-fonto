<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>kameli</rad>/o
</kap>
<drv mrk="kameli.0o">
  <kap><tld/>o</kap>
  <snc mrk="kameli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="te.0acoj">teacoj</ref>
      (<trd lng="la">Camellia</trd>).
    </dif>
    <ref tip="sub" cel="te.0arbo">tearbo</ref>
  </snc>
  <snc mrk="kameli.0o.specio">
    <dif>
      Japana <tld/>o (<trd lng="la">Camellia japonica</trd>), 
      kun tre bela rozsimila floro.
    </dif>
  </snc>
</drv>
</art>
