<vortaro>
<art mrk="$Id: kameli.xml,v 1.24 2023/12/02 21:20:05 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kameli</rad>/o
</kap>
<drv mrk="kameli.0o">
  <kap><tld/>o</kap>
  <snc mrk="kameli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="teac.0oj">teacoj</ref>
      (<trd lng="la">Camellia</trd>):
      <ekz>
        kamelio <klr>[&#x2026;]</klr>: per la origina nomo <nom>Carl von
        Linn&eacute;</nom>
        honoras pastron <nom>Georg Joseph Kamel</nom>, kiu portis
        <nom>C. japonica</nom>
        de Manilo al Hispanio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=551171">Kamelio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="te.0arbo">tearbo</ref>
    <trd lng="hu">kam&eacute;lia</trd>
    <trd lng="pl">kamelia</trd>
  </snc>
  <snc mrk="kameli.0o.specio">
    <dif>
      Japana <tld/>o (<trd lng="la">Camellia japonica</trd>), 
      kun tre bela rozosimila floro.
    </dif>
    <bld lok="&WCU;/thumb/0/07/Kamelie_in_Pillnitz_16.JPG/640px-Kamelie_in_Pillnitz_16.JPG" prm="CC0">Japana kamelio, kastelo Pillnitz, Dresdeno
         <fnt>
           <aut>Br&uuml;cke-Osteuropa</aut>
           <lok><url ref="&WCW;/File:Kamelie_in_Pillnitz_16.JPG">&WCW;/File:Kamelie_in_Pillnitz_16.JPG</url></lok>
         </fnt>
    </bld>
    <trd lng="hu">jap&aacute;n kam&eacute;lia</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="ca">cam&egrave;lia</trd>
  <trd lng="cs">kam&eacute;lie</trd>
  <trd lng="de">Kamelie</trd>
  <trd lng="en">camellia</trd>
  <trd lng="fr">cam&eacute;lia</trd>
  <trd lng="ja">&#x30C4;&#x30D0;&#x30AD;</trd>
  <trd lng="nl">camelia</trd>
  <trd lng="pl">kamelia japo&nacute;ska</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">kam&eacute;lia</trd>
  <trd lng="uk">&#x447;&#x430;&#x439; &#x43A;&#x438;&#x442;&#x430;&#x439;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C71;&#x8336; <pr>sh&#x101;nch&#xE1;</pr></trd>,
    <trd>&#x5C71;&#x8336;&#x82B1; <pr>sh&#x101;nch&#xE1;hu&#x101;</pr></trd>,
    <trd>&#x8336;&#x82B1; <pr>ch&#xE1;hu&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
