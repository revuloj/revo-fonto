<drv mrk="kial.0o">
  <kap><tld/>o</kap>
  <snc mrk="kial.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="kauxz.0o">Ka&ubreve;zo</ref>:
      <ekz>
        jen la <tld/>o de tiu decido;
      </ekz>
      <ekz>
        en &ccirc;i tiu <tld/>o ku&scirc;as &ccirc;iuj aliaj;
      </ekz>
      <ekz>
        doni al iu evit<tld/>ojn;
      </ekz>
      <ekz>
        akvo minacas fari&gcirc;i <tld/>o de konfliktoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006866.html">La blua &leftquot;oro&rightquot; &ccirc;iam malsufi&ccirc;os</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        foje iuj malabonas sen indiki <tld/>on a&ubreve; motivon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/006825.html">http://steloj.de/esperanto/monato2/006825.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        lo&gcirc;antoj de Sarajevo havas multajn bonajn <tld/>ojn por interesi&gcirc;i pri
        mondpolitiko nun
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9310-sarajevo.txt">La fino de la mondo komenci&gcirc;as en Sarajevo</url></vrk>
          <lok>93/10</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_a;</trd>
    <trd lng="de">Grund</trd>
    <trdgrp lng="fr">
      <trd>raison</trd>,
      <trd>pourquoi<klr tip="ind"> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>causa</trd>,
      <trd>motivo</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&samekh;&yod;&bet;&he;</trd>,
      <trd>&ayin;&yod;&lamed;&he;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ok</trd>,
      <trd>(a) <ind>mi&eacute;rt</ind></trd>
    </trdgrp>
    <trd lng="id"><ind>alas</ind>an</trd>
    <trd lng="nl">reden</trd>
    <trdgrp lng="pl">
      <trd>pow&oacute;d</trd>,
      <trd>motyw</trd>,
      <trd>przyczyna</trd>
    </trdgrp>
     <trdgrp lng="pt">
      <trd>causa</trd>,
      <trd>motivo</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>tem&abreve;</trd>,
      <trd>cauza</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>
    <trdgrp lng="sv">
      <trd>sk&auml;l</trd>,
      <trd>orsak</trd>
    </trdgrp>
  </snc>
  <trd lng="af">rede</trd>
  <trd lng="am">&#x121D;&#x12AD;&#x1295;&#x12EB;&#x1275;</trd>
