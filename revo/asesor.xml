<kap>
  <ofc>1</ofc>
  <rad>asesor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="asesor.0o">
  <kap><tld/>o</kap>
  <snc mrk="asesor.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="help.0anto.KOMUNE">Helpanto</ref>
      de ju&gcirc;isto en kunsidoj:
      <ekz>
        gubernatoroj <klr>...</klr> manke de prop<klr>r</klr>a klero,
        helpas sin per <tld/>oj
        <fnt>
          <bib>DonKihxoto</bib>,
          <lok>dua parto, &ccirc;apitro 32a, p. 579a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="jxuri.0o.JUR">&jcirc;uriano</ref>
  <trd lng="be">&c_a;&c_s;&c_e;&c_s;&c_a;&c_r;</trd>
  <trd lng="de">Assessor</trd>
  <trdgrp lng="en">
    <trd>assessor</trd>,
    <trd>advisor</trd>
  </trdgrp>
  <trd lng="es">asesor&iacute;a</trd>
  <trd lng="fr">assesseur</trd>
  <trd lng="it">assessore<klr> (st. e dir.)</klr></trd>
  <trdgrp lng="pl">
    <trd>asesor</trd>,
    <trd>&lstroke;awnik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_je;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;sediaci</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>asesor</trd>,
    <trd>soudn&#xED; p&#x159;&#xED;sed&#xED;c&#xED;</trd>
  </trdgrp>
  <trd lng="he">&#x5D7;&#x5D1;&#x5E8; &#x5D1;&#x5D9;&#x5EA; &#x5D3;&#x5D9;&#x5DF;</trd>
  <trdgrp lng="ja">
    <trd>&#x966A;&#x5E2D;&#x5224;&#x4E8B;  [<ind>&#x966A;&#x5E2D;&#x306F;&#x3093;&#x3058;</ind>]</trd>,
    <trd>&#x966A;&#x5E2D;&#x88C1;&#x5224;&#x5B98;  [<ind>&#x966A;&#x5E2D;&#x3055;&#x3044;&#x3070;&#x3093;&#x304B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">pr&#xED;sediaci na s&#xFA;de</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x441;&#x435;&#x441;&#x43E;&#x440;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x456;&#x434;&#x430;&#x442;&#x435;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>
