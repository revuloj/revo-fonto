<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buked.xml,v 1.25 2023/10/29 19:11:04 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>buked</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>
<drv mrk="buked.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="buked.0o.fasko">
    <dif>
      <ref tip="super" cel="lig.kun0ajxo.KOMUNE">Kunliga&jcirc;o</ref> da detran&ccirc;itaj floroj
      ks:
      <ekz>
        dumvoje &scirc;i ligis <tld/>on el kampaj floroj
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o da diantoj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;e]</klr> la supro, kie kreskis <tld/>o de grandaj folioj kaj daktiloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> montris al li tutan <tld/>on, kiu tute forvelkis
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tie]</klr> ku&scirc;is dek unu blankaj plumoj de cignoj, &scirc;i kolektis ilin kaj
        faris el ili <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mezen en la <tld/>on oni enmetadis la kandelon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur amba&ubreve; bordoj de la rivero sin etendis <klr>[&#x2026;]</klr> palmaj <tld/>oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de fajra&jcirc;o
        <fnt>
          <bib>ChM</bib>
          <lok>3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fask.0o.KOMUNE">fasko</ref>,
      <ref cel="tuf.0o">tufo</ref>
    </refgrp>
    <trd lng="hu">vir&aacute;gcsokor</trd>
    <trd lng="pl">bukiet</trd>
    <trdgrp lng="zh">
    <trd>&#x65BD;&#x7279;&#x52B3;&#x65AF; <pr>sh&#x12B;t&#xE8;l&#xE1;os&#x12B;</pr></trd>,
    <trd>&#x4E89;&#x6267; <pr>zh&#x113;ngzh&#xED;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="buked.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aro da bela&jcirc;oj:
      <ekz>
        <tld/>o da belaj virinoj, da agrablaj versoj;
      </ekz>
      <ekz>
        vera <tld/>o de hispanaj, francaj, italaj, israelaj kaj libanaj kantoj
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li liveris tutan <tld/>on da kialoj por suspekti&gcirc;o
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni diras ke <ind>la <tld/>o de la vino</ind> <klr>(odoro)</klr>de 
        <nom>Mile&scirc;tij Mi&ccirc;j</nom> rakontas pri la
        popolaj trajtoj de moldavianoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Mile&scirc;tj Mi&ccirc;j &ndash; La plej granda subtera urbo en la mondo</lok>
        </fnt>;
        <trd lng="de">Bouquet</trd>
      </ekz>
      <ekz>
        &ccirc;u vi komplezos a&ubreve;ditorii <tld/>on el miaj kompona&jcirc;oj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>?
      </ekz>
      <ekz>
        jen eta <tld/>o el la mesa&gcirc;oj, kiuj eltenas tradukon Esperanten
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/007869.html">Vo&ccirc;o de vo&ccirc;donantoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">csokor</trd>
    <trdgrp lng="pl">
      <trd>bukiet</trd>,
      <trd>kwiat</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
  <trdgrp lng="br">   
      <trd>boked</trd>,
      <trd>bokedad</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>kytice</trd>,
    <trd>puget</trd>
  </trdgrp>
  <trd lng="de">Strau&szlig;</trd>
  <trd lng="en">bouquet</trd>
  <trd lng="es">ramo</trd>
  <trd lng="fr">bouquet</trd>
  <trd lng="he">&zayin;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#x82B1;&#x675F;  <pr>&#x306F;&#x306A;&#x305F;&#x3070;</pr></trd>,
    <trd>&#x4E00;&#x675F;  <pr>&#x3044;&#x3063;&#x305D;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pt">buqu&ecirc;</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
  <trd lng="sk">kytica</trd>
  <trd lng="uk">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
</drv>
</art>
