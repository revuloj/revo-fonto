<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>*</ofc>
    <rad>buked</rad>/o
  </kap>
  <drv mrk="buked.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc num="1">
      <dif>
        Kunliga&jcirc;o da detran&ccirc;itaj floroj:
        <ekz><tld/>o da, el siringofloroj.
        </ekz>
      </dif>
      <trd lng="hu">vir&aacute;gcsokor</trd>
      <trd lng="pl">bukiet</trd>
    </snc>
    <snc num="2">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Aro da bela&jcirc;oj:
        <ekz><tld/>o da belaj virinoj, da agrablaj versoj.
        </ekz>
      </dif>
      <trd lng="hu">csokor</trd>
      <trdgrp lng="pl">
        <trd>bukiet</trd>,
        <trd>kwiat</trd>
      </trdgrp>
    </snc>
    <trd lng="be">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="br">   
      <trd>boked</trd>,
      <trd>bokedad</trd>
    </trdgrp>
    <trd lng="en">bouquet</trd>
    <trd lng="es">ramo</trd>
    <trd lng="fr">bouquet</trd>
    <trd lng="ru">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
    <trd lng="sk">kytica</trd>
  </drv>
</art>
