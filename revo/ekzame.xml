<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ekzamen</rad>/i
</kap>
<drv mrk="ekzame.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzame.0i.esplori">
    <dif>
      Zorgeme rigardi, detale esplori:
      <ekz>
        <tld/>i demandon, problemon,
        aferon;
      </ekz>
      <ekz>
        <tld/>i ne devigas preni<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>untersuchen</trd>,
      <trd>pr&uuml;fen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vizsg&aacute;l</trd>,
      <trd>megvizsg&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">unders&ouml;ka</trd>
  </snc>
  <snc mrk="ekzame.0i.PED">
  <uzo tip="fak">PED</uzo>
    <dif>
      Provi per demandoj la sciojn de kandidato:
      <ekz>
        la kandidato estis <tld/>ita pri la &hcirc;emio.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>pr&uuml;fen</trd>,
      <trd>examinieren</trd>
    </trdgrp>
    <trd lng="hu">vizsg&aacute;ztat</trd>
    <trd lng="ru">&c_e;&c_k;&c_z;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">examinera</trd>
  </snc>
  <trd lng="en">examine</trd>
  <trd lng="es">examinar</trd>
  <trd lng="fr">examiner<klr tip="ind"> (un probl&egrave;me, un
  candidat)</klr></trd>
</drv>

<drv mrk="ekzame.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzame.0o.esploro">
    <dif>
      Detala esploro.
    </dif>
    <trdgrp lng="de">
      <trd>Untersuchung</trd>,
      <trd>Pr&uuml;fung</trd>
    </trdgrp>
    <trd lng="hu">vizsg&aacute;lat</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="sv">unders&ouml;kning</trd>
  </snc>
  <snc mrk="ekzame.0o.PED">
  <uzo tip="fak">PED</uzo>
    <dif>
      Kontrolo de la scioj de kandidato:
      <ekz>
        trapasi, elteni <tld/>on;
      </ekz>
      <ekz>
        fari sian <tld/>(i&gcirc;)on<sup>Z</sup>;
      </ekz>
      <ekz>
        sukcese trapasi, plenumi <tld/>on;
      </ekz>
      <ekz>
        sukcesi <tld/>on;
      </ekz>
      <ekz>
        sin prezenti al <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pr&uuml;fung</trd>,
      <trd>Examen</trd>
    </trdgrp>
    <trd lng="hu">vizsga</trd>
    <trd lng="ru">&c_e;&c_k;&c_z;&c_a;&c_m;&c_je;&c_n;</trd>
    <trdgrp lng="sv">
      <trd>prov</trd>,
      <trd>tentamen</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">zkou&scaron;ka</trd>
  <trd lng="es">ex&aacute;men</trd>
  <trd lng="fr">examen</trd>
</drv>
</art>
