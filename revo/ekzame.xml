<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ekzamen</rad>/i
</kap>
<drv mrk="ekzame.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Zorgeme rigardi, detale esplori:
      <ekz>
        <tld/>i demandon, problemon,
        aferon;
      </ekz>
      <ekz>
        <tld/>i ne devigas preni
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="de">
      <trd>untersuchen</trd>,
      <trd>pr&uuml;fen</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Provi per demandoj la sciojn de kandidato:
      <ekz>
        la kandidato estis <tld/>ita pri la &hcirc;emio.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>pr&uuml;fen</trd>,
      <trd>examenieren</trd>
    </trdgrp>
  </snc>
  <trd lng="en">examine</trd>
</drv>
<drv mrk="ekzame.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Detala esploro. 
    </dif>
    <trdgrp lng="de">
      <trd>Untersuchung</trd>,
      <trd>Pr&uuml;fung</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Kontrolo de la scioj de kandidato:
      <ekz>
        trapasi, elteni <tld/>on;
      </ekz>
      <ekz>
        fari sian <tld/>(i&gcirc;)on<sup>Z</sup>; 
      </ekz>
      <ekz>
        sukcese trapasi, plenumi <tld/>on;
      </ekz>
      <ekz>
        sukcesi <tld/>on;
      </ekz>
      <ekz>
        sin prezenti al <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pr&uuml;fung</trd>,
      <trd>Examen</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">zkou&scaron;ka</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>