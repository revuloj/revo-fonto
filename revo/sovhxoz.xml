<art mrk="$Id: sovhxoz.xml,v 1.7 2023/10/04 22:27:23 revo Exp $">
<kap>
  <rad>sov&hcirc;oz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sovhxoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="sovhxoz.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Granda &scirc;tatbieno, kiaj ekzistis en Sovetio:
      <ekz>
        mia rusa korespondanto Stepanov <klr>...</klr> laboris kiel
        in&gcirc;eniero en ma&scirc;in-traktora stacio de <tld/>o
        <fnt>
          <aut>Kurisu K.</aut>,
          <vrk>Debuti</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1998a, numero 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kolhxo.0o">kol&hcirc;ozo</ref>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_w;&c_g;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Sowchos</trd>,
    <trd>Sowchose</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_w;&a_w;&a_kh;&a_w;&a_z;</trd>
  <trd lng="fr">sovkhoze</trd>
  <trd lng="pl">sowchoz</trd>
  <trd lng="uk">&c_r;&c_a;&c_d;&c_g;&#x43E;&c_s;&#x43F;</trd>
</drv>
</art>

