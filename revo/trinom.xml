<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trinom.xml,v 1.12 2023/09/04 19:01:54 revo Exp $">
<kap>
    <rad>trinom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trinom.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="super" cel="polino.0o">Polinomo</ref> kun tri
      <ref cel="term.0o.polinomo">termoj</ref>;
      <klr>alidire, en elementa algebro:</klr>
      algebra <ref cel="esprim.0o.KOMP">esprimo</ref>, konsistanta 
      el tri <ref cel="term.0o.sumo">termoj</ref>
      kunligitaj per <ref cel="adici.0o">adicio</ref> a&ubreve; 
      <ref cel="subtra.0o">subtraho</ref>:
      <ekz>
        <tld/>a formulo;
      </ekz>
      <ekz>
        la kvadrato de la <tld/>o <frm am="a+b+c"><k>a</k>&#x202F;+&#x202F;<k>b</k>&#x202F;+&#x202F;<k>c</k></frm> estas
        <frm am="a^2+b^2+c^2+2ab+2ac+2bc"><k>a</k><sup>2</sup>&#x202F;+&#x202F;<k>b</k><sup>2</sup>&#x202F;+
<k>c</k><sup>2</sup>
       &#x202F;+&#x202F;<k>2ab</k>&#x202F;+&#x202F;<k>2ac</k>&#x202F;+&#x202F;<k>2bc</k></frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="term.n0o">n-termo</ref>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_o;&c_h;&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  <trd lng="ca">trinomi</trd>
  <trd lng="cs">troj&ccaron;len</trd>
  <trd lng="de">Trinom</trd>
  <trd lng="en">trinomial</trd>
  <trd lng="es">trinomio</trd>
  <trd lng="fr">trin&ocirc;me</trd>
  <trd lng="it">trinomio</trd>
  <trd lng="ja">&#x4E09;&#x9805;&#x5F0F;  [<ind>&#x307F;&#x3063;&#x3053;&#x3046;&#x3057;&#x304D;</ind>]</trd>
  <trd lng="sk">troj&ccaron;len</trd>
  <trd lng="uk">&c_t;&c_r;&c_i;&c_ch;&#x43B;&c_je;&#x43D;</trd>
</drv>
</art>

