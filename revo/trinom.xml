<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>trinom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trinom.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="super" cel="polino.0o">Polinomo</ref> kun tri
      <ref cel="term.0o.polinomo">termoj</ref>;
      <klr>alidire, en elementa algebro:</klr>
      algebra <ref cel="esprim.0o.KOMP">esprimo</ref>, konsistanta 
      el tri <ref cel="term.0o.sumo">termoj</ref>
      kunligitaj per <ref cel="adici.0o">adicio</ref> a&ubreve; 
      <ref cel="subtra.0o">subtraho</ref>.
    </dif>
    <ref tip="vid" cel="term.n0o">n-termo.</ref>
  </snc>
  <trd lng="de">Trinom</trd>
  <trd lng="en">trinomial</trd>
  <trd lng="it">trinomio</trd>
</drv>
</art>

