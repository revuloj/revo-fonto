<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>ripro&ccirc;</rad>/i</kap>
<drv mrk="riproc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi al iu malla&ubreve;don a&ubreve; malkontenton a&ubreve; akuzon pri ties
      ago a&ubreve; konduto ktp:
      <ekz>
        <tld/>i iun<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i ion al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne <tld/>u vian amikon, &ccirc;ar vi mem plimulte meritas
        <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mensogema lango malamas siajn <tld/>antojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>as lin pro lia kulpo, ke li ne restis korpe fidela al &scirc;i
        <fnt><aut>Donald Broadribb</aut>: <vrk>La Purgatorio vidita de
        dektriajarcentulo (Dante Alighieri, Purgatorio)</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 27a</lok></fnt>;
      </ekz>
      <ekz>
        Donald Broadribb <klr>...</klr> <tld/>as la tradukinton de tiu
        verko pri misuzo de la difina artikolo
        <fnt><aut>Albert Cnudde</aut>: <vrk>&leftquot;La&rightquot;, artikolo nur
        difina</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 7a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admon.0i">admoni</ref>,
      <ref cel="akuz.0i">akuzi</ref>,
      <ref cel="aprob">malaprobi</ref>,
      <ref cel="skold.0i">skoldi</ref>,
      <ref cel="tan.0i">tani</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_u;&c_p;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_a;&c_p;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_a;&c_s;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorwerfen</trd>,
    <trd><klr>einen </klr>Vorwurf machen</trd>,
    <trd>tadeln</trd>
  </trdgrp>
  <trd lng="en">reproach</trd>
  <trd lng="es">reprochar</trd>
  <trdgrp lng="fr">
    <trd>bl&acirc;mer</trd>,
    <trd>disputer</trd>,
    <trd>gronder</trd>,
    <trd>reprocher</trd>,
    <trd>r&eacute;primander</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kifog&aacute;sol</trd>,
    <trd>szem&eacute;re h&aacute;ny</trd>,
    <trd>el&iacute;t&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwijten</trd>,
    <trd>berispen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>censurar</trd>,
    <trd>repreender</trd>,
    <trd>incriminar</trd>,    
    <trd>ralhar</trd>,    
    <trd>acoimar</trd>,    
    <trd>exprobar</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_je;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_o;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="vo">leblam&ouml;n</trd>
</drv>

<drv mrk="riproc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malla&ubreve;da parolo al iu pro faro a&ubreve; sinteno lia:
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        pli efikas <tld/>o &ccirc;e sa&gcirc;ulo, ol cent batoj &ccirc;e
        malsa&gcirc;ulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pli bona estas <tld/>o neka&scirc;ita, ol amo ka&scirc;ita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari <tld/>ojn al iu<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_k;&c_o;&c_r;</trd>, 
    <trd>&c_p;&c_a;&c_p;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorwurf</trd>,
    <trd>Tadel</trd>
  </trdgrp>
  <trd lng="es">reproche</trd>
  <trdgrp lng="fr">
    <trd>bl&acirc;me</trd>,
    <trd>remontrance</trd>,
    <trd>reproche</trd>,
    <trd>r&eacute;primande</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szemreh&aacute;ny&aacute;s</trd>,
    <trd>kifog&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">verwijt</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_u;&c_k;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="vo">leblam</trd>
</drv>
</art>
