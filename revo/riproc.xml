  <trd lng="bo">&t_ga;&t_sha;&t_drengbu;&t_tsheg;&t_ga;&t_sha;&t_drengbu;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="de">
