<drv mrk="riproc.0a">
  <kap><tld/>a</kap>
  <snc mrk="riproc.0a.KOMUNE">
    <dif>
      Esprimanta malkontenton:
      <ekz>
        foriru kaj ne persekutu la justulojn! <klr>[&#8230;]</klr> diris li pli la&ubreve;te per
        <tld/>a tono
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>a murmureto <klr>[&scirc;i]</klr> demandis, je kio li ridas
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &jcirc;etis al mi <tld/>an rigardon
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_k;&c_o;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_s;&c_u;&c_d;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>tadelnd</trd>,
    <trd>vorwurfsvoll</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>berang</trd>,
    <trd><baz>cela</baz>mencela<klr tip="amb"> (adj)</klr></trd>,
    <trd>gusar</trd>,
    <trd>marah</trd>,
    <trd>geram (1)</trd>
  </trdgrp>
  <trd lng="ja">&#12392;&#x304C;&#12417;&#x308B;&#12424;&#12358;&#x306A;</trd>
  <trdgrp lng="pl">
    <trd>gani&aogonek;cy</trd>,
