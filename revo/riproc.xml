<drv mrk="riproc.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="riproc.0inda.KOMUNE">
    <dif>
      Kiu meritas <tld/>on:
      <ekz>
        ci tre malvirte agis, farinte tiel <tld/>indan agon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        Herbeno vidis en sia konduto nenion <tld/>indan
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        malica homo, portanta <tld/>indajn malvirtojn
        <fnt>
          <aut>A. Nurak</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr15/spiritismo.html">
          Brazilo kaj Spiritismo</url></vrk>,
          <lok>Nica literatura revuo, 1956-05 &gcirc;is 06 (1/5), p.
          189-192</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxd.mal0inda">malla&ubreve;dinda</ref>,
      <ref cel="mis.0a">misa</ref>,
      <ref cel="ind.mal0a">malinda</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_u;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_a;&c_m;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_je;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verwerflich</trd>,
    <trd>tadelnswert</trd>
  </trdgrp>
  <trd lng="es">reprochable</trd>
