<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>ekspres</rad>/a
</kap>
<drv mrk="ekspre.0a">
<kap><tld/>a</kap>
<snc num="1">
<uzo tip="fak">FER</uzo>
<dif>
<klr>(p.p. vagonaro)</klr>
Tre rapida. 
</dif>
<trd lng="fr">express<klr tip="ind"> (adj.)</klr></trd>
<trdgrp lng="ru">
  <trd>&c_s;&c_k;&c_o;&c_r;&c_y;&c_j;</trd>,
  <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
</trdgrp>
</snc>
<snc num="2">
<dif>
<klr>(p.p. kuriero, letero, komunika&jcirc;o)</klr>
Sendita por speciala celo. 
</dif>
<trd lng="fr">expr&egrave;s<klr tip="ind"> (lettre, porteur,...)</klr></trd>
<trdgrp lng="ru">
  <trd>&c_s;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
</trdgrp>
</snc>
</drv>
<drv mrk="ekspre.0o">
<kap><tld/>o</kap>
<snc num="1">
<uzo tip="fak">FER</uzo>
<dif>
Rapidira vagonaro, kiu haltas nur &ccirc;e &ccirc;efaj stacioj. 
</dif>
<trd lng="fr">express<klr tip="ind"> (subst.)</klr></trd>
<trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_s;</trd>
</snc>
<snc num="2">
<dif>
Speciala kuriero: <ekz>sendi leteron per <tld/>o<fnt>B</fnt>.</ekz>  
</dif>
<trd lng="fr">porteur sp&eacute;cial</trd>
</snc>
</drv>
</art>
