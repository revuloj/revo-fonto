<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekspre.xml,v 1.28 2023/11/07 19:22:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ekspres</rad>/o
</kap>

<drv mrk="ekspre.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekspre.0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Speciala <ref tip="super" cel="kurier.0o.POSX">kuriero</ref>:
      <ekz>
        jam mi estis vokinta la kurieron, por forsendi la leteron
        per
        <tld/>o
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno
        8a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_y;
      <ind>&c_k;&c_u;&c_r;'&c_je;&c_r;</ind></trd>
    <trdgrp lng="de">
      <trd>Eilbote</trd>,
      <trd>Eilkurier</trd>,
      <trd>Expresskurier</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>express courier</trd>,
      <trd>express messenger</trd>,
      <trd>special delivery</trd>
    </trdgrp>
    <trd lng="fr">porteur sp&eacute;cial</trd>
    <trd lng="hu">gyorsfut&aacute;r</trd>
    <trdgrp lng="pl">
      <trd>ekspres</trd>,
      <trd>priorytet</trd>
    </trdgrp>
    <trd lng="pt">expresso</trd>
    <trd lng="ru">&c_n;&c_a;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;<klr>
(&c_s;&c_u;&c_shch;.)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x9001;&#x5FEB;&#x4FE1;&#x7684;&#x4EBA; <pr>s&#xF2;ngku&#xE0;ix&#xEC;nder&#xE9;n</pr></trd>,
    <trd>&#x4F20;&#x9012;&#x6025;&#x4FE1;&#x7684;&#x4FE1;&#x5DEE; <pr>chu&#xE1;nd&#xEC;j&#xED;x&#xEC;ndex&#xEC;nch&#x101;i</pr></trd>,
    <trd>&#x5831;&#x4FE1;&#x8005; <pr>b&#xE0;ox&#xEC;nzh&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="ekspre.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Rapidira <ref tip="super" cel="trajn.0o.FER">trajno</ref>, kiu haltas nur &ccirc;e &ccirc;efaj stacioj:
      <ekz>
        <klr>[ili]</klr> veturis per la frumatena <tld/>o, kredante, ke la aliaj iros
        Berlinon kelkajn horojn poste
        <fnt>
          <bib>TAA</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum neniu anta&ubreve;a jaro kirlo&scirc;tormoj estis tiel fortaj, &gcirc;is 280&#x202F;km/h rapidegaj,
        nur iom malpli ol japanaj <tld/>trajnoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008822.php">Du mil kvin</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_r;&c_e;&c_s;
      <klr tip="amb">(&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_k;)</klr></trd>
    <trdgrp lng="de">
      <trd>Eilzug</trd>,
      <trd>D-Zug</trd>,
      <trd>Schnellzug</trd>,
      <trd>Fernschnellzug</trd>
    </trdgrp>
    <trd lng="en">express train</trd>
    <trd lng="fr">express<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">expressz<klr tip="ind"> (vonat)</klr></trd>
    <trd lng="pl">ekspres</trd>
    <trd lng="pt">expresso</trd>
    <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_s;</trd>
    <trdgrp lng="zh">
    <trd>&#x76F4;&#x901A;&#x5217;&#x8F66; <pr>zh&#xED;d&#xE1;li&#xE8;ch&#x113;</pr></trd>,
    <trd>&#x7279;&#x5FEB;&#x5217;&#x8ECA; <pr>t&#xE8;ku&#xE0;ili&#xE8;ch&#x113;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ca">expr&eacute;s<klr> (subst.)</klr></trd>
  <trd lng="cs">expres <klr>(rychl&iacute;k| rychl&yacute; posel| sp&ecaron;&scaron;n&aacute; z&aacute;silka)</klr></trd>
  <trd lng="he">&alef;&#x5B6;&qof;&#x5B0;&samekh;&#x5B0;&pe;&#x5BC;&resh;&#x5B6;&samekh;</trd>
  <trdgrp lng="ja">
    <trd>&#x6025;&#x884C;&#x5217;&#x8ECA;  <pr>&#x304D;&#x3085;&#x3046;&#x3053;&#x3046;&#x308C;&#x3063;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x901F;&#x9054;  <pr>&#x305D;&#x304F;&#x305F;&#x3064;</pr></trd>,
    <trd>&#x6025;&#x4F7F;  <pr>&#x304D;&#x3085;&#x3046;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>expres <klr>(vlak)</klr></trd>,
    <trd>po&scaron;tov&aacute; z&aacute;sielka</trd>,
    <trd>r&yacute;chly posol</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43A;&c_s;&#x43F;&c_r;&c_je;&c_s;</trd>,
    <trd>&c_sh;&c_v;&c_i;&c_d;&#x43A;&c_i;&c_j; &#x43F;&#x43E;&c_ji;&c_z;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="ekspre.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekspre.0a.rapidega">
    <dif>
      Tre <ref tip="super" cel="rapid.0a.progresa">rapida</ref>, 
      <ref cel="prioritat.0a">prioritate</ref> traktata:
      <ekz>
        tie staris la <tld/>a trajno al Parizo
        <fnt><bib>Mortul&Scirc;ip</bib>,
          <lok>unua libro, &ccirc;apitro 9a</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> mendis <tld/>kafon, tuj pagitan
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni simple kun&jcirc;etis la dosierojn de iuj hazarde alliveritaj disketoj kaj kvaza&ubreve;
        <tld/>-presis la tutan mikspoton
        <fnt><aut>Pejno Simono</aut>,
        <vrk><url ref="&Monato;/2000/006942.php">Porke la kesto sciu pri digipetroj kaj
        sizopoj!</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/09, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[al li]</klr> mi permesis <tld/>an lecionon pri forgesi&gcirc;o
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="urgx.0a">ur&gcirc;a</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_h;&c_u;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_k;&c_u;&c_r;'&c_je;&c_r;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schnell-</trd>,
      <trd>Eil-</trd>,
      <trd>D-</trd>
    </trdgrp>
    <trd lng="fr">express<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="pl">ekspresowy</trd>
    <trd lng="pt">expresso</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_o;&c_r;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#x5F88;&#x5FEB; <pr>h&#x11B;nku&#xE0;i</pr></trd>
</snc>
