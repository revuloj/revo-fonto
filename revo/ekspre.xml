  <trd lng="ca">expr&eacute;s<klr> (adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>expresn&iacute;</trd>,
    <trd>sp&ecaron;&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">express</trd>
  <trdgrp lng="hu">
    <trd>expressz-</trd>,
    <trd>gyors-</trd>
  </trdgrp>
  <trd lng="sk">expresn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x440;&#x43C;&#x456;&#x43D;&#x43E;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x448;&#x432;&#x438;&#x434;&#x43A;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

