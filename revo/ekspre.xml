<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ekspres</rad>/a
</kap>
<drv mrk="ekspre.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekspre.0a.FER">
    <uzo tip="fak">FER</uzo>
    <uzo tip="klr">(pri vagonaro)</uzo>
    <dif>
      Tre rapida:
      <ekz>
        tie staris la <tld/>a trajno al Parizo
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro 
9a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_h;&c_u;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_k;&c_u;&c_r;'&c_je;&c_r;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schnell-</trd>,
      <trd>Eil-</trd>,
      <trd>D-</trd>
    </trdgrp>
    <trd lng="fr">express<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="pt">expresso</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_o;&c_r;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ekspre.0a.POSX">
    <uzo tip="fak">POSX</uzo>
    <uzo tip="klr">(pri kuriero, letero, komunika&jcirc;o)</uzo>
    <dif>
      Sendita por speciala celo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_k;&c_u;&c_r;'&c_je;&c_r;&c_s;&c_k;&c_ib;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_e;&c_s;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Express-</trd>,
      <trd>Eil-</trd>
    </trdgrp>
    <trd lng="fr">expr&egrave;s<klr tip="ind"> (lettre, 
porteur,...)</klr></trd>
    <trd lng="pl">ekspresowy</trd>
    <trd lng="pt">expresso</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="hu">
    <trd>expressz-</trd>,
    <trd>gyors-</trd>
  </trdgrp>
</drv>

<drv mrk="ekspre.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekspre.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Rapidira vagonaro, kiu haltas nur &ccirc;e &ccirc;efaj stacioj.
    </dif>
    <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_r;&c_e;&c_s; <klr 
tip="amb">(&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_k;)</klr></trd>
    <trdgrp lng="de">
      <trd>Eilzug</trd>,
      <trd>D-Zug</trd>,
      <trd>Schnellzug</trd>,
      <trd>Fernschnellzug</trd>
    </trdgrp>
    <trd lng="fr">express<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">expressz<klr tip="ind"> (vonat)</klr></trd>
    <trd lng="pl">ekspres</trd>
    <trd lng="pt">expresso</trd>
    <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_s;</trd>
  </snc>
  <snc mrk="ekspre.0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Speciala kuriero:
      <ekz>
        jam mi estis vokinta la kurieron, por forsendi la leteron per
        <tld/>o
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 8a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_y; 
<ind>&c_k;&c_u;&c_r;'&c_je;&c_r;</ind></trd>
    <trdgrp lng="de">
      <trd>Eilbote</trd>,
      <trd>Eilkurier</trd>,
      <trd>Expresskurier</trd>
    </trdgrp>
    <trd lng="fr">porteur sp&eacute;cial</trd>
    <trd lng="hu">gyorsfut&aacute;r</trd>
    <trd lng="pl">ekspres</trd>
    <trd lng="pt">expresso</trd>
  </snc>
</drv>
</art>
