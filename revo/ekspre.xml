    <trd>sp&ecaron;&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">express</trd>
  <trdgrp lng="hu">
    <trd>expressz-</trd>,
    <trd>gyors-</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24613;&#34892;&#12398;  <pr>&#12365;&#12421;&#12358;&#12371;&#12358;&#12398;</pr></trd>,
    <trd>&#36895;&#36948;&#12398;  <pr>&#12381;&#12367;&#12383;&#12388;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="sk">expresn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&#1084;&c_ib;&#1085;&#1086;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_v;&c_i;&c_d;&#1082;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

