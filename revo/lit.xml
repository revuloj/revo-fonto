<drv mrk="lit.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="lit.0ajxo.MEBL">
    <dif>
      &Ccirc;io, kio servas por pretigi <tld/>on por ku&scirc;i&gcirc;o:
      <ekz>
        oni ja prenos vian <tld/>a&jcirc;on de sub vi
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 22:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin ka&scirc;i sub la <tld/>a&jcirc;on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li elprenis <tld/>otukon, ka&ubreve;&ccirc;ukan kusenon kaj ceterajn <tld/>a&jcirc;ojn el la mura
        &scirc;ranko
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  <ind>&c_b;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Bettzeug</trd>,
    <trd>Bettw&auml;sche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bed clothes</trd>,
    <trd>bed linens</trd>
  </trdgrp>
  <trd lng="es">ropa de cama</trd>
  <trd lng="fr">literie</trd>
  <trd lng="hu">&aacute;gynem&udblac;</trd>
  <trd lng="ja">&#23517;&#20855;  <pr>&#12375;&#12435;&#12368;</pr></trd>
  <trd lng="nl">beddegoed</trd>
  <trd lng="pl">pos&lstroke;anie</trd>
  <trd lng="pt">roupa de cama</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_je;
    &c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
