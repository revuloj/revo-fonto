<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lit.0o.MEBL">
    <dif>
      Meblo a&ubreve; kusenaro sur kiu oni ku&scirc;as por dormi, ripozi:
      <ekz>
        la muso kuris sub la <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        <ind>kiu amas la <tld/>on <klr>(ripozon)</klr>, ne akiros
        profiton</ind>
        <trd lng="fr">l'<ind>avenir</ind> appartient &agrave; ceux qui se l&egrave;vent
        t&ocirc;t</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_o;&c_zh;&c_a;&c_k;</trd>
  <trd lng="cs">postel</trd>
  <trdgrp lng="de">
    <trd>Bett</trd>,
    <trd>Schlafstatt</trd>
  </trdgrp>
  <trd lng="el">&kappa;&rho;&epsilon;&beta;(&beta;)&alfa_ton;&tau;&jota;</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>couch</trd>
  </trdgrp>
  <trd lng="es">cama</trd>
  <trd lng="fr">lit</trd>
  <trd lng="hu">&aacute;gy</trd>
  <trd lng="nl">bed</trd>
  <trd lng="pt">cama</trd>
  <trd lng="pl">&lstroke;&oacute;&zdot;ko</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">s&auml;ng</trd>
  <trd lng="vo">bed</trd>
</drv>

<drv mrk="lit.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="lit.0ajxo.MEBL">
    <dif>
      &Ccirc;io, kio servas por pretigi <tld/>on por ku&scirc;i&gcirc;o:
      <ekz>
        oni ja prenos vian <tld/>a&jcirc;on de sub vi<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bettzeug</trd>,
    <trd>Bettw&auml;sche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bed clothes</trd>,
    <trd>bed linens</trd>
  </trdgrp>
  <trd lng="fr">literie</trd>
  <trd lng="hu">&aacute;gynem&udblac;</trd>
  <trd lng="nl">beddegoed</trd>
  <trd lng="pt">roupa de cama</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_je; &c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lit.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lit.el0igxi.KOMUNE">
    <dif>
      Eli&gcirc;i el <tld/>o, ekzemple veki&gcirc;inte el dormo:
      <ekz>
        jam estas malfrue, mi do devas el<tld/>i&gcirc;i
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 101a, volumo 3a, p.
        310a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_z; &c_l;&c_o;&c_zh;&c_k;&c_u;)</klr></trd>
  <trd lng="fr">se <ind>lever</ind><klr> (sortir du lit)</klr></trd>
  <trd lng="hu">felkel az &aacute;gyb&oacute;l</trd>  
</drv>

<drv mrk="lit.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lit.en0igxi.KOMUNE">
    <dif>
      Ekku&scirc;i en <tld/>o por ripozi, dormi...:
      <ekz>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_ch;&c_y; <klr tip="amb">(&c_u; &c_l;&c_o;&c_zh;&c_a;&c_k;)</klr></trd>
  <trdgrp lng="fr">
    <trd>se <ind>coucher</ind><klr> (se mettre au lit)</klr></trd>,
    <trd>se <ind>mettre</ind> au lit</trd>
  </trdgrp>
  <trd lng="hu">&aacute;gyba fekszik</trd>  
</drv>

<drv mrk="lit.tabul0o">
  <kap>tabul<tld/>o</kap>
  <snc mrk="lit.tabul0o.MEBL">
    <dif>
      Malkomforta <tld/>o, kies suba&jcirc;o konsistas el nuraj tabuloj:
      <ekz>
        generalo Moncada e&ccirc; ne movi&gcirc;is de sur la tabul-<tld/>o
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>grabat</trd>,
    <trd>lit de planches</trd>
  </trdgrp>
</drv>

<drv mrk="lit.0kadro">
  <kap><tld/>kadro</kap>
  <snc>
    <dif>
      Muntkadro, kiu subportas la <tld/>a&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Bettgestell</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>bedstead</trd>
  </trdgrp>
  <trd lng="fr">cadre de lit</trd>
  <trd lng="hu">&aacute;gykeret</trd>
  <trd lng="nl">ledikant</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_o;&c_v; &c_k;&c_r;&c_o;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
