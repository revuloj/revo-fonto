<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="lit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lit.0o.MEBL">
    <dif>
      <ref tip="lst" lst="voko:mebloj" cel="mebl.0o">Meblo</ref> a&ubreve;
      kusenaro sur kiu oni ku&scirc;as por dormi,
      ripozi:
      <ekz>
        la muso kuris sub la <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        <ind>kiu amas la <tld/>on <klr>(ripozon)</klr>, ne akiros
        profiton</ind>
        <trd lng="fr">l'<ind>avenir</ind> appartient &agrave; ceux qui
        se
        l&egrave;vent
        t&ocirc;t</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_o;&c_zh;&c_a;&c_k;</trd>
  <trd lng="bg">&c_l;&c_je;&c_g;&c_l;&c_o;</trd>
  <trdgrp lng="cs">
    <trd>l&uring;&zcaron;ko</trd>,
    <trd>postel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bett</trd>,
    <trd>Schlafstatt</trd>
  </trdgrp>
  <trd
  lng="el">&kappa;&rho;&epsilon;&beta;(&beta;)&alfa_ton;&tau;&jota;</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>couch</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cama</trd>,
    <trd>lecho</trd>,
    <trd>piltra</trd>
  </trdgrp>
  <trd lng="fr">lit</trd>
  <trd lng="hu">&aacute;gy</trd>
  <trd lng="ku">niv&icirc;n</trd>
  <trd lng="nl">bed</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;&oacute;&zdot;ko</trd>,
    <trd>&lstroke;o&zdot;e</trd>
  </trdgrp>
  <trd lng="pt">cama</trd>
  <trd lng="ro">strat</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">poste&lcaron;</trd>
  <trd lng="sv">s&auml;ng</trd>
  <trd lng="vo">bed</trd>
</drv>

<drv mrk="lit.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="lit.0ajxo.MEBL">
    <dif>
      &Ccirc;io, kio servas por pretigi <tld/>on por ku&scirc;i&gcirc;o:
      <ekz>
        oni ja prenos vian <tld/>a&jcirc;on de sub vi
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 22:27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bettzeug</trd>,
    <trd>Bettw&auml;sche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bed clothes</trd>,
    <trd>bed linens</trd>
  </trdgrp>
  <trd lng="es">ropa de cama</trd>
  <trd lng="fr">literie</trd>
  <trd lng="hu">&aacute;gynem&udblac;</trd>
  <trd lng="nl">beddegoed</trd>
  <trd lng="pl">pos&lstroke;anie</trd>
  <trd lng="pt">roupa de cama</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_je;
    &c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lit.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lit.el0igxi.KOMUNE">
    <dif>
      Eli&gcirc;i el <tld/>o, ekzemple veki&gcirc;inte el dormo:
      <ekz>
        jam estas malfrue, mi do devas el<tld/>i&gcirc;i
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 101a, volumo 3a, p.
        310a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">get out of bed</trd>
  <trd lng="be">&c_u;&c_s;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_z;
  &c_l;&c_o;&c_zh;&c_k;&c_u;)</klr></trd>
  <trd lng="de">aufstehen <klr>(aus dem Bett)</klr></trd>
  <trd lng="es">levantarse de la cama</trd>
  <trd lng="fr">se <ind>lever</ind><klr> (sortir du lit)</klr></trd>
  <trd lng="nl">opstaan<klr tip="ind"> (uit bed)</klr></trd>
  <trd lng="hu">felkel az &aacute;gyb&oacute;l</trd>
  <trd lng="pl">wstawa&cacute; <klr>(z
  &lstroke;&oacute;&zdot;ka)</klr></trd>
</drv>

<drv mrk="lit.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lit.en0igxi.KOMUNE">
    <dif>
      Ekku&scirc;i en <tld/>o por ripozi, dormi...:
      <ekz>
        Johano frue en<tld/>i&gcirc;is, faris sian vesperan pre&gcirc;on kaj
        trankvile endormi&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fi&scirc;kaptistoj jam delonge en<tld/>i&gcirc;is, sed en tiu terura
        vetero oni ne povis e&ccirc; pensi pri fermado de okulo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tuj kiam <nom>Hjalmar</nom> en<tld/>i&gcirc;is, la dio de dormo per sia sor&ccirc;a
        &scirc;prucigilo aspergis &ccirc;iujn meblojn en la &ccirc;ambro
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">go to bed</trd>
  <trd lng="be">&c_l;&c_je;&c_g;&c_ch;&c_y; <klr tip="amb">(&c_u;
  &c_l;&c_o;&c_zh;&c_a;&c_k;)</klr></trd>
  <trd lng="de">zu <ind>Bett</ind> gehen</trd>
  <trdgrp lng="es">
    <trd>meterse en cama</trd>,
    <trd>irse a la cama</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>coucher</ind><klr> (se mettre au lit)</klr></trd>,
    <trd>se <ind>mettre</ind> au lit</trd>
  </trdgrp>
  <trd lng="hu">&aacute;gyba fekszik</trd>
  <trd lng="nl">naar <ind>bed</ind> gaan</trd>
  <trd lng="pl">k&lstroke;a&sacute;&cacute; si&eogonek; <klr>(do
  &lstroke;&oacute;&zdot;ka, spa&cacute;)</klr></trd>
</drv>

<drv mrk="lit.brik0o">
  <kap>brik<tld/>o</kap>
  <snc mrk="lit.brik0o.HEJM">
    <dif>
      Aran&gcirc;o de tradiciaj hejmoj en Norda &Ccirc;inujo,
      granda lito el brikoj a&ubreve; adoboj,
      supre kovrita de mato kaj malsupre hejtebla
      per fajrujo:
      <ekz>
        post la man&gcirc;o li ku&scirc;i&gcirc;is sur la brik<tld/>o, kun
        siaj brakoj sub la kapo
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="forn.0o.hejtilo">forno<sncref/></ref>,
    </refgrp>
  </snc>
</drv>

<drv mrk="lit.tabul0o">
  <kap>tabul<tld/>o</kap>
  <snc mrk="lit.tabul0o.MEBL">
    <dif>
      Malkomforta <tld/>o, kies suba&jcirc;o konsistas el nuraj tabuloj:
      <ekz>
        generalo <nom>Moncada</nom> e&ccirc; ne movi&gcirc;is de sur la
        tabul-<tld/>o
        <fnt>
        <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>
        <vrk>Cent Jaroj da Soleco</vrk>
        <lok>1992</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pry&ccaron;na</trd>,
    <trd>str&aacute;&zcaron;n&iacute; nebo v&ecaron;ze&ncaron;sk&eacute;
    l&uring;&zcaron;ko</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grabat</trd>,
    <trd>lit de planches</trd>
  </trdgrp>
  <trd lng="nl">lattenbodem</trd>
  <trdgrp lng="sk">
    <trd>jednoduch&eacute; l&ocirc;&zcaron;ko</trd>,
    <trd>pri&ccaron;&ncaron;a</trd>
  </trdgrp>
</drv>

<drv mrk="lit.0kadro">
  <kap><tld/>kadro</kap>
  <snc>
    <dif>
      Muntkadro, kiu subportas la <tld/>a&jcirc;on:
      <ekz>
        movanta-<tld/>kadro-teorio
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=328947'>Supersojla
          flua&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">r&aacute;m l&uring;&zcaron;ka</trd>
  <trd lng="de">Bettgestell</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>bedstead</trd>
  </trdgrp>
  <trd lng="fr">cadre de lit</trd>
  <trd lng="hu">&aacute;gykeret</trd>
  <trd lng="nl">ledikant</trd>
  <trd lng="pl">rama <ind>&lstroke;&oacute;&zdot;ka</ind></trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_o;&c_v;
  &c_k;&c_r;&c_o;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="sk">l&aacute;m postele</trd>
</drv>
</art>
