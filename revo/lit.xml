<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lit</rad>/o
</kap>
<drv mrk="lit.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Meblo sur kiu oni ku&scirc;as por dormi, ripozi:
      <ekz>kiu amas la <tld/>on (ripozon), ne akiros profiton
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="cs">postel</trd>
  <trdgrp lng="de">
     <trd>Bett</trd>,
     <trd>Schlafstatt</trd>
  </trdgrp>
  <trd lng="el">&kappa;&rho;&epsilon;&beta;(&beta;)&alfa_ton;&tau;&jota;</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>couch</trd>
  </trdgrp>
  <trd lng="es">cama</trd>
  <trd lng="fr">lit</trd>
  <trd lng="hu">&aacute;gy</trd>
  <trd lng="nl">bed</trd>
  <trd lng="pt">cama</trd>
  <trd lng="pl">&lstroke;&oacute;&zdot;ko</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">s&auml;ng</trd>
  <trd lng="vo">bed</trd>
</drv>

<drv mrk="lit.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      &Ccirc;io, kio servas por pretigi <tld/>on por ku&scirc;i&gcirc;o:
      <ekz>
        oni ja prenos vian <tld/>a&jcirc;on de sub vi<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Bettzeug</trd>,
     <trd>Bettw&auml;sche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bed clothes</trd>,
    <trd>bed linens</trd>
  </trdgrp>
  <trd lng="hu">&aacute;gynem&udblac;</trd>
  <trd lng="nl">beddegoed</trd>
  <trd lng="pt">roupa de cama</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_je; &c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lit.0kadro">
  <kap>
    <tld/>kadro
  </kap>
  <snc>
    <dif>
      Muntkadro, kiu subportas la <tld/>a&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Bettgestell</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>bedstead</trd>
  </trdgrp>
  <trd lng="hu">&aacute;gykeret</trd>
  <trd lng="nl">ledikant</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_o;&c_v; &c_k;&c_r;&c_o;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
