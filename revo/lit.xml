<drv mrk="lit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lit.0o.MEBL">
    <dif>
      <ref tip="lst" lst="voko:mebloj" cel="mebl.0o">Meblo</ref> a&ubreve;
      kusenaro sur kiu oni ku&scirc;as por dormi, ripozi:
      <ekz>
        ku&scirc;i en <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la muso kuris sub la <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        la kapa parto de la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        pup<tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        balanceblaj infan<tld/>oj
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#8470; 4 (114)</lok>
        </fnt>;
      </ekz>
      <ekz>
        nokte li dormis en sia labor&ccirc;ambro sur <ind>ka&gcirc;o<tld/>o</ind>, kiun oni refaldis tage
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
        <trd lng="de">Gitterbett</trd>
      </ekz>
      <ekz>
        mia patro <klr>[&#8230;]</klr> jam ku&scirc;is en la <ind>&scirc;rank<tld/>o</ind>
        <klr>(vd <ref cel="alkov.0o">alkovo</ref>)</klr>
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
        <trd lng="de">Schrankbett</trd>
      </ekz>
      <ekz>
        li ku&scirc;as apud &scirc;i en la <ind>geedza <tld/>o</ind>
        <fnt>
          <bib>KrB</bib>
          <lok>Pli da bruo, ol da faro</lok>
        </fnt>;
        <trd lng="de">Ehebett</trd>
      </ekz>
      <ekz>
        &ccirc;u la juna sinjoro jam mo&scirc;tis forlasi sian <ind>baldaken<tld/>o</ind>n
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>?
        <trd lng="de">Himmelbett</trd>
      </ekz>
      <ekz>
        la mezon de la <klr>[&#8230;]</klr> &ccirc;ambro okupis ortegita kanapo<tld/>o, surhavanta
        molajn kusenojn kaj kovrilon el cervaj feloj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        en angulo de la stalo ku&scirc;is la <ind>pajlo<tld/>o</ind>, sur kiu la maristo ripozis
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
        <trd lng="de">Strohsack</trd>
      </ekz>
      <ekz>
        <klr>[mi]</klr> ekdormis kvaza&ubreve; la bran&ccirc;nesto estis lanug<tld/>o
        <fnt>
          <bib>Rob</bib>
          <lok>Parto II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la senarbejo estis duonronde borderita per komfortaj, sunbanaj <ind>&gcirc;arden<tld/>o</ind>j,
        sur kiuj kelkloke sidis a&ubreve; ku&scirc;is homoj
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>;
        <trd lng="de">Gartenliege</trd>
      </ekz>
      <ekz>
        ekstere du viroj nin atendis kun <ind>port<tld/>o</ind>
        <klr>(kp <ref cel="branka.0o">brankardo</ref>)</klr>
        kaj transportis al la <klr>[&#8230;]</klr> transportvagono por malsanuloj
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
        <trd lng="de">Trage</trd>
      </ekz>
      <ekz>
        mi ku&scirc;igis min sur la <ind>soldat<tld/>o</ind>n, kiu staris en la &ccirc;ambro, kaj observis la
        bluajn nubojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 14</lok>
        </fnt>;
        <trd lng="de">Feldbett</trd>
      </ekz>
      <ekz>
        estis kurtenitaj <ind>voja&gcirc;<tld/>o</ind>j, kiuj &scirc;anceli&gcirc;is super niaj kapoj
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
        <trd lng="de">Reisebett</trd>
      </ekz>
      <ekz>
        <ind>kiu amas la <tld/>on <klr>(ripozon)</klr>, ne akiros
        profiton</ind>
        <trd lng="fr">l'<ind>avenir</ind> appartient &agrave; ceux qui
        se l&egrave;vent t&ocirc;t</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="hamak.0o">hamako</ref>,
      <ref cel="lul.0ilo.MEBL">lulilo</ref>
    </refgrp>
  </snc>
  <trd lng="af">bed</trd>
