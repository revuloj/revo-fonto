<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pavez</rad>/o
</kap>
<drv mrk="pavez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Supra&jcirc;o de <ref tip="malprt" cel="sxip.0o">&scirc;ip</ref>rando:
      <ekz>
        transdono de la respondecoj <ctl>trans la pavezon de la
        &scirc;ipo</ctl><fnt><aut>-</aut>: <vrk>Inkotermoj 2000</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 3a, p. 29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_b;&c_o;&c_r;&c_t;</trd>
  <trd lng="de">Schanzkleid</trd>
  <trdgrp lng="en">
    <trd>bulwarks</trd>,
    <trd>topsides</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bastingage</trd>,
    <trd>pavois<klr> (bastingage)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mellv&eacute;d<klr tip="ind"> (haj&oacute;n)</klr></trd>,
    <trd>habv&eacute;d</trd>,
    <trd>fed&eacute;lzeti <ind>korl&aacute;t</ind></trd>
  </trdgrp>
  <trd lng="nl">verschansing<klr tip="ind"> (v.schip)</klr></trd>
  <trd lng="pt">pav&ecirc;s</trd>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_b;&c_o;&c_r;&c_t;</trd>
</drv>
</art>
