  <trd lng="ru">&c_o;&c_b;&c_zh;&c_je;&c_ch;&c_mol;</trd>
  <trd lng="sk">pop&aacute;li&tcaron;</trd>
  <trd lng="uk">&c_o;&c_b;&c_p;&c_je;&c_k;&c_t;&c_i; <klr>(&c_p;&c_a;&c_l;&c_je;&c_c;&c_mol; &c_ib; &c_t;.&c_p;.)</klr></trd>
</drv>

<drv mrk="vund.frosto0o">
  <kap>
    frosto<tld/>o,
    <var><kap>frost<tld/>o</kap></var>
  </kap>
  <snc mrk="vund.frosto0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Rigidi&gcirc;o pro frosti&gcirc;o de korpa loko, kiu blanki&gcirc;as, &scirc;velas, poste eventuale
      nigri&gcirc;as kaj defalas;
      pli grava ol <ref tip="vid" cel="sxvel.frosto0o.MED">frosto&scirc;velo</ref>:
      <ekz>
        la frosto<tld/>oj povas anka&ubreve; infekti&gcirc;i kaj tetanoso povas esti&gcirc;i
        <fnt>
          <aut>C. Bertin</aut>
          <vrk><url ref="https://www.bertin.biz/articles/Sukurado.pdf">Sukurado kaj Tujaj
          Flegoj</url></vrk>
          <lok>1989</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis savita, nur la piedfingroj de la maldekstra piedo frost<tld/>i&gcirc;is
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ustimenko</nom> gvidis lecionon kun flegistinoj kaj helpkuracistoj de la plifortiga
        grupo &mdash; rakontis al ili pri kuracado de frost<tld/>oj
        <fnt>
          <aut>J. German, trad. J. Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/German/kara-09-4.html">Kara mia
          homo</url></vrk>
          <lok>2016-2017</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_a;&c_r;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Erfrierung</trd>,
    <trd>Frostbeule</trd>
  </trdgrp>
  <trd lng="fr">gelure</trd>
  <trd lng="ja">&#x51CD;&#x50B7;  <pr>&#x3068;&#x3046;&#x3057;&#x3087;&#x3046;</pr></trd>
</drv>

<drv mrk="vund.grat0i">
