<drv mrk="vund.brul0i">
  <kap>brul<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vund.brul0i.MED">
    <dif>
      <tld lit="V"/>i per fajro a&ubreve; alia bruliga&jcirc;o:
      <ekz>
        la fajro &ccirc;iam ser&ccirc;is okazon brul<tld/>i al li la fingrojn
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Dua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        saltanta skorio lin brul<tld/>as
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgrandaj sableroj brul<tld/>is kvaza&ubreve; fajreroj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu &ccirc;i kiso brul<tld/>is Viniciuson kaj inspiris al li novan abomenon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="brog.0i.vundi">brogi<sncref/></ref>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_ja;&c_ch;&c_y; <klr>(&c_p;&c_a;&c_r;&c_a;&c_n;&c_ib;&c_c;&c_mol;)</klr></trd>
  <trdgrp lng="cs">
    <trd>pop&aacute;lit <klr>(zranit)</klr></trd>,
    <trd>sp&aacute;lit</trd>,
    <trd>zp&uring;sobit sp&aacute;leninu</trd>
  </trdgrp>
  <trd lng="de">verbrennen <klr>(Haut)</klr></trd>
  <trdgrp lng="en">
    <trd>to burn</trd>,
    <trd>scorch</trd>, <trd>singe</trd>
  </trdgrp>
  <trd lng="fr">bruler<klr tip="amb"> (blesser par brulure)</klr></trd>
  <trd lng="ja">&#x3084;&#x3051;&#x3069;&#x3055;&#x305B;&#x308B;</trd>
  <trd lng="nl">verbranden<klr tip="ind"> (verwonden door te
  branden)</klr></trd>
