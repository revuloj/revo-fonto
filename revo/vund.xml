  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_n;&c_je;&c_n;&c_a;&c_je; <ind>&c_m;&c_je;&c_s;&c_c;&c_a;</ind></trd>,
    <trd>&c_p;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_a;&c_je; <ind>&c_m;&c_je;&c_s;&c_c;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verletzung</trd>,
    <trd>Wundstelle</trd>
