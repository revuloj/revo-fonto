<kap>
<ofc>*</ofc>
<rad>fabl</rad>/o
</kap>
<drv mrk="fabl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
    Fantazia rakonto, ofte versa, kies agantoj estas plej ofte bestoj 
    a&ubreve; senvivaj objektoj, k pruvanta moralan instruon: 
    <ekz><tld/>oj de Ezopo, de La Fontaine.</ekz>
    </dif>
    <refgrp tip="sub">
    <ref cel="apolog.0o">apologo</ref>.
    </refgrp>
  </snc>
  <trd lng="ja">&#x5BD3;&#x8A71;  <pr>&#x3050;&#x3046;&#x308F;</pr></trd>
  <trd lng="lat">fabula, ae</trd>
  <trd lng="be">&c_b;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_s;&c_n;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>fablenn</trd>,
    <trd>mojenn<klr tip="ind"> (fablenn)</klr></trd>
  </trdgrp>
  <trd lng="ca">faula</trd>
  <trd lng="cs">bajka</trd>
  <trd lng="de">Fabel</trd>
  <trdgrp lng="en">
    <trd>fable</trd>,
    <trd>beast <ind>fable</ind></trd>
  </trdgrp>
  <trd lng="es">f&aacute;bula</trd>
  <trdgrp lng="fa">
    <trd>&a_q;&a_S;&a_h;</trd>,
    <trd>&a_H;&f_k;&a_A;&f_y;&a_t;</trd>,
    <trd>&a_A;&a_f;&a_s;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">fable<klr tip="ind"> (genre
  litt&eacute;raire)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;llatmese</trd>,
    <trd>fabula</trd>
  </trdgrp>
  <trd lng="id">fabel</trd>
  <trd lng="nl">fabel</trd>
  <trd lng="pl">bajka</trd>
  <trdgrp lng="pt">
    <trd>ap&oacute;logo</trd>,
    <trd>f&aacute;bula</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_s;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">b&#xE1;jka</trd>
  <trd lng="sv">fabel</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x439;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x442;&#x447;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A7A;&#x8AC7; <pr>k&#x14D;ngt&#xE1;n</pr></trd>,
    <trd>&#x8B0A;&#x8A71; <pr>hu&#x1CE;nghu&#xE0;</pr></trd>,
    <trd>&#x634F;&#x9020;&#x7684;&#x4E8B; <pr>ni&#x113;z&#xE0;odesh&#xEC;</pr></trd>,
    <trd>&#x5BD3;&#x8A00; <pr>y&#xF9;y&#xE1;n</pr></trd>
  </trdgrp>
</drv>
