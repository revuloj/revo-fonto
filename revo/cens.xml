<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cens</rad>/i
</kap>
<drv mrk="cens.0i">
  <kap><tld/>i</kap>
<uzo tip="fak">EKON</uzo>
<uzo tip="fak">POL</uzo>
  <snc>
    <dif>
Datenakiri pri hava&jcirc;oj &ccirc;efe por fiskaj celoj.
    </dif>
  </snc>
  </drv>

  <drv mrk="cens.0o">
  <kap><tld/>o</kap>
  <snc>
<uzo tip="fak">EKON</uzo>
    <dif>
Hava&jcirc;o submetebla al impostado.
    </dif>
  </snc>

  <snc>
  <uzo tip="stl">FIG</uzo>
    <dif>
Monnivelo.
    </dif>
  </snc>
  <trd lng="it">censo</trd>
  </drv>

  <drv mrk="cens.0ado">
  <kap><tld/>ado</kap>
  <snc>
<uzo tip="fak">EKON</uzo>
<uzo tip="fak">POL</uzo>
    <dif>
Nombrado kaj taksado de hava&jcirc;oj.
    </dif>
  </snc>
  <trd lng="it">censimento delle ricchezze</trd>
</drv>

<drv mrk="cens.0isto">
  <kap><tld/>isto</kap>
  <snc>
<uzo tip="fak">EKON</uzo>
<uzo tip="fak">POL</uzo>
    <dif>
Kiu faras censadon.
    </dif>
  </snc>
  <trd lng="it">censitore</trd>
</drv>
</art>

