<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: beletr1.xml,v 1.22 2022/03/12 20:16:57 revo Exp $">
<kap>
  <ofc>1</ofc><rad>beletristik</rad>/o 
</kap>
<drv mrk="beletr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="beletr1.0o.BELE">
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Esplorado kaj studo pri <ref cel="beletr.0o">beletro</ref>:
      <ekz>
        <nom>Won Kenn</nom> estis &ccirc;ina doktoro de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22598">Won Kenn</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
    <trd lng="ca">teoria de la literatura</trd>
    <trdgrp lng="de">
      <trd>Belletristik</trd>,
      <trd>Literaturwissenschaft</trd>
    </trdgrp>
    <trd lng="fr">belles-lettres (&eacute;tudes litt&eacute;raires)</trd>
    <trd lng="hu">irodalomtudom&aacute;ny</trd>
    <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="beletr1.0o.beletro">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      <ref tip="dif" cel="beletr.0o">Beletro</ref>:
      <ekz>
        artikoloj pri literaturaj kaj <tld/>aj demandoj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
    <trd lng="ca">literatura</trd>
    <trdgrp lng="de">
      <trd>Belletristik</trd>,
      <trd>sch&ouml;ne Literatur</trd>,
      <trd>sch&ouml;ngeistige Literatur</trd>
    </trdgrp>
    <trd lng="hu">sz&eacute;pirodalom</trd>
    <trd lng="it">lettere<klr> (studi letterari)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>,
      <trd>&c_b;&c_je;&c_l;&c_l;&c_je;&c_t;&c_r;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="en">belletristic literature</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>beletristika</trd>,
    <trd>nauka o beletrii</trd>
  </trdgrp>
  <trd lng="he">&#x5E1;&#x5E4;&#x5E8;&#x5D5;&#x5EA; &#x5D9;&#x5E4;&#x5D4;</trd>
  <trdgrp lng="sk">
    <trd>beletria</trd>,
    <trd>kr&aacute;sna literat&uacute;ra</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x435;&#x43B;&#x435;&#x442;&#x440;&#x438;&#x441;&#x442;&#x438;&#x43A;&#x430;</trd>
</drv>
</art>
