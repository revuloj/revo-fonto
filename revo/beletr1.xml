<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>beletristik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="beletr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="beletr1.0o.BELE">
    <fnt>PIV</fnt>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Esplorado kaj studo pri <ref cel="beletr.0o">beletro</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Belletristik</trd>,
      <trd>Literaturwissenschaft</trd>
    </trdgrp>
    <trd lng="fr">belles-lettres (&eacute;tudes litt&eacute;raires)</trd>
    <trd lng="hu">irodalomtudom&aacute;ny</trd>
    <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="beletr1.0o.beletro">
    <uzo tip="stl">ARK</uzo>
    <fnt><vrk>Plena Vortaro de Esperanto</vrk></fnt>
    <fnt><bib>VdE</bib>, <lok></lok></fnt>
    <ref tip="dif" cel="beletr.0o">beletro</ref>
    <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Belletristik</trd>,
      <trd>sch&ouml;ne Literatur</trd>,
      <trd>sch&ouml;ngeistige Literatur</trd>
    </trdgrp>
    <trd lng="hu">sz&eacute;pirodalom</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>,
      <trd>&c_b;&c_je;&c_l;&c_l;&c_je;&c_t;&c_r;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="en">belletristic literature</trd>
  </snc>
</drv>
</art>
