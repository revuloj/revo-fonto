<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: santome.xml,v 1.11 2019/02/09 13:10:22 revo Exp $">
<kap>
  <rad>Santome</rad>/o
</kap>

<drv mrk="santome.S0o">
  <kap>
    <tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc mrk="santome.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="vid" cel="atlant.A0o.GEOG">Atlantika</ref> 
      (Gvinea Golfo) <ref tip="lst" cel="insul.0o"
      lst="voko:insuloj">insulo</ref>, konsistiganta kun <ref tip="vid" 
      cel="principe.P0o.GEOG">Principeo</ref>
      &scirc;taton <ref tip="malprt"
      cel="santome.S0oPrincipeo.sxtato">Santomeo kaj Principeo</ref>,
      situo: 0&#730;14&minute;N, 6&#730;36&minute;E.
    </dif>
    <trd lng="bg">&c_S;&c_a;&c_o; &c_T;&c_o;&c_m;&c_je;</trd>
    <trd lng="ca">S&atilde;o Tom&eacute;</trd>
    <trd lng="de">S&atilde;o Tom&eacute;</trd>
    <trd lng="en">S&atilde;o Tom&eacute;</trd>
    <trd lng="fr">Sao Tom&eacute;</trd>
    <trd lng="id">Sao Tome</trd>
    <trd lng="it">S&atilde;o Tom&eacute;</trd>
    <trdgrp lng="pl">
      <trd>Wyspa &Sacute;wi&eogonek;tego Tomasza</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="santome.S0oPrincipeo">
  <kap>
    <tld/>o kaj Principeo
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">ST</mlg>
  <snc mrk="santome.S0oPrincipeo.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref> 
      en la <ref tip="vid" cel="atlant.A0o.GEOG">Atlantikaj</ref> 
      (Gvinea Golfo) insuloj <tld/>o kaj Principeo (kaj kelkaj aliaj)
      kun &ccirc;efurbo <tld/>o:
      <ekz>
        jen la landoj a&ubreve; regionoj, kie la portugala estas oficiala lingvo:
        <klr>[&#8230;]</klr> <tld/>o kaj Principeo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3372"
              >Portugala lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="bg">&c_S;&c_a;&c_o; &c_T;&c_o;&c_m;&c_je; &c_i; &c_P;&c_r;&c_i;&c_n;&c_s;&c_i;&c_p;&c_i;</trd>
    <trd lng="ca">S&atilde;o Tom&eacute; i Pr&iacute;ncipe</trd>
    <trd lng="de">S&atilde;o Tom&eacute; und Pr&iacute;ncipe</trd>
    <trd lng="en">S&atilde;o Tom&eacute; and Pr&iacute;ncipe</trd>
    <trd lng="fr">Sao Tom&eacute; et Principe</trd>
    <trd lng="id"><ind>Sao Tome</ind> dan Principe</trd>
    <trd lng="it">S&atilde;o Tom&eacute; e <ind>Principe</ind></trd>
    <trdgrp lng="pl">
      <trd>Wyspy &Sacute;wi&eogonek;tego Tomasza i
      Ksi&aogonek;&zdot;eca</trd>,
      <trd>Demokratyczna Republika <ind>Wysp &Sacute;wi&eogonek;tego
      Tomasza i 
      Ksi&aogonek;&zdot;&eogonek;cej</ind></trd>
    </trdgrp>
    <trd lng="pt">S&atilde;o Tom&eacute; e Pr&iacute;ncipe</trd>
</drv>

</art>

