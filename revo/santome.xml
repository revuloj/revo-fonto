<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>santome</rad>/o
</kap>

<drv mrk="santome.0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="santome.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="vid" cel="atlant.0o.GEOG">Atlantika</ref> 
      (Gvinea Golfo) insulo, konsitiganta kun <ref tip="vid" 
      cel="principe.0o.GEOG">Principeo</ref>
      &scirc;taton <ref tip="malprt"
      cel="santome.0oprincipeo.sxtato">Santomeo kaj Principeo</ref>.
      Situo: 0&ring;14&#8242;N, 6&ring;36&#8242;E.
    </dif>
    <trd lng="en">S&atilde;o Tom&eacute;</trd>
    <trdgrp lng="pl">
      <trd>Wyspa &Sacute;wi&eogonek;tego Tomasza</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="santome.0oprincipeo">
  <kap>
    <tld lit="S"/>o kaj Principeo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="santome.0oprincipeo.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la <ref tip="vid"
      cel="atlant.0o.GEOG">Atlantikaj</ref> 
      (Gvinea Golfo)
      insuloj Santomeo kaj Principeo (kaj kelkaj aliaj)
      kun &ccirc;efurbo Santomeo.
      ISO-3166 kodo: ST.
    </dif>
    <trd lng="en">S&atilde;o Tom&eacute; and Pr&iacute;ncipe</trd>
    <trdgrp lng="pl">
      <trd>Wyspy &Sacute;wi&eogonek;tego Tomasza i
      Ksi&aogonek;&zdot;eca</trd>,
      <trd>Demokratyczna Republika <ind>Wysp &Sacute;wi&eogonek;tego
      Tomasza i 
      Ksi&aogonek;&zdot;&eogonek;cej</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>