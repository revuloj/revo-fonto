<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>santome</rad>/o
</kap>

<drv mrk="santome.S0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="santome.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="vid" cel="atlant.A0o.GEOG">Atlantika</ref> 
      (Gvinea Golfo) insulo, konsitiganta kun <ref tip="vid" 
      cel="principe.P0o.GEOG">Principeo</ref>
      &scirc;taton <ref tip="malprt"
      cel="santome.S0oprincipeo.sxtato">Santomeo kaj Principeo</ref>.
      Situo: 0&amp;ring14&#8242;N, 6&amp;ring36&#8242;E.
    </dif>
    <trd lng="bg">&c_S;&c_a;&c_o; &c_T;&c_o;&c_m;&c_je;</trd>
    <trd lng="de">S&atilde;o Tom&eacute;</trd>
    <trd lng="en">S&atilde;o Tom&eacute;</trd>
    <trd lng="it">S&atilde;o Tom&eacute;</trd>
    <trdgrp lng="pl">
      <trd>Wyspa &Sacute;wi&eogonek;tego Tomasza</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="santome.S0oprincipeo">
  <kap>
    <tld lit="S"/>o kaj Principeo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="santome.S0oprincipeo.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la <ref tip="vid"
      cel="atlant.A0o.GEOG">Atlantikaj</ref> 
      (Gvinea Golfo)
      insuloj Santomeo kaj Principeo (kaj kelkaj aliaj)
      kun &ccirc;efurbo Santomeo.
      ISO-3166 kodo: ST.
    </dif>
    <trd lng="bg">&c_S;&c_a;&c_o; &c_T;&c_o;&c_m;&c_je; &c_i; &c_P;&c_r;&c_i;&c_n;&c_s;&c_i;&c_p;&c_i;</trd>
    <trd lng="de">S&atilde;o Tom&eacute; und Pr&iacute;ncipe</trd>
    <trd lng="en">S&atilde;o Tom&eacute; and Pr&iacute;ncipe</trd>
    <trd lng="it">S&atilde;o Tom&eacute; e <ind>Principe</ind></trd>
    <trdgrp lng="pl">
      <trd>Wyspy &Sacute;wi&eogonek;tego Tomasza i
      Ksi&aogonek;&zdot;eca</trd>,
      <trd>Demokratyczna Republika <ind>Wysp &Sacute;wi&eogonek;tego
      Tomasza i 
      Ksi&aogonek;&zdot;&eogonek;cej</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

