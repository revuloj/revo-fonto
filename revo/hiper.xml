<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hiper</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hiper.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="hiper.0.SCI">
    <ref tip="dif" cel="tro.0">tro-</ref>
    <subsnc mrk="hiper.0.ANA">
      <uzo tip="fak">ANA</uzo>
      <dif>
        Tro kreskinta, tro granda:
        <ekz>
          <tld/>lordozo
          <klr>(granda kava kurbeco de la spino)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="hiper.0.FIZL">
      <uzo tip="fak">FIZL</uzo>
      <dif>
        Tro aktiva, tro intensa:
        <ekz>
          <tld/>tensio
          <klr>(tro forta sangopremo)</klr>;
        </ekz>
        <ekz>
          <tld/>tiroidismo
          <klr>(troa funkciado de la tiroido)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="ant">
      <ref cel="hipo.0.SCI">hipo-</ref>
    </refgrp>
  </snc>
  <trd lng="fr">hyper-</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>