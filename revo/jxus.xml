<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jxus.xml,v 1.34 2023/12/03 07:09:20 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>&jcirc;us</rad>
  </kap>

<drv mrk="jxus.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc>
    <dif>
      Adverbo indikanta tre, a&ubreve; kiel eble plej, proksiman anta&ubreve;an
      tempon, &leftquot;<ref cel="moment.0o">momenton</ref> anta&ubreve;
      <ref cel="nun.0.cxitempe">nun<sncref/></ref>&rightquot; 
      a&ubreve; &leftquot;momenton <ref cel="antaux.0.tempe">anta&ubreve;</ref> tiam&rightquot;:
      <ekz>
        mi <tld/> alvenis;
      </ekz>
      <ekz>
        li <tld/> elrampis el la ova &scirc;elo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksentinte la odoron de akvo, &gcirc;i denove verdi&gcirc;as, kaj kreskas
        plue, kvaza&ubreve;
        <tld/> plantita
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 14:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre frue en la unua tago de la semajno ili venis al la tombo,
        kiam la suno
        <tld/> levi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Marko 16:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/> diris al vi, kaj vi ne atentis, kial vi volas denove
        &gcirc;in a&ubreve;di
        <fnt>
          <bib>NT</bib>
          <lok>Johano 9:27</lok>
        </fnt>?
      </ekz>
      <ekz>
        <esc>&jcirc;usnaskitaj</esc> su&ccirc;infanoj
        <fnt>
          <bib>NT</bib>
          <lok>I. Petro 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bonaj feinoj de la vivo &ccirc;iuj alportis al la
        <esc>&jcirc;usnaskito</esc> siajn donojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/> disvolvi&gcirc;intan lekanton ili tute ne rimarkis
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la horlo&gcirc;o sur la granda pre&gcirc;eja turo <tld/> batis la kvinan
        horon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabineto, tiel pura, kvaza&ubreve; &scirc;i <tld/> eliris el bano
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo forte ekmovi&gcirc;is, <tld/> &gcirc;i estis ankora&ubreve; klara kiel
        spegulo
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/> balais kaj metis &ccirc;ion en &gcirc;ustan lokon, kaj vi &ccirc;ion
        malordigas
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri oraj haroj de la diablo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kato en sia blanka kaj flava vesto <klr>[&#x2026;]</klr> <tld/>
        veki&gcirc;is kaj oscedis
        post la dol&ccirc;a dormo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/> atingis la edzi&gcirc;ota&ubreve;gan a&gcirc;on, kiam li okaze trafis en
        lavotan tola&jcirc;on
        kune kun &scirc;trumpa rubando
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxust.0e.precize">&gcirc;uste<sncref/></ref>,
      <ref cel="tuj.0.tempe">tuj<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_g;&c_a;&c_d;&c_z;&c_je;</trd>,
    <trd>&c_t;&c_o;&c_l;&c_mol;&c_k;&c_ib; &c_sh;&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>soeben</trd>,
    <trd>gerade <klr>(vor einem Moment)</klr></trd>,
    <trd>einen <ind>Augenblick</ind> zuvor</trd>,
    <trd>einen <ind>Moment</ind> zuvor</trd>
  </trdgrp>
  <trd lng="en">just (a moment ago)</trd>
  <trd lng="es">hace un momento</trd>
  <trdgrp lng="fi">
    <trd><klr tip="amb">(&auml;sken) </klr> juuri</trd>,
    <trd>&auml;sken</trd>
    <trd>vastik&auml;&auml;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>juste<klr tip="ind"> (il y a peu)</klr></trd>,
    <trd>il y a <ind>peu</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;ppen most</trd>,
    <trd>im&eacute;nt</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>barusan</trd>,
    <trd><ind>baru</ind> saja</trd>
  </trdgrp>
  <trd lng="ja">&#x305F;&#x3063;&#x305F;&#x4ECA;  <pr>&#x305F;&#x3063;&#x305F;&#x3044;&#x307E;</pr></trd>
  <trd lng="nl">net<klr tip="ind"> (onlangs)</klr></trd>
  <trd lng="no">nettopp</trd>
  <trd lng="pl">w&lstroke;a&sacute;nie</trd>
  <trdgrp lng="pt">
    <trd>h&aacute; pouco</trd>,
    <trd>agora mesmo</trd>,
    <trd>neste instante <klr tip="amb">(passado)</klr></trd>,
    <trd>de fresco</trd>
  </trdgrp>
  <trd lng="ro">tocmai</trd>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_mol;&c_k;&c_o; &c_ch;&c_t;&c_o;</trd>
  <trdgrp lng="it">
    <trd>adesso<klr> (appena)</klr></trd>,
    <trd>or <ind>ora</ind></trd>,
    <trd>giusto <ind>ora</ind></trd>,
    <trd>appena<klr tip="amb"> (passato)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&#x43E;&c_j;&#x43D;&#x43E;</trd>,
    <trd>&c_t;&c_ib;&#x43B;&#x44C;&#x43A;&c_i; &c_shch;&#x43E;</trd>,
    <trd>&c_t;&c_ib;&#x43B;&#x44C;&#x43A;&c_i;-&#x43D;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9002;&#x624D; <pr>sh&#xEC;c&#xE1;i</pr></trd>,
    <trd>&#x9002;&#x95F4; <pr>sh&#xEC;ji&#x101;n</pr></trd>,
    <trd>&#x521A;&#x521A; <pr>g&#x101;nggang</pr></trd>,
    <trd>&#x624D;&#x7136; <pr>c&#xE1;ir&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="jxus.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="&Jcirc;"/> okazinta:
      <ekz>
        la <tld/>a <klr>(<tld/> aperinta, plej laste
        aperinta)</klr> numero de gazeto;
      </ekz>
      <ekz>
        la miskompreniga el-mia-vidpunkta artikolo de <tld/>a a&ubreve;gusto
        provizore
        konsistigas la apena&ubreve; supereblan kulminon de la beda&ubreve;rinda
        tiurubrika degenero
        <fnt>
          <aut>Pejno Simono</aut>
            <vrk><url ref="&Monato;/2000/007143.php">&Ccirc;u ni mortu miskomprene? (2)</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 23-25</lok>
          </fnt>;
      </ekz>
      <ekz>
        tiu libero de parolo ne estas vere absoluta, kiel pruvas aliaj
        <tld/>aj nova&jcirc;oj,
        ekzemple la fermo de <klr>[&#x2026;]</klr> radiostacio <klr>[&#x2026;]</klr>
        konata pro
        politikaj opinioj kontra&ubreve;aj al <nom>Bush</nom> kaj al grandaj
        entreprenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Juan Jacobo Schmitter-Soto</aut>
          <vrk><url ref="&Monato;/2004/008257.html">Lando
          de la (mal)liberuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        teritorioj okupataj dum la <tld/>a konflikto
        <fnt>
          <bib>Monato</bib>
          <aut>Eskil Svane</aut>
          <vrk><url ref="&Monato;/2003/008197.html">Misinformaj
          komentoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fresx.0a">fre&scirc;a</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_d;&c_a;&c_w;&c_n;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>j&uuml;ngste <klr>(kurz zuvor)</klr></trd>,
    <trd>letzte <klr>(kurz zuvor)</klr></trd>
  </trdgrp>
  <trd lng="en">recent</trd>
  <trd lng="es">reciente</trd>
  <trdgrp lng="fi">
    <trd>&auml;skeinen</trd>,
    <trd>&auml;skett&auml;inen</trd>,
    <trd>tuore</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;cent</trd>
  <trd lng="hu">im&eacute;nti</trd>
  <trd lng="id"><ind>baru</ind>-baru ini</trd>
  <trd lng="it">recente</trd>
  <trdgrp lng="ja">
    <trd>&#x305F;&#x3063;&#x305F;&#x4ECA;&#x306E;  <pr>&#x305F;&#x3063;&#x305F;&#x3044;&#x307E;&#x306E;</pr></trd>,
    <trd>&#x76F4;&#x524D;&#x306E;  <pr>&#x3061;&#x3087;&#x304F;&#x305C;&#x3093;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>jongste</trd>,
    <trd>meest <ind>recente</ind></trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>nyeste</trd>,
    <trd>siste</trd>
  </trdgrp>
  <trd lng="pl">&sacute;wie&zdot;y<klr tip="ind"> (nowy)</klr></trd>
  <trd lng="ro">proasp&abreve;t<klr tip="ind"> (nou)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_je;&c_j;&c_sh;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_zh;&c_a;&c_j;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x43A;&c_i;&c_j; &c_shch;&#x43E;&c_j;&#x43D;&#x43E; &c_v;&c_ib;&c_d;&c_b;&c_u;&c_v;&c_s;&#x44F;/&c_t;&c_r;&c_a;&#x43F;&c_i;&c_v;&c_s;&#x44F;</trd>,
    <trd>&#x44F;&#x43A;&c_i;&c_j; &c_shch;&#x43E;&c_j;&#x43D;&#x43E; &#x43C;&c_a;&c_v; &#x43C;&c_ib;&c_s;&c_c;&c_je;</trd>
  </trdgrp>
  <trd lng="zh">&#x665A;&#x8FD1; <pr>w&#x1CE;nj&#xEC;n</pr></trd>
</drv>
