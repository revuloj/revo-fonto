<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>&jcirc;us</rad>
</kap>
<drv mrk="jxus.0">
    <kap><ofc>*</ofc><tld/></kap>
    <snc>
      <dif>
        Adverbo signifanta &leftquot;&gcirc;uste anta&ubreve; nun
a&ubreve; anta&ubreve;
        tiam&rightquot;:
        <ekz>
          mi <tld/> alvenas;
        </ekz>
        <ekz>
          <tld/> batis la tria horo, kiam ni eniris en la urbon;
        </ekz>
        <ekz>
          &gcirc;uste pri tio mi ja <tld/> parolis<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_g;&c_a;&c_d;&c_z;&c_je;</trd>,
      <trd>&c_t;&c_o;&c_l;&c_mol;&c_k;&c_ib; &c_sh;&c_t;&c_o;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>soeben</trd>,
      <trd>gerade</trd>
    </trdgrp>
    <trd lng="en">just (a moment ago)</trd>
    <trdgrp lng="fi">
      <trd><klr tip="amb">(&auml;sken) </klr> juuri</trd>,
      <trd>&auml;sken</trd>
      <trd>vastik&auml;&auml;n</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>juste<klr tip="ind"> (il y a peu)</klr></trd>,
      <trd>il y a <ind>peu</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;ppen most</trd>,
      <trd>im&eacute;nt</trd>
    </trdgrp>
    <trd lng="nl">net<klr tip="ind"> (onlangs)</klr></trd>
    <trd lng="pl">w&lstroke;a&sacute;nie</trd>
    <trdgrp lng="pt">
      <trd>h&#225; pouco</trd>,
      <trd>agora mesmo</trd>,
      <trd>neste instante <klr tip="amb">(passado)</klr></trd>,
      <trd>de fresco</trd>
    </trdgrp>
    <trd lng="ro">tocmai</trd>
    <trd lng="ru">&c_t;&c_o;&c_l;&c_mol;&c_k;&c_o; &c_ch;&c_t;&c_o;</trd>
</drv>

<drv mrk="jxus.0a">
    <kap><tld/>a</kap>
    <snc>
      <dif>
        <tld lit="&Jcirc;"/> okazinta:
        <ekz>
          plej <tld/>a <klr>(<tld/> aperinta)</klr> numero de
&jcirc;urnalo.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="fresx.0a">fre&scirc;a</ref>.
      </refgrp>
    </snc>
    <trd lng="be">&c_n;&c_ja;&c_d;&c_a;&c_w;&c_n;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>j&uuml;ngste</trd>,
      <trd>letzte</trd>
    </trdgrp>
    <trd lng="en">recent</trd>
    <trdgrp lng="fi">
      <trd>&auml;skeinen</trd>,
      <trd>t&auml;skett&auml;inen</trd>,
      <trd>tuore</trd>
    </trdgrp>
    <trd lng="fr">r&eacute;cent</trd>
    <trd lng="hu">im&eacute;nti</trd>
    <trdgrp lng="nl">
      <trd>jongste</trd>,
      <trd>meest <ind>recente</ind></trd>
    </trdgrp>
    <trd lng="pl">&sacute;wie&zdot;y<klr tip="ind"> (nowy)</klr></trd>
    <trd lng="ro">proasp&#x103;t<klr tip="ind"> (nou)</klr></trd>
</drv>
</art>
