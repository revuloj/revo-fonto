<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: etagx.xml,v 1.26 2021/05/29 14:32:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>eta&gcirc;</rad>/o
</kap>

<drv mrk="etagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="etagx.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Samnivela parto de <ref tip="malprt" cel="dom.0o.konstruajxo">domo</ref>:
      <ekz>
        ni levadis nin &ccirc;iam pli kaj pli alte &gcirc;is la kvara 
        <tld/>o
        <fnt>
        <aut>E. Weilsh&auml;user</aut>,
        <vrk>La Hejmo de la Metiisto</vrk>
        <lok>en: Fundamenta Krestomatio de la lingvo Esperanto, 1904</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="etagx.0o.RIM">
    En multaj e&ubreve;ropaj landoj la ter<tld/>o estas rigardata
    kiel nula, kaj oni nombras la suprajn <tld/>on unua,
    dua...  Al &ccirc;i tio konformas jena ekzemplo:
    <ekz>
      sur la mezo de la &scirc;tuparo &scirc;i renkontis la pordiston,
      kiu portis faskon da ligno al unu el la lo&gcirc;ejoj, kiuj 
      trovi&gcirc;is en la unua <tld/>o
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
    En aliaj landoj la ter<tld/>o estas kalkulata kiel unua <tld/>o.
  </rim>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_ja;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="bo">&#xF50;&#xF7C;&#xF42;&#xF0B;&#xF66;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Stockwerk</trd>,
    <trd>Stock</trd>,
    <trd>Geschoss</trd>,
    <trd>Etage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>floor</trd>,
    <trd>story <klr> (US)</klr></trd>,
    <trd>storey <klr> (BR)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>piso</trd>,
    <trd>planta<klr tip="amb"> (de edificio o casa)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;tage</trd>
  <trdgrp lng="hu">
    <trd>emelet</trd>,
    <trd>szint</trd>
  </trdgrp>
  <trd lng="nl">verdieping</trd>
  <trdgrp lng="pt">
    <trd>andar <klr tip="amb">(de edif&iacute;cio ou casa)</klr></trd>,
    <trd>pavimento</trd>,
    <trd>piso</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pi&eogonek;tro</trd>,
    <trd>kondygnacja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_ja;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="sv">v&aring;ning</trd>
</drv>

<drv mrk="etagx.0areto">
  <kap>
    <tld/>areto
    <fnt><bib>FK</bib></fnt>
  </kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="etagx.0areto.bretaro">
    <dif>
      <ref tip="dif" cel="etagxe.0o">Eta&gcirc;ero</ref>,
      <ref tip="dif" cel="bret.0aro">bretaro</ref>:
      <ekz>
        tie sin trovis anka&ubreve; multaj <tld/>aretoj, egale kovritaj per blua &scirc;tofo kaj
        ornamitaj per bluaj fran&gcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="etagx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="etagx.0igi.KOMUNE">
    <dif>
      Aran&gcirc;i la&ubreve; <tld/>oj:
      <ekz>
        urbo <tld/>igita sur la monteto.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">organizar por nivel de altura</trd>
  <trd lng="fr">&eacute;tager</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;pcs&odblac;z</trd>,
    <trd>szintek szerint <ind>rendez</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_ja;&c_r;&c_u;&c_s;&c_a;&c_m;&c_i;</trd>
</drv>

<drv mrk="etagx.bel0o">
  <kap>bel<tld/>o</kap>
  <snc mrk="etagx.bel0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pli luksa <tld/>o en domo, ofte la unua super la ter<tld/>o, 
      izolita de la strato kaj facile alirebla:
      <ekz>
        ha, kion mi babilas, &ndash; mi tute forgesis, ke mi
        lo&gcirc;as en la bel<tld/>o
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>erstes Obergeschoss</trd>,
    <trd>erste Etage</trd>,
    <trd>erster Stock [nordgermane]</trd>,
    <trd>zweiter Stock [sudgermane]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bel &eacute;tage</trd>,
    <trd>main storey</trd>
  </trdgrp>
  <trd lng="es">planta principal</trd>
  <trd lng="fr">bel &eacute;tage</trd>
  <trd lng="hu">els&odblac; <ind>emelet</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_v;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="etagx.inter0o">
  <kap>inter<tld/>o</kap>
  <snc mrk="etagx.inter0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="E"/>o inter ter<tld/>o kaj unua <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zwischengeschoss</trd>,
    <trd>Zwischenetage</trd>
  </trdgrp>
  <trd lng="en">mezzanine</trd>
  <trd lng="es">entreplanta</trd>
  <trd lng="fr">entresol</trd>
  <trd lng="he">&#x5E7;&#x5D5;&#x5B9;&#x5DE;&#x5B7;&#x5EA;
  &#x5D1;&#x5BC;&#x5B5;&#x5D9;&#x5E0;&#x5B7;&#x5D9;&#x5B4;&#x5DD;</trd>
  <trd lng="hu">f&eacute;lemelet</trd>
  <trd lng="nl">tussenverdieping</trd>
  <trd lng="pl">p&oacute;&lstroke;pi&eogonek;tro</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_e;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_r;&c_je;&c_s;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="etagx.subter0o">
  <kap>subter<tld/>o,
  <var><kap>kel<tld/>o</kap></var></kap>
  <snc mrk="etagx.subter0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="E"/>o sub la nivelo de la grundo:
      <ekz>
        en subter<tld/>o vivis kaj &ccirc;iutage atendadis &scirc;ian 
        revenon du malgrandaj infanoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Untergeschoss</trd>,
    <trd>Kellergeschoss</trd>,
    <trd>Kelleretage</trd>,
    <trd>Keller</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>basement</trd>,
    <trd>cellar</trd>
  </trdgrp>
  <trd lng="es">s&oacute;tano</trd>
  <trd lng="fr">sous-sol</trd>
  <trdgrp lng="hu">
    <trd>alagsor</trd>,
    <trd>szuter&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kelder</trd>,
    <trd>kelderverdieping</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>suterena</trd>,
    <trd>piwnica</trd>,
    <trd>przyziemie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="etagx.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="etagx.ter0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="E"/>o samnivela kun la &ccirc;irka&ubreve;a grundo:
      <ekz>
        planoj, montrantaj ter<tld/>e tri &ccirc;ambrojn kaj kuirejon, 
        kaj subtegmente du &ccirc;ambretojn
        <fnt><bib>Kiso</bib>, <lok>&Aring;ge</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_y; &c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</trd>
  <trdgrp lng="de">
    <trd>Erdgeschoss</trd>,
    <trd>erster Stock [suddgermane]</trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>ground floor (e&ubreve;rope)</trd>,
    <trd>first floor (amerike)</trd>
  </trdgrp>
  <trd lng="es">planta baja</trd>
  <trd lng="fr">rez-de-chauss&eacute;e</trd>
  <trd lng="he">&qof;&vav;&mem;&tav; &qof;&resh;&qof;&ayin;</trd>
  <trd lng="hu">f&ouml;ldszint</trd>
  <trdgrp lng="nl">
    <trd>begane <ind>grond</ind></trd>,
    <trd>benedenverdieping</trd>
  </trdgrp>
  <trd lng="pl">parter</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_v;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>
  </trdgrp>
  <trd lng="tr">zemin kat</trd>
</drv>
</art>
