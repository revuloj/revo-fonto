<drv mrk="etagx.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="etagx.ter0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="E"/>o samnivela kun la &ccirc;irka&ubreve;a grundo:
      <ekz>
        en realo li lo&gcirc;is nur en la ter<tld/>o, &ccirc;ar la supra parto de la domo utilis nur
        kiel dormejo
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        planoj, montrantaj ter<tld/>e tri &ccirc;ambrojn kaj kuirejon, 
        kaj subtegmente du &ccirc;ambretojn
        <fnt><bib>Kiso</bib>, <lok>&Aring;ge</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="parter.0o.ARKI">partero<sncref/></ref>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_y; &c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</trd>
  <trdgrp lng="de">
    <trd>Erdgeschoss</trd>,
    <trd>erster Stock [suddgermane]</trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>ground floor (e&ubreve;rope)</trd>,
    <trd>first floor (amerike)</trd>
  </trdgrp>
  <trd lng="es">planta baja</trd>
  <trd lng="fr">rez-de-chauss&eacute;e</trd>
  <trd lng="he">&qof;&vav;&mem;&tav; &qof;&resh;&qof;&ayin;</trd>
  <trd lng="hu">f&ouml;ldszint</trd>
  <trd lng="ja">&#x4E00;&#x968E;  <pr>&#x3044;&#x3063;&#x304B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>begane <ind>grond</ind></trd>,
    <trd>benedenverdieping</trd>
  </trdgrp>
  <trd lng="pl">parter</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_v;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>
