<drv mrk="etagx.subter0o">
  <kap>subter<tld/>o, <var><kap>sub<tld/>o</kap></var>,
  <var><kap>kel<tld/>o</kap></var></kap>
  <snc mrk="etagx.subter0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="E"/>o sub la nivelo de la grundo:
      <ekz>
        en subter<tld/>o vivis kaj &ccirc;iutage atendadis &scirc;ian 
        revenon du malgrandaj infanoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        en la sub<tld/>o, farante lesivon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la placo anka&ubreve; estas Muzeo pri Belartoj, <klr>[&#x2026;]</klr> en ties kel<tld/>o
        videblas la fundamentoj de fortika&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2006/008899.php">Vizito al Novarto-urbo Nancy</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Untergeschoss</trd>,
    <trd>Kellergeschoss</trd>,
    <trd>Kelleretage</trd>,
    <trd>Keller</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>basement</trd>,
    <trd>cellar</trd>
  </trdgrp>
  <trd lng="es">s&oacute;tano</trd>
  <trd lng="fr">sous-sol</trd>
  <trdgrp lng="hu">
    <trd>alagsor</trd>,
    <trd>szuter&eacute;n</trd>
  </trdgrp>
  <trd lng="ja">&#x5730;&#x968E;  <pr>&#x3061;&#x304B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>kelder</trd>,
    <trd>kelderverdieping</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>suterena</trd>,
    <trd>piwnica</trd>,
    <trd>przyziemie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j; <ind>&c_e;&c_t;&c_a;&c_zh;</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_ib;&c_v;&c_p;&c_ib;&c_d;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_p;&c_o;&c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_p;&c_o;&c_v;&c_je;&c_r;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7AA8; <pr>y&#xEC;n</pr></trd>,
    <trd>&#x7AA8; <pr>y&#xEC;n</pr></trd>
