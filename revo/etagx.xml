<drv mrk="etagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="etagx.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Samnivela parto de <ref tip="malprt" cel="dom.0o.konstruajxo">domo</ref>:
      <ekz>
        ni levadis nin &ccirc;iam pli kaj pli alte &gcirc;is la kvara 
        <tld/>o
        <fnt>
          <bib>FK</bib>
          <aut>E. Weilsh&auml;user</aut>,
          <vrk>La Hejmo de la Metiisto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        junulo <klr>[&#x2026;]</klr> venkite de la dormo, falis malsupren de la tria <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 20:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        kartodomo, kies tri <tld/>oj &ccirc;iuj estas konstruitaj el ludkartoj kun la figura
        flanko internen
        <fnt>
          <bib>Fab2</bib>
          <lok>Konkurso de saltado</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu <tld/>o multe elstaris super la alia, kaj tute sub la tegmento trovi&gcirc;is
        plumba defluilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i lo&gcirc;as en &ccirc;ambreto de subtegment-<tld/>o
        <fnt>
          <bib>LSB</bib>
          <lok>Sibilla Orakolas</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas tute neutile supreniri tri <tld/>ojn por trovi neniun
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi vere komencas kredi, sinjoro <nom>Felson</nom>, ke via supra <tld/>o <klr>(kapo)</klr>
        anka&ubreve; estas iom en
        malordo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim mrk="etagx.0o.RIM">
      En multaj e&ubreve;ropaj landoj la ter<tld/>o estas rigardata
      kiel nula, kaj oni nombras la suprajn <tld/>on unua,
      dua...  Al &ccirc;i tio konformas jena ekzemplo:
      <ekz>
        sur la mezo de la &scirc;tuparo &scirc;i renkontis la pordiston,
        kiu portis faskon da ligno al unu el la lo&gcirc;ejoj, kiuj 
        trovi&gcirc;is en la unua <tld/>o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
      En aliaj landoj la ter<tld/>o estas kalkulata kiel unua <tld/>o.
    </rim>
  <trdgrp lng="de">
    <trd>Etage</trd>,
    <trd>Stockwerk</trd>,
    <trd>Stock <klr>(Etage)</klr></trd>,
    <trd>Geschoss</trd>
  </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x697C;&#x677F; <pr>l&#xF3;ub&#x1CE;n</pr></trd>,
    <trd>&#x697C;&#x9762; <pr>l&#xF3;umi&#xE0;n</pr></trd>,
    <trd>&#x697C; <pr>l&#xF3;u</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="etagx.0o.nivelo">
    <dif>
      Unu el pluraj samaltaj <ref cel="nivel.0o.ebeno">nivelo</ref>j:
      <ekz>
        la kampoj de la bieno estis sur alta&jcirc;o kaj ku&scirc;is sur kvar <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arta lago <nom>Moeris</nom> okupis 300 kilometrojn kvadratajn kaj estis profunda dek du
        <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la muroj, en pluraj <tld/>oj, staris libroj kaj ankora&ubreve; pli multaj libroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Drako por Heroo Hinreko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la groto <klr>[&#x2026;]</klr> konsistas el tri <tld/>oj
        <fnt><aut>Bardhyl Selimi</aut>: 
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        raketo kun tri <tld/>oj.
      </ekz>
    </dif>
    <ref tip="vid" cel="teras.0o.ebeno">teraso<sncref/></ref>
    <trdgrp lng="de">
      <trd>Stufe <klr>(Gel&auml;nde, Rakete)</klr></trd>,
      <trd>Terasse</trd>,
      <trd>Ebene</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_ja;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_naro;&t_ga;&t_tsheg;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="en">
    <trd>floor</trd>,
    <trd>story <klr> (US)</klr></trd>,
    <trd>storey <klr> (BR)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>piso</trd>,
    <trd>planta<klr tip="amb"> (de edificio o casa)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;tage</trd>
  <trdgrp lng="hu">
    <trd>emelet</trd>,
    <trd>szint</trd>
  </trdgrp>
  <trd lng="ja">&#x968E;  <pr>&#x304D;&#x3056;&#x306F;&#x3057;</pr></trd>
  <trd lng="nl">verdieping</trd>
  <trdgrp lng="pt">
    <trd>andar <klr tip="amb">(de edif&iacute;cio ou casa)</klr></trd>,
    <trd>pavimento</trd>,
    <trd>piso</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pi&eogonek;tro</trd>,
    <trd>kondygnacja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_ja;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="sv">v&aring;ning</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_ja;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_sh;&c_a;&c_r;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="etagx.0areto">
  <kap>
    <tld/>areto
    <fnt><bib>FK</bib></fnt>
  </kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="etagx.0areto.bretaro">
    <dif>
      <ref tip="dif" cel="etagxe.0o">Eta&gcirc;ero</ref>,
      <ref tip="dif" cel="bret.0aro">bretaro</ref>:
      <ekz>
        tie sin trovis anka&ubreve; multaj <tld/>aretoj, egale kovritaj per blua &scirc;tofo kaj
        ornamitaj per bluaj fran&gcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="etagx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="etagx.0igi.KOMUNE">
