  </snc>
  <trd lng="es">organizar por nivel de altura</trd>
  <trd lng="fr">&eacute;tager</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;pcs&odblac;z</trd>,
    <trd>szintek szerint <ind>rendez</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x6BB5;&#x72B6;&#x306B;&#x91CD;&#x306D;&#x308B;  <pr>&#x3060;&#x3093;&#x3058;&#x3087;&#x3046;&#x306B;&#x304B;&#x3055;&#x306D;&#x308B;</pr></trd>
