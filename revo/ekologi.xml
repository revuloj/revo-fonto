<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekologi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKOL</uzo>
  <snc mrk="ekologi.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri rilatoj inter viva&jcirc;oj, pri funkciado de naturaj medioj.
    </dif>
    <trd lng="be">&c_e;&c_k;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>&Ouml;kologie</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;kol&oacute;gia</trd>,
      <trd>k&ouml;rnyezettan</trd>
    </trdgrp>
    <trd lng="nl">ecologie</trd>
    <trd lng="pt">ecologia</trd>
    <trd lng="ru">&c_e;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="ekologi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Socia kaj politika movado emfazanta nedifektadon de la naturo.
    </dif>
    <refgrp tip="vid">
      <ref cel="verd.0ulo">verdulo</ref>
    </refgrp>
    <trd lng="be">&c_e;&c_k;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y; &c_r;&c_u;&c_h;</trd>
    <trdgrp lng="de">
      <trd>&ouml;kologische Bewegung</trd>,
      <trd>&Ouml;kologiebewegung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;kol&oacute;giai mozgalom</trd>,
      <trd>z&ouml;ldmozgalom</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ecologische beweging</trd>,
      <trd>groene beweging</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>ecologismo</trd>,
    <trd>ecologia</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_z;&c_je;&c_l;&c_jo;&c_n;&c_o;&c_je; &c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je; <ind>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v; &c_p;&c_r;&c_i;&c_r;&c_o;&c_d;&c_y;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;cologie</trd>
  </trdgrp>
</drv>

<drv mrk="ekologi.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekologi.0a.SCI">
    <dif>
      Rilata al naturaj medioj, al ilia studo:
      <ekz>
        la plantoj &gcirc;enerale kreskas la&ubreve; difinitaj <tld/>aj kondi&ccirc;oj
        <fnt><aut>Robert Roze</aut>: <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 11a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_k;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>&ouml;kologisch</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;kol&oacute;giai</trd>,
      <trd>k&ouml;rnyezeti</trd>
    </trdgrp>
    <trd lng="ru">&c_e;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="ekologi.0a.POL">
    <dif>
      Sendan&gcirc;era por la naturo:
      <ekz>
        10% de la bienoj en tiu lando praktikas la t.n. &leftquot;<tld/>an
        agrikulturon&rightquot;
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>A&ubreve;strio: lando de ekologia
        kultivado</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 9a, p.
        19a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a; &c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_e;&c_k;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a; &c_ch;&c_y;&c_s;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>&ouml;kologisch abbaubar</trd>,
      <trd>&ouml;kologisch unbedenklich</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;kol&oacute;giai</trd>,
      <trd>k&ouml;rnyezetre &aacute;rtalmatlan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_e;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_o; &c_ch;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;cologique</trd>
  </trdgrp>
  <trd lng="nl">ecologisch</trd>
</drv>

<drv mrk="ekologi.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKOL</uzo>
  <snc>
    <dif>
      Fakulo pri <ref tip="vid" cel="ekologi.0o.SCI">ekologio
      <sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>&Ouml;kologe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;cologiste<klr> (scientifique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;kol&oacute;gus</trd>,
    <trd>k&ouml;rnyezettani szakember</trd>
  </trdgrp>
  <trd lng="nl">ecoloog</trd>
  <trd lng="pt">ec&#243;logo</trd>
  <trd lng="ru">&c_e;&c_k;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
