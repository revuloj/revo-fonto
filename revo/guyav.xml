<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>9</ofc>
   <rad>guyav</rad>/o
</kap>
<drv mrk="guyav.0o">
  <kap><ofc>9</ofc><tld/>o
  <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="http://www.akademio-de-esperanto.org/decidoj/9oa.html">
9a Oficiala Aldono</url></vrk>, <lok>guyav/o</lok></fnt></kap>
  <snc mrk="guyav.0o.KUI">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>ujo, tre aroma, ronda a&ubreve; longforma, ru&gcirc;e a&ubreve; flave
      verda, pomo-granda, uzata por konfita&jcirc;o ktp. 
    </dif>
  </snc>
  <trd lng="fr">goyave</trd>
</drv>

<drv mrk="guyav.0ujo">
  <kap><ofc>9</ofc><tld/>ujo, 
  <var><kap><ofc>9</ofc><tld/>arbo</kap></var>
  <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="http://www.akademio-de-esperanto.org/decidoj/9oa.html">
9a Oficiala Aldono</url></vrk>, <lok>guyav/uj/o,
guyav/arb/o</lok></fnt></kap>
  <snc mrk="guyav.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el tropika Ameriko, kreskigata pro sia frukto <tld/>o, 
      el familio <ref tip="super" cel="mirtac.0oj">mirtacoj</ref>
      (<trd lng="la">Psidium guajava</trd>).
    </dif>
  </snc>
  <trd lng="fr">goyavier</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>