<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glav</rad>/o
</kap>
<drv mrk="glav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      <ref tip="super" cel="arm.0ilo">armilo</ref> konsistanta
      el longa klingo kaj mallonga tenilo. 
    </dif>
    <refgrp tip="sub">
      <ref cel="sabr.0o">sabro</ref>,
      <ref cel="spad.0o">spado</ref>,
      <ref cel="rapir.0o">rapiro</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ponard.0o">ponardo</ref>,
      <ref cel="bajone.0o">bajoneto</ref>.
    </refgrp>
  <trd lng="de">Schwert</trd>
  </snc>
  <trdgrp lng="pt">
    <trd>gl&aacute;dio</trd>,
    <trd>espada</trd>
  </trdgrp>
  <trd lng="es">espada</trd>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Punpovo: <ekz>la <tld/>o de la Le&gcirc;o.</ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>glaive</trd>,
    <trd>&eacute;p&eacute;e</trd></trdgrp> 
  <trd lng="nl">zwaard</trd>
  <trd lng="ru">&c_m;&c_je;&c_ch;</trd>
  <trd lng="sv">sv&auml;rd</trd>
</drv>
<drv mrk="glav.0ingo">
  <kap><tld/>ingo</kap>
  <snc>
   <dif>
     &Scirc;irma ingo, en kiu oni enigas la klingon de <tld/>o. 
   </dif>
  </snc>
  <trd lng="de">Scheide</trd>
  <trd lng="fr">fourreau<klr tip="ind"> (d'une &eacute;p&eacute;e)</klr></trd>
  <trd lng="nl">schede<klr tip="ind"> (v.zwaard)</klr></trd>
  <trd lng="pt">bainha</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;&c_n;&c_y; <klr>(&c_m;&c_je;&c_ch;&c_a;)</klr></trd>
  <trd lng="sv"><klr>(sv&auml;rds)</klr>skida</trd>
</drv>
<drv mrk="glav.0bati0otrancxi0opiki">
  <kap><tld/>bati, <tld/>otran&ccirc;i, <tld/>opiki</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Frapi, tran&ccirc;i, piki per <tld/>o.  
    </dif>
  </snc>
  <adm>
  Temas pri 3 diverssencaj verboj. <aut>MB</aut>
  </adm>
  <trd lng="fr">estoquer</trd>
  <trdgrp lng="pt">
    <trd>espadear</trd>,
    <trd>ferir com a espada</trd>
  </trdgrp>
</drv>
</art>
