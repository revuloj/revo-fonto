<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>intendant</rad>/o
</kap>
<drv mrk="intend.0o">
  <kap><tld/>o</kap>
  <snc mrk="intend.0o.privata">
    <dif>
      Persono komisiita por mastrumi domon, bienon de riĉulo aŭ
      nobelo:
      <ekz>
        <tld/>o de la princa palaco.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekonom.0o">ekonomo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Aufseher</trd>,
      <trd>Verwalter</trd>,
      <trd>Oberaufseher</trd>,
      <trd>Oberverwalter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>gazdatiszt</trd>,
      <trd>jószágigazgató</trd>,
      <trd>tiszttartó</trd>,
      <trd>intendáns</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>opzichter</trd>,
      <trd>rentmeester</trd>
    </trdgrp>
    <trd
    lng="ru">управляющий</trd>
  </snc>
  <snc mrk="intend.0o.administra">
    <dif>
      Oficisto reganta la internan funkciadon de granda administra servo:
      <ekz>
        <ind>militista <tld/>o</ind>
        <klr>(komisiita por provizado kaj administrado de la vivrimedoj
        por armeo)</klr>;
        <trd lng="hu">hadbiztos</trd>
      </ekz>
      <ekz>
        <tld/>ino ĉe la ministrejo por higieno;
      </ekz>
      <ekz>
        fabrika <tld/>ino <klr>(zorganta la higienon de laboristoj kaj
        de iliaj infanoj)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Intendant</trd>
    <trd lng="hu">intendáns</trd>
    <trd lng="nl">intendant</trd>
    <trdgrp lng="ru">
      <trd>заведующий</trd>,
      <trd>интендант</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>аканом</trd>,
    <trd>распарадчык</trd>,
    <trd>загадчык</trd>
    <trd>інтэндант</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>superintendent</trd>,
    <trd>steward</trd>,
    <trd>administrator</trd>,
    <trd>curator</trd>,
    <trd>manager</trd>
  </trdgrp>
  <trd lng="es">intendente</trd>
  <trdgrp lng="fr">
    <trd>intendant</trd>,
    <trd>économe<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="pt">intendente</trd>
</drv>
</art>
