<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mocarel.xml,v 1.3 2024/11/26 20:31:06 afido Exp $">
    <kap>
      <rad>mocarel</rad>/o <fnt><aut>R. Eichholtz k. a.</aut> <vrk>Pekoteko</vrk>
      <lok>1994</lok></fnt>
    </kap>

<drv mrk="mocarel.0o">
  <kap><tld/>o</kap>
  <snc mrk="mocarel.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Italdevena, blanka elasta <ref tip="super" cel="fromag.0o">froma&gcirc;o</ref> el bubal- a&ubreve;
      bovlakto:
      <ekz>
        fuma&jcirc;ita <tld/>o
        <fnt>
          <vrk><url ref="https://kuirukunmi.com/2015/05/26/lazanjo-kun-fungojn-kaj-juglandoj/"
              >Lazanjo kun fungoj kaj juglandoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tran&ccirc;u la tomatojn kaj la <tld/>on <klr>[kaj]</klr> garnu per bazilio
        <fnt>
          <vrk><url ref="https://esperantojugend.de/de/gej-gazeto/kvar-receptoj"
              >Salato Kaprese</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vegana ka&jcirc;unuksa <tld/>o
        <fnt>
          <aut>Andy</aut>
          <vrk><url ref="https://veganarto.com/vegana-kajunuksa-mocarelo"
              >Vegana ka&jcirc;unuksa mocarelo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mozzarella</trd>
    <trd lng="en">mozarella</trd>
    <trd lng="fr">mozarella</trd>
    <trd lng="ru">&c_m;&c_o;&c_c;&c_a;&c_r;&c_je;&c_l;&c_l;&c_a;</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_c;&c_a;&c_r;&c_e;&c_l;&c_a;</trd>
</drv>
</art>

