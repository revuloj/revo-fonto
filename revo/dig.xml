<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dig.xml,v 1.32 2023/11/04 14:29:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dig</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dig.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="mur.0ego">Murego</ref> a&ubreve; teramaso starigita por &scirc;irmi lokon
      kontra&ubreve; superakvo el rivero a&ubreve; maro:
      <ekz>
        la maro, kies potenco rompi&gcirc;adis sur la <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        por gardi sin kontra&ubreve; troaj superakvoj, ili konstruis potencajn <tld/>ojn kaj
        fosis akvujojn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar &ccirc;ijara inundo ne estis tiom granda,
        ne minacante la <tld/>on, oni ne ellasis inundon
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o estos for, kaj la torento de liaj deziroj povos pli libere fluegi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ilia sango estas tro bolanta, rompanta
        ofte <tld/>on de prudento
        <fnt>
          <aut>W. Shakespeare, trad. L. L. Zamenhof</aut>
          <vrk>Hamleto, Re&gcirc;ido de Danujo</vrk>
          <lok>1894</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sxtup.akvo0o">akvo&scirc;tupo</ref>,
       <ref cel="bar.val0ajxo">valbara&jcirc;o</ref>,
       <ref cel="kluz.0o.MAR">kluzo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_e;&c_b;&c_l;&c_ja;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_g;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_i;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dic</trd>,
    <trd>escullera</trd>
  </trdgrp>
  <trd lng="cs">hr&aacute;z</trd>
  <trdgrp lng="de">
    <trd>Damm</trd>,
    <trd>Deich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dam</trd>,
    <trd>dike</trd>
  </trdgrp>
  <trd lng="es">dique</trd>
  <trd lng="fr">digue</trd>
  <trd lng="he">&samekh;&kaf;&resh;</trd>
  <trdgrp lng="hu">
    <trd>g&aacute;t</trd>,
    <trd>v&eacute;d&odblac;g&aacute;t</trd>,
    <trd>t&ouml;lt&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5824;&#x9632;  <pr>&#x3066;&#x3044;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x5830;  <pr>&#x305B;&#x304D;</pr></trd>,
    <trd>&#x30C0;&#x30E0;  <pr>&#x3060;&#x3080;</pr></trd>
  </trdgrp>
  <trd lng="nl">dijk</trd>
  <trdgrp lng="pl">
    <trd>grobla</trd>,
    <trd>nasyp</trd>,
    <trd>tama</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dique</trd>,
    <trd>represa</trd>,
    <trd>barreira</trd>,
    <trd>obst&aacute;culo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hr&aacute;dza</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x442;&#x44C;</trd>,
    <trd>&#x433;&#x440;&#x435;&#x431;&#x43B;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x430;&#x442;&#x430;</trd>,
    <trd>&#x434;&#x430;&#x43C;&#x431;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x5824;<klr> [d&imacron;]</klr></trd>
</drv>

<drv mrk="dig.en0igi">
  <kap><tld/>i, <var><kap>en<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dig.en0igi.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="bar.0i.vojon">Bari</ref> per <tld/>o:
      <ekz>
        kiam Li digas la akvon, &gcirc;i elseki&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 12:15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
    &c_p;&c_l;&c_a;&c_c;&c_ib;&c_n;&c_a;&c_j;</trd>
    <trd lng="ca">posar un dic</trd>
    <trdgrp lng="de">
      <trd>eind&auml;mmen</trd>,
      <trd>eindeichen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>dam</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>g&aacute;ttal elz&aacute;r</trd>,
      <trd>g&aacute;tak k&ouml;z&eacute; szor&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zatamowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol;
      &c_p;&c_l;&c_o;&c_t;&c_i;&c_n;&c_o;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dig.en0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mal<ref tip="ant" cel="akcel.0i">akceli</ref>, <ref cel="halt.0igi">haltigi</ref>:
      <ekz>
        &ccirc;iu lingvo kaj do anka&ubreve; Esperanto ne estas <tld/>igebla de la medio kaj socio,
        en kiu &gcirc;i estas uzata
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi preska&ubreve; eksplodis &ndash; &ccirc;u je rido, &ccirc;u kolero mi ja ne scias, 
        sed mi devis <tld/>i mian spritemon
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        en<tld/>ado de konfliktoj
        <fnt>
          <bib>MD14</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brid.0i">bridi</ref>,
      <ref cel="star.kontraux0i">kontra&ubreve;stari</ref>,
      <ref cel="help.mal0i">malhelpi</ref>,
      <ref cel="moder.0igi">moderigi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_mol;
      &c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">detenir</trd>
    <trdgrp lng="de">
      <trd>eind&auml;mmen</trd>,
      <trd>begrenzen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stop</trd>,
      <trd>halt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megf&eacute;kez</trd>,
      <trd>korl&aacute;tok k&ouml;z&eacute; szor&iacute;t</trd>,
      <trd>megzabol&aacute;z</trd>
    </trdgrp>
    <trd lng="pl">tamowa&cacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_p;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="zh">&#x7B51;&#x5824; <pr>zh&#xFA;t&#xED;</pr></trd>
</snc>
  <trdgrp lng="ca">
    <trd>aturar</trd>,
    <trd>retenir</trd>
  </trdgrp>
  <trd lng="fr">endiguer</trd>
  <trd lng="he">&lamed;&samekh;&kaf;&vav;&resh;</trd>
  <trd lng="ja">&#x305B;&#x304D;&#x6B62;&#x3081;&#x308B;  <pr>&#x305B;&#x304D;&#x3068;&#x3081;&#x308B;</pr></trd>
  <trd lng="nl">indijken</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x442;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x430;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43A;&#x440;&#x438;&#x432;&#x430;&#x442;&#x438; &#x433;&#x440;&#x435;&#x431;&#x43B;&#x435;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
