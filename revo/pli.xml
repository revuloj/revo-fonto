<drv mrk="pli.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pli.0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli granda, forta, abunda...:
      <ekz>
        la donacoj multe <tld/>i&gcirc;adis
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        tre malgranda a&ubreve; e&ccirc; tute mankanta <tld/>i&gcirc;o de la sekrecio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        esprimo de <tld/>i&gcirc;anta embaraso aperis sur lia viza&gcirc;o
        <fnt>
          <aut>I. Silone, trad. D. Mistretta</aut>
          <vrk><url ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">La
          sekreto de Luka</url></vrk>
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="de">
     <trd>sich vermehren</trd>,
     <trd>sich vergr&ouml;&szlig;ern</trd>,
     <trd>sich erweitern</trd>
   </trdgrp>
   <trd lng="en">increase</trd>
   <trdgrp lng="fi">
     <trd>lis&auml;&auml;nty&auml;</trd>,
     <trd>kasvaa</trd>,
     <trd>karttua</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>augmenter<klr tip="amb"> (intr.)</klr></trd>,
