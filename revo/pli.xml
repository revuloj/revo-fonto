<drv mrk="pli.0igi">
   <kap><tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="pli.0igi.KOMUNE">
     <dif>
       <tld lit="P"/>grandigi a&ubreve; <tld/>multigi:
      <ekz>
        Vi <tld/>igas mian grandecon kaj denove min konsolas
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 71:21</lok>
        </fnt>;
      </ekz><ekz>
        dua eldono, revidita, korektita, <tld/>igita
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz><ekz>
        <tld/>igi enspezimpostojn
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Rogers</aut>
          <vrk><url ref="&Steloj;monato2/006819.html">Nov-Zelando: Laborema koalicio</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="kresk.0igi">kreskigi</ref>,
       <ref cel="lev.0i.pliigi">levi <sncref/></ref>
     </refgrp>
   </snc>
  <trdgrp lng="cs">
    <trd>mno&zcaron;it</trd>,
    <trd>rozmno&zcaron;it</trd>,
    <trd>zmno&zcaron;it</trd>,
    <trd>zv&ecaron;t&scaron;it po&ccaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>vermehren</trd>,
