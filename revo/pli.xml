<drv mrk="pli.mal0a">
   <kap>mal<tld/>a</kap>
   <snc mrk="pli.0a.malpligranda">
     <dif>
       Mal<tld/> granda, en mal<tld/> alta grado:
       <ekz>
         Manko de sufi&ccirc;a dormado kondukas al mal<tld/>a sekrecio de
         leptino
         <fnt><aut>CHO Sung Ho</aut>:
           <vrk>KOVIMO: Pandemia obezeco</vrk>. <bib>Monato</bib>
           <lok>&#x2116;012868</lok></fnt>;
      </ekz><ekz>
        komplekso de mal<tld/>a valoro
        <fnt><aut>Dragutin WRANKA</aut>: <vrk>La recenzo</vrk>.
          <lok>Norda Prismo, 58:1</lok>.</fnt>;
      </ekz><ekz>
        Nenio mal<tld/>a venigus viron dum tia tago kaj tia horo
        <fnt>
          <aut>A. Conan Doyle, tr. Trevor Steele</aut>:
          <vrk>La kvin oran&gcirc;semoj</vrk>
          <lok>La aventuroj de &Scirc;erloko Holmso. Kaliningrado: Sezonoj, 2014</lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="ant" cel="pli.0a.pligranda">plia<sncref/></ref>
     <trd lng="en">lesser</trd>
     <trd lng="pl">pomniejszy</trd>
     <trdgrp lng="ro">
       <trd>mic</trd>,
       <trd>minor</trd>
     </trdgrp>
     <trdgrp lng="be">
       <trd>&c_m;&c_je;&c_n;&c_sh;&c_y;</trd>,
       <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_sh;&c_a;&c_n;&c_y;</trd>
     </trdgrp>
     <trd lng="ru">&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_i;&c_j;</trd>
   </snc>
</drv>

<drv mrk="pli.mal0igato">
