<drv mrk="pli.0a">
   <kap><tld/>a</kap>
   <snc mrk="pli.0a.KOMUNE">
     <dif>
       Aldona, ekstra:
       <ekz>
         unu <tld/>a mortis;
       </ekz><ekz>
         jen <tld/>an komplezon mi petas de vi;
       </ekz>
      <ekz>
        sur unu tia kubo staris <tld/>a mal<tld/> granda, kaj sur tiu tria
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        akceptinte <tld/>an cenzuran oficon, li &ccirc;iuvespere sub la lampo malligis dikan
        amason da fremdaj gazetoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        de tiu tempo <nom>Zamenhof</nom> eksentis <tld/>an fidon
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz><ekz>
         se vi postulas nenion <tld/>an, mi povas facile plenumi vian
         volon
         <fnt><bib>ElektFab</bib>, <lok>Fabelo pri iu, kiu migris por
         ekkoni timon</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="krom.0a">kroma</ref>
     </refgrp>
   </snc>
  <trdgrp lng="cs">
    <trd>dal&scaron;&iacute;</trd>,
    <trd>nav&iacute;c</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>weiter</trd>,
     <trd>zus&auml;tzlich</trd>
   </trdgrp>
  <trd lng="en">extra</trd>
  <trdgrp lng="fi">
,   <trd>lis&auml;-</trd>,
     <trd>muu</trd>,
     <trd>lis&auml;ksi tuleva</trd>
   </trdgrp>
  <trdgrp lng="fr">
