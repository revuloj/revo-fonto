<drv mrk="pli.0e">
   <kap><tld/>e</kap>
   <snc mrk="pli.0e.krome">
     <dif>
      Krom tio:
      <ekz>
        repagu per &gcirc;ia plena valoro kaj aldonu <tld/>e kvinonon de &gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj <tld/>e, krom &ccirc;io tio, hodia&ubreve; estas jam la tria tago, de kiam tio okazis
        <fnt>
          <bib>NT</bib>
          <lok>Luko 24:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu paro de manoj <tld/>e ne montri&gcirc;is superflua por revuo, kiu bezonis vastan
        kunlaboradon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
         li opinias, ke la laboristaro povas emancipi&gcirc;i nur se
         &gcirc;i estas tutmonde organizita, k<klr>aj</klr> ke <tld/>e tiu organiziteco
         povas esti trafa
         <fnt><bib>Lanti</bib>, <lok>&ccirc;u unue SAT-anoj?</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>au&szlig;erdem</trd>,
       <trd>zudem</trd>,
       <trd>dazu</trd>
     </trdgrp>
     <trd lng="en">additionally</trd>
     <trd lng="es">adem&aacute;s</trd>
     <trd lng="fi">lis&auml;ksi</trd>
     <trdgrp lng="fr">
       <trd>de plus<klr tip="amb"> (en outre)</klr></trd>,
       <trd>en outre</trd>,
       <trd>en plus<klr tip="amb"> (en outre)</klr></trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>r&aacute;ad&aacute;sul</trd>,
       <trd>s&odblac;t</trd>,
       <trd>azonk&iacute;v&uuml;l</trd>,
       <trd>amellett</trd>
     </trdgrp>
     <trd lng="it">in <ind>pi&ugrave;</ind></trd>
     <trd lng="he">&bet;&nun;&vav;&samekh;&fpe;</trd>
     <trd lng="nl">daarenboven</trd>
     <trdgrp lng="pl">
     <trd>opr&oacute;cz tego</trd>,
     <trd>nadto</trd>,
     <trd>ponadto</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_je;&c_shch;&c_jo;</trd>,
       <trd>&c_k;&c_r;&c_o;&c_m;&c_je; &c_t;&c_o;&c_g;&c_o;</trd>
     </trdgrp>
   </snc>
   <snc mrk="pli.0e.pliforte">
     <dif>
       Je <tld/> alta grado:
       <ekz>
         mi pre&gcirc;as, ke via amo abundu ankora&ubreve; <tld/>e kaj <tld/>e
         <fnt><bib>NT</bib>, <lok>&Fil; 1:9</lok></fnt>;
       </ekz>
      <ekz>
        multe <tld/>e la &scirc;ajne malfortaj membroj de la korpo estas necesaj
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 12:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volas certigi vin, ke miaj aferoj <tld/>e efektivi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 1:12</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>mehr</trd>,
       <trd><klr>in </klr>h&ouml;herem <ind>Grad(e)</ind></trd>,
       <trd><klr>in </klr>h&ouml;herem <ind>Ma&szlig;(e)</ind></trd>
     </trdgrp>
     <trd lng="en">more</trd>
     <trd lng="fi">enemm&auml;n</trd>
     <trd lng="fr">&agrave; un plus haut degr&eacute;</trd>
     <trdgrp lng="hu">
       <trd>jobban</trd>,
       <trd>ink&aacute;bb</trd>
     </trdgrp>
     <trd lng="it">di <ind>pi&ugrave;</ind></trd>
     <trd lng="nl">eer</trd>
     <trdgrp lng="pl">
     <trd>wi&eogonek;cej</trd>,
     <trd>bardziej</trd>
     </trdgrp>
     <trd lng="ru">&c_v; &c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_j; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_i;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>a pak</trd>,
    <trd>d&aacute;l</trd>,
    <trd>krom&ecaron; toho</trd>,
    <trd>mimoto</trd>,
    <trd>nadto</trd>,
    <trd>nav&iacute;c</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mimo toho</trd>,
    <trd>na viac</trd>,
    <trd>&dcaron;alej</trd>
  </trdgrp>
</drv>

<drv mrk="pli.0igi">
   <kap><tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="pli.0igi.KOMUNE">
     <dif>
       <tld lit="P"/>grandigi a&ubreve; <tld/>multigi:
      <ekz>
        Vi <tld/>igas mian grandecon kaj denove min konsolas
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 71:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        dua eldono, revidita, korektita, <tld/>igita
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi enspezimpostojn
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Rogers</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006819.html">Nov-Zelando: Laborema koalicio</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="kresk.0igi">kreskigi</ref>,
       <ref cel="lev.0i.pliigi">levi <sncref/></ref>
     </refgrp>
   </snc>
  <trdgrp lng="cs">
    <trd>mno&zcaron;it</trd>,
    <trd>rozmno&zcaron;it</trd>,
    <trd>zmno&zcaron;it</trd>,
    <trd>zv&ecaron;t&scaron;it po&ccaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>vermehren</trd>,
     <trd>erweitern</trd>
   </trdgrp>
  <trd lng="en">increase</trd>
  <trdgrp lng="fi">
     <trd>lis&auml;t&auml;</trd>,
     <trd>kasvattaa</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>accroitre</trd>,
     <trd>augmenter<klr tip="amb"> (tr.)</klr></trd>
   </trdgrp>
  <trd lng="he">&lamed;&he;&vav;&samekh;&yod;&fpe;
   \<ind>&yod;&samekh;&fpe;</ind>\</trd>
  <trdgrp lng="hu">
     <trd>fokoz</trd>,
     <trd>n&ouml;vel</trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>ingrandire<klr> (tr.)</klr></trd>,
     <trd>aumentare<klr> (tr.)</klr></trd>
   </trdgrp>
  <trd lng="nl">vermeerderen</trd>
  <trdgrp lng="pl">
     <trd>zwi&eogonek;ksza&cacute;</trd>,
     <trd>powi&eogonek;ksza&cacute;</trd>
   </trdgrp>
  <trd lng="ru">&c_u;&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>mno&zcaron;i&tcaron;</trd>,
    <trd>rozmno&zcaron;i&tcaron;</trd>
  </trdgrp>
