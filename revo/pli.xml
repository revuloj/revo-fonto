<drv mrk="pli.0e">
   <kap><tld/>e</kap>
   <snc mrk="pli.0e.krome">
     <dif>
      Krom tio:
      <ekz>
        repagu per &gcirc;ia plena valoro kaj aldonu <tld/>e kvinonon de &gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 6:5</lok>
        </fnt>;
      </ekz><ekz>
        kaj <tld/>e, krom &ccirc;io tio, hodia&ubreve; estas jam la tria tago, de kiam tio okazis
        <fnt>
          <bib>NT</bib>
          <lok>Luko 24:21</lok>
        </fnt>;
      </ekz><ekz>
        unu paro de manoj <tld/>e ne montri&gcirc;is superflua por revuo, kiu bezonis vastan
        kunlaboradon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
         li opinias, ke la laboristaro povas emancipi&gcirc;i nur se
         &gcirc;i estas tutmonde organizita, k<klr>aj</klr> ke <tld/>e tiu organiziteco
         povas esti trafa
         <fnt><bib>Lanti</bib>, <lok>&ccirc;u unue SAT-anoj?</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>au&szlig;erdem</trd>,
       <trd>zudem</trd>,
       <trd>dazu</trd>
     </trdgrp>
     <trd lng="en">additionally</trd>
     <trd lng="es">adem&aacute;s</trd>
     <trd lng="fi">lis&auml;ksi</trd>
     <trdgrp lng="fr">
       <trd>de plus<klr tip="amb"> (en outre)</klr></trd>,
       <trd>en outre</trd>,
       <trd>en plus<klr tip="amb"> (en outre)</klr></trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>r&aacute;ad&aacute;sul</trd>,
       <trd>s&odblac;t</trd>,
       <trd>azonk&iacute;v&uuml;l</trd>,
       <trd>amellett</trd>
     </trdgrp>
     <trd lng="it">in <ind>pi&ugrave;</ind></trd>
     <trd lng="he">&bet;&nun;&vav;&samekh;&fpe;</trd>
     <trd lng="nl">daarenboven</trd>
     <trdgrp lng="pl">
     <trd>opr&oacute;cz tego</trd>,
     <trd>nadto</trd>,
     <trd>ponadto</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_je;&c_shch;&c_jo;</trd>,
       <trd>&c_k;&c_r;&c_o;&c_m;&c_je; &c_t;&c_o;&c_g;&c_o;</trd>
     </trdgrp>
   <trd lng="th">&#3609;&#3629;&#3585;&#3592;&#3634;&#3585;&#3609;&#3637;&#3657;</trd>
   </snc>
   <snc mrk="pli.0e.pliforte">
     <dif>
       Je <tld/> alta grado:
       <ekz>
         mi pre&gcirc;as, ke via amo abundu ankora&ubreve; <tld/>e kaj <tld/>e
         <fnt><bib>NT</bib>, <lok>&Fil; 1:9</lok></fnt>;
       </ekz><ekz>
        multe <tld/>e la &scirc;ajne malfortaj membroj de la korpo estas necesaj
        <fnt><bib>NT</bib> <lok>&Ko1; 12:22</lok></fnt>;
      </ekz><ekz>
        mi volas certigi vin, ke miaj aferoj <tld/>e efektivi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>&Fil; 1:12</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>mehr</trd>,
       <trd><klr>in </klr>h&ouml;herem <ind>Grad(e)</ind></trd>,
       <trd><klr>in </klr>h&ouml;herem <ind>Ma&szlig;(e)</ind></trd>
     </trdgrp>
     <trd lng="en">more</trd>
     <trd lng="fi">enemm&auml;n</trd>
     <trd lng="fr">&agrave; un plus haut degr&eacute;</trd>
     <trdgrp lng="hu">
       <trd>jobban</trd>,
       <trd>ink&aacute;bb</trd>
     </trdgrp>
     <trd lng="it">di <ind>pi&ugrave;</ind></trd>
     <trd lng="nl">eer</trd>
     <trdgrp lng="pl">
     <trd>wi&eogonek;cej</trd>,
     <trd>bardziej</trd>
     </trdgrp>
     <trd lng="ru">&c_v; &c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_j; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_i;</trd>
   <trd lng="th">&#3629;&#3618;&#3656;&#3634;&#3591;&#3617;&#3634;&#3585;&#3586;&#3638;&#3657;&#3609;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>a pak</trd>,
    <trd>d&aacute;l</trd>,
    <trd>krom&ecaron; toho</trd>,
    <trd>mimoto</trd>,
    <trd>nadto</trd>,
