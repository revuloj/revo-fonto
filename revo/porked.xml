<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: porked.xml,v 1.13 2022/11/25 16:40:07 revo Exp $">
<kap>
  <rad>porked</rad>/oj,
  <var><kap><rad var="su">sued</rad>/oj</kap></var>
</kap>
<drv mrk="porked.0oj">
  <kap><tld/>oj,
    <var><kap><tld var="su"/>oj</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Suidae</trd>):
      <ekz>
        <klr>[en]</klr> <nom>Allier</nom> <klr>[&#8230;]</klr> ekonomio bazi&gcirc;as 
        sur agrikulturo, bredado (bovedoj, <tld/>oj) kaj kultivado de 
        cerealoj estas la plej gravaj aktivecoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=376"
              >Allier</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej fruaj fosilioj de <tld var="su"/>oj datas el la epoko oligoceno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=502446"
              >Suedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_mol;&c_n;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">suids</trd>
  <trd lng="de">Schweine</trd>
  <trdgrp lng="fr">
    <trd>suid&eacute;s</trd>,
    <trd>porcins</trd>
  </trdgrp>
  <trd lng="hu">sert&eacute;sf&eacute;l&eacute;k</trd>
  <trdgrp lng="nl">
    <trd>varkens<klr tip="ind"> (fam.)</klr></trd>,
    <trd>zwijnen<klr tip="ind"> (fam.)</klr></trd>
  </trdgrp>
  <trd lng="pl">&sacute;winiowate</trd>
  <trd lng="pt">su&iacute;deos</trd>
  <trd lng="ro">suide</trd>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_n;&c_mol;&c_i; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

