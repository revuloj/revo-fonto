<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: popl.xml,v 1.24 2023/09/03 12:45:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>popl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="popl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="popl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="salikac.0oj">salikacoj</ref>
      (<trd lng="la">Populus</trd>) de
      <ref tip="lst" cel="arb.0o" lst="voko:malpigioparencoj">arboj</ref>
      kaj arbedoj kun triangulaj, kor- a&ubreve; ovoformaj longtigaj folioj,
      kun <ref tip="prt" cel="ament.0o">amentoj</ref>
      kaj <ref tip="prt" cel="kapsul.0o.BOT">kapsuloj</ref>
      du- &gcirc;is kvarpartaj, enhavantaj multajn lanugajn
      flugsemojn, similajn al ne&gcirc;eroj:
      <ekz>
        &gcirc;i estis malnova domo, kun altaj muroj kaj altkreskaj <tld/>oj
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_o;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_o;&c_p;&c_o;&c_l;&c_a;</trd>
  <trd lng="cs">topol</trd>
  <trd lng="de">Pappel</trd>
  <trd lng="en">poplar</trd>
  <trd lng="fr">peuplier</trd>
  <trd lng="hu">ny&aacute;r(fa)</trd>
  <trdgrp lng="ja">
    <trd>&#x30DD;&#x30D7;&#x30E9;</trd>,
    <trd>&#x30CF;&#x30B3;&#x30E4;&#x30CA;&#x30AE;</trd>
  </trdgrp>
  <trd lng="nl">populier</trd>
  <trd lng="pl">topola</trd>
  <trdgrp lng="pt">
    <trd>choupo</trd>,
    <trd>&aacute;lamo</trd>
  </trdgrp>
  <trd lng="ro">plop</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">topo&lcaron;</trd>
  <trd lng="sv">poppel</trd>
  <trd lng="uk">&#x442;&#x43E;&#x43F;&#x43E;&#x43B;&#x44F;</trd>
</drv>
</art>
