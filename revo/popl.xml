<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>popl</rad>/o
</kap>
<drv mrk="popl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="salikac.0oj">salikacoj</ref>
      (<trd lng="la">Populus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_o;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_o;&c_p;&c_o;&c_l;&c_a;</trd>
  <trd lng="de">Pappel</trd>
  <trd lng="en">poplar</trd>
  <trd lng="fr">peuplier</trd>
  <trd lng="hu">ny&aacute;r(fa)</trd>
  <trd lng="nl">populier</trd>
  <trd lng="pl">topola</trd>
  <trdgrp lng="pt">
    <trd>choupo</trd>,
    <trd>&aacute;lamo</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sv">poppel</trd>
</drv>
</art>
