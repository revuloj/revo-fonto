<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frunt.xml,v 1.32 2023/11/10 06:08:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frunt</rad>/o
</kap>

<drv mrk="frunt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frunt.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Superokula parto de la <ref tip="malprt" cel="vizagx.0o.ANA">viza&gcirc;o</ref> 
      inter la <ref cel="tempi.0o">tempio</ref>j:
      <ekz>
        &scirc;tonon &jcirc;etis <klr>[li]</klr> kaj trafis la Fili&scirc;ton en lian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 17:49</lok>
        </fnt>;
      </ekz>
      <ekz>
        la faraono a&ubreve;skultis kun sulkigita <tld/>o 
        <klr>(malkontenta)</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Feniciano ankora&ubreve; unu fojon ekfrapis 
        la plankon per la <tld/>o <klr>(<ref cel="rivere.0i">riverencis</ref>)</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite li frapis sin je la <tld/>o
        <klr>(kp ek<ref cel="memor.0i">memori</ref>, 
        <ref cel="kompre.ek0i">ekkompreni</ref>)</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidi&gcirc;is, frotis la <tld/>on kvaza&ubreve; vekita el profunda dormo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faris al &scirc;i sur la <tld/>o kaj sur la brusto la signon de kruco
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        dikaj gutoj da &scirc;vito fluadis de &scirc;ia <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> kisis nin sur la frunto, kaj ni denove faris malla&ubreve;tan pre&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne iru okuloj super la <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Stirn</trd>
  <trdgrp lng="pt">
    <trd>fronte</trd>,
    <trd>testa</trd>
  </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7709;&#x5B87;&#x4E4B;&#x9593; <pr>m&#xE9;iy&#x1D4;zh&#x12B;ji&#x101;n</pr></trd>,
    <trd>&#x984D;&#x982D; <pr>&#xE9;t&#xF3;u</pr></trd>,
    <trd>&#x9859; <pr>s&#x1CE;ng</pr></trd>,
    <trd>&#x524D;&#x984D; <pr>qi&#xE1;n&#xE9;</pr></trd>,
    <trd>&#x8111;&#x95E8; <pr>n&#x1CE;om&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="frunt.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Simila parto de la kapo &ccirc;e bestoj.
    </dif>
  <trdgrp lng="pt">
    <trd>fronte</trd>,
    <trd>testa</trd>
  </trdgrp>
  </snc>
  <refgrp tip="vid">
    Ne konfuzu kun:
    <ref cel="front.0o">fronto</ref>.
  </refgrp>
  <trd lng="be">&c_l;&c_o;&c_b;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_l;&c_o;</trd>
  <trdgrp lng="en">
    <trd>forehead</trd>,
    <trd>brow</trd>
  </trdgrp>
  <trd lng="ca">front</trd>
  <trd lng="es">frente</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">front<klr tip="ind"> (anat.)</klr></trd>
  <trd lng="hu">homlok</trd>
  <trd lng="id">dahi</trd>
  <trd lng="it">fronte<klr> (anat.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x984D;  <pr>&#x3072;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x524D;&#x982D;&#x90E8;  <pr>&#x305C;&#x3093;&#x3068;&#x3046;&#x3076;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorhoofd</trd>
  <trd lng="pl">czo&lstroke;o</trd>
  <trd lng="ru">&c_l;&c_o;&c_b;</trd>
  <trd lng="sv">panna<klr tip="ind"> (anat.)</klr></trd>
  <trd lng="sk">&ccaron;elo</trd>
  <trd lng="cs">&ccaron;elo</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x43E;&#x43B;&#x43E;</trd>,
    <trd>&#x43B;&#x43E;&#x431;</trd>
  </trdgrp>
</drv>

<drv mrk="frunt.0osto">
  <kap><tld/>osto</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ost.0o">Osto</ref> de la homa
      <ref tip="malprt" cel="skelet.0o.ANA">skeleto</ref>, formanta la
      anta&ubreve;an parton de la <ref tip="malprt" cel="krani.0o">kranio</ref>.
    </dif>
    <ref tip="sin" cel="fruntal.0o">fruntalo</ref>
    <refgrp tip="vid">
      <ref cel="pariet.0osto">parietosto</ref>,
      <ref cel="okcipi.0osto">okcipitosto</ref>,
      <ref cel="temporal.0o">temporalo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_o;&c_b;&c_n;&c_a;&c_ja; <ind>&c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="ca">(os) frontal</trd>
  <trd lng="cs">&ccaron;eln&iacute; kost</trd>
  <trd lng="de">Stirnknochen</trd>
  <trd lng="en">frontal bone</trd>
  <trd lng="es">frontal</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n; &f_p;&f_y;&a_sh;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">os frontal</trd>
  <trd lng="hu">homlokcsont</trd>
  <trd lng="it">osso <ind>frontale</ind></trd>
  <trd lng="ja">&#x524D;&#x982D;&#x9AA8;  <pr>&#x305C;&#x3093;&#x3068;&#x3046;&#x3053;&#x3064;</pr></trd>
  <trd lng="nl">voorhoofdsbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; czo&lstroke;owa</trd>
  <trdgrp lng="pt">
    <trd>frontal</trd>,
    <trd>coronal</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_b;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">&ccaron;eln&aacute; kos&tcaron;</trd>
  <trd lng="sv">pannben</trd>
  <trd lng="uk">&#x43B;&#x43E;&#x431;&#x43E;&#x432;&#x430; &#x43A;&#x456;&#x441;&#x442;&#x43A;&#x430;</trd>
</drv>

<drv mrk="frunt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">ARKI</uzo>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="fronto.0o">Frontono</ref>:
      <ekz>
        la mezan parton de la <tld/>a&jcirc;o dividas kvar <klr>[&#x2026;]</klr> fenestroj kun jonaj
        kolonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=526831">Placo Sz&eacute;chenyi (Esztergom)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="ca">front&oacute;</trd>
  <trd lng="de">Stirnseite</trd>
  <trd lng="en">pediment</trd>
  <trd lng="es">frente</trd>
  <trd lng="fa">&a_s;&a_n;&a_t;&a_w;&a_r;&f_y;</trd>
  <trd lng="hu">homlokzat</trd>
  <trd lng="it">frontone</trd>
  <trd lng="nl">fronton</trd>
  <trd lng="pl">fronton</trd>
  <trd lng="sv">fronton</trd>
  <trd lng="zh">&#x7AEF;&#x4FA7; <pr>du&#x101;nc&#xE8;</pr></trd>
</drv>
</art>
