<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frunt</rad>/o
</kap>
<drv mrk="frunt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Superokula parto de la viza&gcirc;o inter la tempioj:
      <ekz>sulkigi la <tld/>on<fnt>B</fnt>;</ekz>
      <ekz>rigardi <tld/>en (rekte en la viza&gcirc;on);</ekz>
      <ekz>montru al li amikan <tld/>on <klr>(viza&gcirc;on)</klr>.</ekz> 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Simila parto de la kapo &ccirc;e bestoj. 
    </dif>
    <refgrp tip="sin">
      <ref cel="front.0o">fronto</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_l;&c_o;&c_b;</trd>
</drv>
<drv mrk="frunt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Frontono.  
    </dif>
  </snc>
</drv>
</art>
