       <trd>Radioskopie</trd>,
       <trd>Durchleuchtung</trd>
   </trdgrp>
  <trd lng="en">radioscopy</trd>
  <trd lng="fr">radioscopie</trd>
  <trd lng="hu">radioszk&oacute;pia</trd>
  <trdgrp lng="ja">
    <trd>&#x900F;&#x8996;  <pr>&#x3068;&#x3046;&#x3057;</pr></trd>,
    <trd>&#xFF38;&#x7DDA;&#x900F;&#x8996;  <pr>&#x3048;&#x3063;&#x304F;&#x3059;&#x305B;&#x3093;&#x3068;&#x3046;&#x3057;</pr></trd>
