    <trd>&c_r;&c_a;&c_d;&c_ib;&#x43E;&c_s;&#x43A;&#x43E;&#x43F;&c_ib;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_v;&c_ib;&c_ch;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_r;&c_je;&#x43D;&c_t;&c_gu;&c_je;&#x43D;</trd>
  </trdgrp>
