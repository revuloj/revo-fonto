<kap>
  <rad>radioskopi</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="radios.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">SCI</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref cel="ekzame.0o.esploro">Ekzameno</ref> de objekto per radioj:
      <ekz>
        dum la sekvaj du tagoj ripeti&gcirc;adis la <tld/>o, la mjelpunkcio, la tedintaj
        ser&ccirc;oj de sentivaj punktoj sur la lumbo kaj sur la abomene senmovaj kruroj
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel </aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/jurto.html">La Jurto de
          Korvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="radi">radiofotografio</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>perradia esplorado</ke>.
  </rim>

  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_jo;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_r;&c_je;&c_n;&c_t;&c_g;&c_je;&c_n;&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_ja;</trd>
  <trd lng="ca">radiosc&ograve;pia</trd>
  <trd lng="cs">radioskopie</trd>
  <trdgrp lng="de">
       <trd>Radioskopie</trd>,
       <trd>Durchleuchtung</trd>
   </trdgrp>
  <trd lng="en">radioscopy</trd>
  <trd lng="fr">radioscopie</trd>
  <trd lng="hu">radioszk&oacute;pia</trd>
  <trdgrp lng="ja">
    <trd>&#36879;&#35222;  <pr>&#12392;&#12358;&#12375;</pr></trd>,
    <trd>&#65336;&#32218;&#36879;&#35222;  <pr>&#12360;&#12387;&#12367;&#12377;&#12379;&#12435;&#12392;&#12358;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">radioscopie</trd>
