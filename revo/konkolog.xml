<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkolog.xml,v 1.3 2023/02/16 07:26:34 revo Exp $">
<kap>
  <rad>konkolog</rad>/o
</kap>
<drv mrk="konkolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkolog.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specialisto pri
      <ref tip="vid" cel="konkologi.0o.ZOO">konkologio</ref>:
      <ekz>
        spertaj <tld/>oj nun mallevas multajn el la tre
        fajnaj specioj de Dorbini (D'Orbigny) kaj aliaj
        al la rango de varia&jcirc;oj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_h;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="fr">
    <trd>conchyliologiste</trd>,
    <trd>conchyliologue</trd>
  </trdgrp>
  <trd lng="pl">koncholog</trd>     
</drv>
</art>

