<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>kolekt</rad>/i
    </kap>
    <drv mrk="kolekt.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="kolekt.0i.kunvenigi">
	<dif>
	  Venigi plurajn personojn en unu saman lokon por ia komuna celo:
	  <ekz>
	    <tld/>i siajn amikojn &ccirc;irka&ubreve; si<fnt>B</fnt>;
	  </ekz><ekz>
	    <tld/>u al mi miajn fidelulojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    &scirc;i embuskas kiel rabisto kaj <tld/>as &ccirc;irka&ubreve; si
	    malhonestulojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    la viza&gcirc;oj <tld/>itaj &ccirc;irka&ubreve; la kameno estis
	    &gcirc;ojaj<fnt>B</fnt>;
	  </ekz><ekz>
	    la kokino <tld/>as siajn idojn sub siaj flugiloj;
	  </ekz><ekz>
	    <tld/>i trupojn, armeon, lernantojn,  kongreson, konsilantaron;
	  </ekz><ekz>
	    tiu societo <tld/>as la inteligentularon;
	  </ekz><ekz>
	    du pentra&jcirc;oj <tld/>is &ccirc;irka&ubreve; si amason da
	    admirantoj<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="ar.0igi">arigi</ref>,
	  <ref cel="mobili.0i">mobilizi</ref>.
	</refgrp>
        <trdgrp lng="de">
           <trd>sammeln</trd>,
           <trd>versammeln</trd>,
           <trd>um sich <ind>scharen</ind></trd>
        </trdgrp>
	<trd lng="nl">verzamelen</trd>
      </snc>
      <snc mrk="kolekt.0i.kunmeti">
	<dif>
	  Kunmeti apartajn objektojn por komuna destino; arigi disa&jcirc;ojn:
	  <ekz>kiu gro&scirc;on ne respektas, ri&ccirc;econ ne <tld/>as<fnt><bib>PrV</bib></fnt>;</ekz><ekz>
	    <tld/>i trezorojn, flugfoliojn, librojn, pentra&jcirc;ojn,
	    po&scirc;tmarkojn, cita&jcirc;ojn, sentencojn
	    (<ref tip="sin" cel="kompil.0i">kompili</ref>);
	  </ekz><ekz>
	    la dense <tld/>itaj tegmentoj<fnt>Z</fnt>;
	  </ekz><ekz>
	    kiu povas <tld/>i la venton en siajn mankavojn<fnt>Z</fnt>?
	  </ekz><ekz>
	    <tld/>i sciigojn, informojn, spertojn;
	  </ekz><ekz>
	    <klr>(f)</klr> fine mi povis <tld/>i miajn senordajn
	    pensojn<fnt>B</fnt>;
	  </ekz><ekz>
	    estis neeble <tld/>i <klr>(ordigi)</klr> siajn
	    pensojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>i sian kura&gcirc;on<fnt>Z</fnt>, siajn
	    fortojn<fnt>Z</fnt>.
	  </ekz>
	</dif>
        <trdgrp lng="de">
           <trd>sammeln</trd>,
           <trd>zusammenbringen</trd>,
           <trd>auflesen</trd>
        </trdgrp>
	<trd lng="nl">verzamelen</trd>
      </snc>
      <snc mrk="kolekt.0i.foren">
	<dif>
	  Levi de tero kaj kunigi por forporti kun si:
	  <ekz>
	    <tld/>i pecetojn de rompita vazo;
	  </ekz><ekz>
	    <tld/>i pajlon<fnt>Z</fnt>,
	  </ekz><ekz>
	    lignon, mortintajn bran&ccirc;ojn por hejtado<fnt>Z</fnt>;
	  </ekz><ekz>
	    la formiko <tld/>as dum la rikolto sian man&gcirc;on<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>i mortintojn sur batalkampo.
	  </ekz>
	</dif>
        <trdgrp lng="de">
           <trd>aufsammeln</trd>,
           <trd>zusammenlesen</trd>
        </trdgrp>
	<trd lng="nl">verzamelen</trd>
      </snc>
      <snc mrk="kolekt.0i.pluki">
	<dif>
	  &Scirc;iri de la trunko kaj kunigi por forporti kun si:
	  <ekz>
	    <tld/>i florojn<fnt>Z</fnt>,
	  </ekz><ekz>
	    vinberojn<fnt>Z</fnt>,
	  </ekz><ekz>
	    pomojn, la grenon;
	  </ekz><ekz>
	    <tld/>ado de fruktoj<fnt>Z</fnt>
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="rikolt.0i">rikolti</ref>,
	  <ref cel="sxir">de&scirc;iri</ref>.
	</refgrp>
        <trdgrp lng="de">
           <trd>pfl&uuml;cken [florojn]</trd>,
           <trd>rupfen [herbojn]</trd>,
           <trd>einsammeln [diversajn fruktojn]</trd>,
           <trd>stechen [asparagon]</trd>,
           <trd>rausmachen [terpomojn]</trd>,
           <trd>einbringen [grenon]</trd>,
           <trd>lesen [vinberojn]</trd>
        </trdgrp>
	<trdgrp lng="nl">
	  <trd>oogsten</trd>,
	  <trd>plukken</trd>
	</trdgrp>
      </snc>
      <snc mrk="kolekt.0i.kotizigi">
	<dif>
	  Depostuli de privatuloj la monon, kiun ili &scirc;uldas al la
	  &scirc;tato, la&ubreve; la bud&gcirc;eto:
	  <ekz>
	    <tld/>i impostojn.
	  </ekz>
	</dif>
        <trdgrp lng="de">
           <trd>sammeln</trd>,
           <trd>einziehen</trd>,
           <trd>erheben</trd>,
           <trd>eintreiben</trd>
        </trdgrp>
	<trd lng="nl">innen</trd>
      </snc>
      <trd lng="cs">sb&iacute;rat</trd>
      <trd lng="en">collect</trd>
      <trdgrp lng="ru">
	<trd>&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_s;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>
    <drv mrk="kolekt.0o">
      <kap><tld/>o</kap>
      <snc>
	<subsnc mrk="kolekt.0o.ghen">
	  <dif>
	    Aro da <tld/>itaj personoj a&ubreve; objektoj: 
	    <ekz>
	      <tld/>o <klr>(amaso)</klr> da homoj<fnt>Z</fnt>
	      <ref tip="vid" cel="arme.0o.FIG">(armeo)</ref>; 
	    </ekz><ekz>
	      <tld/>o de 20&ndash;30 personoj<fnt>Z</fnt>; 
	    </ekz><ekz>
	      tiu frazo estas nur bele sonanta <tld/>o da vortoj<fnt>Z</fnt>.
	    </ekz>
	  </dif>
	</subsnc>
	<subsnc mrk="kolekt.0o.libr">
	  <dif>
	    Libro a&ubreve; publikiga&jcirc;o, kiu konsistas el <tld/>itaj
	    similaj tekstoj a&ubreve; dokumentoj:
	    <ekz>
	      <tld/>o da le&gcirc;oj <ref tip="sub" cel="kod.0o">(kodo)</ref>; 
	    </ekz><ekz>
	      <tld/>o da historiaj faktoj
	      <ref tip="sub" cel="kronik1.0o">(kroniko)</ref>; 
	    </ekz><ekz>
	      <tld/>o da eseoj
	      <ref tip="vid" cel="libr.fest0o">(festlibro)</ref>; 
	    </ekz><ekz>
	      <tld/>o da cita&jcirc;oj
	      <ref tip="vid" cel="kompil.0ajxo">(kompila&jcirc;o)</ref>; 
	    </ekz><ekz>
	      <tld/>o da desegna&jcirc;oj, da fotografa&jcirc;oj
	      <ref tip="vid" cel="album.0o">(albumo)</ref>; 
	    </ekz><ekz>
	      <tld/>o da anekdotoj (anekdotaro); 
	    </ekz><ekz>
	      <tld/>o da poemoj (poemaro).
	    </ekz>
	  </dif>
	  <ref tip="sub" cel="tekst.0aro">tekstaro</ref>
	  <ref tip="vid" cel="ar">-ar</ref>
	  <trd lng="fr">recueil</trd>
	  <trd lng="ru">&c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
	</subsnc>
	<subsnc mrk="kolekt.0o.art">
	  <dif>
	    Aro da objektoj, dokumentoj a&ubreve; natura&jcirc;oj, kiujn oni
	    kune konservas pro ilia arta, scienca, historia valoro,
	    a&ubreve; malofteco: 
	    <ekz>
	      anta&ubreve; la milito li havis ampleksan <tld/>on de japana&jcirc;oj
	      <fnt>(<aut>Stanislav Kamar&yacute;t</aut>:
		<vrk>Historio de Esperanto-movado en
		  &Ccirc;e&hcirc;oslovakio</vrk>,
		<lok>3. Dum la dua mondmilito</lok>)</fnt>; 
	    </ekz><ekz>
	      <tld/>o da pentra&jcirc;oj
	      <refgrp tip="sin">
		<ref cel="galeri.0o.kolekto">(galerio</ref>,
		<ref cel="pinak.0o">pinakoteko)</ref></refgrp>; 
	    </ekz><ekz>
	      <tld/>o da libroj
	      <ref tip="sin" cel="biblio1.0o">(biblioteko)</ref>; 
	    </ekz><ekz>
	      <tld/>o da po&scirc;tmarkoj
	      <ref tip="vid" cel="filate.0o">(filatelo)</ref>; 
	    </ekz><ekz>
	      botanika <tld/>o; 
	    </ekz><ekz>
	      <tld/>o da bestoj
	      <ref tip="sin" cel="menagx.0o">(mena&gcirc;erio)</ref>; 
	    </ekz>
	  </dif>
	  <refgrp tip="vid">
	    <ref cel="muze.0o.kolekto">muzeo</ref>,
	    <ref cel="kabine.0.ekspozicio">kabineto</ref>
	  </refgrp>
	  <refgrp tip="prt">
	    <ref cel="pec.0o">peco</ref>,
	    <ref cel="perl.0o">perlo</ref>
	  </refgrp>
	  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
	</subsnc>
	<subsnc mrk="kolekt.0o.seri">
	  <uzo tip="fak">TIP</uzo>
	  <dif><ref tip="sin" cel="seri.0o.aroTIP">serio</ref>: 
	    <ekz>
	      <tld lit="K"/>o Esperanta aprobita de D-ro Zamenhof.
	    </ekz>
	  </dif>
	  <ref tip="prt" cel="volum.0o">volumo</ref>
	  <ref tip="super" cel="seri.0ajho">seria&jcirc;o</ref>
	  <ref tip="vid" cel="biblio1.0o">biblioteko</ref>
	</subsnc>
	<subsnc mrk="kolekt.0o.gazet">
	  <uzo tip="fak">TIP</uzo>
	  <dif><tld lit="K"/>itaj numeroj de perioda&jcirc;o: 
	    <ekz>
	      mi vidis la tutan <tld/>on de la Nica Literatura Revuo
	      <fnt>(<aut>Shi Chengtai</aut>: <vrk>La Resurekto</vrk>)</fnt>.
	    </ekz>
	  </dif>
	  <trd lng="ru">&c_p;&c_o;&c_d;&c_sh;&c_i;&c_v;&c_k;&c_a;</trd>
	</subsnc>
      </snc>
      <trd lng="nl">verzameling</trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_r;&c_a;&c_n;&c_i;&c_je;</trd>
    </drv>
    <drv mrk="kolekt.0isto">
      <kap><tld/>isto<fnt>Z</fnt></kap>
      <snc>
	<dif>
	  Oficisto, kiu <tld/>as la impostojn.
	</dif>
      </snc>
      <trd lng="nl">ontvanger</trd>
      <trd lng="ru">&c_s;&c_b;&c_o;&c_r;&c_shch;&c_i;&c_k; &c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;</trd>
    </drv>
    <drv mrk="kolekt.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
	<dif>
	  Sin <tld/>i a&ubreve; esti <tld/>ita; kunveni, kuni&gcirc;i,
	  amasi&gcirc;i:
	  <ekz>
	    amaso da zumantaj insektoj <tld/>i&gcirc;is &ccirc;irka&ubreve; ni;
	  </ekz><ekz>
	    <tld/>i&gcirc;is kongreso el fakuloj<fnt>Z</fnt>;
	  </ekz><ekz>
	    la larmoj <tld/>i&gcirc;is en &scirc;iaj okuloj<fnt>Z</fnt>;
	  </ekz><ekz>
	    sub la sankta signo de l' espero <tld/>i&gcirc;as pacaj
	    batalantoj<fnt>Z</fnt>;
	  </ekz><ekz>
	    li mortis kaj al<tld/>i&gcirc;is al sia popolo<fnt>Z</fnt>;
	  </ekz><ekz>
	    baloj kaj <tld/>i&gcirc;oj sekvis unu la alian<fnt>Z</fnt>;
	  </ekz><ekz>
	    la <tld/>i&gcirc;ojn de la akvo Dio nomis maro<fnt>Z</fnt>
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="flu">kunflui</ref>,
	  <ref cel="grup">grupi&gcirc;i</ref>.
	</refgrp>
      </snc>
      <trd lng="nl">bijeenkomen</trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_v;&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
    </drv>
    <drv mrk="kolekt.en0i">
      <kap>en<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  Kunigi en unu tuton por konservi are:
	  <ekz>
	    en<tld/>i grenon<fnt>Z</fnt>,
	    vinberon<fnt>Z</fnt>, ovojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    ni similas al akvo, kiu estas ver&scirc;ata sur la teron, kaj kiun oni
	    ne povas en<tld/>i<fnt>Z</fnt>;
	  </ekz><ekz>
	    en<tld/>ado de fruktoj<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="nl">inzamelen</trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol; <klr>(&c_ch;&c_t;&c_o;-&c_l;. &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
    </drv>
    <drv mrk="kolekt.for0i">
      <kap>for<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  Kunigi kaj forporti, tiel ke nenio plu restu sur la loko.
	</dif>
      </snc>
      <trd lng="nl">alles <ind>meenemen</ind></trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol; <klr>(&c_i; &c_u;&c_n;&c_je;&c_s;&c_t;&c_i;)</klr></trd>
    </drv>
    <drv mrk="kolekt.sxir0i">
      <kap>&scirc;ir<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  <tld/>i 4:
	  <ekz>
	    &scirc;ir<tld/>i florojn<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="nl">plukken</trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; <klr>(&c_s;&c_r;&c_y;&c_v;&c_a;&c_ja;, &c_n;&c_a;&c_p;&c_r;. &c_c;&c_v;&c_je;&c_t;&c_y;)</klr></trd>
    </drv>
    <drv mrk="kolekt.herbo0o">
      <kap>herbo<tld/>o</kap>
      <snc>
	<dif>
	  Kolekto de vegeta&jcirc;oj sekigitaj kaj ordigitaj la&ubreve; scienca
	  klasifiko.
	</dif>
      </snc>
      <trd lng="de">Herbarium</trd>
      <trd lng="fr">herbier</trd>
      <trd lng="nl">herbarium</trd>
      <trd lng="ru">&c_g;&c_je;&c_r;&c_b;&c_a;&c_r;&c_i;&c_j;</trd>
    </drv>
    <drv mrk="kolekt.jar0o">
      <kap>jar<tld/>o</kap>
      <snc>
	<uzo tip="fak">TIP</uzo>
	<dif>La tuto de la dum unu jaro aperintaj numeroj de perioda&jcirc;o.
	</dif>
	<ref tip="vid" cel="volum.0o.jar">jarvolumo</ref>
      </snc>
      <trd lng="fr">ann&eacute;e</trd>
      <trd lng="nl">jaargang</trd>
      <trd lng="ru">&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_p;&c_o;&c_d;&c_sh;&c_i;&c_v;&c_k;&c_a;</trd>
    </drv>
  </art>
