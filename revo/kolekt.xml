      <ekz>
        kiam vi rikoltos en via vinberejo, ne for<tld/>u
        la resta&jcirc;on post vi: io restu por la fremdulo
        <fnt><bib>MT</bib><lok>&Rea; 24:21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_ib;
  &c_a;&c_d;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd>abernten</trd>,
    <trd>abgrasen</trd>,
    <trd>wegsammeln</trd>
  </trdgrp>
  <trd lng="es"><ind>recoger</ind> (para llevar)</trd>
  <trd lng="hu">(hi&aacute;nytalanul) <ind>&ouml;sszeszed</ind></trd>
  <trd lng="nl">alles <ind>meenemen</ind></trd>
  <trd lng="pl">zebra&cacute;</trd>     
  <trd lng="ro">culege</trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol; <klr>(&c_i;
          &c_u;&c_n;&c_je;&c_s;&c_t;&c_i;)</klr></trd>
  <trd lng="sk">zhroma&zcaron;di&tcaron; a&nbsp;odnies&tcaron;</trd>
</drv>

