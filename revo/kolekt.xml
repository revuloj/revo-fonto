<drv mrk="kolekt.mon0o">
  <kap>
    mon<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>ado de mono, ordinare por iu bonafara celo, projekto ks:
      <ekz>
        la mon<tld/>o pro la sanktuloj, kiel mi ordonis al la eklezioj de Galatujo
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 16:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas bonfaremaj, ke vi partoprenas en &ccirc;iuj mon<tld/>oj, filantropiaj
        projektoj kaj aran&gcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>mon<tld/>ujo</ind>n mi ne vidis
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
        <trd lng="de">Spendenb&uuml;chse</trd>
        <trd lng="id"><ind>kotak</ind> amal</trd>
      </ekz>
      <ekz>
        mi priparolis <klr>[&#x2026;]</klr> la teruran malsategon, nuntempe okazantan en
        Hindujo sekve de longa senpluveco, tiam, subite, venis al mi la penso aran&gcirc;i
        mon<tld/>on por tiuj malfeli&ccirc;uloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="almoz.0kesto">almozkesto</ref>,
      <ref cel="benefi.0o">benefico</ref>,
      <ref cel="kvest1.0o">kvesto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_b;&c_o;&c_r; &c_g;&c_r;&c_o;&c_sh;&c_a;&c_w;</trd>,
    <trd>&c_z;&c_b;&c_o;&c_r; &c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w; <klr>(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;&c_h;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kollekte</trd>,
    <trd>Spendensammlung</trd>
  </trdgrp>
  <trd lng="en">offertory</trd>
  <trdgrp lng="id">
    <trd>amal</trd>,
    <trd><baz>sembah</baz>persembahan <klr>(uang)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x52DF;&#x91D1;  <pr>&#x307C;&#x304D;&#x3093;</pr></trd>,
    <trd>&#x96C6;&#x91D1;  <pr>&#x3057;&#x3085;&#x3046;&#x304D;&#x3093;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolekt.sxir0i">
  <kap>&scirc;ir<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="kolekt.0i.pluki"><tld lit="K"/>i<sncref/></ref>:
      <ekz>
        &scirc;ir<tld/>i la figojn kaj vinberojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        arbo, de kiu oni &scirc;ir<tld/>is la fruktojn
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        rozoj, kiujn &scirc;i &scirc;ir<tld/>is en la &gcirc;ardeno
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virino <klr>[&#x2026;]</klr> &scirc;ir<tld/>is al si plenan po&scirc;on da
        nuksoj avelaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi anka&ubreve; donacos al vi du plenajn po&scirc;ojn da teo, tute verda kaj
        fre&scirc;a, kiun mi mem &scirc;ir<tld/>is sur la loko de &gcirc;ia kreskado
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol;
  <klr>(&c_z;&c_r;&c_y;&c_v;&c_a;&c_ju;&c_ch;&c_y;)</klr></trd>
  <trd lng="de">pfl&uuml;cken</trd>
  <trd lng="es">recolectar</trd>
  <trd lng="fr">cueillir</trd>
  <trd lng="hu">szed <klr tip="amb">(term&eacute;st)</klr></trd>
  <trd lng="id"><baz>petik</baz>memetik <klr>(buah, bunga, dsb.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6458;&#x307F;&#x96C6;&#x3081;&#x308B;  <pr>&#x3064;&#x307E;&#x307F;&#x3042;&#x3064;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x53CE;&#x7A6B;&#x3059;&#x308B;  <pr>&#x3057;&#x3085;&#x3046;&#x304B;&#x304F;&#x3059;&#x308B;</pr></trd>
