<drv mrk="kolekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kolekt.0i.kunvenigi">
    <dif>
      Venigi plurajn personojn en unu saman lokon por ia komuna
      celo:
      <ekz>
        <tld/>i siajn amikojn &ccirc;irka&ubreve; si<fnt>B</fnt>;
      </ekz><ekz>
        <tld/>u al mi miajn fidelulojn
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 50:5</lok>
        </fnt>;
      </ekz><ekz>
        &scirc;i embuskas kiel rabisto kaj <tld/>as &ccirc;irka&ubreve; si
        perfidulojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 23:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la fajro]</klr> &jcirc;etas ru&gcirc;etan flamon sur la viza&gcirc;ojn
        <tld/>itajn &ccirc;irka&ubreve; la kameno
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        la kokino <tld/>as siajn idojn sub siaj flugiloj;
      </ekz><ekz>
        <tld/>i trupojn, armeon, lernantojn, kongreson,
        konsilantaron;
      </ekz><ekz>
        tiu societo <tld/>as la inteligentularon;
      </ekz><ekz>
        du pentra&jcirc;oj <tld/>is &ccirc;irka&ubreve; si amason da
        admirantoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ar.0igi">arigi</ref>,
      <ref cel="mobili.0i">mobilizi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>sammeln</trd>,
      <trd>versammeln</trd>,
      <trd>um sich <ind>scharen</ind></trd>
    </trdgrp>
    <trd lng="es">reunir</trd>
    <trdgrp lng="nl">
      <trd>verzamelen</trd>,
      <trd>bijeenbrengen</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolekt.0i.kunmeti">
    <dif>
      Kunmeti apartajn objektojn por komuna destino; arigi
      disa&jcirc;ojn:
      <ekz>
        <ind>kiu gro&scirc;on ne respektas, ri&ccirc;econ ne
          <tld/>as</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ki a
          <ind>fill&eacute;rt</ind> nem becs&uuml;li, a forintot nem
          &eacute;rdemli</trd>
      </ekz><ekz>
        <tld/>i trezorojn, flugfoliojn, librojn, pentra&jcirc;ojn,
        po&scirc;tmarkojn, cita&jcirc;ojn, sentencojn
        <klr>(<ref cel="kompil.0i">kompili</ref>)</klr>;
      </ekz><ekz>
        la dense <tld/>itaj tegmentoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fabeloj estis efektive <tld/>itaj de fratoj <nom>Grimm</nom> kaj
        diktitaj de kamparaj rakontantoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>is la venton en siajn mankavojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:4</lok>
        </fnt>?
      </ekz><ekz>
        <tld/>i sciigojn, informojn, spertojn;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i siajn <klr>(ordigi)</klr> pensojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>i sian kura&gcirc;on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i siajn fortojn, por preti&gcirc;i al la akcepto de la bato
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kolekt.0i.foren">
      <dif>
        Levi de tero kaj kunigi por forporti kun si:
        <ekz>
          <tld/>i pecetojn de rompita vazo;
        </ekz><ekz>
          <tld/>i pajlon
          <fnt>
            <bib>MT</bib>
            <lok>Eliro 5:7</lok>
          </fnt>;
        </ekz><ekz>
          <tld/>i lignon, mortintajn bran&ccirc;ojn por hejtado<fnt>Z</fnt>;
        </ekz><ekz>
          la formiko <tld/>as dum la rikolto sian man&gcirc;on
          <fnt>
            <bib>MT</bib>
            <lok>Sentencoj 6:8</lok>
          </fnt>;
        </ekz><ekz>
          <tld/>i mortintojn sur batalkampo.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>aufsammeln</trd>,
        <trd>zusammenlesen</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>reunir</trd>,
        <trd>recoger</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>verzamelen</trd>,
        <trd>oprapen</trd>,
        <trd>sprokkelen</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kolekt.0i.pluki">
      <dif>
        &Scirc;iri de la trunko kaj kunigi por forporti kun si:
        <ekz>
          (&scirc;ir)<tld/>i florojn
          <fnt>
            <bib>Fab4</bib>
            <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
            filinoj</lok>
          </fnt>
          <fnt>
            <bib>Fab2</bib>
            <lok>An&gcirc;elo</lok>
          </fnt>;
        </ekz><ekz>
          (&scirc;ir)<tld/>i vinberojn
          <fnt>
            <bib>MT</bib>
            <lok>Ju&gcirc;istoj 9:27</lok>
          </fnt>
          <fnt>
            <bib>NT</bib>
            <lok>Mateo 7:16</lok>
          </fnt>
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XIII</lok>
          </fnt>
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro IV</lok>
          </fnt>;
          </ekz><ekz>
            <tld/>i pomojn, la grenon;
          </ekz><ekz>
            <tld/>ado de fruktoj
          <fnt>
            <bib>MT</bib>
            <lok>Eliro 34:22</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="rikolt.0i">rikolti</ref>,
        <ref cel="sxir">de&scirc;iri</ref>.
      </refgrp>
      <trdgrp lng="de">
        <trd><klr>(Blumen) </klr>pfl&uuml;cken </trd>,
        <trd><klr>(Gras) </klr>rupfen </trd>,
        <trd>einsammeln <klr>(Fr&uuml;chte)</klr></trd>,
        <trd>stechen <klr>(Spargel)</klr> </trd>,
        <trd>ausgrabenh <klr>(Kartoffeln)</klr></trd>,
        <trd>einbringen <klr>Getreideernte)</klr></trd>,
        <trd>lesen <klr>(Wein)</klr></trd>
      </trdgrp>
      <trd lng="es">recolectar</trd>
      <trdgrp lng="id">
        <trd><baz>petik</baz>memetik</trd>,
        <trd><baz>panen</baz>memanen</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>oogsten</trd>,
        <trd>plukken</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kolekt.0i.kotizigi">
      <dif>
        Depostuli a&ubreve; peti de privatuloj monon:
        <ekz>
          <tld/>i impostojn
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XII</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>i monon
          <fnt>
            <bib>FK</bib>
          </fnt>
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XIX</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>sammeln</trd>,
        <trd>einziehen</trd>,
        <trd>erheben</trd>,
        <trd>eintreiben</trd>
      </trdgrp>
      <trd lng="es">recaudar</trd>
      <trd lng="id"><baz>koleksi</baz>mengoleksi</trd>
      <trd lng="nl">innen</trd>
    </subsnc>
    <trdgrp lng="de">
      <trd>sammeln</trd>,
      <trd>zusammenbringen</trd>,
      <trd>auflesen</trd>
    </trdgrp>
    <trd lng="es">agrupar</trd>
    <trdgrp lng="nl">
      <trd>verzamelen</trd>,
      <trd>bijeenbrengen</trd>
    </trdgrp>
  </snc>
