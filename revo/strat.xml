<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strat.xml,v 1.36 2021/05/30 09:52:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>strat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="strat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="strat.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="voj.0o.TRA">Vojo</ref> inter konstrua&jcirc;oj en
      urbo a&ubreve; vila&gcirc;o:
      <ekz>
        ni pasigos la nokton sur la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        trairu la <tld/>ojn de <nom>Jerusalem</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 5:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la kelo, tra ka&scirc;ita pordeto <klr>[li]</klr> elkondukis lin en
        malplenan
        <tld/>on de Memfiso
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en la]</klr> nova distrikto de Jerusalemo oni nomos la
        novajn <tld/>ojn ne
        plu la&ubreve; elstaraj mortintaj homoj, sed la&ubreve; la okcidenta zodiako
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9310-artikoletoj.txt">Malkopiilo</url></vrk>
          <lok>93/10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la <tld/>oj de la urbo paradas miloj da bretonoj, kun ri&ccirc;aj
        folkloraj
        kostumoj kune kun tradiciaj muzikinstrumentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006851.html">Francio:
          Quimper</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e kiu <tld/>o trovi&gcirc;as la monfarejo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006859.html">Seborgo:
          Urbeto a&ubreve; sendependa lando?</url></vrk>
        </fnt>?
      </ekz>
      <ekz>
        mi promenis tra la mallar&gcirc;aj <tld/>etoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006859.html">Seborgo:
          Urbeto a&ubreve; sendependa lando?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la melodiojn, verkitajn de la plej bonaj egiptaj komponistoj,
        fajfas la infanoj
        en la <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007063.html">Um
          Keltum'</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">straat</trd>
  <trd lng="be">&c_v;&c_u;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_u;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&#xF63;&#xF58;&#xF0B;&#xF41;</trd>
  <trd lng="ca">carrer</trd>
  <trd lng="cs">ulice</trd>
  <trd lng="de">Stra&szlig;e</trd>
  <trd lng="en">street</trd>
  <trd lng="es">calle</trd>
  <trd lng="fr">rue</trd>
  <trd lng="he">&resh;&het;&vav;&bet;</trd>
  <trd lng="hu">utca</trd>
  <trd lng="id">jalan</trd>
  <trd lng="kk">&c_k;&#x4E9;&c_sh;&c_je;</trd>
  <trd lng="ne">&#x938;&#x921;&#x915;</trd>
  <trd lng="nl">straat</trd>
  <trd lng="oc">carri&egrave;ra</trd>
  <trd lng="pl">ulica</trd>
  <trd lng="pt">rua</trd>
  <trd lng="ru">&c_u;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">ulica</trd>
  <trd lng="sq">rrug&#xEB;</trd>
  <trd lng="tr">sokak</trd>
  <trd lng="vo">s&uuml;t</trd>
  <trd lng="zh">&#x8857;<klr> [ji&#x113;]</klr></trd>
</drv>

<drv mrk="strat.0ulino">
  <kap><tld/>ulino</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="strat.0ulino.AMOR">
    <dif>
      <ref tip="dif" cel="prosti.0itino.AMOR">Prostituitino</ref>:
      <ekz>
        mia bona amikino en nia elementlerneja tempo
        nun fari&gcirc;is <tld/>ulino
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nev&ecaron;stka</trd>,
    <trd>pouli&ccaron;n&iacute; d&ecaron;vka</trd>,
    <trd>&scaron;lapka</trd>
  </trdgrp>
  <trd lng="de">Nutte</trd>
  <trdgrp lng="es">
    <trd>prostituta</trd>
  </trdgrp>
  <trd lng="fr">fille publique</trd>
  <trd lng="id">pe<ind>lacur</ind></trd>
  <trdgrp lng="sk">
    <trd>prostit&uacute;tka</trd>,
    <trd>&lcaron;ahk&aacute; &zcaron;ena</trd>,
    <trd>&scaron;&lcaron;apka</trd>
  </trdgrp>
</drv>

<drv mrk="strat.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <snc mrk="strat.cxef0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Pli granda a&ubreve; pli grava <ref tip="super" cel="strat.0o.TRA"><tld/>o</ref> en vila&gcirc;o, kvartalo, 
      urbo:
      <ekz>
        ili iris silente, &gcirc;is ili atingis unu el la &ccirc;ef<tld/>oj
        <fnt><bib>InfanTorent2</bib>, <lok>&ccirc;apitro 3a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Hortensia</nom> renkontis sur la &ccirc;ef<tld/>o en nia kampara
        urbeto meza&gcirc;an konatinon
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2006/008827.php">Pri
          kutimoj kaj kalsonoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        manifestaciis proksimume 125&#x202F;000 homoj tra la &ccirc;ef<tld/>oj de la
        irlanda &ccirc;efurbo
        Dublino
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2009/009765.php">Unulitera
          diferenco ...</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis du sinagogoj sur la &ccirc;ef<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2012/010361.php">Lisuarte</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hauptstra&szlig;e</trd>
  <trdgrp lng="es">
    <trd>avenida</trd>,
    <trd>calle principal</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>art&egrave;re<klr> (rue)</klr></trd>,
    <trd>grand-rue</trd>,
    <trd>rue principale</trd>
  </trdgrp>
  <trd lng="hu">f&odblac;utca</trd>
  <trdgrp lng="id">
    <trd><ind>jalan</ind> protokol</trd>,
    <trd><ind>jalan</ind> utama</trd>
  </trdgrp>
  <trd lng="nl">hoofdstraat</trd>
</drv>

<drv mrk="strat.flanko0o">
  <kap>flanko<tld/>o, <var><kap>flanka <tld/>o</kap></var></kap>
  <snc mrk="strat.flanko0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="strat.0o.TRA"><tld lit="S"/>o</ref>
      flanke deiranta de alia, ordinare pli granda <tld/>o:
      <ekz>
        grupetoj da homoj kolekti&gcirc;is en kvietaj flanko<tld/>oj kaj 
        kortoj, por priparoli, duonvo&ccirc;e, la strangajn,
        maltrankviligajn nova&jcirc;ojn
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Seitenstra&szlig;e</trd>
  <trd lng="es">calle lateral</trd>
  <trd lng="fr">rue lat&eacute;rale</trd>
  <trdgrp lng="hu">
  <trd>mell&eacute;kutca</trd>,
  <trd>k&ouml;z</trd>
</trdgrp>
  <trd lng="nl">zijstraat</trd>
</drv>

<drv mrk="strat.sak0o">
  <kap>sak<tld/>o</kap>
  <snc mrk="strat.sak0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="strat.0o.TRA"><tld lit="S"/>o</ref>
      kun fermita fino, enirebla kaj elirebla nur tra unu ekstremo:
      <ekz>
        &scirc;ajnas, ke mia a&ubreve;teto eniris sak<tld/>on
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Dombestoj kaj sakstratoj</vrk>,
          <bib>Monato</bib>,
          <lok>1998:6, p. 28a</lok>;
        </fnt>
      </ekz>
      <ekz>
        spioni &ccirc;e sak<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007198.html">Poezio
          sed ne poemoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
     <trdgrp lng="nl">
    <trd>doodlopende <ind>straat</ind></trd>,
    <trd>blinde <ind>straat</ind></trd>
  </trdgrp>
  </snc>
  <snc mrk="strat.sak0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senrezulta agado, sensolva problemo a&ubreve; situacio:
      <ekz>
        tradicia politiko estas sak<tld/>o
        <fnt>
          <aut>K. Berga</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/berga/87150">
          De iam &gcirc;is nun...</url></vrk>,
          <lok>2008-08-26</lok>
        </fnt>;
      </ekz>
      <ekz>
        elekti inter milito unuflanke kaj da&ubreve;ra subpremado de irakanoj
        aliflanke
        prezentas ne vojforkon sed sak<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Ranieri Clerici</aut>
          <vrk><url ref="https://www.monato.be/2003/008048.html">Paco
          alvokata</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi simple ne scias ta&ubreve;gan vojon el tiu &ccirc;i sak<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2003/008130.html">&Scirc;topi&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>slop</trd>,
      <trd>impasse</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="ir.senel0ejo">senelirejo</ref>
  </refgrp>
  <trd lng="de">Sackgasse</trd>
  <trd lng="es">calle sin salida</trd>
  <trd lng="fr">impasse</trd>
  <trd lng="hu">zs&aacute;kutca</trd>
  <trd lng="id"><ind>jalan</ind> buntu</trd> 
  <trd lng="sk">slep&aacute; uli&ccaron;ka</trd>
  <trd lng="cs">slep&aacute; ulice</trd>
</drv>
</art>
