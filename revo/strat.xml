<drv mrk="strat.sak0o">
  <kap>sak<tld/>o</kap>
  <snc mrk="strat.sak0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="strat.0o.TRA"><tld lit="S"/>o</ref>
      kun fermita fino, enirebla kaj elirebla nur tra unu ekstremo:
      <ekz>
        &scirc;ajnas, ke mia a&ubreve;teto eniris sak<tld/>on
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Dombestoj kaj sakstratoj</vrk>,
          <bib>Monato</bib>,
          <lok>1998:6, p. 28a</lok>;
        </fnt>
      </ekz>
      <ekz>
        spioni &ccirc;e sak<tld/>o
        <fnt>
          <aut>Grigori Arosev</aut>
            <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 28</lok>
          </fnt>.
      </ekz>
    </dif>
     <trdgrp lng="nl">
    <trd>doodlopende <ind>straat</ind></trd>,
    <trd>blinde <ind>straat</ind></trd>
  </trdgrp>
  </snc>
  <snc mrk="strat.sak0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senrezulta agado, sensolva problemo a&ubreve; situacio:
      <ekz>
        tradicia politiko estas sak<tld/>o
        <fnt>
          <aut>K. Berga</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/berga/87150">
          De iam &gcirc;is nun...</url></vrk>,
          <lok>2008-08-26</lok>
        </fnt>;
      </ekz>
      <ekz>
        elekti inter milito unuflanke kaj da&ubreve;ra subpremado de irakanoj
        aliflanke
        prezentas ne vojforkon sed sak<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Ranieri Clerici</aut>
          <vrk><url ref="&Monato;/2003/008048.html">Paco
          alvokata</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi simple ne scias ta&ubreve;gan vojon el tiu &ccirc;i sak<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008130.html">&Scirc;topi&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>slop</trd>,
      <trd>impasse</trd>
      <trd>straatje zonder eind</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="ir.senel0ejo">senelirejo</ref>
  </refgrp>
  <trd lng="be">&c_t;&c_u;&c_p;&c_ib;&c_k;&c_o;&c_v;&c_a;&c_ja;
  &c_v;&c_u;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="cs">slep&aacute; ulice</trd>
  <trd lng="de">Sackgasse</trd>
  <trd lng="es">calle sin salida</trd>
  <trd lng="fr">impasse</trd>
  <trd lng="hu">zs&aacute;kutca</trd>
  <trd lng="id"><ind>jalan</ind> buntu</trd>
  <trd lng="ja">&#34955;&#23567;&#36335;  <pr>&#12405;&#12367;&#12429;&#12371;&#12358;&#12376;</pr></trd>
