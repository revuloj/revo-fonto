  <kap><tld/>o</kap>
  <snc mrk="event.0o.okazajxo">
    <dif>
      Difinita, sufi&ccirc;e grava
      <ref tip="super" cel="okaz.0ajxo">okaza&jcirc;o</ref>:
      <ekz>
        oni povus nomi plurajn gravajn <tld/>ojn de tiu jaro <klr>[&#8230;]</klr>, sed la&ubreve;
        literatura vidpunkto <klr>[&#8230;]</klr> la plej signifa infano de 1907 estis la
        unua originala esperanta romano &ndash; Kastelo de Prelongo
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la plej elstara kultura-historia <tld/>o sur la lingva kampo estas la formi&gcirc;o
        kaj ekesto de la Internacia Lingvo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        libro entenanta milojn da informoj pri <tld/>oj, faktoj, nomoj, titoloj, datoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiun multflankan agadon abrupte interrompis la venko de naziismo en Germanio kaj
        la politikaj sekvoj de tiu tragika <tld/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        li estas poeto de momentaj natur<tld/>oj, de subitaj, intuiciaj perceptoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        popolo, kiu pere de kreo kaj partopreno en kulturaj <tld/>oj
        klopodas sekurigi sian propran estadon kaj
        estontecon
        <fnt><aut>Guido Van Damme</aut>: <vrk>Bruliga libro
        (Senad &Ccirc;oli&ccirc;: Spite al &ccirc;io Bosnio)</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 6a, p. 22a</lok></fnt>.
      </ekz>
    </dif>  
    <subsnc mrk="event.0o.kunveno">
      <dif>
        <ref tip="super" cel="arangx.0o">Aran&gcirc;o</ref> sufi&ccirc;e 
        <ref tip="sub" cel="spekta.0o.evento">spektakla</ref>, 
        sensacia <klr>(<ref tip="sub" cel="festival.0o">festivalo</ref>, 
        <ref tip="sub" cel="kermes.0o">kermeso</ref> k.a.)</klr>:
        <ekz>
          festtago, jubileo, <tld/>o, amuzo
          <fnt>
            <bib>VaK</bib>
          </fnt>;
        </ekz>
        <ekz>
          impona kultura <tld/>o <klr>[&#8230;]</klr> estas la Internaciaj Artaj Festvaloj
          <fnt>
            <bib>EeP</bib>
          </fnt>;
        </ekz>  
        <ekz>
          Universala Kongreso de Esperanto estas &ccirc;iam <tld/>o
          <fnt>
            <bib>EfI</bib>
          </fnt>.
        </ekz>    
      </dif>
      <trdgrp lng="de">
        <trd>Erlebnis<ind>veranstaltung</ind></trd>,
        <trd>Event</trd>
      </trdgrp>
    </subsnc>
