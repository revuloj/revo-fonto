<art mrk="$Id: deklar1.xml,v 1.24 2022/03/15 15:50:54 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>deklaraci</rad>/o
</kap>
<drv mrk="deklar1.0o">
  <kap><tld/>o</kap>
  <snc mrk="deklar1.0o.deklaro">
    <dif>
      <ref tip="dif" cel="deklar.0o.KOMUNE">Deklaro</ref>:
      <ekz>
        <uzo tip="fak">ESP</uzo>
        la Bulonja <tld/>o precizigis la oficialan, de &ccirc;iuj
        sendispute
        akceptitan esencon de la Esperantismo
        <fnt><bib>Paroloj</bib>, <lok>anta&ubreve; la Dua Kongreso
        Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto
        1906</lok></fnt>;
      </ekz>
      <ekz>
        la universala <tld/>o pri homaj rajtoj;
      </ekz>
      <ekz>
        se mi ne eraras, vi faras <tld/>on <klr>(amdeklaron)</klr> pri mia
        filino
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 13a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_l;&c_ja;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">declaraci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>deklarace</trd>,
    <trd>prohl&aacute;&scaron;en&iacute; <klr>(slavnostn&iacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erkl&auml;rung</trd>,
    <trd>Verk&uuml;ndung</trd>,
    <trd>Deklaration</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>announcement</trd>,
    <trd>declaration</trd>,
    <trd>manifesto</trd>,
    <trd>proclamation</trd>
  </trdgrp>
  <trd lng="es">declaraci&oacute;n</trd>
  <trd lng="fr">d&eacute;claration</trd>
  <trd lng="he">&#x5D4;&#x5B7;&#x5E6;&#x5D4;&#x5B8;&#x5E8;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>nyilatkozat</trd>,
    <trd>deklar&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="pl">deklaracja</trd>
  <trd lng="pt">declara&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_l;&c_a;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>deklar&aacute;cia</trd>,
    <trd>prejav</trd>,
    <trd>verejn&eacute; vyhl&aacute;senie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x433;&#x43E;&#x43B;&#x43E;&#x448;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x44F;&#x432;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x433;&#x43E;&#x43B;&#x43E;&#x448;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x434;&#x435;&#x43A;&#x43B;&#x430;&#x440;&#x430;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
