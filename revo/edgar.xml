<kap>
  <rad>Edgar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="edgar.0o">
  <kap><tld/>o</kap>
  <snc mrk="edgar.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        kiel vi probable jam scias, <tld/>o el Valio estis volapukisto 
        kaj poste esperantisto
        <fnt>
          <aut>M. Lavallee</aut>,
          <vrk>Occidental-Interlingue relevi&gcirc;as</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>, 1998-12-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on Degason alparolis iu: &leftquot;Pardonu, via
        viza&gcirc;o &scirc;ajnas al 
        mi konata&rightquot;
        <fnt>
          <aut>D. Kowalska</aut>,
          <lok><url ref="http://esperanto.pl/page.php?tid=75">
          </url>Esperanto.pl, Anekdotoj, 2004-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_d;&c_g;&c_a;&c_r;</trd>
  <trd lng="bg">&c_Je;&c_d;&c_g;&c_a;&c_r;</trd>
  <trd lng="de">Edgar</trd>
  <trd lng="fr">Edgar</trd>
  <trd lng="hu">Edg&aacute;r</trd>
  <trd lng="ja">&#x30A8;&#x30C9;&#x30AC;&#x30FC;</trd>
  <trd lng="pl">Edgar</trd>
  <trdgrp lng="pt">
    <trd>Edgardo</trd>,
    <trd>Edgar</trd>,
    <trd>Edgard</trd>
  </trdgrp>
  <trd lng="uk">&c_Je;&c_d;&c_g;&c_a;&c_r;</trd>
</drv>
