<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zirkoni</rad>/o
</kap>
<drv mrk="zirkoni.0o">
  <kap><tld/>o</kap>
  <mlg>Zr</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 40, atompezo 91,22, simbolo <frm>Zr</frm>. Helgriza
substanco kun
      amfotera karaktero. Transir<ref tip="super"
cel="metal.0o">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_k;&c_o;&c_n;&c_ib;&c_j;</trd>       
  <trd lng="br">zirkoniom</trd>
  <trdgrp lng="de">
    <trd>Zirkonium</trd>,
    <trd>Zirkon</trd>
  </trdgrp>
  <trd 
lng="el">&zeta;&jota;&rho;&kappa;&omikron_ton;&ny;&jota;&omikron;</trd>
  <trd lng="en">zirconium</trd>
  <trdgrp lng="es">
    <trd>circonio</trd>,
    <trd>zirconio</trd>
  </trdgrp>   
  <trd lng="fr">zirconium</trd>
  <trd lng="hu">cirk&oacute;nium</trd>
  <trd lng="it">zirconio</trd>
  <trd lng="la">Zirconium</trd>
  <trd lng="nl">zirkonium</trd>
  <trd lng="pl">cyrkon</trd>
  <trdgrp lng="pt">
    <trd>zirc&oacute;nio</trd>,
    <trd>zirc&ocirc;nio<klr> (bras.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_i;&c_r;&c_k;&c_o;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
