<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zirkoni</rad>/o
</kap>
<drv mrk="zirkoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 40, atompezo 91,22, simbolo Zr. Helgriza substanco kun
      amfotera karaktero.
    </dif>
  </snc>
  <trd lng="de">Zirkonium</trd>
  <trd lng="el">&zeta;&jota;&rho;&kappa;&omikron_ton;&ny;&jota;&omikron;</trd>
  <trd lng="en">zirconium</trd>
  <trdgrp lng="es">
    <trd>circonio</trd>,
    <trd>zirconio</trd>
  </trdgrp>
  <trd lng="hu">cirk&oacute;nium</trd>
  <trd lng="nl">zirkonium</trd>
  <trdgrp lng="pt">
    <trd>zirc&oacute;nio</trd>
    <trd>zirc&ocirc;nio <klr>(bras.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_i;&c_r;&c_k;&c_o;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
