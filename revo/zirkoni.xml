<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>zirkoni</rad>/o
</kap>
<drv mrk="zirkoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero 40, atompezo 91,22, simbolo Zr. Helgriza substanco kun amfotera
karaktero.
    </dif>
  </snc>
<trd lng="de">Zirkonium</trd>
<trd lng="en">zirconium</trd>
<trdgrp lng="es">
   <trd>circonio</trd>, 
   <trd>zirconio</trd>
</trdgrp>
<trd lng="hu">cirk&oacute;nium</trd>
<trd lng="nl">zirkonium</trd>
</drv>
</art>

