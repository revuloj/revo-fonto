  <trd lng="hu">din&aacute;r</trd>
  <trd lng="id">dinar</trd>
  <trd lng="nl">dinar</trd>
  <trd lng="pl">dinar</trd>
  <trd lng="pt">dinar</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_r;</trd>
  <trd lng="sk">din&aacute;r</trd>
</drv>
