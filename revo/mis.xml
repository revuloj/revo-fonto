<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mis.xml,v 1.22 2019/04/10 18:10:18 revo Exp $">
<kap>
  <ofc>1929</ofc>
  <rad>mis</rad>- <fnt><bib>PV</bib></fnt>
</kap>
<subart>
<drv mrk="mis.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="mis.0.KOMUNE">
    <dif>
      Malbone, erare, mal&gcirc;uste, maltrafe, malta&ubreve;ge, ne
      konvene:
      <ekz>
        <ind><tld/>apliko</ind>,
        <trd lng="en">misapplication</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_je;
          &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>a&ubreve;di</ind>,
        <trd lng="en">mishear</trd>
        <trd lng="nl">verkeerd verstaan</trd>
        <trd
        lng="ru">&c_o;&c_s;&c_l;&c_y;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </ekz>
      <ekz>
        <ind><tld/>citi</ind>,
        <trd lng="en">misquote</trd>
      </ekz>
      <ekz>
        <tld/>edzi&gcirc;o,
      </ekz>
      <ekz>
        <ind><tld/>formi</ind>,
        <trd lng="en">misform</trd>
      </ekz>
      <ekz>
        <ind><tld/>gvidi</ind>,
        <trd lng="en">misguide</trd>
      </ekz>
      <ekz>
        <ind><tld/>informi</ind>,
        <trd lng="en">misinform</trd>
        <trd lng="nl">verkeerd informeren</trd>
      </ekz>
      <ekz>
        <ind><tld/>kalkulo</ind>,
        <trd lng="en">miscalculation</trd>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_ch;&c_jo;&c_t;</trd>
      </ekz>
      <ekz>
        <ind><tld/>uzi</ind>,
        <trd lng="en">misuse</trd>
        <trd lng="hu">vissza&eacute;l</trd>
        <trd lng="nl">misbruiken</trd>
        <trd lng="pt">prefixo que indica <ind>mal</ind></trd>
        <trdgrp lng="ru">
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>,
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;
          &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;
          &c_n;&c_je; &c_p;&c_o;
          &c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_ju;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>komposto</ind>,
        <trd lng="hu">szed&eacute;shiba</trd>
        <trd lng="nl">zetfout</trd>
        <trd lng="ru">&c_o;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
      </ekz>
      <ekz>
        <ind><tld/>kompreno</ind>,
        <trd lng="en">misunderstanding</trd>
        <trd lng="hu">f&eacute;lre&eacute;rt&eacute;s</trd>
        <trd lng="nl">misverstand</trd>
        <trd
        lng="ru">&c_n;&c_je;&c_d;&c_o;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
      <ekz>
        <ind><tld/>pa&scirc;o</ind>,
        <trd lng="en">misstep</trd>
        <trd lng="nl">misstap</trd>
        <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;
        &c_sh;&c_a;&c_g;</trd>
      </ekz>
      <ekz>
        <ind><tld/>konduto</ind>,
        <trd lng="en">misconduct</trd>
      </ekz>
      <ekz>
        <ind><tld/>loki</ind>, 
        <trd lng="en">mislocate</trd>
      </ekz>
      <ekz>
        <ind><tld/>nomi</ind>,
        <trd lng="en">misname</trd>
      </ekz>
      <ekz>
        <ind><tld/>ortografii</ind>,
        <trd lng="en">misspell</trd>
      </ekz>
      <ekz>
        <ind><tld/>prononci</ind>,
        <trd lng="en">mispronounce</trd>
        <trd lng="nl">verkeerd uitspreken</trd>
      </ekz>
      <ekz>
        <tld/>raporto, <tld/>regi,
      </ekz>
      <ekz>
        <ind><tld/>traduki</ind>;
        <trd lng="en">mistranslate</trd>
      </ekz>
      <ekz>
        li interpretis la tekston tute <ind><tld/>e</ind>.
        <trdgrp lng="hu">
          <trd>t&eacute;vesen</trd>,
          <trd>hib&aacute;san</trd>,
          <trd>helytelen&uuml;l</trd>,
          <trd>rosszul</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_o;</trd>,
          <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fusx.0i">fu&scirc;i</ref>,
      <ref cel="lam">lame</ref>
    </refgrp>
  </snc>
  <trd lng="en">mis-</trd>
  <trdgrp lng="fr">
    <trd>mal-<klr> (pr&eacute;fixe d'erreur)</klr></trd>,
    <trd>mis-<klr> (pr&eacute;fixe d'erreur)</klr></trd>
  </trdgrp>
  <trd lng="io">mis-</trd>
  <trd lng="nl">mis-</trd>
</drv>
</subart>

<subart>
  <dif>
    Samsignifa memstara vortero.
  </dif>
  <drv mrk="mis.0e">
    <kap><tld/>e</kap>
    <snc mrk="mis.0e.KOMUNE">
      <dif>
        Malkonvene, malbone:
        <ekz>
          hidropso, kiu membrojn <tld/>e formas<fnt>K</fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="cs">
      <trd>chybn&ecaron;</trd>,
      <trd>nespr&aacute;vn&ecaron;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>amiss</trd>,
      <trd>faultily</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>err&oacute;neamente</trd>,
      <trd>incorrectamente</trd>,
      <trd>imperfectamente</trd>
    </trdgrp>
    <trd lng="fr">de travers</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_h;&c_o;</trd>,
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_o;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>chybne</trd>,
      <trd>nespr&aacute;vne</trd>
    </trdgrp>
  </drv>

  <drv mrk="mis.0a">
    <kap><tld/>a</kap>
    <snc mrk="mis.0a.KOMUNE">
      <dif>
        Malkonvena, malbona, maltrafa, malperfekta:
      </dif>
      <ekz>
        <tld/>a &jcirc;eto de lanco.
      </ekz><ekz>
        abrupte li devis forlasi tiun &ccirc;i <tld/>an mondon, pro
        akcidento, kiu mortigis anka&ubreve; lian edzinon
        <fnt>
          <aut>Stefan MAUL</aut>: <vrk>A&ubreve;toroj</vrk>.
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </snc>
    <trdgrp lng="cs">
      <trd>chybn&yacute;</trd>,
      <trd>myln&yacute;</trd>,
      <trd>nezda&rcaron;en&yacute;</trd>
    </trdgrp>
    <trd lng="en">amiss</trd>
    <trdgrp lng="es">
      <trd>incorrecto</trd>,
      <trd>err&oacute;neo</trd>,
      <trd>malo</trd>,
      <trd>desacertado</trd>,
      <trd>imperfecto</trd>
    </trdgrp>
    <trd lng="fr">rat&eacute;</trd>
    <trdgrp lng="sk">
      <trd>chybn&yacute;</trd>,
      <trd>myln&yacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_h;&c_o;&c_j;</trd>,
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </drv>

  <drv mrk="mis.0ajxo">
    <kap><tld/>a&jcirc;o, <var>
        <kap><tld/>o</kap>
      </var>
    </kap>
    <snc>
      <dif>
        Io <tld/>a, malkonvena, malbona, maltrafa, malfeli&ccirc;a:
        <ekz>
          necesos nur malgranda <tld/>a&jcirc;o - kelkaj malbonaj
          rikoltoj, ekzemple, a&ubreve; ekapero de bovinmalsano -
          por ke
          la tuta afero ekflagru denove
          <fnt>
            <aut>A. L&ouml;wenstein</aut>: <vrk>La &scirc;tona
            urbo</vrk>.
            <lok>FEL, 2000.</lok>
          </fnt>;
        </ekz><ekz>
          &gcirc;i <klr>(la akcidento)</klr> okazis <klr>(...)</klr>
          sur
          la flughaveno de Tenerifo rekte sur la alteri&gcirc;ejo
          kaj
          estis &mdash; post &ccirc;eno de <tld/>a&jcirc;oj &mdash;
          la rezulto de radiofonia
          <tld/>funkcio pro du samfrekvencaj elsendadoj
          <fnt>
            <aut>PEJNO Simono</aut>:
            <vrk>&Ccirc;u ni mortu miskomprene? (2)</vrk>.
            <bib>Monato</bib>.
          </fnt>
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="es">
      <trd>error</trd>,
      <trd>fallo</trd>
    </trdgrp>
    <trd lng="fr">action <ind>rat&eacute;e</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_h;</trd>,
      <trd>&c_s;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_je;&c_d;&c_a;</trd>
    </trdgrp>
  </drv>
</subart>
</art>
