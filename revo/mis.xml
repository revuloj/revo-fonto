<subart>
  <dif>
    Samsignifa memstara vortero.
  </dif>
<drv mrk="mis.0a">
  <kap><tld/>a</kap>
  <snc mrk="mis.0a.KOMUNE">
    <dif>
      <ref tip="sin" cel="konven.mal0a">Malkonvena</ref>, <ref tip="sin" cel="bon.mal0a">malbona</ref>, 
      mal<ref tip="ant" cel="traf.0a">trafa</ref>, mal<ref tip="ant" cel="perfek.0a.siaspece">perfekta</ref>:
      <ekz>
        la tuta afero aperis fu&scirc;a-<tld/>a, kiel melodramo odoranta falsecon
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        abrupte li devis forlasi tiun &ccirc;i <tld/>an mondon, pro
        akcidento
        <fnt>
          <aut>Stefan Maul</aut>: <vrk>A&ubreve;toroj</vrk>.
          <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Azazello</nom> ironie &jcirc;etis sur &scirc;in oblikvan rigardon de sia <tld/>a okulo
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        ununura aminda esta&jcirc;o en la <tld/>a familio
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        la aliaj estas malbonaj kaj <tld/>aj, se &ccirc;i tiu civito estas &gcirc;usta
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        esploristoj de la naturaj sciencoj prezentas mirindan akumula&jcirc;on de <tld/>aj
        kalkuloj, malgravaj malkovroj kaj trompaj evidenta&jcirc;oj
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>chybn&yacute;</trd>,
    <trd>myln&yacute;</trd>,
    <trd>nezda&rcaron;en&yacute;</trd>
  </trdgrp>
  <trd lng="en">amiss</trd>
  <trdgrp lng="es">
    <trd>incorrecto</trd>,
    <trd>err&oacute;neo</trd>,
    <trd>malo</trd>,
    <trd>desacertado</trd>,
    <trd>imperfecto</trd>
  </trdgrp>
  <trd lng="fr">rat&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x9593;&#x9055;&#x3063;&#x305F;  <pr>&#x307E;&#x3061;&#x304C;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x5931;&#x6557;&#x306E;  <pr>&#x3057;&#x3063;&#x3071;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x4E0D;&#x5F53;&#x306A;  <pr>&#x3075;&#x3068;&#x3046;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nie w porz&aogonek;dku</trd>,
    <trd>niedobry</trd>,
    <trd>chybiony</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>nu corect</trd>,
    <trd>gre&#x219;it</trd>,
    <trd>ratat</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_h;&c_o;&c_j;</trd>,
    <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chybn&yacute;</trd>,
    <trd>myln&yacute;</trd>
  </trdgrp>    
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_m;&c_i;&c_l;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x5BF9; <pr>b&#xF9;du&#xEC;</pr></trd>,
    <trd>&#x4E0D;&#x5C0D; <pr>b&#xF9;du&#xEC;</pr></trd>,
    <trd>&#x4E0D;&#x5BF9;&#x5934; <pr>b&#xF9;du&#xEC;t&#xF3;u</pr></trd>,
    <trd>&#x4E0D;&#x5C0D;&#x982D; <pr>b&#xF9;du&#xEC;t&#xF3;u</pr></trd>,
    <trd>&#x4E0D;&#x662F;&#x5473;&#x513F; <pr>b&#xF9;sh&#xEC;w&#xE8;ir</pr></trd>,
    <trd>&#x4E0D;&#x662F;&#x5473;&#x5152; <pr>b&#xF9;sh&#xEC;w&#xE8;ir</pr></trd>
  </trdgrp>
</drv><drv mrk="mis.0e">
  <kap><tld/>e</kap>
  <snc mrk="mis.0e.KOMUNE">
    <dif>
      <ref tip="vid" cel="gxust.mal0a">Mal&gcirc;uste</ref>, 
      mal<ref tip="ant" cel="konven.0e">konvene</ref>, 
      mal<ref tip="ant" cel="bon.0e">bone</ref>:
      <ekz>
        mia koro kapriolis fu&scirc;e-<tld/>e, jen haltis, jen galopis
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io impresas <tld/>e
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> parkeras komercajn terminojn, <klr>[&#x2026;]</klr> kaj
        &ccirc;e &ccirc;iu okazo li uzas ilin, &gcirc;uste a&ubreve; <tld/>e
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la gajulo <klr>[&#x2026;]</klr> <tld/>e parolis la anglan
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri mi estas <tld/>e kaj mi malvolas vidi vin pereanta pro mi
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        kulpis ne la vetero, sed <tld/>e administrataj akvocentraloj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi/pg</aut>
          <vrk><url ref="&Monato;/2010/009968.php">Inundo da kritiko</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>chybn&ecaron;</trd>,
    <trd>nespr&aacute;vn&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amiss</trd>,
    <trd>faultily</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>err&oacute;neamente</trd>,
    <trd>incorrectamente</trd>,
    <trd>imperfectamente</trd>
  </trdgrp>
  <trd lng="fr">de travers</trd>
  <trdgrp lng="ja">
    <trd>&#x9593;&#x9055;&#x3063;&#x3066;  <pr>&#x307E;&#x3061;&#x304C;&#x3063;&#x3066;</pr></trd>,
    <trd>&#x8AA4;&#x3063;&#x3066;  <pr>&#x3042;&#x3084;&#x307E;&#x3063;&#x3066;</pr></trd>,
    <trd>&#x4E0D;&#x5F53;&#x306B;  <pr>&#x3075;&#x3068;&#x3046;&#x306B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&lstroke;&eogonek;dnie</trd>,
    <trd>omy&lstroke;kowo</trd>,
    <trd>niew&lstroke;a&sacute;ciwie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>gre&#x219;it</trd>,
    <trd>&icirc;n mod eronat</trd>,
    <trd>din greseala</trd>,
    <trd>necorespunz&abreve;tor</trd>  
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_h;&c_o;</trd>,
    <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chybne</trd>,
    <trd>nespr&aacute;vne</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_m;&c_i;&c_l;&c_k;&c_o;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="mis.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="M"/>e fari, agi, trafi ktp:
      <ekz>
        la maljuna damo de la titolo suspektas, ke la murdisto <tld/>is kaj origine
        celis &scirc;in
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 6 (104)</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la tago li por &ccirc;iam ion <tld/>is, ke nun la <tld/>on li volas &gcirc;ustigi per
        agoj bagatelaj, vantaj kaj <klr>[&#x2026;]</klr> tro malfruaj
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la klarigoj estas perfektaj, nur specifa noto iomete <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010411.php">La gramatikiloj de Esperanto la&ubreve; ABC</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;rinde tiu &ccirc;i priskribita esperantista lingvo-elito iam kaj iam <tld/>as en
        sia klopodo paroli Esperanton
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2013/011040.php">Toter Lehrer, guter Lehrer</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia unua vizito al Kaliningrad en 1994 estis fiasko, preska&ubreve; &ccirc;io <tld/>is
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116; 8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;irejon fine de tiu rekta linio la veturilo <tld/>is kaj &gcirc;i falis frakasi&gcirc;e
        <fnt>
          <bib>ChB</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluraj membroj de la du regantaj partioj en la irlanda koalicio konsentas, ke la
        kandidatigo de <nom>O'Flaherty</nom> <tld/>as
        <fnt>
          <aut>Garbhan MacAoidh/pg</aut>
            <vrk><url ref="&Monato;/2000/007134.php">Malbona investo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 12</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erar.0i">erari</ref>,
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="fusx.0i">fu&scirc;i</ref>,
      <ref cel="traf.mal0i">maltrafi</ref>,
      <ref cel="far.mis0i">misfari</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>verfehlen</trd>,
    <trd><ind>schief</ind> laufen</trd>
  </trdgrp>
  <trd lng="en">fail</trd>
  <trdgrp lng="zh">
    <trd>&#x601D;&#x604B; <pr>s&#x12B;li&#xE0;n</pr></trd>,
    <trd>&#x601D;&#x6200; <pr>s&#x12B;li&#xE0;n</pr></trd>,
    <trd>&#x672A;&#x6253;&#x4E2D; <pr>w&#xE8;id&#x1CE;zh&#x14D;ng</pr></trd>,
    <trd>&#x6F0F;&#x9776; <pr>l&#xF2;ub&#x1CE;</pr></trd>
  </trdgrp>
</drv>














  <drv mrk="mis.0ajxo">
    <kap><tld/>a&jcirc;o, <var>
        <kap><tld/>o</kap>
      </var>
    </kap>
    <snc>
      <dif>
        Io <tld/>a, malkonvena, malbona, maltrafa, malfeli&ccirc;a:
        <ekz>
          necesos nur malgranda <tld/>a&jcirc;o - kelkaj malbonaj
          rikoltoj, ekzemple, a&ubreve; ekapero de bovinmalsano -
          por ke
          la tuta afero ekflagru denove
          <fnt>
            <aut>A. L&ouml;wenstein</aut>: <vrk>La &scirc;tona
            urbo</vrk>.
            <lok>FEL, 2000.</lok>
          </fnt>;
        </ekz><ekz>
          &gcirc;i <klr>(la akcidento)</klr> okazis <klr>(...)</klr>
          sur
          la flughaveno de Tenerifo rekte sur la alteri&gcirc;ejo
          kaj
          estis &mdash; post &ccirc;eno de <tld/>a&jcirc;oj &mdash;
          la rezulto de radiofonia
          <tld/>funkcio pro du samfrekvencaj elsendadoj
          <fnt>
            <aut>PEJNO Simono</aut>:
            <vrk>&Ccirc;u ni mortu miskomprene? (2)</vrk>.
            <bib>Monato</bib>.
          </fnt>
        </ekz>
      </dif>
    </snc>
    <trd lng="bo">&t_sa;&t_kata;&t_yata;&t_naro;&t_na;&t_tsheg;</trd>
  <trdgrp lng="es">
      <trd>error</trd>,
      <trd>fallo</trd>
    </trdgrp>
    <trd lng="fr">action <ind>rat&eacute;e</ind></trd>
    <trdgrp lng="ja">
    <trd>&#x9593;&#x9055;&#x3044;  <pr>&#x307E;&#x3061;&#x304C;&#x3044;</pr></trd>,
    <trd>&#x30DF;&#x30B9;  <pr>&#x307F;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>gafa</trd>,
      <trd>g&lstroke;upi b&lstroke;&aogonek;d</trd>,
      <trd>wpadka</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>gaf&abreve;</trd>,
      <trd>gre&#x219;eal&abreve; prosteasc&abreve;</trd>
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_h;</trd>,
      <trd>&c_s;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_je;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_m;&c_i;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_h;</trd>
  </trdgrp>
</drv>
