<drv mrk="mis.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="mis.0.KOMUNE">
    <dif>
      <ref cel="bon.mal0a">Malbone</ref>, <ref cel="erar.0a">erare</ref>, 
      <ref cel="gxust.mal0a">mal&gcirc;uste</ref>, maltrafe, <ref cel="tauxg.mal0a">malta&ubreve;ge</ref>, 
      <ref cel="konven.mal0a">malkonvene</ref>:
      <ekz>
        <ind><tld/>apliko</ind>,
        <trd lng="en">misapplication</trd>
        <trd lng="pl">niew&lstroke;a&sacute;ciwe zastosowanie</trd>
        <trd lng="ro">aplicarea incorect&abreve;</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_je;
          &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>      
      <ekz>
        <ref cel="auxd.mis0i"><tld/>a&ubreve;di</ref>,
        <ref cel="kompre.mis0o">miskompreno</ref>;
      </ekz>
      <ekz>
        <ind><tld/>edzi&gcirc;o</ind>;
        <trd lng="pl">mezalians</trd>
        <trd lng="ro">mezalian&#x21B;&abreve;</trd>
      </ekz>      
      <ekz>
        <ref cel="cit.mis0i"><tld/>citi</ref>,
        <ref cel="inform.mis0i">misinformi</ref>,
        <ref cel="nom.mis0i"><tld/>nomi</ref>,        
        <ref cel="raport.mis0o"><tld/>raporto</ref>;
      </ekz>     
      <ekz>       
        <ref cel="pronon.mis0i"><tld/>prononci</ref>,        
        <ref cel="ortogr.mis0i"><tld/>ortografii</ref>,
        <ref cel="skrib.mis0i">misskribi</ref>,
        <ref cel="traduk.mis0i"><tld/>traduki</ref>,   
        <ref cel="kompos.mis0ajxo"><tld/>komposta&jcirc;o</ref>;         
      </ekz>  
      <ekz>
        <ref cel="form.mis0i"><tld/>formi</ref>,
         <ref cel="gvid.mis0i"><tld/>gvidi</ref>,
        <ref cel="kalkul.mis0i"><tld/>kalkuli</ref>,
        <ref cel="kondut.mis0i"><tld/>konduti</ref>,
        <ref cel="lok.mis0i"><tld/>loki</ref>, 
        <ref cel="reg.mis0i"><tld/>regi</ref>,
        <ref cel="uz.mis0i"><tld/>uzi</ref>,
        <ref cel="pasx.mis0o">mispa&scirc;o</ref>.
      </ekz>
