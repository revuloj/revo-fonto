<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mis.xml,v 1.24 2021/05/28 12:15:11 revo Exp $">
<kap>
  <ofc>1929</ofc>
  <rad>mis</rad>- <fnt><bib>PV</bib></fnt>
</kap>
<subart>
<drv mrk="mis.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="mis.0.KOMUNE">
    <dif>
      Malbone, erare, mal&gcirc;uste, maltrafe, malta&ubreve;ge, ne
      konvene:
      <ekz>
        <ind><tld/>apliko</ind>,
        <trd lng="en">misapplication</trd>
        <trd lng="pl">niew&lstroke;a&sacute;ciwe zastosowanie</trd>
        <trd lng="ro">aplicarea incorect&abreve;</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_je;
          &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>a&ubreve;di</ind>,
        <trd lng="en">mishear</trd>
        <trd lng="nl">verkeerd verstaan</trd>
        <trdgrp lng="pl">
          <trd>przes&lstroke;ysze&cacute; si&eogonek;</trd>,
          <trd>&zacute;le us&lstroke;ysze&cacute;</trd>
        </trdgrp>
        <trdgrp lng="ro">
          <trd>auzit gre&#x219;it</trd>,
          <trd>auzi prost</trd>
        </trdgrp>
        <trd lng="ru">&c_o;&c_s;&c_l;&c_y;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </ekz>
      <ekz>
        <ind><tld/>citi</ind>,
        <trd lng="en">misquote</trd>
        <trd lng="pl">&zacute;le zacytowa&cacute;</trd>
        <trd lng="ro">citat gresit</trd>
      </ekz>
      <ekz>
        <ind><tld/>edzi&gcirc;o</ind>,
        <trd lng="pl">mezalians</trd>
        <trd lng="ro">mezalian&tcedil;&abreve;</trd>
      </ekz>
      <ekz>
        <ind><tld/>formi</ind>,
        <trd lng="en">misform</trd>
        <trdgrp lng="pl">
          <trd>oszpeca&cacute;</trd>,
          <trd>zniekszta&lstroke;ca&cacute;</trd>
        </trdgrp>
        <trdgrp lng="ro">
          <trd>desfigura</trd>,
          <trd>denatura</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>gvidi</ind>,
        <trd lng="en">misguide</trd>
        <trd lng="pl">prowadzi&cacute; &zacute;le</trd>
        <trd lng="ro">conduce prost</trd>
      </ekz>
      <ekz>
        <ind><tld/>informi</ind>,
        <trd lng="en">misinform</trd>
        <trd lng="nl">verkeerd informeren</trd>
        <trd lng="pl">wprowadza&cacute; w b&lstroke;&aogonek;d</trd>
        <trd lng="ro">induce &icirc;n eroare</trd>
      </ekz>
      <ekz>
        <ind><tld/>kalkulo</ind>,
        <trd lng="en">miscalculation</trd>
        <trd lng="pl">przeliczenie si&eogonek;</trd>
        <trd lng="ro">gre&#x219;eal&abreve; de calcul</trd>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_ch;&c_jo;&c_t;</trd>
      </ekz>
      <ekz>
        <ind><tld/>uzi</ind>,
        <trd lng="en">misuse</trd>
        <trd lng="hu">vissza&eacute;l</trd>
        <trd lng="nl">misbruiken</trd>
        <trdgrp lng="pl">
          <trd>nadu&zdot;ywa&cacute;</trd>,
          <trd>u&zdot;ywa&cacute; w spos&oacute;b niew&lstroke;a&sacute;ciwy</trd>,
          <trd>zmarnowa&cacute;</trd>
        </trdgrp> 
        <trd lng="pt">prefixo que indica <ind>mal</ind></trd>
        <trdgrp lng="ro">
          <trd>abuza</trd>,
          <trd>abuz</trd>,
          <trd>risip&abreve;</trd>
        </trdgrp> 
        <trdgrp lng="ru">
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>,
          <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;
          &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_n;&c_je; &c_p;&c_o; &c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_ju;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>komposto</ind>,
        <trd lng="hu">szed&eacute;shiba</trd>
        <trd lng="nl">zetfout</trd>
        <trd lng="pl">niew&lstroke;a&sacute;ciwa kompozycja</trd>
        <trd lng="ro">compozi&#x21B;ie gre&#x219;it&abreve;</trd>
        <trd lng="ru">&c_o;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
      </ekz>
      <ekz>
        <ind><tld/>kompreno</ind>,
        <trd lng="en">misunderstanding</trd>
        <trd lng="hu">f&eacute;lre&eacute;rt&eacute;s</trd>
        <trd lng="nl">misverstand</trd>
        <trdgrp lng="pl">
          <trd>nieporozumienie</trd>,
          <trd>scysja</trd>,
          <trd>niesnaska</trd>
        </trdgrp>   
        <trdgrp lng="ro">
          <trd>confuzie</trd>,
          <trd>ne&icirc;n&#x21B;elegere</trd>,
          <trd>ceart&abreve;</trd>,
          <trd>diferend</trd>
        </trdgrp>   
        <trd lng="ru">&c_n;&c_je;&c_d;&c_o;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
      <ekz>
        <ind><tld/>pa&scirc;o</ind>,
        <trd lng="en">misstep</trd>
        <trd lng="nl">misstap</trd>
        <trdgrp lng="pl">
          <trd>dewiacja</trd>,
          <trd>odchylenie</trd>
        </trdgrp>
        <trdgrp lng="ro">
          <trd>deviatie</trd>,
          <trd>divergen&#x21B;&abreve;</trd>
        </trdgrp>
        <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;
        &c_sh;&c_a;&c_g;</trd>
      </ekz>
      <ekz>
        <ind><tld/>konduto</ind>,
        <trd lng="en">misconduct</trd>
        <trdgrp lng="pl">
          <trd>post&eogonek;pek</trd>,
          <trd>z&lstroke;e prowadzenie si&eogonek;</trd>
        </trdgrp>
        <trdgrp lng="ro">
          <trd>act</trd>,
          <trd>abatere</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>loki</ind>, 
        <trd lng="en">mislocate</trd>
        <trdgrp lng="pl">
          <trd>zawieruszy&cacute;</trd>,
          <trd>umie&sacute;ci&cacute; w z&lstroke;ym miejscu</trd>,
          <trd>zaprzepa&sacute;ci&cacute;</trd>,
          <trd>zapodzia&cacute;</trd>
         </trdgrp> 
         <trdgrp lng="ro">
           <trd>r&abreve;t&abreve;ci</trd>,
           <trd>pus &icirc;ntr-un loc gre&#x219;it</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>nomi</ind>,
        <trd lng="en">misname</trd>
        <trd lng="pl">&zacute;le nazwa&cacute;</trd>
        <trd lng="ro">numi&#x21B;i-l gre&#x219;it</trd>
      </ekz>
      <ekz>
        <ind><tld/>ortografii</ind>,
        <trd lng="en">misspell</trd>
        <trd lng="pl">b&lstroke;&eogonek;dnie pisa&cacute;</trd>
        <trd lng="ro">scris gre&#x219;it</trd>
      </ekz>
      <ekz>
        <ind><tld/>prononci</ind>,
        <trd lng="en">mispronounce</trd>
        <trd lng="nl">verkeerd uitspreken</trd>
        <trd lng="pl">wymawia&cacute; b&lstroke;&eogonek;dnie</trd>
        <trd lng="ro">pronun&#x21B;&abreve; incorect</trd>
      </ekz>
      <ekz>
        <ind><tld/>raporto</ind>,
        <trd lng="pl">dezinformacja</trd>
        <trd lng="ro">dezinformare</trd>
      </ekz>
      <ekz>
        <ind><tld/>regi</ind>,
        <trd lng="pl">&zacute;le rz&aogonek;dzi&cacute;</trd>
        <trd lng="ro">proast&abreve; guvernare</trd>
      </ekz>
      <ekz>
        <ind><tld/>traduki</ind>;
        <trd lng="en">mistranslate</trd>
        <trd lng="pl">&zacute;le przet&lstroke;umaczy&cacute;</trd>
        <trd lng="ro">traduce gre&scedil;it</trd>
      </ekz>
      <ekz>
        li interpretis la tekston tute <ind><tld/>e</ind>.
        <trdgrp lng="hu">
          <trd>t&eacute;vesen</trd>,
          <trd>hib&aacute;san</trd>,
          <trd>helytelen&uuml;l</trd>,
          <trd>rosszul</trd>
        </trdgrp>
        <trd lng="pl">b&lstroke;&eogonek;dnie</trd>
        <trdgrp lng="ru">
          <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_o;</trd>,
          <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fusx.0i">fu&scirc;i</ref>,
      <ref cel="lam">lame</ref>
    </refgrp>
  </snc>
  <trd lng="en">mis-</trd>
  <trdgrp lng="fr">
    <trd>mal-<klr> (pr&eacute;fixe d'erreur)</klr></trd>,
    <trd>mis-<klr> (pr&eacute;fixe d'erreur)</klr></trd>
  </trdgrp>
  <trd lng="io">mis-</trd>
  <trd lng="nl">mis-</trd>
  <trd lng="pl">prefiks oznaczaj&aogonek;cy &zacute;le albo b&lstroke;&eogonek;dnie</trd>
  <trd lng="ro">prefix care &icirc;nseamn&abreve; gre&#x219;it</trd>
</drv>
</subart>

<subart>
  <dif>
    Samsignifa memstara vortero.
  </dif>
  <drv mrk="mis.0e">
    <kap><tld/>e</kap>
    <snc mrk="mis.0e.KOMUNE">
      <dif>
        Malkonvene, malbone:
        <ekz>
          hidropso, kiu membrojn <tld/>e formas<fnt>K</fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="cs">
      <trd>chybn&ecaron;</trd>,
      <trd>nespr&aacute;vn&ecaron;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>amiss</trd>,
      <trd>faultily</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>err&oacute;neamente</trd>,
      <trd>incorrectamente</trd>,
      <trd>imperfectamente</trd>
    </trdgrp>
    <trd lng="fr">de travers</trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;&eogonek;dnie</trd>,
      <trd>omy&lstroke;kowo</trd>,
      <trd>niew&lstroke;a&sacute;ciwie</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>gre&#x219;it</trd>,
      <trd>&icirc;n mod eronat</trd>,
      <trd>din greseala</trd>,
      <trd>necorespunz&abreve;tor</trd>  
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_h;&c_o;</trd>,
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_o;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>chybne</trd>,
      <trd>nespr&aacute;vne</trd>
    </trdgrp>
  </drv>

  <drv mrk="mis.0a">
    <kap><tld/>a</kap>
    <snc mrk="mis.0a.KOMUNE">
      <dif>
        Malkonvena, malbona, maltrafa, malperfekta:
      </dif>
      <ekz>
        <tld/>a &jcirc;eto de lanco.
      </ekz><ekz>
        abrupte li devis forlasi tiun &ccirc;i <tld/>an mondon, pro
        akcidento, kiu mortigis anka&ubreve; lian edzinon
        <fnt>
          <aut>Stefan MAUL</aut>: <vrk>A&ubreve;toroj</vrk>.
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </snc>
    <trdgrp lng="cs">
      <trd>chybn&yacute;</trd>,
      <trd>myln&yacute;</trd>,
      <trd>nezda&rcaron;en&yacute;</trd>
    </trdgrp>
    <trd lng="en">amiss</trd>
    <trdgrp lng="es">
      <trd>incorrecto</trd>,
      <trd>err&oacute;neo</trd>,
      <trd>malo</trd>,
      <trd>desacertado</trd>,
      <trd>imperfecto</trd>
    </trdgrp>
    <trd lng="fr">rat&eacute;</trd>
    <trdgrp lng="pl">
      <trd>nie w porz&aogonek;dku</trd>,
      <trd>niedobry</trd>,
      <trd>chybiony</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>nu corect</trd>,
      <trd>gre&#x219;it</trd>,
      <trd>ratat</trd>
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_h;&c_o;&c_j;</trd>,
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>chybn&yacute;</trd>,
      <trd>myln&yacute;</trd>
    </trdgrp>    
  </drv>

  <drv mrk="mis.0ajxo">
    <kap><tld/>a&jcirc;o, <var>
        <kap><tld/>o</kap>
      </var>
    </kap>
    <snc>
      <dif>
        Io <tld/>a, malkonvena, malbona, maltrafa, malfeli&ccirc;a:
        <ekz>
          necesos nur malgranda <tld/>a&jcirc;o - kelkaj malbonaj
          rikoltoj, ekzemple, a&ubreve; ekapero de bovinmalsano -
          por ke
          la tuta afero ekflagru denove
          <fnt>
            <aut>A. L&ouml;wenstein</aut>: <vrk>La &scirc;tona
            urbo</vrk>.
            <lok>FEL, 2000.</lok>
          </fnt>;
        </ekz><ekz>
          &gcirc;i <klr>(la akcidento)</klr> okazis <klr>(...)</klr>
          sur
          la flughaveno de Tenerifo rekte sur la alteri&gcirc;ejo
          kaj
          estis &mdash; post &ccirc;eno de <tld/>a&jcirc;oj &mdash;
          la rezulto de radiofonia
          <tld/>funkcio pro du samfrekvencaj elsendadoj
          <fnt>
            <aut>PEJNO Simono</aut>:
            <vrk>&Ccirc;u ni mortu miskomprene? (2)</vrk>.
            <bib>Monato</bib>.
          </fnt>
        </ekz>
      </dif>
    </snc>
    <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF7C;&#xF53;&#xF0B;</trd>
  <trdgrp lng="es">
      <trd>error</trd>,
      <trd>fallo</trd>
    </trdgrp>
    <trd lng="fr">action <ind>rat&eacute;e</ind></trd>
    <trdgrp lng="pl">
      <trd>gafa</trd>,
      <trd>g&lstroke;upi b&lstroke;&aogonek;d</trd>,
      <trd>wpadka</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>gaf&abreve;</trd>,
      <trd>gre&#x219;eal&abreve; prosteasc&abreve;</trd>
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_h;</trd>,
      <trd>&c_s;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_je;&c_d;&c_a;</trd>
    </trdgrp>
  </drv>
</subart>
</art>
