    <refgrp tip="vid">
      <ref cel="fusx.0i">fu&scirc;i</ref>,
      <ref cel="lam">lame</ref>
    </refgrp>
  </snc>
  <url ref="&AdE;/akademia_vortaro/klarigoj.html#decido-1929">Akademia
  Vortaro, MIS/</url>
  <trd lng="en">mis-</trd>
  <trdgrp lng="fr">
    <trd>mal-<klr> (pr&eacute;fixe d'erreur)</klr></trd>,
    <trd>mis-<klr> (pr&eacute;fixe d'erreur)</klr></trd>
  </trdgrp>
  <trd lng="io">mis-</trd>
  <trd lng="nl">mis-</trd>
  <trd lng="pl">prefiks oznaczaj&aogonek;cy &zacute;le albo b&lstroke;&eogonek;dnie</trd>
  <trd lng="ro">prefix care &icirc;nseamn&abreve; gre&#x219;it</trd>
</drv>
