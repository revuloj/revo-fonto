<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korimb</rad>/o
</kap>
<drv mrk="korimb.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>,
      &ccirc;e kiu la lango de la pedunkloj estas diversa, sed tamen
      la portitaj floroj atingas la saman alton kaj imitas umbelon.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schirmtraube</trd>,
     <trd>Doldentraube</trd>
  </trdgrp>
  <trd lng="en">corymb</trd>
  <trd lng="ru">&c_shch;&c_i;&c_t;&c_o;&c_k; <klr>(&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="korimb.0a">
  <kap>
    <tld/>a
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      (pri floroj). Kreskanta <tld/>e.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schirmtraubig</trd>,
     <trd>doldentraubig</trd>
  </trdgrp>
</drv>

<drv mrk="korimb.0forma">
  <kap>
    <tld/>forma
  </kap>
  <snc>
    <dif>
      Havanta formon de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schirmtraubenf&ouml;rmig</trd>,
     <trd>doldentraubenf&ouml;rmig</trd>
  </trdgrp>
</drv>

<drv mrk="korimb.0porta">
  <kap>
    <tld/>(oport)a
  </kap>
  <snc>
    <dif>
      Havanta florojn la&ubreve; <tld/>a aran&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schirmtraubentragend</trd>,
     <trd>doldentraubentragend</trd>
  </trdgrp>
</drv>

</art>
