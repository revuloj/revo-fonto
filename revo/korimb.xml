<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korimb.xml,v 1.19 2023/10/18 17:23:25 revo Exp $">
<kap>
  <rad>korimb</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korimb.0o">
  <kap><tld/>o</kap>
  <snc mrk="korimb.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="flor.0aro">Floraro</ref>,
      &ccirc;e kiu la longo de la pedunkloj estas diversa, sed tamen
      la portataj floroj atingas la saman alton kaj similas umbelon:
      <ekz>
        akileo <klr>[&#8230;]</klr>: la floroj, blankaj, foje rozkoloraj a&ubreve; purpuraj, grupi&gcirc;as en blankaj
        kapituloj, 4-7&nbsp;mm lar&gcirc;aj, kiuj formas <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10914">Akileo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grapolo povas esti en kunmetitaj floraroj, kiel la grapolaro (a&ubreve; paniklo),
        kiu estas grapolo de grapoletoj, se la floroj trovi&gcirc;as en unu ebeno, &gcirc;i nomi&gcirc;as <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16573">Grapolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_t;&c_o;&c_k; <klr tip="amb">(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
  <trdgrp lng="cs">
    <trd>chochol&iacute;k</trd>,
    <trd>chochol&iacute;k neprav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Schirmtraube</trd>,
     <trd>Doldentraube</trd>
  </trdgrp>
  <trd lng="en">corymb</trd>
  <trd lng="fr">corymbe</trd>
  <trd lng="hu">s&aacute;torvir&aacute;gzat</trd>
  <trd lng="ja">&#25955;&#x623F;&#x82B1;&#x5E8F;  <pr>&#12373;&#12435;&#x307C;&#12358;&#x304B;&#12376;&#12423;</pr></trd>
  <trd lng="pl">baldachogrono</trd>
  <trd lng="pt">corimbo</trd>
  <trd lng="ro">corimb</trd>
  <trd lng="ru">&c_shch;&c_i;&c_t;&c_o;&c_k; <klr>(&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">chochol&iacute;k</trd>
</drv>

<drv mrk="korimb.0a">
  <kap><tld/>a</kap>
  <snc mrk="korimb.0a.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="klr">(pri floroj)</uzo>
    <dif>
      Kreskanta <tld/>e.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schirmtraubig</trd>,
     <trd>doldentraubig</trd>
  </trdgrp>
  <trd lng="fr">corymbiflore</trd>
  <trd lng="hu">s&aacute;tor alak&uacute; <klr tip="ind">(vir&aacute;gzat)</klr></trd>
</drv>

<drv mrk="korimb.0forma">
  <kap>
    <tld/>forma
  </kap>
  <snc>
    <dif>
      Havanta formon de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schirmtraubenf&ouml;rmig</trd>,
     <trd>doldentraubenf&ouml;rmig</trd>
  </trdgrp>
  <trd lng="fr">corymbiforme</trd>
  <trd lng="hu">s&aacute;tor alak&uacute; <klr tip="ind">(vir&aacute;gzat)</klr></trd>
</drv>

<drv mrk="korimb.0porta">
  <kap><tld/>oporta, <var><kap><tld/>a</kap></var></kap>
  <snc>
    <dif>
      Havanta florojn la&ubreve; <tld/>a aran&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schirmtraubentragend</trd>,
     <trd>doldentraubentragend</trd>
  </trdgrp>
  <trd lng="fr">corymbif&egrave;re</trd>
  <trd lng="hu">s&aacute;torvir&aacute;gzat&uacute;</trd>
  <trd lng="ja">&#25955;&#x623F;&#x82B1;&#x5E8F;&#x306E; <pr>&#12373;&#12435;&#12412;&#12358;&#12363;&#12376;&#12423;&#12398;</pr></trd>
</drv>

</art>
