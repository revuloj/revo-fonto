<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gril</rad>/o</kap>
<drv mrk="gril.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gril.0o.familiano">
    <dif>
      Iu el familio <ref tip="malprt" cel="griled.0oj"><tld/>edoj</ref>:
      <ekz>
        vi povas manĝi la <ref tip="vid" cel="akrid.0o">akridon</ref>
        kun ĝia speco kaj la <ref tip="vid" cel="cikad.0o">cikadon</ref>
        kun ĝia speco kaj la <tld/>on kun ĝia speco
        kaj la <ref tip="vid" cel="lokust.0o">lokuston</ref> kun ĝia
        speco;
        <fnt><bib>MT</bib>, <lok>&Lev; 11:22</lok></fnt>
      </ekz>
      <ekz>
        la strigoj tamburadis, la griloj ĉirpadis
        kaj la nigraj akridoj blovadis en la buŝharmonikon
        <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ia <tld/>o zumas al li en la cerbo <klr>(li estas frenezeta,
        havas strangajn ideojn)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="gril.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="griled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gryllus</trd>),
      vivanta ĉe sunaj deklivoj kaj herbejrandoj en terfendoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>цвыркун</trd>,
    <trd>конік</trd>
  </trdgrp>
  <trd lng="de">Grille</trd>
  <trd lng="en">cricket</trd>
  <trd lng="fr">grillon</trd>
  <trd lng="hu">tücsök</trd>
  <trd lng="nl">krekel</trd>
  <trd lng="pt">grilo</trd>
  <trdgrp lng="ru">
    <trd>сверчок</trd>,
    <trd>кузнечик</trd>
  </trdgrp>
</drv>
</art>
