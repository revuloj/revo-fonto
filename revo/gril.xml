<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gril.xml,v 1.28 2023/11/11 11:36:37 revo Exp $">
<kap><ofc>*</ofc>
<rad>gril</rad>/o <fnt><bib>VdE</bib></fnt></kap>
<drv mrk="gril.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gril.0o.familiano">
    <dif>
      Iu el familio <ref tip="malprt" cel="griled.0oj"><tld/>edoj</ref>:
      <ekz>
        vi povas man&gcirc;i la <ref tip="vid" cel="akrid.0o">akridon</ref>
        kun &gcirc;ia speco kaj la <ref tip="vid" cel="cikad.0o">cikadon</ref>
        kun &gcirc;ia speco kaj la <tld/>on kun &gcirc;ia speco
        kaj la <ref tip="vid" cel="lokust.0o">lokuston</ref> kun &gcirc;ia speco;
        <fnt><bib>MT</bib>, <lok>&Lev; 11:22</lok></fnt>
      </ekz><ekz>
        la strigoj tamburadis, la griloj &ccirc;irpadis
        kaj la nigraj akridoj blovadis en la bu&scirc;harmonikon
        <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        ia <tld/>o zumas al li en la cerbo <klr>(li estas frenezeta,
        havas strangajn ideojn)</klr>;
      </ekz><ekz>
        &leftquot;<ind>La <tld/>o kaj la formiko</ind>&rightquot; <klr>(fablo)</klr>
        <trd lng="ru">&c_S;&c_t;&c_r;&c_je;&c_k;&c_o;&c_z;&c_a; &c_i; &c_m;&c_u;&c_r;&c_a;&c_v;&c_je;&c_j;<klr> (&c_b;&c_a;&c_s;&c_n;&c_ja;)</klr></trd>
        <fnt><aut>&K;</aut>:
          <lok><url ref="https://egalite.hu/kalocsay/ezopo/la%20grilo%20kaj%20la%20formiko.htm">Ezopa
          sa&gcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="gril.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="griled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gryllus</trd>),
      vivanta &ccirc;e sunaj deklivoj kaj herbejrandoj en terfendoj.
    </dif>
    <bld lok="&WCU;/thumb/e/ef/Gryllus_campestris_%2835634613106%29.jpg/640px-Gryllus_campestris_%2835634613106%29.jpg" prm="CC BY-SA 2.0">kampa <tld/>o (Gryllus campestris)
        <fnt>
          <aut>xulescu_g</aut>
          <lok><url ref="&WCW;/File:Gryllus_campestris_(35634613106).jpg">&WCW;/File:Gryllus_campestris_(35634613106).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_v;&c_y;&c_r;&c_k;&c_u;&c_n;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">grill</trd>
  <trd lng="cs">cvr&ccaron;ek</trd>
  <trd lng="de"><ofc>*</ofc>Grille</trd>
  <trd lng="en"><ofc>*</ofc>cricket<klr> (insect)</klr></trd>
  <trd lng="fa">&a_j;&f_y;&a_r;&a_j;&f_y;&a_r;&f_k;</trd>
  <trd lng="fr"><ofc>*</ofc>grillon</trd>
  <trd lng="hu">t&uuml;cs&ouml;k</trd>
  <trd lng="ja">&#x30B3;&#x30AA;&#x30ED;&#x30AE;</trd>
  <trd lng="nl">krekel</trd>
  <trd lng="pl"><ofc>*</ofc>&sacute;wierk</trd>
  <trd lng="pt">grilo</trd>
  <trd lng="ru"><ofc>*</ofc>&c_s;&c_v;&c_je;&c_r;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">svr&ccaron;ek</trd>
  <trd lng="uk">&c_c;&c_v;&c_ib;&c_r;&c_k;&c_u;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x86CC; <pr>g&#x1D4;</pr></trd>,
    <trd>&#x87C0; <pr>shu&#xE0;i</pr></trd>,
    <trd>&#x87CB;&#x87C0; <pr>x&#x12B;shu&#xE0;i</pr></trd>,
    <trd>&#x86D0; <pr>q&#x16B;</pr></trd>,
    <trd>&#x87CB; <pr>x&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
