<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>somal</rad>/o
</kap>

<drv mrk="somal.0ujo">
  <kap>
    <tld lit="S"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="somal.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> en la orienta 
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo 
      Mogadi&scirc;o.
      Teritorio de Respubliko Somalujo.
      ISO-3166 kodo: SO.
      &Gcirc;i limas kun
      <ref tip="vid" cel="etiop.0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="gxibuti.0o.GEOG">&Gcirc;ibutio</ref>,
      <ref tip="vid" cel="kenj.0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="mar.ArabaM0o">Araba Maro</ref>
      kaj Hinda Oceano.
    </dif>
    <trd lng="ar">&a_A;&a_l;&a_S;&a_w;&a_m;&a_A;&a_l;</trd>
    <trd lng="bg">&c_S;&c_o;&c_m;&c_a;&c_l;&c_i;&c_ja;</trd>
    <trd lng="de">Somalia</trd>
    <trd lng="en">Somalia</trd>
    <trd lng="nl">Somali&euml;</trd>
    <trdgrp lng="pl">
      <trd>Somalia</trd>,
      <trd>Republika <ind>Somalijska</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

