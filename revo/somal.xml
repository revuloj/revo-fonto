<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: somal.xml,v 1.17 2023/10/21 07:46:47 revo Exp $">
<kap>
  <ofc>10</ofc>
  <rad>somal</rad>/o
</kap>

<drv mrk="somal.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <snc mrk="somal.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de <ref tip="malprt"
      cel="somal.S0ujo.GEOG"><tld lit="S"/>ujo</ref>:
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> lo&gcirc;as en la Korno de Afriko.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=230090">Somaloj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="somal.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        <tld/>a lingvo <klr>[&#8230;]</klr> apartenas al la orient-ku&scirc;ida lingvaro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=172742">Somala lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_l;&c_ib;&c_je;&c_c;</trd>
  <trd lng="ca">somali</trd>
  <trd lng="de">Somali</trd>
  <trd lng="en">Somali</trd>
  <trd lng="es">somal&iacute;</trd>
  <trdgrp lng="fr">
    <trd>Somalien</trd>,
    <trd>Somalienne</trd>
  </trdgrp>
  <trd lng="ja">&#x30BD;&#x30DE;&#x30EA;&#x4EBA;  <pr>&#x30BD;&#x30DE;&#x30EA;&#12376;&#12435;</pr></trd>
  <trd lng="nl">Somali&euml;r</trd>
  <trd lng="pl">Somalijczyk</trd>
  <trdgrp lng="pt">
    <trd>somali</trd>,
    <trd>somaliano</trd>,
    <trd>somaliana</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#x43E;&#x43C;&c_a;&#x43B;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_i;&c_j; &c_sh;&c_i;&#x43B;&c_ib;&#x43D;&c_gu;</trd>
</drv>

<drv mrk="somal.S0ujo">
  <kap>
    <ofc>10</ofc><tld lit="S"/>ujo
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">SO</mlg>
  <snc mrk="somal.S0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> en la orienta
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo
      Mogadi&scirc;o; teritorio de Respubliko <tld lit="S"/>ujo,
      limanta kun
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="gxibuti.GX0o.GEOG">&Gcirc;ibutio</ref>,
      <ref tip="vid" cel="kenj.K0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="mar.ArabaM0o">Araba Maro</ref>
      kaj Hinda Oceano:
      <ekz>
        la oroma lingvo estas parolata en Etiopujo, Kenjo kaj <tld lit="S"/>ujo de pli ol 5 milionoj
        da parolantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=172747">Oroma lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_S;&c_a;&c_m;&c_a;&c_l;&c_ib;</trd>
    <trd lng="ar">&a_A;&a_l;&a_S;&a_w;&a_m;&a_A;&a_l;</trd>
    <trd lng="bg">&c_S;&c_o;&c_m;&c_a;&c_l;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Som&agrave;lia</trd>,
      <trd>Rep&uacute;blica Democr&agrave;tica Somali</trd>
    </trdgrp>
    <trd lng="de">Somalia</trd>
    <trd lng="en">Somalia</trd>
    <trdgrp lng="fr">
      <trd>Somalie</trd>,
      <trd>R&eacute;publique f&eacute;d&eacute;rale de Somalie</trd>
    </trdgrp>
    <trd lng="id">Somalia</trd>
    <trd lng="nl">Somali&euml;</trd>
    <trdgrp lng="pl">
      <trd>Somalia</trd>,
      <trd>Republika <ind>Somalijska</ind></trd>
    </trdgrp>
</drv>

</art>

