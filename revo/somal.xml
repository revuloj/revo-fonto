<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>somal</rad>/o
</kap>

<drv mrk="somal.0o">
  <kap><tld/>o</kap>
  <snc mrk="somal.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de <ref
      tip="malprt" cel="somal.S0ujo.GEOG"><tld lit="S"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="somal.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="ca">somali</trd>
  <trd lng="en">Somali</trd>
  <trd lng="es">somal&iacute;</trd>
  <trdgrp lng="fr">
    <trd>Somalien</trd>,
    <trd>Somalienne</trd>
  </trdgrp>
  <trd lng="nl">Somali&euml;r</trd>
  <trdgrp lng="pt">
    <trd>somali</trd>,
    <trd>somaliano</trd>,
    <trd>somaliana</trd>
  </trdgrp>
</drv>

<drv mrk="somal.S0ujo">
  <kap>
    <tld lit="S"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="somal.S0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> en la orienta 
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo 
      Mogadi&scirc;o.
      Teritorio de Respubliko Somalujo.
      ISO-3166 kodo: SO.
      &Gcirc;i limas kun
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="gxibuti.GX0o.GEOG">&Gcirc;ibutio</ref>,
      <ref tip="vid" cel="kenj.K0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="mar.ArabaM0o">Araba Maro</ref>
      kaj Hinda Oceano.
    </dif>
    </snc>
    <trd lng="be">&c_S;&c_a;&c_m;&c_a;&c_l;&c_ib;</trd>
    <trd lng="ar">&a_A;&a_l;&a_S;&a_w;&a_m;&a_A;&a_l;</trd>
    <trd lng="bg">&c_S;&c_o;&c_m;&c_a;&c_l;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Som&agrave;lia</trd>,
      <trd>Rep&uacute;blica Democr&agrave;tica Somali</trd>
    </trdgrp>
    <trd lng="de">Somalia</trd>
    <trd lng="en">Somalia</trd>
    <trdgrp lng="fr">
      <trd>Somalie</trd>,
      <trd>R&eacute;publique f&eacute;d&eacute;rale de Somalie</trd>
    </trdgrp>
    <trd lng="id">Somalia</trd>
    <trd lng="nl">Somali&euml;</trd>
    <trdgrp lng="pl">
      <trd>Somalia</trd>,
      <trd>Republika <ind>Somalijska</ind></trd>
    </trdgrp>
</drv>

</art>

