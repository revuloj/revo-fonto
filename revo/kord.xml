<drv mrk="kord.kunvibra0o">
  <kap>kunvibra <tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="kord.kunvibra0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Netu&scirc;ata kordo de
      <ref tip="malprt" cel="instru2.kord0o">kordinstrumento</ref>,
      kiu pro proporcia longo resone vibras, kiam oni sonigas alian
      kordon.
    </dif>
    <ref tip="vid" cel="resona.0o">resonanco</ref>
  </snc>
  <trd lng="ca">corda harm&ograve;nica</trd>
  <trd lng="de">Resonanzsaite</trd>
  <trdgrp lng="fr">
    <trd>corde sympathique</trd>
  </trdgrp>
