<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tas.xml,v 1.44 2025/09/09 03:10:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tas</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="tas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tas.0o.ujo">
    <dif>
      <ref tip="super" cel="trink.0ujo">Trinkujo</ref> destinita
      por trinkado de varmaj trinka&jcirc;oj, precipe kafo a&ubreve;
      teo. <tld lit="T"/>o estas proksimume duonsfera, duoncilindra,
      konusstumpa a&ubreve; similforma, ordinare malalta kun lar&gcirc;a
      malferma&jcirc;o, <ref tip="prt" cel="ans.0o">anso</ref>, kaj
      estas
      farita el <ref tip="prt" cel="porcel.0o">porcelano</ref>,
      <ref tip="prt" cel="fajenc.0o">fajenco</ref> a&ubreve; alia
      materialo:
      <ekz>
        tamen oni ne faris por la domo de la Eternulo ar&gcirc;entajn
        <tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Re2; 12:13</lok></fnt>;
      </ekz>
      <ekz>
        simila al granda &Hcirc;ina <tld/>o, sur kiu estas prezentitaj
        bluaj arboj
        kaj pintaj pontoj
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="he">&samekh;&pe;&lamed;</trd>
    <trd lng="bo">&t_da;&t_ka;&t_ra;&t_tsheg;&t_ya;&t_naro;&t_la;&t_tsheg;</trd>
  </snc>
  <snc mrk="tas.0o.enhavo">
    <dif>
      Enhavo de tia trinkujo:
      <ekz>
        kuiri por li bonan <tld/>on da sambuka teo
        <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
      </ekz>
      <ekz>
        prepari por &scirc;i <tld/>on da varma biero kun butero kaj
        sukero
        <fnt><bib>Fab3</bib>, <lok>&Scirc;i estis tute
        senta&ubreve;ga</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tas.0o.mezuro">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Volumeno de tia trinkujo, proksimume 150 ml, uzata kiel
      <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> en kuirado:
      <ekz>
        1 <tld/>o da papavo
        <fnt><bib>Kukobak</bib>, <lok>Papavkuko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_l;&c_ib;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_a;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_a;&c_sh;&c_a;</trd>
  <trd lng="ca">tassa</trd>
  <trdgrp lng="cs">
    <trd>&scaron;&aacute;lek</trd>,
    <trd>kofl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Tasse</trd>
  <trd lng="en">cup</trd>
  <trd lng="es">taza</trd>
  <trd lng="fa">&a_f;&a_n;&a_j;&a_A;&a_n;</trd>
  <trd lng="fr">tasse</trd>
  <trd lng="he">&kaf;&vav;&samekh;</trd>
  <trd lng="hu">cs&eacute;sze</trd>
  <trd lng="id">cangkir</trd>
  <trdgrp lng="ja">
    <trd>&#33590;&#30871;  <pr>&#12385;&#12419;&#12431;&#12435;</pr></trd>,
    <trd>&#12459;&#12483;&#12503;  <pr>&#12363;&#12387;&#12407;</pr></trd>,
    <trd>&#28271;&#12398;&#12415;  <pr>&#12422;&#12398;&#12415;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kop</trd>,
    <trd>kopje</trd>
  </trdgrp>
  <trd lng="pl">fili&zdot;anka</trd>
  <trdgrp lng="pt">
   <trd>x&iacute;cara</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">&scaron;&aacute;lka</trd>
  <trd lng="sv">kopp</trd>
  <trd lng="tr">fincan</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_a;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_ib;&c_l;&c_ib;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_ib;&c_l;&c_k;&c_a; (&c_v;&c_a;&c_g;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26479; <pr>b&emacron;i</pr></trd>,
    <trd>&#30402; <pr>y&uacute;</pr></trd>,
    <trd>&#26479;&#23376; <pr>b&emacron;izi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tas.pesil0o">
  <kap>pesil<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>oforma ujo de pesilo, en kiun oni metas la
      pesota&jcirc;on:
      <ekz>
        &gcirc;usta pesilo kaj &gcirc;ustaj pesil<tld/>oj estas de la
        Eternulo
        <fnt><bib>MT</bib>, <lok>&Sen; 16:11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>platet</trd>,
    <trd>balan&ccedil;&oacute;</trd>
  </trdgrp>
  <trd lng="cs">miska vah</trd>
  <trd lng="de">Waagschale</trd>
  <trd lng="en">pan<klr> (of a balance scale)</klr></trd>
  <trd lng="fa">&f_k;&a_f;&a_h;&a_hamza_sure; &a_t;&a_r;&a_A;&a_z;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>plateau de balance</trd>
  </trdgrp>
  <trd lng="he">&kaf;&fpe;-&mem;&alef;&zayin;&nun;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;rlegserpeny&odblac;</trd>,
    <trd>m&eacute;rlegt&aacute;ny&eacute;r</trd>
  </trdgrp>
  <trd lng="id"><baz>piring</baz>piring timbangan</trd>
  <trdgrp lng="ja">
    <trd>&#12390;&#12435;&#12403;&#12435;&#30399;  <pr>&#12390;&#12435;&#12403;&#12435;&#12373;&#12425;</pr></trd>,
    <trd>&#12399;&#12363;&#12426;&#12374;&#12425;</trd>
  </trdgrp>
  <trd lng="nl">weegschaal</trd>
  <trd lng="pl">szalka</trd>
  <trd lng="ru">&c_ch;&c_a;&c_sh;&c_a; &c_v;&c_je;&c_s;&c_o;&c_v;</trd>
  <trd lng="sk">miska v&aacute;h</trd>
</drv>

<drv mrk="tas.sub0o">
  <kap>sub<tld/>o</kap>
  <snc>
    <dif>
      Telereto metata sub <tld/>on por ke la trinka&jcirc;o
      ver&scirc;ante ne
      malpurigu tablon:
      <ekz>
        La taso, abrupte tintinte &ccirc;e la subtaso, renversigis, kaj
        densa
        teo, aromanta je mento, versi&gcirc;is sur tablotukon.
        <fnt>
          <aut>Vjaceslavo Rybako, trad. Jurij Finke</aut>,
          <vrk>Gravitavio &#171;Carido&#187;</vrk>,
          <lok>Denove Peterburgo, &ccirc;. 5</lok>
       </fnt>;
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>telereto</ke>, <ke>subtasa&jcirc;o</ke>.
  </rim>

  <trd lng="be">&c_s;&c_p;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="ca">sotacopa</trd>
  <trdgrp lng="cs">
    <trd>pod&scaron;&aacute;lek</trd>,
    <trd>tal&iacute;&rcaron;ek</trd>
  </trdgrp>
  <trd lng="en">saucer</trd>
  <trd lng="fa">&a_n;&a_ayn;&a_l;&a_b;&f_k;&f_y;</trd>
  <trdgrp lng="id">
    <trd>lapik</trd>,
    <trd>lepek</trd>,
    <trd><baz>piring</baz>piring kecil</trd>,
    <trd><baz>tatak</baz>tatakan</trd>,
    <trd>cawan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21463;&#12369;&#30399;  <pr>&#12358;&#12369;&#12374;&#12425;</pr></trd>,
    <trd>&#33590;&#12383;&#12367;  <pr>&#12385;&#12419;&#12383;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spodek</trd>,
    <trd>podstawek</trd>
  </trdgrp>
  <trd lng="sk">tanierik pod &scaron;&aacute;lku</trd>
  <trd lng="uk">&c_b;&c_l;&c_ju;&c_d;&c_c;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#23567;&#30879;&#23376; <pr>xi&#462;odi&eacute;zi</pr></trd>,
    <trd>&#30879;&#23376; <pr>di&eacute;zi</pr></trd>
  </trdgrp>
</drv>
</art>
