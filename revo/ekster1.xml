<kap>
<ofc>*</ofc>
<rad>eksterm</rad>/i
</kap>
<drv mrk="ekster1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plene pereigi, tute detrui, neniigi:
      <ekz>
        la fajro <tld/>os la palacojn
        <fnt><bib>MT</bib>, <lok>&Amo; 1:12</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i popolon;
      </ekz>
      <ekz>
        <tld/>i urbon;
      </ekz>
      <ekz>
        <tld/>i la memoron pri
        <fnt><bib>MT</bib>, <lok>&Psa; 34:16</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i malbonan kutimon;
      </ekz>
      <ekz>
        ili estos <tld/>itaj kiel tute seka pajlo
        <fnt><bib>MT</bib>, <lok>&Nah; 1:10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj pereos de glavo k de malsato &gcirc;is ili tute
        <tld/>i&gcirc;os
        <fnt><bib>MT</bib>, <lok>&Jer; 44:27</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>anta fajro
        <fnt><bib>MT</bib>, <lok>&Psa; 18:8</lok></fnt>..
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="bucx.0i">bu&ccirc;i</ref>,
      <ref cel="cindr.0igi">cindrigi</ref>,
      <ref cel="polv">dispolvigi</ref>,
      <ref cel="radik.el0igi">elradikigi</ref>,
      <ref cel="visx.for0i">forvi&scirc;i</ref>,
      <ref cel="neni.0igi">neniigi</ref>.
    </refgrp>
  </snc>
  <adm>
  Mi volonte dirus, ke ~i normale apliki&gcirc;as al homoj,
  popoloj ks. La uzo "~i palacon", "~i kutimon" estas figursencaj.
  <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_ib;&c_ch;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausrotten</trd>,
    <trd>vertilgen</trd>,
    <trd>ausl&ouml;schen</trd>,
    <trd>vernichten</trd>
  </trdgrp>
  <trd lng="en">exterminate</trd>
  <trd lng="es">exterminar</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&f_y;&a_sh;&a_h;&zwnj;&f_k;&a_n;
    &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_n;&a_A;&a_b;&a_w;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>an&eacute;antir</trd>,
    <trd>exterminer</trd></trdgrp> 
  <trd lng="hu">ki&iacute;rt</trd>
  <trdgrp lng="ja">
    <trd>&#x6839;&#x7D76;&#x3059;&#x308B;  <pr>&#x3053;&#x3093;&#x305C;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7686;&#x6BBA;&#x3057;&#x306B;&#x3059;&#x308B;  <pr>&#x307F;&#x306A;&#x3054;&#x308D;&#x3057;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7D76;&#x6EC5;&#x3055;&#x305B;&#x308B;  <pr>&#x305C;&#x3064;&#x3081;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5168;&#x6EC5;&#x3055;&#x305B;&#x308B;  <pr>&#x305C;&#x3093;&#x3081;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x58CA;&#x6EC5;&#x3055;&#x305B;&#x308B;  <pr>&#x304B;&#x3044;&#x3081;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">uitroeien</trd>
  <trdgrp lng="pl">
    <trd>eksterminowa&cacute;</trd>,
    <trd>wyt&eogonek;pi&cacute;</trd>,
    <trd>wygubi&cacute;</trd>
  </trdgrp>
  <trd lng="pt">exterminar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_r;&c_je;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_o;&c_r;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">utrota</trd>
  <trdgrp lng="sk">
    <trd>vyhubi&#x165;</trd>,
    <trd>vykyno&#x17E;i&#x165;</trd>,
    <trd>zni&#x10D;i&#x165;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hubit</trd>,
    <trd>sprovodit</trd>,
    <trd>vyb&#xED;t</trd>,
    <trd>vyhladit</trd>,
    <trd>vyhubit</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43D;&#x438;&#x449;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x43D;&#x438;&#x449;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x43E;&#x440;&#x456;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8A85;&#x6EC5; <pr>zh&#x16B;mi&#xE8;</pr></trd>,
    <trd>&#x6251;&#x706D; <pr>p&#x16B;mi&#xE8;</pr></trd>,
