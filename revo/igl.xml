<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>igl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="igl.0o">
  <kap><tld/>o</kap>
  <snc mrk="igl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Duonsfera
      <ref tip="super" cel="dom.0o.konstruajxo">domo</ref>
      konstruita el ne&gcirc;brikoj, tradicia &scirc;irmejo de
      eskimoj:
      <ekz>
        kiel la eskimoj, Patro Henry sin nutris per fi&scirc;a&jcirc;o, kaj dormis 
        dumsomere en kabano kovrita per tolo kaj dumvintre en <tld/>o
         <fnt>
          <aut>A. De Salvo</aut>,
          <vrk>Katolika Horizonto</vrk>,
          <lok><url
          ref="http://www.esperokatolika.org/ek19761980/ek1979_05.htm#indekso">
          Espero Katolika, 1979:5</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_g;&c_l;&c_u;</trd>
  <trd lng="de">Iglu</trd>
  <trd lng="es">igl&uacute;</trd> 
  <trd lng="fr">iglou</trd>
</drv>
</art>

