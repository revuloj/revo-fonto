<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>papaj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="papaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="papaj.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref> de <tld/>arbo, kun
      flava a&ubreve; oran&gcirc;a karno man&gcirc;ebla kruda a&ubreve; kuirita:
      <ekz>
        en ujon oni metas salumitajn krabojn, salikokojn, pecigitan
        <tld/>on
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>man&gcirc;o kaj imagpovo, p.
        43a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">papaye</trd>
  <trdgrp lng="pt">
    <trd>mam&atilde;o</trd>,
    <trd>papaia</trd>,
    <trd>bepaia</trd>,        
    <trd>mamoa</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_p;&c_a;&c_j;&c_ja;</trd>
</drv>

<drv mrk="papaj.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="papaj.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Mezamerikdevena arbo el genro <ref tip="super"
      cel="karik.0o.BOT">kariko</ref>, kultivata en varmaj landoj pro siaj
      fruktoj (<trd lng="la">Carica papaya</trd>).
    </dif>
  </snc>
  <trd lng="fr">papayer</trd>
  <trdgrp lng="pt">
    <trd>mamoeiro</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_p;&c_a;&c_j;&c_ja;</trd>
</drv>
</art>

