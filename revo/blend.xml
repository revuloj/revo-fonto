<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>blend</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="blend.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Natura zinka sulfido. ZnS.
    </dif>
    <trd lng="fr">blende</trd>
    <trdgrp lng="hu">
      <trd>szfalerit</trd>,
      <trd>cinkf&eacute;nyle</trd>,
      <trd>cinkszulfid</trd>,
    </trdgrp>
    <trd lng="pl">blenda cynkowa</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_f;&c_a;&c_l;&c_je;&c_r;&c_i;&c_t;</trd>,
      <trd>&c_c;&c_i;&c_n;&c_k;&c_o;&c_v;&c_a;&c_ja;
      &c_o;&c_b;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_l;&c_je;&c_n;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="hom">
    Ne miksu kun <ref cel="blend1.0o">blendo</ref> (kiraso).
  </refgrp>
</drv>
</art>
