<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blend.xml,v 1.22 2025/03/02 11:21:24 revo Exp $">
<kap>
  <rad>blend</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="blend.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      <frm>ZnS</frm>; natura <ref tip="prt" cel="zink.0o">zinka</ref> sulfido:
      <ekz>
        mineraloj kiuj estas plej bone konvenaj por &ccirc;i tiu datigo estas nigra glimo,
        muskovito, profundeca/alte metamorfa kornea <tld/>o, vulkana feldspato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=283499">Izotopoj de kalio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>blenda</trd>,
      <trd>esfalerita</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sphalerit</trd>,
      <trd>Zinkblende</trd>,
      <trd>Zinksulfid</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>blende</trd>,
      <trd>zinc sulfate</trd>,
      <trd>zinc sulphate</trd>,
      <trd>sphalerite</trd>
    </trdgrp>
    <trd lng="fr">blende</trd>
    <trdgrp lng="hu">
      <trd>szfalerit</trd>,
      <trd>cinkf&eacute;nyle</trd>,
      <trd>cinkszulfid</trd>,
    </trdgrp>
    <trd lng="pl">blenda cynkowa</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_f;&c_a;&c_l;&c_je;&c_r;&c_i;&c_t;</trd>,
      <trd>&c_c;&c_i;&c_n;&c_k;&c_o;&c_v;&c_a;&c_ja;
      &c_o;&c_b;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_l;&c_je;&c_n;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="blend1.0o">blendo</ref> (kiraso).
  </rim>
  <trdgrp lng="be">
    <trd>&c_c;&c_y;&c_n;&c_k;&c_a;&c_v;&c_y; <ind>&c_p;&c_a;&c_d;&c_a;&c_b;&c_n;&c_ja;&c_k;</ind></trd>,
    <trd>&c_c;&c_y;&c_n;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_p;&c_a;&c_d;&c_m;&c_a;&c_n;&c_k;&c_a;</ind></trd>,
    <trd>&c_s;&c_f;&c_a;&c_l;&c_je;&c_r;&c_y;&c_t;</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5E9;&#x5D5;&#x5E8;&#x5D9;&#x5D9;&#x5DF;</trd>
  <trdgrp lng="ja">
    <trd>&#x9583;&#x4E9C;&#x925B;&#x9271;  <pr>&#x305B;&#x3093;&#x3042;&#x3048;&#x3093;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x786B;&#x5316;&#x9271;&#x7269;  <pr>&#x308A;&#x3085;&#x3046;&#x304B;&#x3053;&#x3046;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sfalerit</trd>,
    <trd>sulfid zino&ccaron;nat&yacute;</trd>
  </trdgrp>
  <trd lng="cs">panc&eacute;&rcaron;ov&yacute; kryt</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43B;&c_je;&#x43D;&c_d;&c_a;</trd>,
    <trd>&#x43E;&c_b;&#x43C;&c_a;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_s;&c_f;&c_a;&#x43B;&c_je;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_c;&c_i;&#x43D;&#x43A;&#x43E;&c_v;&c_a; &#x43E;&c_b;&#x43C;&c_a;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_j;&c_s;&#x44C;&#x43A;.&#xA0;&c_b;&c_r;&#x43E;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&c_a;&#x43D;&c_c;&c_je;&c_r;</trd>,
    <trd>&#x43F;&c_a;&#x43D;&c_c;&c_i;&c_r;</trd>
  </trdgrp>
</drv>
</art>
