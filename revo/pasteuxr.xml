<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Paste&ubreve;r</rad>/o
</kap>
<drv mrk="pasteuxr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve: <trd lng="fr">Pasteur<klr tip="ind"> (Louis)</klr></trd>, 1822-1895.
      Franca sciencisto, fondinto de
      <ref tip="vid" cel="mikrob1.0o">mikrobiologio</ref> kaj imunologio.
      Inter aliaj malkovroj, <tld/>o malkovris la 
      naturon de <ref tip="vid" cel="fermen.0ado">fermentado</ref>
      kaj proponis metodon de
      <ref tip="vid" cel="pasteuxriz.0o">paste&ubreve;rizo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_s;&c_t;&c_e;&c_r;</trd>
  <trd lng="de">Pasteur</trd>
  <trd lng="hu">Pasteur</trd>
  <trd lng="nl">Pasteur<klr tip="ind"> (Louis)</klr></trd>
  <trd lng="ru">&c_P;&c_a;&c_s;&c_t;&c_je;&c_r;</trd>
</drv>
</art>

