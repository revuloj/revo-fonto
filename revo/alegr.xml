<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alegr.xml,v 1.18 2021/02/28 15:34:26 revo Exp $">
<kap>
  <rad>alegr</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="alegr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref>, kies movo estas viva kaj gaja:
      <ekz>
        movimento-sinsekvoj <klr>[de]</klr> klasika simfonio: <tld/>o &ndash; <ref cel="adagx.0o">ada&gcirc;o</ref>
        a&ubreve; <ref cel="andant.0o">andanto</ref> &ndash; <ref cel="menuet.0o">menueto</ref>
        a&ubreve; <ref cel="skerc.0o">skerco</ref> &ndash; <tld/>o a&ubreve; presto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=252283">Movimento (muziko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_g;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>allegro</trd>,
    <trd>rychl&aacute; skladba</trd>
  </trdgrp>
  <trd lng="de">Allegro</trd>
  <trd lng="en">allegro</trd>
  <trd lng="es">alegro<klr tip="amb"> (m&uacute;sica)</klr></trd>
  <trd lng="fr">allegro</trd>
  <trd lng="he">&#x5E9;&#x5C2;&#x5B7;&#x5DE;&#x5B5;&#x5D7;&#x5B7;</trd>
  <trd lng="hu">allegro</trd>
  <trd lng="nl">allegro</trd>
  <trd lng="pl">allegro</trd>
  <trd lng="pt">alegro</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_r;&c_o;</trd>
  <trd lng="sk">r&yacute;chla skladba</trd>
</drv>

<drv mrk="alegr.0e">
  <kap><tld/>e</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Vive kaj gaje.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_g;&c_r;&c_a;</trd>
  <trd lng="cs">allegro</trd>
  <trdgrp lng="de">
    <trd>heiter <klr>(Musik)</klr></trd>,
    <trd>schnell <klr>(Musik)</klr></trd>
  </trdgrp>
  <trd lng="es">alegremente</trd>
  <trd lng="fr">allegro</trd>
  <trd lng="he">&#x5E2;&#x5B7;&#x5DC;&#x5B4;&#x5D9;&#x5D6;</trd>
  <trdgrp lng="hu">
    <trd>allegro</trd>,
    <trd>&eacute;l&eacute;nken</trd>,
    <trd>vid&aacute;man</trd>
  </trdgrp>
  <trd lng="nl">allegro</trd>
  <trdgrp lng="pl">
    <trd>&zdot;ywo</trd>,
    <trd>weso&lstroke;o</trd>,
    <trd>&zdot;wawo</trd>,
    <trd>allegro</trd>
  </trdgrp>
  <trd lng="pt">alegro</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_r;&c_o;</trd>
  <trd lng="sk">allegro</trd>
</drv>
<drv mrk="alegr.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref>, kies movo estas rapideta.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_g;&c_r;&c_e;&c_t;&c_a;</trd>
  <trd lng="cs">allegretto</trd>
  <trd lng="de">Allegretto</trd>
  <trd lng="en">allegretto</trd>
  <trd lng="fr">allegretto</trd>
  <trd lng="he">&#x5E9;&#x5C2;&#x5B4;&#x5DE;&#x5B0;&#x5D7;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">allegretto</trd>
  <trd lng="nl">allegretto</trd>
  <trd lng="pl">allegretto</trd>
  <trd lng="pt">alegreto</trd>
  <trd lng="ru">&c_a;&c_l;&c_je;&c_g;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
  <trd lng="sk">allegretto</trd>
</drv>
<drv mrk="alegr.0ete">
  <kap><tld/>ete</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Rapidete.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_g;&c_r;&c_e;&c_t;&c_a;</trd>
  <trd lng="cs">allegretto</trd>
  <trd lng="fr">allegretto</trd>
  <trdgrp lng="hu">
    <trd>allegretto</trd>,
    <trd>kiss&eacute; <ind>&eacute;l&eacute;nken</ind></trd>,
    <trd>kiss&eacute; <ind>vid&aacute;man</ind></trd>
  </trdgrp>
  <trd lng="nl">allegretto</trd>
  <trd lng="pt">alegreto</trd>
  <trd lng="sk">allegretto</trd>
</drv>
</art>
