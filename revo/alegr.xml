<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alegr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="alegr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika&jcirc;o, kies movo estas viva kaj gaja.
    </dif>
  </snc>
  <trd lng="fr">allegro</trd>
  <trd lng="nl">allegro</trd>
  <trd lng="pt">alegro</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_r;&c_o;</trd>
</drv>
<drv mrk="alegr.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Vive kaj gaje.
    </dif>
  </snc>
  <trd lng="fr">allegro</trd>
  <trd lng="nl">allegro</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_g;&c_r;&c_o;</trd>
</drv>
<drv mrk="alegr.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Muzika&jcirc;o, kies movo estas rapideta.
    </dif>
  </snc>
  <trd lng="fr">allegretto</trd>
  <trd lng="nl">allegretto</trd>
  <trd lng="pt">alegreto</trd>
  <trd lng="ru">&c_a;&c_l;&c_je;&c_g;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
</drv>
<drv mrk="alegr.0ete">
  <kap><tld/>ete</kap>
  <snc>
    <dif>
      Rapidete.
    </dif>
  </snc>
  <trd lng="fr">allegretto</trd>
  <trd lng="nl">allegretto</trd>
</drv>
</art>
