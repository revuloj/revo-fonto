<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: valv.xml,v 1.33 2023/10/21 16:50:35 revo Exp $">
<kap>
  <rad>valv</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="valv.0o">
  <kap><tld/>o</kap>
  <snc mrk="valv.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="mehxan1.0o.pecoj">Me&hcirc;anismo</ref>
      tralasanta fluon en unu direkto,
      samtempe barante &gcirc;in en alia direkto:
      <ekz>
        disko<tld/>o, dren<tld/>o, ka&ubreve;&ccirc;ukoklapa <tld/>o,
        prem<tld/>o, ringa <tld/>o, tuba <tld/>o;
      </ekz>
      <ekz>
         <tld/>o de pne&ubreve;o;
      </ekz>
      <ekz>
        mi povis distingi nek krankon disde <tld/>o nek bielon de &scirc;afto
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="klap.0o.TEK">klapo<sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_e;&c_n;&c_t;&c_y;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="ca">v&agrave;lvula</trd>
    <trdgrp lng="de">
      <trd>Ventil</trd>,
      <trd>Stellklappe</trd>
    </trdgrp>
    <trd lng="es">valva</trd>
    <trdgrp lng="fr">
      <trd>soupape</trd>,
      <trd>clapet</trd>
    </trdgrp>
    <trd lng="hu">szelep</trd>
    <trdgrp lng="nl">
      <trd>ventiel</trd>,
      <trd>klep</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="valv.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <ref tip="dif" cel="rektif.0ilo">rektifilo</ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_s;&c_t;&c_a;&c_ch;</trd>
    </trdgrp>
    <trd lng="de">Gleichrichter</trd>
    <trd lng="fr">redresseur<klr> (&eacute;lectrique)</klr></trd>
  </snc>
  <snc mrk="valv.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Ccirc;iu el la du partoj de dupeca
      <ref tip="malprt" cel="konk.0o.tego">konko</ref>;
      <ref tip="sin" cel="klap.0o.ZOO">klapo<sncref/></ref>.
    </dif>
    <trd lng="be">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
    <trd lng="ca">valva <klr>(mol&middot;luscs)</klr></trd>
    <trd lng="hu">kagyl&oacute;h&eacute;j</trd>
    <trd lng="nl">schaal<klr tip="ind"> (v.dier)</klr></trd>
    <trd lng="ru">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="valv.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iu aparta parto de <ref tip="malprt" cel="perika1.0o">perikarpo</ref>.
    </dif>
    <trd lng="be">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
    <trd lng="ca">valva (fruits)</trd>
    <trdgrp lng="es">
      <trd>valva</trd>,
      <trd>ventalla</trd>
    </trdgrp>
    <trd lng="hu">term&eacute;sh&eacute;j</trd>
    <trd lng="nl">lob</trd>
    <trd lng="ru">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>klapka</trd>,
    <trd>ventil</trd>,
    <trd>z&aacute;klopka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F01;  <pr>&#x3079;&#x3093;</pr></trd>,
    <trd>&#x30D0;&#x30EB;&#x30D6;</trd>,
    <trd>&#x30C1;&#x30E5;&#x30FC;&#x30D6;&#x30D0;&#x30EB;&#x30D6;</trd>,
    <trd>&#x6574;&#x6D41;&#x7BA1;  <pr>&#x305B;&#x3044;&#x308A;&#x3085;&#x3046;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x6BBB;&#x7247;  <pr>&#x304B;&#x3089;&#x304B;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="pt">v&aacute;lvula</trd>
  <trdgrp lng="sk">
    <trd>klapka</trd>,
    <trd>ventil</trd>,
    <trd>z&aacute;klopka</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_u;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="valv.du0uloj">
  <kap>du<tld/>uloj</kap>
  <snc mrk="valv.du0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref>
       el la filumo <ref tip="malprt" cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Bivalvia</trd>),
      kun duvalva konko kaj kun pluraj man&gcirc;eblaj specioj:
      <ekz>
        la <ind>Jakoba pekteno</ind> estas du<tld/>ulo <klr>[vivanta]</klr> en Mediteraneo kaj
        Atlantiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=300395">Jakoba pekteno</url></lok>
        </fnt>.
        <trd lng="de">Jakobsmuschel</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_u;&c_h;&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;&c_v;&c_y;&c_ja;
    <klr>(&c_k;&c_l;&c_ja;&c_s; &c_m;&c_a;&c_l;&c_ju;&c_s;&c_k;&c_a;&c_w;)</klr>
  </trd>
  <trdgrp lng="ca">
    <trd>lamel&middot;libranquis</trd>,
    <trd>bivalves</trd>
  </trdgrp>
  <trd lng="cs">ml&zcaron;i</trd>
  <trd lng="de">Muscheln<klr tip="ind"> (Klasse)</klr></trd>
  <trd lng="en">bivalves</trd>
  <trd lng="es">bivalvos</trd>
  <trd lng="fr">bivalves<klr> (classe des)</klr></trd>
  <trd lng="hu">kagyl&oacute;k<klr tip="amb"> (oszt&aacute;ly)</klr></trd>
  <trd lng="nl">mosselen<klr tip="ind"> (klasse)</klr></trd>
  <trd lng="pt"><klr tip="ind">(moluscos) </klr>bivalves</trd>
  <trd lng="ru">&c_d;&c_v;&c_u;&c_s;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_y;&c_je; &c_m;&c_o;&c_l;&c_l;&c_ju;&c_s;&c_k;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="sk">last&uacute;rniky</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&c_o;&c_s;&c_t;&c_u;&c_l;&c_k;&c_o;&c_v;&c_ib; &c_m;&c_o;&c_l;&c_ju;&c_s;&c_k;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_z;&c_ja;&c_b;&c_r;&c_o;&c_v;&c_ib; &c_m;&c_o;&c_l;&c_ju;&c_s;&c_k;&c_i;</trd>,
    <trd>&c_s;&c_o;&c_k;&c_i;&c_r;&c_o;&c_n;&c_o;&c_g;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="valv.elektrona0o">
  <kap>elektrona <tld/>o</kap>
  <snc mrk="valv.elektrona0o.ELET">
    <uzo tip="fak">ELET</uzo>
    <dif>
      <ref tip="super" cel="aparat.0o">Aparato</ref>, kreanta kaj
      direktanta elektronan fluon en vakuo a&ubreve; gaso;
      elektrona <ref tip="sin" cel="tub.0o.RAD">tubo</ref>:
      <ekz>
        ni havis grandan <tld/>an radion kun diskoturnilo kaj varma sono (ne nur pro la
        tuboj)
        <fnt>
          <bib>Monato</bib>
          <aut>Jean Pierre VandenDaele</aut>
          <vrk><url ref="&Monato;/2012/010697.php">De analogo al cifereco: evoluo de persona historio</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_d;&c_y;&c_jo;&c_l;&c_ja;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja;
      <ind>&c_l;&c_ja;&c_m;&c_p;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca">tub de raigs cat&ograve;dics</trd>
  <trd lng="cs">elektronka</trd>
  <trdgrp lng="de">
    <trd>R&ouml;hre</trd>,
    <trd>Elektronenr&ouml;hre</trd>
  </trdgrp>
  <trd lng="es">v&aacute;alvula</trd>
  <trd lng="fr">tube &eacute;lectronique</trd>
  <trdgrp lng="hu">
    <trd>elektroncs&odblac;</trd>,
    <trd>r&aacute;di&oacute;cs&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>radiobuis</trd>,
    <trd>radiolamp</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;&c_a;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">elektr&oacute;nka</trd>
</drv>
</art>
