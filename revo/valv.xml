<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>valv</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="valv.0o">
  <kap><tld/>o</kap>
  <snc mrk="valv.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de (precipe klapa) ventolilo:
      <ekz>
        disko<tld/>o, dren<tld/>o, ka&ubreve;&ccirc;ukoklapa <tld/>o,
        prem<tld/>o, ringa <tld/>o, tuba <tld/>o.
      </ekz>
    </dif>
    <ref tip="super" cel="klap.0o">klapo</ref>
    <trdgrp lng="de">
       <trd>Ventil</trd>,
       <trd>Stellklappe</trd></trdgrp>
    <trdgrp lng="nl">
      <trd>ventiel</trd>,
      <trd>klep</trd>
    </trdgrp>
  </snc>
  <snc mrk="valv.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Elektrona lampo.
    </dif>
    <trdgrp lng="de">
       <trd>R&ouml;hre</trd>,
       <trd>Elektronenr&ouml;hre</trd></trdgrp>
  </snc>
  <snc mrk="valv.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Ccirc;iu el la du partoj de duklapa
      <ref tip="malprt" cel="konk.0o.tego">konko</ref>.
    </dif>
  </snc>
  <snc mrk="valv.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iu aparta parto de perikarpo.
    </dif>
  </snc>
</drv>
<drv mrk="valv.du0uloj">
  <kap>du<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso el la filumo <ref tip="super" cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Bivalvia</trd>).
    </dif>
  </snc>
  <trd lng="de">Muscheln <klr tip="ind">(Klasse)</klr></trd>
</drv>
</art>

