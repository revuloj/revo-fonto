<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>valv</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="valv.0o">
  <kap><tld/>o</kap>
  <snc mrk="valv.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de (precipe klapa) ventolilo:
      <ekz>
        disko<tld/>o, dren<tld/>o, ka&ubreve;&ccirc;ukoklapa <tld/>o,
        prem<tld/>o, ringa <tld/>o, tuba <tld/>o.
      </ekz>
    </dif>
    <ref tip="super" cel="klap.0o">klapo</ref>
    <trdgrp lng="de">
      <trd>Ventil</trd>,
      <trd>Stellklappe</trd>
    </trdgrp>
    <trd lng="es">valva</trd>
    <trdgrp lng="nl">
      <trd>ventiel</trd>,
      <trd>klep</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="valv.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Elektrona lampo.
    </dif>
    <trdgrp lng="de">
      <trd>R&ouml;hre</trd>,
      <trd>Elektronenr&ouml;hre</trd>
    </trdgrp>
    <trd lng="es">v&aacute;alvula</trd>
    <trdgrp lng="nl">
      <trd>radiobuis</trd>,
      <trd>radiolamp</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;&c_a;&c_m;&c_p;&c_a;</trd>,
      <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_a;&c_m;&c_p;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="valv.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Ccirc;iu el la du partoj de duklapa
      <ref tip="malprt" cel="konk.0o.tego">konko</ref>.
    </dif>
    <trd lng="nl">schaal<klr tip="ind"> (v.dier)</klr></trd>
    <trd lng="ru">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="valv.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iu aparta parto de perikarpo.
    </dif>
    <trdgrp lng="es">
      <trd>valva</trd>,
      <trd>ventalla</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </snc>
</drv>

<drv mrk="valv.du0uloj">
  <kap>du<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso el la filumo <ref tip="super" cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Bivalvia</trd>).
    </dif>
  </snc>
  <trd lng="de">Muscheln<klr tip="ind"> (Klasse)</klr></trd>
  <trd lng="en">bivalves</trd>
  <trd lng="es">bivalvos</trd>
  <trd lng="nl">mosselen<klr tip="ind"> (klasse)</klr></trd>
  <trd lng="ru">&c_d;&c_v;&c_u;&c_s;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_y;&c_je; &c_m;&c_o;&c_l;&c_l;&c_ju;&c_s;&c_k;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>
</art>
