<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>himn</rad>/o
</kap>
<drv mrk="himn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Kantota poemo por honori Dion, dia&jcirc;on, heroon, idealon kc.:
      <ekz>la <tld/>o de la esp-istaro.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="psalm.0o">psalmo</ref>,
      <ref cel="kantik.0o">kantiko</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">hymna</trd>
  <trdgrp lng="de">
     <trd>Hymne</trd>,
     <trd>Hymnus</trd>,
     <trd>Lobgesang</trd>,
     <trd>Loblied</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;</trd>
  <trd lng="sv">hymn</trd>
</drv>
</art>
