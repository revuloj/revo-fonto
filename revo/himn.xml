<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: himn.xml,v 1.24 2020/12/19 08:45:29 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>himn</rad>/o <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="himn.0o">
  <kap><tld/>o</kap>
  <snc mrk="himn.0o.REL">
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">POE</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="kant.0ajxo">Kanta&jcirc;o</ref> kaj precipe ties teksto por honori Dion, dia&jcirc;on,
      heroon, idealon a&ubreve; similan:
      <ekz>
        <klr>[ili]</klr> sendas al &ccirc;ielo <tld lit="H"/>ojn, &gcirc;emojn, amokantojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        fari ina&ubreve;guron <klr>[&#8230;]</klr> kun glor<tld/>oj kaj kantoj, kun cimbaloj,
        psalteroj, kaj harpoj
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 12:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        kantinte <tld/>on, ili foriris al la monto
        <fnt>
          <bib>NT</bib>
          <lok>Marko 14:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        admonu unu la alian per psalmoj kaj <tld/>oj kaj spiritaj kantoj
        <fnt>
          <bib>NT</bib>
          <lok>Koloseanoj 3:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i forgesis kunkanti la <tld/>on, &scirc;i forgesis pre&gcirc;i sian Patronian
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de &gcirc;ojo
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        funebra <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu knabino kantis nian plej sanktan <tld/>on, kiun a&ubreve;di havas la rajton nur la
        faraono
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas nia <tld/>o en muziko de la svedo <nom>Adelsk&ouml;ld</nom>
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 17a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">REL</uzo>
        <ind>star<tld/>o</ind> (&ccirc;e kies plenumo en diservo oni staras)
        al la Sanktega Dipatrino.
        <trd lng="de">Akathistos</trd>
        <trd lng="el">&alfa;&kappa;&alfa_ton;&theta;&jota;&sigma;&tau;&omikron;&sigma_fina;</trd>
        <trd lng="en">acathistus</trd>
        <trdgrp lng="ru">
          <trd>&c_a;&c_k;&c_a;&c_f;&c_i;&c_s;&c_t;</trd>,
          <trd>&c_n;&c_je;&c_s;&c_je;&c_d;&c_a;&c_l;&c_je;&c_n;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kantik.0o">kantiko</ref>,
      <ref cel="psalm.0o">psalmo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_m;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>chvalozp&ecaron;v</trd>,
    <trd>hymna</trd>,
    <trd>hymnus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hymne</trd>,
    <trd>Hymnus</trd>,
    <trd>Lobgesang</trd>,
    <trd>Loblied</trd>
  </trdgrp>
  <trd lng="el">&ypsilon_ton;&my;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="es">himno</trd>
  <trd lng="fr">hymne</trd>
  <trd lng="hu">himnusz</trd>
  <trd lng="id">himne</trd>
  <trd lng="it">inno</trd>
  <trd lng="nl">hymne</trd>
  <trd lng="pl">hymn</trd>
  <trd lng="pt">hino</trd>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>chv&aacute;lospev</trd>,
    <trd>hymnus</trd>
  </trdgrp>
  <trd lng="sv">hymn</trd>
</drv>

<drv mrk="himn.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="himn.0aro.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Libroforma <ref tip="super" cel="kolekt.0o.libr">kolekto</ref>
      da <tld/>oj religiaj:
      <ekz>
        en 2007 oni solenos la centjaran jubileon de la kristana <tld/>aro
        esperantlingva (Ordo de Diservo, Kembri&gcirc;o, 1907)
        <fnt>
          <vrk><url ref="https://himnologio.blogspot.com/">Himnoj kaj himnologio | Himnaro Esperanta 4a eld.</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Montague Butler</nom> (kiu estis Kvakero) tradukis multajn
        <tld/>ojn kaj kompilis la <tld/>aron esperantan
        <fnt>
          <lok><url ref="http://verdadrako.50g.com/bulteno6.htm">El nia bulteneto, Okt. 2002</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kancion&aacute;l</trd>
  <trdgrp lng="de">
    <trd>Hymnen-Sammlung</trd>,
    <trd>Gesangbuch <klr>(Religion)</klr></trd>
  </trdgrp>
  <trd lng="fr">hymnaire</trd>
  <trdgrp lng="it">
    <trd>innario</trd>,
    <trd>innodia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k; &c_g;&c_i;&c_m;&c_n;&c_o;&c_v;</trd>,
    <trd>&c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k;
    &c_p;&c_je;&c_s;&c_n;&c_o;&c_p;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">kancion&aacute;l</trd>
</drv>

</art>
