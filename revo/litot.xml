<kap>
  <rad>litot</rad>/o
</kap>
<drv mrk="litot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="tro.mal0igo.litoto">Maltroigo</ref>, kutime
      esprimita per duobla negado, ekz-e &leftquot;li ne estas sen talento&rightquot; por esprimi
      ke li ja havas grandan talenton:
      <ekz>
        <tld/>on oni precipe uzas por maski modestecon
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas multe pli ofta ol <ref tip="ant" cel="hiperb.0o">hiperbolo</ref>
        <klr>[&#8230;]</klr>,
        maldrastaj eldiroj tre ofte akompanas heroajn agojn
        <fnt>
          <vrk><url
          ref="https://books.google.de/books?id=PAuSMlrubLoC&amp;lpg=PA13&amp;ots=iNeMIwegQr&amp;dq=litoto&amp;hl=de&amp;pg=PA13#v=onepage&amp;q=litoto&amp;f=false"
            >Sagao de Njal</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>litotes</trd>,
    <trd>&rcaron;e&ccaron;nick&yacute; dvojz&aacute;por</trd>
  </trdgrp>
  <trd lng="de">Litotes</trd>
  <trd lng="en">litotes</trd>
  <trd lng="es">l&iacute;tote</trd>
  <trd lng="fi">litoteesi</trd>
  <trdgrp lng="fr">
    <trd>litote</trd>
  </trdgrp>
  <trd lng="gl">l&iacute;tote</trd>
  <trdgrp lng="hu">
    <trd>litot&eacute;sz</trd>,
    <trd>t&uacute;lz&oacute; kicsiny&iacute;t&eacute;s</trd>
  </trdgrp>
  <trd lng="id">litotes</trd>
  <trd lng="it">litote</trd>
  <trd lng="ja">&#32233;&#21465;&#27861;</trd>
  <trd lng="ka">&#4314;&#4312;&#4322;&#4317;&#4322;&#4308;&#4321;&#4312;</trd>
  <trd lng="ko">&#44257;&#50616;&#48277;</trd>
  <trd lng="lt">litot&#279;</trd>
  <trd lng="mk">&c_l;&c_i;&c_t;&c_o;&c_t;&c_a;</trd>
  <trd lng="nl">litotes</trd>
  <trd lng="no">litotes</trd>
  <trd lng="pl">litota</trd>
  <trd lng="pt">l&iacute;totes</trd>
  <trdgrp lng="ro">
    <trd>meioz&abreve;</trd>,
    <trd>litote</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_l;&c_i;&c_t;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="scn">litoti</trd>
  <trdgrp lng="sk">
    <trd>litotes</trd>,
    <trd>re&ccaron;n&iacute;cky dvojz&aacute;por</trd>
  </trdgrp>
  <trd lng="sv">litotes</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_t;&#x43E;&c_t;&c_a;</trd>
</drv>
