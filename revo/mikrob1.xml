<vortaro>
<art mrk="$Id: mikrob1.xml,v 1.20 2019/06/30 18:21:48 revo Exp $">
<kap>
  <rad>mikrobiologi</rad>/o
</kap>
<drv mrk="mikrob1.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrob1.0o.SCI">
    <uzo tip="fak">BAK</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      pri la <ref tip="vid" cel="mikrob.0o.BAK">mikroboj</ref>:
      <ekz>
        plej diversaj temoj: historio, sociologio, <tld/>o, ekonomio, medicino, gerontologio, psikologio
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006807.html">Fascina gazeto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i dedi&ccirc;is sin al laboratoria esplorado kaj doktori&gcirc;is pri <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009080.php">Kial la malestimo je la afrika medicino?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        certaj regionoj en Belgio havas propran, lokan <tld/>an medion
        <fnt>
          <bib>Monato</bib>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007144.html">Bierlando Belgio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mikrobiolog.0o">mikrobiologo</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_b;&c_ib;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_i;&c_k;&c_r;&c_o;&c_b;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">microbiologia</trd>
  <trd lng="de">Mikrobiologie</trd>
  <trd lng="en">microbiology</trd>
  <trd lng="es">microbiolog&iacute;a</trd>
  <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_b;&f_y;&a_w;&a_l;&a_w;&f_zh;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>bact&eacute;riologie</trd>,
    <trd>microbiologie</trd>
  </trdgrp>
  <trd lng="hu">mikrobiol&oacute;gia</trd>
  <trd lng="id">mikrobiologi</trd>
  <trd lng="it">microbiologia</trd>
  <trd lng="nl">microbiologie</trd>
  <trd lng="pl">mikrobiologia</trd>
  <trd lng="pt">microbiologia</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_b;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
