<art mrk="$Id: truso.xml,v 1.6 2024/05/10 10:43:47 revo Exp $">
<kap>
  <rad>truso</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="truso.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="truso.0o.MOR">
    <dif>
      <ke><ref tip="dif" cel="dot.0o">Doto</ref></ke>:
      <ekz>
        &scirc;ia patro <klr>[&#x2026;]</klr> devis hipoteki la domon por a&ccirc;eti &scirc;ian
        <tld/>on
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Aussteuer</trd>,
    <trd>Brautausstattung</trd>
  </trdgrp>
  <trd lng="en">trousseau</trd>
  <trd lng="fr">trousseau</trd>
  <trd lng="it">corredo</trd>
  <trd lng="pl">wiano</trd>
  <trdgrp lng="zh">
    <trd>&#x5941; <pr>li&#xE1;n</pr></trd>,
    <trd>&#x7C62; <pr>li&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

