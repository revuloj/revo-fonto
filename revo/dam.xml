<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>dam</rad>/o
    </kap>
    <drv mrk="dam.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Atuta disko de la <tld/>ludo. 
        </dif>
        <trd lng="ru">&c_d;&c_a;&c_m;&c_k;&c_a; <klr>(&c_v; &c_sh;&c_a;&c_sh;&c_k;&c_a;&c_h;)</klr></trd>
      </snc>
      <snc mrk="dam.0o.karta">
        <dif>
          Figuro de re&gcirc;ino en la kartludoj. 
        </dif>
        <trd lng="ru">&c_d;&c_a;&c_m;&c_a; <klr>(&c_v; &c_k;&c_a;&c_r;&c_t;&c_a;&c_h;)</klr></trd>
      </snc>
      <snc mrk="dam.0o.sxaka">
        <dif>
          &Scirc;akpeco, kiu movi&gcirc;as kiel kuriero kaj kiel turo.
        </dif>
        <trd lng="ru">&c_f;&c_je;&c_r;&c_z;&c_mol;</trd>
      </snc>
    </drv>
    <drv mrk="dam.0oj0ludo">
      <kap><tld/>oj, <tld/>ludo</kap>
      <snc>
        <dif>
          Ludo por du personoj, ludata sur tabulo dividita la&ubreve; cent
          kvadratoj alterne nigraj kaj blankaj, sur kiu oni pu&scirc;as
          disketojn.
        </dif>
      </snc>
      <trd lng="ru">&c_sh;&c_a;&c_sh;&c_k;&c_i;</trd>
    </drv>
  </art>
  <!--
  $Log$
  Revision 1.5  2001/01/30 17:11:39  revo
  Yury Finkel: +shaka senco, +trd ru

  -->
</vortaro>
