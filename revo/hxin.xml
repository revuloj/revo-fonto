<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&hcirc;in</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hxin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <ref tip="dif" cel="cxin.0o">&ccirc;ino</ref>
  </snc>
  <trd lng="de">Chinese</trd>
  <trd lng="el">&kappa;&jota;&ny;&epsilon_ton;&zeta;&omikron;&sigma_fina;</trd>
  <trd lng="en">Chinaman</trd>
  <trd lng="es">chino</trd>
  <trd lng="fr">chinois</trd>
  <trd lng="hu">k&iacute;nai</trd>
  <trd lng="pt">chin&ecirc;s</trd>
  <trd lng="ru">&c_k;&c_i;&c_t;&c_a;&c_je;&c_c;</trd>
  <trd lng="sk">&Ccaron;&iacute;&ncaron;an</trd>
  <trd lng="zh">&#20013;&#22269;&#20154;</trd>
</drv>

<drv mrk="hxin.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      &Ccirc;ina:
      <ekz>
        &gcirc;i estas por mi <tld/>a scienco
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li nur anstata&ubreve;igis la lakitajn duon&scirc;uojn per
        <tld/>aj ru&gcirc;aj pantofloj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 4a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">chinesisch</trd>
  <trd lng="en">Chinese</trd>
  <trd lng="fr">chinois</trd>
  <trd lng="hu">k&iacute;nai</trd>
</drv>

<drv mrk="hxin.Hx0ujo">
  <kap><tld lit="&Hcirc;"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <ref tip="dif" cel="cxin.Cx0ujo">&ccirc;inujo</ref>
  </snc>
  <trd lng="de">China</trd>
  <trd lng="el">&Kappa;&jota_ton;&ny;&alfa;</trd>
  <trd lng="en">China</trd>
  <trd lng="es">China</trd>
  <trd lng="fr">Chine</trd>
  <trd lng="hu">K&iacute;na</trd>
  <trd lng="pt">China</trd>
  <trd lng="ru">&c_K;&c_i;&c_t;&c_a;&c_j;</trd>
  <trd lng="sk">&Ccaron;&iacute;na</trd>
  <trd lng="zh">&#20013;&#22269;</trd>
</drv>
</art>

