<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lavang</rad>/o
</kap>
<drv mrk="lavang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ne&gcirc;amasego rulfalanta de monto:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e kreski
        <klr>(plii&gcirc;i sen&ccirc;ese kaj rapide kiel rulanta ne&gcirc;bulo)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>oj da homoj, da vortoj, ripro&ccirc;oj, mensogoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de indigno ruli&gcirc;is de unu angulo de la lando &gcirc;is la alia.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="torent.0o">torento</ref>,
      <ref cel="pluv.0o">pluvo</ref>,
      <ref cel="hajl.0o">hajlo</ref>.
    </refgrp>
  </snc>
  <trd lng="el">&chi;&jota;&omikron;&ny;&omikron;&sigma;&tau;&jota;&beta;&alfa_ton;&delta;&alfa;</trd>
  <trd lng="en">avalanche</trd>
  <trd lng="es">alud</trd>
  <trd lng="fr">avalanche</trd>
  <trd lng="nl">lawine</trd>
  <trd lng="pt">avalanche</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sv">lavin</trd>
</drv>
</art>
