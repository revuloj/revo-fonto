<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tener</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tener.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Elmontranta aman, mildan atenton, karesemon:
      <ekz>
        el &scirc;ia danka submeti&gcirc;o kaj lia protektemo i&gcirc;is iom post iom
        <tld/>a rilato
        <fnt>
        <aut>Renervo Da&scirc;o</aut>,
        <vrk>Ne-atenditaj sekvoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 8a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dolcx.0a.milda">dol&ccirc;a <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>,
    <trd>liebevoll</trd>,
    <trd>weichherzig</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>afectuoso(a)</trd>,
    <trd>tierno(a)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affectueux</trd>,
    <trd>tendre<klr> (affectueux)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="tener.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kun ama mildo, prizorgeme:
      <ekz>
        Feniksa <klr>...</klr> <tld/>e lin demandis pri la farto
        <fnt>
        <bib>RugxDom</bib>,
        <lok>&ccirc;apitro 69a, volumo 2a, p. 468a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>,
    <trd>liebevoll</trd>,
    <trd>weichherzig</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>afectuosamente</trd>,
    <trd>tiernamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affectueusement</trd>,
    <trd>tendrement</trd>
  </trdgrp>
</drv>
</art>
