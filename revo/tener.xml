<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tener.xml,v 1.22 2024/11/21 10:34:20 revo Exp $">
<kap>
  <ofc>8</ofc><rad>tener</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tener.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Elmontranta aman, mildan atenton, karesemon:
      <ekz>
        <tld/>a kareso
        <fnt>
          <bib>ChV</bib>
          <lok>15</lok>
        </fnt>;
      </ekz>
      <ekz>
        el &scirc;ia danka submeti&gcirc;o kaj lia protektemo i&gcirc;is
        iom post iom <tld/>a rilato
        <fnt>
        <aut>Renervo Da&scirc;o</aut>,
        <vrk>Ne-atenditaj sekvoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 8a, p. 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;nu, nu&rightquot;, li diris per plej <tld/>a vo&ccirc;o
        <fnt>
          <bib>ChB</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        okuloj preska&ubreve; <tld/>aj
        <fnt>
          <bib>CKv</bib>
          <lok>23</lok>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>a kiso &scirc;i pu&scirc;etis lin &ccirc;e la &scirc;ultro
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&Ouml;SSLER</aut>
          <vrk><url ref="&Monato;/2004/008358.html"
              >Kuracbaneje</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dolcx.0a.milda">dol&ccirc;a<sncref/></ref>,
      <ref cel="delika.0a">delikata</ref>,
      <ref cel="mild.0a">milda</ref>,
      <ref cel="subtil.0a">subtila</ref>,
      <ref cel="sent.0ema">sentema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_je;&c_zh;&c_je;&c_n;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_ja;&c_shch;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_drengbu;&t_ma;&t_sa;&t_tsheg;&t_ba;&t_za;&t_nga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>tendre</trd>,
    <trd>dol&ccedil;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&aacute;skypln&yacute;</trd>,
    <trd>n&ecaron;&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>,
    <trd>liebevoll</trd>,
    <trd>weichherzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tender</trd>,
    <trd>affectionate</trd>,
    <trd>fond</trd>,
    <trd>loving</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>afectuoso(a)</trd>,
    <trd>tierno(a)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affectueux</trd>,
    <trd>tendre<klr> (affectueux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyeng&eacute;d</trd>,
    <trd>l&aacute;gy</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20778;&#12375;&#12356;  <pr>&#12420;&#12373;&#12375;&#12356;</pr></trd>,
    <trd>&#24859;&#24773;&#12398;&#12371;&#12418;&#12387;&#12383;  <pr>&#12354;&#12356;&#12376;&#12423;&#12358;&#12398;&#12371;&#12418;&#12387;&#12383;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czu&lstroke;y</trd>,
    <trd>delikatny</trd>,
    <trd>&lstroke;agodny</trd>,
    <trd>tkliwy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>terno</trd>,
    <trd>afetuoso</trd>,
    <trd>carinhoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&aacute;skypln&yacute;</trd>,
    <trd>ne&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_ib;&c_zh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28201;&#23384; <pr>w&emacron;nc&uacute;n</pr></trd>,
    <trd>&#28331;&#23384; <pr>w&emacron;nc&uacute;n</pr></trd>,
    <trd>&#32382; <pr>qi&#462;n</pr></trd>,
    <trd>&#32561; <pr>qi&#462;n</pr></trd>,
    <trd>&#26580;&#24773; <pr>r&oacute;uq&iacute;ng</pr></trd>,
    <trd>&#20307;&#36148; <pr>t&#464;ti&emacron;</pr></trd>,
    <trd>&#39636;&#36028; <pr>t&#464;ti&emacron;</pr></trd>,
    <trd>&#32048;&#23273; <pr>x&igrave;n&egrave;n</pr></trd>,
    <trd>&#32454;&#23273; <pr>x&igrave;n&egrave;n</pr></trd>,
    <trd>&#26580;&#21644; <pr>r&oacute;uh&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tener.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kun ama mildo, prizorgeme:
      <ekz>
        Feniksa <klr>[&#8230;]</klr> <tld/>e lin demandis pri la farto
        <fnt>
        <bib>RugxDom</bib>,
        <lok>&ccirc;apitro 69a, volumo 2a, p. 468a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li direktis al la parolinta virino tre mildan, dol&ccirc;an, karesan rigardon, kiu
        igis &scirc;in fandi&gcirc;i <tld/>e
        <fnt>
          <bib>ChV</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        Stefanon &scirc;i amis <tld/>e
        <fnt>
          <bib>ChR</bib>
          <lok>Stefano fu&gcirc;as de la polico</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_a;&c_zh;&c_n;&c_o;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_ja;&c_shch;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tendrament</trd>,
    <trd>dol&ccedil;ament</trd>
  </trdgrp>
  <trd lng="cs">teneramente</trd>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>,
    <trd>liebevoll</trd>,
    <trd>weichherzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tenderly</trd>,
    <trd>affectionately</trd>,
    <trd>fondly</trd>,
    <trd>lovingly</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>afectuosamente</trd>,
    <trd>tiernamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affectueusement</trd>,
    <trd>tendrement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyeng&eacute;den</trd>,
    <trd>l&aacute;gyan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czule</trd>,
    <trd>delikatnie</trd>,
    <trd>&lstroke;agodnie</trd>,
    <trd>tkliwie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_s; <ind>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_mol;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="sk">ne&zcaron;ne</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_ib;&c_zh;&c_n;&c_o;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="tener.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tener.0i.konduti">
    <dif>
      Konduti <tld/>e:
      <ekz>
        &scirc;i sentis emon &gcirc;in karesi, &gcirc;in kisi, <tld/>i al &gcirc;i
        <fnt>
          <bib>ChM</bib>
          <lok>1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kares.0i">karesi</ref>,
      <ref cel="kajxol.0i">ka&jcirc;oli</ref>,
      <ref cel="knufl.0i">knufli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">liebkosen</trd>
<trd lng="en">snuggle</trd>
</drv>

</art>
