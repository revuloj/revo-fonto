<drv mrk="prav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      <klr>(iu)</klr> Havanta <ref cel="opini.0o">opinio</ref>n a&ubreve;
      konduton konforman al vero kaj justo:
      <ekz>
        oni ju&gcirc;u ilin, kaj oni deklaru <tld/>a la <tld/>ulon, kaj la mal<tld/>ulon oni
        kondamnu
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 25:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        plendito, kion vi povas diri por via <tld/>i&gcirc;o
        <fnt>
          <bib>DL</bib>
        </fnt>?
      </ekz>
      <ekz>
        kolero <tld/>econ ne donas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        larmoj <tld/>econ ne pruvas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Recht haben</trd>,
      <trd>im <ind>Recht</ind> sein</trd>
    </trdgrp>
    <trd lng="nl">gelijk hebbend</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; <klr tip="amb">(&c_t;&c_o;&c_t;, &c_k;&c_t;&c_o; &c_p;&c_r;&c_a;&c_v;)</klr></trd>
  </snc>
  <snc>
    <dif>
      <klr>(io)</klr> Konforma al vero kaj justo, pp opinio:
      <ekz>
        kial vi malla&ubreve;das <tld/>ajn vortojn
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 6:25</lok>
        </fnt>?
      </ekz>
      <ekz>
        la kalkulo &scirc;ajne estas tute <tld/>a kaj povas timigi &ccirc;iun
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ia <tld/>eco estas metita ekster &ccirc;ian dubon
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">richtig</trd>
    <trdgrp lng="nl">
      <trd>juist</trd>,
      <trd>gegrond</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>maj&iacute;c&iacute; pravdu</trd>,
    <trd>pravdiv&yacute;</trd>,
    <trd>prav&yacute;</trd>,
    <trd>spr&aacute;vn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>im <ind>Recht</ind></trd>,
  </trdgrp>
  <trd lng="en">true</trd>
  <trdgrp lng="fr">
    <trd>justifi&eacute;</trd>,
    <trd>ayant <ind>raison</ind></trd>
  </trdgrp>
  <trd lng="hu">igaz</trd>
  <trdgrp lng="id">
    <trd>benar</trd>,
    <trd>betul</trd>
