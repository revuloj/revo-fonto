  <trdgrp lng="ja">
    <trd>&#x6B63;&#x5F53;&#x306A;  <pr>&#x305B;&#x3044;&#x3068;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x6B63;&#x3057;&#x3044;  <pr>&#x305F;&#x3060;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x9053;&#x7406;&#x306B;&#x304B;&#x306A;&#x3063;&#x305F; 
    <pr>&#x3069;&#x3046;&#x308A;&#x306B;&#x304B;&#x306A;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x3082;&#x3063;&#x3068;&#x3082;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="pl">
