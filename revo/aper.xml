    <trd>vydat knihu</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tsadi;&yod;&gimel;</trd>
  <trdgrp lng="ja">
    <trd>&#x73FE;&#x308F;&#12377;  <pr>&#12354;&#12425;&#x308F;&#12377;</pr></trd>,
    <trd>&#x767A;&#x884C;&#12377;&#x308B;  <pr>&#x306F;&#12387;&#12371;&#12358;&#12377;&#x308B;</pr></trd>
