<drv mrk="aper.formal0i">
  <kap>
    formal<tld/>i
  </kap>
  <snc>
    <dif>
      Mal<tld/>i definitive; perdi&gcirc;i, morti, ka&scirc;e fu&gcirc;i:
      <ekz>
        la akirita&jcirc;o de la potenculo formal<tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 49:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;asa&jcirc;o estas jam proksime! oni devas kiel eble plej rapide &gcirc;in &ccirc;irka&ubreve;bari,
        &ccirc;ar &gcirc;i povus formal<tld/>i
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        kiel forfalas la flora&jcirc;o de la pomarbo <klr>[tiel]</klr> anka&ubreve; la homoj
        formal<tld/>as
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i penis nerimarkite formal<tld/>i la&ubreve;longe de la muro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_z;&c_a;&c_w;&c_s;&c_jo;&c_d;&c_y;)</klr></trd>
  <trdgrp lng="de">
