<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aper</rad>/i
</kap>
<drv mrk="aper.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sin vidigi, ekmontri&gcirc;i, fari&gcirc;i videbla:
      <ekz>
        iu nekonata <tld/>is &ccirc;e la pordo;
      </ekz>
      <ekz>
        la unuaj floroj <tld/>as en la &gcirc;ardeno;
      </ekz>
      <ekz>tempo toleras, sed vero <tld/>as<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        post silenta tempo <tld/>os revigli&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiam povas <tld/>i ia dubo, la rajto de decidado apartenas nur al
        la LK<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en tiuj okazoj, kie malkompreni&gcirc;o povas <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i neceseco <tld/>as
        nur tre malofte<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nun la unuan fojon nia
        kongreso <tld/>as sub la oficiala sankcio de
        registaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        iufoje en la kapo de iu samideano <tld/>as la sekvanta
        demando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se <tld/>as iaj demandoj, kiuj
        koncernas la tutan esperantistaron, tiam kiu havas la rajton ilin
        solvi?<fnt>Z</fnt>
      </ekz>
      <ekz>
        tiu ideo <tld/>is &ccirc;e li tre
        frue<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspekt.0i">aspekti</ref>,
      <ref cel="sxpruc.0i">&scirc;pruci</ref>,
      <ref cel="leg">esti legebla</ref>,
      <ref cel="bril.0i">brili</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>appara&icirc;tre</trd>,
      <trd>se <ind>montrer</ind></trd>,
      <trd>se faire <ind>jour</ind></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Sin montri ia a&ubreve; tia, &scirc;ajni:
      <ekz>
        <tld/>i senkulpa<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i pli granda<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i mensoganto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        miaj rimedoj de solvo <tld/>os eble kiel tro simplaj al la
        leganto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Esperanto vere <tld/>as kiel sola
        kandidato por lingvo internacia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiel grandega
        malhelpo <tld/>os la reciproka envio de la popoloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tia &scirc;an&gcirc;o <tld/>as kiel tute neakceptebla<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>appara&icirc;tre</trd>,
      <trd>para&icirc;tre</trd>,
      <trd>sembler</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TIP</uzo>
    <dif>
      Esti publikigata (pri libroj, gazetoj):
      <ekz>
        la revuo <tld/>as &ccirc;iumonate.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>para&icirc;tre</trd>,
      <trd>&ecirc;tre <ind>publi&eacute;</ind></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Sin prezenti ie:
      <ekz>
        en granda nombro ili <tld/>is por deziri al ni
        bonvenon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>as anta&ubreve; vi kiel
        tradicia malfermanto de niaj kongresoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        jam la trian fojon vi <tld/>as anta&ubreve; la tribunalo!
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>para&icirc;tre</trd>,
      <trd>se <ind>pr&eacute;senter</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;'&c_ja;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">objevit se</trd>
  <trd lng="de">erscheinen</trd>
  <trd lng="en">appear</trd>
  <trdgrp lng="it">
    <trd>apparire</trd>,
    <trd>mostrarsi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&#x43f;&#x43e;&#x44f;&#x432;&#x43b;&#x44f;&#x442;&#x44c;&#x441;&#x44f;</trd>,
    <trd>&#x44f;&#x432;&#x43b;&#x44f;&#x442;&#x44c;&#x441;&#x44f;</trd>,
    <trd>&#x43e;&#x431;&#x44a;&#x44f;&#x432;&#x43b;&#x44f;&#x442;&#x44c;&#x441;&#x44f;</trd>
  </trdgrp>
</drv>

<drv mrk="aper.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc mrk="aper.0ajxo.lauxverba">
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        li rigardadis la levi&gcirc;on de la suno, kaj tiu
        <tld/>a&jcirc;o lin ravis;
      </ekz>
      <ekz>
        fantoma <tld/>a&jcirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_z;'&c_ja;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Erscheinung</trd>
    <trd lng="fr">apparition</trd>
    <trd lng="ru">&#x44f;&#x432;&#x43b;&#x435;&#x43d;&#x438;&#x435;</trd>
  </snc>
  <snc mrk="aper.0ajxo.fenomeno">
    <dif>
      <klr>(ark)</klr>
      <ref tip="dif" cel="fenome.0o">fenomeno</ref>:
      <ekz>
        tio estas <tld/>o tute natura<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu lingvo natura sur &ccirc;iu pa&scirc;o ni
        renkontas la sekvantan <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">ph&eacute;nom&egrave;ne</trd>
    <trdgrp
      lng="ru">
      <trd>&#x44f;&#x432;&#x43b;&#x435;&#x43d;&#x438;&#x435;</trd>,
      <trd>&#x444;&#x435;&#x43d;&#x43e;&#x43c;&#x435;&#x43d;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aper.0ajxo.ekz">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Nomo, signo, simbolo, vorto a&ubreve; frazo konsiderata kun la
      loko en kiu &gcirc;i <tld/>as:
      <ekz>
        la redaktilo trovis 100 <tld/>ojn de la variablo
        <frm>x</frm> en la programo;
      </ekz>
      <ekz>
        difina, aplika <tld/>o de variablonomo en programo.
      </ekz>
    </dif>
    <ref tip="super" cel="ekzemp1.0o">ekzemplero</ref>
    <ref tip="malprt" cel="tekst.kun0ocxirkaux0o">kunteksto</ref>
    <trdgrp lng="en">
      <trd>occurrence</trd>, <trd>appearance</trd>,
      <trd>instance</trd>
    </trdgrp>
    <trd lng="fr">occurrence</trd>
    <trd
    lng="ru">&#x432;&#x445;&#x43e;&#x436;&#x434;&#x435;&#x43d;&#x438;&#x435;</trd>
  </snc>
</drv>

<drv mrk="aper.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Vidigi:
      <ekz>
        sian tutan koleron <tld/>igas malsa&gcirc;ulo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 29, 11</lok>)</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kovr.mal0i">malkovri</ref>,
      <ref cel="kasx.mal0i">malka&scirc;i</ref>,
      <ref cel="manife.0i">manifesti</ref>,
      <ref cel="don.el0i">eldoni</ref>,
      <ref cel="sxel.el0igi">el&scirc;eligi</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>montrer</trd>,
      <trd>laisser <ind>para&icirc;tre</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="aper.mal0i">
  <kap><ofc>*</ofc>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari&gcirc;i nevidebla, ne plu esti videbla:
      <ekz>mal<tld/>is kiel vaporo<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>mal<tld/>is kiel &scirc;tono en maron
        <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="paf.0i">pafi</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Nenii&gcirc;i, ne plu esti:
      <ekz>
        tiel mal<tld/>is tiu granda homo;
      </ekz>
      <ekz>
        mal<tld/>is ilia forto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jeremia 51, 30</lok>)</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxes.0i">&ccirc;esi</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="pere.0i">perei</ref>,
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="dron.0i">droni</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="scen.0o">sceno</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="de">verschwinden</trd>
  <trd lng="fr">dispara&icirc;tre</trd>
  <trd lng="it">sparire</trd>
  <trd
  lng="ru">&#x438;&#x441;&#x447;&#x435;&#x437;&#x430;&#x442;&#x44c;</trd>
</drv>

<drv mrk="aper.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi, fari ne plu videbla, ekz-e per formovo (ka&scirc;i,
      ekzili) a&ubreve; neniigo.
    </dif>
    <refgrp tip="vid">
      <ref cel="vual.0o">vualo</ref>;
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="detru.0i">detrui</ref>,
      <ref cel="fand.dis0ifor0i">forfandi</ref>.
    </refgrp>
    <trdgrp
      lng="ru">
      <trd>&#x437;&#x430;&#x441;&#x442;&#x430;&#x432;&#x438;&#x442;&#x44c;
      <ind>&#x438;&#x441;&#x447;&#x435;&#x437;&#x43d;&#x443;&#x442;&#x44c;</ind></trd>,
      <trd>&#x443;&#x434;&#x430;&#x43b;&#x438;&#x442;&#x44c;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>dispara&icirc;tre</ind></trd>,
    <trd>escamoter</trd>,
    <trd>supprimer</trd>
  </trdgrp>
</drv>

<drv mrk="aper.re0anto">
  <kap>re<tld/>anto</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fantom.0o">Fantomo</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="ombr.0o">ombro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>revenant</trd>,
    <trd>apparition</trd>
  </trdgrp>
  <trd lng="it">apparizione</trd>
  <trd lng="ru">&#x43f;&#x440;&#x438;&#x437;&#x440;&#x430;&#x43a;</trd>
</drv>
</art>
