<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aper</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aper.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aper.0i.sinVidigi">
    <dif>
      Sin vidigi, ekmontri&gcirc;i, fari&gcirc;i videbla:
      <ekz>
        la unuaj floroj <tld/>as en la &gcirc;ardeno;
      </ekz>
      <ekz>
        en la pordo de l' &gcirc;ardeno <tld/>is la sana figuro de bela junulo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>tempo toleras, sed vero <tld/>as</ind><fnt><bib>PrV</bib>
        </fnt>;
        <trd lng="hu">
        az igazs&aacute;g el&odblac;bb-ut&oacute;bb kider&uuml;l</trd>
      </ekz>
      <ekz>
        ne voku diablon, &ccirc;ar li povas <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        jen la Morto <tld/>is &ccirc;e lia lito
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>;
      </ekz>
      <ekz>
        post <klr>[&#8230;]</klr> silenta tempo <klr>[&#8230;]</klr> <tld/>os revigli&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj okazoj, kie malkompreni&gcirc;o povas <tld/>i, ni devas &gcirc;in eviti
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i neceseco <tld/>as nur tre malofte
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Volap&uuml;k</nom> <tld/>is tiam, kiam la entuziasmo de la publiko por la nova ideo estis
        ankora&ubreve; tute fre&scirc;a
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun la unuan fojon nia
        kongreso <tld/>as sub la oficiala sankcio de
        <klr>[&#8230;]</klr> registaro
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        iufoje en la kapo de iu samideano <tld/>as la sekvanta
        demando
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>as iaj demandoj, kiuj koncernas la tutan 
        esperantistaron, <klr>[&#8230;]</klr> tiam kiu havas la rajton ilin
        solvi
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto 1911</vrk>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspekt.0i">aspekti</ref>,
      <ref cel="bril.0i">brili</ref>,
      <ref cel="leg">esti legebla</ref>,
      <ref cel="sxpruc.0i">&scirc;pruci</ref>
    </refgrp>
    <trd lng="br">dont war wel</trd>
    <trd lng="es">aparecer</trd>
    <trd lng="ms">menjemal</trd>
    <trdgrp lng="fr">
      <trd>appara&icirc;tre</trd>,
      <trd>se <ind>montrer</ind></trd>,
      <trd>se faire <ind>jour</ind></trd>
    </trdgrp>
    <trd lng="hu">megjelenik</trd>
    <trdgrp lng="pl">
      <trd>pojawia&cacute; si&eogonek;</trd>,
      <trd>ukaza&cacute; si&eogonek;</trd>,
      <trd>pokaza&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="pt">aparecer</trd>
  </snc>
  <snc mrk="aper.0i.sxajni">
    <dif>
      Sin montri ia a&ubreve; tia, &scirc;ajni:
      <ekz>
        <tld/>i senkulpa;
      </ekz>
      <ekz>
        kiam li ekvidis la altajn montojn, la mondo denove <tld/>is al li pli granda
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne <tld/>u mensoganto
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj rimedoj de solvo <tld/>os eble kiel tro simplaj <klr>[al la
        leganto]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto vere <tld/>as kiel sola
        kandidato por lingvo internacia
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel grandega
        malhelpo tie &ccirc;i <tld/>os <klr>[&#8230;]</klr> la reciproka envio de la popoloj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &scirc;an&gcirc;o <klr>[&#8230;]</klr> devas <tld/>i kiel tute ne akceptebla
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>seblantout</trd>,
      <trd>ha&ntilde;valout</trd>
    </trdgrp>
    <trd lng="es">parecer</trd>
    <trdgrp lng="fr">
      <trd>appara&icirc;tre</trd>,
      <trd>para&icirc;tre</trd>,
      <trd>sembler</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&udblac;nik</trd>,
      <trd>l&aacute;tszik</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>jawi&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="sk">javi&tcaron; sa</trd>
  </snc>
  <snc mrk="aper.0i.publigata">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Esti publikigata (pri libroj, gazetoj):
      <ekz>
        la revuo <tld/>as &ccirc;iumonate;
      </ekz>
      <ekz>
        la revuo Progreso da&ubreve;re aperas ekde 1908
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2010/009999.php"
              >Interlingvo: komparo inter du solvoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        regule <tld/>as gazetaraj komunikoj de UEA
        <fnt>
          <bib>Monato</bib>
          <aut>Grigorij Arosev, Andrej Grigorjevskij</aut>
          <vrk><url ref="https://www.monato.be/2004/008210.html"
              >Abonu!</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">dont er-maez<klr tip="ind"> (levrio&ugrave;)</klr>
    </trd>
    <trd lng="es">aparecer</trd>
    <trdgrp lng="fr">
      <trd>para&icirc;tre</trd>,
      <trd>&ecirc;tre <ind>publi&eacute;</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megjelenik<klr tip="ind"> (kiadv&aacute;ny)</klr></trd>,
      <trd>kiadj&aacute;k</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ukaza&cacute; si&eogonek;</trd>,
      <trd>pokaza&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aper.0i.sinPrezenti">
    <dif>
      Sin prezenti ie:
      <ekz>
        en kia granda nombro ili <tld/>is por deziri al ni
        bonvenon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ante anta&ubreve; vi kiel
        tradicia malfermanto de niaj kongresoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam la trian fojon vi <tld/>as anta&ubreve; la tribunalo!
      </ekz>
    </dif>
    <trd lng="br">en em gavout</trd>
    <trd lng="es">aparecer</trd>
    <trdgrp lng="fr">
      <trd>para&icirc;tre</trd>,
      <trd>se <ind>pr&eacute;senter</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;'&c_ja;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>objevit se</trd>,
    <trd>vyj&iacute;t <klr>(kniha, periodikum)</klr></trd>
  </trdgrp>
  <trd lng="de">erscheinen</trd>
  <trd lng="en">appear</trd>
  <trd lng="hu">megjelenik</trd>
  <trdgrp lng="it">
    <trd>apparire</trd>,
    <trd>mostrarsi</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pojawia&cacute; si&eogonek;</trd>,
    <trd>zjawia&cacute; si&eogonek;</trd>,
    <trd>pokaza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>
    &c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>objavi&tcaron; sa</trd>,
    <trd>vyjs&tcaron; <klr>(kniha, &ccaron;asopis)</klr></trd>,
    <trd>zjavi&tcaron; sa</trd>
  </trdgrp>
  <trd lng="tp">kama</trd>
</drv>

<drv mrk="aper.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="aper.0ajxo.lauxverba">
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        li rigardadis la levi&gcirc;on de la suno, kaj tiu
        <tld/>a&jcirc;o lin ravis;
      </ekz>
      <ekz>
        fantoma <tld/>a&jcirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_z;'&c_ja;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
    </trd>
    <trd lng="br">gweledigezh</trd>
    <trd lng="de">Erscheinung</trd>
    <trd lng="es">aparici&oacute;n</trd>
    <trd lng="fr">apparition</trd>
    <trd lng="hu">l&aacute;tv&aacute;ny</trd>
    <trdgrp lng="pl">
      <trd>zjawisko</trd>,
      <trd>widziad&lstroke;o</trd>,
      <trd>pokaz</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="aper.0ajxo.fenomeno">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="fenome.0o">fenomeno</ref>
    <ekz>
      tio estas <tld/>o tute natura<fnt>Z</fnt>;
    </ekz>
    <ekz>
      en &ccirc;iu lingvo natura sur &ccirc;iu pa&scirc;o ni
      renkontas la sekvantan <tld/>on<fnt>Z</fnt>.
    </ekz>
    <trd lng="fr">ph&eacute;nom&egrave;ne</trd>
    <trd lng="hu">jelens&eacute;g</trd>
    <trdgrp lng="pl">
      <trd>zjawisko</trd>
    </trdgrp>
    <trdgrp
      lng="ru">
      <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aper.0ajxo.ekz">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Nomo, signo, simbolo, vorto a&ubreve; frazo konsiderata kun la
      loko en kiu &gcirc;i <tld/>as:
      <ekz>
        la redaktilo trovis 100 <tld/>ojn de la variablo
        <frm>x</frm> en la programo;
      </ekz>
      <ekz>
        difina, aplika <tld/>o de variablonomo en programo.
      </ekz>
    </dif>
    <ref tip="super" cel="ekzemp1.0o">ekzemplero</ref>
    <ref tip="malprt" cel="tekst.kun0o.LIN">kunteksto</ref>
    <trdgrp lng="en">
      <trd>occurrence</trd>, <trd>appearance</trd>,
      <trd>instance</trd>
    </trdgrp>
    <trd lng="fr">occurrence</trd>
    <trd lng="hu">el&odblac;fordul&aacute;s</trd>
    <trdgrp lng="pl">
      <trd>wyst&aogonek;pienie</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_h;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
    <trd lng="sk">v&yacute;skyt</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>nabyt&iacute; v&ecaron;dom&iacute; po anestezii</trd>,
    <trd>objeven&iacute; se</trd>,
    <trd>vyno&rcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&uacute;kaz</trd>,
    <trd>objavenie sa</trd>,
    <trd>vynorenie</trd>
  </trdgrp>
</drv>

<drv mrk="aper.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="aper.0igi.vidigi">
    <dif>
      Vidigi:
      <ekz>
        sian tutan koleron <tld/>igas malsa&gcirc;ulo
        <fnt><bib>MT</bib>, <lok>&Sen; 29:11</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.el0i">eldoni</ref>,
      <ref cel="sxel.el0igi">el&scirc;eligi</ref>,
      <ref cel="kovr.mal0i">malkovri</ref>,
      <ref cel="kasx.mal0i">malka&scirc;i</ref>,
      <ref cel="manife.0i">manifesti</ref>
    </refgrp>
    <trd lng="de">erscheinen lassen</trd>
    <trdgrp lng="fr">
      <trd>montrer</trd>,
      <trd>laisser <ind>para&icirc;tre</ind></trd>
    </trdgrp>
    <trd lng="hu">megjelentet</trd>
    <trdgrp lng="pl">
      <trd>pokaza&cacute;</trd>,
      <trd>ujawni&cacute;</trd>
    </trdgrp>
    <trd lng="sk">prejavi&tcaron;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>uk&aacute;zat</trd>,
    <trd>vydat knihu</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>publikova&tcaron;</trd>,
    <trd>uverejni&tcaron;</trd>,
    <trd>vyda&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="aper.mal0i">
  <kap><ofc>*</ofc>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aper.mal0i.nevidebligxi">
    <dif>
      Fari&gcirc;i nevidebla, ne plu esti videbla:
      <ekz>
        <ind>mal<tld/>is kiel vaporo</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">elillant, mint a k&aacute;mfor</trd>
        <trd lng="pl">znikn&aogonek;&lstroke; jak kamfora</trd>
      </ekz>
      <ekz>
        <ind>mal<tld/>is kiel &scirc;tono en maron</ind>
        <fnt><bib>PrV</bib></fnt>
        <trd lng="hu">elt&udblac;nt, mint k&odblac; a v&iacute;zben
        </trd>
        <trd lng="pl">przepad&lstroke; jak kamie&nacute; w wod&eogonek;
        </trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="paf.0i">pafi</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd>ste&ubreve;zia&ntilde;</trd>,
      <trd>mont diwar wel</trd>
  	</trdgrp>
    <trd lng="hu">elt&udblac;nik</trd>
    <trdgrp lng="pl">
      <trd>znikn&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aper.mal0i.neniigxi">
    <dif>
      Nenii&gcirc;i, ne plu esti:
      <ekz>
        tiel mal<tld/>is tiu granda homo;
      </ekz>
      <ekz>
        mal<tld/>is ilia forto
        <fnt><bib>MT</bib>, <lok>&Jer; 51:30</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxes.0i">&ccirc;esi</ref>,
      <ref cel="dron.0i">droni</ref>,
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="pere.0i">perei</ref>,
      <ref cel="scen.0o">sceno</ref>
    </refgrp>
    <trd lng="br">mont da get</trd>
    <trd lng="hu">eleny&eacute;szik</trd>
    <trdgrp lng="pl">
      <trd>zgin&aogonek;&cacute;</trd>,
      <trd>przepa&sacute;&cacute;</trd>,
      <trd>zagin&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>mizet</trd>,
    <trd>zmizet</trd>
  </trdgrp>
  <trd lng="de">verschwinden</trd>
  <trd lng="fr">dispara&icirc;tre</trd>
  <trd lng="it">sparire</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prepadn&uacute;&tcaron; sa</trd>,
    <trd>rozplyn&uacute;&tcaron; sa</trd>,
    <trd>zmizn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="aper.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aper.mal0igi.forigi">
    <dif>
      Forigi, fari ne plu videbla, ekzemple per formovo (ka&scirc;i,
      ekzili) a&ubreve; neniigo.
    </dif>
    <refgrp tip="vid">
      <ref cel="detru.0i">detrui</ref>,
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="fand.dis0ifor0i">forfandi</ref>,
      <ref cel="vual.0o">vualo</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; 
      <ind>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_u;&c_t;&c_mol;</ind>
      </trd>,
      <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>odstranit</trd>,
    <trd>zni&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">verschwinden lassen</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>dispara&icirc;tre</ind></trd>,
    <trd>escamoter</trd>,
    <trd>effacer</trd>,
    <trd>supprimer</trd>
  </trdgrp>
