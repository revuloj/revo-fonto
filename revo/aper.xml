<drv mrk="aper.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="aper.0igi.vidigi">
    <dif>
      <ref tip="dif" cel="vid.0igi">Vidigi</ref>,
      <ref tip="sin" cel="montr.0i.prezenti">montri</ref>:
      <ekz>
        sian tutan koleron <tld/>igas malsa&gcirc;ulo
        <fnt><bib>MT</bib>, <lok>&Sen; 29:11</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;e klaso li jam <tld/>igis kleron neordinaran kaj talenton por verkado
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne <tld/>igis tamen miron nek ian alian senton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia viza&gcirc;o <tld/>igis jen mortan palecon, jen koloron de sango
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de"><ind>erscheinen</ind> lassen</trd>
    <trdgrp lng="fr">
      <trd>montrer</trd>,
      <trd>laisser <ind>para&icirc;tre</ind></trd>
    </trdgrp>
    <trd lng="hu">megjelentet</trd>
    <trdgrp lng="pl">
      <trd>pokaza&cacute;</trd>,
      <trd>ujawni&cacute;</trd>
    </trdgrp>
    <trd lng="sk">prejavi&tcaron;</trd>
  </snc>
  <snc mrk="aper.0igi.estigi">
    <dif>
      <ref tip="dif" cel="est.0igi">Estigi</ref>,
      <ref tip="sin" cel="nask.0i.estigi">naski</ref>:
      <ekz>
        la tero <tld/>igu vivajn esta&jcirc;ojn, la&ubreve; ilia speco, brutojn kaj rampa&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Eternulo <tld/>igis tondrojn kaj hajlon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 9:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la figarbo <klr>[&#8230;]</klr> jam moli&gcirc;as kaj <tld/>igas foliojn
        <fnt>
          <bib>NT</bib>
          <lok>Marko 13:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi povoscias <tld/>igi fajrerojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="de">hervorbringen</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="don.el0i">eldoni</ref>,
    <ref cel="sxel.el0igi">el&scirc;eligi</ref>,
    <ref cel="kovr.mal0i">malkovri</ref>,
    <ref cel="kasx.mal0i">malka&scirc;i</ref>,
    <ref cel="manife.0i">manifesti</ref>
  </refgrp>
  <trdgrp lng="cs">
    <trd>uk&aacute;zat</trd>,
    <trd>vydat knihu</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tsadi;&yod;&gimel;</trd>
  <trdgrp lng="sk">
    <trd>publikova&tcaron;</trd>,
    <trd>uverejni&tcaron;</trd>,
    <trd>vyda&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;; &c_v;&c_i;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i;; &c_v;&c_i;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i; &c_u; &c_s;&c_v;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="aper.mal0i">
  <kap><ofc>*</ofc>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aper.mal0i.nevidebligxi">
    <dif>
      Fari&gcirc;i nevidebla, ne plu esti videbla:
      <ekz>
        la globo mal<tld/>is, kaj la puto estis tiel profunda, ke oni ne povis vidi la
        fundon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Re&gcirc;o rano a&ubreve; fera Henriko</lok>
        </fnt>;
      </ekz>
      <ekz>
        Amalio forkuris, la grafo subite mal<tld/>is
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ree forkuris kaj fine tute mal<tld/>is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun tiuj &ccirc;i vortoj li mal<tld/>is en la arbaron
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>is kiel vaporo</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">elillant, mint a k&aacute;mfor</trd>
        <trd lng="pl">znikn&aogonek;&lstroke; jak kamfora</trd>
      </ekz>
      <ekz>
        <ind>mal<tld/>is kiel &scirc;tono en maron</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">elt&udblac;nt, mint k&odblac; a v&iacute;zben
        </trd>
        <trd lng="pl">przepad&lstroke; jak kamie&nacute; w wod&eogonek;
        </trd>
      </ekz>
    </dif>

    <refgrp tip="vid">
      <ref cel="for.0igxi">fori&gcirc;i</ref>,
      <ref cel="paf.for0igxi">forpafi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="br">
      <trd>ste&ubreve;zia&ntilde;</trd>,
      <trd>mont diwar wel</trd>
  	</trdgrp>
    <trd lng="hu">elt&udblac;nik</trd>
    <trdgrp lng="pl">
      <trd>znikn&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aper.mal0i.neniigxi">
    <dif>
      Nenii&gcirc;i, ne plu esti:
      <ekz>
        tiel mal<tld/>is tiu granda homo;
      </ekz>
      <ekz>
        li mal<tld/>is, &ccirc;ar Dio lin prenis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 5:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>is ilia forto
        <fnt><bib>MT</bib>, <lok>&Jer; 51:30</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[iuj]</klr> tradicioj rapide mal<tld/>as
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam havo mal<tld/>is, sa&gcirc;o aperis
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        le&gcirc;o mal<tld/>as, moro da&ubreve;ras
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        matenas, vesperas &dash; kaj tago mal<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxes.0i">&ccirc;esi</ref>,
      <ref cel="dron.0i">droni</ref>,
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="velk.for0i">forvelki</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="pal.0igxi.FIG">pali&gcirc;i<sncref/></ref>,
      <ref cel="pas.0i.forpasi">pasi<sncref/></ref>,
      <ref cel="pere.0i">perei</ref>
    </refgrp>
    <trd lng="br">mont da get</trd>
    <trd lng="hu">eleny&eacute;szik</trd>
    <trdgrp lng="pl">
      <trd>zgin&aogonek;&cacute;</trd>,
      <trd>przepa&sacute;&cacute;</trd>,
      <trd>zagin&aogonek;&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>mizet</trd>,
    <trd>zmizet</trd>
  </trdgrp>
  <trd lng="de">verschwinden</trd>
  <trd lng="fr">dispara&icirc;tre</trd>
  <trd lng="it">sparire</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prepadn&uacute;&tcaron; sa</trd>,
    <trd>rozplyn&uacute;&tcaron; sa</trd>,
    <trd>zmizn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_shch;&c_je;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
<drv mrk="aper.formal0i">
  <kap>
    formal<tld/>i
  </kap>
  <snc>
    <dif>
      Mal<tld/>i definitive; perdi&gcirc;i, morti, ka&scirc;e fu&gcirc;i:
      <ekz>
        la akirita&jcirc;o de la potenculo formal<tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 49:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;asa&jcirc;o estas jam proksime! oni devas kiel eble plej rapide &gcirc;in &ccirc;irka&ubreve;bari,
        &ccirc;ar &gcirc;i povus formal<tld/>i
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        kiel forfalas la flora&jcirc;o de la pomarbo <klr>[tiel]</klr> anka&ubreve; la homoj
        formal<tld/>as
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i penis nerimarkite formal<tld/>i la&ubreve;longe de la muro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>vergehen</trd>,
    <trd><ind>verloren</ind> gehen</trd>,
    <trd>sich in Luft <ind>aufl&ouml;sen</ind></trd>,
    <trd>unbemerkt <ind>fliehen</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>evaporar</ind>se<klr tip="ind">
      (desaparecer)</klr></trd>,
    <trd><ind>volatilizar</ind>se<klr tip="ind">
      (desaparecer)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind><klr tip="ind">
      (dispara&icirc;tre)</klr></trd>,
    <trd>se <ind>volatiliser</ind><klr tip="ind">
      (dispara&icirc;tre)</klr></trd>
  </trdgrp>
  <trd lng="nl">in <ind>rook</ind> opgaan</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
</drv>






<drv mrk="aper.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aper.mal0igi.forigi">
    <dif>
      <ref tip="dif" cel="for.0igi">Forigi</ref>, fari ne plu videbla, ekzemple per formovo (ka&scirc;i,
      ekzili) a&ubreve; neniigo:
      <ekz>
        vi ne mal<tld/>igos mian nomon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 24:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maliculoj estos mal<tld/>igitaj de tie
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 2:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vi mal<tld/>igis &ccirc;iun memoron pri ili
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 26:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        per komplikaj procedoj dungitoj de la banko mal<tld/>igis 4,5 milionojn da USD
        <fnt>
          <bib>Monato</bib>
          <aut>Serge-Benjamin Zandandu</aut>
          <vrk><url ref="&Monato;/2006/009044.php">Kongaj bankoj ekhavas malbonan
          reputacion</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        infrastrukturaj projektoj en la malsupra Danubo povus tute mal<tld/>igi la forte
        minacatajn populaciojn de sturgo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2010/010038.php">Priatenti la
          naturprotekton</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detru.0i">detrui</ref>,
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="fand.dis0ifor0i">forfandi</ref>,
      <ref cel="vual.0o">vualo</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; 
      <ind>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_u;&c_t;&c_mol;</ind>
      </trd>,
      <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odstranit</trd>,
    <trd>zni&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>verschwinden</ind> lassen</trd>,
    <trd><klr>(etwas)</klr> entfernen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>dispara&icirc;tre</ind></trd>,
    <trd>escamoter</trd>,
    <trd>effacer</trd>,
    <trd>supprimer</trd>
  </trdgrp>
  <trd lng="hu">elt&uuml;ntet</trd>
  <trdgrp lng="pl">
    <trd>ukry&cacute;</trd>,
    <trd>schowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odstr&aacute;ni&tcaron;</trd>,
    <trd>zni&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_i;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="aper.re0anto">
  <kap>re<tld/>anto</kap>
  <snc mrk="aper.re0anto.fantomo">
    <dif>
      <ref tip="dif" cel="fantom.0o">Fantomo</ref>:
      <ekz>
        kiam &scirc;i rigardis la filon, &scirc;i sentis pikon en sia interno, li staris
        <klr>[&#8230;]</klr> kiel re<tld/>anto de sia avo
