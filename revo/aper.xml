    <trd>&#x6CEF;&#x706D; <pr>m&#x1D0;nmi&#xE8;</pr></trd>,
    <trd>&#x6D88;&#22833; <pr>xi&amacron;osh&#x12B;</pr></trd>,
    <trd>&#x6D88;&#36864; <pr>xi&amacron;otu&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
