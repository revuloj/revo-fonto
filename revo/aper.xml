<drv mrk="aper.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aper.0i.sinVidigi">
    <dif>
      Sin vidigi, ekmontri&gcirc;i, fari&gcirc;i videbla:
      <ekz>
        la unuaj floroj <tld/>as en la &gcirc;ardeno;
      </ekz>
      <ekz>
        en la pordo de l' &gcirc;ardeno <tld/>is la sana figuro de bela junulo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>tempo toleras, sed vero <tld/>as</ind><fnt><bib>PrV</bib>
        </fnt>;
        <trd lng="hu">
        az igazs&aacute;g el&odblac;bb-ut&oacute;bb kider&uuml;l</trd>
      </ekz>
      <ekz>
        ne voku diablon, &ccirc;ar li povas <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        jen la Morto <tld/>is &ccirc;e lia lito
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>;
      </ekz>
      <ekz>
        post <klr>[&#8230;]</klr> silenta tempo <klr>[&#8230;]</klr> <tld/>os revigli&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj okazoj, kie malkompreni&gcirc;o povas <tld/>i, ni devas &gcirc;in eviti
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i neceseco <tld/>as nur tre malofte
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Volap&uuml;k</nom> <tld/>is tiam, kiam la entuziasmo de la publiko por la nova ideo estis
        ankora&ubreve; tute fre&scirc;a
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun la unuan fojon nia
        kongreso <tld/>as sub la oficiala sankcio de
        <klr>[&#8230;]</klr> registaro
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        iufoje en la kapo de iu samideano <tld/>as la sekvanta
        demando
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>as iaj demandoj, kiuj koncernas la tutan
        esperantistaron, <klr>[&#8230;]</klr> tiam kiu havas la rajton ilin
        solvi
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
          1911</vrk>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspekt.0i">aspekti</ref>,
      <ref cel="bril.0i">brili</ref>,
      <ref cel="leg">esti legebla</ref>,
      <ref cel="sxpruc.0i">&scirc;pruci</ref>
    </refgrp>
    <trd lng="br">dont war wel</trd>
    <trd lng="es">aparecer</trd>
    <trd lng="ms">menjemal</trd>
    <trdgrp lng="fr">
      <trd>appara&icirc;tre</trd>,
      <trd>se <ind>montrer</ind></trd>,
      <trd>se faire <ind>jour</ind></trd>
    </trdgrp>
    <trd lng="hu">megjelenik</trd>
    <trdgrp lng="pl">
      <trd>pojawia&cacute; si&eogonek;</trd>,
      <trd>ukaza&cacute; si&eogonek;</trd>,
      <trd>pokaza&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="pt">aparecer</trd>
    <trdgrp lng="zh">
    <trd>&#x51FA;&#38754; <pr>ch&#x16B;mi&#xE0;n</pr></trd>,
    <trd>&#x4EFF;&#x4F5B; <pr>f&#x1CE;ngf&#xFA;</pr></trd>,
    <trd>&#x5F77;&#x5F7F; <pr>f&#x1CE;ngf&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="aper.0i.sxajni">
    <dif>
      Sin montri ia a&ubreve; tia, &scirc;ajni:
      <ekz>
        <tld/>i senkulpa;
      </ekz>
      <ekz>
        kiam li ekvidis la altajn montojn, la mondo denove <tld/>is al li pli granda
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne <tld/>u mensoganto
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj rimedoj de solvo <tld/>os eble kiel tro simplaj <klr>[al la
        leganto]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto vere <tld/>as kiel sola
        kandidato por lingvo internacia
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel grandega
        malhelpo tie &ccirc;i <tld/>os <klr>[&#8230;]</klr> la reciproka envio de la popoloj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &scirc;an&gcirc;o <klr>[&#8230;]</klr> devas <tld/>i kiel tute ne akceptebla
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>seblantout</trd>,
      <trd>ha&ntilde;valout</trd>
    </trdgrp>
    <trd lng="es">parecer</trd>
    <trdgrp lng="fr">
      <trd>appara&icirc;tre</trd>,
      <trd>para&icirc;tre</trd>,
      <trd>sembler</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&udblac;nik</trd>,
      <trd>l&aacute;tszik</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>jawi&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="sk">javi&tcaron; sa</trd>
  </snc>
  <snc mrk="aper.0i.publigata">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Esti publikigata (pri libroj, gazetoj):
      <ekz>
        la revuo <tld/>as &ccirc;iumonate;
      </ekz>
      <ekz>
        la revuo Progreso da&ubreve;re aperas ekde 1908
        <fnt>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2010/009999.php">Interlingvo: komparo inter du
          solvoj</url></vrk>
          <bib>Monato</bib>
          <lok>2010/04, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        regule <tld/>as gazetaraj komunikoj de UEA
        <fnt>
          <aut>Grigorij Arosev, Andrej Grigorjevskij</aut>
          <vrk><url ref="&Monato;/2004/008210.html">Abonu!</url></vrk>
          <bib>Monato</bib>
          <lok>2004/03, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">dont er-maez<klr tip="ind"> (levrio&ugrave;)</klr>
    </trd>
    <trd lng="es">aparecer</trd>
    <trdgrp lng="fr">
      <trd>para&icirc;tre</trd>,
      <trd>&ecirc;tre <ind>publi&eacute;</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megjelenik<klr tip="ind"> (kiadv&aacute;ny)</klr></trd>,
      <trd>kiadj&aacute;k</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ukaza&cacute; si&eogonek;</trd>,
      <trd>pokaza&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x51FA;&#29256; <pr>ch&#x16B;b&#x1CE;n</pr></trd>,
    <trd>&#x5F95; <pr>l&#xE1;i</pr></trd>,
    <trd>&#x5FA0; <pr>l&#xE1;i</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="aper.0i.sin_prezenti">
    <dif>
      Sin prezenti ie:
      <ekz>
        en kia granda nombro ili <tld/>is por deziri al ni
        bonvenon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ante anta&ubreve; vi kiel
        tradicia malfermanto de niaj kongresoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam la trian fojon vi <tld/>as anta&ubreve; la tribunalo!
      </ekz>
    </dif>
    <trd lng="br">en em gavout</trd>
    <trd lng="es">aparecer</trd>
    <trdgrp lng="fr">
      <trd>para&icirc;tre</trd>,
      <trd>se <ind>pr&eacute;senter</ind></trd>
    </trdgrp>
    <trd lng="zh">&#x51FA;&#38754; <pr>ch&#x16B;mi&#xE0;n</pr></trd>
</snc>
