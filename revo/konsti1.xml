  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj le&gcirc;oj.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Posedanta <tld/>on:
      <ekz>
        <tld/>a lando.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="konsti1.0rompo">
  <kap>
    <tld/>rompo
  </kap>
  <snc>
    <dif>
      &scirc;tatrenverso.
    </dif>
  </snc>
</drv>
</art>

