<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konsti1.xml,v 1.20 2023/09/01 15:46:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>konstituci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konsti1.0o">
  <kap><tld/>o</kap>
  <snc mrk="konsti1.0o.POL">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Aro de fundamentaj le&gcirc;oj, kiuj difinas la formon de politika
      <ref tip="vid" cel="regxim.0o.POL">re&gcirc;imo<sncref/></ref>:
      <ekz>
        kiel parto de la interkonsento, kiu finis la internan
        militon, la nova <tld/>o estis adoptita en septembro 2015
        <fnt>
          <aut>S. Lamichhane</aut>
          <vrk>Nepalo: unuaj lokaj elektoj en 20 jaroj</vrk>
          <bib>Monato</bib>
          <lok>2017:08-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="konsti1.0o.MED">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref cel="kompleksi.0o">Kompleksio</ref>:
      <ekz>
        mi observis <klr>[&#x2026;]</klr> tiun strangan malsanecon, kontra&ubreve; kiu mia <tld/>o min
        &ccirc;iam anta&ubreve;gardis
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Verfassung</trd>,
     <trd>Grundgesetz</trd>
  </trdgrp>
  <trd lng="en">constitution</trd>
  <trd lng="fr">constitution</trd>
  <trdgrp lng="ja">
    <trd>&#x61B2;&#x6CD5;  [<ind>&#x3051;&#x3093;&#x307D;&#x3046;</ind>]</trd>,
    <trd>&#x4F53;&#x8CEA;  [<ind>&#x305F;&#x3044;&#x3057;&#x3064;</ind>]</trd>,
    <trd>&#x4F53;&#x683C;  [<ind>&#x305F;&#x3044;&#x304B;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">grondwet</trd>
  <trd lng="pl">konstytucja</trd>
  <trd lng="pt">constitui&ccedil;&atilde;o</trd>
  <trd lng="ro">constitu&#x21B;ie</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="konsti1.0a">
  <kap><tld/>a</kap>
  <snc mrk="konsti1.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj le&gcirc;oj;
      </ekz>
      <ekz>
        la <tld lit="K"/>a Konsilio de Francio <klr>[&#x2026;]</klr>
        kontrolas novajn le&gcirc;ojn
        <fnt>
          <aut>R. Triolle</aut>
          <vrk>Mortis elstara virino</vrk>
          <bib>Monato</bib>
          <lok>2017:08-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>verfassungsm&auml;&szlig;ig</trd>,
      <trd>verfassungsgem&auml;&szlig;</trd>,
      <trd>konstitutionell</trd>
    </trdgrp>
  </snc>
  <snc mrk="konsti1.0a.konstitucihava">
    <dif>
      Posedanta <tld/>on:
      <ekz>
        <tld/>a lando.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>verfasst</trd>,
      <trd>konstitutionell</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trd lng="en">constitutional</trd>
  <trd lng="fr">constitutionnel</trd>
  <trdgrp lng="ja">
    <trd>&#x61B2;&#x6CD5;&#x306E;  [<ind>&#x3051;&#x3093;&#x307D;&#x3046;&#x306E;</ind>]</trd>,
    <trd>&#x7ACB;&#x61B2;&#x7684;&#x306A;  [<ind>&#x7ACB;&#x61B2;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>grondwettelijk</trd>,
    <trd>constitutioneel</trd>
  </trdgrp>
  <trd lng="pl">konstytucyjny</trd>
  <trd lng="ro">constitu&#x21B;ional</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="konsti1.0rompo">
  <kap><tld/>rompo</kap>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika ago, kiu malkonformas al la <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Verfassungsbruch</trd>,
     <trd>Versto&szlig; gegen das Grundgesetz</trd>
  </trdgrp>
  <trd lng="ja">&#x61B2;&#x6CD5;&#x505C;&#x6B62;  [<ind>&#x3051;&#x3093;&#x307D;&#x3046;&#x3066;&#x3044;&#x3057;</ind>]</trd>
  <trd lng="pl">konstytucji z&lstroke;amanie</trd>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_u;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_i;</trd>
</drv>
</art>

