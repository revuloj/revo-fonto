<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konstituci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konsti1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Aro de fundamentaj le&gcirc;oj, kiuj difinas la formon de politika
      re&gcirc;imo.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Verfassung</trd>,
     <trd>Grundgesetz</trd>
  </trdgrp>
  <trd lng="nl">grondwet</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="konsti1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj le&gcirc;oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>verfassungsm&auml;&szlig;ig</trd>,
      <trd>verfassungsgem&auml;&szlig;</trd>,
      <trd>konstitutionell</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Posedanta <tld/>on:
      <ekz>
        <tld/>a lando.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>verfasst</trd>,
      <trd>konstitutionell</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>grondwettelijk</trd>,
    <trd>constitutioneel</trd>
  </trdgrp>
<trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="konsti1.0rompo">
  <kap>
    <tld/>rompo
  </kap>
  <snc>
    <dif>
      &Scirc;tatrenverso.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Verfassungsbruch</trd>,
     <trd>Versto&szlig; gegen das Grundgesetz</trd>
  </trdgrp>
<trd lng="nl">staatsgreep</trd>
</drv>
</art>

