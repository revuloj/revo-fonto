<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: keiran.xml,v 1.18 2024/12/18 08:36:25 revo Exp $">
<kap>
  <rad>keirant</rad>/o
  <fnt><bib>PVEF</bib></fnt>,
  <var><kap>
    <rad>&hcirc;ejrant</rad>/o
    <fnt><aut>&Eacute;mile Boirac</aut>:
      <vrk>Plena vortaro Esperanto-Esperanta kaj Esperanto-Franca</vrk>,
      <lok>1909-1910</lok></fnt>
  </kap></var>,
  <var><kap>
    <rad>keir</rad>/o
    <fnt><bib>PIV2</bib></fnt>
  </kap></var>
</kap>
<drv mrk="keiran.0o">
  <kap><tld/>o,
    <var><kap>&hcirc;ejranto</kap></var>,
    <var><kap>keiro</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="erizim.0o">erizimo</ref>
      (<trd lng="la">Cheiranthus cheiri</trd> =
        <trd lng="la">Erysimum cheiri</trd>),
      kun flavaj a&ubreve; brunaj floroj, kulturata en &gcirc;ardeno:
      <ekz>
        en la lingvo de floroj <tld/>o simbolas <ref tip="vid" cel="sopir.0o.sento">sopiro</ref>n;
      </ekz>
      <ekz>
        en la unua jarcento Dioskurido priskribis &leftquot;<ref cel="levkoj.0o">levkojo</ref>n&rightquot;,
        kiun li kategoriis la&ubreve; la florkoloro (blanka, flava, blua
        kaj purpura), nur la flavan (kiun botanikistoj konsideras <tld/>on) li aplikis kiel
        kuracilo.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1f/Cheiranthus_cheiri_a1.jpg/359px-Cheiranthus_cheiri_a1.jpg" prm="CC
    BY-SA 4.0"><tld/>o (Cheiranthus cheiri)
        <fnt>
          <aut>Jerzy Opio&lstroke;a, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Cheiranthus_cheiri_a1.jpg">&WCW;/File:Cheiranthus_cheiri_a1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_w;&c_t;&c_a;&c_f;&c_ib;&c_jo;&c_l;&c_mol;</trd>
  <trd lng="de">Goldlack</trd>
  <trd lng="en">wallflower</trd>
  <trd lng="nl">muurbloem</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;rgaviola</trd>,
    <trd>t&eacute;li <ind>viola</ind></trd>
  </trdgrp>
  <trd lng="pl">lak ogrodowy</trd>
  <trd lng="ro">mic&#537;unea</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_f;&c_i;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sv">gyllenlack</trd>
  <trd lng="uk">&#x43B;&c_a;&#x43A;&c_f;&c_ib;&#x43E;&#x43B;&#x44C;</trd>
</drv>
</art>
