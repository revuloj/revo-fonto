<kap>
  <rad>livi</rad>/o
</kap>
<drv mrk="livi.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kolomb.rok0o">Rokkolombo</ref>:
      <ekz>
        la hejmkolombo, a&ubreve; domkolombo, estas &ccirc;u subspecio &ccirc;u individuaj birdoj devenaj
        el la specio rokkolombo, a&ubreve; <tld/>o, kiu estis enhejmita el kelkaj miloj da
        jaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=219354">Hejmkolombo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_z;&c_y; <ind>&c_g;&c_o;&c_l;&c_u;&c_b;</ind></trd>
  <trd lng="ca"><ind>colom</ind> roquer</trd>
  <trd lng="de">Felsentaube</trd>
  <trd lng="hu">szirti <ind>galamb</ind></trd>
  <trd lng="ja">&#x30AB;&#x30EF;&#x30E9;&#x30D0;&#x30C8;</trd>
  <trd lng="la">Columba livia</trd>
  <trd lng="nl">rotsduif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b skalny</trd>
  <trd lng="pt">pomba-da-rocha</trd>
  <trd lng="ru">&c_s;&c_i;&c_z;&c_y;&c_j; <ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind></trd>
  <trd lng="uk">&#x441;&#x438;&#x437;&#x438;&#x439; &#x433;&#x43E;&#x43B;&#x443;&#x431;</trd>
  <trdgrp lng="zh">
    <trd>&#x91CE;&#x9D3F; <pr>y&#x11B;g&#x113;</pr></trd>,
    <trd>&#x91CE;&#x9E3D; <pr>y&#x11B;g&#x113;</pr></trd>
  </trdgrp>
</drv>
