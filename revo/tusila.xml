<art mrk="$Id: tusila.xml,v 1.21 2023/09/04 19:01:55 revo Exp $">
<kap>
  <rad>tusilag</rad>/o
</kap>
<drv mrk="tusila.0o">
  <kap><tld/>o</kap>
  <snc mrk="tusila.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Tussilago</trd>), kies plej konata specio
      (<trd lng="la">Tussilago farfara</trd>) havas grandajn
      koroformajn,
      dorsflanke grizalanajn foliojn, aperantajn nur post la flavaj
      kapituloj, kiuj po unu staras sur skvama tigo. La sekigitaj
      kapituloj kaj folioj estas farmacie uzataj kontra&ubreve; tuso:
      <ekz>
        brilis la kapetoj de sunflavaj <tld/>oj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/tusilag.jpg">
    <tld/>o
  </bld>
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_je;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>pota de cavall</trd>,
    <trd>tuss&iacute;lag</trd>
  </trdgrp>
  <trd lng="de">Huflattich</trd>
  <trd lng="fa">&f_p;&a_A;&f_y; &a_kh;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>pas d'&acirc;ne</trd>,
    <trd>tussilage</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>martilapu</trd>,
    <trd>l&oacute;k&ouml;r&ouml;mf&udblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30AD;&#x30BF;&#x30F3;&#x30DD;&#x30DD;</trd>
  <trd lng="nl">hoefblad</trd>
  <trd lng="pl">podbia&lstroke;</trd>
  <trdgrp lng="pt">
   <trd>tussilagem</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_mol;-&c_i;-&c_m;&c_a;&c_ch;&c_je;&c_h;&c_a;</trd>
  <trd lng="sv">h&auml;sthov<klr> (&ouml;rt)</klr></trd>
  <trd lng="sk">podbe&#x13E;</trd>
  <trd lng="cs">podb&#x11B;l</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x431;&#x456;&#x43B;</trd>,
    <trd>&#x43C;&#x430;&#x442;&#x438;-&#x439;-&#x43C;&#x430;&#x447;&#x443;&#x445;&#x430;</trd>
  </trdgrp>
</drv>
</art>
