<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kakt</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kakt.0o">
  <kap><tld/>o</kap>
  <snc mrk="kakt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Amerikdevenaj vegeta&jcirc;oj, plej ofte senfoliaj sed kun pikiloj, 
      tenantaj akvon en la karneca tigo, el familio de dukotiledonaj 
      vegeta&jcirc;oj (<trd lng="la">Cactaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="bg">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="de">Kaktus</trd>
  <trd lng="en">cactus</trd>
  <trd lng="fi">kaktus</trd>
  <trdgrp lng="es">
    <trd>cactus</trd>,
    <trd>cacto</trd>
  </trdgrp>
  <trd lng="fr">cactus</trd>
  <trdgrp lng="he">
    <trd>&tsadi;&bet;&resh;</trd>,
    <trd>&qof;&qof;&tet;&vav;&samekh;</trd>
  </trdgrp>
  <trd lng="hu">kaktusz</trd>
  <trd lng="nl">cactus</trd>
  <trd lng="pl">kaktus</trd>
  <trd lng="pt">cacto</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="sv">kaktus</trd>
</drv>

<drv mrk="kakt.figo0o">
  <kap>figo<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kakt.figo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kakto, <ref tip="super" cel="opunti.0o.BOT">opuntio</ref>
      kun man&gcirc;eblaj fruktoj, precipe el la specio <trd lng="la">opuntia
      ficus-indica</trd>:
      <ekz>
        figo<tld/>o <klr>...</klr>, kies frukto iom similas la frukton
        de vera figarbo
        <fnt>
        <aut>G. Neves</aut>,
        <vrk><url ref="http://esperanto.org/Ondo/L-neves2.htm">
        &Ccirc;u Esperanto konas hermafroditojn?</url></vrk>,
        <bib>LOdE</bib>,
        <lok>2003:8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Feigenkaktus</trd>
  <trd lng="fr">figuier de barbarie</trd>
</drv>
</art>
