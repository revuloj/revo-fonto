<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>8</ofc>
   <rad>kakt</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kakt.0o">
   <kap><tld/>o</kap>
   <snc mrk="kakt.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       Iu el familio de dukotiledonaj vegeta&jcirc;oj
       (<trd lng="la">Cactaceae</trd>), amerikdevenaj,
       plej ofte senfoliaj sed kun pikiloj, tenantaj
       akvon en la karneca tigo.
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
   <trd lng="de">Kaktus</trd>
   <trd lng="en">cactus</trd>
   <trd lng="fi">kaktus</trd>
   <trdgrp lng="es">
     <trd>cactus</trd>,
     <trd>cacto</trd>
   </trdgrp>
   <trd lng="fr">cactus</trd>
   <trdgrp lng="he">
     <trd>&tsadi;&bet;&resh;</trd>,
     <trd>&qof;&qof;&tet;&vav;&samekh;</trd>
   </trdgrp>
   <trd lng="hu">kaktusz</trd>
   <trd lng="nl">cactus</trd>
   <trd lng="pl">kaktus</trd>
   <trd lng="pt">cacto</trd>
   <trd lng="ru">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
   <trd lng="sv">kaktus</trd>
</drv>
</art>
