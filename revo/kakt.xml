<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kakt.xml,v 1.38 2024/03/01 09:27:01 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kakt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kakt.0o">
  <kap><tld/>o</kap>
  <snc mrk="kakt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Iu el familio <trd lng="la">Cactaceae</trd>, vegeta&jcirc;o
      kun &scirc;velaj karnecaj tigoj, neniuj a&ubreve; malmultaj skvamecaj folioj kaj pikiloj,
      preferantaj sekajn lokojn:
      <ekz>
        <klr>[tie]</klr> kreskas la fonta&jcirc;oj de la dezerto (t.&#x202F;e. <tld/>oj)
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj staras rekte kiel sepobrakaj kandelingoj
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        seka sova&gcirc;ejo kun <tld/>oj kaj tolao-arbetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php">Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/7/76/Botanischer_Garten_M&uuml;nchen_Gew&auml;chshaus_mit_Kakteen.JPG/640px-Botanischer_Garten_M&uuml;nchen_Gew&auml;chshaus_mit_Kakteen.JPG"
    prm="CC BY-SA 3.0">forcejo kun diversaj <tld/>oj, botanika &gcirc;ardeno en Munkeno, Germanujo
        <fnt>
          <aut>Usien, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Botanischer_Garten_M%C3%BCnchen_Gew%C3%A4chshaus_mit_Kakteen.JPG">&WCW;/File:Botanischer_Garten_M&uuml;nchen_Gew&auml;chshaus_mit_Kakteen.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="bg">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="ca">cactus</trd>
  <trd lng="cs">kaktus</trd>
  <trd lng="de">Kaktus</trd>
  <trd lng="en">cactus</trd>
  <trdgrp lng="es">
    <trd>cactus</trd>,
    <trd>cacto</trd>
  </trdgrp>
  <trd lng="fi">kaktus</trd>
  <trd lng="fr">cactus</trd>
  <trdgrp lng="he">
    <trd>&tsadi;&bet;&resh;</trd>,
    <trd>&qof;&qof;&tet;&vav;&samekh;</trd>
  </trdgrp>
  <trd lng="hu">kaktusz</trd>
  <trd lng="ja">&#x30B5;&#x30DC;&#x30C6;&#x30F3;</trd>
  <trd lng="nl">cactus</trd>
  <trd lng="pl">kaktus</trd>
  <trd lng="pt">cacto</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="sk">kaktus</trd>
  <trd lng="sv">kaktus</trd>
  <trd lng="tr">kakt&uuml;s</trd>
  <trd lng="uk">&c_k;&c_a;&c_k;&c_t;&c_u;&c_s;</trd>
  <trd lng="zh">&#x4ED9;&#x4EBA;&#x638C; <pr>xi&amacron;nr&#xE9;nzh&#x1CE;ng</pr></trd>
</drv>

<drv mrk="kakt.figo0o">
  <kap>figo<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kakt.figo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kakto el la genro <trd lng="la">Opuntia</trd>,
      kun man&gcirc;eblaj fruktoj &ccirc;e la specio <trd lng="la">Opuntia ficus-indica</trd>:
      <ekz>
        figo<tld/>o <klr>[&#8230;]</klr>, kies frukto iom similas la frukton
        de vera figarbo
        <fnt>
        <aut>G. Neves</aut>,
        <vrk><url ref="&LOdE;L-neves2.htm">
        &Ccirc;u Esperanto konas hermafroditojn?</url></vrk>,
        <bib>LOdE</bib>,
        <lok>2003:8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/04/Figa_de_moro_03.JPG/360px-Figa_de_moro_03.JPG" prm="CC BY-SA
    2.5">figo<tld/>o (Opuntia ficus-indica), insulo Formentera, Hispanujo
        <fnt>
          <aut>Vicens</aut>
          <lok><url ref="&WCW;/File:Figa_de_moro_03.JPG">&WCW;/File:Figa_de_moro_03.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_u;&c_n;&c_c;&c_y;&c_ja; &c_ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_a;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>figuera de pala</trd>,
    <trd>figuera de moro</trd>
  </trdgrp>
  <trd lng="de">Feigenkaktus</trd>
  <trdgrp lng="en">
    <trd>cactus fig</trd>,
    <trd>prickly pear</trd>, <trd>nopal</trd>
  </trdgrp>
  <trd lng="fr">figuier de barbarie</trd>
  <trdgrp lng="nl">
    <trd>vijgencactus</trd>,
    <trd>vijgcactus</trd>,
    <trd>vijgdistel</trd>,
    <trd>opuntia</trd>
  </trdgrp>
  <trd lng="pl">opuncja</trd>
  <trd lng="uk">&c_ib;&c_n;&c_d;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_a; &c_s;&c_m;&c_o;&c_k;&c_o;&c_v;&c_n;&c_i;&c_c;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x4ED9;&#x4EBA;&#x638C;&#x679C; <pr>xi&amacron;nr&#xE9;nzh&#x1CE;nggu&#x1D2;</pr></trd>,
    <trd>&#x7E2E;&#x523A;&#x4ED9;&#x4EBA;&#x638C;
    <pr>su&#x14D;c&#xEC;xi&amacron;nr&#xE9;nzh&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
