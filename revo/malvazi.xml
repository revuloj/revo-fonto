<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: malvazi.xml,v 1.12 2024/10/24 09:31:00 revo Exp $">
<kap>
  <rad>malvazi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="malvazi.0o">
  <kap><tld/>o</kap>
  <snc mrk="malvazi.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Grekdevena <ref cel="ber.vin0o">vinber</ref>speco kun blankaj beroj, kultivata &ccirc;irka&ubreve;
      Mediteraneo:
      <ekz>
        odoras dol&ccirc;a <tld/>o &#11005; kun citrono kaj adonido
        <fnt>
          <aut>Jan Stanis&lstroke;aw Skorupski</aut>,
          <vrk>Soneto preska&ubreve; ambrozia</vrk>,
          <lok><url ref="http://skorupski.bitflux.ch/sonets/index.php?lang=eo">Sonetado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">malvezij</trd>
  </snc>
  <snc mrk="malvazi.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="vin.0o.KUI">Vino</ref> farita el <tld/>o:
      <ekz>
        <nom>Walter Scott</nom> elektis la plej ri&ccirc;an vazaron por &gcirc;ui la delican
        <tld/>on
        <fnt>
          <aut>Juan R&eacute;gulo P&eacute;rez</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr35/mitoju.html">Misteroj,
          mitoj kaj faktoj</url></vrk>
          <lok>Nica Literatura Revuo, 1958-05 &gcirc;is 06, 3/5, pa&gcirc;oj
          194a-200a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">malvezijwijn</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_z;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">malvasia</trd>
  <trd lng="cs">malvaz</trd>
  <trd lng="de">Malvasia</trd>
  <trd lng="en">malmsey</trd>
  <trd lng="fr">malvoisie</trd>
  <trd lng="hu">malv&aacute;zia</trd>
  <trd lng="it">malvasia</trd>
  <trd lng="pl">malwazja</trd>
  <trd lng="pt">malvasia</trd>
  <trd lng="sk">malvaz</trd>
  <trd lng="uk">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_z;&c_ib;&c_ja; (&c_s;&c_o;&c_r;&c_t; &c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_u; &c_ib; &c_v;&c_i;&c_n;&c_a;)</trd>
</drv>
</art>

