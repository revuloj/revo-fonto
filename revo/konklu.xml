<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konklud</rad>/i
</kap>
<drv mrk="konklu.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tiri per rezono logikajn konsekvencojn el argumentoj,
      principoj a&ubreve; faktoj:
      <ekz>
        el tio, kion ni scias, ni povas <tld/>i, ke...;
      </ekz>
      <ekz>
        <tld/>i el aparta fakto &gcirc;eneralan principon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>folgern</trd>,
     <trd>schlie&szlig;en</trd>,
     <trd>einen <ind>Schluss ziehen</ind></trd>,
     <trd>schlussfolgern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conclude</trd>,
    <trd>deduce</trd>,
    <trd>infer</trd>
  </trdgrp>
  <trd lng="nl">concluderen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_v;&c_y;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="konklu.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Fina konsekvenco de rezonado.
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.el0i">elflui</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Folgerung</trd>,
     <trd>Schluss</trd>,
     <trd>Schlussfolgerung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conclusion</trd>,
    <trd>deduction</trd>,
    <trd>inference</trd>
  </trdgrp>
  <trd lng="nl">conclusie</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="konklu.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Kiel <tld/>o
    </dif>
    <refgrp tip="sin">
      <ref cel="fin.0e">fine</ref>,
      <ref cel="do.0">do</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>folglich</trd>,
     <trd>ergo</trd>,
     <trd>also</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v; &c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="konklu.0iga">
  <kap>
    <tld/>iga
  </kap>
  <snc>
    <dif>
      Ebliganta <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schl&uuml;ssig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conclusive</trd>,
    <trd>clinching</trd>,
    <trd>convincing</trd>,
    <trd>decisive</trd>
  </trdgrp>
</drv>
</art>
