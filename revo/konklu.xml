<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konklu.xml,v 1.27 2024/03/05 21:21:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konklud</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konklu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konklu.0i.LOG">
    <dif>
      Tiri per rezono <ref cel="logik.0a">logikajn</ref>
      <ref cel="konsek1.0o">konsekvencojn</ref> el
      <ref cel="argume.0o.LOG">argumentoj</ref>,
      <ref cel="princi.0o.de_rezono">principoj</ref> a&ubreve;
      <ref cel="fakt.0o">faktoj</ref>:
      <ekz>
        <tld/>i el aparta fakto &gcirc;eneralan principon;
      </ekz>
      <ekz>
        &ccirc;ar ni scias, ke <klr>[&#8230;]</klr>, ni facile <tld/>as, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is el lia rakonto, ke vi nepre devas koni lin
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> fine <tld/>is, ke oni ne bezonas timi la kvietan homan esta&jcirc;on
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        ho, sinjoroj, vi do jam <tld/>is, ke mi estas kulpa
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        de seneraraj signoj la pastroj <tld/>is, ke Egipton minacas ia granda malfeli&ccirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kandid</nom> perdinte sian kredon je optimismo, <tld/>as, ke la plej bona maniero por
        pasigi la vivon estas kulturi sian &gcirc;ardenon
        <fnt>
          <bib>Kandid</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la&ubreve;]</klr> la bela perono, kiu ornamis tiun eniron, oni povis <tld/>i, ke
        la lo&gcirc;ejoj <klr>[&#8230;]</klr> estas komfortaj kaj belaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni do <tld/>as, ke homo pravi&gcirc;as per fido, ekster la faroj de la le&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 3:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam oni <klr>[la limakan paron]</klr> kuiris kaj neniam oni ilin metis sur
        ar&gcirc;entan pladon kaj de tio ili <tld/>is ke la palaco disfalis kaj &ccirc;iuj homoj
        en la mondo mortis
        <fnt>
          <bib>Fab2</bib>
          <lok>Feli&ccirc;a familio</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dedukt.0i">dedukti</ref>,
      <ref cel="hipote2.0i">hipotezi</ref>,
      <ref cel="konjek.0i">konjekti</ref>,
      <ref cel="rezon.0i">rezoni</ref>,
      <ref cel="supoz.0i">supozi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_v;&c_y;&c_s;&c_n;&c_o;&c_v;&c_u;</ind></trd>
  <trdgrp lng="cs">
    <trd>doch&aacute;zet k z&aacute;v&ecaron;ru</trd>,
    <trd>konkludovat</trd>,
    <trd>soudit</trd>,
    <trd>uzav&iacute;rat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>folgern</trd>,
     <trd>schlie&szlig;en</trd>,
     <trd>einen <ind>Schluss ziehen</ind></trd>,
     <trd>schlussfolgern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conclude</trd>,
    <trd>deduce</trd>,
    <trd>infer</trd>
  </trdgrp>
  <trd lng="fr">conclure</trd>
  <trd lng="hu">k&ouml;vetkeztet</trd>
  <trdgrp lng="ja">
    <trd>&#x7D50;&#x8AD6;&#12377;&#x308B;  <pr>&#12369;&#12388;&#x308D;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x7D50;&#x8AD6;&#12434;&#x4E0B;&#12377;  <pr>&#12369;&#12388;&#x308D;&#12435;&#12434;&#x304F;&#12384;&#12377;</pr></trd>,
    <trd>&#x7DE0;&#12417;&#x304F;&#x304F;&#x308B;  <pr>&#12375;&#12417;&#x304F;&#x304F;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>concluderen</trd>,
    <trd>afleiden</trd>,
    <trd>besluiten</trd>,
    <trd>een gevolgtrekking maken</trd>,
    <trd>opmaken</trd>,
    <trd>tot de slotsom komen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>konkludowa&cacute;</trd>,
    <trd>wnioskowa&cacute;</trd>,
    <trd>wyci&aogonek;ga&cacute; <ind>wnioski</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>concluir</trd>,
    <trd>tirar <ind>conclus&atilde;o</ind> de</trd>,
    <trd>depreender</trd>,
    <trd>inferir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_v;&c_y;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>doch&aacute;dza&tcaron; k z&aacute;veru</trd>,
    <trd>s&uacute;di&tcaron;</trd>,
    <trd>uzatv&aacute;ra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8AD6;&#x65B7; <pr>l&#xF9;ndu&#xE0;n</pr></trd>,
    <trd>&#x65AD;&#x5B9A; <pr>du&#xE0;nd&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konklu.0o">
  <kap><tld/>o</kap>
  <snc mrk="konklu.0o.LOG">
    <dif>
      Fina <ref tip="sin" cel="konsek1.0o">konsekvenco</ref> de
      <ref cel="rezon.0i">rezonado</ref>:
      <ekz>
        oni devas kunigi unu fakton kun alia, por veni al <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 7:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de simpla logiko diras, ke tiel esti devas kaj ke alie esti ne povas
        <fnt>
          <bib>EE</bib>
          <lok>VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de l&#8217; enketo tiel nevenkeble rebatis &ccirc;iujn kontra&ubreve;dirojn, ke &ccirc;ia ajn
        dubo estis neebla
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi longe tre longe pripensadis, kaj alvenis al tiu mala <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        enskribu &ccirc;iun gravan <tld/>on, kiu pasos tra via kapo, kaj post kelke da tagoj
        esploru la aferon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faris <tld/>ojn el la tabeloj, sed la plej gravajn ili komunikis al neniu
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dedukt.0o">dedukto</ref>,
      <ref cel="hipote2.0o">hipotezo</ref>,
      <ref cel="konjek.0o">konjekto</ref>,
      <ref cel="rezon.0o">rezono</ref>,
      <ref cel="supoz.0o">supozo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="flu.el0i">elflui</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>konkluze</trd>,
    <trd>z&aacute;v&ecaron;r</trd>,
    <trd>&uacute;sudek</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Folgerung</trd>,
     <trd>Schluss</trd>,
     <trd>Schlussfolgerung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conclusion</trd>,
    <trd>deduction</trd>,
    <trd>inference</trd>
  </trdgrp>
  <trd lng="fr">conclusion</trd>
  <trd lng="hu">k&ouml;vetkeztet&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x7D50;&#x8AD6;  <pr>&#12369;&#12388;&#x308D;&#12435;</pr></trd>,
    <trd>&#x7D50;&#12403;  <pr>&#12416;&#12377;&#12403;</pr></trd>,
    <trd>&#x7DE0;&#12417;&#x304F;&#x304F;&#x308A;  <pr>&#12375;&#12417;&#x304F;&#x304F;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>conclusie</trd>,
    <trd>gevolgtrekking</trd>,
    <trd>slotsom</trd>,
    <trd>besluit</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>konkluzja</trd>,
    <trd>wniosek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konkl&uacute;zia</trd>,
    <trd>uz&aacute;ver</trd>,
    <trd>z&aacute;ver</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8AD6;&#x65B7; <pr>l&#xF9;ndu&#xE0;n</pr></trd>,
    <trd>&#x7D50;&#x8AD6; <pr>ji&#xE9;l&#xF9;n</pr></trd>,
    <trd>&#x63A8;&#29702; <pr>tu&#x12B;l&#x1D0;</pr></trd>,
    <trd>&#x63A8;&#x8AD6; <pr>tu&#x12B;l&#xF9;n</pr></trd>,
    <trd>&#x540E;&#25928; <pr>h&#xF2;uxi&#xE0;o</pr></trd>,
    <trd>&#x540E;&#x679C; <pr>h&#xF2;ugu&#x1D2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konklu.0e">
  <kap><tld/>e</kap>
  <snc mrk="konklu.0e.LOG">
    <dif>
      Kiel <tld/>o,
      <ref cel="fin.0e">fine</ref>,
      <ref cel="do.0">do</ref>:
      <ekz>
        nu, &scirc;i diris <tld/>e: &ccirc;io plej bone aran&gcirc;i&gcirc;as
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e li proponis al diplomatoj starigi la sekvantajn le&gcirc;ojn sub internacia
        garantio
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volas emfazi <tld/>e pri du konceptoj <klr>[&#8230;]</klr>
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Tradukoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_v;&c_y;&c_n;&c_ib;&c_k;&c_u;</ind></trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_y;&c_k;&c_a;&c_n;&c_c;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tud&iacute;&zcaron;</trd>,
    <trd>z&aacute;v&ecaron;rem</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>folglich</trd>,
     <trd>ergo</trd>,
     <trd>also</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en conclusion</trd>,
    <trd>par cons&eacute;quent</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;vetkez&eacute;sk&eacute;ppen</trd>
  <trdgrp lng="ja">
    <trd>&#x7D50;&#x8AD6;&#12392;&#12375;&#12390;  <pr>&#12369;&#12388;&#x308D;&#12435;&#12392;&#12375;&#12390;</pr></trd>,
    <trd>&#12375;&#x305F;&#x304C;&#12387;&#12390;</trd>,
    <trd>&#x305D;&#x308C;&#12422;&#12360;</trd>
  </trdgrp>
  <trd lng="nl">bijgevolg</trd>
  <trdgrp lng="pl">
    <trd>w <ind>konkluzji</ind></trd>,
    <trd>konkluduj&aogonek;c</trd>,
    <trd>wi&eogonek;c</trd>,
    <trd>st&aogonek;d</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;
    &c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;verom</trd>
  <trdgrp lng="zh">
    <trd>&#x56E0;&#x6B64; <pr>y&#x12B;nc&#x1D0;</pr></trd>,
    <trd>&#x56E0;&#x800C; <pr>y&#x12B;n&#xE9;r</pr></trd>,
    <trd>&#25152;&#x4EE5; <pr>su&#x1D2;y&#x1D0;</pr></trd>,
    <trd>&#25925;&#x6B64; <pr>g&#xF9;c&#x1D0;</pr></trd>,
    <trd>&#25925;&#x800C; <pr>g&#xF9;&#xE9;r</pr></trd>,
    <trd>&#x4E8E;&#x662F; <pr>y&#xFA;sh&#xEC;</pr></trd>,
    <trd>&#x662F;&#x4EE5; <pr>sh&#xEC;y&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konklu.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="konklu.0iga.LOG">
    <dif>
      Ebliganta <tld/>on:
      <ekz>
        la punkto <klr>[&#8230;]</klr> estas tre sugesta &ndash; fakte ni rajtas nomi &gcirc;in <tld/>iga
        <fnt>
          <vrk><url ref="http://www.verkoj.com/lauteme/klasika/doyle/kazo-de-identeco/">Kazo de
          identeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la malkovro ne estis <tld/>iga kaj estis poste malakceptita
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/PSR_B1257%2B12">PSR B1257+12</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koher.0a.penso">kohera<sncref/></ref>,
      <ref cel="konsek1.0a">konsekvenca</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>konkludentn&iacute;</trd>,
    <trd>p&rcaron;esv&ecaron;d&ccaron;iv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schl&uuml;ssig</trd>,
    <trd>naheliegend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conclusive</trd>,
    <trd>clinching</trd>,
    <trd>convincing</trd>,
    <trd>decisive</trd>,
    <trd>sound</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;terminant<klr> (permettant de conclure)</klr></trd>,
    <trd>conclusif</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meggy&odblac;z&odblac;</trd>,
    <trd>bizony&iacute;t&oacute; erej&udblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x6C7A;&#x5B9A;&#30340;&#x306A;  <pr>&#12369;&#12387;&#12390;&#12356;&#12390;&#x304D;&#x306A;</pr></trd>
  <trdgrp lng="nl">
    <trd>afdoend</trd>,
    <trd>besluitend</trd>,
    <trd>afrondend</trd>,
    <trd>een conclusie wettigend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>umo&zdot;liwiaj&aogonek;cy wyci&aogonek;gni&eogonek;cie
    <ind>wniosku</ind></trd>,
    <trd>przekonywaj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="sk">presved&ccaron;iv&yacute;</trd>
  <trd lng="zh">&#x51B3;&#x5B9A;&#24615; <pr>ju&#xE9;d&#xEC;ngx&#xEC;ng</pr></trd>
</drv>

<drv mrk="konklu.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Konduki al <tld/>o:
      <ekz>
        la mallonga interparolo, kiun mi havis kun vi, <tld/>igis min, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio povus <tld/>igi, ke, male <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/007930.html">Interna pejza&gcirc;o de alia virino</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sugest.0i.supozigi">sugesti<sncref/></ref>,
<ref cel="supoz.0igi">supozigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>zum <ind>Schluss</ind> f&uuml;hren</trd>,
    <trd>nahelegen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prowadzi&cacute; do <ind> wniosku</ind></trd>,
    <trd>przekonywa&cacute;</trd>
  </trdgrp>
</drv>
</art>
