<kap>
<rad>enciklik</rad>/o
</kap>
<drv mrk="encikl.0o">
<kap><tld/>o</kap>
<uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Papa <ref tip="super" cel="cirkul1.0o">cirkulero</ref>
      pri iu punkto de doktrino:
      <ekz>
        <nac>Deus Caritas est</nac> (Dio estas amo) de la 25-a de decembro 2005 estas la unua
        <tld/>o de papo Benedikto la 16-a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=108130">Deus Caritas est</url></lok>
        </fnt>.
      </ekz>  
    </dif>
    <ref tip="vid" cel="bule.0o">buleo</ref>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_c;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;</trd>
  <trdgrp lng="de">
     <trd>p&auml;pstliches Rundschreiben</trd>,
     <trd>Enzyklika</trd>
  </trdgrp>
  <trd lng="es">enc&iacute;clica</trd>
  <trd lng="fr">encyclique</trd>
  <trdgrp lng="hu">
     <trd>enciklika</trd>,
     <trd>p&aacute;pai <ind>k&ouml;rlev&eacute;l</ind></trd>
  </trdgrp>
  <trd lng="it">enciclica</trd>
  <trd lng="ja">&#x56DE;&#x52C5;  <pr>&#x304B;&#x3044;&#x3061;&#x3087;&#x304F;</pr></trd>
  <trd lng="pl">encyklika</trd>
  <trd lng="pt">enc&iacute;clica</trd>
  <trd lng="ru">&c_e;&c_n;&c_c;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">okru&zcaron;n&yacute; list</trd>
  <trd lng="cs">encyklika <klr>(pape&zcaron;sk&aacute;)</klr></trd>
  <trd lng="uk">&#x435;&#x43D;&#x446;&#x438;&#x43A;&#x43B;&#x456;&#x43A;&#x430;</trd>
</drv>
