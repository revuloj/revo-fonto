<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>fraksen</rad>/o</kap>
<drv mrk="frakse.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Fraxinus</trd>),
      arbo, kies fleksebla ligno estas uzata por fari
      tenilojn, meblojn, bastonojn ktp; ties folioj servas por prepari
      sanigan trinka&jcirc;on.
    </dif>
  </snc>
  <bld lok="../bld/fraksen.jpg">
    folioj <klr>(Fraxinus excelsior)</klr>
  </bld>
  <trd lng="be">&c_ja;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Esche</trd>
  <trd lng="fr">fr&ecirc;ne</trd>
  <trd lng="hu">k&odblac;ris</trd>
  <trd lng="nl">es</trd> 
  <trd lng="pl">jesion</trd> 
  <trd lng="pt">freixo</trd>
  <trd lng="ru">&c_ja;&c_s;&c_je;&c_n;&c_mol;</trd>
</drv>
</art>
