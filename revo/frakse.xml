<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fraksen</rad>/o
</kap>
<drv mrk="frakse.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ole1.0acoj">oleacoj</ref>
      (<trd lng="la">Fraxinus</trd>),
      arbo, kies fleksebla ligno estas uzata por fari
      tenilojn, meblojn, bastonojn ktp; ties folioj servas por prepari
      sanigan trinka&jcirc;on.
    </dif>
  </snc>
  <bld lok="../bld/fraksen.jpg">
    folioj <klr>(Fraxinus excelsior)</klr>
  </bld>
  <trd lng="de">Esche</trd>
</drv>
</art>
