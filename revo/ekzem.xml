<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekzem.xml,v 1.27 2023/11/07 19:22:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ekzem</rad>/o
</kap>
<drv mrk="ekzem.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzem.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Nekonta&gcirc;a ha&ubreve;ta <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>:
      <ekz>
        la ha&ubreve;to de la malsupra palpebro estas atakata de <tld/>o
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, kronika
        katara konjunktivito, (el la libro de profesoro E.
        Fuchs)</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o restis &ccirc;iufoje &ccirc;irka&ubreve; tri semajnojn
        <fnt>
          <vrk><url ref="https://www.bruno-groening.org/eo/sani%C4%9Doj/korpaj-sanigoj/255-hauxto/ekzemo">Sani&gcirc;oj de ekzemo</url></vrk>
          <lok>Amikaro Bruno Gr&ouml;ning, 2023 </lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o &ccirc;e ventroha&ubreve;to
        <fnt>
          <aut>Louis-Ferdinand Celine, trad. Armela LeQuint kaj &Jcirc;ak Le Puil</aut>
          <vrk><url ref="https://books.google.de/books?id=02exBgAAQBAJ&amp;lpg=PA87&amp;ots=b3936oWLjE&amp;pg=PA87#v=onepage">Voja&gcirc;o &gcirc;is noktofino</url></vrk>
          <lok>p. 87</lok>
        </fnt>;
      </ekz>
    </dif>
    <ref tip="sin" cel="dermit.0o">dermatito</ref>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_e;&c_m;&c_a;</trd>
  <trd lng="ca">&egrave;czema</trd>
  <trdgrp lng="cs">
    <trd>ekz&eacute;m</trd>,
    <trd>kop&rcaron;ivka</trd>,
    <trd>li&scaron;ej</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ekzem</trd>,
    <trd>Juckflechte</trd>,
    <trd>Dermatitis</trd>
  </trdgrp>
  <trd lng="en">eczema</trd>
  <trd lng="es">eccema</trd>
  <trd lng="fa">&a_A;&f_g;&a_z;&a_m;&a_A;</trd>
  <trd lng="fr">ecz&eacute;ma</trd>
  <trd lng="hu">ekc&eacute;ma</trd>
  <trd lng="id">eksem</trd>
  <trd lng="it">eczema</trd>
  <trd lng="ja">&#x6E7F;&#x75B9;  <pr>&#x3057;&#x3063;&#x3057;&#x3093;</pr></trd>
  <trd lng="pl">egzema</trd>
  <trd lng="pt">eczema</trd>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_je;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>ekz&eacute;m</trd>,
    <trd>vyr&aacute;&zcaron;ka</trd>
  </trdgrp>
  <trd lng="sv">eksem</trd>
  <trd lng="uk">&c_je;&c_k;&c_z;&c_je;&c_m;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x76AE;&#x708E; <pr>p&#xED;y&#xE1;n</pr></trd>,
    <trd>&#x76AE;&#x80A4;&#x708E; <pr>p&#xED;f&#x16B;y&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
