<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ekzem</rad>/o
</kap>
<drv mrk="ekzem.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzem.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Ha&ubreve;ta malsano karakterizata per veziketoj kaj
      skvameti&gcirc;o de
      epidermo:
      <ekz>
      la ha&ubreve;to de la malsupra palpebro estas atakata de <tld/>o
      <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, kronika
      katara konjunktivito, (el la libro de profesoro E.
      Fuchs)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_e;&c_m;&c_a;</trd>
  <trd lng="ca">&egrave;czema</trd>
  <trdgrp lng="de">
    <trd>Ekzem</trd>,
    <trd>Juckflechte</trd>,
    <trd>Schuppenflechte</trd>
  </trdgrp>
  <trd lng="en">eczema</trd>
  <trd lng="es">eccema</trd>
  <trd lng="fa">&a_A;&f_g;&a_z;&a_m;&a_A;</trd>
  <trd lng="fr">ecz&eacute;ma</trd>
  <trd lng="hu">ekc&eacute;ma</trd>
  <trd lng="it">eczema</trd>
  <trd lng="pl">egzema</trd>
  <trd lng="pt">eczema</trd>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_je;&c_m;&c_a;</trd>
  <trd lng="sv">eksem</trd>
</drv>
</art>
