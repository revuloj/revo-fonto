<kap><rad>jovial</rad>/a <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="jovial.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Bonhumore <ref tip="vid" cel="gaj.0a">gaja</ref> kaj gajiga.
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>gajema</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>,
    <trd>&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_y;
    </trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vesel&#253;</trd>,
    <trd>&#382;ovi&#225;ln&#237;</trd>
  </trdgrp>
  <trd lng="de">jovial</trd>
  <trd lng="es">jovial</trd>
  <trdgrp lng="fr">
    <trd>jovial</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ked&eacute;lyes</trd>,
    <trd>jovi&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">gioviale</trd>
  <trd lng="nl">joviaal</trd>
  <trdgrp lng="pt">
    <trd>alegre</trd>,
    <trd>jovial</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_z;&c_n;&c_je;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobrej n&#225;lady</trd>,
    <trd>rozmarn&#253;</trd>,
    <trd>&#382;ovi&#225;lny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1074;&#1077;&#1089;&#1077;&#1083;&#1080;&#1081;</trd>,
    <trd>&#1078;&#1080;&#1090;&#1090;&#1108;&#1088;&#1072;&#1076;&#1110;&#1089;&#1085;&#1080;&#1081;</trd>,
    <trd>&#1073;&#1077;&#1079;&#1090;&#1091;&#1088;&#1073;&#1086;&#1090;&#1085;&#1080;&#1081;</trd>,
    <trd>&#1076;&#1086;&#1073;&#1088;&#1086;&#1076;&#1091;&#1096;&#1085;&#1080;&#1081;</trd>,
    <trd>&#1087;&#1088;&#1080;&#1074;&#1110;&#1090;&#1083;&#1080;&#1074;&#1080;&#1081;</trd>
  </trdgrp>
  <trd lng="zh">&#21644;&#34108; <pr>h&#233;&#462;i</pr></trd>
</drv>

<drv mrk="jovial.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Videbla, senceremonia gajo:
      <ekz>
        &leftquot;vin, sinjoroj&rightquot;, aldonis li kun ia malnovmoda 
<tld/>eco, &leftquot;permesu
        al mi inviti&rightquot;
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p. 117a</lok>
</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_s;&c_ja;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>
&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;
</trd>
  </trdgrp>
  <trd lng="cs">&#382;ovi&#225;lnost</trd>
  <trd lng="de">Jovialit&auml;t</trd>
  <trd lng="es">jovialidad</trd>
  <trdgrp lng="fr">
    <trd>jovialit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ked&eacute;lyess&eacute;g</trd>,
    <trd>jovialit&aacute;s</trd>
  </trdgrp>
  <trd lng="it">giovialit&agrave;</trd>
  <trd lng="nl">jovialiteit</trd>
  <trd lng="ru">
&c_zh;&c_i;&c_z;&c_n;&c_je;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;
</trd>
  <trd lng="sk">&#382;ovi&#225;lnos&#357;</trd>
  <trd lng="zh">&#24841;&#24555;&#30340;&#35328;&#34892; <pr>y&#250;ku&#224;idey&#225;nx&#237;ng</pr></trd>
</drv>
