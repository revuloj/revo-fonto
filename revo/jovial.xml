<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>jovial</rad>/a <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="jovial.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Bonhumore <ref tip="vid" cel="gaj.0a">gaja</ref> kaj gajiga.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>,
    <trd>&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="es">jovial</trd>
  <trdgrp lng="fr">
    <trd>jovial</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ked&eacute;lyes</trd>,
    <trd>jovi&aacute;lis</trd>
  </trdgrp>
  <trd lng="nl">joviaal</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_z;&c_n;&c_je;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="jovial.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Videbla, senceremonia gajo:
      <ekz>
        &leftquot;vin, sinjoroj&rightquot;, aldonis li kun ia malnovmoda <tld/>eco, &leftquot;permesu
        al mi inviti&rightquot;
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p. 117a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_s;&c_ja;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">jovialidad</trd>
  <trdgrp lng="fr">
    <trd>jovialit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ked&eacute;lyess&eacute;g</trd>,
    <trd>jovialit&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">jovialiteit</trd>
  <trd lng="ru">&c_zh;&c_i;&c_z;&c_n;&c_je;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
