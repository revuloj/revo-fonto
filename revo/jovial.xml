<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jovial</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jovial.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Bonhumore <ref tip="vid" cel="gaj.0a">gaja</ref> kaj gajiga.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>jovial</trd>
  </trdgrp>
</drv>

<drv mrk="jovial.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Videbla, senceremonia gajo:
      <ekz>
        &leftquot;vin, sinjoroj&rightquot;, aldonis li kun ia malnovmoda <tld/>eco, &leftquot;permesu
        al mi inviti&rightquot;
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p. 117a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>jovialit&eacute;</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
