<art mrk="$Id: jovial.xml,v 1.15 2025/12/18 20:33:44 revo Exp $">
<kap><rad>jovial</rad>/a <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="jovial.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Bonhumore <ref tip="vid" cel="gaj.0a">gaja</ref> kaj gajiga.
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>gajema</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>,
    <trd>&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_y;
    </trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vesel&yacute;</trd>,
    <trd>&zcaron;ovi&aacute;ln&iacute;</trd>
  </trdgrp>
  <trd lng="de">jovial</trd>
  <trd lng="es">jovial</trd>
  <trdgrp lng="fr">
    <trd>jovial</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ked&eacute;lyes</trd>,
    <trd>jovi&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">gioviale</trd>
  <trdgrp lng="pl">
    <trd>jowialny</trd>,
    <trd>&zdot;artobliwy</trd>
  </trdgrp>
  <trd lng="nl">joviaal</trd>
  <trdgrp lng="pt">
    <trd>alegre</trd>,
    <trd>jovial</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_z;&c_n;&c_je;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobrej n&aacute;lady</trd>,
    <trd>rozmarn&yacute;</trd>,
    <trd>&zcaron;ovi&aacute;lny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_t;&c_t;&c_jeu;&c_r;&c_a;&c_d;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_t;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_ib;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#21644;&#34108; <pr>h&eacute;&#462;i</pr></trd>
</drv>

<drv mrk="jovial.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Videbla, senceremonia gajo:
      <ekz>
        &leftquot;vin, sinjoroj&rightquot;, aldonis li kun ia malnovmoda
<tld/>eco, &leftquot;permesu
        al mi inviti&rightquot;
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p. 117a</lok>
</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_s;&c_ja;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>
&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_r;&c_a;&c_d;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;
</trd>
  </trdgrp>
  <trd lng="cs">&zcaron;ovi&aacute;lnost</trd>
  <trd lng="de">Jovialit&auml;t</trd>
  <trd lng="es">jovialidad</trd>
  <trdgrp lng="fr">
    <trd>jovialit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ked&eacute;lyess&eacute;g</trd>,
    <trd>jovialit&aacute;s</trd>
  </trdgrp>
  <trd lng="it">giovialit&agrave;</trd>
  <trd lng="nl">jovialiteit</trd>
  <trd lng="pl">jowialno&sacute;&cacute;</trd>
  <trd lng="ru">
&c_zh;&c_i;&c_z;&c_n;&c_je;&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;
</trd>
  <trd lng="sk">&zcaron;ovi&aacute;lnos&tcaron;</trd>
  <trd lng="zh">&#24841;&#24555;&#30340;&#35328;&#34892; <pr>y&uacute;ku&agrave;idey&aacute;nx&iacute;ng</pr></trd>
</drv>
</art>
