<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pietis.xml,v 1.24 2023/10/20 16:38:16 revo Exp $">
<kap>
  <rad>pietism</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pietis.0o">
  <kap><tld/>o</kap>
  <snc mrk="pietis.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Luterana doktrino emfazanta personan piecon,
      esti&gcirc;inta en la 17a jarcento.
    </dif>
  </snc>
  <snc mrk="pietis.0o.konduto">
    <uzo tip="stl">FIG</uzo>
    <dif>
       Konduto konforma al tiu doktrino, <ref tip="sin" cel="pi.0eco">pieco</ref>, 
       <ref cel="modest.0a">modest</ref>egeco:
      <ekz>
        la &ccirc;efa merito de fratoj <nom>Grimm</nom> estas unue, ke ili sukcesis 
        kolekti tiel grandan nombron da fabeloj, kaj due, ke <klr>[&#x2026;]</klr>
        kun respekta <tld/>o ili skribis tion, kion ili a&ubreve;dis de popolaj
        rakontantoj, &scirc;an&gcirc;ante nenion, e&ccirc; konservante iafoje la popolan dialekton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_je;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">pietizmus</trd>
  <trd lng="de">Pietismus</trd><trd lng="de">pietismo</trd>
  <trd lng="en">pietism</trd>
  <trd lng="es">pietismo</trd>
  <trd lng="fa">&a_t;&a_q;&a_w;&a_A;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">pi&eacute;tisme</trd>
  <trd lng="hu">pietizmus</trd>
  <trd lng="ja">&#x656C;&#x8654;&#x4E3B;&#x7FA9;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x3057;&#x3085;&#x304E;</pr></trd>
  <trd lng="nl">pi&euml;tisme</trd>
  <trd lng="pl">pietyzm</trd>
  <trd lng="pt">pietismo</trd>
  <trd lng="ru">&c_p;&c_i;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">pietizmus</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_jeu;&c_t;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_p;&c_ib;&c_jeu;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_g;&c_l;&c_i;&c_b;&c_o;&c_k;&c_a; &c_p;&c_o;&c_v;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_o;&c_b;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pietis.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="pietis.0ano.KRI">
    <dif>
      Ano de <ref tip="vid" cel="pietis.0o.KRI"><tld/>o</ref>;
      <ref tip="sin" cel="pietist.0o.KRI">pietisto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_je;&c_t;&c_y;&c_s;&c_t;</trd>
  <trd lng="en">pietist</trd>
  <trd lng="fr">pi&eacute;tiste</trd>
</drv>
</art>
