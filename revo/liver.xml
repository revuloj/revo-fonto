    <trd>doru&ccaron;i&tcaron;</trd>,
    <trd>odovzda&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>,
