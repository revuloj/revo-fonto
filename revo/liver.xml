    <trd>&#x5F15;&#x304D;&#x6E21;&#x3059;  <pr>&#x3072;&#x304D;&#x308F;&#x305F;&#x3059;</pr></trd>,
    <trd>&#x7D0D;&#x54C1;&#x3059;&#x308B;  <pr>&#x306E;&#x3046;&#x3072;&#x3093;&#x3059;&#x308B;</pr></trd>,
