<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>liver</rad>/i
</kap>
<drv mrk="liver.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="liver.0i.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Transdoni en ies posedon:
      <ekz>
        <tld/>i al la a&ccirc;etinto la venditajn varojn;
      </ekz>
      <ekz>
        <tld/>i krudmaterialon al fabriko, viandon al armeo dokumentojn al
        malamiko;
      </ekz>
      <ekz>
        <tld/>i &gcirc;is la domo de iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &gcirc;is hejme
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proviz.0i">provizi</ref>.
    </refgrp>
   <trdgrp lng="de">
     <trd>liefern</trd>,
     <trd>&uuml;berbringen</trd>
   </trdgrp>
  </snc>
  <snc mrk="liver.0i.havigi">
    <dif>
      Havigi:
      <ekz>
        ne&gcirc;o <tld/>as akvon al la kampoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi povos <tld/>i al vi vivrimedojn per mia profesio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        brando <tld/>as vivigan varmon al lia organismo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la historio <tld/>as multajn pruvojn ke...;
      </ekz>
      <ekz>
        la farmbieno <tld/>is (naskis, ka&ubreve;zis) pli kaj pli multajn
        &ccirc;agrenojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la ri&ccirc;a&jcirc;oj ne <tld/>as (estigas) la feli&ccirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro <tld/>as sian opinion pri sportoj kiel kuracisto
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.0i">doni</ref>,
      <ref cel="prezen1.0i">prezenti</ref>.
    </refgrp>
   <trdgrp lng="de">
     <trd>ausstatten</trd>,
     <trd>bringen</trd>,
     <trd>mit sich bringen</trd>
   </trdgrp>
  </snc>
  <trd lng="en">deliver</trd>
  <trd lng="es">entregar</trd>
  <trdgrp lng="fr">
    <trd>fournir</trd>,
    <trd>livrer</trd>
  </trdgrp>
  <trd lng="nl">leveren</trd>
  <trdgrp lng="pt">
    <trd>entregar</trd>,
    <trd>fornecer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_a;&c_b;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="liver.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiu, kiu havigas varojn la&ubreve; interkonsentitaj kondi&ccirc;oj
    </dif>
    <refgrp tip="vid">
      <ref cel="per.0anto">peranto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Lieferant</trd>,
     <trd>Lieferer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fournisseur</trd>
  </trdgrp>
  <trd lng="nl">leverancier</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_shch;&c_i;&c_k;</trd>
</drv>
</art>
