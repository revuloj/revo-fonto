<kap>
  <ofc>*</ofc>
  <rad>liver</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="liver.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="liver.0i.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Transdoni en ies posedon:
      <ekz>
        <tld/>i al la a&ccirc;etinto la venditajn varojn;
      </ekz>
      <ekz>
        <tld/>i krudmaterialon al fabriko, viandon al armeo dokumentojn
        al
        malamiko;
      </ekz>
      <ekz>
        <tld/>i &gcirc;is la domo de iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &gcirc;is hejme
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proviz.0i">provizi</ref>.
    </refgrp>
   <trdgrp lng="ca">
    <trd>lliurar</trd>,
    <trd>entregar</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>liefern</trd>,
     <trd>&uuml;berbringen</trd>
   </trdgrp>
  <trd lng="hu">sz&aacute;ll&iacute;t</trd>
  <trd lng="th">&#3618;&#3585;&#3651;&#3627;&#3657;</trd>   
  </snc>
  <snc mrk="liver.0i.havigi">
    <dif>
      Havigi:
      <ekz>
        ne&gcirc;o <tld/>as akvon al la kampoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi povos <tld/>i al vi vivrimedojn per mia profesio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        brando <tld/>as vivigan varmon al lia organismo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la historio <tld/>as multajn pruvojn ke...;
      </ekz>
      <ekz>
        la farmbieno <tld/>is (naskis, ka&ubreve;zis) pli kaj pli
        multajn
        &ccirc;agrenojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la ri&ccirc;a&jcirc;oj ne <tld/>as (estigas) la
        feli&ccirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro <tld/>as sian opinion pri sportoj kiel
        kuracisto
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.0i">doni</ref>,
      <ref cel="prezen1.0i">prezenti</ref>.
    </refgrp>
   <trd lng="ca">donar</trd>
    <trdgrp lng="de">
     <trd>ausstatten</trd>,
     <trd>bringen</trd>,
     <trd>mit sich bringen</trd>
   </trdgrp>
    <trd lng="th">&#3607;&#3635;&#3651;&#3627;&#3657;&#3617;&#3637;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>doru&ccaron;it</trd>,
    <trd>odevzdat</trd>,
    <trd>p&rcaron;edat</trd>
  </trdgrp>
  <trd lng="en">deliver</trd>
  <trd lng="es">entregar</trd>
  <trdgrp lng="fr">
    <trd>fournir</trd>,
    <trd>livrer</trd>
  </trdgrp>
  <trd lng="hu">hoz</trd>
  <trdgrp lng="ja">
    <trd>&#37197;&#36948;&#12377;&#12427;  <pr>&#12399;&#12356;&#12383;&#12388;&#12377;&#12427;</pr></trd>,
    <trd>&#23626;&#12369;&#12427;  <pr>&#12392;&#12393;&#12369;&#12427;</pr></trd>,
    <trd>&#24341;&#12365;&#28193;&#12377;  <pr>&#12402;&#12365;&#12431;&#12383;&#12377;</pr></trd>,
    <trd>&#32013;&#21697;&#12377;&#12427;  <pr>&#12398;&#12358;&#12402;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#20379;&#32102;&#12377;&#12427;  <pr>&#12365;&#12423;&#12358;&#12365;&#12421;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">leveren</trd>
  <trdgrp lng="pl">
    <trd>dor&eogonek;cza&cacute;</trd>,
    <trd>dostawia&cacute;</trd>,
    <trd>szacowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>entregar</trd>,
    <trd>fornecer</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nm&icirc;na</trd>,
    <trd>adaug&abreve;</trd>,
    <trd>evalua</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_a;&c_b;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
