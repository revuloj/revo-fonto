<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: liver.xml,v 1.18 2022/03/22 18:26:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>liver</rad>/i
</kap>
<drv mrk="liver.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="liver.0i.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Transdoni en ies posedon:
      <ekz>
        <tld/>i al la a&ccirc;etinto la venditajn varojn;
      </ekz>
      <ekz>
        <tld/>i krudmaterialon al fabriko, viandon al armeo dokumentojn
        al
        malamiko;
      </ekz>
      <ekz>
        <tld/>i &gcirc;is la domo de iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &gcirc;is hejme
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proviz.0i">provizi</ref>.
    </refgrp>
   <trdgrp lng="de">
     <trd>liefern</trd>,
     <trd>&uuml;berbringen</trd>
   </trdgrp>
  <trd lng="hu">sz&aacute;ll&iacute;t</trd>
  <trd lng="th">&#xE22;&#xE01;&#xE43;&#xE2B;&#xE49;</trd>   
  </snc>
  <snc mrk="liver.0i.havigi">
    <dif>
      Havigi:
      <ekz>
        ne&gcirc;o <tld/>as akvon al la kampoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi povos <tld/>i al vi vivrimedojn per mia profesio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        brando <tld/>as vivigan varmon al lia organismo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la historio <tld/>as multajn pruvojn ke...;
      </ekz>
      <ekz>
        la farmbieno <tld/>is (naskis, ka&ubreve;zis) pli kaj pli
        multajn
        &ccirc;agrenojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la ri&ccirc;a&jcirc;oj ne <tld/>as (estigas) la
        feli&ccirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro <tld/>as sian opinion pri sportoj kiel
        kuracisto
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.0i">doni</ref>,
      <ref cel="prezen1.0i">prezenti</ref>.
    </refgrp>
   <trdgrp lng="de">
     <trd>ausstatten</trd>,
     <trd>bringen</trd>,
     <trd>mit sich bringen</trd>
   </trdgrp>
    <trd lng="th">&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE21;&#xE35;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>doru&ccaron;it</trd>,
    <trd>odevzdat</trd>,
    <trd>p&rcaron;edat</trd>
  </trdgrp>
  <trd lng="en">deliver</trd>
  <trd lng="es">entregar</trd>
  <trdgrp lng="fr">
    <trd>fournir</trd>,
    <trd>livrer</trd>
  </trdgrp>
  <trd lng="hu">hoz</trd>
  <trd lng="nl">leveren</trd>
  <trdgrp lng="pl">
    <trd>dor&eogonek;cza&cacute;</trd>,
    <trd>dostawia&cacute;</trd>,
    <trd>szacowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>entregar</trd>,
    <trd>fornecer</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nm&icirc;na</trd>,
    <trd>adaug&abreve;</trd>,
    <trd>evalua</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_a;&c_b;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>doda&tcaron;</trd>,
    <trd>doru&ccaron;i&tcaron;</trd>,
    <trd>odovzda&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x441;&#x442;&#x430;&#x432;&#x43B;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x43E;&#x441;&#x442;&#x430;&#x432;&#x43B;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x43E;&#x441;&#x442;&#x430;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x434;&#x430;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x440;&#x443;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x434;&#x430;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="liver.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiu, kiu havigas varojn la&ubreve; interkonsentitaj kondi&ccirc;oj
    </dif>
    <refgrp tip="vid">
      <ref cel="per.0anto">peranto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_a;&c_w;&c_ch;&c_sh;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>dodavatel</trd>,
    <trd>dopravce</trd>,
    <trd>liferant</trd>,
    <trd>lifrant</trd>,
    <trd>spedit&eacute;r</trd>,
    <trd>zasilatel</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Lieferant</trd>,
     <trd>Lieferer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fournisseur</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;ll&iacute;t&oacute;</trd>
  <trd lng="nl">leverancier</trd>
  <trd lng="pl">dostawca</trd>     
  <trd lng="ro">furnizor</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_shch;&c_i;&c_k;</trd>
  <trd lng="sk">dod&aacute;vate&lcaron;</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x441;&#x442;&#x430;&#x447;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x43A;</trd>
</drv>
</art>
