<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: turki.xml,v 1.9 2018/09/11 19:10:15 revo Exp $">
<kap>
  <rad>Turki</rad>/o
</kap>

<drv mrk="turki.0o">
  <kap>
    <tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc mrk="turki.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="turk.T0ujo.GEOG">Turkujo</ref>:
      <ekz>
        en a&ubreve;gusto <nom>Marta</nom> kaj &scirc;ia amikino Aniko decidis somerumi en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest </aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007050.html">La kaptilo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        malnova balkana droga&ccirc;o-vojo, kiu kunligis <tld/>on, Bulgarion, Jugoslavion kaj Okcidenton, malaperis
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9310-artikoletoj.txt">Malkopiilo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        cele al defendo de tiu bara&jcirc;lago <tld/>o zorgeme instalis apud &gcirc;i ter-aer-raketojn
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006866.html">La blua "oro" &ccirc;iam malsufi&ccirc;os</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_T;&c_u;&c_r;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_T;&c_u;&c_r;&c_e;&c_ch;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Turquia</trd>,
    <trd>Rep&uacute;blica de Turquia</trd>
  </trdgrp>
  <trd lng="de">T&uuml;rkei</trd>
  <trd lng="en">Turkey</trd>
  <trd lng="es">Turqu&iacute;a</trd>
  <trd lng="fa">&a_t;&a_r;&f_k;&f_y;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>Turquie</trd>,
    <trd>R&eacute;publique de Turquie</trd>
  </trdgrp>
  <trd lng="fy">Turkije</trd>
  <trd lng="id">Turki</trd>
  <trd lng="io">Turkia</trd>
  <trd lng="nl">Turkije</trd>
  <trd lng="pl">Turcja</trd>
  <trd lng="pt">Turquia</trd>
  <trd lng="ro">&c_T;urcia</trd>
  <trd lng="ru">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sv">Turkiet</trd>
  <trd lng="tp">ma Tuki</trd>
  <trd lng="tr">T&uuml;rkiye</trd>
  <trd lng="vo">T&uuml;rk&auml;n</trd>
</drv>

</art>

