<art mrk="$Id: turki.xml,v 1.17 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>Turki</rad>/o
</kap>

<drv mrk="turki.0o">
  <kap>
    <tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc mrk="turki.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="turk.T0ujo.GEOG">Turkujo</ref>:
      <ekz>
        en a&ubreve;gusto <nom>Marta</nom> kaj &scirc;ia amikino Aniko decidis somerumi en <tld/>o
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2000/007050.php">La kaptilo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 21</lok>
          </fnt>;
      </ekz>
      <ekz>
        malnova balkana droga&ccirc;o-vojo, kiu kunligis <tld/>on, Bulgarion, Jugoslavion kaj Okcidenton,
        malaperis
        <fnt>
          <aut>div.</aut>
          <vrk><url ref="&Monato;/1993/A931002.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 4</lok>
         </fnt>;
      </ekz>
      <ekz>
        cele al defendo de tiu bara&jcirc;lago <tld/>o zorgeme instalis apud &gcirc;i ter-aer-raketojn
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006866.php">La blua &leftquot;oro&#x201D; &ccirc;iam malsufi&ccirc;os</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_T;&c_u;&c_r;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_T;&c_u;&c_r;&c_e;&c_ch;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Turquia</trd>,
    <trd>Rep&uacute;blica de Turquia</trd>
  </trdgrp>
  <trd lng="de">T&uuml;rkei</trd>
  <trd lng="en">Turkey</trd>
  <trd lng="es">Turqu&iacute;a</trd>
  <trd lng="fa">&a_t;&a_r;&f_k;&f_y;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>Turquie</trd>,
    <trd>R&eacute;publique de Turquie</trd>
  </trdgrp>
  <trd lng="fy">Turkije</trd>
  <trd lng="id">Turki</trd>
  <trd lng="io">Turkia</trd>
  <trd lng="ja">&#x30C8;&#x30EB;&#x30B3;  <pr>&#x3068;&#x308B;&#x3053;</pr></trd>
  <trd lng="nl">Turkije</trd>
  <trd lng="pl">Turcja</trd>
  <trd lng="pt">Turquia</trd>
  <trd lng="ro">Turcia</trd>
  <trd lng="ru">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sv">Turkiet</trd>
  <trd lng="tok">ma Tuki</trd>
  <trd lng="tr">T&uuml;rkiye</trd>
  <trd lng="uk">&c_T;&c_u;&c_r;&c_je;&c_ch;&c_ch;&c_i;&#x43D;&c_a;</trd>
  <trd lng="vo">T&uuml;rk&auml;n</trd>
</drv>

</art>

