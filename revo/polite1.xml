    <trd>polyteista</trd>,
    <trd>vyznava&ccaron; mnohobo&zcaron;stv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Polytheist</trd>
  <trd lng="es">politeista</trd>
  <trd lng="fr">polyth&eacute;iste</trd>
  <trdgrp lng="hu">
    <trd>sokistenh&iacute;v&odblac;</trd>,
    <trd>politesita</trd>
  </trdgrp>
  <trd lng="ja">&#x591A;&#x795E;&#x6559;&#x5F92;  <pr>&#x305F;&#x3057;&#x3093;&#x304D;&#x3087;&#x3046;&#x5F92;</pr></trd>
  <trd lng="nl">polythe&iuml;st</trd>
  <trdgrp lng="pl">
    <trd>ba&lstroke;wochwalca</trd>,
    <trd>politeista</trd>
  </trdgrp>
  <trd lng="pt">polite&iacute;sta</trd>
  <trdgrp lng="ro">
    <trd>p&abreve;g&acirc;n</trd>,
    <trd>politeist</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_t;&c_je;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">polyteista</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&c_ib;&c_t;&c_je;&c_ji;&c_s;&c_t;</trd>
