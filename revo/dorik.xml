<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dorik.xml,v 1.2 2022/03/16 11:42:20 revo Exp $">
<kap>
  <rad>dorik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dorik.0o">
  <kap><tld/>o,
  <var><kap><tld/>a ordo</kap></var></kap>
  <snc mrk="dorik.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Plej antikva kaj simpla stilo
      (faklingve <ref tip="vid" cel="ord.0o.ARKI">ordo<sncref/></ref>)
      de antikva greka konstruarto kun difinitaj proporcioj,
      formoj kaj ornamoj de konstrueroj, precipe de kolonoj:
      <ekz>
        ni preterlasos la plej antikvan monumenton
        konservitan en Pompejo &mdash; <tld/>an templon
        <fnt><bib>IntArkeol</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">style <ind>dorique</ind></trd>
</drv>

<drv mrk="dorik.0a">
  <kap><tld/>a</kap>
  <snc mrk="dorik.0a.ARKI">
    <dif>
      Konforma al <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">dorique</trd>
  <trd lng="uk">&#x434;&#x43E;&#x440;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
