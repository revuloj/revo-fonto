<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: multan.xml,v 1.5 2024/01/26 19:05:54 afido Exp $">
    <kap>
      <rad>Multan</rad>/o
    </kap>

<drv mrk="multan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="pangxab.0o">Pan&gcirc;abo</ref>, 
      <ref tip="malprt" cel="pakistan.P0o">Pakistano</ref>, 
      &ccirc;. 300&#x202F;km sudokcidente de <ref cel="lahor.0o">Lahoro</ref>:
      <ekz>
        <tld/>o estas sufi&ccirc;e granda urbo kaj &gcirc;i havas multajn e&ubreve;ropstilajn konstrua&jcirc;ojn
        faritajn, kiam Pakistano estis kolonio de Britio
        <fnt>
          <vrk><url ref="&Monato;/2017/011998.php">Voja&gcirc;o en Suda Pan&gcirc;abio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas konata kiel la &leftquot;urbo de sufioj&rightquot; pro la multnombro de ma&ubreve;zoleoj kaj
        tomboj de sufioj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Multano">Multano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_u;&c_l;&c_t;&c_a;&c_n;</trd>
    <trd lng="de">Multan</trd>
    <trd lng="en">Multan</trd>
    <trd lng="fr">Multan</trd>
    <trd lng="pl">Multan</trd>
    <trd lng="ro">Multan</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x5C14;&#x5766; <pr>m&#xF9;&#x11B;rt&#x1CE;n</pr></trd>,
    <trd>&#x6728;&#x723E;&#x5766; <pr>m&#xF9;&#x11B;rt&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

  </art>

