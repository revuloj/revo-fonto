<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dagerotipi.xml,v 1.8 2023/11/04 13:16:26 revo Exp $">
<kap>
  <rad>dagerotipi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dagerotipi.0o">
  <kap><tld/>o</kap>
  <snc mrk="dagerotipi.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Procedo per kiu oni faradis
      <ref tip="vid" cel="dagerotip.0o.FOT">dagerotipojn</ref>:
      <ekz>
        <nom>Melqu&iacute;ades</nom> <klr>[&#x2026;]</klr> lecionis al li pri la <tld/>o
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o estas fotografada <klr>[&#x2026;]</klr> metodo de la 19-a
        jarcento, inventita de la franca pentristo <nom>Louis Daguerre</nom> inter
        1835 kaj 1839
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Dagerotipio">
          dagerotipio</url>, 2010-09-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_g;&c_je;&c_r;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>dagerotypie</trd>,
    <trd>daguerrotypie</trd>
  </trdgrp>
  <trd lng="de">Daguerreotypie</trd>
  <trd lng="fr">daguerr&eacute;otypie</trd>
  <trd lng="ru">&c_d;&c_a;&c_g;&c_je;&c_r;&c_o;&c_t;&c_i;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">dagerotypia</trd>
  <trd lng="uk">&c_d;&c_a;&c_gu;&c_je;&c_r;&c_o;&c_t;&c_i;&c_p;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x9280;&#x7248;&#x651D;&#x5F71;&#x6CD5; <pr>y&#xED;nb&#x1CE;nsh&#xE8;y&#x1D0;ngf&#x1CE;</pr></trd>,
    <trd>&#x9280;&#x677F;&#x7167;&#x76F8;&#x6CD5; <pr>y&#xED;nb&#x1CE;nzh&#xE0;oxi&#xE0;ngf&#x1CE;</pr></trd>
  </trdgrp>
</drv>
</art>

