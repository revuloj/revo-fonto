<art mrk="$Id: diadem.xml,v 1.22 2023/10/14 11:26:53 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diadem</rad>/o
</kap>
<drv mrk="diadem.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Re&gcirc;a krono:
      <ekz>
        Princino Ozma prenis la <tld/>on de sia kapo kaj metis &gcirc;in
        sur la kapon de la konkurvenkinto
        <fnt><bib>DOz</bib>, <lok>&leftquot;la na&ubreve;
        porketoj&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">kroon</trd>
    <trd lng="pl">diadem</trd>
  </snc>
  <snc>
    <dif>
      Kronforma kapornamo tenanta la harojn de virinoj, ofte ornamita
      per
      juveloj, simbolanta la re&gcirc;econ de ties portanto.
    </dif>
    <trd lng="nl">diadeem</trd>
    <trd lng="pl">diadem</trd>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_d;&c_e;&c_m;&c_a;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_d;&c_je;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>diad&#xE9;m</trd>,
    <trd>&#x10D;elenka</trd>
  </trdgrp>
  <trd lng="de">Diadem</trd>
  <trd lng="en">diadem</trd>
  <trd lng="es">diadema</trd>
  <trd lng="fr">diad&egrave;me</trd>
  <trdgrp lng="he">
    <trd>&#x5E0;&#x5B5;&#x5D6;&#x5B6;&#x5E8;</trd>,
    <trd>&#x5E2;&#x5D8;&#x5E8;&#x5D4;</trd>
  </trdgrp>
  <trd lng="hu">diad&eacute;m</trd>
  <trdgrp lng="ja">
    <trd>&#x738B;&#x51A0;&#x578B;&#x9AEA;&#x98FE;&#x308A;  <pr>&#x304A;&#x3046;&#x304B;&#x3093;&#x304B;&#x305F;&#x304B;&#x307F;&#x304B;&#x3056;&#x308A;</pr></trd>,
    <trd>&#x738B;&#x51A0;  <pr>&#x304A;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pt">diadema</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_d;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">&#x10D;elenka <klr>(drahocenn&#xE1;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x456;&#x430;&#x434;&#x435;&#x43C;&#x430;</trd>,
    <trd>&#x446;&#x430;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x432;&#x456;&#x43D;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
</drv>
