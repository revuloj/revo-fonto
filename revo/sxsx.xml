    <trd>csitt!</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schsch!</trd>,
    <trd>Psst!</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd><ind>st</ind>!</trd>,
    <trd><ind>sst</ind>!</trd>
  </trdgrp>
  <trd lng="ja">&#x30B7;&#x30C3;&#xFF01;&#x9759;&#x304B;&#x306B;&#xFF01;  [<ind>&#x30B7;&#x30C3;&#xFF01;&#x3057;&#x305A;&#x304B;&#x306B;&#xFF01;</ind>]</trd>
  <trdgrp lng="pt">
    <trd>psiu</trd>,
    <trd>psit</trd>
  </trdgrp>
</drv>
