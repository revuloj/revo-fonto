<art mrk="$Id$">
<kap>
  <rad>&scirc;&scirc;</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxsx.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio silentiga</vspec></gra>
  <snc>
    <dif>
      Silentu, ne bruu:
      <ekz>
        Urbestro eniras tre malla&ubreve;te per la pintoj de la piedoj. &ndash; <tld
        lit="&Scirc;"/>... <tld/>
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 9a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">chut !</trd>
  <trdgrp lng="hu">
    <trd>pszt!</trd>,
    <trd>csitt!</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schsch!</trd>,
    <trd>Psst!</trd>
  </trdgrp>
</drv>
</art>

