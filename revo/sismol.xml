<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sismol.xml,v 1.21 2022/03/26 20:43:13 revo Exp $">
<kap>
  <rad>sismologi</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="e">seismologi</rad>/o <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>
<drv mrk="sismol.0o">
  <kap><tld/>o,
  <var><kap><tld var="e"/>o</kap></var></kap>
  <snc mrk="sismol.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>,
      kiu studas la tertremojn,
      alivorte <ref cel="sism.0o.GEOL">sismojn</ref>,
      kaj iliajn efikojn:
      <ekz>
        memoro pri tiuj tremoj, okazintaj en epoko, en kiu oni ankora&ubreve; ne
        kolektis sciencajn, fidindajn donita&jcirc;ojn pri <tld/>o, estis en la
        kapo de la lokuloj &ndash; kvankam ne en tiu de la fakuloj &ndash; tute malaperinta
        <fnt>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php">
          &Gcirc;is la tero tremas</url></vrk>
          <bib>Monato</bib>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="e"/>a observatorio
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://enesperantujo.blogspot.com/2018/12/la-suno-hispana-1.html">La Suno Hispana</url></vrk>
          <lok>Esperantaj bitoj, 2018-12-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_j;&c_s;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_je;&c_i;&c_z;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">sismologia</trd>
  <trd lng="cs">seismologie</trd>
  <trd lng="de">Seismologie</trd>
  <trd lng="en">seismology</trd>
  <trd lng="fa">&a_z;&a_l;&a_z;&a_l;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">sismologie</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldreng&eacute;stan</trd>,
    <trd>szeizmol&oacute;gia</trd>
  </trdgrp>
  <trd lng="nl">seismologie</trd>
  <trd lng="ru">&c_s;&c_je;&c_j;&c_s;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">seizmol&oacute;gia</trd>
  <trd lng="uk">&#x441;&#x435;&#x439;&#x441;&#x43C;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
</drv>
