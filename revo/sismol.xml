<art mrk="$Id$">
<kap>
  <rad>sismologi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sismol.0o">
  <kap><tld/>o</kap>
  <snc mrk="sismol.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>,
      kiu studas la tertremojn kaj iliajn efikojn:
      <ekz>
        tremoj, okazintaj en epoko, en kiu oni ankora&ubreve; ne kolektis sciencajn, fidindajn
        donita&jcirc;ojn pri <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php"
              >&Gcirc;is la tero tremas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sism.0a">sisma</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_j;&c_s;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_je;&c_i;&c_z;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">sismologia</trd>
  <trd lng="cs">seismologie</trd>
  <trd lng="de">Seismologie</trd>
  <trd lng="fa">&a_z;&a_l;&a_z;&a_l;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">sismologie</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldreng&eacute;stan</trd>,
    <trd>szeizmol&oacute;gia</trd>
  </trdgrp>
  <trd lng="nl">seismologie</trd>
  <trd lng="ru">&c_s;&c_je;&c_j;&c_s;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">seizmol&oacute;gia</trd>
</drv>
