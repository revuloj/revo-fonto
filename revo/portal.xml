  <trdgrp lng="sk">
    <trd>hlavn&aacute; br&aacute;na</trd>,
    <trd>hlavn&yacute; vchod <klr>(stav.)</klr></trd>
  </trdgrp>
  <trd lng="sv">portal</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&c_t;&c_a;&#x43B;</trd>
  <trdgrp lng="zh">
    <trd>&#22823;&#38272;&#x53E3; <pr>d&#xE0;m&#xE9;nk&#x1D2;u</pr></trd>,
    <trd>&#22823;&#x95E8;&#x53E3; <pr>d&#xE0;m&#xE9;nk&#x1D2;u</pr></trd>,
    <trd>&#x6B63;&#38272; <pr>zh&#xE8;ngm&#xE9;n</pr></trd>,
    <trd>&#x6B63;&#x95E8; <pr>zh&#xE8;ngm&#xE9;n</pr></trd>,
    <trd>&#38272;&#25142;&#x7DB2;&#x7AD9; <pr>m&#xE9;nh&#xF9;w&#x1CE;ngzh&#xE0;n</pr></trd>,
    <trd>&#x95E8;&#25143;&#x7F51;&#x7AD9; <pr>m&#xE9;nh&#xF9;w&#x1CE;ngzh&#xE0;n</pr></trd>
  </trdgrp>
