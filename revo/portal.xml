<art mrk="$Id: portal.xml,v 1.20 2023/09/03 12:45:10 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>portal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="portal.0o">
  <kap><tld/>o</kap>
  <snc mrk="portal.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Ccirc;efa monumenta enirejo de pre&gcirc;ejo a&ubreve; granda konstruo:
      <ekz>
        la <tld/>o konsistas el tri pordoj kaj kvar kolonoj, kies 
        surfaco estas inkrustita per verda marmoro
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-lvyou/vidindaj_lokoj/06.htm">
          Pitoreskejo Jiugongshan</url></vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_a;&c_l;</trd>
    <trd lng="de">Portal</trd>
    <trd lng="en">portal</trd>
    <trd lng="es">portal</trd>
    <trd lng="fr">portes<klr> (monumentales)</klr></trd>
    <trdgrp lng="hu">
      <trd>d&iacute;szkapu</trd>,
      <trd>f&odblac;bej&aacute;rat</trd>
    </trdgrp>
    <trd lng="nl">portaal</trd>
    <trd lng="pl">portal</trd>
    <trd lng="pt">portal</trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_a;&c_l;</trd>
  </snc>
  <snc mrk="portal.0o.KOMP">
    <fnt><bib>KompLeks</bib></fnt>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Granda <ref tip="super" cel="ret.0ejo.KOMP">retejo</ref>
      ordinare dedi&ccirc;ita al iu temo kaj provizanta diversajn
      servojn : forumojn, retpo&scirc;ton, noticojn...:
      <ekz>
        la <klr>...</klr> brazila entrepreno CIDKON <klr>...</klr> 
        administras la plej grandan interretan <tld/>on en la lingvo 
        Esperanto &ndash; Gxangalo.com
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/esperanto-nordkalifornio/message/2717">
          Unua televidkanalo...</url></vrk>,
          <lok>2005-12-31</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">portal</trd>
    <trd lng="es">portal</trd>
    <trd lng="pl">portal</trd>
  </snc>
  <trd lng="cs">port&aacute;l</trd>
  <trd lng="fr">portail</trd>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x9762;&#x5165;&#x53E3;  [<ind>&#x3057;&#x3087;&#x3046;&#x3081;&#x3093;&#x3044;&#x308A;&#x3050;&#x3061;</ind>]</trd>,
    <trd>&#x6B63;&#x9580;  [<ind>&#x305B;&#x3044;&#x3082;&#x3093;</ind>]</trd>,
    <trd>&#x30DD;&#x30FC;&#x30BF;&#x30EB;&#x30B5;&#x30A4;&#x30C8;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hlavn&aacute; br&aacute;na</trd>,
    <trd>hlavn&yacute; vchod <klr>(stav.)</klr></trd>
  </trdgrp>
  <trd lng="sv">portal</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x440;&#x442;&#x430;&#x43B;</trd>
</drv>
</art>
