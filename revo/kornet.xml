<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kornet</rad>/o
</kap>
<drv mrk="kornet.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.blov0o">Blovinstrumento</ref> simila
      al trumpeto kun klavbutonoj.
    </dif>
    <trd lng="en">cornet</trd>
    <trd lng="fr">cornet &agrave; pistons</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_n;&c_je;&c_t;</trd>,
      <trd>&c_k;&c_o;&c_r;&c_n;&c_je;&c_t;-&c_a;-&c_p;&c_i;&c_s;&c_t;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
