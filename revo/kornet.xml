<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kornet.xml,v 1.18 2023/09/01 15:46:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kornet</rad>/o
</kap>
<drv mrk="kornet.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="kornet.0o.blovinstrumento">
    <dif>
      <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">Blovinstrumento</ref> simila
      al trumpeto kun klavbutonoj:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> havas bora&jcirc;on konusan, pli kompaktan formon kaj pli malakran sonon ol
        la trumpeto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=135246">Korneto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_n;&c_e;&c_t; <klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;
    &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
    <trd lng="ca">corneta</trd>
    <trdgrp lng="de">
       <trd>Kornett</trd>
    </trdgrp>
    <trd lng="en">cornet</trd>
    <trd lng="es">corneta</trd>
    <trd lng="fr">cornet &agrave; pistons</trd>
    <trd lng="hu">kornett</trd>
    <trd lng="nl">cornet</trd>
    <trd lng="pl">kornet <klr>(instrument)</klr></trd>
    <trd lng="pt">corneta <klr tip="amb">(&ecirc;)</klr></trd>
    <trd lng="ro">cornet</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_n;&c_je;&c_t;</trd>,
      <trd>&c_k;&c_o;&c_r;&c_n;&c_je;&c_t;-&c_a;-&c_p;&c_i;&c_s;&c_t;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kornet.0o.registro">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Orgena registro; <ref tip="dif" cel="klario.0o">klariono</ref>:
      <ekz>
        la &ccirc;efa suborgeno enhavas kompletan principalaron,
        ofte sur 16&minute;-bazo, kun miksturoj en relative malalta pozicio
        kaj plenkalikaj lamenvo&ccirc;oj, kiuj formas kune kun la <tld/>o apartan lamenplenon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19603">Orgeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
<!-- ne traduku evitindajn...
    <trd lng="de"></trd>
-->
  </snc>
  <ref tip="hom" cel="kornet1.0o">korneto (rajdista oficiro)</ref>
  <trd lng="ja">&#x30B3;&#x30EB;&#x30CD;&#x30C3;&#x30C8;</trd>
</drv>
</art>
