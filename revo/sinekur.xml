<kap>
  <rad>sinekur</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sinekur.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sinekur.0o.KOMUNE">
    <dif>
      Facila, rutineca <ref tip="super" cel="ofic.0o">ofico</ref>:
      <ekz>
        la <nom>RTVV</nom>-amaskomunikilaro <klr>[&#x2026;]</klr> ta&ubreve;gis por havigi
        <tld/>ojn al lakeoj <klr>[&#x2026;]</klr> de la reganta partio
        <fnt>
          <aut>J.-H. Petres-Blezo</aut>,
          <vrk><url ref="http://europamaldekstro.wordpress.com/2014/03/11/valencilando-restas-sen-katalunlingva-televido/">
          Valencilando restas sen katalunlingva televido</url></vrk>,
          <lok>Etnismo, 2013-12-31 (92)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_e;&c_k;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>d&#x16F;chod bez z&#xE1;vazk&#x16F;</trd>,
    <trd>sinekura</trd>,
    <trd>v&#xFD;nosn&#xE9; m&#xED;sto</trd>
  </trdgrp>
  <trd lng="de">geruhsames <ind>P&ouml;stchen</ind></trd>
  <trd lng="fr">sin&eacute;cure</trd>
  <trdgrp lng="ja">
    <trd>&#x9591;&#x8077;  <pr>&#x304B;&#x3093;&#x3057;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x540D;&#x8A89;&#x8077;  <pr>&#x3081;&#x3044;&#x3088;&#x3057;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x697D;&#x306A;&#x5730;&#x4F4D;  <pr>&#x3089;&#x304F;&#x306A;&#x3061;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">synekura</trd>
  <trdgrp lng="sk">
    <trd>d&#xF4;chodok bez z&#xE1;v&#xE4;zkov</trd>,
    <trd>sinek&#xFA;ra</trd>,
    <trd>v&#xFD;nosn&#xE9; miesto</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_i;&#x43D;&c_je;&#x43A;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9591;&#x8077; <pr>xi&#xE1;nzh&#xED;</pr></trd>,
    <trd>&#x95F2;&#x804C; <pr>xi&#xE1;nzh&#xED;</pr></trd>,
    <trd>&#x5C38;&#x4F4D;&#x7D20;&#x9910; <pr>sh&#x12B;w&#xE8;is&#xF9;c&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>

