<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lirl.xml,v 1.8 2024/01/15 19:02:15 revo Exp $">
<kap>
  <rad>lirl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lirl.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">POE</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lirl.0i.SON">
    <dif>
      Delikate murmuri, kiel fluanta rivereto;
      milde <ref tip="vid" cel="glugl.0i.SON">glugli</ref>:
      <ekz>
        en la Valo Jinxiu <tld/>as akvofluo,
        diversformas arboj kaj aromas floroj
        <fnt>
          <aut>Zhao Xi, Wang Lihua</aut>,
          <vrk><url ref="http://www.espero.com.cn/2013-09/25/content_30129103.htm">
          La Valo Jinxiu...</url></vrk>,
          <lok>EEl Popola &Ccirc;inio, 2013-09-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">pl&auml;tschern</trd>
  <trdgrp lng="en">
    <trd>burble</trd>,
    <trd>gurgle</trd>,
    <trd>purl</trd>  
  </trdgrp>    
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>&scaron;um&ecaron;t</trd>
  </trdgrp>
  <trd lng="fr">murmurer<klr> (ruisseau etc.)</klr></trd>
  <trdgrp lng="pl">
    <trd>gaworzy&cacute;</trd>,
    <trd>szemra&cacute;</trd>,
    <trd>trajkota&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>p&abreve;l&abreve;vr&abreve;gi</trd>,
    <trd>susura</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bubla&tcaron;</trd>,
    <trd>klokota&tcaron;</trd>,
    <trd>zur&ccaron;a&tcaron;</trd>,
    <trd>&scaron;umie&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_z;&c_ju;&c_r;&c_ch;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_s;&c_t;&c_r;&c_u;&c_m;&c_o;&c_k;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x6D41;&#x98DB;&#x6FFA; <pr>shu&#x1D0;li&#xFA;f&#x113;iji&#xE0;n</pr></trd>,
    <trd>&#x6C34;&#x6D41;&#x98DE;&#x6E85; <pr>shu&#x1D0;li&#xFA;f&#x113;iji&#xE0;n</pr></trd>,
    <trd>&#x6C69;&#x6C69; <pr>g&#x1D4;g&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lirl.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="lirl.0o.SON">
    <dif>
      Delikata <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>
      de fluanta akvo:
      <ekz>
        &ccirc;irpas gril' &ccirc;e rivereto, /
        io en la akvo pla&ubreve;das, /
        la migranto <tld/>on a&ubreve;das
        <fnt>
          <aut>H. Heine, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.literaturo.dk/kanzonoj.htm">
          Kanzonoj</url></vrk>,
          <lok>Esperanto, 1956, p. 148a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bubl&aacute;n&iacute;</trd>,
    <trd>klokot&aacute;n&iacute;</trd>,
    <trd>&scaron;um&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Pl&auml;tschern</trd>
  <trd lng="fr">murmure<klr> (ruisseau etc.)</klr></trd>
  <trd lng="pl">szemranie</trd>     
  <trd lng="ro">fream&abreve;t</trd>
  <trdgrp lng="sk">
    <trd>bublanie</trd>,
    <trd>klokotanie</trd>,
    <trd>&scaron;umenie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_z;&c_ju;&c_r;&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_d;&c_z;&c_ju;&c_r;&c_ch;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

