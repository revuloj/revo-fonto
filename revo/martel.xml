<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>martel</rad>/o
</kap>
<drv mrk="martel.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <snc>
     <dif>
       Batilo konsistanta el peza ferpeco kun ligna tenilo kaj servanta por
       enigi najlojn, for&gcirc;i feron ktp.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>,
     <trd>&c_m;&c_o;&c_l;&c_a;&c_t;</trd>
   </trdgrp>
   <trd lng="br">morzhol</trd>
   <trd lng="de">Hammer</trd>
   <trd lng="en">hammer</trd>
   <trd lng="hu">kalap&aacute;cs</trd>
   <trd lng="nl">hamer</trd>
   <trd lng="pt">martelo</trd>
   <trdgrp lng="ru">
     <trd>&c_m;&c_o;&c_l;&c_o;&c_t;</trd>,
     <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_k;</trd>
   </trdgrp>
   <trd lng="sv">hammare</trd>
     <trd lng="fr">marteau</trd>
</drv>

<drv mrk="martel.0i">
   <kap>
     <tld/>i
   </kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Batadi per <tld/>o.
     </dif>
     <trd lng="be">&c_b;&c_ib;&c_c;&c_mol; &c_m;&c_o;&c_l;&c_a;&c_t;&c_a;&c_m;</trd>
     <trd lng="br">morzholat</trd>
     <trd lng="de">h&auml;mmern</trd>
     <trd lng="en">hammer</trd>
     <trd lng="hu">kalap&aacute;l</trd>
     <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; 
&c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_m;</trd>
     <trd lng="sv">hamra</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Aparte kaj forte akcenti:
       <ekz>
         <tld/>i &ccirc;iun vorton<fnt>Z</fnt>
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_a;&c_k;&c_c;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
     </trdgrp>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Komprenigi, memorigi ion per ripetata penado:
       <ekz>
         <tld/>i ion en la
         memoron de iu (<ref tip="sin" cel="cerb.en0igi">encerbigi</ref>);
       </ekz>
       <ekz>
         <tld/>i per reklamo en la cerbojn de la publiko la
         efikecon de iu medikamento
       </ekz>
     </dif>
     <trd lng="be">&c_u;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
     <trd lng="hu">belever</trd>
     <trd lng="ru">&c_v;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
   </snc>
   <trd lng="fr">marteler</trd>
   <trd lng="nl">hameren</trd>
   <trd lng="pt">martelar</trd>
</drv>
</art>
