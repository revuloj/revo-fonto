<kap>
  <rad>&gcirc;enr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="gxenr.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxenr.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="katego.0o.klaso">Kategorio<sncref/></ref>
      de verkoj kun komunaj karakterizoj:
      <ekz>
        Berlioz enigis en simfonion anka&ubreve; tiajn <tld/>ojn kiel 
        kon&ccirc;erto, oratorio
        <fnt><aut>J. Mura&scirc;kovskij, I. Mura&scirc;kovska</aut>, 
<vrk>Kien fluas roj&#x2019; Castalie?</vrk>, <lok>1984-1987</lok></fnt>;
      </ekz>
      <ekz>
        estas vere, ke el &ccirc;iuj <tld/>oj mi favoras la poezion
        <fnt>
          <aut>Poul Thorsen</aut>,
          <vrk>Babile kun mi mem. - Beletra: 1996</vrk>,
          <lok>
<url ref="http://www.poulthorsen.dk/finbabile.htm">Finbabile?</url></lok>
        </fnt>.            
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Genre</trd>,
      <trd>Kunst<ind>gattung</ind></trd>
    </trdgrp>
    <trd lng="es">g&eacute;nero<klr tip="amb">(m&uacute;s., lit., arte)</klr></trd>
    <trd lng="fa">&f_zh;&a_A;&a_n;&a_r;</trd>
    <trd lng="fr">genre<klr> (litt., musical etc.)</klr></trd>
    <trd lng="it">genere<klr tip="amb">(mus, let, arte)</klr></trd>
    <trdgrp lng="pl">
      <trd>scena rodzajowa</trd>,
      <trd>obraz rodzajowy</trd>
    </trdgrp>
    <trd lng="pt">g&ecirc;nero <klr tip="amb">(mus., lit.)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x4F53;&#x88C1; <pr>t&#x1D0;c&#xE1;i</pr></trd>,
    <trd>&#x6D41;&#x6D3E; <pr>li&#xFA;p&#xE0;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_r;</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30E3;&#x30F3;&#x30EB;  <pr>&#x3058;&#x3083;&#x3093;&#x308B;</pr></trd>,
    <trd>&#x69D8;&#x5F0F;  <pr>&#x3088;&#x3046;&#x3057;&#x304D;</pr></trd>,
    <trd>&#x98A8;&#x4FD7;&#x753B;  <pr>&#x3075;&#x3046;&#x305E;&#x304F;&#x304C;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_a;&c_n;&c_r;</trd>
  <trd lng="uk">&c_zh;&c_a;&#x43D;&c_r;</trd>
