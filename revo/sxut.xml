<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxut.xml,v 1.39 2024/03/25 07:28:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ut</rad>/i
</kap>
<drv mrk="sxut.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.0i.rektasence">
    <dif>
      <ref tip="super" cel="fal.0igi">Faligi</ref> ien substancon nefluidan, konsistantan el multaj
      apartaj,
      relative malgrandaj eroj, kiel seka sablo a&ubreve; greno:
      <ekz>
        ili &scirc;utis polvon sur siajn kapojn <klr>(por penti)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 7:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia edzino prenis kaj sternis kovrotukon <klr>[&#x2026;]</klr> kaj &scirc;utis sur &gcirc;in grion
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 17:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili prenis &ccirc;iu sian incensujon kaj metis sur &gcirc;in fajron kaj <tld/>is sur &gcirc;in
        incenson
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 16:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjorino Fradeko <tld/>is unu duonon de la blanka pulvoro en unu el tasoj
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la tombon nun <tld/>u teron, kiom ajn vi volas
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi dis<tld/>is ilin per <ind><tld/>ilo</ind> tra la pordegojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 15:7</lok>
        </fnt>.
        <trd lng="de">Worfschaufel</trd>
        <trd lng="en">winnowing shovel</trd>
        <trd lng="la">ventilabrum</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="versx.0i">ver&scirc;i</ref>
    <trd lng="be">&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">sch&uuml;tten</trd>
    <trd lng="en">pour</trd>
    <trd lng="es">verter (un s&oacute;lido en granos)</trd>
    <trd lng="fr">verser (un solide en grains)</trd>
    <trd lng="hu">sz&oacute;r</trd>
    <trd lng="nl">uitschudden</trd>
    <trd lng="pl">sypa&cacute;</trd>
    <trd lng="ru">&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
    <trdgrp lng="it">
      <trd>rovesciare<klr> (di sostanze in granuli)</klr></trd>,
      <trd>versare<klr> (di sostanze in granuli)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="sxut.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Grandnombre <ref cel="port.al0i.havigi">alporti</ref>, donadi:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        flatajn vortojn li <tld/>is
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili <klr>[&#x2026;]</klr> <tld/>is komplimentojn al la a&ubreve;toro
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la amaso dis<tld/>i&gcirc;is kaj kuras en diversajn flankojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 14:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as facilajn parolojn al li kaj postulas du &scirc;ilingojn
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la elektrokompanioj <klr>[&#x2026;]</klr> abunde <tld/>as monon en la komunumon
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011596.php">Anta&ubreve;en malanta&ubreve;en</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">&uuml;bersch&uuml;tten</trd>
    <trd lng="en">pour</trd>
    <trd lng="es">desbordar</trd>
    <trd lng="hu">el&aacute;raszt<klr tip="ind">
    (&aacute;tv.)</klr></trd>
    <trd lng="nl">uitschudden</trd>
    <trd lng="pl">sypa&cacute;</trd>
    <trd lng="ru">&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
    <trd lng="it">riversare</trd>
    <trdgrp lng="zh">
    <trd>&#x503E;&#x5012; <pr>q&#x12B;ngd&#xE0;o</pr></trd>,
    <trd>&#x50BE;&#x5012; <pr>q&#x12B;ngd&#xE0;o</pr></trd>,
    <trd>&#x6C4B; <pr>zhu&#xF3;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="sxut.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Transmeti datumojn amase kaj sen zorgi pri ilia strukturo
      a&ubreve; signifo.
    </dif>
    <rim>
      En la deriva&jcirc;oj
      <ref tip="sub" cel="sxut.al0i.KOMP">al<tld/>i</ref> kaj
      <ref tip="sub" cel="sxut.el0i.KOMP">el<tld/>i</ref>
      la prefiksoj esprimas rilaton al iu <ctl>Granda Bitujo</ctl>
      (&ccirc;efmemoro, &ccirc;efkomputilo, servilo): oni al<tld/>as
      bitojn 
      en &gcirc;in kiel oni alver&scirc;as akvon en samovaron a&ubreve;
      teujon; oni el<tld/>as bitojn por sia uzo, kiel oni elver&scirc;as
      
      teon por sia aparta uzo.
    </rim>
    <trd lng="be">&c_p;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;
<klr>(&c_f;&c_a;&c_j;&c_l;&c_y;,
&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
    <trd lng="bg">&c_p;&c_r;&c_je;&c_h;&c_v;&c_malmol;&c_r;&c_l;&c_ja;&c_m;</trd>
    <trd lng="de">laden</trd>
    <trd lng="nl">sponzen</trd>
    <trd lng="en">dump <klr>(data)</klr></trd>
    <trd lng="es">cargar <klr>(datos)</klr></trd>
    <trd lng="hu">t&ouml;lt<klr> (adatot)</klr></trd>
    <trdgrp lng="pl">
      <trd>zgra&cacute;<klr> (dane)</klr></trd>,
      <trd>zrzuca&cacute;<klr> (dane)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;
<klr>(&c_d;&c_a;&c_n;&c_n;&c_y;&c_je;)</klr></trd>
  </snc>
  <trd lng="bg">&c_s;&c_i;&c_p;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="ja">
    <trd>&#x307E;&#x304F;</trd>,
    <trd>&#x3070;&#x3089;&#x307E;&#x304F;</trd>,
    <trd>&#x3076;&#x3061;&#x307E;&#x3051;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_s;&c_i;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_i;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxut.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.al0i.rektasence">
    <dif>
      <ref tip="super" cel="don.al0i">Aldoni</ref> <tld/>ante:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li al<tld/>is novajn informojn
        <fnt><bib>MkM</bib>, <lok>P.293.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="es">esparcir</trd>
    <trd lng="hu">odasz&oacute;r</trd>
    <trd lng="pl">dosypa&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxut.al0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Transmeti programon a&ubreve; datumon de periferia komputilo al
      &ccirc;efkomputilo a&ubreve; servilo, la malo de el<tld/>i:
      <ekz>
        oni povas al<tld/>i 200 megabajtojn da dokumentoj (fotoj, filmoj, sondosieroj)
        monate
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2007/009333.php">&Ccirc;io en unu programo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni jam al<tld/>is pli ol 200 filmetojn
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> faras en la propra libertempo fotografiojn pri si mem kaj poste
        al<tld/>as tiujn bildojn al la reto
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011294.php">Tro da (mem)fotado, ja perturba agado</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;
<klr>(&c_f;&c_a;&c_j;&c_l;&c_y;,
&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
    <trd lng="bg">&c_k;&c_a;&c_ch;&c_v;&c_a;&c_m;</trd>
    <trd lng="de">hochladen</trd>
    <trd lng="en">upload<klr> (data)</klr></trd>
    <trdgrp lng="es">
      <trd>subir<klr> (datos)</klr></trd>,
      <trd>cargar<klr> (datos)</klr></trd>
    </trdgrp>
    <trd lng="hu">felt&ouml;lt <klr> (adatokat)</klr></trd>
    <trd lng="id">meng<ind>unggah</ind></trd>
    <trdgrp lng="nl">
      <trd>uploaden</trd>,
      <trd>verzenden<klr tip="ind"> (v.data)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wgra&cacute;<klr> (dane)</klr></trd>,
      <trd>za&lstroke;adowa&cacute;<klr> (dane)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;
<klr>
(&c_d;&c_a;&c_n;&c_n;&c_y;&c_je; &c_v;&c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;)
</klr></trd>,
      <trd>&c_v;&c_y;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; 
<klr>(&c_n;&c_a;&c_s;&c_je;&c_r;&c_v;&c_je;&c_r;)</klr>
</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4E0A;&#x8F09; <pr>sh&#xE0;ngz&#xE0;i</pr></trd>,
    <trd>&#x4E0A;&#x8F7D; <pr>sh&#xE0;ngz&#xE0;i</pr></trd>,
    <trd>&#x4E0A;&#x4F20; <pr>sh&#xE0;ngchu&#xE1;n</pr></trd>,
    <trd>&#x4E0A;&#x50B3; <pr>sh&#xE0;ngchu&#xE1;n</pr></trd>
  </trdgrp>
</snc>
<trd lng="it">caricare</trd>
  <trd lng="ja">&#x30A2;&#x30C3;&#x30D7;&#x30ED;&#x30FC;&#x30C9;&#x3059;&#x308B;  <pr>&#x3042;&#x3063;&#x3077;&#x30ED;&#x30FC;&#x30C9;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_i;&c_p;&c_a;&c_t;&c_i; (&c_shch;&c_o;-&c_n;. &c_d;&c_o; &c_ch;&c_o;&c_g;&c_o;-&c_n;.)</trd>,
    <trd>&c_z;&c_a;&c_v;&c_a;&c_n;&c_t;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxut.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <fnt><bib>PIV2</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;igi <tld/>ante, 
      &ccirc;irka&ubreve;igi per abundo da:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kvankam &ccirc;irka&ubreve;<tld/>ataj per liaj favoroj, <klr>[ili]</klr> tamen insultas lin
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_z;&c_ja;&c_m;&c_l;&c_jo;&c_ju;)</klr></trd>,
    <trd>&c_z;&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_i;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_s;&c_i;&c_p;&c_a;&c_t;&c_i; (&c_n;&c_a;&c_v;&c_k;&c_o;&c_l;&c_o;)</trd>
  </trdgrp>
</drv>

<drv mrk="sxut.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.el0i.rektasence">
    <dif>
      <ref tip="super" cel="el.0igi">Eligi</ref> <tld/>ante:
      <ekz>
        oni el<tld/>is la ar&gcirc;enton, trovitan en la domo
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 34:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li elpelis &ccirc;iujn el la templo <klr>[&#x2026;]</klr> el<tld/>is la monerojn de la
        mon&scirc;an&gcirc;istoj kaj renversis la tablojn
        <fnt>
          <bib>NT</bib>
          <lok>Johano 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> el<tld/>as el kofroj ilian interna&jcirc;on
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> demetas la dorsosakon kaj el<tld/>as la kafon kaj foriras
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vagonaroj konstante envenis kaj el<tld/>is amasojn da homoj
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="bg">&c_i;&c_z;&c_s;&c_i;&c_p;&c_v;&c_a;&c_m;</trd>
    <trd lng="hu">kisz&oacute;r</trd>
    <trd lng="nl">uitschudden</trd>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
    <trd lng="es">descargar</trd>
  </snc>
  <snc mrk="sxut.el0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Transmeti programon a&ubreve; datumon el &ccirc;efmemoro a&ubreve;      
      servilo al krommemoro a&ubreve; periferia komputilo, la malo de 
      al<tld/>i:
      <ekz>
        modernan Unikod-kapablan foliumilon vi povas 
        senpage el<tld/>i de <nom>Microsoft</nom> a&ubreve; <nom>Netscape</nom>
        <fnt><lok>
          <url ref="http://www.phon.ucl.ac.uk/home/wells/lek.htm">
          Londona Esperanto-Klubo</url></lok>;
        </fnt>;
      </ekz>
      <ekz>
        por li krei <ind>el<tld/>ejo</ind>n &ccirc;e Vinilkosmo estas rapidigi la morton de la KDisko,
        dronigi la eldonejon kaj kolapsigi la produktadon
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
        <trd lng="de">Downloadbereich</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;
<klr>(&c_f;&c_a;&c_j;&c_l;&c_y;,
&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
    <trdgrp lng="bg">
      <trd>&c_s;&c_v;&c_a;&c_l;&c_ja;&c_m;</trd>,
      <trd>&c_i;&c_z;&c_t;&c_je;&c_g;&c_l;&c_ja;&c_m;</trd>
    </trdgrp>
    <trd lng="de">herunterladen</trd>
    <trd lng="en">download<klr> (data)</klr></trd>
    <trdgrp lng="es">
      <trd>bajar<klr> (datos)</klr></trd>,
      <trd>descargar<klr> (datos)</klr></trd>
    </trdgrp>
    <trd lng="fr">t&eacute;l&eacute;charger</trd>
    <trd lng="hu">let&ouml;lt<klr> (adatokat)</klr></trd>
    <trd lng="id">meng<ind>unduh</ind></trd>
    <trdgrp lng="nl">
      <trd>sponzen<klr tip="ind"> (v.data)</klr></trd>,
      <trd>downloaden</trd>,
      <trd>binnenhalen<klr tip="ind"> (v.data)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zgra&cacute;<klr> (dane)</klr></trd>,
      <trd>zrzuca&cacute;<klr> (dane)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;
<klr>(&c_d;&c_a;&c_n;&c_n;&c_y;&c_je;)</klr></trd>
  </snc>
    <trd lng="it">scaricare</trd>
  <trd lng="ja">&#x30C0;&#x30A6;&#x30F3;&#x30ED;&#x30FC;&#x30C9;&#x3059;&#x308B;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_s;&c_i;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_a;&c_n;&c_t;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_shch;&c_o;&c_s;&c_mol; &c_s;&c_i;&c_p;&c_k;&c_je;)</trd>
  </trdgrp>
</drv>

<drv mrk="sxut.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.super0i.kovri">
    <dif>
      Tre abunde kovri per <tld/>ado:
      <ekz>
        <klr>[la]</klr> kampoj estas super<tld/>itaj de sablo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie sub la super<tld/>ita supra&jcirc;o ni havas ne argilon, sed sablojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj flaveblondaj haroj estis <klr>[&#x2026;]</klr> iom super<tld/>itaj per apena&ubreve;
        rimarkebla polveto el blanka pudro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
