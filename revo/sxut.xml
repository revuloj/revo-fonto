<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ut</rad>/i
</kap>
<drv mrk="sxut.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.0i.rektasence">
    <dif>
      Elglitigi substancon nefluidan, konsistantan el multaj apartaj,
      relative malgrandaj eroj, kiel seka sablo a&ubreve; greno:
      <ekz>
        la enhavo de la sakoj estas <tld/>ata en korbojn;
      </ekz>
      <ekz>
        la vento <tld/>is blankajn florojn sur niaj kapoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        kun<tld/>i &scirc;tonojn senorde<fnt>B</fnt>;
      </ekz>
      <ekz>
        sur<tld/>i la aleojn per sablo<fnt>B</fnt>;
      </ekz>
      <ekz>
        sur<tld/>i la plankon per floroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        kolekti per balailo la dis<tld/>itajn cindrojn;
      </ekz>
      <ekz>
        dis<tld/>i per <tld/>ilo<fnt>Z</fnt>
        <klr>(&scirc;ovelilo)</klr>;
      </ekz>
      <ekz>
        <tld/>plenigi truon;
      </ekz>
      <ekz>
        <tld/>i&gcirc;ema (neligita) hararo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="versx.0i">ver&scirc;i</ref>
    <trd lng="be">&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">sch&uuml;tten</trd>
    <trd lng="en">pour</trd>
    <trd lng="fr">verser (un solide en grains)</trd>
    <trd lng="hu">sz&oacute;r</trd>
    <trd lng="nl">uitschudden</trd>
    <trd lng="pl">sypa&cacute;</trd>
    <trd lng="ru">&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxut.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Grandnombre porti, doni:
      <ekz>
        la petoj pri sciigoj tuj al<tld/>i&gcirc;is;
      </ekz>
      <ekz>
        la amaso dis<tld/>i&gcirc;is kaj kuris en diversajn
        flankojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        multaj anoj dis<tld/>itaj (dissemitaj) sur la tuta terglobo.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="en">pour</trd>
    <trd lng="hu">el&aacute;raszt<klr tip="ind"> (&aacute;tv.)</klr></trd>
    <trd lng="nl">uitschudden</trd>
    <trd lng="pl">sypa&cacute;</trd>
    <trd lng="ru">&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxut.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Transmeti datumojn amase kaj sen zorgi pri ilia strukturo
      a&ubreve; signifo.
    </dif>
    <rim>
      En la deriva&jcirc;oj
      <ref tip="sub" cel="sxut.al0i.KOMP">al<tld/>i</ref> kaj
      <ref tip="sub" cel="sxut.el0i.KOMP">el<tld/>i</ref>
      la prefiksoj esprimas rilaton al iu <ctl>Granda Bitujo</ctl>
      (&ccirc;efmemoro, &ccirc;efkomputilo, servilo): oni al<tld/>as bitojn
en
      &gcirc;in kiel oni alver&scirc;as akvon en samovaron a&ubreve; teujon;
oni
      el<tld/>as bitojn por sia uzo, kiel oni elver&scirc;as teon por
      sia sia aparta uzo.
    </rim>
    <trd lng="be">&c_p;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;
<klr>(&c_f;&c_a;&c_j;&c_l;&c_y;,
&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
    <trd lng="nl">sponzen</trd>
    <trd lng="en">dump <klr>(data)</klr></trd>
    <trd lng="hu">t&ouml;lt<klr> (adatot)</klr></trd>
    <trdgrp lng="pl">
      <trd>zgra&cacute;<klr> (dane)</klr></trd>,
      <trd>zrzuca&cacute;<klr> (dane)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;
<klr>(&c_d;&c_a;&c_n;&c_n;&c_y;&c_je;)</klr></trd>
  </snc>
</drv>

<drv mrk="sxut.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.al0i.rektasence">
    <dif>
      Aldoni <tld/>ante:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li al<tld/>is novajn informojn
        <fnt><bib>MkM</bib>, <lok>P.293.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
    <trd lng="hu">odasz&oacute;r</trd>
    <trd lng="pl">dosypa&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxut.al0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Transmeti programon a&ubreve; datumon de periferia komputilo al
      &ccirc;efkomputilo a&ubreve; servilo, la malo de el<tld/>i.
    </dif>
    <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;
<klr>(&c_f;&c_a;&c_j;&c_l;&c_y;,
&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
    <trd lng="de">hochladen</trd>
    <trd lng="en">upload<klr> (data)</klr></trd>
    <trd lng="hu">felt&ouml;lt <klr> (adatokat)</klr></trd>
    <trdgrp lng="nl">
      <trd>uploaden</trd>,
      <trd>verzenden<klr tip="ind"> (v.data)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wgra&cacute;<klr> (dane)</klr></trd>,
      <trd>za&lstroke;adowa&cacute;<klr> (dane)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;
<klr>(&c_d;&c_a;&c_n;&c_n;&c_y;&c_je; &c_v;
&c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;)</klr></trd>,
      <trd>&c_v;&c_y;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; <klr>(&c_n;&c_a;
&c_s;&c_je;&c_r;&c_v;&c_je;&c_r;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="sxut.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <fnt><bib>PIV2</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;igi <tld/>ante, 
      &ccirc;irka&ubreve;igi per abundo da.
    </dif>
  </snc>
</drv>

<drv mrk="sxut.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxut.el0i.rektasence">
    <dif>
      Eligi <tld/>ante:
      <ekz>
        el<tld/>i sablon el kamiono
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
      <trd lng="be">&c_v;&c_y;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
      <trd lng="hu">kisz&oacute;r</trd>
      <trd lng="nl">uitschudden</trd>
      <trd lng="ru">&c_v;&c_y;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
    </dif>
  </snc>
  <snc mrk="sxut.el0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Transmeti programon a&ubreve; datumon el &ccirc;efmemoro a&ubreve;
servilo
      al krommemoro a&ubreve; periferia komputilo, la malo de al<tld/>i:
      <ekz>
        modernan Unikod-kapablan foliumilon vi povas
        senpage el<tld/>i de Microsoft a&ubreve; Netscape
        <fnt><lok><url
        ref="http://www.phon.ucl.ac.uk/home/wells/lek.htm">Londona
        Esperanto-Klubo</url></lok>.</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;
<klr>(&c_f;&c_a;&c_j;&c_l;&c_y;,
&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
    <trd lng="de">herunterladen</trd>
    <trd lng="en">download <klr>(data)</klr></trd>
    <trd lng="fr">t&eacute;l&eacute;charger</trd>
    <trd lng="hu">let&ouml;lt <klr> (adatokat)</klr></trd>
    <trdgrp lng="nl">
      <trd>sponzen<klr tip="ind"> (v.data)</klr></trd>,
      <trd>downloaden</trd>,
      <trd>binnenhalen<klr tip="ind"> (v.data)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zgra&cacute;<klr> (dane)</klr></trd>,
      <trd>zrzuca&cacute;<klr> (dane)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;
<klr>(&c_d;&c_a;&c_n;&c_n;&c_y;&c_je;)</klr></trd>
  </snc>
</drv>

<drv mrk="sxut.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tre abunde kovri per <tld/>ado:
      <ekz>
        super<tld/>i &ccirc;erkon per tero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">&uuml;bersch&uuml;tten</trd>
    <trd lng="fr">recouvrir</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre abunde doni:
      <ekz>
        super<tld/>i iun per donacoj, favoroj;
      </ekz>
      <ekz>
        la kajo estis super<tld/>ita de atendantoj;
      </ekz>
      <ekz>
        mi estas super<tld/>ita de laboro, per koresponda&jcirc;oj;
      </ekz>
      <ekz>
        la polico estas super<tld/>ita per plendoj<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dron.0i">droni</ref>,
      <ref cel="flu.0i">flui</ref>.
    </refgrp>
    <trd lng="de">&uuml;berh&auml;ufen</trd>
    <trd lng="fr">submerger</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>
  <trd lng="hu">elbor&iacute;t</trd>
  <trd lng="nl">overstelpen</trd>
  <trd lng="pl">zasypa&cacute;</trd>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
