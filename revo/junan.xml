<vortaro>
  <art mrk="$Id: junan.xml,v 1.6 2023/11/11 09:24:16 afido Exp $">
    <kap>
      <rad>Junan</rad>/o
    </kap>

<drv mrk="junan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="junan.0o.provinco">
    <dif>
      Sud&ccirc;ina <ref tip="super" cel="provin.0o.POL">provinco</ref>,
      oriente de Mjanmao kaj
      norde de Laoso kaj Vjetnamujo:
      <ekz>
        en la jaro 1938 <klr>[&#8230;]</klr> estis neniu buldozo dum
        la konstruado de la &scirc;oseo
        inter la provinco <tld/>o, sud-okcidenta &Ccirc;inio, kaj Birmo
        <fnt>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
              <bib>Monato</bib>
          <lok>2011/05, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la azia elefanto <klr>[&#8230;]</klr> trovi&gcirc;as en Suda Azio,
        Sudorienta Azio inkluzive
        partoj de la Malaja Insularo, kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48909">Azia elefanto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ju;&c_n;&c_mol;&c_n;&c_a;&c_n;&c_mol;</trd>
    <trd lng="de">Yunnan</trd>
    <trd lng="en">Yunnan</trd>
    <trd lng="fr">Yunnan</trd>
    <trd lng="nl">Yunnan</trd>
    <trdgrp lng="zh">
      <trd>&#x96F2;&#21335;</trd>,
      <trd>&#x4E91;&#21335;</trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x30E6;&#x30F3;&#x30CA;&#x30F3;&#30465; 
  <pr>&#x30E6;&#x30F3;&#x30CA;&#x30F3;&#12375;&#12423;&#12358;</pr></trd>
</drv>

