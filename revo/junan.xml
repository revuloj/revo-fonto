<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: junan.xml,v 1.1 2023/07/15 18:30:11 afido Exp $">
    <kap>
      <rad>Junan</rad>/o
    </kap>

<drv mrk="junan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="junan.0o.provinco">
    <dif>
      Sud&ccirc;ina <ref tip="super" cel="provin.0o.POL">provinco</ref>,
      oriente de Mjanmao kaj
      norde de Laoso kaj Vjetnamujo:
      <ekz>
        en la jaro 1938 <klr>[&#8230;]</klr> estis neniu buldozo dum
        la konstruado de la &scirc;oseo
        inter la provinco <tld/>o, sud-okcidenta &Ccirc;inio, kaj Birmo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2011/010328.php"
              >Fragmentoj de la dua mondmilito</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la azia elefanto <klr>[&#8230;]</klr> trovi&gcirc;as en Suda Azio,
        Sudorienta Azio inkluzive
        partojn de la Malaja Insularo, kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48909"
              >Azia elefanto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Yunnan</trd>
    <trd lng="en">Yunnan</trd>
    <trd lng="fr">Yunnan</trd>
    <trdgrp lng="zh">
      <trd>&#38642;&#21335;</trd>,
      <trd>&#20113;&#21335;</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

