<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: matema.xml,v 1.47 2025/07/20 06:00:24 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>matematik</rad>/o
<fnt><bib>JED</bib></fnt></kap>

<drv mrk="matema.0o">
   <kap><tld/>o</kap>
   <fnt><bib>MatTerm</bib>, <lok>p. 1</lok></fnt>
   <snc mrk="matema.0o.SCI">
     <uzo tip="fak">MAT</uzo>
     <uzo tip="fak">SCI</uzo>
     <dif>
       <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref>,
       kiu per rezonado, studas la ecojn de abstraktaj objektoj
       (nombroj,
       geometriaj figuroj, strukturoj, ktp) kaj la rilatojn inter ili.
     </dif>
     <refgrp tip="sub">
       <ref cel="nombr.0o.MAT">nombro</ref>,
       <ref cel="operac.0o.MAT_supersenco">operacio</ref>,
       <ref cel="algebr.0o.fako">algebro</ref>,
       <ref cel="geomet1.0o.MAT">geometrio</ref>,
       <ref cel="funkci.0o.MAT.elemente">funkcio</ref>,
       <ref cel="strukt.0o.MAT">strukturo</ref>,
       <ref cel="ar.0teorio">aroteorio</ref>,
       <ref cel="topolo.0o.fako">topologio</ref>,
       <ref cel="analit.0o">analitiko</ref>,
       <ref cel="bild.0o.MAT">bildigo</ref>,
       <ref cel="statis.0o.matematika">statistiko</ref>,
       <ref cel="kalkul.probablo0o">probablokalkulo</ref>
     </refgrp>
     <adm>
       La &ccirc;e&hcirc;a traduko &scirc;ajnas al mi iom
       nefaka. &Ccirc;u &gcirc;i ta&ubreve;gas? <aut>MB</aut>
     </adm>
   </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="bo">&#3944;&#3908;&#3851;&#3938;&#4009;&#3954;&#3942;&#3851;</trd>
  <trd lng="br">matematik</trd>
  <trdgrp lng="ca">
     <trd>matem&agrave;tica</trd>,
     <trd>matem&agrave;tiques</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>matematika</trd>,
    <trd>po&ccaron;ty</trd>
  </trdgrp>
  <trd lng="cv">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
   <trd lng="de">Mathematik</trd>
  <trd lng="en">mathematics</trd>
  <trdgrp lng="es">
     <trd>matem&aacute;ticas</trd>,
     <trd>matem&aacute;tica</trd>
   </trdgrp>
   <trd lng="fa">&a_r;&f_y;&a_A;&a_D;&f_y;&a_A;&a_t;</trd>
  <trdgrp lng="fr">
     <trd>math&eacute;matique(s)</trd>,
     <trd>math</trd>
   </trdgrp>
  <trd lng="hu">matematika</trd>
  <trd lng="id">matematika</trd>
  <trd lng="io">matematiko</trd>
  <trd lng="ja">&#25968;&#23398;  <pr>&#12377;&#12358;&#12364;&#12367;</pr></trd>
  <trd lng="nl">wiskunde</trd>
  <trd lng="pl">matematyka</trd>
  <trd lng="pt">matem&aacute;tica</trd>
  <trd lng="ro">matematic&abreve;</trd>
   <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">matematika</trd>
  <trd lng="sv">matematik</trd>
  <trd lng="tr">matematik</trd>
  <trd lng="uk">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="vo">matemat</trd>
  <trdgrp lng="zh">
    <trd>&#25968;&#23398; <pr>sh&ugrave;xu&eacute;</pr></trd>,
    <trd>&#25976;&#23416; <pr>sh&ugrave;xu&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="matema.0isto">
   <kap><tld/>isto</kap>
   <snc mrk="matema.0isto.SCI">
     <dif>
       Fakulo a&ubreve; profesiulo pri <tld/>o:
       <ekz>
         mi konati&gcirc;is kun samlandano mia, Pierre
C&eacute;r&eacute;sole, in&gcirc;eniero kaj
         lerta <tld/>isto
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 45a, p.
         103a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_k;</trd>
  <trd lng="bg">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
  <trd lng="br">matematikour</trd>
  <trd lng="ca">matem&agrave;tic</trd>
  <trd lng="cs">matematik</trd>
  <trd lng="cv">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
   <trd lng="de">Mathematiker</trd>
  <trd lng="en">mathematician</trd>
  <trd lng="es">matem&aacute;tico</trd>
   <trd lng="fa">&a_r;&f_y;&a_A;&a_D;&f_y;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">math&eacute;maticien<klr> (subst.)</klr></trd>
  <trd lng="hu">matematikus</trd>
  <trd lng="id">matematikawan</trd>
  <trd lng="io">matematikisto</trd>
  <trd lng="ja">&#25968;&#23398;&#32773;  <pr>&#12377;&#12358;&#12364;&#12367;&#12375;&#12419;</pr></trd>
  <trdgrp lng="nl">
     <trd>mathematicus</trd>,
     <trd>wiskundige</trd>
   </trdgrp>
   <trd lng="pt">matem&aacute;tico</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">matematik</trd>
  <trd lng="uk">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
  <trd lng="vo">matematan</trd>
  <trdgrp lng="zh">
    <trd>&#25968;&#23398;&#23478; <pr>sh&ugrave;xu&eacute;ji&#257;</pr></trd>,
    <trd>&#25976;&#23416;&#23478; <pr>sh&ugrave;xu&eacute;ji&#257;</pr></trd>
  </trdgrp>
</drv>
</art>
