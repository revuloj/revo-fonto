<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flok.xml,v 1.24 2023/11/09 20:48:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flok</rad>/o
</kap>
<drv mrk="flok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flok.0o.tufeto">
    <dif>
      Malpeza <ref tip="super" cel="tuf.0o">tufeto</ref> da materio,
      &scirc;vebanta en aero:
      <ekz>
        <tld/>oj da ne&gcirc;o <klr>(<ref tip="sub" cel="negx.0ero">ne&gcirc;ero</ref>)</klr>
        flugadis &ccirc;irka&ubreve; li
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>(leontodaj semoj)</klr> flugas en formo de
        malgrandaj lanaj <tld/>oj super la vojo
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni surblovas ilin, la plumoj kaj la <tld/>oj disflugas kaj ne restas el ili pli
        ol el pasero
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fulgo<tld/>oj flugis tiel dense kiel la akridoj <klr>[&#x2026;]</klr> dum plago
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de hel-verda kaj ru&gcirc;a drapo kaj de kaduka tolo kelkloke pendis sur li
        <fnt>
          <bib>LOdE</bib>
          <aut>Aleksandr Pu&scirc;kin, trad. Mi&hcirc;ail Ma&hcirc;na&ccirc;</aut>
          <vrk>&Ccirc;erkisto</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bukl.0o.hara">buklo<sncref/></ref>,
      <ref cel="lanug.0o">lanugo</ref>,
      <ref cel="lan.0o">lano</ref>,
      <ref cel="sxpin.0i.TEKS">&scirc;pini<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Fluse</trd>,
      <trd>Fussel</trd>
    </trdgrp>
  </snc>
  <snc mrk="flok.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Plata sekigita prema&jcirc;o de grenero, 
      fritita tran&ccirc;a&jcirc;o de terpomo ks:
      <ekz>
        aven<tld/>oj
        <fnt>
          <aut>dorotka</aut>
          <vrk><url ref="https://apetito.ikso.net/recepto/pano-aux-pano-kiu-sxangxas-vivon">
          Pano (a&ubreve; "Pano kiu &scirc;an&gcirc;as vivon")</url></vrk>
          <lok>Bonan Apetiton, 2015-02-25</lok>
        </fnt>;
      </ekz>
      <ekz>
        skatolo de <ind>maiz<tld/>oj</ind>
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>;
        <trd lng="be">&c_k;&c_o;&c_r;&c_n;&c_f;&c_l;&c_je;&c_k;&c_s;</trd>
        <trdgrp lng="bg">
          <trd>&c_c;&c_a;&c_r;&c_je;&c_v;&c_i;&c_ch;&c_n;&c_i; &c_p;&c_r;&c_malmol;&c_ch;&c_i;&c_c;&c_i;</trd>,
          <trd>&c_k;&c_o;&c_r;&c_n;&c_f;&c_l;&c_je;&c_j;&c_k;&c_s;</trd>
        </trdgrp>
        <trdgrp lng="ca">
          <trd>cereals</trd>,
          <trd>flocs de blat de moro</trd>
        </trdgrp>
        <trdgrp lng="cs">
          <trd>corn flakes</trd>,
          <trd>cornflakes</trd>,
          <trd>kuku&rcaron;i&ccaron;n&eacute; vlo&ccaron;ky</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Cornflakes</trd>,
          <trd>Maisflocken</trd>
        </trdgrp>
        <trd lng="en">corn flakes</trd>
        <trd lng="es">copos de ma&iacute;z</trd>
        <trd lng="fa">&f_k;&a_w;&a_r;&a_n;&zwnj;&a_f;&a_l;&f_k;&a_s;</trd>
        <trdgrp lng="fr">
          <trd>cornflakes</trd>,
          <trd>flocons de ma&iuml;s</trd>
        </trdgrp>
        <trd lng="hu">kukoricapehely</trd>
        <trd lng="id"><ind>sereal</ind> jagung</trd>
        <trd lng="nl">cornflakes</trd>
        <trd lng="pl">p&lstroke;atki <ind>kukurydziane</ind></trd>
        <trd lng="pt">flocos de <ind>milho</ind></trd>
        <trd lng="sk">kukuri&ccaron;n&eacute; vlo&ccaron;ky</trd>
      </ekz>
      <ekz>
        saketoj enhavantaj <klr>[&#x2026;]</klr> nuksojn a&ubreve; terpom<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>John Gobourne</aut>
          <vrk><url ref="&Monato;/2012/010556.php">Tiam ... kaj nun</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bran.0o">brano</ref>,
      <ref cel="cxips.0o">&ccirc;ipso</ref>,
      <ref cel="gri.0o">grio</ref>,
      <ref cel="maiz.krev0o">krevmaizo, pufmaizo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_m;&c_u;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_m;&c_y;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;</trd>,
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">floc</trd>
  <trd lng="de">Flocke</trd>
  <trdgrp lng="en">
    <trd>flake</trd>,
    <trd>flock</trd>,
    <trd>wisp</trd>
  </trdgrp>
  <trd lng="es">copo<klr> (de nieve, ma&iacute;z, algod&oacute;n, ...)</klr></trd>
  <trd lng="fr">flocon</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x623F;  <pr>&#x3072;&#x3068;&#x3075;&#x3055;</pr></trd>,
    <trd>&#x5C0F;&#x7247;  <pr>&#x3057;&#x3087;&#x3046;&#x3078;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">vlok</trd>
  <trdgrp lng="pt">
    <trd>floco</trd>,
    <trd>froco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_l;&c_o;&c_s;&c_k;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;perie</trd>,
    <trd>vlo&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chm&yacute;&rcaron;&iacute;</trd>,
    <trd>p&aacute;pe&rcaron;&iacute;</trd>,
    <trd>vlo&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x436;&#x43C;&#x443;&#x442;&#x438;&#x43A;</trd>,
    <trd>&#x43A;&#x43B;&#x430;&#x43F;&#x442;&#x438;&#x43A;</trd>,
    <trd>&#x43F;&#x43B;&#x430;&#x442;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&#x43B;&#x430;&#x441;&#x442;&#x456;&#x432;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x43F;&#x443;&#x448;&#x438;&#x43D;&#x43A;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9C57;&#x7247; <pr>l&#xED;npi&#xE0;n</pr></trd>,
    <trd>&#x9C57; <pr>l&#xED;n</pr></trd>
  </trdgrp>
</drv>


