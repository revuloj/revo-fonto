<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flok</rad>/o
</kap>
<drv mrk="flok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Peceto, tufeto da maldensa kaj malpeza materio,
      kiu balanci&gcirc;as en aero, malrapide falante:
      <ekz>
        <ind><tld/>oj</ind><trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_mol;&c_ja;</trd>
        de lano<fnt>B</fnt>,
        ne&gcirc;o<fnt>Z</fnt>,
        fulgo, vato;
      </ekz>
      <ekz><tld/>aj nuboj.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="er1.0o">ero</ref>,
      <ref cel="gut.0o">guto</ref>.
    </refgrp>
  </snc>
  <ref cel="ter.0pomfloko">terpomfloko</ref>
  <trdgrp lng="be">
    <trd>&c_zh;&c_m;&c_u;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_m;&c_y;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;</trd>,
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">floc</trd>
  <trd lng="de">Flocke</trd>
  <trdgrp lng="en">
    <trd>flake</trd>,
    <trd>flock</trd>,
    <trd>wisp</trd>
  </trdgrp>
  <trd lng="es">copo<klr> (de nieve, ma&iacute;z, algod&oacute;n, ...)</klr></trd>
  <trd lng="fr">flocon</trd>
  <trd lng="nl">vlok</trd>
  <trdgrp lng="pt">
    <trd>floco</trd>,
    <trd>froco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_l;&c_o;&c_s;&c_k;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;perie</trd>,
    <trd>vlo&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chm&yacute;&rcaron;&iacute;</trd>,
    <trd>p&aacute;pe&rcaron;&iacute;</trd>,
    <trd>vlo&ccaron;ka</trd>
  </trdgrp>
</drv>
</art>
