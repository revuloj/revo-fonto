<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>cement</rad>/o
</kap>
<drv mrk="cement.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Speciala mortero el pulvoro de argilaj kaj kalkaj &scirc;tonoj, kiu
      seki&gcirc;ante fari&gcirc;as tre malmola kaj nepenetrebla de la akvo:
      <ekz>
        <ind>Portlanda <tld/>o</ind>;
        <trd lng="be">&c_p;&c_o;&c_r;&c_t;&c_l;&c_ja;&c_n;&c_d;-&c_c;&c_e;&c_m;&c_e;&c_n;&c_t;</trd>
        <trd lng="hu">portlandi</trd>
        <trd lng="pl">cement</trd>
        <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_l;&c_a;&c_n;&c_d;-&c_c;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lingvo ne povas esti simpla kolekto da vortoj, sed devas havi
        <tld/>on kaj vivon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="beton.0o">betono</ref>.
    </refgrp>
    <trd lng="be">&c_c;&c_e;&c_m;&c_e;&c_n;&c_t;</trd>
    <trd lng="de">Zement</trd>
    <trd lng="fr">ciment</trd>
    <trd lng="hu">cement</trd>
    <trd lng="pl">cement</trd>
    <trd lng="pt">cimento</trd>
    <trd lng="ru">&c_c;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
    <trd lng="sv">cement</trd>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Pulvoro el ligna karbo, kiun oni aldonas al la fandata fero por &gcirc;in
      &scirc;taligi.
    </dif>
    <trd lng="be">&c_c;&c_e;&c_m;&c_e;&c_n;&c_t;</trd>
    <trd lng="fr">c&eacute;ment</trd>
    <trd lng="hu">cement&aacute;l&oacute;por</trd>
    <trd lng="pl">cement</trd>
    <trd lng="pt">cemento</trd>
    <trd lng="ru">&c_c;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Ekstera tavolo de la dentradikoj.
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_a;&c_ja; <ind>&c_e;&c_m;&c_a;&c_l;&c_mol;</ind></trd>
    <trd lng="hu">fogcement</trd>
    <trd lng="pl">szkliwo</trd>
    <trd lng="pt">cemento</trd>
    <trd lng="ru">&c_e;&c_m;&c_a;&c_l;&c_mol; <klr tip="amb">(&c_z;&c_u;&c_b;&c_n;&c_a;&c_ja;)</klr></trd>
  </snc>
  <adm>
    PIV1 transportis la ANA kaj TEK sencon al paronimo "&ccirc;emento".
    <aut>MB</aut>
  </adm>
  <trd lng="fr">c&eacute;ment</trd>
  <trd lng="sk">cement</trd>
</drv>

<drv mrk="cement.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Firme kunigi per <tld/>o.
    </dif>
    <trd lng="de">zementieren</trd>
    <trd lng="fr">cimenter</trd>
    <trd lng="hu">cementez</trd>
    <trd lng="pl">cementowa&cacute;</trd>
    <trd lng="pt">cimentar</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Firme kunigi, firmigi:
      <ekz>
        forta solidareco <tld/>is nian grupon.
      </ekz>
    </dif>
    <trd lng="de">zementieren</trd>
    <trd lng="fr">cimenter</trd>
    <trd lng="hu">&ouml;sszekov&aacute;csol</trd>
    <trd lng="pl">cementowa&cacute;</trd>
    <trd lng="pt">cimentar</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Modifi la konsiston de metalo, plej ofte de fero, intermetante &gcirc;in
      kun karboligna pulvoro en mufloj kaj lasante &gcirc;in sub la efiko de tre
      alta temperaturo.
    </dif>
    <trdgrp lng="en">
      <trd>case-harden</trd>,
      <trd>carburize</trd>
    </trdgrp>
    <trd lng="fr">c&eacute;menter</trd>
    <trdgrp lng="hu">
      <trd>cement&aacute;l</trd>,
      <trd>felszen&iacute;t</trd>
    </trdgrp>
    <trd lng="pt">cementar</trd>
    <trd lng="de">h&auml;rten</trd>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_m;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_c;&c_je;&c_m;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
