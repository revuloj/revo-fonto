<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kecal.xml,v 1.15 2023/12/03 09:58:48 revo Exp $">
<kap>
  <rad>kecal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kecal.0o">
  <kap><tld/>o</kap>
  <snc mrk="kecal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdospecio (<trd lng="la">Pharomachrus mocinno</trd>) el la
      ordo trogonoformaj, vivanta en Centra Ameriko, kun ru&gcirc;a brusto,
      aliloke brila verda plumaro, kaj en la virseksulo longa vosto;
      sankta
      birdo de la aztekoj, nacia birdo de Gvatemalo:
      <ekz>
        <tld/>o estis konsiderata dieca, asociata al la &leftquot;serpenta dio&rightquot;,
        Kecalkoatlo de anta&ubreve;kolumbaj centramerikaj civilizoj, &gcirc;iaj
        brilegaj
        verdaj vostoplumoj, simbolo de printempa plantorekresko, estis
        adorataj 
        de la antikvaj Majaoj kaj Aztekoj, kiuj rigardis la kecalon kiel
        dio
        de la aero kaj simbolo de dieco kaj lumo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=129893">Kecalo</url></lok>
        </fnt>.
      </ekz>  
    </dif>
    <bld lok="&WCU;/thumb/0/03/Pharomachrus_mocinno_-male.jpg/512px-Pharomachrus_mocinno_-male.jpg" prm="PD"><tld/>o (Pharomachrus mocinno)
    
        <fnt>
          <aut>Peter F&ouml;rster, propra verko</aut>
          <lok><url ref="&WCW;/File:Pharomachrus_mocinno_-male.jpg">&WCW;/File:Pharomachrus_mocinno_-male.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_k;&c_v;&c_e;&c_z;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_je;&c_c;&c_a;&c_l;&c_mol; <klr tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="kecal.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON" lst="voko:monunuoj">Monunuo</ref> de Gvatemalo.
    </dif>
    <trd lng="be">&c_k;&c_je;&c_c;&c_a;&c_l;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_l;&c_ju;&c_t;&c_a;)</klr></trd>
   </snc>
  <trdgrp lng="cs">
    <trd>kvesal</trd>,
    <trd>quetzal</trd>
  </trdgrp>
  <trd lng="de">Quetzal</trd>
  <trd lng="en">quetzal</trd>
  <trd lng="es">quetzal</trd>
  <trd lng="fr">quetzal</trd>
  <trd lng="hu">quetzal</trd>
  <trd lng="id">quetzal</trd>
  <trd lng="nl">quetzal</trd>
  <trd lng="pl">kwezal herbowy</trd>
  <trd lng="pt">quetzal</trd>
  <trd lng="ro">quetzal resplendent</trd>
  <trdgrp lng="sk">
    <trd>guetzal</trd>,
    <trd>kecal</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&c_v;&c_je;&c_z;&c_a;&#x43B;&#x44C;</trd>
  <trd lng="zh">&#x51E4;&#x5C3E;&#x7EFF;&#x54AC;&#x9E43; <pr>f&#xE8;ngy&#x1D0;l&#xF9;:y&#x1CE;oju&#x101;n</pr></trd>
</drv>

</art>
