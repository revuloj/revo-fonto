<art mrk="$Id: stabil.xml,v 1.30 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>9</ofc><rad>stabil</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stabil.0a">
  <kap><ofc>9</ofc><tld/>a</kap>
  <snc mrk="stabil.0a.KOMUNE">
    <dif>
      Ema firme restadi en sama loko, pozicio, stato:
      <ekz>
        al la nova Germanio ni dankas 50jaran <tld/>an pacon &ndash; la
        plej longan senmilitan periodon en tiu &ccirc;i parto de
        E&ubreve;ropo
        <fnt>
        <aut>S. Maul</aut>
        <vrk>Germanio-Auschwitz/Malbonaj konsciencoj</vrk>
        <bib>Monato</bib>
        <lok>1995:3, p. 5a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_ta;&t_na;&t_tsheg;&t_ga;&t_ta;&t_na;&t_tsheg;</trd>
  <trdgrp lng="de">
      <trd>fest</trd>,
      <trd>stabil</trd>
  </trdgrp>
  <trd lng="en">stable</trd>
  <trd lng="es">estable</trd>
  <trdgrp lng="fr">
    <trd>stable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szil&aacute;rd</trd>,
    <trd>stabil</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5B89;&#x5B9A;&#12375;&#x305F;  <pr>&#12354;&#12435;&#12390;&#12356;&#12375;&#x305F;</pr></trd>,
    <trd>&#x6C38;&#x7D9A;&#12377;&#x308B;  <pr>&#12360;&#12356;&#x305E;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5B89;&#x5B9A;&#x306A;  <pr>&#12354;&#12435;&#12390;&#12356;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stevig</trd>,
    <trd>stabiel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sta&lstroke;y</trd>,
    <trd>stateczny</trd>
  </trdgrp>
  <trd lng="pt">est&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_b;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_ib;&c_j;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="stabil.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="stabil.0eco.KOMUNE">
    <dif>
      Situacio sen rompo de ekvilibro, sen risko de renversi&gcirc;o:
      <ekz>
        estas konstatite, ke DNA estas kemie multe pli <tld/>a ol RNA,
        kaj <tld/>eco estas tre grava por konservi la recepton de vivantaj
        organismoj dum milionoj da jaroj
        <fnt>
        <aut>S. Norvell</aut>
        <vrk>Riboj</vrk>
        <bib>Monato</bib>
        <lok>2000:11, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>stabilita</trd>,
    <trd>stabilnost</trd>,
    <trd>st&aacute;lost</trd>
  </trdgrp>
  <trd lng="de">Stabilit&auml;t</trd>
  <trd lng="en">stability</trd>
  <trd lng="es">estabilidad</trd>
  <trdgrp lng="fr">
    <trd>stabilit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szil&aacute;rds&aacute;g</trd>,
    <trd>stabilit&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x5B89;&#x5B9A;&#24615;  <pr>&#12354;&#12435;&#12390;&#12356;&#x305B;&#12356;</pr></trd>
  <trdgrp lng="nl">
    <trd>stevigheid</trd>
    <trd>stabiliteit</trd>
  </trdgrp>
  <trd lng="pl">stabilno&sacute;&cacute;</trd>
  <trd lng="pt">estabilidade</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pevnos&tcaron;</trd>,
    <trd>stabilita</trd>,
    <trd>stabilnos&tcaron;</trd>,
    <trd>st&aacute;los&tcaron;</trd>,
    <trd>ust&aacute;lenos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_b;&c_ib;&#x43B;&#x44C;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_s;&c_t;&c_ib;&c_j;&#x43A;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="stabil.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stabil.0igi.KOMUNE">
    <dif>
      Firmigi, provizi per apogoj a&ubreve; helpoj kontra&ubreve;
      renversi&gcirc;oj:
      <ekz>
        evidenti&gcirc;is, ke la internaciaj organiza&jcirc;oj kreitaj
        por <tld/>igi la mondan ekonomion fakte estas senpovaj kontra&ubreve;
        spekulado
        <fnt>
          <aut>R. Rotsaert</aut>
          <vrk>Attac - Atakata de sia idolo</vrk>,
          <bib>Monato</bib>
          <lok>2002:1, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekvili.0igi">ekvilibrigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>stabilizovat</trd>,
    <trd>ust&aacute;lit</trd>
  </trdgrp>
  <trd lng="de">stabilisieren</trd>
  <trdgrp lng="en">
    <trd>stabilize</trd>,
    <trd>stabilise</trd>
  </trdgrp>
  <trd lng="es">estabilizar</trd>
  <trdgrp lng="fr">
    <trd>affermir</trd>,
    <trd>asseoir<klr> (stabiliser)</klr></trd>,
    <trd>&eacute;quilibrer</trd>,
    <trd>stabiliser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megszil&aacute;rd&iacute;t</trd>,
    <trd>stabiliz&aacute;l</trd>
  </trdgrp>
  <trd lng="ja">&#x5B89;&#x5B9A;&#12373;&#x305B;&#x308B;  <pr>&#12354;&#12435;&#12390;&#12356;&#12373;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">stabiliseren</trd>
  <trdgrp lng="pl">
    <trd>stabilizowa&cacute;</trd>,
    <trd>utrwala&cacute;</trd>
  </trdgrp>
  <trd lng="pt">estabilizado</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_b;&c_i;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>stabilizova&tcaron;</trd>,
    <trd>upev&ncaron;ova&tcaron;</trd>,
    <trd>usta&lcaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_b;&c_ib;&#x43B;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&#x43B;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="stabil.0igilo">
  <kap>
    <tld/>igilo
  </kap>
  <snc mrk="stabil.0igilo.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Rimedo, ordinare vertikale a&ubreve; oblikve enakvigita plato, kiu <tld/>igas &scirc;ipon
      kontra&ubreve; rulado:
      <ekz>
        la &scirc;ipo <nom>Patris</nom> ne havis <tld/>igilojn,
        kaj la veturo inter Melburno kaj Perto
        estis tre malglata
        <fnt>
          <aut>T. Steele</aut>
          <vrk>Konvinka Kamufla&jcirc;o</vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nagx.0ilo">na&gcirc;ilo</ref>,
      <ref cel="kil.0o">kilo</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;rive</trd>,
      <trd>stabilisateur</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Stabilisator</trd>
  <trdgrp lng="ja">
    <trd>&#x5B89;&#x5B9A;&#x88C5;&#x7F6E;  <pr>&#12354;&#12435;&#12390;&#12356;&#x305D;&#12358;&#12385;</pr></trd>,
    <trd>&#x5B89;&#x5B9A;&#21092;  <pr>&#12354;&#12435;&#12390;&#12356;&#12374;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>statecznik</trd>,
    <trd>stabilizator</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_b;&c_ib;&#x43B;&c_ib;&c_z;&c_a;&c_t;&#x43E;&c_r;</trd>
</drv>

<drv mrk="stabil.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="stabil.mal0a.KOMUNE">
    <dif>
      Facile renversi&gcirc;ema, fluktuanta, eble difekti&gcirc;ema pro propraj
      a&ubreve; eksteraj premoj:
      <ekz>
        merkata ekonomio la&ubreve; <nom>Keynes</nom> esence estas
        mal<tld/>a
        <fnt>
        <aut>Bo Sandelin</aut>
        <vrk>Makroekonomiko en la 20a jarcento</vrk>
        <bib>Monato</bib>
        <lok>2001:9, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>instabiln&iacute;</trd>,
    <trd>nest&aacute;l&yacute;</trd>,
    <trd>vratk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>wackelig</trd>,
      <trd>unstabil</trd>,
      <trd>instabil</trd>
  </trdgrp>
  <trd lng="en">unstable</trd>
  <trd lng="es">inestable</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;s&eacute;quilibr&eacute;</trd>,
    <trd>instable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ingatag</trd>,
    <trd>instabil</trd>,
    <trd>labilis</trd>
  </trdgrp>
  <trd lng="ja">&#x4E0D;&#x5B89;&#x5B9A;&#x306A;  <pr>&#12405;&#12354;&#12435;&#12390;&#12356;&#x306A;</pr></trd>
  <trdgrp lng="nl">
    <trd>onstevig</trd>
    <trd>instabiel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niestabilny</trd>,
    <trd>chwiejny</trd>
  </trdgrp>
  <trd lng="pt">inst&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_t;&c_a;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>instabiln&yacute; nest&aacute;ly</trd>,
    <trd>vratk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_s;&c_t;&c_a;&c_b;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43B;&c_a;&c_b;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_i;&c_t;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43C;&c_ib;&#x43D;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="stabil.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc mrk="stabil.mal0eco.KOMUNE">
    <dif>
      Situacio en kiu diversaj malegalaj fortoj ka&ubreve;zas falojn,
      neregatajn &scirc;an&gcirc;ojn:
      <ekz>
        li kolektis multajn informojn en Ruando, &gcirc;uste en la
        periodo de
        granda politika malstabileco post la invado de la ribeluloj
        (1990)
        <fnt>
        <aut>W. M. A. De Smet</aut>
        <vrk>Kiu murdis Dian Fossey?</vrk>,
        <bib>Monato</bib>
        <lok>1995:12, p. 23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>labilnost</trd>,
    <trd>nestabilita</trd>,
    <trd>nest&aacute;lost</trd>,
    <trd>vratkost</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Instabilit&auml;t</trd>
  </trdgrp>
  <trd lng="en">instability</trd>
  <trd lng="es">inestabilidad</trd>
  <trdgrp lng="fr">
    <trd>instabilit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ingatags&aacute;g</trd>,
    <trd>instabilit&aacute;s</trd>,
    <trd>labilit&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x4E0D;&#x5B89;&#x5B9A;&#24615;  <pr>&#12405;&#12354;&#12435;&#12390;&#12356;&#x305B;&#12356;</pr></trd>
  <trdgrp lng="nl">
    <trd>onstevigheid</trd>
    <trd>instabiliteit</trd>
  </trdgrp>
  <trd lng="pl">niestabilno&sacute;&cacute;</trd>
  <trd lng="pt">instabilidade</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_t;&c_a;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>labilnos&tcaron;</trd>,
    <trd>nest&aacute;los&tcaron;</trd>,
    <trd>vratkos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="stabil.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stabil.mal0igi.KOMUNE">
    <dif>
      &Gcirc;eni la egalpezon, ka&ubreve;zante ekfalon:
      <ekz>
        ekmiliti kontra&ubreve; Kabila por mal<tld/>igi kaj faligi la
        re&gcirc;imon
        <fnt>
        <aut>Zandandu Ntomono Zola</aut>
        <vrk>Soldatoj - sed neniaj solvoj</vrk>
        <bib>Monato</bib>
        <lok>2000:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekvili.mal0igi">malekvilibrigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_mol;
    <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
  </trdgrp>
  <trd lng="cs">destabilizovat</trd>
  <trdgrp lng="de">
      <trd>zum <ind>Kippen</ind> bringen</trd>,
      <trd>destabilisieren</trd>,
      <trd>instabil machen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>destabilize</trd>
    <trd>destabilise</trd>
  </trdgrp>
  <trd lng="es">desestabilizar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;s&eacute;quilibrer</trd>,
    <trd>d&eacute;stabiliser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megingat</trd>,
    <trd>instabiliz&aacute;l</trd>,
    <trd>labiliss&aacute; tesz</trd>
  </trdgrp>
  <trd lng="ja">&#x4E0D;&#x5B89;&#x5B9A;&#x306B;&#12377;&#x308B;  <pr>&#12405;&#12354;&#12435;&#12390;&#12356;&#x306B;&#12377;&#x308B;</pr></trd>
  <trd lng="nl">destabiliseren</trd>
  <trd lng="pl">destabilizowa&cacute;</trd>
  <trd lng="pt">desestabilizar</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_t;&c_a;&c_b;&c_i;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">destabilizova&tcaron;</trd>
</drv>
</art>
