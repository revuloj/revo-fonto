<drv mrk="stabil.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stabil.0igi.KOMUNE">
    <dif>
      Firmigi, provizi per apogoj a&ubreve; helpoj kontra&ubreve;
      renversi&gcirc;oj:
      <ekz>
        <nom>Mozart</nom> <tld/>igis komprenon de simfonio, kiel orkestran &gcirc;enron de muziko
        <fnt>
          <bib>KFl</bib>
          <lok>III. Atomoj de evoluo</lok>
        </fnt>;
      </ekz>
      <ekz>
        por ke la pagodoj eltenu la tertremojn, oni pendigas supre arbotrunkon, kiu
        <tld/>igas la konstrua&jcirc;on dum la tertremoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        evidenti&gcirc;is, ke la internaciaj organiza&jcirc;oj kreitaj
        por <tld/>igi la mondan ekonomion fakte estas senpovaj kontra&ubreve;
        spekulado
        <fnt>
          <aut>R. Rotsaert</aut>
          <vrk>Attac - Atakata de sia idolo</vrk>,
          <bib>Monato</bib>
          <lok>2002:1, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekvili.0igi">ekvilibrigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
