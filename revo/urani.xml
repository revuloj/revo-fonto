<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>urani</rad>/o
</kap>

<drv mrk="urani.0o">
<kap><tld/>o</kap>
<mlg>U</mlg>
<uzo tip="fak">KEM</uzo>
<snc>
<dif>
&Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero 92, atompezo 238,0289, simbolo U. &Ccirc;iuj izotopoj de tiu
elemento estas radioaktivaj.
<ref tip="super" cel="metal.0o">Metalo</ref>.
Preferata baza materialo por atombomboj kaj atomcentraloj. La&ubreve; la
atomnumero lasta elemento, kiu trovi&gcirc;as en la naturo. La elementoj
kun pli alta atomnumero estas artefaritaj elementoj kaj nomi&gcirc;as
transuraniaj elementoj a&ubreve; transuranioj.
</dif>
</snc>
<trd lng="be">&c_u;&c_r;&c_a;&c_n; 
<klr>(&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y; 
&c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
<trd lng="br">uraniom</trd>
<trd lng="de">Uran</trd>
<trd lng="el">&omikron;&ypsilon;&rho;&alfa_ton;&ny;&jota;&omikron;</trd>
<trd lng="en">uranium</trd>
<trd lng="es">uranio</trd>
<trd lng="fr">uranium</trd>
<trdgrp lng="hu">
<trd>ur&aacute;nium</trd>,
<trd>ur&aacute;n</trd>
</trdgrp>
<trd lng="it">uranio</trd>
<trdgrp lng="nl">
<trd>uraan</trd>,
<trd>uranium</trd>
</trdgrp>
<trd lng="pt">ur&acirc;nio</trd>
<trd lng="ru">&c_u;&c_r;&c_a;&c_n;</trd>
<trd lng="sk">ur&aacute;n</trd>
</drv>

<drv mrk="urani.trans0a">
  <kap>trans<tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
Rilate al &hcirc;emia <ref tip="super" 
cel="elemen.0o.KEM">elemento</ref> kun
atomnumero pli alta ol 92. &Ccirc;iuj tiaj elementoj estas artefaritaj,
t.e. ili ne trovi&gcirc;as en la naturo.
    </dif>
  </snc>
  <trd lng="en">transuranic</trd>
  <trd lng="es">transur&aacute;nico</trd>
</drv>

<drv mrk="urani.trans0o">
<kap>trans<tld/>o</kap>
<uzo tip="fak">KEM</uzo>
<snc>
<dif>
&Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero pli alta ol 92. &Ccirc;iuj tiaj elementoj estas artefaritaj,
t.e. ili ne trovi&gcirc;as en la naturo.
</dif>
</snc>
<trd 
lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_u;&c_r;&c_a;&c_n;&c_a;&c_v;&c_y; 
&c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;</trd>
<trd lng="de">Transuran</trd>
<trd lng="en"><ind>transuranic</ind> (element)</trd>
<trd lng="fr">transuranien</trd>
<trd lng="hu">transzur&aacute;n</trd>
<trd lng="nl">transuraan</trd>
<trdgrp lng="pt">
<trd>transur&acirc;nico</trd>,
<trd>transuraniano</trd>
</trdgrp>
<trd
lng="ru">&c_t;&c_r;&c_a;&c_n;&c_s;&c_u;&c_r;&c_a;&c_n;&c_o;&c_v;&c_y;&c_j;
&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

</art>
