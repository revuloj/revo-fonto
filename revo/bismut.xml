<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bismut</rad>/o
</kap>
<mlg>Bi</mlg>
<drv mrk="bismut.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">KEM</uzo>
<snc>
<dif>
&Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero 83, atompezo 208,980, simbolo <frm>Bi</frm>. Flavblanka 
<ref tip="super" cel="metal.0o">metalo</ref> uzata en medicino. 
</dif>
</snc>
<trd lng="be">&c_v;&c_ib;&c_s;&c_m;&c_u;&c_t;</trd>
<trd lng="ca">bismut</trd>
<trd lng="br">bismut</trd>
<trdgrp lng="de">
  <trd>Bismut</trd>,
  <trd>Wismut</trd>
</trdgrp>
<trd lng="en">bismuth</trd>
<trd lng="es">bismuto</trd>
<trd lng="fr">bismuth</trd>
<trd lng="hu">bizmut</trd>
<trd lng="it">bismuto</trd>
<trd lng="la">Bismutum</trd>
<trd lng="nl">bismut</trd>
<trd lng="pl">bizmut</trd>
<trd lng="pt">bismuto</trd>
<trd lng="ru">&c_v;&c_i;&c_s;&c_m;&c_u;&c_t;</trd>
<trd lng="sk">bizmut</trd>
</drv>
</art>
