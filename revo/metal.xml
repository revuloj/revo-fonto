<drv mrk="metal.teralkala0o">
  <kap>
    teralkala <tld/>o
  </kap>
  <snc mrk="metal.teralkala0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Ccirc;iu elemento de la perioda tabelo el grupo 2 la&ubreve; la
IUPAC-sistemo, nome:
      <ref tip="sub" cel="berili.0o">berilio</ref>,
      <ref tip="sub" cel="magnez1.0o">magnezio</ref>,
      <ref tip="sub" cel="kalci.0o">kalcio</ref>,
      <ref tip="sub" cel="stronci.0o">stroncio</ref>,
      <ref tip="sub" cel="bari.0o">bario</ref>,
      <ref tip="sub" cel="radium.0o">radiumo</ref>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_zh;&c_n;&c_a;&c_z;&c_ja;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_y; <ind>&c_m;&c_e;&c_t;&c_a;&c_l;</ind></trd>
  <trd lng="fa">&a_f;&a_l;&a_z; &a_q;&a_l;&f_y;&a_A;&f_y;&f_y; &a_kh;&a_A;&f_k;&f_y;</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30AB;&#x30EA;&#x571F;&#x985E;&#x91D1;&#x5C5E;  <pr>&#x30A2;&#x30EB;&#x30AB;&#x30EA;&#x3069;&#x308B;&#x3044;&#x304D;&#x3093;&#x305E;&#x304F;</pr></trd>
  <trdgrp lng="pl">
    <trd>metal ziem alkalicznych</trd>,
    <trd>berylowiec</trd>,
    <trd>wapniowiec</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&c_u;&c_zh;&#x43D;&#x43E;&c_z;&c_je;&#x43C;&c_je;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &#x43C;&c_je;&c_t;&c_a;&#x43B;</trd>
</drv>
