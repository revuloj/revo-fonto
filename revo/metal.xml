<drv mrk="metal.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <tld/>aj krampoj, <tld/>a &scirc;ranko
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a signo <klr>[&#x2026;]</klr> sur la &ccirc;apo
        <fnt>
          <bib>Gmn</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">metallen</trd>
  </snc>
  <snc mrk="metal.0a.rilata">
    <dif>
      Rilata al <tld/>o, karakterizata de <tld/>o:
      <ekz>
        la <tld/>a bruo de sonorilo
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;as kolera <tld/>a frapado per man&gcirc;ilaro
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        se iel eble, oni parolu rekte, sen la&ubreve;tparoliloj, ili &ccirc;iam iel <tld/>igas la
        vo&ccirc;on
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">metallisch</trd>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="en">metallic</trd>
  <trdgrp lng="ja">
    <trd>&#x91D1;&#x5C5E;&#x306E;  <pr>&#x304D;&#x3093;&#x305E;&#x304F;&#x306E;</pr></trd>,
