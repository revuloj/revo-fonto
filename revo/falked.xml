<art mrk="$Id: falked.xml,v 1.16 2024/03/12 15:23:35 revo Exp $">
<kap><rad>falked</rad>/oj</kap>
<drv mrk="falked.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
      el la ordo <ref tip="malprt" cel="bird.rabo0oj">rabobirdoj</ref>
      (<trd lng="la">Falconidae</trd>):
      <ekz>
        falkedoj <klr>[&#8230;]</klr> inkludas la karakarojn kaj la mikrasturojn,
        <klr>[&#8230;]</klr> <ref tip="malprt" cel="falk.0o">falkojn</ref>, turfalkojn kaj falketojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16227">Falkedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_k;&c_a;&c_l;&c_ib;&c_n;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_o;&c_k;&c_o;&c_l;&c_i;</trd>
  <trd lng="ca">falc&ograve;nids</trd>
  <trd lng="cs">sokolovit&iacute;</trd>
  <trd lng="de">Falken</trd>
  <trdgrp lng="en">
    <trd>falcons</trd>,
    <trd>caracaras</trd>
  </trdgrp>
  <trd lng="fr">Falconid&eacute;s</trd>
  <trd lng="hu">s&oacute;lyomf&eacute;l&eacute;k</trd>
  <trd lng="nl">valken en caracara's<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pt">
    <trd>falcon&iacute;deos</trd>,
    <trd>falc&ocirc;nidas</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_k;&c_o;&c_l;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">sokolovit&eacute;</trd>
  <trd lng="uk">&c_s;&#x43E;&#x43A;&#x43E;&#x43B;&#x43E;&c_v;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x96BC;&#x5C5E; <pr>zh&#x1D4;nsh&#x1D4;</pr></trd>,
    <trd>&#x96BC;&#x79D1; <pr>zh&#x1D4;nk&emacron;</pr></trd>
  </trdgrp>
</drv>
</art>
