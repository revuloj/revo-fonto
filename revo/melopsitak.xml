<art mrk="$Id: melopsitak.xml,v 1.13 2024/12/04 08:37:54 revo Exp $">
<kap>
    <rad>melopsitak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="melopsitak.0o">
  <kap><tld/>o</kap>
  <snc mrk="melopsitak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      A&ubreve;stralia birdo,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="papage.0oj"><tld/>edoj</ref>
      (<trd lng="la">Melopsittacus</trd>):
      <ekz>
        kelkaj funkcioj rilate parolon &ccirc;e <tld/>oj kaj homoj funkcias simile
        <fnt>
          <aut>enretigita de Barbara Pietrzak</aut>
          <vrk><url ref="http://pola-retradio.org/2016/08/e_elsendo-el-la-30-08-2016/">la 577-a
          E-elsendo &ccirc;e www.pola-retradio.org el la 30.08.2016</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/f/f5/Melopsittacus_undulatus_-Fort_Worth_Zoo-8a-4c.jpg/480px-Melopsittacus_undulatus_-Fort_Worth_Zoo-8a-4c.jpg"
    prm="CC BY 2.0"><tld/>o (Melopsittacus undulatus), besto&gcirc;ardeno Fort Worth, Texas, Usono
        <fnt>
          <aut>Jerry Tillery</aut>
          <lok><url
          ref="&WCW;/File:Melopsittacus_undulatus_-Fort_Worth_Zoo-8a-4c.jpg">&WCW;/File:Melopsittacus_undulatus_-Fort_Worth_Zoo-8a-4c.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y; <ind>&c_p;&c_a;&c_p;&c_u;&c_g;&c_a;&c_j;</ind></trd>
  <trd lng="bg">&c_v;&c_malmol;&c_l;&c_n;&c_i;&c_s;&c_t;&c_o; &c_p;&c_a;&c_p;&c_a;&c_g;&c_a;&c_l;&c_ch;&c_je;</trd>
  <trd lng="de">Wellensittich</trd>
  <trd lng="en">budgerigar</trd>
  <trd lng="fr">perruche ondul&eacute;e</trd>
  <trdgrp lng="hu">
  <trd>hull&aacute;mos <ind>papag&aacute;j</ind></trd>,
  <trd>t&ouml;rpepapag&aacute;j</trd>
  </trdgrp>
  <trd lng="nl">grasparkiet</trd>
  <trd lng="pl">papu&zdot;ka falista</trd>
  <trd lng="ro">peru&#537;</trd>
  <trd lng="uk">&#x43F;&c_a;&#x43F;&c_u;&c_zh;&#x43E;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x864E;&#x76AE;&#x9E1A;&#x9D61; <pr>h&#x1D4;p&#xED;y&#x12B;ngw&#x1D4;</pr></trd>,
    <trd>&#x864E;&#x76AE;&#x9E66;&#x9E49; <pr>h&#x1D4;p&#xED;y&#x12B;ngw&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>

