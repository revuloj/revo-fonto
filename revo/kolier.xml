<kap>
  <rad>kolier</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kolier.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Valora korpornama&jcirc;o, portata &ccirc;irka&ubreve; la kolo, provizita
      per perloj, koraloj, juvel&scirc;tonoj ks:
      <ekz>
        mi &jcirc;us vidis reklamon pri a&ubreve;tentika &jcirc;urasparka sukceno &dash; <tld/>oj, orelringoj 
        <fnt>
          <aut>Marjorie Boulton</aut>
          <vrk><url ref="&Monato;/1994/A940201.php">Dinomanio</url></vrk>
          <bib>Monato</bib>
          <lok>1994/02, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nova perlo aldoni&gcirc;is al la <tld/>o de via repertuaro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6944">Bulgara Esperanto-Teatro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="cxen.kol0o">kol&ccirc;eno</ref>,
      <ref cel="kol.cxirkaux0o.ornamajxo">&ccirc;irka&ubreve;kolo</ref>      
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_je;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_i;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>collaret</trd>,
    <trd>enfilall</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hal&zcaron;e</trd>,
    <trd>koli&eacute;r</trd>,
    <trd>n&aacute;hrdeln&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Collier</trd>,
    <trd>Kollier</trd>
  </trdgrp>
  <trd lng="fr">collier<klr tip="ind"> (ornement)</klr></trd>
  <trdgrp lng="hu">
    <trd>nyak&eacute;k</trd>,
    <trd>nyakl&aacute;nc</trd></trdgrp>
  <trd lng="ja">&#x30CD;&#x30C3;&#x30AF;&#x30EC;&#x30B9;  <pr>&#x306D;&#x3063;&#x304F;&#x308C;&#x3059;</pr></trd>
  <trd lng="nl">halssnoer</trd>
  <trdgrp lng="pl">
    <trd>kolia</trd>,
    <trd>naszyjnik</trd>
  </trdgrp>   
  <trd lng="pt">colar</trd>
  <trd lng="ro">colier</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_je;</trd>
  <trd lng="sk">n&aacute;hrdeln&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43B;&#x44C;&c_jeu;</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_i;&c_s;&c_t;&#x43E;</trd>
  </trdgrp>
</drv>
</art>

