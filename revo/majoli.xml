<art mrk="$Id: majoli.xml,v 1.21 2024/01/16 09:22:23 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>majolik</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="majoli.0o">
  <kap><tld/>o</kap>
  <snc mrk="majoli.0o.CER">
    <dif>
      <ref tip="super" cel="fajenc.0o">Fajenco</ref> itala &ccirc;irka&ubreve; la 16a jc,
      kolore surpentrita kaj glazurita per stano, k.s.:
      <ekz>
        italoj inventis la <tld/>an teknikon en la 16a jarcento, kiu ebligas artistojn
        pentri rekte sur malseka argilo, kiu trovi&gcirc;as sur tavolo de blanka emajlo
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html"
              >Impresoj pri Portugalio</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tupforma ceramika tombo&scirc;tono de <nom>Kusam ibn Abbas</nom> havas kvar tavolojn,
        kovritajn per reliefaj <tld/>aj kaheloj kun tre ri&ccirc;a plant-ornamo
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php"
              >Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis leganta a&ubreve; pre&gcirc;anta &ccirc;e gracia
        <tld/>a petrollampo
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="azulehx.0o">azule&hcirc;o</ref>,
      <ref cel="emajl.0o.TEK">emajlo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_jo;&c_l;&c_ib;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_j;&c_o;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">maj&ograve;lica</trd>
  <trd lng="cs">majolika</trd>
  <trd lng="de">Majolika</trd>
  <trd lng="en">majolica</trd>
  <trd lng="es">may&oacute;lica</trd>
  <trd lng="fr">majolique</trd>
  <trd lng="hu">majolika</trd>
  <trdgrp lng="ja">
    <trd>&#12510;&#12472;&#12519;&#12522;&#12459;&#28988;  <pr>&#12510;&#12472;&#12519;&#12522;&#12459;&#12420;</pr></trd>,
    <trd>&#12510;&#12520;&#12522;&#12459;</trd>
  </trdgrp>
  <trd lng="nl">majolica</trd>
  <trd lng="pl">majolika</trd>
  <trdgrp lng="pt">
    <trd>mai&oacute;lica</trd>,
    <trd>maj&oacute;lica</trd>
  </trdgrp>
  <trd lng="ro">majolic&abreve;</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;&c_o;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">majolika</trd>
  <trd lng="sv">majolika</trd>
  <trd lng="uk">&c_m;&c_a;&c_j;&c_o;&c_l;&c_ib;&c_k;&c_a;</trd>
</drv>
</art>
