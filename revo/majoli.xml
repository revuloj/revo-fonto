  <trd lng="en">majolica</trd>
  <trd lng="es">may&oacute;lica</trd>
  <trd lng="fr">majolique</trd>
  <trd lng="hu">majolika</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30B8;&#x30E7;&#x30EA;&#x30AB;&#x713C;  [<ind>&#x30DE;&#x30B8;&#x30E7;&#x30EA;&#x30AB;&#x3084;</ind>]</trd>,
    <trd>&#x30DE;&#x30E8;&#x30EA;&#x30AB;</trd>
  </trdgrp>
  <trd lng="nl">majolica</trd>
  <trd lng="pl">majolika</trd>
  <trdgrp lng="pt">
    <trd>mai&oacute;lica</trd>,
    <trd>maj&oacute;lica</trd>
  </trdgrp>
  <trd lng="ro">majolic&abreve;</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;&c_o;&c_l;&c_i;&c_k;&c_a;</trd>
