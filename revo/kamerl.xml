<art mrk="$Id: kamerl.xml,v 1.17 2023/08/31 19:24:09 revo Exp $">
<kap>
  <rad>kamerling</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kamerl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref cel="kardin.0o.KAT">Kardinalo</ref>
      administranta la eklezion, dum vakas la Sankta Se&gcirc;o:
      <ekz>
        la kardinalo <tld/>o proklamas la morton de papo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36097">Konklavo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_l;&c_ib;&c_n;&c_g;</trd>
    <trd lng="ca">camarlenc</trd>
    <trd lng="de">K&auml;mmerling</trd>
    <trd lng="es">camarlengo</trd>
    <trd lng="fr">camerlingue</trd>
    <trd lng="hu">kamarling&oacute;</trd>
    <trd lng="it">camerlengo</trd>
    <trdgrp lng="nl">
      <trd>kamerling</trd>,
      <trd>camerlengo</trd>
    </trdgrp>
    <trd lng="pl">kamerling</trd>
  </snc>
  <trd lng="cs">pape&zcaron;sk&yacute; komo&rcaron;&iacute;</trd>
  <trd lng="ja">&#x30AB;&#x30E1;&#x30EB;&#x30EC;&#x30F3;&#x30B4;</trd>
  <trd lng="sk">p&aacute;pe&zcaron;sk&yacute; komorn&iacute;k</trd>
  <trd lng="uk">&#x43A;&#x430;&#x43C;&#x435;&#x440;&#x43B;&#x456;&#x43D;&#x433; (&#x43A;&#x443;&#x440;&#x456;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439; &#x43A;&#x430;&#x440;&#x434;&#x438;&#x43D;&#x430;&#x43B;)</trd>
</drv>
</art>
