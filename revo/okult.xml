<drv mrk="okult.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al misteraj fenomenoj, kiujn kelkaj homoj opinias
      supernaturaj, kaj kies majstradon provizas parte sekretaj artoj
      (magio, astrologio, elvokado de mortintoj...):
      <ekz>
        promeson pri <tld/>aj povoj (telepatio, klarvideco, klara&ubreve;deco,
        prekogno, a&ubreve;gurado), &scirc;i ne plenumas<fnt><aut>Christian
        Declerck</aut>: <vrk>A&ubreve;dace honesta romano (Eli Urbanov&aacute;,
        Hetajro
        dancas)</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 4a, p.
        24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>ocult</trd>,
    <trd>esot&egrave;ric</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>okultn&iacute;</trd>,
    <trd>skryt&yacute;</trd>
  </trdgrp>
  <trd lng="de">okkult</trd>
  <trd lng="en">occult</trd>
  <trdgrp lng="es">
    <trd>oculto</trd>,
    <trd>esot&eacute;rico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>occulte</trd>
  </trdgrp>
  <trd lng="hu">okkult</trd>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x79D8;&#x7684;&#x306A;  <pr>&#x3057;&#x3093;&#x3074;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x8D85;&#x81EA;&#x7136;&#x7684;&#x306A;  <pr>&#x3061;&#x3087;&#x3046;&#x3057;&#x305C;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x9B54;&#x8853;&#x7684;&#x306A;  <pr>&#x307E;&#x3058;&#x3085;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x30AA;&#x30AB;&#x30EB;&#x30C8;&#x306E;  <pr>&#x304A;&#x304B;&#x308B;&#x3068;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">occult</trd>
  <trdgrp lng="pl">
    <trd>nadzmys&lstroke;owy</trd>,
    <trd>okultystyczny</trd>
  </trdgrp>
  <trd lng="pt">oculto</trd>
  <trdgrp lng="ro">
    <trd>suprasensibil&abreve;</trd>,
    <trd>transcendental</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_k;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>okultn&yacute;</trd>,
    <trd>skryt&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43A;&#x443;&#x43B;&#x44C;&#x442;&#x43D;&#x438;&#x439;</trd>,
