<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>okult</rad>/a
</kap>
<drv mrk="okult.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al misteraj fenomenoj, kiujn kelkaj homoj kredas
      supernaturaj, kaj kies le&gcirc;ojn esploras kelkaj &scirc;ajnaj sciencoj
      (magio, astrologio, elvokado de mortintoj ktp).
    </dif>
  </snc>
  <trd lng="de">okkult</trd>
  <trd lng="nl">occult</trd>
  <trd lng="ru">&c_o;&c_k;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="okult.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Doktrina esplorado pri <tld/>aj fenomenoj
    </dif>
    <refgrp tip="sin">
      <ref cel="spirit1.0o">spiritismo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Okkultismus</trd>
  <trd lng="nl">occultisme</trd>
  <trd lng="ru">&c_o;&c_k;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
