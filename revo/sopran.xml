<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sopran</rad>/o
</kap>
<drv mrk="sopran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="sopran.0o.registro">
    <dif>
      Plej alta <ref tip="super" cel="regist.0o.MUZ">registro</ref>,
      kante uzata precipe de inoj kaj infanoj.
    </dif>
  </snc>
  <snc mrk="sopran.0o.vocxo">
    <dif>
      Muzikilo, vo&ccirc;o a&ubreve; kantisto kun soprana registro.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;</trd>
  <trd lng="de">Sopran</trd>
  <trd lng="en">soprano</trd>
  <trdgrp lng="fr">
    <trd>soprano</trd>
  </trdgrp>
  <trd lng="hu">szopr&aacute;n</trd>
  <trd lng="nl">sopraan</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_a;&c_n;&c_o;<klr tip="amb"> (&c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
</drv>

<drv mrk="sopran.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De plej alta registro inter vo&ccirc;oj
      a&ubreve; inter samfamiliaj instrumentoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>soprano<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">szopr&aacute;n</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_a;&c_n;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="sopran.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Sopranvo&ccirc;a <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>soprano<klr> (chanteuse)</klr></trd>,
    <trd>sopraniste</trd>
  </trdgrp>
  <trd lng="hu">szopr&aacute;n&eacute;nekes</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_a;&c_n;&c_o;<klr tip="amb"> (&c_p;&c_je;&c_v;&c_i;&c_c;&c_a;)</klr></trd>
</drv>
</art>
