<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sopran.xml,v 1.27 2023/10/21 07:46:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sopran</rad>/o
</kap>
<drv mrk="sopran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="sopran.0o.registro">
    <dif>
      Plej alta vo&ccirc;o, <ref tip="super" cel="sopran.0o.vocxo">registro</ref>,
      kantata precipe de virinoj kaj infanoj:
      <ekz>
        infanoj <klr>[&#8230;]</klr> &ccirc;u ili for&jcirc;etos la arton, kiam ili ne povos pli kanti per
        <tld/>o?
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sopran.0o.vocxo">
    <dif>
      Muzikilo, vo&ccirc;o a&ubreve; kantisto kun soprana registro:
      <ekz>
        <nom>Inva Mula</nom> (<tld/>o, debutinta en Parizo)
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2006/008910.php">Eksterlande eminentaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;</trd>
  <trd lng="cs">sopr&aacute;n</trd>
  <trd lng="de">Sopran</trd>
  <trd lng="en">soprano</trd>
  <trdgrp lng="fr">
    <trd>soprano</trd>
  </trdgrp>
  <trd lng="hu">szopr&aacute;n</trd>
  <trd lng="it">soprano</trd>
  <trdgrp lng="ja">
    <trd>&#x30BD;&#x30D7;&#x30E9;&#x30CE;</trd>,
    <trd>&#x30BD;&#x30D7;&#x30E9;&#x30CE;&#x697D;&#22120; 
    <pr>&#x30BD;&#x30D7;&#x30E9;&#x30CE;&#x304C;&#12387;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">sopraan</trd>
  <trdgrp lng="pl">
    <trd>sopran</trd>,
    <trd>dyszkant</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_a;&c_n;&c_o;<klr tip="amb"> (&c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  <trd lng="sk">sopr&aacute;n</trd>
  <trd lng="uk">&c_s;&#x43E;&#x43F;&c_r;&c_a;&#x43D;&#x43E; (&c_g;&#x43E;&#x43B;&#x43E;&c_s;)</trd>
</drv>

<drv mrk="sopran.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De plej alta registro inter vo&ccirc;oj
      a&ubreve; inter samfamiliaj instrumentoj:
      <ekz>
        <klr>[ili]</klr> havas ordinare vo&ccirc;on <tld/>an
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">Sopran-</trd>
  <trd lng="en">soprano</trd>
  <trdgrp lng="fr">
    <trd>soprano<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">szopr&aacute;n</trd>
  <trdgrp lng="it">
    <trd>soprano<klr> (agg.)</klr></trd>,
    <trd>bianco<klr> (mus.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30BD;&#x30D7;&#x30E9;&#x30CE;&#x306E; 
  <pr>&#x305D;&#12407;&#12425;&#x306E;&#x306E;</pr></trd>
  <trd lng="pl">cienki  <ind>g&lstroke;os</ind></trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_a;&c_n;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="sopran.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Sopranvo&ccirc;a <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>:
      <ekz>
        la kantistoj <nom>Heman</nom>, <nom>Asaf</nom>, kaj <nom>Etan</nom>
        estis por ludado sur kupraj cimbaloj,
        <nom>Ze&hcirc;arja</nom>, <nom>Aziel</nom>, <nom>&Scirc;emiramot</nom>,
        <nom>Je&hcirc;iel</nom>, <nom>Uni</nom>, <nom>Eliab</nom>, <nom>Maaseja</nom>
        kaj <nom>Benaja</nom> &dash; sur psalteroj, por <tld/>uloj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 15:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        al la &hcirc;orestro de la Kora&hcirc;idoj, por <tld/>uloj, kanto
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 46</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_n;&c_a; <klr>(&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">sopranista</trd>
  <trd lng="de">Sopran</trd>
  <trd lng="en">soprano singer</trd>
  <trdgrp lng="fr">
    <trd>soprano<klr> (chanteuse)</klr></trd>,
    <trd>sopraniste</trd>
  </trdgrp>
  <trd lng="hu">szopr&aacute;n&eacute;nekes</trd>
  <trd lng="it">soprano</trd>
  <trdgrp lng="ja">
    <trd>&#x30BD;&#x30D7;&#x30E9;&#x30CE;&#x6B4C;&#x624B; 
    <pr>&#x30BD;&#x30D7;&#x30E9;&#x30CE;&#x304B;&#12375;&#12421;</pr></trd>,
    <trd>&#x30BD;&#x30D7;&#x30E9;&#x30CE;&#x306E;&#x58F0;&#12434;&#12375;&#x305F;&#x4EBA; 
    <pr>&#x305D;&#12407;&#12425;&#x306E;&#x306E;&#12371;&#12360;&#12434;&#12375;&#x305F;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">sopran  <klr>(&sacute;piewaczka)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_a;&c_n;&c_o;<klr tip="amb"> (&c_p;&c_je;&c_v;&c_i;&c_c;&c_a;)</klr></trd>
  <trd lng="sk">sopranista</trd>
</drv>
</art>
