<vortaro>
<art mrk="$Id: brajl1.xml,v 1.15 2024/10/11 13:33:39 revo Exp $">
<kap>
  <rad>brajl</rad>/i
</kap>

<drv mrk="brajl1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Faldi velon sur la jardon:
      <ekz>
         &leftquot;jen la glacio!&rightquot; diris maristo en la plej alta masto, La &scirc;ipo da&ubreve;rigis norden, kaj la
         maristoj brajlis la velojn
         <fnt>
           <aut>Tim Westover</aut>
           <vrk><url ref="https://bulteno.esperanto-usa.org/a/2009/02/70-beletre/">Orfi&scirc;eto kaj la
           Glacia Monto</url></vrk>
           <lok>Esperanto USA, 2009/02</lok>
         </fnt>;
      </ekz>
      <ekz>
         ili <tld/>is la velojn, surmarigis boaton, kaj dek du bone armitaj francoj kun arkebuzoj
         kaj brule pretaj me&ccirc;oj ensaltis
         <fnt>
           <aut>Miguel de Cervantes, trad. Fernando de Diego</aut>
           <vrk><url ref="https://www.verkoj.com/lauteme/klasika/don-kihot/">La in&gcirc;enia kavaliro Don
           Ki&hcirc;oto el La Man&ccirc;a</url></vrk>
           <lok>&ccirc;apitro 41</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">ps&#xE1;t slepeck&#xFD;m p&#xED;smem</trd>
  <trdgrp lng="de">
    <trd>aufgeien</trd>,
    <trd>geien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>brail</trd>,
    <trd>draw in</trd>,
    <trd>haul up</trd>
  </trdgrp>
  <trd lng="fr">carguer</trd>
  <trdgrp lng="hu">
    <trd><klr tip="amb">(vitolrl&aacute;t)</klr> felteker</trd>,
    <trd><klr tip="amb">(vitolrl&aacute;t)</klr> becsavar</trd>
  </trdgrp>
  <trd lng="ja">&#x7D5E;&#x308B;  <pr>&#12375;&#x307C;&#x308B;</pr></trd>
  <trdgrp lng="ru">
   <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a;
&c_g;&c_i;&c_t;&c_o;&c_v;&c_y;</trd>,
   <trd>&c_u;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;
&c_p;&c_a;&c_r;&c_u;&c_s;&c_a;</trd>
</trdgrp>
  <trd lng="sk">posklada&tcaron; plachtu na rahno</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_g;&c_ib;&c_t;&#x43E;&c_v;&c_i;</trd>,
    <trd>&c_z;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;&#xA0;(&c_v;&c_ib;&c_t;&c_r;&c_i;&#x43B;&c_a;)</trd>
  </trdgrp>
</drv>
</art>
