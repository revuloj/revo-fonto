<drv mrk="brajl1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Faldi velon sur la jardon.
    </dif>
  </snc>
  <trd lng="cs">ps&#xE1;t slepeck&#xFD;m p&#xED;smem</trd>
  <trdgrp lng="de">
    <trd>aufgeien</trd>,
    <trd>geien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>brail</trd>,
    <trd>draw in</trd>,
    <trd>haul up</trd>
  </trdgrp>
  <trd lng="fr">carguer</trd>
