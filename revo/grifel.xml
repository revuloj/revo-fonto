<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>grifel</rad>/o
</kap>
<drv mrk="grifel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pinta &scirc;tala ileto, oblikve tran&ccirc;ita &ccirc;e unu el siaj ekstrema&jcirc;oj,
      kiun la gravuristo uzas por desegni sur metalplaton. 
    </dif>
    <refgrp tip="sin">
      <ref cel="cizel.0ilo">cizelilo</ref>,
      <ref cel="cxiz.0ilo">&ccirc;izilo</ref>,
      <ref cel="gravur.0ilo">gravurilo</ref>.      
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_je;&c_c; <klr>(&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r;&c_a;)</klr></trd>,
    <trd>&c_sh;&c_t;&c_i;&c_h;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
