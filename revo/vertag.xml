<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vertag</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="vertag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="vertag.0o.ZOO">
    <dif>
    <ref tip="sin" cel="hund.mel0o.ZOO">melhundo</ref>:
    <ekz>
      el la du hundoj de miaj geavoj, <tld/>o kaj miksrasulo
      Bunda&scirc;, mi amas precipe la duan
      <fnt>
      <aut>Stefan Maul</aut>,
      <vrk>El verva vivo &jcirc;urnalista</vrk>,
      <lok>p. 12</lok>
      </fnt>.
    </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_a; <klr tip="ind">(&c_s;&c_a;&c_b;&c_a;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_d;&c_a;&c_k;&c_je;&c_l;</trd>
  <trd lng="ca">dachshund<klr> (gos salsitxa)</klr></trd>
  <trd lng="cs">jezev&ccaron;&iacute;k</trd>
  <trd lng="de">Dackel</trd>
  <trd lng="en">dachshund</trd>
  <trdgrp lng="fr">
    <trd>basset</trd>,
    <trd>teckel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tacsk&oacute;</trd>,
    <trd>dakszli</trd>,
    <trd>borzeb</trd>
  </trdgrp>
  <trd lng="nl">dashond</trd>
  <trd lng="pl">jamnik</trd>
  <trd lng="pt">bass&ecirc;</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_a;<klr tip="amb"> (&c_p;&c_o;&c_r;&c_o;&c_d;&c_a;
  &c_s;&c_o;&c_b;&c_a;&c_k;)</klr></trd>
  <trd lng="sk">jazvečík</trd>
</drv>
</art>
