  <trd lng="pl">jamnik</trd>
  <trd lng="pt">bass&ecirc;</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_a;<klr tip="amb"> (&c_p;&c_o;&c_r;&c_o;&c_d;&c_a;
  &c_s;&c_o;&c_b;&c_a;&c_k;)</klr></trd>
  <trd lng="sk">jazve&ccaron;&iacute;k</trd>
  <trd lng="uk">&c_t;&c_a;&#x43A;&c_s;&c_a; (&#x43F;&#x43E;&c_r;&#x43E;&c_d;&c_a; &c_s;&#x43E;&c_b;&c_a;&#x43A;)</trd>
