  <trd lng="ca">dachshund<klr> (gos salsitxa)</klr></trd>
  <trdgrp lng="cs">
    <trd>jamn&iacute;k</trd>,
    <trd>jezev&ccaron;&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dackel</trd>,
    <trd>Teckel</trd>
  </trdgrp>
  <trd lng="en">dachshund</trd>
  <trdgrp lng="fr">
