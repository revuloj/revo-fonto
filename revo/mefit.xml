<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mefit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mefit.0o">
  <kap><tld/>o</kap>
  <snc mrk="mefit.0o.ZOO">
   <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda mamulo kun dika malhela felo kaj blankaj strioj,
      vivanta en Ameriko, Indonezio, Filipinoj, kiu
      defendas sin, el&scirc;prucigante malbonodoran likva&jcirc;on;
      genro el familio mefitedoj
      (<trd lng="la">Mefitis</trd>);
      <ref tip="sin" cel="skunk.0o.ZOO">skunko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_u;&c_n;&c_s;</trd>
  <trd lng="bg">&c_s;&c_k;&c_u;&c_n;&c_k;&c_s;</trd>
  <trd lng="ca">mofeta</trd>
  <trd lng="cs">skunk</trd>
  <trdgrp lng="de">
    <trd>Stinktier</trd>,
    <trd>Skunk</trd>
  </trdgrp>
  <trd lng="en">skunk</trd>
  <trd lng="fr">moufette</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;zborz</trd>,
    <trd>szkunk</trd>
  </trdgrp>
  <trd lng="nl">stinkdier</trd>
  <trdgrp lng="pt">
    <trd>jaritataca</trd>,
    <trd>cangamb&aacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_u;&c_n;&c_s;</trd>,
    <trd>&c_v;&c_o;&c_n;&c_ju;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">skunk</trd>
</drv>
</art>
