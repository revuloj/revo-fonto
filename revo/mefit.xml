<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mefit</rad>/o
</kap>
<drv mrk="mefit.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Mephitis</trd>), amerika mamulo, kiu
      defendas sin, el&scirc;prucigante malbonodoran likva&jcirc;on
      sekreciatan el &gcirc;iaj glandoj kontra&ubreve; atakanto.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_u;&c_n;&c_s;</trd>
  <trd lng="cs">skunk</trd>
  <trdgrp lng="de">
    <trd>Stinktier</trd>,
    <trd>Skunk</trd>
  </trdgrp>
  <trd lng="en">skunk</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;zborz</trd>,
    <trd>szkunk</trd>
  </trdgrp>
  <trd lng="nl">stinkdier</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_u;&c_n;&c_s;</trd>,
    <trd>&c_v;&c_o;&c_n;&c_ju;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
