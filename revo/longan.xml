<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>longan</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="longan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <tld/>arbo, malgranda ovoforma <ref tip="super"
      cel="drup.0o">drupo</ref> roz- a&ubreve; pupurkolora kun blanka, suka
      karno:
      <ekz>
        <tld/>e granda perlo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 92a, volumo 3a, p.
        178a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">longane</trd>
</drv>

<drv mrk="longan.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="longan.0ujo.BOT">
    <ref tip="dif" cel="longan.0arbo"><tld/>arbo</ref>
  </snc>
  <trd lng="fr">longanier</trd>
</drv>

<drv mrk="longan.0arbo">
  <kap><tld/>arbo</kap>
  <snc mrk="longan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Orientazia kaj oceania arbo (<trd lng="la">Euphoria longan</trd>) kun
      man&gcirc;eblaj fruktoj.
    </dif>
  </snc>
  <trd lng="fr">longanier</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>