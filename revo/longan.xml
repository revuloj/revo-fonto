<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: longan.xml,v 1.12 2024/02/20 09:21:06 revo Exp $">
<kap>
  <rad>longan</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="longan.0o">
  <kap><tld/>o</kap>
  <snc mrk="longan.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <tld/>arbo, malgranda ovoforma <ref tip="super" cel="drup.0o">drupo</ref> flaveta kun blanka, suka
      karno:
      <ekz>
        <tld/>e granda perlo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 92a, volumo 3a, p.
        178a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      La frukto iom similas al <ref cel="licxi.0o">li&ccirc;io</ref>.
    </rim>
    <bld
      lok="&WCU;/thumb/2/2d/Dimocarpus_longan_fruits.jpg/315px-Dimocarpus_longan_fruits.jpg"
      prm="PD"
      ><tld/>oj
        <fnt>
          <lok><url ref="&WCW;/File:Dimocarpus_longan_fruits.jpg"
              >https://commons.wikimedia.org/wiki/File:Dimocarpus_longan_fruits.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="cs">longan</trd>
  <trd lng="de">Linkang-Fr&uuml;chte</trd>
  <trd lng="fr">longane</trd>
  <trd lng="hu">longan</trd>
  <trdgrp lng="pl">
    <trd>longan</trd>,
    <trd>&zdot;y&zacute;nica</trd>
  </trdgrp>
  <trd lng="ro">longan</trd>
  <trd lng="sk">longan</trd>
  <trd lng="uk">&c_l;&c_o;&c_n;&c_g;&c_a;&c_n;</trd>
</drv>

<drv mrk="longan.0arbo">
  <kap><tld/>arbo,
  <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="longan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Orientazia kaj oceania <ref tip="super" cel="arb.0o.BOT">arbo</ref>;
      sepcio el <ref tip="malprt" cel="sapindac.0oj">sapindacoj</ref> (<trd lng="la">Dimocarpus longan</trd>)
      kun man&gcirc;eblaj fruktoj:
      <ekz>
        malanta&ubreve; la lernejo fluis rivereto, sur kies
        bordoj kreskis <tld/>-arboj
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Linkang-Baum</trd>
  <trd lng="fr">longanier</trd>
  <trd lng="hu">longanfa</trd>
  <trd lng="pl">longan</trd>
  <trd lng="pt">longana</trd>
  <trd lng="ro">longan</trd>

  <trdgrp lng="zh">
    <trd>&#40845;&#30524; <pr>l&oacute;ngy&#462;n</pr></trd>,
    <trd>&#40857;&#30524; <pr>l&oacute;ngy&#462;n</pr></trd>
  </trdgrp>
</drv>
</art>

