<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>Aleksandr</rad>/o
</kap>
<drv mrk="aleksandr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira anta&ubreve;nomo grekdevena:
      <ekz>
        <tld/>o ne volas lerni
        <fnt><bib>F</bib>, <lok>&FE; 9</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_l;&c_ja;&c_k;&c_s;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_A;&c_l;&c_je;&c_s;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">Alexandre</trd>
  <trd lng="ru">&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r;</trd>
</drv>

<drv mrk="aleksandr.0o.la.granda">
  <kap><tld/>o la Granda</kap>
  <snc>
    <dif>
      <tld lit="A"/>o la 3a de Makedonujo (356 a. K. &ndash; 323 a. K.),
      konkerinto de vasta efemera imperio de Grekujo okcidente
      &gcirc;is nordokcidenta Hindujo oriente, kiu vastigis la influon de la helena
      kulturo.
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_l;&c_ja;&c_k;&c_s;&c_a;&c_n;&c_d;&c_a;&c_r; &c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>
  <trd lng="fr">Alexandre le grand</trd>
  <trdgrp lng="ru">
    <trd>&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r; &c_V;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r; &c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

