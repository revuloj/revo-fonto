<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aleksandr.xml,v 1.17 2021/05/09 18:08:05 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>Aleksandr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="aleksandr.A0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        <tld/>o ne volas lerni
        <fnt><bib>F</bib>, <lok>&FE; 9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_l;&c_ja;&c_k;&c_s;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_A;&c_l;&c_je;&c_s;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Alexander</trd>
  <trd lng="en">Alexander</trd>
  <trdgrp lng="fr">
   <trd>Alexandre</trd>,
   <trd>Alexis</trd>, 
   <trd>Alex</trd> 
  </trdgrp>  
  <trd lng="it">Alessandro</trd>
  <trd lng="nl">Alexander</trd>
  <trd lng="no">Aleksander</trd>
  <trdgrp lng="pl">
    <trd>Aleksander</trd>,
    <trd>Alek</trd>,
    <trd>Olek</trd>
  </trdgrp>
  <trd lng="ru">&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r;</trd>
</drv>

<drv mrk="aleksandr.A0o_la_Granda">
  <kap><tld/>o la Granda</kap>
  <snc>
  <uzo tip="fak">HIS</uzo>
    <dif>
      <tld lit="A"/>o la 3a de Makedonujo (356 a. K. &ndash; 323 a. K.),
      konkerinto de vasta efemera imperio de Grekujo okcidente
      &gcirc;is nordokcidenta Hindujo oriente, kiu vastigis la influon
      de la helena kulturo:
      <ekz>
        330 jc a.K. &#8208; post la disfalo de Persa imperio pro la atakoj de
        <tld/>o la Granda, aramea lingvo cedas la pozicion de oficiala
        lingvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=202'>Aramea
          lingvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en <ctl>Paralelaj Vivoj</ctl> Plutarko <klr>[...]</klr> skribis
        pri la vivoj de Aleksandro la Granda, Cezaro, Cicerono,
        <klr>[...]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref='https://eo.wikipedia.org/?curid=3462'>Plutarko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_l;&c_ja;&c_k;&c_s;&c_a;&c_n;&c_d;&c_a;&c_r;
  &c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>
  <trd lng="de">Alexander der Gro&szlig;e</trd>
  <trd lng="en">Alexander the Great</trd>
  <trd lng="fr">Alexandre le Grand</trd>
  <trdgrp lng="it">
    <trd><ind>Alessandro</ind> Magno</trd>,
    <trd><ind>Alessandro</ind> il macedone</trd>
  </trdgrp>
  <trd lng="nl">Alexander de Grote</trd>
  <trd lng="no">Aleksander den store</trd>
  <trdgrp lng="pl">
    <trd>Aleksander Wielki</trd>,
    <trd>Aleksander Macedo&nacute;ski</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r;
    &c_V;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r;
    &c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

