<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>televid</rad>/i
</kap>

<drv mrk="televid.0i">
  <kap><tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="televid.0i.TEL">
    <dif>
      Rigardi filmojn, movbildojn kaj akompanajn sonojn transsendatajn
      je grandaj distancoj per elektromagnetaj ondoj a&ubreve; aliaj
      rimedoj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol; &c_p;&c_a;  <ind>&c_t;&c_e;&c_l;&c_je;&c_v;&c_ib;&c_z;&c_ib;&c_ib;</ind>
  </trd>
  <trd lng="bg">&c_g;&c_l;&c_je;&c_d;&c_a;&c_m; <ind>&c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_z;&c_i;&c_ja;</ind>
  </trd>
  <trd lng="ca">mirar la televisi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>dívat se na televizi</trd>,
    <trd>sledovat televizi</trd>
  </trdgrp>
  <trd lng="en">watch television</trd>
  <trd lng="es">ver la televisi&oacute;n</trd>
  <trd lng="fr">regarder la t&eacute;l&eacute;vision</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;v&eacute;zik</trd>,
    <trd>telev&iacute;zi&oacute;zik</trd>
  </trdgrp>
  <trd lng="nl">televisie kijken</trd>
  <trdgrp lng="pl">
    <trd>ogl&aogonek;da&cacute; <ind>telewizj&eogonek;</ind></trd>,
    <trd>siedzie&cacute; przed <ind>telewizorem</ind> <klr>(pot.)</klr></trd>,
    <trd>gapi&cacute; si&eogonek; na <ind>telewizor</ind> <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ver televis&atilde;o</trd>,
    <trd>assistir &agrave; televis&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_t;&c_r;&c_je;&c_t;&c_mol; &c_p;&c_o; <ind>&c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_ju;</ind>
  </trd>
  <trd lng="sk">sledovať televíziu</trd>
  <trd lng="sv">se p&aring; television</trd>
</drv>

<drv mrk="televid.0o">
  <kap><tld/>o</kap>
  <snc mrk="televid.0o.TEL">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">TEL</uzo>
    <dif>
      La &gcirc;enerala aran&gcirc;o kaj ma&scirc;inaro, per kiu oni
      telesendas kaj ricevas movbildojn kaj sonon:
      <ekz>
        la baza ideo en &ccirc;ifrado de analoga <tld/>o estas, ke oni
        sendas la bildon en mal&gcirc;usta ordo
        <fnt>
        <aut>Franck ARNAUD</aut>:
        <vrk><url ref="&Monato;/006340.html">Preterle&gcirc;a televidado
        en E&ubreve;ropo</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 03/1999</lok>.
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>telewizja</trd>,
      <trd>TV</trd>
    </trdgrp>
  </snc>
  <snc mrk="televid.0o.televidado">
    <dif>
      <tld lit="T"/>ado:
      <ekz>
        kaj e&ccirc; se mi devas pasigi multajn vesperojn
        a&ubreve;skultante liajn priskribojn de la vivo en aliaj epokoj,
        tio estas malpli enuiga ol <tld/>o
        <fnt>
        <aut>William AULD</aut>:
        <vrk><url ref="&Monato;/002000.html">Tempo fu&gcirc;as</url>
        </vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 01/1987</lok>.
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>ogl&aogonek;danie <ind>telewizji</ind></trd>,
      <trd>siedzenie przed <ind>telewizorem</ind>
      <klr>(pot.)</klr></trd>,
      <trd>gapienie si&eogonek; na <ind>telewizor</ind> <klr>(pot.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="televid.0o.televidajxo">
    <ref tip="dif" cel="televid.0ajxo.TEL"><tld/>a&jcirc;o</ref>
      <ekz>
        la najbaroj delonge estis revenintaj de la laborejoj kaj nun
        eble ili vesperman&gcirc;is a&ubreve; spektis <tld/>on
        <fnt>
        <aut>Julian MODEST</aut>:
        <vrk><url ref="&Monato;/005343.html">En la ombro de la silento
        </url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 03/1997</lok>.
        </fnt>
      </ekz>
      <trd lng="pl">telewizja</trd>
  </snc>
  <snc mrk="televid.0o.televidilo">
    <ref tip="dif" cel="televid.0ilo.TEL"><tld/>ilo</ref>
      <ekz>
        la infano tuj &scirc;altis la <tld/>on kaj eksidis anta&ubreve;
        &gcirc;i
        <fnt>
        <aut>Julian MODEST</aut>:
        <vrk>
        <url ref="&Monato;/005343.html">En la ombro de la silento</url>
        </vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 03/1997</lok>.
        </fnt>
      </ekz>
      <trd lng="pl">telewizja</trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_v;&c_ib;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">televisi&oacute;</trd>
  <trd lng="cs">televize</trd>
  <trdgrp lng="en">
    <trd>television</trd>,
    <trd>T.V.</trd>
  </trdgrp>
  <trd lng="es">televisi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;</trd>,
    <trd>t&eacute;l&eacute;vision</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>telev&iacute;zi&oacute;</trd>,
    <trd>t&eacute;v&eacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>televisi</trd>,
    <trd>TV</trd>
  </trdgrp>
  <trd lng="nl">televisie</trd>
  <trd lng="ru">
  &c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">televízia</trd>
  <trdgrp lng="sv">
    <trd>television</trd>,
    <trd>TV</trd>,
    <trd>teve</trd>
  </trdgrp>
</drv>

<drv mrk="televid.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="televid.0ajxo.TEL">
    <dif>
      Aparta filmo a&ubreve; elsenda&jcirc;o <tld/>a:
      <ekz>
        mi ne estis certa pri la &jcirc;argono; mi estis lerninta
        &gcirc;in el malnovaj <tld/>a&jcirc;oj
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 2a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>programa</trd>,
    <trd>espai televisiu</trd>
  </trdgrp>
  <trd lng="fr">&eacute;mission de t&eacute;l&eacute;vision</trd>
  <trdgrp lng="pl">
    <trd>audycja <ind>telewizyjna</ind></trd>,
    <trd>program <ind>telewizyjny</ind></trd>,
    <trd>program <ind>TV</ind></trd>
  </trdgrp>
</drv>

<drv mrk="televid.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="televid.0ilo.TEL">
    <dif>
      Aparato per kiu oni <tld/>as:
      <ekz>
        e&ccirc; la tujtradukado de la Francaj nova&jcirc;oj el la
        hotela <tld/>ilo ne plu savis lin de la stigmato
        <fnt>
        <aut>PEJNO Simono</aut>:
        <vrk><url ref="&Monato;/006329.html">La lernemulo</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 06/1999</lok>.
        </fnt>
      </ekz>
      <ekz>
        en tiuj cirkonstancoj kompreneble ne estas bezono je
        kolor<tld/>ilo, lavma&scirc;ino, fridujo
        <fnt>
        <aut>MU Binghua</aut>:
        <vrk><url ref="&Monato;/007168.html">Elektraj aparatoj mankas en
        la kamparo</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/2000</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_v;&c_ib;&c_z;&c_a;&c_r;</trd>
  <trd lng="bg">&c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_z;&c_o;&c_r;</trd>
  <trd lng="ca">televisor</trd>
  <trd lng="cs">televizor</trd>
  <trdgrp lng="en">
    <trd>television set</trd>,
    <trd>T.V. set</trd>
  </trdgrp>
  <trd lng="es">televisor</trd>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;viseur</trd>,
    <trd>t&eacute;l&eacute;vision<klr tip="amb">
    (t&eacute;l&eacute;viseur)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;v&eacute;</trd>,
    <trd>t&eacute;v&eacute;k&eacute;sz&uuml;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>televisi</trd>,
    <trd>TV</trd>
  </trdgrp>
  <trd lng="nl">televisietoestel</trd>
  <trdgrp lng="pl">
    <trd>odbiornik <ind>telewizyjny</ind></trd>,
    <trd>odbiornik <ind>TV</ind></trd>,
    <trd>telewizor</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_z;&c_o;&c_r;</trd>
  <trd lng="sk">televízor</trd>
  <trdgrp lng="sv">
    <trd>TV-mottagare</trd>,
    <trd>TV</trd>,
    <trd>teve</trd>
  </trdgrp>
</drv>

<drv mrk="televid.kabla0o">
  <kap>kabla <tld/>o</kap>
  <fnt><bib>PIV2</bib></fnt>
  <snc mrk="televid.kabla0o.TEL">
  <uzo tip="fak">TEL</uzo>
    <dif>
      <tld lit="T"/>a sistemo, en kiu la programoj estas dissendataj
      al la klientoj pere de <ref cel="kabl.0o.dratofasko">kabloj</ref>.
    </dif>
  </snc>
  <trd lng="ca">televisi&oacute; per cable</trd>
  <trd lng="cs">kabelová televize</trd>
  <trd lng="en">cable television</trd>
  <trd lng="es">televisi&oacute;n por cable</trd>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;vision par c&acirc;ble</trd>,
    <trd>c&acirc;ble<klr tip="ind">
    (t&eacute;l&eacute;vision par c&acirc;ble)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&aacute;belt&eacute;v&eacute;</trd>
  <trd lng="id"><ind>TV</ind> kabel</trd>
  <trdgrp lng="pl">
    <trd><ind>telewizja</ind> kablowa</trd>,
    <trd>TVK</trd>
  </trdgrp>
  <trd lng="ru">
  &c_k;&c_a;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
  &c_t;&c_je;&c_l;&c_je;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">káblová televízia</trd>
  <trd lng="sv">kabel-TV</trd>
</drv>
</art>
