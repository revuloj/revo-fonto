<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sigismund.xml,v 1.8 2023/04/11 07:24:29 revo Exp $">
  <kap>
    <rad>Sigismund</rad>/o
  </kap>

  <drv mrk="sigismund.0o">
    <kap>
      <tld/>o
    </kap>
    <snc mrk="sigismund.0o.NOM">
      <uzo tip="fak">NOM</uzo>
      <dif>
        Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
        <ekz>
          ili tiam multfoje vidis <tld/>on von Ramungen vagi tra la 
          kastelo
          <fnt>
            <aut>Radio Polonia</aut>,
            <vrk>
              <url
              ref="http://www.polskieradio.pl/polonia/article.asp?tId=31535">
                Kastelo en Gniew</url></vrk>,
            <lok>2006-01-06</lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="sigmund.0o.NOM">Sigmundo</ref>
      <trdgrp lng="be">
        <trd>&c_S;&c_ib;&c_g;&c_ib;&c_z;&c_m;&c_u;&c_n;&c_d;</trd>,
        <trd>&c_Z;&c_y;&c_g;&c_m;&c_u;&c_n;&c_t;</trd>
      </trdgrp>
      <trd lng="de">Sigismund</trd>
      <trd lng="en">Sigismund</trd>
      <trd lng="fr">Sigismond</trd>
      <trd lng="it">Sigismondo</trd>
      <trd lng="la">Sigismundus</trd>
      <trd lng="pl">Zygmunt</trd>
      <trd lng="pt">Sigismundo</trd>
      <trd lng="ru">&c_S;&c_i;&c_g;&c_i;&c_z;&c_m;&c_u;&c_n;&c_d;</trd>
    </snc>
  </drv>
</art>

