<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tra&hcirc;e</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trahxe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trahxe.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Spirtubo &ccirc;e vertebruloj kondukanta la aeron de la laringo
      al la pulmoj kaj dividi&gcirc;anta &ccirc;e la malsupra
      parto en du bran&ccirc;ojn nomatajn bronkoj.
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_h;&c_je;&c_ja;</trd>
    <trd lng="ca">tr&agrave;quea</trd>
    <trdgrp lng="de">
      <trd>Luftr&ouml;hre</trd>,
      <trd>Trachee</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>trachea</trd>,
      <trd>wind pipe</trd>,
    </trdgrp>
    <trd lng="es">tr&aacute;quea</trd>
    <trd lng="fr">trach&eacute;e</trd>
    <trd lng="hu">l&eacute;gcs&odblac;</trd>
    <trd lng="la">trachea</trd>
    <trd lng="nl">luchtpijp</trd>
    <trd lng="pl">tchawica</trd>
    <trd lng="pt">traqu&eacute;ia</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_h;&c_je;&c_ja;</trd>
  </snc>
  <snc mrk="trahxe.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Spirtubo cxe surteraj artikpieduloj.
    </dif>
    <trd lng="la">trachea</trd>
    <trd lng="pl">tchawka</trd>
  </snc>
  <snc mrk="trahxe.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="vaskul.0o">Vaskulo</ref>,
      kies &ccirc;elo malhavas transversajn septojn.
    </dif>
    <ref tip="sin" cel="vaskul.0o.perfekta">perfekta vaskulo</ref>
    <refgrp tip="ant">
      <ref cel="vaskul.0o.neperfekta">neperfekta vaskulo</ref>
    </refgrp>
  </snc>
</drv>

<drv mrk="trahxe.0uloj">
  <kap><tld/>uloj <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="trahxe.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      (<trd lng="la">Tracheata</trd>) de <ref tip="super"
      cel="artik.0pieduloj">artikpieduloj</ref>,
      spirantaj per <tld/>oj kaj plejparte vivantaj
      sur tero aux en areo: araneuloj, multpieduloj,
      insektoj, k. a.
    </dif>
  </snc>
  <trdgrp lng="pl">
    <trd>tchawkowce</trd>,
    <trd>tchawkodyszne</trd>,
    <trd>tchawkodyszce</trd>
  </trdgrp>
</drv>

<drv mrk="trahxe.0ito">
  <kap><tld/>ito <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="trahxe.0ito.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="inflam.0o">Inflamo</ref>
      de la <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="pl">
    <trd>zapalenie <ind>tchawicy</ind></trd>
  </trdgrp>
</drv>

<drv mrk="trahxe.0oplantoj">
  <kap><tld/>oplantoj <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="trahxe.0oplantoj.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="vaskul.0o.planto">vaskulplantoj</ref>
  </snc>
  <ref tip="sin" cel="trahxe.0ofitoj"><tld/>ofitoj</ref>
</drv>

<drv mrk="trahxe.0ofitoj">
  <kap><tld/>ofitoj <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="trahxe.0ofitoj.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="vaskul.0o.planto">vaskulplantoj</ref>
  </snc>
  <ref tip="sin" cel="trahxe.0oplantoj"><tld/>oplantoj</ref>
</drv>
</art>
