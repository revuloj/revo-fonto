<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trahxe.xml,v 1.31 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tra&hcirc;e</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trahxe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trahxe.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Spirtubo &ccirc;e vertebruloj kondukanta la aeron de la laringo
      al la pulmoj kaj dividi&gcirc;anta &ccirc;e la malsupra
      parto en du bran&ccirc;ojn nomatajn <ref cel="bronk.0o">bronkoj</ref>:
      <ekz>
        anta&ubreve; 45 000 jaroj vivis la ina mamutido <klr>[&#x2026;]</klr>, en &scirc;iaj naza kavo,
        fa&ubreve;ko kaj <tld/>o oni trovis &scirc;limon, ver&scirc;ajne &scirc;i sufoki&gcirc;is pro &scirc;lima lavango
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011414.php">Glaciaj mumioj sor&ccirc;as A&ubreve;strion</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_h;&c_je;&c_ja;</trd>
    <trd lng="ca">tr&agrave;quea</trd>
    <trdgrp lng="de">
      <trd>Luftr&ouml;hre</trd>,
      <trd>Trachee</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>trachea</trd>,
      <trd>wind pipe</trd>,
    </trdgrp>
    <trd lng="es">tr&aacute;quea</trd>
    <trd lng="fr">trach&eacute;e</trd>
    <trd lng="hu">l&eacute;gcs&odblac;</trd>
    <trd lng="la">trachea</trd>
    <trd lng="nl">luchtpijp</trd>
    <trd lng="pl">tchawica</trd>
    <trd lng="pt">traqu&eacute;ia</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_h;&c_je;&c_ja;</trd>
  </snc>
  <snc mrk="trahxe.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Spirtubo &ccirc;e surteraj artikuloj:
      <ekz>
        <tld/>oj, sistemoj de brankaj tuneloj <klr>[&#x2026;]</klr> liveras oksigenon rekte al
        la unuopaj &ccirc;eloj &ccirc;e multaj insektoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=366">Artropodoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Trachee</trd>
    <trd lng="la">trachea</trd>
    <trd lng="pl">tchawka</trd>
  </snc>
  <snc mrk="trahxe.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="vaskul.0o">Vaskulo</ref>,
      kies &ccirc;elo malhavas transversajn vandojn.
    </dif>
    <ref tip="sin" cel="vaskul.perfekta0o">perfekta vaskulo</ref>
  </snc>
  <trd lng="ca">tr&agrave;quea</trd>
  <trd lng="it">trachea</trd>
  <trdgrp lng="ja">
    <trd>&#x6C17;&#x7BA1;  <pr>&#x304D;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x5C0E;&#x7BA1;  <pr>&#x3069;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&c_h;&c_je;&#x44F;</trd>,
    <trd>&c_d;&c_i;&c_h;&c_a;&#x43B;&#x44C;&#x43D;&c_je; &c_g;&#x43E;&c_r;&#x43B;&#x43E;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="trahxe.0uloj">
  <kap><tld/>uloj <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="trahxe.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tradicia <ref tip="lst" cel="grup.0o.BIO" lst="voko:zoologiaj_grupoj">grupo</ref>
      ampleksanta multpiedulojn kaj sespiedulojn;
      <ref tip="malprt" cel="artik.0uloj.ZOO">artikuloj</ref>,
      spirantaj per <tld/>oj kaj plejparte vivantaj
      sur tero a&ubreve; en aero: araneuloj, multpieduloj,
      insektoj, kaj aliaj
      (<trd lng="la">Tracheata</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_h;&c_je;&c_j;&c_n;&c_a;&c_d;&c_y;&c_h;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_ja; <klr>(&c_p;&c_a;&c_d;&c_t;&c_y;&c_p;)</klr></trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_w;&c_n;&c_a;&c_v;&c_u;&c_s;&c_y;&c_ja; <klr>(&c_p;&c_a;&c_d;&c_t;&c_y;&c_p;)</klr></trd>
  </trdgrp>
  <trd lng="de"> Tracheentiere</trd>
  <trdgrp lng="it">
    <trd>tracheati<klr> (zool.)</klr></trd>,
    <trd>antennati<klr> (zool.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tchawkowce</trd>,
    <trd>tchawkodyszne</trd>,
    <trd>tchawkodyszce</trd>
  </trdgrp>
</drv>
</art>
