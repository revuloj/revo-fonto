<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tra&hcirc;e</rad>/o
</kap>
<drv mrk="trahxe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trahxe.0o.ANA">
    <uzo tip="fak">ANA</uzo>
      <dif>
        Spirtubo &ccirc;e <ref tip="vid"
cel="verteb.0uloj">vertebruloj</ref> 
        kondukanta la aeron de la <ref tip="vid"
cel="laring.0o">laringo</ref> 
        al la <ref tip="vid" cel="pulm.0o">pulmoj</ref> kaj
        dividi&gcirc;anta &ccirc;e la malsupra parto en du
bran&ccirc;ojn nomatajn 
        <ref tip="vid" cel="bronk.0o">bronkoj</ref>.
      </dif>
    <fnt><bib>PIV2</bib></fnt>
    <trd lng="be">&c_t;&c_r;&c_a;&c_h;&c_je;&c_ja;</trd>
    <trd lng="ca">tr&agrave;quea</trd>
    <trdgrp lng="de">
      <trd>Luftr&ouml;hre</trd>,
      <trd>Trachee</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>trachea</trd>,
      <trd>wind pipe</trd>,
    </trdgrp>
    <trd lng="es">tr&aacute;quea</trd>
    <trd lng="fr">trach&eacute;e</trd>
    <trd lng="hu">l&eacute;gcs&odblac;</trd>
    <trd lng="la">trachea</trd>
    <trd lng="nl">luchtpijp</trd>
    <trd lng="pl">tchawica</trd>
    <trd lng="pt">traqu&eacute;ia</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_h;&c_je;&c_ja;</trd>
  </snc>
  <snc mrk="trahxe.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
      <dif>
        Spirtubo cxe surteraj <ref tip="super"
cel="artik.0pieduloj">artikpieduloj</ref>.
      </dif>
      <fnt><bib>PIV2</bib></fnt>
    <trd lng="la">trachea</trd>
    <trd lng="pl">tchawka</trd>
  </snc>
  <snc mrk="trahxe.0o.BOT">
    <uzo tip="fak">BOT</uzo>
      <dif>
    	<ref tip="super" cel="vaskul.0o">Vaskulo</ref>, 
    	kies &ccirc;elo malhavas transversajn <ref tip="vid"
cel="sept.0o.vando">septojn</ref> 
      </dif>
      <fnt><bib>PIV2</bib></fnt>
      <ref tip="sin" cel="vaskul.0o.perfekta">perfekta vaskulo</ref>
      <refgrp tip="ant">
      	<ref cel="vaskul.0o.neperfekta">neperfekta vaskulo</ref>
        <ref cel="trahxoid.0o"><tld/>oido</ref>
      </refgrp>
  </snc>
</drv>
<drv mrk="trahxe.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <trd lng="la">(Tracheata)</trd> de <ref tip="super"
cel="artik.0pieduloj">artikpieduloj</ref>, 
      spirantaj per <tld/>oj kaj plejparte vivantaj 
      sur tero aux en areo: 
      <ref tip="vid" cel="arane.0uloj">araneuloj</ref>, 
      <ref tip="vid" cel="miriap.0oj.ZOO">miriapodoj</ref>, 
      <ref tip="vid" cel="insekt.0oj">insektoj</ref>, k. a.
    </dif>
    <fnt><bib>PIV2</bib></fnt>
    <trdgrp lng="pl">
      <trd>tchawkowce</trd>,
      <trd>tchawkodyszne</trd>,
      <trd>tchawkodyszce</trd>
    </trdgrp>
</drv>
<drv mrk="trahxe.0ito">
  <kap><tld/>ito</kap>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="inflam.0o">Inflamo</ref> 
      de la <ref tip="vid" cel="trahxe.0o"><tld/>o</ref>.
    </dif>
    <fnt><bib>PIV2</bib></fnt>
    <trdgrp lng="pl">
      <trd>zapalenie <ind>tchawicy</ind></trd>
    </trdgrp>
</drv>
<drv mrk="trahxe.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="vaskul.0o.planto">Vaskulplantoj</ref>.
    </dif>
    <fnt><bib>PIV2</bib></fnt>
    <ref tip="sin" cel="trahxe.0ofitoj"><tld/>ofitoj</ref>
</drv>
<drv mrk="trahxe.0ofitoj">
  <kap><tld/>ofitoj</kap>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="vaskul.0o.planto">Vaskulplantoj</ref>..
    </dif>
    <fnt><bib>PIV2</bib></fnt>
    <ref tip="sin" cel="trahxe.0oplantoj"><tld/>oplantoj</ref>
</drv>

</art>
