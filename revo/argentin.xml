<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: argentin.xml,v 1.19 2023/10/28 21:23:11 revo Exp $">
<kap><rad>Argentin</rad>/o</kap>
<drv mrk="argentin.A0o">
  <kap><tld/>o</kap>
  <mlg kod="ISO-3166">AR</mlg>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&scirc;tato</ref>, limanta al <ref
      tip="vid" cel="brazil.B0o">Brazilo</ref>, <ref tip="vid" cel="urugvaj.U0o">Urugvajo</ref>,
      <ref tip="vid" cel="paragvaj.P0o">Paragvajo</ref>, <ref tip="vid"
      cel="bolivi.B0o">Bolivio</ref>, <ref tip="vid" cel="cxili.CX0o">&Ccirc;ilio</ref> kaj al <ref
      tip="vid" cel="atlant.A0o">Atlantiko</ref>:
      <ekz>
        al la supra listo de landoj ni devas aldoni <tld/>on kaj
        Israelon
        <fnt>
          <aut>Marjorie Boulton</aut>
          <vrk>Ne nur leteroj de plumamikoj</vrk>
          <lok>Esperanta poezio: mirakla rikolto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa pinto, kiun la indi&gcirc;enoj nomis Anon, signas la landlimon
        inter <tld/>o kaj &Ccirc;ilio
        <fnt>
          <aut>Werner Schad</aut>
          <vrk>Argentino: Tondrulo montego de la Andoj</vrk>
          <bib>Monato</bib>
          <lok>jaro 2003a, numero 2a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_r;&c_g;&c_je;&c_n;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">Argentina</trd>
  <trd lng="da">Argentina</trd>
  <trd lng="de">Argentinien</trd>
  <trd lng="en">Argentina</trd>
  <trd lng="es">Argentina</trd>
  <trd lng="fa">&a_A_madda;&a_r;&f_zh;&a_A;&a_n;&a_t;&f_y;&a_n;</trd>
  <trd lng="fi">Argentiina</trd>
  <trd lng="fr">Argentine</trd>
  <trd lng="fy">Argentynje</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D2;&#x5E0;&#x5D8;&#x5D9;&#x5E0;&#x5D4;</trd>
  <trd lng="id">Argentina</trd>
  <trd lng="io">Arjentinia</trd>
  <trd lng="it">Argentina</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30BC;&#x30F3;&#x30C1;&#x30F3;
  <pr>&#12354;&#x308B;&#x305C;&#12435;&#12385;&#12435;</pr></trd>
  <trd lng="nl">Argentini&euml;</trd>
  <trd lng="pl">Argentyna</trd>
  <trd lng="pt">Argentina</trd>
  <trd lng="ru">&c_A;&c_r;&c_g;&c_je;&c_n;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">Argent&iacute;na</trd>
  <trd lng="sl">Argentina</trd>
  <trd lng="sv">Argentina</trd>
  <trd lng="tok">ma Alensina</trd>
  <trd lng="uk">&c_A;&c_r;&c_g;&c_je;&c_n;&c_t;&c_ib;&c_n;&c_a;</trd>
  <trd lng="vo">Larg&auml;nt&auml;n</trd>
</drv>

<drv mrk="argentin.0a">
  <kap><tld lit="a"/>a</kap>
  <snc mrk="argentin.0a.GOEG">
    <dif>
      Rilata al <tld/>o:
      <ekz>petu informojn pri la loko al <tld/>a samideano
        <fnt>
          <aut>Rafael Zerbetto</aut>
          <vrk>La Bonaera Universala Kongreso</vrk>
          <lok>Brazila Esperantisto, 2014-03, numero 348a</lok>
        </fnt>;
      </ekz>
      <ekz>
        okazas foje, ke iu a&ubreve; alia <tld lit="a"/>a ido ka&scirc;iras en
        vestejon kaj sidas tie sekrete farante hejmtaskojn
        <fnt>
          <aut>Astrid Lindgren, trad. Sven Alexandersson</aut>
          <vrk>Pipi &Scirc;trumpolonga</vrk>
          <lok>Pipi komencas la lernejon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_je;&c_n;&c_t;&c_y;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">argentinisch</trd>
  <trdgrp lng="en">
    <trd>Argentinian</trd>,
    <trd>Argentine</trd>,
    <trd>Argentinean</trd>
  </trdgrp>
  <trd lng="fr">argentin<klr> (adj.)</klr></trd>
  <trd lng="fy">Argentynsk</trd>
  <trdgrp lng="io">
    <trd>Arjentiniana</trd>,
    <trd>Arjentina</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30BC;&#x30F3;&#x30C1;&#x30F3;&#x306E;
  <pr>&#12354;&#x308B;&#x305C;&#12435;&#12385;&#12435;&#x306E;</pr></trd>
  <trd lng="nl">Argentijns</trd>
  <trd lng="tok">pi ma Alensina</trd>
  <trd lng="vo">Larg&auml;nt&auml;nik</trd>
</drv>

<drv mrk="argentin.0ano">
  <kap><tld lit="a"/>ano</kap>
  <snc mrk="argentin.0ano.GEOG">
    <dif>
      Civitano de <tld/>o:
      <ekz>
        ili &ccirc;iuj, tre almozdonemaj, volas nun helpi la malri&ccirc;ajn kaj
        malsatajn <tld lit="a"/>anojn
        <fnt>
          <aut>Ro Sartor</aut>
          <vrk>Ploru pri Argentino!</vrk>
          <bib>Monato</bib>
          <lok>jaro 2002a, numero 2a, p. 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        post cent jaroj de la apero de la bro&scirc;uro de Zamenhof, estas
        kortu&scirc;a afero por ni, <tld lit="a"/>anoj, memorigi tiun fakton
        <fnt>
          <aut>Roberto Sartor. </aut>
          <vrk><url ref="&LOdE;79-lode.htm#79-28">Zamenhof,
          hilelismo kaj Argentino</url></vrk>
          <bib>LOdE</bib>
          <lok>2001:5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_je;&c_n;&c_t;&c_y;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Argentinier</trd>
  <trdgrp lng="en">
    <trd>Argentinian</trd>,
    <trd>Argentine</trd>,
    <trd>Argentinean</trd>
  </trdgrp>
  <trd lng="fr">Argentin<klr> (subst.)</klr></trd>
  <trd lng="fy">Argentyn</trd>
  <trdgrp lng="io">
    <trd>Arjentiniano</trd>,
    <trd>Arjentino</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30BC;&#x30F3;&#x30C1;&#x30F3;&#x4EBA;
  <pr>&#12354;&#x308B;&#x305C;&#12435;&#12385;&#12435;&#12376;&#12435;</pr></trd>
  <trd lng="nl">Argentijn</trd>
  <trd lng="tok">jan pi ma Alensina</trd>
  <trd lng="vo">Larg&auml;nt&auml;nan</trd>
</drv>
</art>
