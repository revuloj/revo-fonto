<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Argentin</rad>/o</kap>
<drv mrk="argentin.A0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <mlg kod="ISO-3166">AR</mlg>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>, limanta
      al <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="urugvaj.U0o">Urugvajo</ref>, <ref tip="vid"
      cel="paragvaj.P0o">Paragvajo</ref>,
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>, <ref tip="vid"
      cel="cxili.CX0o">&Ccirc;ilio</ref>
      kaj al <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_r;&c_g;&c_je;&c_n;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">Argentina</trd>
  <trd lng="da">Argentina</trd>
  <trd lng="de">Argentinien</trd>
  <trd lng="en">Argentina</trd>
  <trd lng="es">Argentina</trd>
  <trd lng="fa">&a_A_madda;&a_r;&f_zh;&a_A;&a_n;&a_t;&f_y;&a_n;</trd>
  <trd lng="fi">Argentiina</trd>
  <trd lng="fr">Argentine</trd>
  <trd lng="fy">Argentynje</trd>
  <trd lng="id">Argentina</trd>
  <trd lng="io">Arjentinia</trd>
  <trd lng="it">Argentina</trd>
  <trd lng="ja">&#12450;&#12523;&#12476;&#12531;&#12481;&#12531;</trd>
  <trd lng="nl">Argentini&euml;</trd>
  <trd lng="pl">Argentyna</trd>
  <trd lng="pt">Argentina</trd>
  <trd lng="ru">&c_A;&c_r;&c_g;&c_je;&c_n;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">Argent&iacute;na</trd>
  <trd lng="sl">Argentina</trd>
  <trd lng="sv">Argentina</trd>
  <trd lng="tp">ma Alensina</trd>
  <trd lng="uk">&c_A;&c_r;&c_g;&c_je;&c_n;&c_t;&c_ib;&c_n;&c_a;</trd>
  <trd lng="vo">Larg&auml;nt&auml;n</trd>
</drv>

<drv mrk="argentin.0a">
   <kap><tld lit="a"/>a</kap>
   <snc mrk="argentin.0a.GOEG">
     <dif>
       Rilata al <tld/>o.
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>Argentinian</trd>,
     <trd>Argentine</trd>,
     <trd>Argentinean</trd>
   </trdgrp>
   <trd lng="fy">Argentynsk</trd>
   <trdgrp lng="io">
     <trd>Arjentiniana</trd>,
     <trd>Arjentina</trd>
   </trdgrp>
   <trd lng="nl">Argentijns</trd>
   <trd lng="vo">Larg&auml;nt&auml;nik</trd>
</drv>

<drv mrk="argentin.0ano">
   <kap><tld lit="a"/>ano</kap>
   <snc mrk="argentin.0ano.GEOG">
     <dif>
       Civitano de <tld/>o.
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>Argentinian</trd>,
     <trd>Argentine</trd>,
     <trd>Argentinean</trd>
   </trdgrp>
   <trd lng="fy">Argentyn</trd>
   <trdgrp lng="io">
     <trd>Arjentiniano</trd>,
     <trd>Arjentino</trd>
   </trdgrp>
   <trd lng="nl">Argentijn</trd>
   <trd lng="tp">jan Alensina</trd>
   <trd lng="vo">Larg&auml;nt&auml;nan</trd>
</drv>
</art>

