<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: omer.xml,v 1.9 2020/12/21 14:10:51 revo Exp $">
<kap>
  <rad>omer</rad>/o
</kap>
<drv mrk="omer.0o">
  <kap><tld/>o</kap>
     <fnt><bib>ZR</bib></fnt>
     <fnt><bib>PIV1</bib></fnt>
     <fnt><bib>KrED</bib></fnt>
  <snc mrk="omer.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Malnova hebrea volumena <ref tip="lst" lst="voko:mezurunuoj"
      cel="unu.mezur0o.SCI">mezurunuo</ref>,
      ekvivalenta al <ref cel="garb.0o.AGR">garbo</ref> da greno:
      <ekz>
        unu <tld/>o estas &ccirc;irka&ubreve; 3,5 litroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Omer</trd>
  <trd lng="es">omer</trd>
  <trd lng="en">omer</trd>
  <trd lng="fr">omer</trd>
  <trd lng="hu">homer</trd>
  <trd lng="nl">omer</trd>
  <trd lng="no">omer</trd>
  <trdgrp lng="pt">
    <trd>gomor</trd>,
    <trd>omer</trd>
  </trdgrp>
</drv>
</art>

