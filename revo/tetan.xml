<art mrk="$Id: tetan.xml,v 1.24 2023/09/04 18:15:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tetan</rad>/o
</kap>
<drv mrk="tetan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Infekta, plej ofte mortiga 
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref> karakterizata per fortegaj
      konvulsioj:
      <ekz>
        vakcinado kontra&ubreve; ses malsanoj: tuberkulozo, difterio, koklu&scirc;o, morbilo, poliomjelito kaj <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39165">Edward Jenner</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_u;&c_p;&c_n;&c_ja;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>t&egrave;tan</trd>,
    <trd>t&egrave;tanus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>svalov&aacute; k&rcaron;e&ccaron;</trd>,
    <trd>tetanus</trd>
  </trdgrp>
  <trd lng="de">Tetanus</trd>
  <trd lng="en">tetanus</trd>
  <trd lng="fa">&f_k;&a_z;&a_A;&a_z;</trd>
  <trd lng="fr">t&eacute;tanos</trd>
  <trd lng="hu">tetanusz</trd>
  <trd lng="id">tetanus</trd>
  <trdgrp lng="ja">
    <trd>&#x7834;&#x50B7;&#x98A8;  [<ind>&#x306F;&#x3057;&#x3087;&#x3046;&#x3075;&#x3046;</ind>]</trd>,
    <trd>&#x5F37;&#x7E2E;  [<ind>&#x304D;&#x3087;&#x3046;&#x3057;&#x3085;&#x304F;</ind>]</trd>,
    <trd>&#x5F37;&#x76F4;&#x3051;&#x3044;&#x308C;&#x3093;  [<ind>&#x304D;&#x3087;&#x3046;&#x3061;&#x3087;&#x3063;&#x3051;&#x3044;&#x308C;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">tetanus</trd>
  <trd lng="pl">t&eogonek;&zdot;ec</trd>
  <trdgrp lng="pt">
    <trd>t&eacute;tano</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_t;&c_a;&c_n;&c_u;&c_s;</trd>
  <trd lng="sk">tetanus</trd>
  <trd lng="uk">&#x442;&#x435;&#x442;&#x430;&#x43D;&#x443;&#x441;</trd>
</drv>
</art>
