<vortaro>
<art mrk="$Id$">
<kap>
   <rad>muon</rad>/o
</kap>
<drv mrk="muon.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">FIZ</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="lepton.0o">Leptono</ref> kun maso inter
       200-oble kaj 220-oble pli granda ol tiu de
       <ref cel="elektr14.0o">elektrono</ref>.
     </dif>
     <ref tip="vid" cel="neuxtr3.0o">ne&ubreve;trino</ref>
   </snc>
   <trd lng="be">&c_m;&c_ju;&c_o;&c_n;</trd>
   <trd lng="de">Myon</trd>
   <trd lng="fa">&a_m;&a_w;&a_y_hamza;&a_w;&a_n;</trd>
   <trd lng="fr">muon</trd>
   <trd lng="en">muon</trd>
   <trd lng="es">mu&oacute;n</trd>
   <trd lng="hu">m&uuml;on</trd>
   <trd lng="nl">muon</trd>
   <trd lng="pl">mion</trd>
  <trdgrp lng="pt">
    <trd>m&uacute;on <klr tip="amb">(f&iacute;s.)</klr></trd>,
    <trd>m&uacute;on-menos <klr tip="amb">(f&iacute;s.)</klr></trd>
  </trdgrp>
   <trd lng="ru">&c_m;&c_ju;&c_o;&c_n;</trd>
</drv>
</art>
