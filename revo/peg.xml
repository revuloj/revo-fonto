  <rad>peg</rad>/o
</kap>
<drv mrk="peg.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <snc mrk="peg.0o.pegedano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="peged.0oj"><tld/>edoj</ref>,
      birdo kun longa fortika beko, kiu, pikante
      &scirc;elon de arboj, man&gcirc;as tie lo&gcirc;antajn insektojn:
      <ekz>
        legendoj kaj rakontoj <klr>[&#8230;]</klr> pri apero de kelkaj bestoj, kiel la <tld/>o, ekzemple
        <fnt>
          <bib>Monato</bib>
          <aut>Adriana Pavani</aut>
          <vrk><url ref="https://www.monato.be/2003/008147.html">Rumanaj legendoj el
          Brazilo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; flugsciuro bezonas nestokeston, se &gcirc;i ne trovas truon, faritan de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2010/009971.php">Nestokestoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_c;&c_je;&c_l;</trd>
    <trd lng="bg">&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_ch;</trd>
    <trd lng="ca">picot</trd>
    <trd lng="de">Specht</trd>
    <trd lng="en">woodpecker</trd>
    <trd lng="hu">hark&aacute;ly</trd>
    <trd lng="nl">specht</trd>
    <trd lng="pl">dzi&eogonek;cio&lstroke;</trd>
    <trd lng="pt">pica-pau</trd>
    <trd lng="ru">&c_d;&c_ja;&c_t;&c_je;&c_l;</trd>
  </snc>
  <snc mrk="peg.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="peged.0oj"><tld/>edoj</ref>
      (<trd lng="la">Picus</trd>).
    </dif>
    <trd lng="ca">g&egrave;nere Picus</trd>
    <trd lng="cs">&zcaron;luna</trd>
    <trd lng="hu">k&uuml;ll&odblac;<klr tip="ind">
    (mad&aacute;r)</klr></trd>
    <trd lng="ru">&c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_je;
    &c_d;&c_ja;&c_t;&c_l;&c_y; <klr
    tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
  <trd lng="es">p&aacute;jaro carpintero</trd>
  <trd lng="fr">pic<klr tip="amb"> (oiseau)</klr></trd>
</drv>

<drv mrk="peg.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="peg.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Piciformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">piciformes</trd>
  <trd lng="cs">&scaron;plhavci</trd>
  <trd lng="de">Spechtv&ouml;gel</trd>
  <trd lng="hu">hark&aacute;lyform&aacute;j&uacute;ak</trd>
  <trd
  lng="ru">&c_d;&c_ja;&c_t;&c_l;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">&scaron;plhavci</trd>
</drv>

