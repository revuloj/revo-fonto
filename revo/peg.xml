  <rad>peg</rad>/o
</kap>
<drv mrk="peg.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <snc mrk="peg.0o.pegedano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="peged.0oj"><tld/>edoj</ref>,
      birdo kun longa fortika beko, kiu, pikante &scirc;elon de arboj,
      man&gcirc;as tie lo&gcirc;antajn insektojn:
      <ekz>
        Pego, sur-abie, &#x2AFD; Frapetas, poste flugas, malaperas ie
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        legendoj kaj rakontoj <klr>[&#x2026;]</klr> pri apero de kelkaj bestoj, kiel la <tld/>o, ekzemple
        <fnt>
          <aut>Adriana Pavani</aut>
          <vrk><url ref="&Monato;/2003/008147.html">Rumanaj legendoj el
          Brazilo</url></vrk>
          <bib>Monato</bib>
          <lok>2003/12, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; flugsciuro bezonas nestokeston, se &gcirc;i ne trovas truon, faritan de <tld/>oj
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2010/009971.php">Nestokestoj</url></vrk>
          <bib>Monato</bib>
          <lok>2010/03, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_ch;</trd>
    <trd lng="ca">picot</trd>
    <trd lng="de">Specht</trd>
    <trd lng="en">woodpecker</trd>
    <trd lng="hu">hark&aacute;ly</trd>
    <trd lng="nl">specht</trd>
    <trd lng="pl">dzi&eogonek;cio&lstroke;</trd>
    <trd lng="pt">pica-pau</trd>
    <trd lng="ru">&c_d;&c_ja;&c_t;&c_je;&c_l;</trd>
  </snc>
  <snc mrk="peg.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="peged.0oj"><tld/>edoj</ref>
      (<trd lng="la">Picus</trd>).
    </dif>
    <trd lng="ca">g&egrave;nere Picus</trd>
    <trd lng="cs">&zcaron;luna</trd>
    <trd lng="hu">k&uuml;ll&odblac;<klr tip="ind">
    (mad&aacute;r)</klr></trd>
    <trd lng="ru">&c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_je;
    &c_d;&c_ja;&c_t;&c_l;&c_y; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_c;&c_je;&c_l;</trd>
  <trd lng="es">p&aacute;jaro carpintero</trd>
  <trd lng="fr">pic<klr tip="amb"> (oiseau)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30C4;&#x30C4;&#x30AD;</trd>,
    <trd>&#x30A2;&#x30AA;&#x30B2;&#x30E9;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_ja;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_zh;&c_o;&c_v;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="peg.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="peg.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Piciformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_t;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">piciformes</trd>
  <trd lng="cs">&scaron;plhavci</trd>
  <trd lng="de">Spechtv&ouml;gel</trd>
  <trd lng="hu">hark&aacute;lyform&aacute;j&uacute;ak</trd>
  <trd lng="ru">&c_d;&c_ja;&c_t;&c_l;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">&scaron;plhavci</trd>
  <trd lng="uk">&c_d;&c_ja;&c_t;&c_l;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
</drv>

