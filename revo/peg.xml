  <rad>peg</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="peg.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <snc mrk="peg.0o.pegedano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="peged.0oj"><tld/>edoj</ref>,
      birdo kun longa fortika beko, kiu, pikante &scirc;elon de arboj,
      man&gcirc;as tie lo&gcirc;antajn insektojn:
      <ekz>
        Pego, sur-abie, &#11005; Frapetas, poste flugas, malaperas ie
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        legendoj kaj rakontoj <klr>[&#8230;]</klr> pri apero de kelkaj bestoj, kiel la <tld/>o, ekzemple
        <fnt>
          <aut>Adriana Pavani</aut>
          <vrk><url ref="&Monato;/2003/008147.html">Rumanaj legendoj el
          Brazilo</url></vrk>
          <bib>Monato</bib>
          <lok>2003/12, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; flugsciuro bezonas nestokeston, se &gcirc;i ne trovas truon, faritan de <tld/>oj
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2010/009971.php">Nestokestoj</url></vrk>
          <bib>Monato</bib>
          <lok>2010/03, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_ch;</trd>
    <trdgrp lng="ca">
      <trd>picot</trd>,
      <trd>pigot</trd>
    </trdgrp>
    <trd lng="cv">&c_u;&c_p;&c_a; &c_t;&c_a;&c_k;&c_k;&c_a;</trd>
    <trd lng="de">Specht</trd>
    <trd lng="en">woodpecker</trd>
    <trd lng="hu">hark&aacute;ly</trd>
    <trd lng="nl">specht</trd>
    <trd lng="pl">dzi&eogonek;cio&lstroke;</trd>
    <trd lng="pt">pica-pau</trd>
    <trd lng="ru">&c_d;&c_ja;&c_t;&c_je;&c_l;</trd>
    <trdgrp lng="zh">
    <trd>&#21828;&#26408;&#40165; <pr>zhu&oacute;m&ugrave;ni&#462;o</pr></trd>,
    <trd>&#21828;&#26408;&#40479; <pr>zhu&oacute;m&ugrave;ni&#462;o</pr></trd>,
    <trd>&#19733; <pr>li&egrave;</pr></trd>,
    <trd>&#40247; <pr>li&egrave;</pr></trd>,
    <trd>&#40247; <pr>li&egrave;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="peg.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="peged.0oj"><tld/>edoj</ref>
      (<trd lng="la">Picus</trd>).
    </dif>
    <trd lng="ca">g&egrave;nere Picus</trd>
    <trd lng="cs">&zcaron;luna</trd>
    <trd lng="en">Picus <klr>(genus)</klr></trd>
    <trd lng="hu">k&uuml;ll&odblac;<klr tip="ind">
    (mad&aacute;r)</klr></trd>
    <trd lng="ru">&c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_je;
    &c_d;&c_ja;&c_t;&c_l;&c_y; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#21828;&#26408;&#40165; <pr>zhu&oacute;m&ugrave;ni&#462;o</pr></trd>,
    <trd>&#21828;&#26408;&#40479; <pr>zhu&oacute;m&ugrave;ni&#462;o</pr></trd>
  </trdgrp>
</snc>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_c;&c_je;&c_l;</trd>
  <trd lng="es">p&aacute;jaro carpintero</trd>
  <trd lng="fr">pic<klr tip="amb"> (oiseau)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12461;&#12484;&#12484;&#12461;</trd>,
    <trd>&#12450;&#12458;&#12466;&#12521;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_ja;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_zh;&c_o;&c_v;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

