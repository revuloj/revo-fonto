<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>peg</rad>/o
</kap>
<drv mrk="peg.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="peg.0o.pegedano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="peged.0oj"><tld/>edoj</ref>,
      birdo kun longa fortika beko, kiu, pikante
      &scirc;elon de arboj, man&gcirc;as tie lo&gcirc;antajn insektojn.
    </dif>
    <trd lng="de">Specht</trd>
    <trd lng="en">woodpecker</trd>
    <trd lng="pl">dzi&eogonek;cio&lstroke;</trd>
    <trd lng="ru">&c_d;&c_ja;&c_t;&c_je;&c_l;</trd>
  </snc>
  <snc mrk="peg.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="peged.0oj"><tld/>edoj</ref>
      (<trd lng="la">Picus</trd>).
    </dif>
    <trd lng="cs">&zcaron;luna</trd>
    <trd lng="hu">kaj&uuml;ll&odacute;</trd>
  </snc>
</drv>

<drv mrk="peg.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Piciformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Spechtv&ouml;gel</trd>
</drv>
</art>
