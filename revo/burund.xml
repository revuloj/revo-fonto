<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Burund</rad>/o
</kap>

<drv mrk="burund.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="burund.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Bu&jcirc;umburo.
      Teritorio de Respubliko Burundo.
      ISO-3166 kodo: BI.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="ruand.R0o.GEOG">Ruando</ref>
      kaj <ref tip="vid" cel="tanzani.T0o.GEOG">Tanzanio</ref>.
    </dif>
    <trd lng="be">&c_B;&c_u;&c_r;&c_u;&c_n;&c_d;&c_z;&c_ib;</trd>
    <trd lng="bg">&c_B;&c_u;&c_r;&c_u;&c_n;&c_d;&c_i;</trd>
    <trd lng="de">Burundi</trd>
    <trd lng="en">Burundi</trd>
    <trdgrp lng="fr">
      <trd>Burundi</trd>,
      <trd>R&eacute;publique du Burundi</trd>
    </trdgrp>
    <trd lng="hu">Burundi</trd>
    <trdgrp lng="pl">
      <trd>Burundi</trd>,
      <trd>Republika <ind>Burundi</ind></trd>
    </trdgrp>
    <trd lng="pt">Burundi</trd>
  </snc>
</drv>

</art>

