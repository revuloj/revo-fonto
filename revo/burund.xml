<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: burund.xml,v 1.18 2023/10/29 19:11:04 revo Exp $">
<kap>
  <rad>Burund</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="burund.B0o">
  <kap>
    <tld lit="B"/>o
  </kap>
  <mlg kod="ISO-3166">BI</mlg>
  <snc mrk="burund.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Bu&jcirc;umburo &gcirc;is la jaro 2018, nun Gitego; teritorio de Respubliko <tld/>o,
      limanta kun
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="ruand.R0o.GEOG">Ruando</ref>
      kaj <ref tip="vid" cel="tanzani.T0o.GEOG">Tanzanio</ref>:
      <ekz>
        la armeoj de Ruando, Ugando kaj Burundo sendis siajn militistojn por militi apud
        la kongaj ribelaj armeoj kontra&ubreve; la konga armeo
        <fnt>
          <bib>Monato</bib>
          <aut>Serge Zandandu Ntomono Zola</aut>
          <vrk><url ref="&Monato;/2003/007982.html">Disrabado de naturaj ri&ccirc;a&jcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o estas tri triboj: tucioj, kiuj reprezentas &ccirc;. 15&#x202F;% de la popolo,
        hutuoj, kiuj reprezentas 84&#x202F;%, kaj tvaoj, kiuj reprezentas 1&#x202F;%
        <fnt>
          <bib>Monato</bib>
          <aut>Sabiyumva J&eacute;r&eacute;mie</aut>
          <vrk><url ref="&Monato;/2004/008387.html">Survoje al paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o la biciklo i&gcirc;as kutima kaj plej &scirc;atata transportilo de granda parto
        de la lo&gcirc;antaro
        <fnt>
          <bib>Monato</bib>
          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="&Monato;/2009/009690.php">Pendole pedale</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        strikis pro malbonaj laborkondi&ccirc;oj kuracistoj kaj flegistoj &ccirc;e publikaj
        malsanulejoj en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="&Monato;/2009/009691.php">Malsana servo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_B;&c_u;&c_r;&c_u;&c_n;&c_d;&c_z;&c_ib;</trd>
    <trd lng="bg">&c_B;&c_u;&c_r;&c_u;&c_n;&c_d;&c_i;</trd>
    <trd lng="de">Burundi</trd>
    <trd lng="en">Burundi</trd>
    <trdgrp lng="fr">
      <trd>Burundi</trd>,
      <trd>R&eacute;publique du Burundi</trd>
    </trdgrp>
    <trd lng="hu">Burundi</trd>
    <trd lng="id">Burundi</trd>
    <trdgrp lng="pl">
      <trd>Burundi</trd>,
      <trd>Republika <ind>Burundi</ind></trd>
    </trdgrp>
    <trd lng="pt">Burundi</trd>
    <trdgrp lng="zh">
    <trd>&#x5E03;&#x9686;&#x8FEA; <pr>b&#xF9;l&#xF3;ngd&#xED;</pr></trd>,
    <trd>&#x84B2;&#x9686;&#x5730; <pr>p&#xFA;l&#xF3;ngd&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="he">&#x5D1;&#x5D5;&#x5E8;&#x5D5;&#x5E0;&#x5D3;&#x5D9;</trd>
  <trd lng="ja">&#x30D6;&#x30EB;&#x30F3;&#x30B8;</trd>
  <trd lng="uk">&#x411;&#x443;&#x440;&#x443;&#x43D;&#x434;&#x456;</trd>
</drv>

</art>

