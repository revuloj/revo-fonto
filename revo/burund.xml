<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>burund</rad>/o
</kap>

<drv mrk="burund.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="burund.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Bu&jcirc;umburo.
      Teritorio de Respubliko Burundo.
      ISO-3166 kodo: BI.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kong.0o.kinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="ruand.0o.GEOG">Ruando</ref>
      kaj <ref tip="vid" cel="tanzani.0o.GEOG">Tanzanio</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Burundi</trd>,
      <trd>Republika <ind>Burundi</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

