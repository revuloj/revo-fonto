<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vanil</rad>/o
</kap>
<drv mrk="vanil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="orkidac.0oj">orkidacoj</ref>
      (<trd lng="la">Vanilla planifolia</trd>),
      kies grajnujon oni uzas kiel parfumigan spica&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Vanille</trd>
  <trd lng="en">vanilla</trd>
  <trd lng="es">vainilla</trd>
  <trd lng="fr">vanille</trd>
  <trd lng="nl">vanille</trd>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_i;&c_l;&c_mol;</trd>
  <trd lng="tr">vanilya</trd>
</drv>
</art>
