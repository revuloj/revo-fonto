<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plutoni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="plutoni.0o">
  <kap><tld/>o</kap>
  <mlg>Pu</mlg>
  <snc mrk="plutoni.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 94, atompezo 244,0642, simbolo <ctl>Pu</ctl>.
      &Ccirc;iuj izotopoj de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_t;&c_o;&c_n;&c_ib;&c_j;</trd>
  <trd lng="de">Plutonium</trd>
  <trd lng="fr">plutonium</trd>
  <trd lng="hu">plut&oacute;nium</trd>
  <trd lng="nl">plutonium</trd>
  <trd lng="ru">&c_p;&c_l;&c_u;&c_t;&c_o;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
