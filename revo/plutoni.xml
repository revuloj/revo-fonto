<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plutoni</rad>/o
</kap>
<drv mrk="plutoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 94, atompezo 244,0642, simbolo Pu. <ref tip="super"
      cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu elemento
      estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Plutonium</trd>
  <trd lng="nl">plutonium</trd>
  <trd lng="ru">&c_p;&c_l;&c_u;&c_t;&c_o;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
