<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>frukt</rad>/o
</kap>
<drv mrk="frukt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frukt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;e <ref tip="malprt" cel="flor.0oplantoj">floroplantoj</ref>,
      produkto esti&gcirc;inta el fekundi&gcirc;inta floro
      enhavanta la <ref tip="prt" cel="grajn.0o.BOT">grajnojn</ref>:
      <ekz>
        la&ubreve; la <tld/>o oni arbon ekkonas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        porti <klr>(produkti)</klr> fruktojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frukt.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      &Ccirc;iu produkto devenanta de la tero a&ubreve; de la vivantaj
      estuloj:
      <ekz>
        la <tld/>oj de la tero, de brutaro;
      </ekz>
      <ekz>
        la infano, <tld/>o de la patrina ventro.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frukt.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rezulto:
      <ekz>
        lernado sen <tld/>oj ne restas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        mizero estas ofte <tld/>o de mallaboremo;
      </ekz>
      <ekz>
        la lasta kongreso donis gravajn <tld/>ojn;
      </ekz>
      <ekz>
        la <tld/>oj de la sperto de miljaroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o de l' imago<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>plod</trd>,
    <trd>ovoce</trd>
  </trdgrp>
  <trd lng="de">Frucht</trd>
  <trd lng="en">fruit</trd>
  <trd lng="fr">fruit</trd>
</drv>
<drv mrk="frukt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Doni <tld/>ojn a&ubreve; idojn:
      <ekz>
        la Izraelidoj <tld/>is <klr>(naskis)</klr> kaj diskreskis
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Eliro 1, 7</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Doni rezultaton, sukcesi:
      <ekz>
        la plano bone <tld/>is.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">fructifier</trd>
</drv>
<drv mrk="frukt.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unuopa maturi&gcirc;inta ovario de
      <ref tip="malprt" cel="frukt.plur0o">plur<tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="de">Fr&uuml;chtchen</trd>
</drv>
<drv mrk="frukt.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi multe<tld/>a:
      <ekz>
        <tld/>iga superakvo.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="frukt.0odona0oporta">
  <kap><tld/>odona, <tld/>oporta</kap>
  <snc>
    <dif>
      Ri&ccirc;a, donanta  multajn <tld/>ojn.
    </dif>
    <ref tip="sin" cel="fekund.0a">fekunda</ref>
  </snc>
  <trd lng="de">fruchtbar</trd>
</drv>
<drv mrk="frukt.sen0a">
  <kap>sen<tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ne <tld/>odona.
    </dif>
  </snc>
  <snc>
    <dif>
      Vana, senrezultata.
    </dif>
  </snc>
</drv>
<drv mrk="frukt.0uzo">
  <kap><tld/>uzo</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Le&gcirc;a rajto la&ubreve; kiu iu povas utiligi aferon, poseda&jcirc;on,
      hereda&jcirc;on, prenante por sia profito nur la produkta&jcirc;ojn
      a&ubreve; rentojn, sed ne tu&scirc;ante la bienon a&ubreve; la kapitalon.
    </dif>
  </snc>
  <trd lng="fr">usufruit</trd>
</drv>
<drv mrk="frukt.fendo0o">
  <kap>fendo<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Formo de <ref tip="super" cel="nuks.0o.BOT">nukso</ref>,
      &ccirc;e kiu la fruktofolioj disi&gcirc;as &ccirc;e la
      maturi&gcirc;o enhavantaj po unu grajnon, ekz.
      &ccirc;e acero.
    </dif>
  </snc>
  <trd lng="de">Spaltfrucht</trd>
</drv>
<drv mrk="frukt.ferm0o">
  <kap>ferm<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT"><tld lit="F"/>o</ref> kiu restas
      fermita &ccirc;e maturi&gcirc;o kaj distribui&gcirc;as kun la
      semgrajnoj.
    </dif>
  </snc>
  <bld lok="../bld/frukt1.png">
    fermfruktoj<klr>: 1. nukso, 2. bero, 3. fendo<tld/>o, 4. drupo</klr>
  </bld>
  <trd lng="de">Schlie&szlig;frucht</trd>
</drv>
<drv mrk="frukt.0ofolio">
  <kap><tld/>ofolio</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Parto de <ref tip="malprt" cel="flor.0o.BOT">floro</ref>,
      sur kiu formi&gcirc;as la ovuloj. Ili formas ujon &ccirc;e
      la magnolioplantoj, sed estas malfermitaj &ccirc;e la
      koniferoplantoj.
    </dif>
    <ref tip="sin" cel="karpel.0o">karpelo</ref>
  </snc>
  <trd lng="de">Fruchtblatt</trd>
</drv>
<drv mrk="frukt.malferm0o">
  <kap>malferm<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT"><tld lit="F"/>o</ref> kiu
      &ccirc;e maturi&gcirc;o malfermi&gcirc;as kaj elfaligas la
      semgrajnojn.
    </dif>
  </snc>
  <bld lok="../bld/frukt2.png">
    malfermfruktoj<klr>: 1. foliklo, 2. silikvoj, 3. kapsulo,
    4. gu&scirc;o<tld/>o</klr>
  </bld>
  <trd lng="de">&Ouml;ffnungsfrucht</trd>
</drv>
<drv mrk="frukt.plur0o">
  <kap>plur<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT"><tld lit="F"/>o</ref>
      esti&gcirc;inta el pluraj liberaj pistiloj.
    </dif>
  </snc>
  <trd lng="de">Sammelfrucht</trd>
</drv>
</art>
