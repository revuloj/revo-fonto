<drv mrk="frukt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frukt.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;e <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>,
      produkto esti&gcirc;inta el fekundi&gcirc;inta floro
      enhavanta la <ref tip="prt" cel="grajn.0o.BOT">grajnojn</ref>:
      <ekz>
        <ind>la&ubreve; la <tld/>o oni arbon ekkonas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">on reconnait l'arbre &agrave; ses fruits</trd>
        <trd lng="hu">gy&uuml;m&ouml;lcs&eacute;r&odblac;l ismerni meg a f&aacute;t</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_o;&c_d;</trd>,
      <trd>&c_f;&c_r;&c_u;&c_k;&c_t;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_l;&c_o;&c_d;</trd>
    <trd lng="de">St&uuml;ck <ind>Obst</ind></trd>
    <trd lng="hu">term&eacute;s</trd>
    <trd lng="nl">stuk <ind>fruit</ind></trd>
  <trdgrp lng="pt">
    <trd>fruto</trd>,
    <trd>carpo</trd>
  </trdgrp>
    <trd lng="ru">&c_f;&c_r;&c_u;&c_k;&c_t;</trd>
  </snc>
  <snc mrk="frukt.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <tld/>o <sncref ref="frukt.0o.BOT"/> a&ubreve;
      plur<tld/>o:
      <ekz>
        pomo, avelo, frago, kukumo;
      </ekz>
      <ekz>
        la &gcirc;ardenisto alportis <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        kreskigu <klr>[&#8230;]</klr> fruktarbon, kiu donas la&ubreve; sia speco <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:11</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="mangx.0ajxo">man&gcirc;a&jcirc;o</ref>
    <ref tip="vid" cel="legom.0o">legomo</ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_o;&c_d;</trd>,
      <trd>&c_f;&c_r;&c_u;&c_k;&c_t;</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>fruta</trd>,
    <trd>fruto</trd>
  </trdgrp>
  </snc>
  <snc mrk="frukt.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      &Ccirc;iu produkto devenanta de la tero a&ubreve; de la vivantaj
      estuloj:
      <ekz>
        la fruktoj de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 7:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        bongustaj mar<tld/>oj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        via filino povus porti <tld/>ojn
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_l;&c_o;&c_d;</trd>
    <trd lng="pt">fruto</trd>
  </snc>
  <snc mrk="frukt.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="rezult.0o">Rezulto</ref>:
      <ekz>
        vi ne hezitus <klr>[&#8230;]</klr> ekspluati la <tld/>ojn de mia gajno
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Dua Mondmilito <klr>[&#8230;]</klr> ruinigis la <tld/>ojn de multjara strebado
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tia stulta&jcirc;o povus esti <tld/>o de &scirc;ia malsana cerbo
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>lernado sen <tld/>oj ne restas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">a tanul&aacute;s meghozza gy&uuml;m&ouml;lcs&eacute;t</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uz.frukt0o">fruktuzo</ref>,
      <ref cel="konsek1.0o">konsekvenco</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_l;&c_jo;&c_n;</trd>
    <trd lng="pt">fruto</trd>
  </snc>
