<drv mrk="frukt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="frukt.0i.AGR">
    <dif>
      Doni <tld/>ojn a&ubreve; idojn:
      <ekz>
        la Izraelidoj <tld/>is <klr>(naskis)</klr> kaj diskreskis
        <fnt><bib>MT</bib>, <lok>&Eli; 1:7</lok></fnt>;
      </ekz>
      <ekz>
        la citronarbo <klr>[&#8230;]</klr> komencis de si mem <tld/>i, tio signifas, ke &gcirc;i
        i&gcirc;is <tld/>odona arbo
        <fnt>
          <bib>Monato</bib>
          <aut>Sigmond J&uacute;lia</aut>
          <vrk><url ref="&Monato;/2007/008896.php">Sep citronsemoj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frukt.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Doni rezultaton, <ref tip="super" cel="sukces.0i">sukcesi</ref>:
      <ekz>
        vidu, mia plano estas tia, kaj mi esperas, ke &gcirc;i bone <tld/>os
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu ser&ccirc;ado ne <tld/>is
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        la interparolo <tld/>is ne multe
        <fnt>
          <bib>ChC</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; 
    <ind>&c_p;&c_l;&c_a;&c_d;&c_y;</ind></trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; 
    <ind>&c_p;&c_l;&c_jo;&c_n;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&eacute;st ovoce</trd>,
    <trd>plodit</trd>,
    <trd>semenit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bear fruit</trd>,
    <trd>fruit</trd>
  </trdgrp>
  <trd lng="es">dar <ind>fruto</ind></trd>
  <trd lng="fr">fructifier</trd>
  <trdgrp lng="hu">
    <trd>gy&uuml;m&ouml;lcs&ouml;zik</trd>,
    <trd>gy&uuml;m&ouml;lcs&ouml;t ad</trd>,
    <trd>gy&uuml;m&ouml;lcs&ouml;t hoz</trd>,
    <trd>term&eacute;st hoz</trd>
  </trdgrp>
  <trd lng="nl">vruchten dragen</trd>
  <trdgrp lng="pl">
    <trd>owocowa&cacute;</trd>,
    <trd>dawa&cacute; owoce</trd>,
    <trd>rodzi&cacute; owoce</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_d;&c_o;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol; 
    &c_p;&c_l;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; 
    &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nies&tcaron; ovocie</trd>,
    <trd>plodi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x43E;&c_d;&#x43E;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43B;&#x43E;&c_d;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43F;&#x43B;&c_ib;&c_d;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&#x43F;&#x43B;&#x43E;&c_d;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i; &#x43F;&#x43B;&#x43E;&c_d;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43B;&#x43E;&c_d;&c_i;/&#x43D;&c_a;&c_s;&#x43B;&c_ib;&c_d;&#x43A;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="frukt.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -a&jcirc;'</lok></fnt></kap>
  <snc>
    <dif>
      <ref tip="malprt" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref> farita el
      <tld/>oj:
      <ekz>
        <tld/>a&jcirc;oj, kiel ekzemple <tld/>oj sekigitaj, fruktaj gelatenoj, marmeladoj,
        konfita&jcirc;oj, <tld/>aj sukoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        far&ccirc;o <klr>[estas]</klr> hakita kaj spicita viando, legoma&jcirc;o a&ubreve; <tld/>a&jcirc;o, kiun
        oni enmetas en la internon de baka&jcirc;o a&ubreve; kuirota legomo a&ubreve; besto
        <fnt>
          <vrk><url ref="https://dvd.ikso.net/vortaro/Hejma_vortaro.html">Hejma Vortaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">fruit product</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_v;&c_a; &c_z; &c_f;&c_r;&c_u;&#x43A;&c_t;&c_ib;&c_v;</trd>
</drv>

<drv mrk="frukt.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="frukt.0ero.KOMUNE">
    <dif>
      Ero de <tld/>o:
      <ekz>
        paste&ccirc;o el <tld/>eroj
        <fnt>
          <vrk><url
          ref="https://fb2.top/trans-la-spegulo-kaj-kion-alico-trovis-tie-509331/read">Trans la
          spegulo kaj kion Alico trovis tie</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Fruchtst&uuml;ck</trd>
    <trd lng="en">piece of fruit</trd>
  </snc>
  <snc mrk="frukt.0ero.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Unuopa maturi&gcirc;inta ovario de
      <ref tip="malprt" cel="frukt.plur0o">plur<tld/>o</ref>:
      <ekz>
        partigebla fendo<tld/>o, kies <tld/>eroj estas karakterizaj flugilhavaj nuksoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19588">Aceracoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Fr&uuml;chtchen</trd>
  <trd lng="hu">r&eacute;szterm&eacute;s</trd>  
  <trdgrp lng="uk">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a; &c_z;&c_b;&c_ib;&c_r;&#x43D;&#x43E;&c_g;&#x43E; &#x43F;&#x43B;&#x43E;&c_d;&c_u;</trd>,
    <trd>&#x44F;&c_g;&c_ib;&c_d;&#x43A;&c_a;</trd>,
    <trd>&c_s;&c_ib;&#x43C;&#8217;&#x44F;&#x43D;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="frukt.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="frukt.0igi.AGR">
    <dif>
      Igi ke io portas <tld/>ojn:
      <ekz>
        la Plejpotenca benu vin kaj <tld/>igu vin kaj multigu vin, kaj kreskigu el vi
        amason
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 28:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        se virino <tld/>igos semon kaj naskos knabeton, tiam &scirc;i estos malpura dum sep
        tagoj
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 12:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        Nilo <klr>[&#8230;]</klr> superver&scirc;as kaj <tld/>igas
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        fonteto <klr>[&#8230;]</klr> estis <tld/>iginta la novan parkon
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>oplodnit</trd>,
    <trd>z&uacute;rodnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>befruchten</trd>,
    <trd><ind>fruchtbar</ind> machen</trd>
  </trdgrp><trdgrp lng="en">
    <trd>make fruitful</trd>,
    <trd>cause to bear fruit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rendre <ind>f&eacute;cond</ind></trd>,
    <trd>rendre <ind>fertile</ind></trd>,
    <trd>fertiliser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>term&eacute;kenny&eacute; tesz</trd>,
    <trd>megterm&eacute;keny&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">vruchtbaar maken</trd>
  <trdgrp lng="sk">
    <trd>oplodni&tcaron;</trd>,
    <trd>z&uacute;rodni&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&#x43F;&#x43B;&c_ib;&c_d;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="frukt.0odona0oporta">
  <kap><tld/>odona, <var><kap><tld/>oporta</kap></var></kap>
  <snc mrk="frukt.0odona0oporta.AGR">
    <dif>
      Donanta, portanta <tld/>ojn, 
      <ref tip="sin" cel="fekund.0a">fekunda</ref>:
      <ekz>
        &gcirc;i ne estas arbo fruktodona, vi povas <klr>[&#8230;&gcirc;in]</klr> forhaki
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 20:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio faris min <tld/>oporta en la lando de mia suferado
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:52</lok>
        </fnt>;
      </ekz>
      <ekz>
        kampoj <tld/>odonaj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 1:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oporta vinberbran&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 128:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>odona bieno
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        sankta, <tld/>odona rivero
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiom <tld/>odona estas la ekzisto de tiuj institucioj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_d;&c_a;&c_n;&c_o;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_d;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_d;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bohat&ecaron; plod&iacute;c&iacute;</trd>,
    <trd>plodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fruchtbar</trd>,
    <trd><ind>Fr&uuml;chte</ind> tragend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fertile</trd>,
    <trd>fruitful</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>f&eacute;cond</trd>,
    <trd>fertile</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>term&eacute;keny</trd>,
    <trd>gy&uuml;m&ouml;lcs&ouml;z&odblac;</trd>,
    <trd>gy&uuml;m&ouml;lcsad&oacute;</trd>
  </trdgrp>
  <trd lng="nl">vruchtbaar</trd>
  <trd lng="pl">owocny</trd>
  <trdgrp lng="ru">
    <trd>
    &c_p;&c_l;&c_o;&c_d;&c_o;&c_n;&c_o;&c_s;&c_ja;&c_shch;&c_i;&c_j;
    </trd>,
    <trd>&c_p;&c_l;&c_o;&c_d;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_d;&c_o;&c_t;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trd lng="sk">plodonosn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x43E;&c_d;&#x43E;&#x43D;&#x43E;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43B;&c_ib;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_u;&#x43B;&#x44C;&c_t;&c_a;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="frukt.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="frukt.sen0a.sterila">
    <dif>
      <klr>(pri viva&jcirc;o)</klr> Ne <tld/>odona, <ref tip="sin" cel="steril.0a.KOMUNE">sterila</ref>:
      <ekz>
        <nom>Saraj</nom> estis sen<tld/>a kaj ne havis infanon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tunaj arboj sen<tld/>aj, dufoje mortintaj
        <fnt>
          <bib>NT</bib>
          <lok>Judas 1:12</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">unfruchtbar</trd>
  </snc>
  <snc mrk="frukt.sen0a.malfekunda">
    <dif>
      <klr>(pri grundo)</klr> <ref tip="sin" cel="fekund.mal0a">Malfekunda</ref>, <ref
      cel="dezert.0a">dezerta</ref>:
      <ekz>
        la akvo estas malbona, kaj la tero estas sen<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 2:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        lando sen<tld/>a kaj ne lo&gcirc;ata
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 17:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maro ruli&gcirc;as sur la sen<tld/>an sablan grundon
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">unfruchtbar</trd>
  </snc>
  <snc mrk="frukt.sen0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="van.0a">Vana</ref>, 
      <ref tip="dif" cel="rezult1.sen0a">senrezultata</ref>:
      <ekz>
        nia alveno al vi ne montri&gcirc;is sen<tld/>a
        <fnt>
          <bib>NT</bib>
          <lok>I. Tesalonikanoj 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        fido sen faroj estas sen<tld/>a
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 2:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia ser&ccirc;ado restis same sen<tld/>a
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>a disputo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas sen<tld/>a la klopodo de viaj filozofoj por elpensi la &scirc;taton de la komuna
        beato
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="senc.sen0a.propre">sensenca<sncref/></ref>,
      <ref cel="valor.sen0a">senvalora</ref>,
      <ref cel="vant.0a.io">vanta<sncref/></ref>
    </refgrp>
  <trdgrp lng="de">
    <trd>fruchtlos</trd>,
    <trd>unn&uuml;tz</trd>,
    <trd>vergeblich</trd>
  </trdgrp></snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_p;&c_l;&c_jo;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jalov&yacute;</trd>,
    <trd>neplodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>barren</trd>,
    <trd>fruitless</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>st&eacute;rile</trd>,
    <trd>infertile</trd>
  </trdgrp>
  <trd lng="hu">term&eacute;ketlen</trd>
  <trd lng="nl">steriel</trd>
  <trd lng="pl">bezowocny</trd>
  <trd lng="ru">
  &c_b;&c_je;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>jalov&yacute;</trd>,
    <trd>neplodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&#x43F;&#x43B;&c_ib;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43D;. &#x43C;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_je;&#x43C;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="frukt.fendo0o">
  <kap>fendo<tld/>o</kap>
  <snc mrk="frukt.fendo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Formo de <ref tip="super" cel="nuks.0o.BOT">nukso</ref>,
      &ccirc;e kiu la fruktofolioj disi&gcirc;as &ccirc;e la
      maturi&gcirc;o, enhavantaj po unu grajnon, ekz.
      &ccirc;e acero:
      <ekz>
        simfitoj estas plurjaraj herboj kun <klr>[&#8230;]</klr> kvarpartaj fendo<tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19917">Boragacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spaltfrucht</trd>
  <trd lng="hu">sz&eacute;thasad&oacute; <ind>term&eacute;s</ind></trd>  
  <trd lng="nl">splijtvrucht</trd>
</drv>

<drv mrk="frukt.ferm0o">
  <kap>ferm<tld/>o</kap>
  <snc mrk="frukt.ferm0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT"><tld lit="F"/>o</ref>, kiu 
      restas fermita &ccirc;e maturi&gcirc;o kaj distribui&gcirc;as 
      kun la semgrajnoj:
      <ekz>
        al la fermfruktoj apartenas: bero, drupo, nukso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=569211">Fermfrukto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/frukt1.png">
    fermfruktoj<klr>: 1. nukso, 2. bero, 3. fendo<tld/>o, 4. drupo</klr>
  </bld>
  <trd lng="de">Schlie&szlig;frucht</trd>
  <trd lng="en">indehiscent fruit</trd>
  <trd lng="hu">z&aacute;rt <ind>term&eacute;s</ind></trd>  
</drv>

<drv mrk="frukt.malferm0o">
  <kap>malferm<tld/>o</kap>
  <snc mrk="frukt.malferm0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT"><tld lit="F"/>o</ref> kiu
      &ccirc;e maturi&gcirc;o malfermi&gcirc;as kaj elfaligas la
      semgrajnojn:
      <ekz>
        <ref tip="sub" cel="lent.0o.semo">lento</ref> <klr>[&#8230;]</klr> estas malferm<tld/>o jam konata
        en la epoko de la Malnova
        testamento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=166353">Lento</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/frukt2.png">
    malfermfruktoj<klr>: 1. foliklo, 2. silikvoj, 3. kapsulo,
    4. gu&scirc;o</klr>
  </bld>
  <trd lng="de">&Ouml;ffnungsfrucht</trd>
  <trd lng="en">dehiscent fruit</trd>
  <trd lng="hu">felny&iacute;l&oacute; <ind>term&eacute;s</ind></trd>  
</drv>

<drv mrk="frukt.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="frukt.mar0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla marviva&jcirc;o alia ol fi&scirc;o, precipe
      <ref tip="sub" cel="molusk.0oj.ZOO">molusko</ref> a&ubreve;
      <ref tip="sub" cel="krust.0uloj.ZOO">krustulo</ref>:
      <ekz>
        mar<tld/>oj ekzemple <klr>[&#8230;]</klr> omaroj, kankroj, ostroj, 
        petunkoj, mituloj, marlimakoj ktp
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 15a</lok></fnt>;
      </ekz>
      <ekz>
        la lokan tradicion reprezentos giganta paeljo <klr>[&#8230;]</klr> 
        hispania kaj speciale valencia plado el rizo, kokida&jcirc;o, 
        mar<tld/>oj
        <fnt>
          <aut>-</aut>,
          <vrk>La Centra Artikolo</vrk>,
          <lok><url ref="http://www.eventeo.net/web/">
          eventeo</url>, 1993-07-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">seekos</trd>
  <trd lng="be">&c_m;&c_o;&c_r;&c_a;&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_t;</trd>
  <trd lng="ca">marisc</trd>
  <trd lng="de">Meeresfr&uuml;chte</trd>
  <trdgrp lng="en">
    <trd>seafood</trd>,
    <trd>seafood</trd>
  </trdgrp>
  <trd lng="es">marisco</trd>
  <trd lng="fr">fruit de mer</trd>
  <trd lng="gl">marisco</trd>
  <trd lng="ko">&#xD574;&#xC0B0;&#xBB3C;</trd>
  <trd lng="nl">zeevrucht</trd>
  <trd lng="pt">fruto do mar</trd>
  <trd lng="ro">fructe de mare</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_je;&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;</trd>
</drv>

<drv mrk="frukt.plur0o">
  <kap>plur<tld/>o</kap>
  <snc mrk="frukt.plur0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT"><tld lit="F"/>o</ref>
      esti&gcirc;inta el pluraj liberaj pistiloj:
      <ekz>
        <ref tip="sub" cel="framb.0o">frambo</ref> <klr>[&#8230;]</klr> ne estas bero, sed plur<tld/>o de
        multaj drupetoj &ccirc;irka&ubreve;
        mola kerno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=72225">Frambo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_k;&c_a;</ind></trd>,
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Sammelfrucht</trd>
  <trd lng="en">compound fruit</trd>
  <trdgrp lng="hu">
    <trd>term&eacute;scsoport</trd>,
    <trd>&ouml;sszetett <ind>term&eacute;s</ind></trd>
  </trdgrp>
</drv>

<drv mrk="frukt.0ofolio">
  <kap><tld/>ofolio</kap>
  <snc mrk="frukt.0ofolio.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="flor.0o.BOT">floro</ref>,
      sur kiu formi&gcirc;as la ovuloj. Ili formas ujon &ccirc;e
      la magnolioplantoj, sed estas malfermitaj &ccirc;e la
      koniferoplantoj:
      <ekz>
        silikvo estas dufaka kapsulo <klr>[&#8230;]</klr> esti&gcirc;anta el du <tld/>ofolioj kaj
        havanta mezan muron sur kiu situas la semgrajnoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20027">Silikvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="karpel.0o">karpelo</ref>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_d;&c_a;&c_l;&c_ib;&c_s;&c_mol;&c_c;&c_ib;&c_k;</trd>
  <trd lng="cs">plodolist</trd>
  <trd lng="de">Fruchtblatt</trd>
  <trd lng="fr">carpelle</trd>
  <trd lng="hu">term&odblac;lev&eacute;l</trd>
  <trd lng="nl">vruchtblad</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_d;&c_o;&c_l;&c_i;&c_s;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">plodolist</trd>
  <trd lng="uk">&#x43F;&#x43B;&#x43E;&c_d;&#x43E;&#x43B;&c_i;&c_s;&c_t;&c_i;&#x43A;</trd>
</drv>


