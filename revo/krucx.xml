<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krucx.xml,v 1.33 2022/08/21 18:33:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kru&ccirc;</rad>/o
</kap>
<drv mrk="krucx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krucx.0o.VAZ">
    <dif>
      <ref tip="super" cel="vaz.0o">Vazo</ref> el diversaj substancoj,
      iom lar&gcirc;a, ofte ventre lar&gcirc;a, kun tenilo, ver&scirc;ilo,
      destinita por enteni flua&jcirc;on:
      <ekz>
        Arkadio trinkis akvon, kiun la mastrino de l' gastejo alportis
        al li en fera <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 2a, p. 9a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>longe &ccirc;erpas la <tld/>o, &gcirc;is &gcirc;i fine rompi&gcirc;as</ind>
        <klr>(kiu tro ofte sin riskas al dan&gcirc;ero, fine falas en
        &gcirc;in)</klr>
        <trd lng="es">tanto va el c&aacute;ntaro a la fuente que al final se
        rompe</trd>
        <trdgrp lng="fr">
          <trd>tant va la cruche &agrave; l'eau, qu'elle se casse</trd>
        </trdgrp>
        <trd lng="hu">addig j&aacute;r a kors&oacute; a k&uacute;tra, m&iacute;g el nem
        t&ouml;rik</trd>
        <trd lng="nl">de <ind>kruik</ind> gaat zo lang te water tot ze
        breekt</trd>
        <trd lng="pl">dop&oacute;ty dzban wod&eogonek; nosi, dop&oacute;ki mu si&eogonek; ucho nie 
        urwie</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_l;&c_a;&c_ch;</trd>,
    <trd>&c_z;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_f;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">gerra</trd>
  <trdgrp lng="cs">
    <trd>d&zcaron;b&aacute;n</trd>,
    <trd>konvice</trd>,
    <trd>konev</trd>,
    <trd>bandaska</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Krug</trd>,
    <trd>Kanne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jug</trd>,
    <trd>cruse</trd>,
    <trd>ewer</trd>,
    <trd>pitcher</trd>,
    <trd>pot</trd>,
    <trd>vessel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>c&aacute;ntaro</trd>,
    <trd>jarra</trd>,
    <trd>jarr&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>broc</trd>,
    <trd>cruche</trd>,
    <trd>pichet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kors&oacute;</trd>,
    <trd>kancs&oacute;</trd>,
    <trd>kanna</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kruik</trd>,
    <trd>kan<klr tip="ind"> (kruik)</klr></trd>
  </trdgrp>
  <trd lng="pl">dzbanek</trd>
  <trdgrp lng="pt">
    <trd>jarro</trd>,
    <trd>jarra</trd>,
    <trd>bilha</trd>,
    <trd>c&acirc;ntaro</trd>
  </trdgrp>
  <trd lng="ro">caraf&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>testi</trd>,
    <trd>s&uuml;rahi</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_l;&c_je;(&c_ch;&c_i;)&c_k;</trd>,
    <trd>&c_k;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_z;&c_b;&c_a;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="krucx.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc mrk="krucx.0eto.VAZ">
    <dif>
      Malgranda kru&ccirc;o,
      vazo entenanta proksimume inter 25 kaj 50 centilitroj,
      kun tenilo, uzebla kiel ver&scirc;ilo a&ubreve; rekte kiel trinkujo:
      <ekz>
        kopenhagaj bur&gcirc;oj kaj anka&ubreve; kelke da scienculoj sidis en vigla interparolado &ccirc;e
        siaj <tld/>etoj kaj malmulte atentis la eniranton
        <fnt>
          <bib>Fab1</bib>
          <vrk>Galo&scirc;oj de feli&ccirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">K&auml;nnchen</trd>
  <trdgrp lng="en">
    <trd>chope</trd>,
    <trd>cruchon</trd>,
    <trd>moque</trd>,
    <trd>mug</trd>
  </trdgrp>
</drv>

<drv mrk="krucx.benzin0o">
  <kap>
    benzin<tld/>o
  </kap>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kanistr.benzin0o.AUT">Benzinkanistro</ref>:
      <ekz>
        kiam oni trovas benzin<tld/>on da multekosta rara benzino en la domo, la preparo
        de molotovkokteloj balda&ubreve; pretas
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>
          <vrk>La Talpo&ccirc;asisto</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- evitindan formon prefere ne traduku -->
</drv>

<drv mrk="krucx.bier0o">
  <kap>
    bier<tld/>o
  </kap>
  <snc mrk="krucx.bier0o.VAZ">
    <dif>
      <ref tip="super" cel="krucx.0o"><tld lit="K"/>o</ref>
      por ricevi kaj trinki bieron:
      <ekz>
        la kelneroj disportadis nebultegitajn bier<tld/>ojn, ra&ubreve;ke kaj malame kriis:
        &leftquot;Pardonon, civitano!&rightquot;
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bierkrug</trd>
  <trdgrp lng="fr">
    <trd>bock</trd>,
    <trd>chope</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_i;&c_v;&c_n;&c_i;&c_j; &c_k;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
</drv>

<drv mrk="krucx.kaf0o">
  <kap>kaf<tld/>o,
  <var><kap>kafo<tld/>o</kap></var></kap>
  <snc mrk="krucx.kaf0o.VAZ">
    <dif>
      <ref tip="super" cel="krucx.0o"><tld lit="K"/>o</ref> por fari,
      konservi varma a&ubreve; ver&scirc;i kafon:
      <ekz>
        <nom>Stanislav</nom> tiumomente alproksimi&gcirc;is kun la kafo<tld/>o
        <fnt>
          <aut>B. Traven, trad. H. G. Kaiser </aut>
          <vrk>Mortula &scirc;ipo</vrk>
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">konvice na k&aacute;vu</trd>
  <trd lng="de">Kaffeekanne</trd>
  <trd lng="en">coffee pot</trd>
  <trd lng="ca">cafetera</trd>
  <trd lng="es">cafetera</trd>
  <trd lng="fr">cafeti&egrave;re<klr tip="amb"> (verseuse)</klr></trd>
  <trd lng="hu">k&aacute;v&eacute;skanna</trd>
  <trd lng="it">caffettiera</trd>
  <trd lng="nl">koffiekan</trd>
  <trd lng="pl">dzbanek na kaw&eogonek;</trd>     
  <trd lng="ro">cafetiere</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
  <trd lng="tr">cezve</trd>
</drv>

<drv mrk="krucx.te0o">
  <kap><ofc>*</ofc>te<tld/>o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5219">te'kru&ccirc;'</url></lok></fnt></kap>
  <snc mrk="krucx.te0o.VAZ">
    <dif>
      <ref tip="super" cel="krucx.0o"><tld lit="K"/>o</ref> por konservi
      varma kaj ver&scirc;i teon:
      <ekz>
        <nom>Umka</nom> instruis al la knabo, kiel ekbruligi la fajron, kiel meti surfajren la
        te<tld/>on kaj kiel forpreni &gcirc;in, se &gcirc;i ekbolas
        <fnt>
          <bib>VCh</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_kha;&t_naro;&t_ga;&t_tsheg;&t_la;&t_tata;&t_gigu;&t_ra;&t_tsheg;</trd>
  <trd lng="cs">konvice na &ccaron;aj</trd>
  <trd lng="de">Teekanne</trd>
  <trd lng="es">tetera</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;i&egrave;re<klr> (verseuse)</klr></trd>
  </trdgrp>
  <trd lng="hu">te&aacute;skanna</trd>
  <trd lng="ca">tetera</trd>
  <trd lng="nl">theekan</trd>
  <trdgrp lng="pl">
    <trd>czajnik</trd>,
    <trd>imbryk</trd>,
    <trd>dzbanek na herbat&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>ceainic</trd>,
    <trd>ibric</trd>,
    <trd>oal&abreve; de ceai</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_a;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;
  <ind>&c_ch;&c_a;&c_j;&c_n;&c_i;&c_k;</ind></trd>
  <trd lng="tr">&ccedil;aydanl&inodot;k</trd>
  <trd lng="uk">&c_ch;&c_a;&c_j;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
