<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kru&ccirc;</rad>/o
</kap>
<drv mrk="krucx.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Vazo el diversaj materioj, kun lar&gcirc;a ventro, tenilo kaj
      ver&scirc;ilo, destinita por enteni flua&jcirc;on:
      <ekz>longe &ccirc;erpas la <tld/>o, &gcirc;is &gcirc;i fine
        rompi&gcirc;as
        <klr>(kiu tro ofte sin riskas al dan&gcirc;ero, fine falas en
        &gcirc;in)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        kaf<tld/>o, te<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Krug</trd>,
    <trd>Kanne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jug</trd>,
    <trd>cruse</trd>,
    <trd>ewer</trd>,
    <trd>pitcher</trd>,
    <trd>pot</trd>,
    <trd>vessel</trd>
  </trdgrp>
  <trd lng="fr">cruche</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
