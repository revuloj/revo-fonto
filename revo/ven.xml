<drv mrk="ven.0onta">
  <kap><tld/>onta</kap>
  <snc mrk="ven.0onta.TEMP">
    <dif>
      Kiu <tld/>os poste, en estonta,
      <ref tip="vid" cel="proksi.0a.kiam">proksima<sncref/></ref>
      tempo, kontraste al io
      <ref tip="ant" cel="pas.0inta.TEMP">pasinta</ref>:
      <ekz>
        la <tld/>ontan semajnon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>prochain</trd>,
      <trd>&agrave; venir</trd>
    </trdgrp>
  </snc>
</drv>

