<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vert.xml,v 1.25 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vert</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vert.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vert.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Supro de <ref tip="malprt" cel="kap.0o.ANA">kapo</ref>:
      <ekz>
        la Sinjoro senharigos la <tld/>on de la filinoj de
        Cion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de la plando de liaj piedoj &gcirc;is lia <tld/>o estis en li nenia
        malla&ubreve;dinda&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        frakasi ies <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">coronell</trd>
    <trdgrp lng="en">
      <trd>pate</trd>,
      <trd>crown of the head</trd>,
      <trd>top of the head</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_f;&a_r;&a_q;<klr tip="ind"> (&a_s;&a_r;)</klr></trd>,
      <trd>&f_k;&a_l;&a_shadda;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fejtet&odblac;</trd>,
      <trd>koponyatet&odblac;</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd><ind>sommit&agrave;</ind> della testa</trd>,
      <trd>cocuzzolo</trd>
    </trdgrp>
    <trd lng="nl">kruin</trd>
    <trd lng="es">coronilla</trd>
  </snc>
  <snc mrk="vert.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="supr.0o.KOMUNE">Supro</ref>:
      <ekz>
        la luno ekstaris sur la <tld/>o de la monto.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>cim</trd>,
      <trd>&agrave;pex</trd>
    </trdgrp>
    <trd lng="en">top</trd>
    <trdgrp lng="fa">
      <trd>&a_q;&a_l;&a_h;</trd>,
      <trd>&a_s;&a_t;&f_y;&a_gh;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&uacute;cs<klr tip="ind"> (&aacute;tv)</klr></trd>,
      <trd>tet&odblac;<klr tip="ind"> (&aacute;tv)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>vertice</trd>,
      <trd>sommit&agrave;</trd>,
      <trd>cocuzzolo</trd>
    </trdgrp>
    <trd lng="nl">top</trd>
    <trd lng="es">&aacute;pice</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>t&eacute;m&ecaron;</trd>,
    <trd>vrchol</trd>
  </trdgrp>
  <trd lng="de">Scheitel</trd>
  <trdgrp lng="fr">
    <trd>point culminant</trd>,
    <trd>sommet</trd>,
    <trd>sommet du <ind>cr&acirc;ne</ind></trd>,
    <trd>vertex</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x982D;&#x9802;  <pr>&#x3068;&#x3046;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x9802;&#x70B9;  <pr>&#x3061;&#x3087;&#x3046;&#x3066;&#x3093;</pr></trd>,
    <trd>&#x9802;&#x4E0A;  <pr>&#x3061;&#x3087;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ciemi&eogonek;</trd>,
    <trd>szczyt <klr>(g&lstroke;owy)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>temeno</trd>,
    <trd>vrchol</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x456;&#x43C;&#x2019;&#x44F;</trd>,
    <trd>&#x43C;&#x430;&#x43A;&#x456;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x432;&#x435;&#x440;&#x448;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x43A;&#x443;&#x43B;&#x44C;&#x43C;&#x456;&#x43D;&#x430;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
