<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vert</rad>/o
</kap>
<drv mrk="vert.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Supro de <ref tip="malprt" cel="kap.0o.ANA">kapo</ref>:
      <ekz>
        la Sinjoro senharigos la <tld/>on de la filinoj de Cion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de la plando de liaj piedoj &gcirc;is lia <tld/>o estis en li nenia malla&ubreve;dinda&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        frakasi ies <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">kruin</trd>
    <trdgrp lng="fr">
      <trd>sommet du <ind>cr&acirc;ne</ind></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Supro:
      <ekz>
        la luno ekstaris sur la <tld/>o de la monto
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kulmin">kulmino</ref>.
      <ref cel="pint.0o">pinto</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>point culminant</trd>,
      <trd>sommet</trd>
    </trdgrp>
    <trd lng="nl">top</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_m;&c_ja;</trd>, 
    <trd>&c_m;&c_a;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">Scheitel</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
