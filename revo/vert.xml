<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vert</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vert.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vert.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Supro de <ref tip="malprt" cel="kap.0o.ANA">kapo</ref>:
      <ekz>
        la Sinjoro senharigos la <tld/>on de la filinoj de
        Cion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de la plando de liaj piedoj &gcirc;is lia <tld/>o estis en li nenia
        malla&ubreve;dinda&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        frakasi ies <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>pate</trd>,
      <trd>crown of the head</trd>,
      <trd>top of the head</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_f;&a_r;&a_q;<klr tip="ind"> (&a_s;&a_r;)</klr></trd>,
      <trd>&f_k;&a_l;&a_shadda;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fejtet&odblac;</trd>,
      <trd>koponyatet&odblac;</trd>
    </trdgrp>
    <trd lng="nl">kruin</trd>
  </snc>
  <snc mrk="vert.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Supro:
      <ekz>
        la luno ekstaris sur la <tld/>o de la monto
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kulmin.0o.supro">kulmino <sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pint.0o.ekstremo">pinto <sncref/></ref>
    </refgrp>
    <trd lng="en">top</trd>
    <trdgrp lng="fa">
      <trd>&a_q;&a_l;&a_h;</trd>,
      <trd>&a_s;&a_t;&f_y;&a_gh;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&uacute;cs<klr tip="ind"> (&aacute;tv)</klr></trd>,
      <trd>tet&odblac;<klr tip="ind"> (&aacute;tv)</klr></trd>
    </trdgrp>
    <trd lng="nl">top</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Scheitel</trd>
  <trdgrp lng="fr">
    <trd>point culminant</trd>,
    <trd>sommet</trd>,
    <trd>sommet du <ind>cr&acirc;ne</ind></trd>,
    <trd>vertex</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ciemi&eogonek;</trd>,
    <trd>szczyt <klr>(g&lstroke;owy)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
