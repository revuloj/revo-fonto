<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>jug</rad>/o
</kap>
<drv mrk="jug.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Transversa ligna peco, kiun oni ligas &ccirc;e la kornoj a&ubreve; sur la kolo de
      la bovoj, por ilin jungi.
    </dif>
    <trd lng="en">yoke</trd>
    <trdgrp lng="pt">
      <trd>canga</trd>,
      <trd>jugo</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_r;&c_m;&c_o;</trd>
  </snc>
  <snc>
    <dif>
      Transversa lanco, ligita al du aliaj fiksitaj en la tero, sub kiu la
      antikvaj Romanoj devigis la venkitojn trapasi.
    </dif>
    <trd lng="en">Roman yoke</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sklaveco, servuto, trudo de ia ajn a&ubreve;toritato:
      <ekz>
        iri sub <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        forskui, de&jcirc;eti la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kurbi&gcirc;i sub la <tld/>o de la despotismo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la decoj, devo, le&gcirc;oj;
      </ekz>
      <ekz>
        <tld/>o propravola ne estas malmola<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la popolo sin liberigis de (a&ubreve; el sub) la absolutista <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="liber">mallibera</ref>,
      <ref cel="katen.0o">kateno</ref>,
      <ref cel="karcer.0o">karcero</ref>,
      <ref cel="tiran">tiraneco</ref>,
      <ref cel="despot">despoteco</ref>,
      <ref cel="prem.0i">premi</ref>.
    </refgrp>
    <trd lng="en">subjugation</trd>
    <trd lng="pt">jugo</trd>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_r;&c_m;&c_o;</trd>,
      <trd>&c_g;&c_n;&c_jo;&c_t;</trd>,
      <trd>&c_i;&c_g;&c_o;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Joch</trd>
  <trd lng="hu">iga</trd>
  <trd lng="nl">juk</trd>
  <trd lng="sv">ok</trd>
</drv>

<drv mrk="jug.sub0igi">
  <kap>sub<tld/>igi</kap>
  <snc>
    <dif>
      Sklavigi, subpremi.
    </dif>
  </snc>
  <trd lng="de">unterjochen</trd>
  <trd lng="en">subjugate</trd>
  <trd lng="hu">leig&aacute;z</trd>
  <trd lng="nl">onderdrukken</trd>
  <trd lng="pt">escravizar</trd>
  <trd lng="ru">&c_u;&c_g;&c_n;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
