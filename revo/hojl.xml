<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hojl.xml,v 1.19 2025/01/12 10:26:22 revo Exp $">
<kap>
  <rad>hojl</rad>/i
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hojl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="hurl.0i.lupo">Hurli</ref>:
      <ekz>
        Kvar lupoj en arbar-obskuro &parsl;
        &ccirc;iu po unu virinkruro &parsl;
        <tld/>e portas for
        <fnt>
          <aut>K. J. Erben, trad. T. Pumpr</aut>:
          <vrk>Ora rad&scirc;pinilo</vrk>,
          <lok>en: Bukedo el &Ccirc;e&hcirc;aj naciaj mitoj, 1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="hurl.0i.lupo">hurli</ref>,
      <ref cel="boj.tir0i">tirboji</ref>
    </refgrp>
    <ref tip="vid" cel="ulul.0i.ZOO_lupo">ululi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&ncaron;u&ccaron;et</trd>,
    <trd>v&yacute;t</trd>
  </trdgrp>
  <trd lng="de">heulen</trd>
  <trd lng="el">&omikron;&ypsilon;&rho;&lambda;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">howl</trd>
  <trd lng="fa">&a_z;&a_w;&a_z;&a_h; &f_k;&a_sh;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">hurler<klr tip="ind"> (en p. d'un loup)</klr></trd>
  <trd lng="hu">von&iacute;t</trd>
  <trd lng="nl">huilen</trd>
  <trd lng="pt">uivar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sku&ccaron;a&tcaron;</trd>,
    <trd>zav&yacute;ja&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_p;&c_s;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_v;&c_k;&c_a; &c_ib; &c_t;.&c_p;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29378;&#21536; <pr>ku&aacute;ngf&egrave;i</pr></trd>,
    <trd>&#22158; <pr>h&aacute;o</pr></trd>,
    <trd>&#21495;&#21483; <pr>h&aacute;oji&agrave;o</pr></trd>,
    <trd>&#29436;&#21989; <pr>l&aacute;ngh&aacute;o</pr></trd>
  </trdgrp>
</drv>
<adm>
  &Ccirc;u kunigi kun hurl/ kiel var?
  <aut>WD</aut>
</adm>
</art>
