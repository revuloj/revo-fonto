<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hojl</rad>/i
</kap>
<drv mrk="hojl.0i">
  <kap><tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi longan, senpa&ubreve;zan bojon, similan al bleko de
      lupo:
      <ekz>
        Kvar lupoj en arbar-obskuro /
        &ccirc;iu po unu virinkruro /
        <tld/>e portas for
        <fnt>
        (<aut>Karel Jarom&iacute;r Erben, trad. de Tom&aacute;&scaron; Pumpr</aut>:
        <vrk>Bukedo el &Ccirc;e&hcirc;aj naciaj mitoj (1938)</vrk>,
        <lok>Ora rad&scirc;pinilo vi</lok>)
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="hurl.0i.lupo">hurli</ref>,
      <ref cel="boj.tir0i">tirboji</ref>,
      <ref cel="ulul.0i.lupo">ululi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">heulen</trd>
  <trd lng="el">&omikron;&ypsilon;&rho;&lambda;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="fa">&a_z;&a_w;&a_z;&a_h; &f_k;&a_sh;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">hurler<klr tip="ind"> (en p. d'un loup)</klr></trd>
  <trd lng="hu">von&iacute;t</trd>
  <trd lng="nl">huilen</trd>
  <trd lng="pt">uivar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
