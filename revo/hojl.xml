<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hojl.xml,v 1.17 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>hojl</rad>/i
</kap>
<drv mrk="hojl.0i">
  <kap><tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi longan, senpa&ubreve;zan bojon, similan al 
      <ref tip="lst" cel="blek.0i" lst="voko:blekoj">bleko</ref> de
      lupo:
      <ekz>
        Kvar lupoj en arbar-obskuro /
        &ccirc;iu po unu virinkruro /
        <tld/>e portas for
        <fnt>
        <aut>K. J. Erben, trad. T. Pumpr</aut>:
        <vrk>Ora rad&scirc;pinilo</vrk>,
        <lok>en: Bukedo el &Ccirc;e&hcirc;aj naciaj mitoj, 1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="hurl.0i.lupo">hurli</ref>,
      <ref cel="boj.tir0i">tirboji</ref>,
      <ref cel="ulul.0i.ZOO_lupo">ululi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&ncaron;u&ccaron;et</trd>,
    <trd>v&yacute;t</trd>
  </trdgrp>
  <trd lng="de">heulen</trd>
  <trd lng="el">&omikron;&ypsilon;&rho;&lambda;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">howl</trd>  
  <trd lng="fa">&a_z;&a_w;&a_z;&a_h; &f_k;&a_sh;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">hurler<klr tip="ind"> (en p. d'un loup)</klr></trd>
  <trd lng="hu">von&iacute;t</trd>
  <trd lng="nl">huilen</trd>
  <trd lng="pt">uivar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sku&ccaron;a&tcaron;</trd>,
    <trd>zav&yacute;ja&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x432;&#x438;&#x432;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x43F;&#x441;&#x430;</trd>,
    <trd>&#x432;&#x43E;&#x432;&#x43A;&#x430; &#x456; &#x442;.&#x43F;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x72C2;&#x5420; <pr>ku&#xE1;ngf&#xE8;i</pr></trd>,
    <trd>&#x568E; <pr>h&#xE1;o</pr></trd>,
    <trd>&#x53F7;&#x53EB; <pr>h&#xE1;oji&#xE0;o</pr></trd>,
    <trd>&#x72FC;&#x55E5; <pr>l&#xE1;ngh&#xE1;o</pr></trd>
  </trdgrp>
</drv>
</art>
