<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tern</rad>/i
</kap>
<drv mrk="tern.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Spasme kaj brue elspiri aeron per nazo kaj bu&scirc;o pro subita ekscito
      de la nazaj nervoj:
      <ekz>
        ne ekzistas <tld/>o sen nazo<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fum.0o">fumo</ref>,
      <ref cel="tus.0i">tusi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">niesen</trd>
  <trd lng="en">sneeze</trd>
  <trd lng="fr">&eacute;ternuer</trd>
  <trdgrp lng="nl">
    <trd>niezen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
