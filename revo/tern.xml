<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tern.xml,v 1.22 2024/05/10 08:31:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tern</rad>/i
</kap>
<drv mrk="tern.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Spasme kaj brue elspiri aeron per <ref cel="naz.0o">nazo</ref> kaj bu&scirc;o pro subita
      ekscito de la nazaj nervoj:
      <ekz>
        la knabo <tld/>is sep fojojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 4:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sova&gcirc;aj kreska&jcirc;oj disvastigadis tiel spican odoron, 
        ke mi devis <tld/>i
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj haroj <klr>[&#x2026;]</klr> tiel forte odoris konvalojn, 
        ke la gardostaranto devis
        <tld/>i
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi rigardas la &ccirc;ielon nur tiam, kiam mi volas <tld/>i
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        je lia apero &ccirc;iuj &scirc;ajnigis <tld/>i
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistas <tld/>o sen nazo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rukt.0i">rukti</ref>,
      <ref cel="blov.spir0i">spirblovi</ref>,
      <ref cel="tus.0i">tusi</ref>
    </refgrp>
  </snc>
  <trd lng="ar">&a_ayn;&a_T;&a_s;</trd>
  <trd lng="be">&c_ch;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">esternudar</trd>
  <trd lng="cs">k&yacute;chat</trd>
  <trd lng="cy">tisian</trd>
  <trd lng="da">nyse</trd>
  <trd lng="de">niesen</trd>
  <trd lng="en">sneeze</trd>
  <trd lng="es">estornudar</trd>
  <trd lng="et">aevastama</trd>
  <trd lng="eu">zintz egin</trd>
  <trd lng="fa">&a_ayn;&a_T;&a_s;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fi">aivastaa</trd>
  <trd lng="fr">&eacute;ternuer</trd>
  <trd lng="gd">sreothart</trd>
  <trd lng="he">&ayin;&yod;&tet;&vav;&shin;</trd>
  <trd lng="hi">&#x91B;&#x940;&#x902;&#x915;</trd>
  <trd lng="hr">kihnuti</trd>
  <trd lng="hu">t&uuml;sszent</trd>
  <trd lng="hy">&#x583;&#x57C;&#x577;&#x57F;&#x561;&#x56C;</trd>
  <trd lng="ia">sternutar</trd>
  <trd lng="it">starnutire</trd>
  <trd lng="ja">&#x304F;&#x3057;&#x3083;&#x307F;&#x3092;&#x3059;&#x308B;</trd>
  <trd lng="ka">&#x10D3;&#x10D0;&#x10EA;&#x10D4;&#x10DB;&#x10D8;&#x10DC;&#x10D4;&#x10D1;&#x10D0;</trd>
  <trd lng="ko">&#xC7AC;&#xCC44;&#xAE30;&#xD558;&#xB2E4;</trd>
  <trd lng="ml">&#xD13;&#xD2E;&#xD4D;&#xD2A;&#xD41;&#xD15;&#x200C;</trd>
  <trdgrp lng="nl">
    <trd>niezen</trd>
  </trdgrp>
  <trd lng="no">nyse</trd>
  <trd lng="pl">kicha&cacute;</trd>
  <trdgrp lng="pt">
    <trd>espirrar</trd>
  </trdgrp>
  <trd lng="ro">str&abreve;nuta</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="scn">stranutari</trd>
  <trd lng="sk">k&yacute;cha&tcaron;</trd>
  <trd lng="sl">kihniti</trd>
  <trd lng="sv">nysa</trd>
  <trd lng="sw">chichimua</trd>
  <trd lng="te">&#xC24;&#xC41;&#xC2E;&#xC4D;&#xC2E;&#xC41;</trd>
  <trd lng="th">&#xE08;&#xE32;&#xE21;</trd>
  <trd lng="tr">hap&scedil;&inodot;rmak</trd>
  <trd lng="uk">&c_ch;&c_h;&c_a;&c_t;&c_i;</trd>
  <trd lng="vi">nh&#x1EA3;y mui</trd>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x55B7;&#x568F; <pr>d&#x1CE;p&#x113;nt&#xEC;</pr></trd>,
    <trd>&#x6253;&#x5674;&#x568F; <pr>d&#x1CE;p&#x113;nt&#xEC;</pr></trd>,
    <trd>&#x568F; <pr>t&#xEC;</pr></trd>,
    <trd>&#x55B7;&#x568F; <pr>p&#x113;nt&#xEC;</pr></trd>,
    <trd>&#x5674;&#x568F; <pr>p&#x113;nt&#xEC;</pr></trd>,
    <trd>&#x568F;&#x55B7; <pr>t&#xEC;fen</pr></trd>,
    <trd>&#x568F;&#x5674; <pr>t&#xEC;fen</pr></trd>,
    <trd>&#x6253;&#x568F;&#x55B7; <pr>d&#x1CE;t&#xEC;pen</pr></trd>,
    <trd>&#x6253;&#x568F;&#x5674; <pr>d&#x1CE;t&#xEC;pen</pr></trd>
  </trdgrp>
</drv>
</art>
