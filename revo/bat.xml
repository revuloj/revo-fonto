<drv mrk="bat.0">
  <kap><tld/>!</kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="bat.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>o, de io subite trafanta ion;
      <ref tip="vid" cel="bac.0.SON">bac</ref>,
      <ref tip="vid" cel="frap.0.SON">frap</ref>:
      <ekz>
        a&ubreve; &ccirc;u venas, sur aero, ripetata frapo? /
        pulso-frapo-pulsofrapo- <tld/>' <tld/>' <tld/>'
        <fnt>
          <aut>C. N. Newell</aut>,
          <vrk>Afrika nokto</vrk>,
          <lok><url ref="http://literaturamondo.org/PDF/1937/LM%201937-05c.pdf">
          Literatura Mondo, 1937:5</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>&#xA1;bam!</trd>,
    <trd>&#xA1;paf!</trd>
  </trdgrp>
  <trd lng="fr">paf !</trd>
</drv>

