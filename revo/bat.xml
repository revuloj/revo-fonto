<drv mrk="bat.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bat.dis0i.KOMUNE">
    <dif>
      <tld lit="B"/>i por disigi:
      <ekz>
        dis<tld/>i muron;
      </ekz><ekz>
        el la tero dis<tld/>ita de la piedoj fari&gcirc;is
        mar&ccirc;o<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        li revenis dis<tld/>ita ne nur korpe sed anka&ubreve; anime;
      </ekz><ekz>
        &ccirc;iuj liaj revoj estis krude dis<tld/>itaj;
      </ekz><ekz>
        lia spirito estis dis<tld/>ita per kontra&ubreve;aj pasioj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bat.dis0i.venki">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Komplete, definitive
      <ref tip="super" cel="venk.0i.batale">venki<sncref/></ref>:
      <ekz>
        La krakova ribelo estis dis<tld/>ita komence de marto 1846
        <fnt>
          <aut>K. Markso kaj F. Engelso, tr. Vilhelmo Lutermano</aut>:
          <vrk>Manifesto de la Komunista Partio</vrk>.
          <lok>Monda Asembleo Socia, 2015, ISBN 978-2-918300-91-5</lok>
        </fnt>;
      </ekz><ekz>
        oni rajtas re<tld/>i kaj dis<tld/>i la ideojn kaj argumentojn
        de la a&ubreve;toro
        <fnt><aut>(F.R. Banham</aut>: <vrk>Recenza relativeco</vrk>.
          <lok>Nica Literatura Revuo, 3/4</lok></fnt>;
      </ekz><ekz>
        jen kurioza&jcirc;o: li &gcirc;isfunde dis<tld/>is &ccirc;iujn kvin pruvojn &mdash;
        kaj poste <klr>&#x2026;</klr> konstruis sian propran, sesan pruvon!
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 1&#xAA;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>rozb&iacute;t</trd>,
    <trd>rozklepat</trd>,
    <trd>rozrazit</trd>
  </trdgrp>
  <trd lng="de">zerschlagen</trd>
  <trd lng="en">smash</trd>
  <trdgrp lng="es">
    <trd>destrozar</trd>,
    <trd>destruir</trd>,
    <trd>hacer astillas</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>briser</trd>,
    <trd>rouer<klr tip="ind"> (de coups)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&nun;&tav;&ftsadi;</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;t&uuml;t</trd>,
    <trd>sz&eacute;tver</trd>,
    <trd>t&ouml;nkrez&uacute;z</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozbija&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;</trd>
    <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozbi&tcaron;</trd>
</drv>

