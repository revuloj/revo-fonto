<drv mrk="bat.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="bat.0ilo.KOMUNE">
    <dif>
      Objekto uzata por <tld/>i.
    </dif>
    <subsnc mrk="bat.0ilo.MAS">
      <uzo tip="fak">MAS</uzo>
      <dif>
        Peco de ma&scirc;ino, kiu <tld/>as:
        <ekz>
          trukarto jam estis konata de a&ubreve;tomataj muzikiloj,
          sed tie temas normale nur pri la simpla manipulado
          de valvo a&ubreve; <tld/>ilo
          <fnt>
            <aut>R. F&ouml;ssmeier</aut>,
            <vrk><url ref="http://www.ais-sanmarino.org/kursoj/s1/kultprog/teksto.html">
                Kulturaj aspektoj de
                komputil-programado</url></vrk>,
            <lok>SUS 22, 2000</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="bat.0ilo.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Ilo per kiu oni <tld/>as pilkon a&ubreve; alian cela&jcirc;on:
        <ekz>
          per la <tld/>ilo li a&ubreve; &scirc;i <tld/>as la pilkon,
          &gcirc;i devas flugi super la reto
          <fnt>
            <aut>[B. Jensenius]</aut>,
            <vrk><url ref="http://www.bonvenon.dk/gemelaj.htm">
                &Gcirc;emelaj Urboj</url></vrk>,
            <lok>[2007]</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="klab.0o.SPO">klabo<sncref/></ref>,
        <ref cel="raked.0o.SPO">rakedo</ref>
      </refgrp>
      <trdgrp lng="ru">
        <trd>&c_b;&c_i;&c_t;&c_a;</trd>,
        <trd>&c_r;&c_a;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>klepa&ccaron;</trd>,
    <trd>pl&aacute;ca&ccaron;ka</trd>,
    <trd>p&aacute;lka</trd>,
    <trd>tlouk</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>batidora</trd>,
    <trd>bate (de hockey, b&eacute;isbol)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>batte</trd>,
    <trd>marteau</trd>
  </trdgrp>
  <trd lng="he">&#x5DB;&#x5DC;&#x5D9; &#x5E0;&#x5E9;&#x5E7;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&#x301;&c_l;&c_o;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;lka</trd>,
    <trd>raketa</trd>
  </trdgrp>
</drv>

<drv mrk="bat.de0i">
  <kap>de<tld/>i,
    <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bat.de0i.KOMUNE">
    <dif>
      Per <tld/>o faligi, &scirc;ovi, movi for de la normala loko, de
