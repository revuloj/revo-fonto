<drv mrk="bat.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="bat.0ado.MOV">
    <dif>
      Ago <tld/>i ripete:
      <ekz>
        la <ind>inter<tld/>ado</ind> longe da&ubreve;ris;
        <trdgrp lng="de">
          <trd>Kampf</trd>,
          <trd>Gefecht</trd>
        </trdgrp>
        <trd lng="en">fight</trd>
        <trdgrp lng="es">
          <trd>combate</trd>,
          <trd>lucha</trd>
        </trdgrp>
        <trd lng="fr">combat</trd>
      </ekz><ekz>
        <tld/>ado de tapi&scirc;o;
      </ekz><ekz>
        <tld/>ado de lakto produktas buteron
        <fnt><bib>MT</bib>, <lok>&Sen; 30:33</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;&c_c;&c_jo;</trd>
  <trdgrp lng="cs">
    <trd>bit&iacute;</trd>,
    <trd>ml&aacute;cen&iacute;</trd>,
    <trd>tlu&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>beating</trd>,
    <trd>barrage</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sacudida</trd>
  </trdgrp>
  <trd lng="fr">battage</trd>
  <trdgrp lng="hu">
    <trd>&uuml;tlegel&eacute;s</trd>,
    <trd>ver&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bicie</trd>,
    <trd>ci&eogonek;gi</trd>,
    <trd>lanie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_t;&c_mol;&c_jo;</trd>,
    <trd>&c_b;&c_i;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klepanie</trd>,
    <trd>tep</trd>,
    <trd>tlkot</trd>
  </trdgrp>
