<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>fru</rad>/a
</kap>

<drv mrk="fru.0a">
    <kap><ofc>*</ofc><tld/>a</kap>
    <snc>
      <dif>
        Okazanta plej tuje, plej proksime de la ta&ubreve;ga, difinita
        deirtempo:
        <ekz>
          &ccirc;e, en <tld/>a mateno<fnt>Z</fnt>,
        </ekz>
        <ekz>
          <ind><tld/>matene</ind>;
          <trdgrp lng="be">
            <trd>&c_z;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>,
            <trd>&c_z;&c_r;&c_a;&c_n;&c_k;&c_u;</trd>,
            <trd>&c_r;&c_a;&c_n;&c_ib;&c_c;&c_a;&c_ju;</trd>
          </trdgrp>
          <trd lng="fr">de bon <ind>matin</ind></trd>
          <trd lng="hu">kora reggel</trd>
          <trd lng="nl">vroeg in de <ind>ochtend</ind></trd>
          <trd lng="pl">wcze&sacute;nie rano</trd>
          <trd lng="ro">diminea&tcedil;a devreme</trd>
          <trd lng="tr">erken saatte</trd>
        </ekz>
        <ekz>
          en lia <ind><tld/>a juneco</ind>;
          <trd lng="fr">prime <ind>jeunesse</ind></trd>
          <trd lng="hu">kora ifj&uacute;s&aacute;g&aacute;ban</trd>
          <trd lng="nl">prille <ind>jeugd</ind></trd>
          <trd lng="pl">wczesna m&lstroke;odo&sacute;&cacute;</trd>
        </ekz>
        <ekz>
          <tld/>aj, <ind><tld/>matura</ind>j fruktoj;
          <trd
lng="be">&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_l;&c_y;</trd>
          <trdgrp lng="fr">
            <trd>h&acirc;tif<klr tip="ind"> (pr&eacute;coce)</klr></trd>,
            <trd>pr&eacute;coce<klr tip="ind"> (h&acirc;tif)</klr></trd>
          </trdgrp>
          <trd lng="hu">kora&eacute;rett</trd>
          <trd lng="pl">niedojrza&lstroke;y</trd>
        </ekz>
        <ekz>
          <ind>tro<tld/>a</ind> <klr>(anta&ubreve;tempa)</klr> morto.
          <trdgrp lng="be">
            <trd>&c_d;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>,
            <trd>&c_z;&c_a;&c_w;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>,
            <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
          </trdgrp>
          <trdgrp lng="en">
            <trd>premature</trd>,
            <trd>untimely</trd>
          </trdgrp>
          <trdgrp lng="fr">
            <trd>pr&eacute;coce<klr tip="ind">
(pr&eacute;matur&eacute;)</klr></trd>,
            <trd>pr&eacute;matur&eacute;</trd>
          </trdgrp>
          <trdgrp lng="he">
            <trd>&bet;&tet;&resh;&fmem; &ayin;&tav;</trd>,
            <trd>&mem;&vav;&qof;&dalet;&fmem; &mem;&dalet;&yod;</trd>
          </trdgrp>
          <trdgrp lng="hu">
            <trd>t&uacute;l <ind>korai</ind></trd>,
            <trd>t&uacute;l <ind>kor&aacute;n</ind> bek&ouml;vetkezett</trd>
          </trdgrp>  
          <trd lng="nl">voortijdig</trd>
          <trd lng="pl">przedwczesny</trd>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="jun.0a">juna</ref>.
      </refgrp>
    </snc>
  <trd lng="be">&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ib;</trd>
  <trd lng="bg">&c_r;&c_a;&c_n;&c_o;</trd>
  <trd lng="cs">&ccaron;asn&yacute;</trd>
  <trd lng="de">fr&uuml;h</trd>
  <trd lng="en">early</trd>
  <trdgrp lng="es">
       <trd>temprano</trd>,
       <trd>puntual</trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>anticip&eacute;</trd>,
      <trd>h&acirc;tif<klr tip="ind"> (pr&eacute;coce)</klr></trd>,
      <trd>pr&eacute;coce<klr tip="ind"> (h&acirc;tif)</klr></trd>
    </trdgrp>
  <trd lng="he">&mem;&vav;&qof;&dalet;&fmem;</trd>
  <trd lng="hu">korai</trd>
  <trd lng="nl">vroeg</trd>
  <trd lng="pl">wczesny</trd>
  <trdgrp lng="pt">
    <trd>prematuro</trd>,
    <trd>antecipado</trd>,
    <trd>precoce</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_n;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ranný</trd>,
    <trd>skorý</trd>
  </trdgrp>
  <trd lng="sv">tidig</trd>
</drv>

<drv mrk="fru.0e0tempe">
    <kap><tld/>e, <var><kap><tld/>tempe<fnt>Z</fnt></kap></var></kap>
    <snc>
      <dif>
        En <tld/>a tempo:
        <ekz>
          <tld/>e levi&gcirc;u kaj <tld/>e edzi&gcirc;u
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <ind>kiu pli <tld/>e venas, pli bonan lokon prenas</ind>
          <fnt><bib>PrV</bib></fnt>;
          <trd lng="be">&c_R;&c_a;&c_n;&c_a;
&c_w;&c_s;&c_t;&c_a;&c_n;&c_je;&c_sh; - &c_b;&c_o;&c_l;&c_je;&c_j;
&c_z;&c_r;&c_o;&c_b;&c_ib;&c_sh;</trd>
          <trd lng="en">first come, first served</trd>
          <trd lng="fr">premier arriv&eacute;, premier servi</trd>
          <trd lng="hu">kiszolg&aacute;l&aacute;s &eacute;rkez&eacute;si
sorrendben</trd>
        </ekz>
        <ekz>
          la horlo&gcirc;o iras tro <tld/>e<fnt>Z</fnt>
          <klr>(&gcirc;i <ref cel="fru.0i.horlogxo"><tld/>as
<sncref/></ref>)</klr>;
        </ekz>
        <ekz>
          la komitato devas dissendi kiel eble plej <tld/>tempe
          inviton<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <ind>pli a&ubreve; malpli <tld/>e</ind> lingvo internacia nepre
          estos enkondukita
          <fnt><bib>EE</bib></fnt>;
          <trd lng="be">&c_r;&c_a;&c_n;&c_je;&c_j; &c_c;&c_ib;
&c_p;&c_a;&c_z;&c_mol;&c_n;&c_je;&c_j;</trd>
          <trd lng="en">sooner or later</trd>
          <trd lng="fr">t&ocirc;t ou tard</trd>
          <trd lng="he">&bet;&mem;&vav;&qof;&dalet;&fmem; &alef;&vav;
&bet;&mem;&alef;&vav;&het;&resh;</trd>
        <trd lng="hu">el&odblac;bb vagy ut&oacute;bb</trd>
        <trd lng="pl">wcze&sacute;niej czy p&oacute;&zacute;niej</trd>
        <trd lng="tr">er ge&ccedil;</trd>
        </ekz>
        <ekz>
          ne pli <tld/>e ol <klr>(ne anta&ubreve;)</klr>
          matene li venis hejmen<fnt>Z</fnt>;
        </ekz>
        <ekz>
          ne pli <tld/>e ol kiam...<fnt>Z</fnt> <klr>(ne anta&ubreve;
ol)</klr>;
        </ekz>
        <ekz>
          oni ne devas tro <tld/>e fanfaroni.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="lauxd.0i">la&ubreve;di</ref>,
        <ref cel="mok.0i">moki</ref>,
        <ref cel="rid.0i">ridi</ref>,
        <ref cel="man.0o">mano</ref>,
        <ref cel="urs.0o">urso</ref>,
        <ref cel="venk.0o">venko</ref>,
        <ref cel="vesper.0o">vespero</ref>,
        <ref cel="triumf.0i">triumfi</ref>.
      </refgrp>
    </snc>
  <trd lng="be">&c_r;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="en">
      <trd>early</trd>,
      <trd>soon</trd>
    </trdgrp>
  <trd lng="es">temprano</trd>
  <trdgrp lng="fr">
      <trd>t&ocirc;t</trd>,
      <trd>de bonne <ind>heure</ind></trd>
    </trdgrp>
  <trdgrp lng="he">
      <trd>&mem;&vav;&qof;&dalet;&fmem;</trd>,
      <trd>&bet;&mem;&vav;&qof;&dalet;&fmem;</trd>
    </trdgrp>
  <trd lng="hu">kor&aacute;n</trd>
  <trd lng="nl">vroegtijdig</trd>
  <trd lng="pl">wcze&sacute;nie</trd>
  <trd lng="ro">devreme</trd>
  <trd lng="ru">&c_r;&c_a;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>skoro</trd>,
    <trd>včas</trd>,
    <trd>čoskoro</trd>
  </trdgrp>
  <trd lng="sv">tidigt</trd>
</drv>

<drv mrk="fru.0i">
    <kap><tld/>i</kap>
    <snc mrk="fru.0i.okazi">
      <dif>
        Okazi a&ubreve; alveni <tld/>e:
        <ekz>
          &ccirc;i-jare <tld/>as la printempo, la legomoj;
        </ekz>
        <ekz>
          Anjo-la-Pesto ial kutimis elliti&gcirc;i eksterordinare <tld/>e,
          sed &ccirc;i-foje &scirc;i <tld/>is supermezure
          <fnt><bib>MkM</bib><lok>&ccirc;ap. 24</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_b;&c_y;&c_c;&c_mol;
<ind>&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ib;&c_m;</ind></trd>
      <trd lng="pl">by&cacute; <ind>wcze&sacute;nie</ind></trd>
      <trdgrp lng="ru">
        <trd>&c_b;&c_y;&c_t;&c_mol;,
&c_v;&c_y;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;
<ind>&c_r;&c_a;&c_n;&c_n;&c_i;&c_m;</ind></trd>
      </trdgrp>
      <trd lng="hu">kor&aacute;n &eacute;rkezik</trd>
    </snc>
    <snc mrk="fru.0i.senpersone">
      <dif>
        <klr>(senpersone)</klr>
        Proksime al la komenco de konvena periodo, kiam &gcirc;i
        &jcirc;us komenci&gcirc;is a&ubreve; e&ccirc; estas nur komenconta:
        <ekz>
          ankora&ubreve; tro fruas por diri...
          <fnt>
          <aut>Marko Naoki LINS</aut>:
          <vrk>Bruselo-Babelo: lingvo-kulturo en E&ubreve;ropa Unio</vrk>.
          <bib>Monato</bib>.
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_r;&c_a;&c_n;&c_a;<klr>
(&c_b;&c_je;&c_z;&c_a;&c_s;&c_a;&c_b;&c_o;&c_v;&c_y;
&c_s;&c_k;&c_a;&c_z;)</klr></trd>
      <trdgrp lng="hu">
        <trd>korai</trd>,
        <trd>kor&aacute;n van</trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_a;&c_n;&c_o;<klr>
(&c_n;&c_a;&c_z;&c_y;&c_v;&c_n;&c_o;&c_je;
&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
    </snc>
    <snc mrk="fru.0i.horlogxo">
      <dif>
        <klr>(p.p. horlo&gcirc;o)</klr>
        Montri ankora&ubreve; ne venintan tempon:
        <ekz>
          la horlo&gcirc;o <tld/>as je kvin minutoj
          <klr>(tro rapidas kaj montras tempon kiu fakte estos post
          5 minutoj)</klr>.
        </ekz>
      </dif>
      <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_sh;&c_a;&c_c;&c_c;&c_a; <klr
tip="amb"> (&c_p;&c_r;&c_a;
&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;)</klr></trd>
      <trd lng="hu">siet <klr tip="amb">(&oacute;ra)</klr></trd>
      <trd lng="ru">&c_s;&c_p;&c_je;&c_sh;&c_i;&c_t;&c_mol;<klr
tip="amb"> (&c_o; &c_ch;&c_a;&c_s;&c_a;&c_h;)</klr></trd>
      <trdgrp lng="de">
        <trd>vorgehen</trd>
      </trdgrp>
    </snc>
  <trdgrp lng="en">
      <trd>be <ind>early</ind></trd>,
      <trd>be <ind>fast <klr>(clock)</klr></ind></trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>&ecirc;tre en <ind>avance</ind></trd>,
      <trd>&ecirc;tre <ind>pr&eacute;coce</ind><klr tip="ind"> (saison,
v&eacute;g&eacute;tation)</klr></trd>,
      <trd>&ecirc;tre <ind>t&ocirc;t</ind></trd>,
      <trd>avancer<klr tip="ind"> (intr, horloge)</klr></trd>
    </trdgrp>
  <trdgrp lng="he">
      <trd>&lamed;<ind>&he;&qof;&dalet;&yod;&fmem;</ind></trd>,
      <trd>&lamed;&he;&yod;&vav;&tav;
<ind>&mem;&vav;&qof;&dalet;&fmem;</ind></trd>
    </trdgrp>
  <trd lng="pl">by&cacute; <ind>wcze&sacute;nie</ind></trd>
  <trd lng="sk">predbiehať <klr>(o hodinách)</klr></trd>
</drv>

<drv mrk="fru.mal0a">
    <kap>mal<tld/>a</kap>
    <snc>
      <dif>
        Okazanta nur post la ta&ubreve;ga, difinita limtempo:
        <ekz>
          en la mal<tld/>a nokto <klr>(mal<tld/>e en la nokto)</klr>;
        </ekz>
        <ekz>
          mal<tld/>aj rikoltoj
          <klr>(tro mal<tld/>e maturi&gcirc;intaj)</klr>;
        </ekz>
        <ekz>
          <ind>por mal<tld/>a <klr>(tro mal<tld/>e veninta)</klr>
          gasto restas nur osto</ind>
          <fnt><bib>PrV</bib></fnt>;
          <trd lng="hu">k&eacute;s&odblac;n j&ouml;tt vend&eacute;gnek
csont a vacsor&aacute;ja</trd>
        </ekz>
        <ekz>
          la horlo&gcirc;o iras tro <ind>mal<tld/>e</ind><fnt>Z</fnt>
          <klr>(&gcirc;i <ref cel="fru.mal0i.horlogxo">mal<tld/>as
<sncref/></ref>)</klr>;
          <trd lng="be">&c_p;&c_o;&c_z;&c_n;&c_a;</trd>
          <trdgrp lng="fr">
            <trd>tard</trd>,
            <trd>avec <ind>retard</ind></trd>
          </trdgrp>
          <trd lng="hu">k&eacute;sik</trd>
        </ekz>
        <ekz>
          <ind>pli bone mal<tld/>e ol neniam</ind>
          <fnt><bib>PrV</bib></fnt>.
          <trd lng="be">&c_l;&c_je;&c_p;&c_sh;
&c_p;&c_o;&c_z;&c_n;&c_a;, &c_ch;&c_y;&c_m;
&c_n;&c_ib;&c_k;&c_o;&c_l;&c_ib;</trd>
          <trd lng="en">better late than never</trd>
          <trd lng="fr">mieux vaut tard que jamais</trd>
          <trd lng="hu">jobb k&eacute;s&odblac;n, mint soha</trd>
          <trd lng="pl">lepiej p&oacute;&zacute;niej ni&zdot; wcale</trd>
          <trd lng="tr">ge&ccedil;</trd>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="kat.0o">kato</ref>,
        <ref cel="mustar.0o">mustardo</ref>,
        <ref cel="moment.0o">momento</ref>,
        <ref cel="dent.0o">dento</ref>,
        <ref cel="ide.0o">ideo</ref>.
      </refgrp>
    </snc>
  <trd lng="be">&c_p;&c_o;&c_z;&c_mol;&c_n;&c_ib;</trd>
  <trd lng="bg">&c_k;&c_malmol;&c_s;&c_je;&c_n;</trd>
  <trd lng="de">sp&auml;t</trd>
  <trdgrp lng="en">
      <trd>late</trd>,
      <trd>slow <klr>(clock)</klr></trd>
    </trdgrp>
  <trd lng="fr">tardif</trd>
  <trd lng="he">&mem;&alef;&vav;&het;&resh;</trd>
  <trdgrp lng="hu">
      <trd>k&eacute;s&odblac;</trd>,
      <trd>k&eacute;s&odblac;i</trd>
    </trdgrp>
  <trd lng="nl">laat</trd>
  <trd lng="pl">p&oacute;&zacute;ny</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_d;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">neskorý</trd>
  <trd lng="sv">sen</trd>
</drv>

<drv mrk="fru.mal0o">
    <kap>mal<tld/>o</kap>
    <snc>
      <dif>
        Stato de io a&ubreve; iu mal<tld/>a:
        <ekz>
          <klr>[la Hebreino]</klr> estas la ka&ubreve;zo de la mal<tld/>o
          de la levi&gcirc;o de Nilo
          <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 9</lok></fnt>;
        </ekz>
        <ekz>
          <klr>[la leterportisto]</klr> kutime pasas &gcirc;ustatempe,
          tamen mal<tld/>o povas fojfoje okazi
          <fnt><bib>Lasu</bib>, <lok>&ccirc;ap. 18b</lok></fnt>.
        </ekz>
      </dif>
    </snc>
  <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_k;&c_a;</trd>,
      
<trd>&c_s;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
  <trd lng="en">delay</trd>
  <trd lng="fr">retard</trd>
  <trd lng="he">&alef;&yod;&het;&vav;&resh;</trd>
  <trd lng="hu">k&eacute;s&eacute;s</trd>
  <trd lng="pl">sp&oacute;&zacute;nienie</trd>
  <trd lng="ro">&icirc;nt&acirc;rziere</trd>
  <trd lng="sk">pokročilosť času</trd>
  <trd lng="tr">gecikme</trd>
</drv>

<drv mrk="fru.mal0i">
    <kap>mal<tld/>i</kap>
    <snc mrk="fru.mal0i.okazi">
      <dif>
        Okazi a&ubreve; alveni tro mal<tld/>e:
        <ekz>
          pardonu, ke mi mal<tld/>as;
        </ekz>
        <ekz>
          la karavano mal<tld/>as jam plenan tagon
          <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 1a</lok></fnt>;
        </ekz>
        <ekz>
          Izraelo mal<tld/>as kun la tributo <klr>(mal<tld/>e &gcirc;in
          pagas)</klr> kaj pagas malpli kaj malpli
          <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 3</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_s;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="hu">k&eacute;sik</trd>
      <trd lng="pl">sp&oacute;&zacute;ni&cacute;</trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_p;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_p;&c_a;&c_z;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </snc>
    <snc mrk="fru.mal0i.senpersone">
      <dif>
        <klr>(senpersone)</klr>
        Proksime al la fino de konvena periodo, kiam &gcirc;i
        fini&gcirc;as a&ubreve; e&ccirc; finis:
        <ekz>
          &scirc;ajnas, ke mi devas iri... mal<tld/>as
          <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 24a.</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_o;&c_z;&c_n;&c_a;<klr>
(&c_b;&c_je;&c_z;&c_a;&c_s;&c_a;&c_b;&c_o;&c_v;&c_y;
&c_s;&c_k;&c_a;&c_z;)</klr></trd>
      <trd lng="hu">k&eacute;s&odblac; van</trd>
      <trd lng="ru">&c_p;&c_o;&c_z;&c_d;&c_n;&c_o;<klr>
(&c_n;&c_a;&c_z;&c_y;&c_v;&c_n;&c_o;&c_je;
&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
    </snc>
    <snc mrk="fru.mal0i.horlogxo">
      <dif>
        <klr>(p.p. horlo&gcirc;o)</klr>
        Montri jam pasintan tempon:
        <ekz>
          la horlo&gcirc;o mal<tld/>as kvin minutojn
          <klr>(ne sufi&ccirc;e rapidas kaj montras pli <tld/>an
tempon)</klr>
          <fnt><aut>&B;</aut>, <lok>la&ubreve; PIV2, p. 370.</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_c;&c_a;<klr
tip="amb"> (&c_p;&c_r;&c_a;
&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;)</klr></trd>
      <trd lng="hu">k&eacute;sik <klr tip="amb">(&oacute;ra)</klr></trd>
      <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_mol;<klr
tip="amb"> (&c_o; &c_ch;&c_a;&c_s;&c_a;&c_h;)</klr></trd>
    </snc>
  <trdgrp lng="de">
      <trd><ind>sp&auml;t</ind> kommen</trd>,
      <trd>nachgehen <klr tip="ind">(Uhr)</klr>
<klr>(horlo&gcirc;o)</klr></trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>be <ind>late</ind></trd>,
      <trd>be <ind>slow <klr>(clock)</klr></ind></trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>&ecirc;tre en <ind>retard</ind></trd>,
      <trd>&ecirc;tre <ind>tard</ind></trd>,
      <trd>&ecirc;tre <ind>tardif</ind><klr tip="ind"> (saison,
v&eacute;g&eacute;tation)</klr></trd>,
      <trd>retarder<klr tip="ind"> (intr.)</klr></trd>
    </trdgrp>
  <trd lng="he">&lamed;<ind>&alef;&het;&resh;</ind></trd>
  <trd lng="nl">te <ind>laat</ind> zijn</trd>
  <trd lng="pl">sp&oacute;&zacute;ni&cacute;</trd>
  <trd lng="sk">oneskoriť sa</trd>
  <trd lng="sv">vara <ind>sen</ind></trd>
  <trd lng="tr">ge&ccedil; kalmak</trd>
</drv>

<drv mrk="fru.mal0igxi">
    <kap>mal<tld/>i&gcirc;i</kap>
    <snc>
      <dif>
        Lasi sin fari (veni, fini laboron ktp) mal<tld/>e:
        <ekz>
          la laboro mal<tld/>i&gcirc;as, la mendoj ne estas
          plenumataj &gcirc;ustatempe
          <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>:
          <vrk>Marta.</vrk></fnt>;
        </ekz>
        <ekz>
          mi nur iom mal<tld/>i&gcirc;os al la tagman&gcirc;o, sed tio
          ne estas grava
          <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>:
          <vrk>Marta.</vrk></fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          kial mal<tld/>i&gcirc;as la radoj de liaj kale&scirc;oj?
          <fnt><bib>MT</bib>, <lok>&Jug; 5:28</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_s;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
        
<trd>&c_z;&c_a;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
        
<trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="de"><ind>sp&auml;t</ind> kommen</trd>
      <trdgrp lng="fr">
        <trd>prendre du <ind>retard</ind></trd>,
        <trd>&ecirc;tre en <ind>retard</ind></trd>,
        <trd>retarder<klr tip="ind"> (intr.)</klr></trd>
      </trdgrp>
      <trd lng="hu">(el)<ind>k&eacute;sik</ind></trd>      
      <trd lng="nl">te <ind>laat</ind> zijn</trd>
      <trd lng="pl">sp&oacute;&zacute;ni&cacute; si&eogonek;</trd>
      <trd lng="ro">a &icirc;nt&acirc;rzia</trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_p;&c_a;&c_z;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,

<trd>&c_z;&c_a;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
      <trd lng="sv">vara <ind>sen</ind></trd>
      <trd lng="tr">gecikmek</trd>
    </snc>
    <snc mrk="fru.mal0igxi.senpersone">
      <dif>
        <klr>(senpersone)</klr>
        Esti mal<tld/>e:
        <ekz>
          <ind>jam mal<tld/>i&gcirc;is</ind>
          <fnt><bib>PIV1</bib></fnt>.
          <trd lng="fr">il se fait <ind>tard</ind></trd>
        </ekz>
      </dif>
      <trd lng="de"><ind>sp&auml;t</ind> sein</trd>
    </snc>
    <trd lng="en">get, become <ind>late</ind></trd>
    <trd lng="he">&lamed;<ind>&he;&tav;&alef;&het;&resh;</ind></trd>
    <trd lng="hu">k&eacute;s&odblac;re j&aacute;r</trd>
    <trd lng="pl">sp&oacute;&zacute;ni&cacute; si&eogonek;</trd>
    <trd lng="ro">a &icirc;nt&acirc;rzia</trd>
</drv>

<drv mrk="fru.mal0igi">
    <kap>mal<tld/>igi</kap>
    <snc>
      <dif>
        Fari, ke io okazas mal<tld/>e, ka&ubreve;zi prokraston:
        <ekz>
          la malbona vetero mal<tld/>igis la rikoltojn;
        </ekz>
        <ekz>
          tiu akcidento mal<tld/>igis nian alvenon.
        </ekz>
      </dif>
    </snc>
  <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
  <trd lng="de">verz&ouml;gern</trd>
  <trd lng="en">delay</trd>
  <trdgrp lng="fr">
      <trd>mettre en <ind>retard</ind></trd>,
      <trd>retarder<klr tip="ind"> (tr., causer un retard
&agrave;)</klr></trd>
    </trdgrp>
  <trd lng="he">&lamed;<ind>&ayin;&kaf;&bet;</ind></trd>
  <trd lng="hu">k&eacute;sleltet</trd>
  <trd lng="nl">vertragen</trd>
  <trd lng="pl">op&oacute;&zacute;ni&cacute;</trd>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">oneskoriť</trd>
  <trd lng="sv">f&ouml;rsena</trd>
  <trd lng="tr">geciktirmek</trd>
</drv>
<adm>
    Indus simetriigi la derivajxojn enkondukante: mal~e, ~igi, ~igxi...
    <aut>MB</aut>
</adm>
