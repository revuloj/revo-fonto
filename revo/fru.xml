<drv mrk="fru.mal0igxi">
    <kap>mal<tld/>i&gcirc;i</kap>
    <snc>
      <dif>
        Lasi sin fari (veni, fini laboron ktp) mal<tld/>e:
        <ekz>
          la laboro mal<tld/>i&gcirc;as, la mendoj ne estas
          plenumataj &gcirc;ustatempe
          <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>:
          <vrk>Marta.</vrk></fnt>;
        </ekz>
        <ekz>
          mi nur iom mal<tld/>i&gcirc;os al la tagman&gcirc;o, sed tio
          ne estas grava
          <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>:
          <vrk>Marta.</vrk></fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          kial mal<tld/>i&gcirc;as la radoj de liaj kale&scirc;oj?
          <fnt><bib>MT</bib>, <lok>&Jug; 5:28</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_s;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_z;&c_a;&c_p;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="de"><ind>sp&auml;t</ind> kommen</trd>
      <trdgrp lng="fr">
        <trd>prendre du <ind>retard</ind></trd>,
        <trd>&ecirc;tre en <ind>retard</ind></trd>,
        <trd>retarder<klr tip="ind"> (intr.)</klr></trd>
      </trdgrp>
      <trd lng="hu">(el)<ind>k&eacute;sik</ind></trd>
      <trd lng="nl">te <ind>laat</ind> zijn</trd>
      <trd lng="pl">sp&oacute;&zacute;ni&cacute; si&eogonek;</trd>
      <trd lng="ro">&icirc;nt&acirc;rzia</trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_p;&c_a;&c_z;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
      <trd lng="sv">vara <ind>sen</ind></trd>
      <trd lng="tr">gecikmek</trd>
    </snc>
    <snc mrk="fru.mal0igxi.senpersone">
      <dif>
        <klr>(senpersone)</klr>
        Esti mal<tld/>e:
        <ekz>
          <ind>jam mal<tld/>i&gcirc;is</ind>
          <fnt><bib>PIV1</bib></fnt>.
          <trd lng="fr">il se fait <ind>tard</ind></trd>
        </ekz>
      </dif>
      <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_m;</ind></trd>
      <trd lng="de"><ind>sp&auml;t</ind> sein</trd>
    </snc>
    <trd lng="en">get, become <ind>late</ind></trd>
    <trd lng="he">&lamed;<ind>&he;&tav;&alef;&het;&resh;</ind></trd>
    <trd lng="hu">k&eacute;s&odblac;re j&aacute;r</trd>
    <trd lng="ja">&#x9045;&#x308C;&#x308B;  <pr>&#x304A;&#x304F;&#x308C;&#x308B;</pr></trd>
  <trd lng="pl">sp&oacute;&zacute;ni&cacute; si&eogonek;</trd>
    <trd lng="ro">&icirc;nt&acirc;rzia</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_z;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_p;&c_ib;&c_z;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_i;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_ja;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x59CD;&#x59CD;&#x4F86;&#x9072; <pr>sh&#x101;nsh&#x101;nl&#xE1;ich&#xED;</pr></trd>
