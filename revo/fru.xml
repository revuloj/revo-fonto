<drv mrk="fru.0e0tempe">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a tempo:
      <ekz>
        <tld/>e levi&gcirc;u kaj <tld/>e edzi&gcirc;u
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>kiu pli <tld/>e venas, pli bonan lokon prenas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="be">&c_R;&c_a;&c_n;&c_a; &c_w;&c_s;&c_t;&c_a;&c_n;&c_je;&c_sh; &ndash; &c_b;&c_o;&c_l;&c_je;&c_j; &c_z;&c_r;&c_o;&c_b;&c_ib;&c_sh;</trd>
        <trd lng="en">first come, first served</trd>
        <trd lng="fr">premier arriv&eacute;, premier servi</trd>
        <trd lng="hu">kiszolg&aacute;l&aacute;s &eacute;rkez&eacute;si
        sorrendben</trd>
      </ekz>
      <ekz>
        <klr>[la]</klr> horlo&gcirc;o <klr>[&#x2026;]</klr> &ccirc;iam iris tro <tld/>e, sed la maljunaj
        homoj diris, ke tio estas pli bona ol se &gcirc;i irus tro mal<tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pli a&ubreve; malpli <tld/>e</ind> lingvo internacia nepre
        estos enkondukita
        <fnt><bib>EE</bib></fnt>;
        <trd lng="be">&c_r;&c_a;&c_n;&c_je;&c_j; &c_c;&c_ib; &c_p;&c_a;&c_z;&c_mol;&c_n;&c_je;&c_j;</trd>
        <trd lng="en">sooner or later</trd>
        <trd lng="fr">t&ocirc;t ou tard</trd>
        <trd lng="he">&bet;&mem;&vav;&qof;&dalet;&fmem; &alef;&vav;
&bet;&mem;&alef;&vav;&het;&resh;</trd>
      <trd lng="hu">el&odblac;bb vagy ut&oacute;bb</trd>
      <trd lng="pl">wcze&sacute;niej czy p&oacute;&zacute;niej</trd>
      <trd lng="ro">mai devreme sau mai t&acirc;rziu</trd>
      <trd lng="tr">er ge&ccedil;</trd>
      </ekz>
      <ekz>
        ne pli <tld/>e ol <klr>(ne anta&ubreve;)</klr>
        matene <klr>[li]</klr> venis hejmen
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne pli <tld/>e ol kiam...
        <klr>(ne anta&ubreve; kiam)</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne devas tro <tld/>e fanfaroni.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxd.0i">la&ubreve;di</ref>,
      <ref cel="mok.0i">moki</ref>,
      <ref cel="rid.0i">ridi</ref>,
