<drv mrk="dank.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="dank.0o.KOMUNE">
    <dif>
      Vortoj a&ubreve; faroj por <tld/>i:
      <ekz>
        <ind><tld/>on</ind>!
        <trd lng="be">&c_d;&c_z;&c_ja;&c_k;&c_u;&c_j;</trd>
        <trd lng="br">trugarekadenn</trd>
        <trdgrp lng="ca">
          <trd>gr&agrave;cies!</trd>,
          <trd>merc&egrave;s!</trd>
        </trdgrp>
        <trd lng="cs">d&iacute;ky</trd>
        <trd lng="de">danke</trd>
        <trdgrp lng="en">
          <trd>thank you</trd>,
          <trd>thanks</trd>
        </trdgrp>
        <trd lng="es">gracias</trd>
        <trd lng="fr">merci</trd>
        <trd lng="he">&tav;&vav;&dalet;&he;</trd>
        <trd lng="hu">k&ouml;sz&ouml;n&ouml;m</trd>
        <trd lng="id"><ind>terima kasih</ind>!</trd>
        <trd lng="nl">dank u</trd>
        <trdgrp lng="no">
          <trd>takk</trd>,
          <trd>tusen takk</trd>
        </trdgrp>
        <trd lng="pl">dzi&eogonek;kuj&eogonek;</trd>
        <trd lng="pt">obrigado</trd>
        <trd lng="ro">mul&tcedil;umesc</trd>
        <trd lng="ru">&c_s;&c_p;&c_a;&c_s;&c_i;&c_b;&c_o;</trd>
        <trd lng="tok">pona</trd>
        <trdgrp lng="zh">
          <trd>&#35874;&#35874;</trd>,
          <trd><klr>tr. </klr>&#35613;&#35613; [<ind>xi&egrave;xie</ind>]</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>on pro tio, ke vi venis al mi
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>;
      </ekz>
      <ekz>
        kanti <tld/>on al la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 7:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        psalmo de <tld/>ado
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 100</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio estas via <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>?
      </ekz>
      <ekz>
        kritikojn ni akceptas kun <tld/>o, &ccirc;ar ili <klr>[&#8230;]</klr> helpas al nia
        perfekti&gcirc;o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi esprimas anka&ubreve; koran <tld/>on al tiuj personoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni &scirc;uldas <tld/>on al la lando mem por la invito
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la ferma ceremonio de la Sesa Kongreso Esperantista en Washington en
          la 20a de a&ubreve;gusto 1910 (a)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on al vi por tiu &ccirc;i feli&ccirc;a elpenso
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi a&ubreve;skultos vian <tld/>adon en pli oportuna okazo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
