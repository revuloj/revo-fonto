<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kejl</rad>/o
</kap>
<drv mrk="kejl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ligna a&ubreve; metala peco, &gcirc;enerale kojnoforma, uzata anstata&ubreve; najlo,
      por perforte &scirc;topi truon a&ubreve; rigide kunfiksi du partopecojn: 
      <ekz>
        al&gcirc;ustiga, duobla, kalkana, konkava, kvadrata, ronda <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>spie</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_sh;&c_i;&c_p;</trd>
  </trdgrp>
</drv>
<drv mrk="kejl.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Ligi a&ubreve; fiksi per <tld/>o.  
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="nl">vastpinnen</trd>
</drv>
</art>
