<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kejl.xml,v 1.20 2022/03/21 17:51:57 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kejl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kejl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ligna a&ubreve; metala peco, &gcirc;enerale kojnoforma, uzata anstata&ubreve; najlo,
      por perforte &scirc;topi truon a&ubreve; rigide kunfiksi du partopecojn:
      <ekz>
        al&gcirc;ustiga, duobla, kalkana, konkava, kvadrata, ronda <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_v;&c_a;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_y;&c_p;</trd>
  </trdgrp>
  <trd lng="br">ibil</trd>
  <trdgrp lng="cs">
    <trd>kl&iacute;n</trd>,
    <trd>kol&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Keil</trd>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="fr">cheville<klr tip="ind"> (de fixation)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;k</trd>,
    <trd>csap</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>spie</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="pl">klin</trd>     
  <trd lng="ro">pan&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_sh;&c_i;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klin <klr>(sp&aacute;jac&iacute;, upev&ncaron;ovac&iacute;)</klr></trd>,
    <trd>klin <klr>(sp&aacute;jac&iacute;| upev&ncaron;ovac&iacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x447;&#x435;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x432;&#x456;&#x437;&#x434;&#x43E;&#x43A;</trd>,
    <trd>&#x434;&#x435;&#x440;&#x435;&#x432;&#x2019;&#x44F;&#x43D;&#x438;&#x439; &#x446;&#x432;&#x44F;&#x445;</trd>,
    <trd>&#x43A;&#x456;&#x43B;&#x43E;&#x447;&#x43E;&#x43A;</trd>,
    <trd>&#x448;&#x438;&#x43F;</trd>,
    <trd>&#x437;&#x430;&#x439;&#x432;&#x456; &#x441;&#x43B;&#x43E;&#x432;&#x430;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x442;&#x44F;&#x433;&#x43D;&#x443;&#x442;&#x456;&#x441;&#x442;&#x44C; (&#x443; &#x432;&#x456;&#x440;&#x448;&#x456;)</trd>
  </trdgrp>
</drv>

<drv mrk="kejl.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Ligi a&ubreve; fiksi per <tld/>o.
    </dif>
  </snc>
  <trd lng="br">ibilia&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>kl&iacute;novat</trd>,
    <trd>zakl&iacute;novat</trd>
  </trdgrp>
  <trd lng="de">verkeilen</trd>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="fr">cheviller</trd>
  <trdgrp lng="hu">
    <trd>&ouml;ssze&eacute;kel</trd>,
    <trd>&ouml;csapol</trd>
  </trdgrp>
  <trd lng="nl">vastpinnen</trd>
  <trdgrp lng="pl">
    <trd>klinowa&cacute;</trd>,
    <trd>mocowa&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>bloca</trd>,
    <trd>fixa</trd>
  </trdgrp>   
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_r;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_k;&c_o;&c_m;</ind></trd>
  <trdgrp lng="sk">
    <trd>sp&aacute;ja&tcaron;</trd>,
    <trd>upev&ncaron;ova&tcaron; pomocou klinov</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x440;&#x456;&#x43F;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438; (&#x448;&#x438;&#x43F;&#x430;&#x43C;&#x438;</trd>,
    <trd>&#x446;&#x432;&#x44F;&#x445;&#x430;&#x43C;&#x438;</trd>,
    <trd>&#x447;&#x435;&#x43A;&#x43E;&#x44E; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
