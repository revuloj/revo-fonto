<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kejl.xml,v 1.23 2022/05/01 08:09:24 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kejl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kejl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ligna a&ubreve; metala peco, &gcirc;enerale <ref tip="vid" cel="kojn.0o">kojno</ref>forma, uzata anstata&ubreve; najlo,
      por perforte &scirc;topi truon a&ubreve; rigide kunfiksi du partopecojn:
      <ekz>
        al&gcirc;ustiga, duobla, kalkana, konkava, kvadrata, ronda <tld/>o;
      </ekz>
      <ekz>
        la trabkuni&gcirc;o funkciis nur per <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Elisabeth Le Dru</aut>
          <vrk><url ref="&Monato;/2010/009948.php"
              >Vizito al Vannes</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie kaj tie &scirc;i vidis mapojn kaj bildojn pendantajn de <tld/>oj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 10 (60)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="najl.0o">najlo</ref>,
      <ref cel="stift.0o">stifto</ref>,
      <ref cel="sxtop.0ilo">&scirc;topilo</ref>,
      <ref cel="tenon.0o">tenono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_v;&c_a;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_y;&c_p;</trd>
  </trdgrp>
  <trd lng="br">ibil</trd>
  <trdgrp lng="cs">
    <trd>kl&iacute;n</trd>,
    <trd>kol&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stopfen</trd>,
    <trd>Pfropfen</trd>,
    <trd>Stift</trd>,
    <trd>Keil</trd>,
    <trd>Holznagel</trd>,
    <trd>Hering <klr>(Zeltnagel)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="fr">cheville<klr tip="ind"> (de fixation)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;k</trd>,
    <trd>csap</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>spie</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="pl">klin</trd>     
  <trd lng="ro">pan&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_sh;&c_i;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klin <klr>(sp&aacute;jac&iacute;, upev&ncaron;ovac&iacute;)</klr></trd>,
    <trd>klin <klr>(sp&aacute;jac&iacute;| upev&ncaron;ovac&iacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_v;&c_ib;&c_z;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&#8217;&c_ja;&c_n;&c_i;&c_j; &c_c;&c_v;&c_ja;&c_h;</trd>,
    <trd>&c_k;&c_ib;&c_l;&c_o;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_i;&c_p;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_v;&c_ib; &c_s;&c_l;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_ib;&c_s;&c_t;&c_mol; (&c_u; &c_v;&c_ib;&c_r;&c_sh;&c_ib;)</trd>
  </trdgrp>
</drv>

<drv mrk="kejl.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Ligi a&ubreve; fiksi per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_l;&c_k;&c_o;&c_m;</ind></trd>,
    <trd>&c_k;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">ibilia&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>kl&iacute;novat</trd>,
    <trd>zakl&iacute;novat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verkeilen</trd>,
    <trd>verklemmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="fr">cheviller</trd>
  <trdgrp lng="hu">
    <trd>&ouml;ssze&eacute;kel</trd>,
    <trd>&ouml;csapol</trd>
  </trdgrp>
  <trd lng="nl">vastpinnen</trd>
  <trdgrp lng="pl">
    <trd>klinowa&cacute;</trd>,
    <trd>mocowa&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>bloca</trd>,
    <trd>fixa</trd>
  </trdgrp>   
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_r;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_k;&c_o;&c_m;</ind></trd>
  <trdgrp lng="sk">
    <trd>sp&aacute;ja&tcaron;</trd>,
    <trd>upev&ncaron;ova&tcaron; pomocou klinov</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_ib;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i; (&c_sh;&c_i;&c_p;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_c;&c_v;&c_ja;&c_h;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_ch;&c_je;&c_k;&c_o;&c_ju; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>
</art>
