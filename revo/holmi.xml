<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: holmi.xml,v 1.23 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>holmi</rad>/o
</kap>
<drv mrk="holmi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ho</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="67">elemento</ref> kun
      atomnumero 67, atompezo 164,930;
      ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>,
      lantanoido:
      <ekz>
        <tld/>o posedas specialan magnetan econ <klr>[&#x2026;]</klr> kun 10,6&#x202F;&#xB5;B &gcirc;i havas plej
        altan magnetan momenton el &ccirc;iuj naturaj elementoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28693">Holmio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_l;&c_mol;&c_m;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_h;&c_o;&c_l;&c_m;&c_i;&c_j;</trd>
  <trd lng="br">holmiom</trd>
  <trdgrp lng="cs">
    <trd>holmium</trd>,
    <trd>zn. Ho</trd>
  </trdgrp>
  <trd lng="de">Holmium</trd>
  <trd lng="en">holmium</trd>
  <trd lng="es">holmio</trd>
  <trd lng="fa">&a_h;&a_w;&a_l;&a_m;&f_y;&a_m;</trd>
  <trd lng="fr">holmium</trd>
  <trd lng="hu">holmium</trd>
  <trd lng="id">holmium</trd>
  <trd lng="it">olmio</trd>
  <trd lng="la">Holmium</trd>
  <trd lng="nl">holmium</trd>
  <trd lng="pl">holm</trd>
  <trd lng="pt">h&oacute;lmio</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_mol;&c_m;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Ho</trd>,
    <trd>holmium</trd>
  </trdgrp>
  <trd lng="sv">holmium</trd>
  <trd lng="uk">&#x433;&#x43E;&#x43B;&#x44C;&#x43C;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x9225; <pr>hu&#x1D2;</pr></trd>,
    <trd>&#x9335; <pr>hu&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
