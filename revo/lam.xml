<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lam</rad>/a
</kap>
<drv mrk="lam.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="lam.0a.MED">
    <dif>
      <ref tip="super" cel="kripl.0a.MED">Kripla</ref>, malfacile
      pa&scirc;anta
      pro difekto de kruro a&ubreve; piedo:
      <ekz>
        en Listra sidadis unu viro senforta en la piedoj, kiu estis
        <tld/>a de la patrina ventro
        <fnt><bib>NT</bib>, <lok>&Ago; 14:8</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">lahm</trd>
    <trdgrp lng="en">
      <trd>lame</trd>,
      <trd>halt</trd>,
      <trd>hobbling</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>mank</trd>,
      <trd>kreupel</trd>,
      <trd>hinkend</trd>
    </trdgrp>
    <trd lng="pt">manco</trd>
  </snc>
  <snc mrk="lam.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne ta&ubreve;ga, mal&gcirc;usta, nekompleta:
      <ekz>
        <tld/>a tablo, benko;
      </ekz>
      <ekz>
        <tld/>aj <klr>(nekonformaj al la reguloj)</klr> versoj;
      </ekz>
      <ekz>
        <tld/>a rezono;
      </ekz>
      <ekz>
        <tld/>e pensi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la afero iras <tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>lahm</trd>,
      <trd>wackelig</trd>,
      <trd>defekt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>lame</trd>,
      <trd>weak</trd>,
      <trd>feeble</trd>
    </trdgrp>
    <trd lng="nl">mank</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_v;&c_y;</trd>
  <trd lng="ca">coix</trd>
  <trdgrp lng="br">
    <trd>kamm<klr tip="ind"> (ag.)</klr></trd>,
    <trd>jilgamm<klr tip="ind"> (ag.)</klr></trd>
  </trdgrp>
  <trd lng="es">cojo</trd>
  <trdgrp lng="fr">
    <trd>boiteux</trd>,
    <trd>&eacute;clop&eacute;</trd>
  </trdgrp>
  <trd lng="hu">s&aacute;nta</trd>
  <trdgrp lng="pt">
    <trd>manco</trd>,
    <trd>capenga</trd>,
    <trd>coxo</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_o;&c_j;</trd>
</drv>

<drv mrk="lam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lam.0i.MED">
    <dif>
      Esti <tld/>a, mistakte, malfacile iri.
    </dif>
    <trdgrp lng="br">
      <trd>kamma&ntilde;<klr tip="ind"> (v.g.)</klr></trd>,
      <trd>jilgamma&ntilde;</trd>
    </trdgrp>
    <trd lng="ca">coixejar</trd>
    <trd lng="de">lahmen</trd>
    <trd lng="es">cojear</trd>
    <trd lng="fr">boiter</trd>
    <trd lng="nl"><ind>mank</ind> lopen</trd>
  </snc>
  <snc mrk="lam.0i.FIG">
    <dif>
      Ne ta&ubreve;ge funkcii:
      <ekz>
        la ritmo, la komparo beda&ubreve;rinde <tld/>as;
      </ekz>
      <ekz>
        la propagando nun <tld/>as
        <klr>(estas malrapida kaj malmulte sukcesa)</klr>;
      </ekz>
      <ekz>
        io <tld/>as en nia movado.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>hinken</trd>,
      <trd>wackeln</trd>,
      <trd>nicht funzen</trd>
    </trdgrp>
    <trd lng="br">mont <ind>a-dreuz</ind></trd>
    <trd lng="fr">aller de <ind>travers</ind></trd>
    <trd lng="nl">niet goed <ind>functioneren</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_b;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="hu">s&aacute;nt&iacute;t</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="lam.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo malfacile iranta pro malbona kruro a&ubreve; piedo:
      <ekz>
        blinduloj kaj <tld/>uloj venis al li en la templo, kaj li
        sanigis
        ilin
        <fnt><bib>NT</bib>, <lok>&Mat; 21:14</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="kripl.0ulo">kriplulo</ref>
  </snc>
  <trdgrp lng="br">
    <trd>kamm<klr tip="ind"> (ak.)</klr></trd>,
    <trd>jilgamm<klr tip="ind"> (ak.)</klr></trd>
  </trdgrp>
  <trd lng="ca">coix</trd>
  <trd lng="es">cojo</trd>
  <trd lng="fr">boiteux<klr> (subst.)</klr></trd>
  <trd lng="nl">lamme</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_o;&c_j;<klr tip="amb"> (&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>
</art>
