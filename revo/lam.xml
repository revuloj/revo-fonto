<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lam</rad>/a
</kap>
<drv mrk="lam.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Malfacile pa&scirc;anta pro kripleco de kruro a&ubreve; piedo.
    </dif>
    <trd lng="de">lahm</trd>
    <trdgrp lng="en">
      <trd>lame</trd>,
      <trd>halt</trd>,
      <trd>hobbling</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>mank</trd>,
      <trd>kreupel</trd>,
      <trd>hinkend</trd>
    </trdgrp>
    <trd lng="pt">manco</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne ta&ubreve;ga, mal&gcirc;usta, nekompleta:
      <ekz>
        <tld/>a tablo, benko;
      </ekz>
      <ekz>
        <tld/>aj (nekonformaj al la reguloj) versoj;
      </ekz>
      <ekz>
        <tld/>a rezono;
      </ekz>
      <ekz>
        <tld/>e pensi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la afero iras <tld/>e;
      </ekz>
      <ekz>
        <tld/>bastono.
      <trd lng="hu">mank&aacute;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>lahm</trd>,
     <trd>wackelig</trd>,
     <trd>defekt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>lame</trd>,
      <trd>weak</trd>,
      <trd>feeble</trd>
    </trdgrp>
    <trd lng="nl">mank</trd>
  </snc>
  <trd lng="hu">s&aacute;nta</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_o;&c_j;</trd>
</drv>

<drv mrk="lam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ritmo, la komparo beda&ubreve;rinde <tld/>as.
      </ekz>
    </dif>
    <trd lng="de">lahmen</trd>
    <trd lng="nl">mank lopen</trd>
  </snc>
  <snc>
    <dif>
      Ne ta&ubreve;ge funkcii:
      <ekz>
        la propagando nun <tld/>as
        <klr>(estas malrapida kaj malmulte sukcesa)</klr>;
      </ekz>
      <ekz>
        io <tld/>as en nia movado.
      </ekz>
    </dif>
    <trd lng="nl">niet goed <ind>functioneren</ind></trd>
    <trdgrp lng="de">
     <trd>hinken</trd>,
     <trd>wackeln</trd>,
     <trd>nicht funzen</trd>
    </trdgrp>
  </snc>
  <trd lng="hu">s&aacute;nt&iacute;t</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
