<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fu&scirc;</rad>/i
</kap>
<drv mrk="fusx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malbone fari pro intenca a&ubreve; nevola senzorgeco; fara&ccirc;i:
      <ekz>la tajloro <tld/>is mian veston;</ekz>
      <ekz>vi <tld/>is la kuira&jcirc;on;</ekz>
      <ekz>la lernanto <tld/>is sian taskon.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sabot.0i">saboti</ref>.
    </refgrp>
  </snc>
  <trd lng="en">bungle</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_je;-&c_k;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_s;&c_p;&c_je;&c_h;</trd>,
    <trd>&c_s;&c_h;&c_a;&c_l;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fusx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="F"/>ita tasko, objekto:
      <ekz>tia libro estas nur <tld/>a&jcirc;o.</ekz>
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_h;&c_a;&c_l;&c_t;&c_u;&c_r;&c_n;&c_a;&c_ja; &c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
</drv>
<drv mrk="fusx.0ulo0isto">
  <kap><tld/>ulo, <tld/>isto</kap>
  <snc>
    <dif>
      Homo kiu kutime <tld/>as. 
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_k;&c_o;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_t;&c_u;&c_r;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
<drv mrk="fusx.0palpi">
  <kap><tld/>palpi</kap>
  <snc>
    <dif>
      Difekti palpante, maldelikate palpi. 
    </dif>
  </snc>
</drv>
<drv mrk="fusx.0uzi">
  <kap><tld/>uzi</kap>
  <snc>
    <dif>
      Difekti uzante, malbone uzi. 
    </dif>
    <refgrp tip="sin">
      <ref cel="mis.0">mis</ref>,
      <ref cel="acx">a&ccirc;</ref>.
    </refgrp>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
