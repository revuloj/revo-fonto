<drv mrk="fusx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; ties rezulto:
      <ekz>
        mi provis fari ordon en mia kapo, kaj mi sentas, ke tia <tld/>o ne estas
        tolerinda
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas <tld/>o
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiel li povos eviti superfluajn malkorekta&jcirc;ojn, mal&gcirc;entila&jcirc;ojn a&ubreve; e&ccirc; <tld/>on de
        la tuta parolado
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili timegis, <klr>[...]</klr> ke la tuta urbo parolos pri ilia
        misfaro, pri tiu gravega <tld/>o
        <fnt><bib>Lasu</bib>, <lok>Jen vi ree trinkas!</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_y;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_h;&c_ib;&c_b;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_h;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_m;&c_a;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pfusch</trd>,
    <trd>Murks</trd>
  </trdgrp><trd lng="es">chapuza</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;vue</trd>,
    <trd>g&acirc;chis</trd>,
    <trd>gaffe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hiba</trd>,
    <trd>balfog&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stommiteit</trd>,
    <trd>knoeiboel</trd>
  </trdgrp>
  <trdgrp lng="ru">
