<drv mrk="fusx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malbone fari pro intenca a&ubreve; nevola senzorgeco; fara&ccirc;i:
      <ekz>
        la verko estas iom <tld/>ita kaj neniom sprita
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi volas <klr>[&#8230;]</klr> scii pri nenio, kio povus <tld/>i mian vivon kaj detrui
        &gcirc;ian belecon
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro tiu malharmonio <klr>[&#8230;]</klr> la tuta parolado, a&ubreve; almena&ubreve; grava &gcirc;ia parto,
        estas <tld/>ita
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> tiel <tld/>is la aferojn, ke oni ne povis reordigi ilin dum
        kelkaj monatoj
        <fnt>
          <bib>TAA</bib>
        </fnt>;
      </ekz>
      <ekz>
        kial la animon, kiel ion <tld/>itan kaj konfuzitan, ni devas sen&ccirc;ese analizi tra
        longaj libroj
        <fnt>
          <bib>VaK</bib>
        </fnt>?
      </ekz>
      <ekz>
        ili ne nur detran&ccirc;igis siajn orellobojn, sed krome klopodis per &ccirc;iaj neimageblaj
        fia&jcirc;oj <tld/>i la naturan belecon de la korpo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi nenion misfaris, mi nenion <tld/>is, mi nenion malbonigis:
        nur okazis tio, ke mi malsukcesis
        <fnt><bib>Lasu</bib>, <lok>Malkontentas la
        me&hcirc;anikisto</lok></fnt>;
      </ekz>
      <ekz>
        tute ne estas nature, ke la motoro tiamaniere <tld/>i&gcirc;is, se
        li agis normale
        <fnt><bib>Lasu</bib>, <lok>Malkontentas la
        me&hcirc;anikisto</lok></fnt>;
      </ekz>
      <ekz>
        mi ne imagis tiuepoke, ke vi <tld/>os &ccirc;ion &ccirc;i per
        brandemo, ke vi <tld/>os vin mem, ke vi <tld/>os nian kunan
        vivon, manke de volo
        <fnt><bib>Lasu</bib>, <lok>Jen vi ree trinkas!</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="bon.mal0igi">malbonigi</ref>,
      <ref cel="sabot.0i">saboti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
&c_s;&c_mol;&c_p;&c_je;&c_h;&c_a;&c_m;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_a;&c_b;&c_y;-&c_ja;&c_k;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_t;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pfuschen</trd>,
    <trd>verpfuschen</trd>,
    <trd>verhunzen</trd>,
    <trd>verderben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bungle</trd>,
    <trd>botch</trd>,
    <trd>mess up</trd>
    <trd>screw up</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hacer mal</trd>,
    <trd>hacer chapuceramente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>b&acirc;cler</trd>,
    <trd>bousiller<klr tip="ind"> (g&acirc;cher)</klr></trd>,
    <trd>g&acirc;cher<klr tip="ind"> (bousiller)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elront</trd>,
    <trd>elhib&aacute;z</trd>,
    <trd>elpusk&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4ED5;&#x640D;&#12376;&#x308B;  <pr>&#12375;&#x305D;&#12435;&#12376;&#x308B;</pr></trd>,
    <trd>&#x53F0;&#x306A;&#12375;&#x306B;&#12377;&#x308B;  <pr>&#12384;&#12356;&#x306A;&#12375;&#x306B;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>knoeien</trd>,
    <trd>verknoeien</trd>
  </trdgrp>
  <trdgrp lng="pt">
