    <trd>atamancar</trd>,
    <trd>fazer &agrave;s pressas</trd>,
    <trd>borrar <klr tip="amb">(pintura, escrita)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
&c_k;&c_o;&c_je;-&c_k;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
&c_n;&c_a;&c_s;&c_p;&c_je;&c_h;</trd>,
    <trd>&c_s;&c_h;&c_a;&c_l;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_t;&c_mol;
&c_d;&c_je;&c_l;&c_o;</trd>
  </trdgrp>
  <trd lng="tok">pakala</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_shch;&#x43E;-&#x43D;. &#x43D;&c_a;&c_s;&#x43F;&c_ib;&c_h;</trd>,
    <trd>&#x43D;&c_je;&c_v;&#x43C;&c_ib;&#x43B;&#x43E;</trd>,
    <trd>&#x43D;&c_je;&c_d;&c_b;&c_a;&#x43B;&#x43E;</trd>,
    <trd>&c_a;&c_b;&c_i;&#x44F;&#x43A;</trd>,
    <trd>&#x43A;&c_je;&#x43F;&c_s;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&c_h;&c_a;&#x43B;&c_t;&c_u;&c_r;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#20599;&#x5DE5;&#x51CF;&#26009; <pr>t&#x14D;ug&#x14D;ngji&#x1CE;nli&#xE0;o</pr></trd>
</drv>

<drv mrk="fusx.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        la &scirc;nuro estis <klr>[&#8230;]</klr> <tld/>e nodita
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vizitanto <klr>[&#8230;]</klr> konfuzi&gcirc;is kaj <tld/>e klarigis celon de sia vizito
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        vanta tromemfido igas ilin agi <tld/>e
        <fnt>
          <bib>CKv</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia koro kapriolis <tld/>e-mise, jen haltis, jen galopis
        <fnt>
          <bib>ChC</bib>
          <lok>18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mis.0e">mise</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_y;-&c_ja;&c_k;</trd>,
    <trd>&c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_a;&c_ja;&c_k;&c_a;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_s;&c_l;&c_u;&c_sh;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="de">verkehrt</trd>
</drv>

<drv mrk="fusx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o; <tld/>anta; <tld/>ata:
      <ekz>
        tro malfrua kaj eble <tld/>a pretigo de la kampoj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Alice</nom> iom ridetis kaj petis en <tld/>a rusa lingvo, ke &scirc;in oni direktu
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni sukcesis preska&ubreve; tute detrui la <tld/>an agadon de la
        idistoj en niaj laboristaj rondoj
        <fnt><bib>Lanti</bib>, <lok>Pri la Biblio</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la supraj principoj]</klr> estas sen&ccirc;ese malobeataj,
        <tld/>e aplikataj de tiuj, kiujn oni kutimas nomi niaj plej
        bonaj
        a&ubreve;toroj
        <fnt><bib>Lanti</bib>, <lok>Pri stilo</lok></fnt>;
      </ekz>
      <ekz>
        kiu ne havas idealon, tiu iel similas al ia <tld/>a radofaristo,
        kiu tute ne zorgus, ke la radoj estu cirkloformaj
        <fnt><bib>Lanti</bib>, <lok>Idealo estas necesa</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_a;&c_l;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_s;&c_p;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_m;&c_je;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hib&aacute;s</trd>,
    <trd>rossz</trd>,
    <trd>elpusk&aacute;zott</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4ED5;&#x640D;&#12376;&#x306E;  <pr>&#12375;&#x305D;&#12435;&#12376;&#x306E;</pr></trd>,
    <trd>&#x51FA;&#26469;&#x640D;&#x306A;&#12356;&#x306E;  <pr>&#12391;&#x304D;&#x305D;&#12371;&#x306A;&#12356;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">knoei-</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_k;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&c_i;&c_j; &#x43D;&c_a;&c_s;&#x43F;&c_ib;&c_h;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&c_i;&c_j;
    &#x43D;&c_je;&c_d;&c_b;&c_a;&#x43B;&#x43E;/&#x43D;&c_je;&c_v;&#x43C;&c_ib;&#x43B;&#x43E;/&c_a;&c_b;&c_i;&#x44F;&#x43A;/&#x43A;&c_je;&#x43F;&c_s;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&#x43F;&c_a;&c_r;&c_t;&c_a;&c_c;&#x44C;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_ib;&#x43F;&c_s;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_a;&#x43B;&c_t;&c_u;&c_r;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
