<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: speleologi.xml,v 1.6 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>speleologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="speleologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="speleologi.0o.SCI">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="vid" cel="kavern.0o.KOMUNE">Kavern</ref>esplorado, 
      studo de la subtera fizika kaj viva medio:
      <ekz>
         J&aacute;n Vajs <klr>...</klr> inter la jaroj 1989 &ndash; 1994
         organizis
         5 
         ekspediciojn en E&ubreve;ropo kaj Mongolio dum kiuj estis
         kunigitaj 
         <tld/>o, Esperanto kaj turismo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.lek.szm.sk/Membroj/Vajs.htm">
          Membroj de lek</url></vrk>,
          <lok>[vidita en 2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">espeleologia</trd>
  <trdgrp lng="cs">
    <trd>speleologie</trd>,
    <trd>věda o jeskyních</trd>
  </trdgrp>
  <trd lng="fr">sp&eacute;l&eacute;ologie</trd>
  <trdgrp lng="nl">
    <trd>holenkunde</trd>,
    <trd>speleologie</trd>
  </trdgrp>
  <trd lng="pl">speleologia</trd>
  <trd lng="sk">veda o jaskyniach</trd>
</drv>

<drv mrk="speleologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="speleologi.0isto.SCI">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fakulo pri <ref tip="vid"
      cel="speleologi.0o.SCI">speleologio</ref>:
      <ekz>
        ekde kelkaj jaroj la <tld/>istoj de Narni, &ccirc;arma urbeto de
        
        Umbrio (centra Italio) eldonas belan retejon, kiu anta&ubreve;
        nelonge
        
        i&gcirc;is internacie konata
        <fnt>
          <aut>G. Aquilina</aut>,
          <vrk><url ref="http://esperanto-usa.org/en/node/766">
          Grava speleologia retejo...</url></vrk>,
          <lok>2007-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">espele&ograve;leg</trd>
  <trd lng="fr">sp&eacute;l&eacute;ologiste</trd>
  <trdgrp lng="nl">
    <trd>speleoloog</trd>,
    <trd>holenkundige</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>speleolog</trd>,
    <trd>groto&lstroke;az</trd>
  </trdgrp>
</drv>
</art>

