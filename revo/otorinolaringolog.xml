<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>otorinolaringolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="otorinolaringolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="otorinolaringolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="otorinolaringologi.0o"><tld/>io</ref>.
    </dif>
    <rim>
      PIV mencias anka&ubreve; la terminon <em>laringologo</em>
      <fnt>
        <aut>Plena Ilustrita Vortaro</aut>,
        <vrk><url
        ref="http://vortaro.net/#laringologo">laringologo</url></vrk>
      </fnt>,
      kiel sinonimon de tia specialisto.
    </rim>
  </snc>
  <trdgrp lng="en">
    <trd>otorhinolaryngologist</trd>,
    <trd>otolaryngologist</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oto-rhino-laryngologiste</trd>,
    <trd>ORL</trd>
  </trdgrp>
</drv>
</art>

