<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: otorinolaringolog.xml,v 1.3 2019/02/02 11:10:25 revo Exp $">
<kap>
    <rad>otorinolaringolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="otorinolaringolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="otorinolaringolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="otorinolaringologi.0o"><tld/>io</ref>:
      <ekz>
        <nom>Alad&aacute;r T&oacute;th</nom> <klr>[&#8230;]</klr> estis hungara kuracisto, <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=527123">Alad&aacute;r T&oacute;th (kuracisto)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      PIV mencias anka&ubreve; la terminon <em>laringologo</em>
      <fnt>
        <aut>Plena Ilustrita Vortaro</aut>,
        <vrk><url ref="http://vortaro.net/#laringologo">laringologo</url></vrk>
      </fnt>,
      kiel sinonimon de tia specialisto.
    </rim>
    <trdgrp lng="de">
      <trd>HNO-Arzt</trd>,
      <trd>Hals-Nasen-Ohren-Arzt</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>otorhinolaryngologist</trd>,
    <trd>otolaryngologist</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oto-rhino-laryngologiste</trd>,
    <trd>ORL</trd>
  </trdgrp>
</drv>
</art>

