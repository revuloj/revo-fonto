<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>polen</rad>/o
</kap>
<drv mrk="polen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      La fekundiga polvo de floroj.
    </dif>
  </snc>
  <trd lng="de">Pollen</trd>
  <trdgrp lng="nl">
    <trd>pollen</trd>,
    <trd>stuifmeel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pollen</trd>,
    <trd>vir&aacute;gpor</trd>,
    <trd>h&iacute;por</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_y;&c_l;&c_mol;&c_c;&c_a;</trd>
  <trd lng="sv">pollen</trd>
</drv>

<drv mrk="polen.0ujo">
  <kap><tld/>ujo, <tld/>sako</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="anter.0o">antero</ref>,
      sako en kiu estas formata la
      <ref tip="prt" cel="polen.0o">poleno</ref>.
    </dif>
  </snc>
  <trd lng="de">Pollensack</trd>
  <trd lng="hu">pollenzs&aacute;k</trd>
</drv>
</art>
