<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>polen</rad>/o
</kap>
<drv mrk="polen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      La fekundiga polvo de floroj.
    </dif>
  </snc>
  <trd lng="de">Pollen</trd>
</drv>
<drv mrk="polen.0ujo">
  <kap><tld/>ujo, <tld/>sako</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="anter.0o">antero</ref>,
      sako en kiu estas formata la
      <ref tip="prt" cel="polen.0o">poleno</ref>.
    </dif>
  </snc>
  <trd lng="de">Pollensack</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
