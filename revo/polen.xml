<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polen.xml,v 1.26 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>9</ofc> <rad>polen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="polen.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="polen.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      La fekundiga polvo de floroj:
      <ekz>
        la &ccirc;efa celo de la <tld/>o estas fekundigi la inajn
        flororganojn
        <fnt>
          <aut>R. Roze</aut>,
          <vrk>Vivisekcio de polenero</vrk>,
          <bib>Monato</bib>,
          <lok>1997:11, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        escepte de nur malmultaj <tld/>oj, kiuj povis
        atingi la pistilojn, sennombraj polenoj pereas
        en la lago
        <fnt>
          <aut>&Scirc;. Hori</aut>
          <vrk><url ref="&STEB;biologio/la-volvotigo-de-amikeco.pdf">
          La Volvotigo de Amikeco</url></vrk>
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_l;&c_o;&c_k; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_y;)</klr></trd>
  <trd lng="bg">&c_c;&c_v;&c_je;&c_t;&c_je;&c_n; <ind>&c_p;&c_r;&c_a;&c_sh;&c_je;&c_c;</ind></trd>
  <trd lng="de">Pollen</trd>
  <trd lng="en">pollen</trd>
  <trd lng="fr">pollen</trd>
  <trdgrp lng="hu">
    <trd>pollen</trd>,
    <trd>vir&aacute;gpor</trd>,
    <trd>h&iacute;por</trd>
  </trdgrp> 
  <trd lng="ja">&#x82B1;&#x7C89;  <pr>&#x304B;&#x3075;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>pollen</trd>,
    <trd>stuifmeel</trd>
  </trdgrp>
  <trd lng="pl">py&lstroke;ek  <klr>(bot.)</klr></trd>
  <trd lng="pt">p&oacute;len</trd>
  <trd lng="ro">polen</trd>
  <trd lng="ru">&c_p;&c_y;&c_l;&c_mol;&c_c;&c_a;</trd>
  <trd lng="sv">pollen</trd>
  <trd lng="uk">&#x43F;&c_i;&#x43B;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="polen.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="polen.0ero.BOT">
    <dif>
      <tld lit="P"/>a semeto, maksimume kelkcent mikrometrojn granda,
      kun aspekto kaj ecoj
      propraj de la devena florspecio:
      <ekz>
        la <tld/>eroj estas la plej multenombraj el la spermatozooj sur
        nia planedo
        <fnt>
          <aut>R. Roze</aut>,
          <vrk>Vivisekcio de polenero</vrk>,
          <bib>Monato</bib>,
          <lok>1997:11, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_l;&c_k;&c_o;&c_v;&c_a;&c_je; &c_z;&c_je;&c_r;&c_n;&c_je;</trd>
  <trd lng="bg">&c_z;&c_malmol;&c_r;&c_n;&c_o; &c_n;&c_a; &c_p;&c_r;&c_a;&c_sh;&c_je;&c_c;</trd>
  <trd lng="cs">pylov&eacute; zrnko</trd>
  <trd lng="en">grain of pollen</trd>
  <trd lng="fr">grain de <ind>pollen</ind></trd>
  <trd lng="ja">&#x82B1;&#x7C89;&#x7C92;  <pr>&#x304B;&#x3075;&#x3093;&#x3064;&#x3076;</pr></trd>
  <trd lng="nl">stuifmeelkorrel</trd>
  <trd lng="pl">drobinka py&lstroke;ku kwiatowego</trd>
  <trd lng="ro">o bucat&abreve; de polen de flori</trd>
  <trd lng="sk">pe&lcaron;ov&eacute; zrnko</trd>
  <trd lng="uk">&#x43F;&c_i;&#x43B;&c_i;&#x43D;&#x43A;&c_a;</trd>
</drv>

<drv mrk="polen.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>sako</kap></var></kap>
  <snc mrk="polen.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="anter.0o">antero</ref>,
      sako en kiu estas formata la
      <ref tip="prt" cel="polen.0o">poleno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_l;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_e;&c_r;&c_a;</trd>
  <trd lng="cs">pra&scaron;n&iacute;k</trd>
  <trd lng="de">Pollensack</trd>
  <trd lng="fr">loge pollinique</trd>
  <trd lng="hu">pollenzs&aacute;k</trd>
  <trd lng="ja">&#x82B1;&#x7C89;&#x56A2;  <pr>&#x304B;&#x3075;&#x3093;&#x56A2;</pr></trd>
  <trd lng="nl">stuifmeelzak</trd>
  <trd lng="pl">pr&eogonek;cik  <klr>(bot.)</klr></trd>
  <trd lng="ro">stamin&abreve;</trd>
  <trdgrp lng="sk">
    <trd>pra&scaron;n&iacute;k</trd>,
    <trd>ty&ccaron;inka</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_i;&#x43B;&#x44F;&#x43A;</trd>
</drv>
</art>
