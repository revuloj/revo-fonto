<kap>
  <rad>delkreder</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="delkre.0o">
  <kap><tld/>o</kap>
  <snc mrk="delkre.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="garant.0o">Garantio</ref>
      prenita sur sin pri solventeco a&ubreve; pagokapableco de iu:
      <ekz>
        en malliberejo <klr>[&#8230;]</klr> li restis dum jaro kaj preska&ubreve; mortis, nur pro
        <tld/>o <klr>[&#8230;]</klr> oni finfine liberigis lin
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=160612">Lev Landau</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_mol;&c_k;&c_r;&c_e;&c_d;&c_e;&c_r;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>ru&ccaron;en&iacute;</trd>,
    <trd>ru&ccaron;itelstv&iacute;</trd>,
    <trd>&uacute;v&ecaron;rov&eacute; ru&ccaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="de">Delkredere</trd>
  <trd lng="en">delcredere</trd>
  <trd lng="fr">ducroire</trd>
  <trd lng="he">&#x5E2;&#x5E8;&#x5D1;&#x5D5;&#x5EA; &#x5DB;&#x5E1;&#x5E4;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">hitelszavatoss&aacute;g</trd>
  <trd lng="ja">&#x652F;&#25173;&#x4FDD;&#x8A3C;  <pr>&#12375;&#x306F;&#12425;&#12356;&#x307B;&#12375;&#12423;&#12358;</pr></trd>
  <trd lng="nl">delcredere</trd>
  <trd lng="pl">por&eogonek;czenie <klr> (finansowe)</klr></trd>
  <trd lng="pt">del credere</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">ru&ccaron;enie</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&#x43B;&#x44C;&#x43A;&c_r;&c_je;&c_d;&c_je;&c_r;&c_je;</trd>,
    <trd>&#x43F;&#x43E;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_je;&#x43B;&#x44C;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&c_gu;&c_a;&c_r;&c_a;&#x43D;&c_t;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
