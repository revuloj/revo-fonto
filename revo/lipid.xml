<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lipid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="lipid.0o">
  <kap><tld/>o</kap>
  <snc mrk="lipid.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Grasa substanco en la konsisto de nutra&jcirc;o:
      <ekz>
        <tld/>oj, nome grasa&jcirc;oj, estas en pasta&jcirc;o praktike nulaj 
        (malpli ol 1%)
        <fnt>
          <aut>M. Picasso</aut>,
          <vrk><url ref="http://www.luin.se/inko/075-9.pdf">Pasta&jcirc;o, konsideroj kaj historio</url></vrk>,
          <lok>Inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lipide</trd>,
    <trd>Fett</trd>
  </trdgrp>
  <trd lng="en">lipid</trd>
  <trd lng="fr">lipide</trd>
  <trd lng="nl">lipide</trd>
  <trdgrp lng="pt">
    <trd>lip&iacute;dio</trd>,
    <trd>lip&iacute;deo</trd>,
    <trd>l&iacute;pide</trd>,
    <trd>l&iacute;pido</trd>    
  </trdgrp>
</drv>
</art>

