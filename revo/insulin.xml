  <ofc>9</ofc><rad>insulin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="insulin.0o">
  <kap><tld/>o</kap>
  <snc mrk="insulin.0o.MED">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="hormon.0o">Hormono</ref>,
      kiu reguligas la karbonhidratan
      <ref tip="vid" cel="metabol.0o">metabolon</ref>:
      <ekz>
        <tld/>o estas rimedo kontra&ubreve;
        <ref tip="vid" cel="diabet.0o">diabeto</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_s;&c_u;&c_l;&c_ib;&c_n;</trd>
  <trd lng="cs">inzul&#xED;n</trd>
  <trd lng="de">Insulin</trd>
  <trd lng="en">insulin</trd>
  <trd lng="es">insulina</trd>
  <trd lng="fa">&a_A;&a_n;&a_s;&a_w;&a_l;&f_y;&a_n;</trd>
  <trd lng="fr">insuline</trd>
  <trd lng="hu">inzulin</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30F3;&#x30B7;&#x30E5;&#x30EA;&#x30F3;</trd>,
    <trd>&#x30A4;&#x30F3;&#x30B9;&#x30EA;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">insuline</trd>
  <trd lng="pt">insulina</trd>
  <trd lng="ru">&c_i;&c_n;&c_s;&c_u;&c_l;&c_i;&c_n;</trd>
  <trd lng="sk">inzul&#xED;n</trd>
  <trd lng="uk">&#x456;&#x43D;&#x441;&#x443;&#x43B;&#x456;&#x43D;</trd>
  <trd lng="zh">&#x80F0;&#x5C9B;&#x7D20; <pr>y&#xED;d&#x1CE;os&#xF9;</pr></trd>
</drv>
