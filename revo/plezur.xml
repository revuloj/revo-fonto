<drv mrk="plezur.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plezur.mal0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi mal<tld/>on al...:
      <ekz>
        por ke li tiel malafable min mal<tld/>igu <klr>[&#8230;]</klr>, estos necese, ke li
        havu potencegajn motivojn
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne &gcirc;enu, ne mal<tld/>igu la infanfestojn
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2013/011169.php">&Ccirc;u rasisma? Neniel!</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne mal<tld/>igu min post la penado, kiun mi suferis
        <fnt>
          <aut>-</aut>,
          <vrk>-</vrk>,
          <lok><url ref="http://tatoeba.org/">
          Tatoeba, [vidita en 2014]</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_n;&c_je;&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>znechutit</trd>,
    <trd>znep&rcaron;&iacute;jemnit</trd>
  </trdgrp>
  <trd lng="en">displease</trd>
  <trdgrp lng="fr">
    <trd>contrarier</trd>,
    <trd>f&acirc;cher</trd>
  </trdgrp>
  <trd lng="pl">sprawi&cacute;  <ind>przykro&sacute;&cacute;</ind></trd>
  <trd lng="pt">desagradar</trd>
  <trdgrp lng="ro">
    <trd>ma intristeaza</trd>,
    <trd>s&abreve; faci r&abreve;u</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>znechuti&tcaron;</trd>,
    <trd>znepr&iacute;jemni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.kun_0o">
  <kap>
    kun <tld/>o,
    <var><kap><tld/>e</kap></var>
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
      &Gcirc;entila esprimo por montri sin <ref tip="sin" cel="pret.servo0a">servopreta</ref>, <ref
      tip="sin" cel="help.0ema">helpema</ref>;
      <ref tip="sin" cel="kompre.mem0eble">memkompreneble</ref>:
      <ekz>
        <klr>[mi]</klr> kun <tld/>o parolos kun vi senceremonie
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        Donu &gcirc;in al mi prunte &#8208; rapide! Certe, certe ... kun granda <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjorino Anneto kun <tld/>o plenumis lian modestan deziron, &mdash; &scirc;i donis al li
        sian adreson, prenis de li lian
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        al la malamik' en kuro faru ponton kun <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Jes, <tld/>e, mia estimata sinjorino, <tld/>e.
        <fnt>
          <bib>MrA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cert.0e">certe</ref>,
       <ref cel="jes.0ja.emfaza">jes ja<sncref/></ref>,
       <ref cel="klar.0e.kompreneble">klare<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;
    <ind>&c_z;&c_a;&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;&c_m;</ind></trd>,
    <trd>&c_z; <ind>&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mit <ind>Vergn&uuml;gen</ind></trd>,
    <trd>gerne</trd>
  </trdgrp>
  <trd lng="en">with <ind>pleasure</ind> <klr>(being of help)</klr></trd>
  <trd lng="fr">avec <ind>plaisir</ind></trd>
  <trd lng="id">dengan <ind>senang</ind> hati</trd>
  <trd lng="pl">z  <ind>przyjemno&sacute;ci&aogonek;</ind></trd>
  <trd lng="pt">com prazer</trd>
  <trd lng="ro">cu  <ind>pl&abreve;cere</ind></trd>
  <trd lng="zh">&#27941;&#27941;&#26377;&#21619; <pr>j&imacron;nj&imacron;ny&#466;uw&egrave;i</pr></trd>
</drv>

