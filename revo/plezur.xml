    <trd>Wohlgefallen</trd>,
    <trd>Behagen</trd>
  </trdgrp>
  <trd lng="en">pleasure</trd>
  <trd lng="es">placer</trd>
  <trd lng="fr">plaisir</trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&ouml;m</trd>,
    <trd>gy&ouml;ny&ouml;r</trd>
    <trd>&eacute;lvezet</trd>,
    <trd>megel&eacute;gedetts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>gembira</baz>kegembiraan</trd>,
    <trd><baz>nikmat</baz>kenikmatan</trd>,
    <trd><baz>puas</baz>kepuasan</trd>,
    <trd><baz>senang</baz>kesenangan</trd>,
    <trd><ind>senang</ind> hati</trd>
  </trdgrp>
  <trdgrp lng="ja">
