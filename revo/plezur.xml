<drv mrk="plezur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plezur.0o.KOMUNE">
    <dif>
      Agrabla sento de <ref cel="stat.bon0o">bonstato</ref>, komforteco, 
      <ref cel="gxoj.0o.plezuro">&gcirc;ojo</ref>, kiun naskas
      kontentigo de deziro a&ubreve; inklino:
      <ekz>
        <klr>[ili]</klr> tiel la&ubreve;te kaj agrable kantas, ke estas <tld/>o ilin a&ubreve;di
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia sola <tld/>o estis sidi en sia &gcirc;ardeneto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas por mi <tld/>o vidi, ke vi nun iom ripozas, li diris al sia edzino
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ru&gcirc;i&gcirc;is de <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        veturo de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>o oni aran&gcirc;as festenojn
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 10:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> trovas <tld/>on en la malordo de la malboneco
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 2:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj karesoj &gcirc;uigu vin <klr>[&#x2026;]</klr> &scirc;ia amo &ccirc;iam donu al vi <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        ser&ccirc;u <tld/>on &ccirc;e la Eternulo, kaj Li plenumos la dezirojn de via koro
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 37:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas nenio pli bona, ol ke homo &gcirc;uu <tld/>on de siaj faroj
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 3:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi en silko kaj veluro, en &gcirc;ojo kaj <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        malhumileco estas kara <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>anta&ubreve; &ccirc;io zorgu oficon &dash; <tld/>o atendos sian vicon</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">erst die Arbeit, dann das <ind>Vergn&uuml;gen</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxu.0o.plezuro">&gcirc;uo<sncref/></ref>,
      <ref cel="volupt.0o">volupto</ref>
    </refgrp>
    <ref tip="vid" cel="plezur.kun_0o">kun <tld/>o</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_a;&c_l;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>plaer</trd>,
    <trd>delit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pot&ecaron;&scaron;en&iacute;</trd>,
    <trd>pot&ecaron;&scaron;en&iacute;</trd>,
    <trd>radost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vergn&uuml;gen</trd>,
    <trd>Freude</trd>,
    <trd>Wohlgefallen</trd>,
    <trd>Behagen</trd>
  </trdgrp>
  <trd lng="en">pleasure</trd>
  <trd lng="es">placer</trd>
  <trd lng="fr">plaisir</trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&ouml;m</trd>,
    <trd>gy&ouml;ny&ouml;r</trd>
    <trd>&eacute;lvezet</trd>,
    <trd>megel&eacute;gedetts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plezier</trd>,
    <trd>genoegen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prazer</trd>,
    <trd>deleite</trd>,
    <trd>del&iacute;cia</trd>,
    <trd>regalo</trd>,        
    <trd>gosto</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>pote&scaron;enie</trd>,
    <trd>rados&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x434;&#x43E;&#x432;&#x43E;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x454;&#x43C;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0a">
  <kap><tld/>a</kap>
  <snc mrk="plezur.0a.KOMUNE">
    <dif>
      Plena je <tld/>o, akompanata de <tld/>o, 
      <ref tip="super" cel="agrabl.0a">agrabla</ref>:
      <ekz>
        liberan vivon havas ni, <tld/>an kaj sen mastro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konsideradis kiel bonan &ccirc;ion, kio estis al li <tld/>a
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&gcirc;o tro matura ne estas <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0a">&ccirc;arma</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>plaent</trd>,
      <trd>agradable</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vergn&uuml;glich</trd>,
      <trd>vergn&uuml;gt</trd>,
      <trd>angenehm</trd>
    </trdgrp>
    <trd lng="fr">agr&eacute;able</trd>
    <trd lng="nl">plezierig</trd>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="plezur.0a.amuza">
    <dif>
      Celanta <tld/>on:
      <ekz>
        <tld/>a voja&gcirc;o;
      </ekz>
      <ekz>
        la <tld/>aj artoj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        irante el la legoma al la 
        <ref tip="vid" cel="gxarde.plezur0o.HOR"><tld/>a &gcirc;ardeno</ref>
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>de plaer</trd>,
      <trd>per plaer</trd>
    </trdgrp>
    <trd lng="fr">d'<ind>agr&eacute;ment</ind></trd>
    <trd lng="nl">plezier-</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
  <trd lng="en">pleasurable</trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&ouml;mteli</trd>,
    <trd>gy&ouml;ny&ouml;rteli</trd>
    <trd>kellemes</trd>,
    <trd>&eacute;lvezetes</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x454;&#x43C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x440;&#x430;&#x434;&#x456;&#x441;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="plezur.0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        dia, <tld/>iga vivmaniero
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi plantis planta&jcirc;ojn <tld/>igajn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 17:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen <tld/>iga respondo, kiel ne venis al vi en la kapon diri, ke post monato oni
        povas scii&gcirc;i ankora&ubreve; pli bone
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvis la <tld/>iga tradicio iri al la kafejo
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj tre gajis, forgesis la zorgojn, kantis kaj ridis, kaj sonis tie tia
        <tld/>iga muziko
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">delectable</trd>
  <trdgrp lng="cs">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>Vergn&uuml;gen</ind> bereitend</trd>,
    <trd>erquickend</trd>
  </trdgrp>
  <trd lng="en">enjoyable</trd>
  <trd lng="fr">d&eacute;licieux</trd>
  <trdgrp lng="hu">
    <trd>&eacute;lvezetes</trd>,
    <trd>gy&ouml;ny&ouml;rk&ouml;dtet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">prettig</trd>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;
  <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>
  <trdgrp lng="sk">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="plezur.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on al...:
      <ekz>
        mi ser&ccirc;is en mia koro, kiel <tld/>igi mian korpon per vino
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 2:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        gesinjoroj de Prelongo, dezirante <tld/>igi sian filinon, konsentis veni &ccirc;iujare
        en Parizon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        kara Petro, por min <tld/>igi venu kun mi
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu vin kiel &ccirc;iuj aliaj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">delectar</trd>
  <trd lng="cs">pot&ecaron;&scaron;it</trd>
  <trd lng="de">Vergn&uuml;gen bereiten</trd>
  <trd lng="en">gratify</trd>
  <trd lng="fr">faire <ind>plaisir</ind> &agrave;</trd>
  <trd lng="hu">gy&ouml;ny&ouml;rk&ouml;dtet</trd>
  <trd lng="nl">plezier doen</trd>
  <trd lng="oc">agradar</trd>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
  <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>
  <trd lng="sk">pote&scaron;i&tcaron; <klr>(niekoho)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43D;&#x43E;&#x441;&#x438;&#x442;&#x438; &#x437;&#x430;&#x434;&#x43E;&#x432;&#x43E;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;/&#x43D;&#x430;&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x443;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x43F;&#x440;&#x438;&#x454;&#x43C;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x440;&#x430;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0i">
  <kap><tld/>i, <var><kap><tld/>i&gcirc;i</kap></var></kap>
  <snc>
    <dif>
      Senti <tld/>on:
      <ekz>
        &ccirc;iuj &ccirc;i bienoj estas viaj, je via komuna dispono por promenadi kaj, <tld/>i
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam falas via malamiko, ne &gcirc;oju, kaj &ccirc;e lia malfeli&ccirc;o via koro ne <tld/>i&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 24:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni &gcirc;uu sufi&ccirc;e volupton &gcirc;is la mateno, ni <tld/>i&gcirc;u per la amo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu, <tld/>u, Jozefo, <tld/>u &ndash; &ccirc;ar ega feli&ccirc;o trafis vin
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>!
      </ekz>
      <ekz>
        se <klr>[li]</klr> ne estus tiom turmentita <klr>[&#x2026;]</klr>, li ver&scirc;ajne <tld/>us
        rakontante pri la akcepto en la kliniko
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">delectar-se en</trd>
  <trd lng="en">take pleasure in</trd>
  <trd lng="fr">avoir <ind>plaisir</ind></trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43F;&#x440;&#x438;&#x454;&#x43C;&#x43D;&#x456;&#x441;&#x442;&#x44C;/&#x43D;&#x430;&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x443;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      <ref cel="agrabl.mal0ajxo">Malagrabla&jcirc;o</ref>, 
      <ref tip="sin" cel="cxagre.0o">&ccirc;agreno</ref>:
      <ekz>
        kun nuanco de mal<tld/>o en la vo&ccirc;tono
        <fnt>
          <aut>H. A. Luyken</aut>
          <vrk><url ref="&Gutenberg;ebooks/55954">Stranga
          hereda&jcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li &ccirc;iam esprimadis sian mal<tld/>on al la malfruinta studento
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro la multaj mal<tld/>oj li tute grizi&gcirc;is
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu sinjorino <klr>[&#x2026;]</klr> volas doni al vi lecionojn de franca lingvo,
        memoru, ke vi faru al &scirc;i nenian mal<tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la virino montris embarason kaj mal<tld/>on &ccirc;e la vido de fremdaj vizitantoj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dorn.0o">dorno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_d;&c_a;&c_v;&c_o;&c_l;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">disgust</trd>
  <trdgrp lng="cs">
    <trd>mrzutost</trd>,
    <trd>nep&rcaron;&iacute;jemnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Missvergn&uuml;gen</trd>,
    <trd>&Auml;rger</trd>
  </trdgrp>
  <trd lng="en">displeasure</trd>
  <trd lng="fr">d&eacute;plaisir</trd>
  <trdgrp lng="hu">
    <trd>f&aacute;jdalom</trd>,
    <trd>keser&udblac;s&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">misnoegen</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mrzutos&tcaron;</trd>,
    <trd>nepr&iacute;jemnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x43F;&#x440;&#x438;&#x454;&#x43C;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43D;&#x435;&#x432;&#x434;&#x43E;&#x432;&#x43E;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x434;&#x43E;&#x441;&#x430;&#x434;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x43C;&#x443;&#x447;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="plezur.mal0a.KOMUNE">
    <dif>
      Sena je <tld/>o, <ref cel="agrabl.mal0a">malagrabla</ref>:
      <ekz>
        la rideto &ccirc;irka&ubreve; &scirc;ia bu&scirc;o transiris en malestiman kaj mal<tld/>an esprimon
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bastonfrapoj komencis tamburi mal<tld/>an
        ritmon sur la dorso
        <fnt>
          <aut>M. Wajsblum</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5703/kankro.html">
          Kankro ne estas fi&scirc;o...</url></vrk>,
          <lok>Norda Prismo, 1957:3, p. 172a-174a</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen kaj jen li pro sia afabla komplezemo al fremduloj, kiuj ne &ccirc;iam uzas tion
        diskrete kaj takte, venis en mal<tld/>ajn situaciojn, kiuj solvi&gcirc;is nur pro
        la sa&gcirc;a kaj energia interveno de lia edzino
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;sagr&eacute;able</trd>
</drv>

<drv mrk="plezur.mal0iga">
  <kap>mal<tld/>iga</kap>
  <snc mrk="plezur.mal0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta mal<tld/>on:
      <ekz>
        pardonu min, se mi diros ion mal<tld/>igan
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;plaisant</trd>,
    <trd>f&acirc;cheux</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plezur.mal0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi mal<tld/>on al...:
      <ekz>
        por ke li tiel malafable min mal<tld/>igu <klr>[&#x2026;]</klr>, estos necese, ke li
        havu potencegajn motivojn
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne &gcirc;enu, ne mal<tld/>igu la infanfestojn
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2013/011169.php">&Ccirc;u rasisma? Neniel!</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne mal<tld/>igu min post la penado, kiun mi suferis
        <fnt>
          <aut>-</aut>,
          <vrk>-</vrk>,
          <lok><url ref="http://tatoeba.org/">
          Tatoeba, [vidita en 2014]</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>znechutit</trd>,
    <trd>znep&rcaron;&iacute;jemnit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contrarier</trd>,
    <trd>f&acirc;cher</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>znechuti&tcaron;</trd>,
    <trd>znepr&iacute;jemni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.kun_0o">
  <kap>
    kun <tld/>o
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc>    
    <dif>
      &Gcirc;entila esprimo por montri sin <ref tip="sin" cel="pret.servo0a">servopreta</ref>, <ref tip="sin" cel="help.0ema">helpema</ref>;
      <ref tip="sin" cel="kompre.mem0eble">memkompreneble</ref>:
      <ekz>
        <klr>[mi]</klr> kun <tld/>o parolos kun vi senceremonie
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        Donu &gcirc;in al mi prunte &dash; rapide! Certe, certe ... kun granda <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjorino Anneto kun <tld/>o plenumis lian modestan deziron, &mdash; &scirc;i donis al li
        sian adreson, prenis de li lian
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        al la malamik' en kuro faru ponton kun <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cert.0e">certe</ref>,
       <ref cel="jes.0ja.emfaza">jes ja<sncref/></ref>,
       <ref cel="klar.0e.kompreneble">klare<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>mit <ind>Vergn&uuml;gen</ind></trd>,
    <trd>gerne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>with <ind>pleasure</ind></trd>,
    <trd>surely</trd>,
    <trd>you are <ind>welcome</ind></trd>
  </trdgrp>
  <trd lng="fr">avec <ind>plaisir</ind></trd>
</drv>


