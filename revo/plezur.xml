    <trd>&#27426;&#23089; <pr>hu&amacron;ny&uacute;</pr></trd>,
    <trd>&#27489;&#23067; <pr>hu&amacron;ny&uacute;</pr></trd>,
    <trd>&#23067;&#27138;&#27963;&#21205; <pr>y&uacute;l&egrave;hu&oacute;d&ograve;ng</pr></trd>,
    <trd>&#23089;&#20048;&#27963;&#21160; <pr>y&uacute;l&egrave;hu&oacute;d&ograve;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0a">
  <kap><tld/>a</kap>
  <snc mrk="plezur.0a.KOMUNE">
    <dif>
      Plena je <tld/>o, akompanata de <tld/>o,
      <ref tip="super" cel="agrabl.0a">agrabla</ref>:
      <ekz>
        liberan vivon havas ni, <tld/>an kaj sen mastro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konsideradis kiel bonan &ccirc;ion, kio estis al li <tld/>a
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&gcirc;o tro matura ne estas <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0a">&ccirc;arma</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>plaent</trd>,
      <trd>agradable</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vergn&uuml;glich</trd>,
      <trd>vergn&uuml;gt</trd>,
      <trd>angenehm</trd>
    </trdgrp>
    <trd lng="fr">agr&eacute;able</trd>
    <trd lng="id"><baz>senang</baz>menyenangkan</trd>
    <trd lng="nl">plezierig</trd>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="plezur.0a.amuza">
    <dif>
      Celanta <tld/>on:
      <ekz>
        <tld/>a voja&gcirc;o;
      </ekz>
      <ekz>
        la <tld/>aj artoj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        irante el la legoma al la
        <ref tip="vid" cel="gxarde.plezur0o.HOR"><tld/>a &gcirc;ardeno</ref>
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>de plaer</trd>,
      <trd>per plaer</trd>
    </trdgrp>
    <trd lng="fr">d'<ind>agr&eacute;ment</ind></trd>
    <trd lng="nl">plezier-</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
  <trd lng="en">pleasurable</trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&ouml;mteli</trd>,
    <trd>gy&ouml;ny&ouml;rteli</trd>
    <trd>kellemes</trd>,
    <trd>&eacute;lvezetes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24841;&#24555;&#12394;  <pr>&#12422;&#12363;&#12356;&#12394;</pr></trd>,
    <trd>&#21916;&#12400;&#12375;&#12356;  <pr>&#12424;&#12429;&#12371;&#12400;&#12375;&#12356;</pr></trd>,
    <trd>&#27005;&#12375;&#12356;  <pr>&#12383;&#12398;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mi&lstroke;y</trd>,
    <trd>przyjemny</trd>,
    <trd>sympatyczny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agrad&aacute;vel</trd>,
    <trd>prazeroso</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>amabil</trd>,
    <trd>agreabil</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_jeu;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#29245; <pr>shu&#462;ng</pr></trd>
</drv>

<drv mrk="plezur.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="plezur.0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        dia, <tld/>iga vivmaniero
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi plantis planta&jcirc;ojn <tld/>igajn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 17:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen <tld/>iga respondo, kiel ne venis al vi en la kapon diri, ke post monato oni
        povas scii&gcirc;i ankora&ubreve; pli bone
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvis la <tld/>iga tradicio iri al la kafejo
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj tre gajis, forgesis la zorgojn, kantis kaj ridis, kaj sonis tie tia
        <tld/>iga muziko
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ja;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_u;&c_c;&c_je;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">delectable</trd>
  <trdgrp lng="cs">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>Vergn&uuml;gen</ind> bereitend</trd>,
    <trd>erquickend</trd>
  </trdgrp>
  <trd lng="en">enjoyable</trd>
  <trd lng="fr">d&eacute;licieux</trd>
  <trdgrp lng="hu">
    <trd>&eacute;lvezetes</trd>,
    <trd>gy&ouml;ny&ouml;rk&ouml;dtet&odblac;</trd>
  </trdgrp>
  <trd lng="id"><baz>senang</baz>menyenangkan</trd>
  <trdgrp lng="ja">
    <trd>&#24841;&#24555;&#12395;&#12373;&#12379;&#12427;  <pr>&#12422;&#12363;&#12356;&#12395;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#27005;&#12375;&#12367;&#12373;&#12379;&#12427;
    <pr>&#12383;&#12398;&#12375;&#12367;&#12373;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">prettig</trd>
  <trdgrp lng="pl">
    <trd>zadawalaj&aogonek;cy</trd>,
    <trd>ucieszny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agrad&aacute;vel</trd>,
    <trd>prazeroso</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>satisf&abreve;c&abreve;tor</trd>,
    <trd>caraghios</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;
  <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>
  <trdgrp lng="sk">
    <trd>radostn&yacute;</trd>,
    <trd>z&aacute;bavn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="plezur.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on al...:
      <ekz>
        mi ser&ccirc;is en mia koro, kiel <tld/>igi mian korpon per vino
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 2:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        gesinjoroj de Prelongo, dezirante <tld/>igi sian filinon, konsentis veni &ccirc;iujare
        en Parizon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        kara Petro, por min <tld/>igi venu kun mi
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu vin kiel &ccirc;iuj aliaj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">delectar</trd>
  <trd lng="cs">pot&ecaron;&scaron;it</trd>
  <trd lng="de">Vergn&uuml;gen bereiten</trd>
  <trd lng="en">gratify</trd>
  <trd lng="fr">faire <ind>plaisir</ind> &agrave;</trd>
  <trd lng="hu">gy&ouml;ny&ouml;rk&ouml;dtet</trd>
  <trdgrp lng="id">
    <trd><baz>senang</baz>menyenangkan</trd>,
    <trd><baz>puas</baz>memuaskan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21916;&#12400;&#12379;&#12427;  <pr>&#12424;&#12429;&#12371;&#12400;&#12379;&#12427;</pr></trd>,
    <trd>&#27005;&#12375;&#12414;&#12379;&#12427;  <pr>&#12383;&#12398;&#12375;&#12414;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">plezier doen</trd>
  <trd lng="oc">agradar</trd>
  <trdgrp lng="pl">
    <trd>bawi&cacute;</trd>,
    <trd>sprawia&cacute; <ind>przyjemno&sacute;&cacute;</ind></trd>,
    <trd>umila&cacute;</trd>,
    <trd>uprzyjemnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">agradar</trd>
  <trdgrp lng="ro">
    <trd>juca</trd>,
    <trd>satisface</trd>,
    <trd>face pl&abreve;cut&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
  <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>
  <trd lng="sk">pote&scaron;i&tcaron; <klr>(niekoho)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_n;&c_o;&c_s;&c_i;&c_t;&c_i;
    &c_z;&c_a;&c_d;&c_o;&c_v;&c_o;&c_l;&c_je;&c_n;&c_n;&c_ja;/&c_n;&c_a;&c_s;&c_o;&c_l;&c_o;&c_d;&c_u;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i;
    &c_p;&c_r;&c_i;&c_jeu;&c_m;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0i">
  <kap><tld/>i, <var><kap><tld/>i&gcirc;i</kap></var></kap>
  <snc>
    <dif>
      Senti <tld/>on:
      <ekz>
        &ccirc;iuj &ccirc;i bienoj estas viaj, je via komuna dispono por promenadi kaj, <tld/>i
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam falas via malamiko, ne &gcirc;oju, kaj &ccirc;e lia malfeli&ccirc;o via koro ne <tld/>i&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 24:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni &gcirc;uu sufi&ccirc;e volupton &gcirc;is la mateno, ni <tld/>i&gcirc;u per la amo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu, <tld/>u, Jozefo, <tld/>u &ndash; &ccirc;ar ega feli&ccirc;o trafis vin
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>!
      </ekz>
      <ekz>
        se <klr>[li]</klr> ne estus tiom turmentita <klr>[&#8230;]</klr>, li ver&scirc;ajne <tld/>us
        rakontante pri la akcepto en la kliniko
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
