<art mrk="$Id: forcep.xml,v 1.18 2022/03/20 11:57:21 revo Exp $">
<kap><rad>forceps</rad>/o</kap>
<drv mrk="forcep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Hcirc;irurgia <ref tip="super" cel="instru2.0o">instrumento</ref>
      uzata en malfacilaj okazoj de <ref cel="akusx.0o">aku&scirc;ado</ref>:
      <ekz>
        se la dua fazo naska da&ubreve;ras pli longe ol du horoj a&ubreve; se minacas
        oksigena manko de la infano, aku&scirc;isto alpa&scirc;as al uzo de <tld/>o, 
        eventuale oni faros akutan cezaran sekcon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=97153">Nasko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="akusx.0ilo">aku&scirc;ilo</ref>
  </snc>
  <trd lng="bg">&c_f;&c_o;&c_r;&c_c;&c_je;&c_p;&c_s;</trd>
  <trd lng="de">Geburtszange</trd>
  <trd lng="es">f&oacute;rceps</trd>
  <trd lng="fa">&a_f;&a_w;&a_r;&a_s;&f_p;&a_s;</trd>
  <trd lng="fr">forceps</trd>
  <trd lng="hu">sz&uuml;l&eacute;szeti <ind>fog&oacute;</ind></trd>
  <trdgrp lng="nl">
    <trd>verlostang</trd>,
    <trd>forceps</trd>
  </trdgrp>
  <trd lng="pt">f&oacute;rceps</trd>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_s;&c_k;&c_i;&c_je; &c_shch;&c_i;&c_p;&c_c;&c_y;</trd>
  <trd lng="sk">p&ocirc;rodn&eacute; klie&scaron;te</trd>
  <trdgrp lng="cs">
    <trd>porodnick&eacute; kle&scaron;t&ecaron;</trd>,
    <trd>porodn&iacute; kle&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x43E;&#x440;&#x446;&#x435;&#x43F;&#x441;</trd>,
    <trd>&#x430;&#x43A;&#x443;&#x448;&#x435;&#x440;&#x441;&#x44C;&#x43A;&#x456; &#x449;&#x438;&#x43F;&#x446;&#x456;</trd>
  </trdgrp>
</drv>
