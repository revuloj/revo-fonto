<drv mrk="ond.0igxi">
  <kap>
    <tld/>i,
    <var><kap><tld/>adi</kap></var>,
    <var><kap><tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ond.0igxi.KOMUNE">
    <dif>
      Levi&gcirc;i kaj mallevi&gcirc;i a&ubreve; &scirc;vele-mal&scirc;vele disvasti&gcirc;i, kiel
      <tld/>oj:
      <ekz>
        l torentoj, en kiuj <tld/>i&gcirc;as la akvo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 46:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la profundo <tld/>i&gcirc;adis palmaj arbaroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj longaj belegaj haroj senorde <tld/>i&gcirc;adis &ccirc;irka&ubreve; la bela kapo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        abundaj bukloj <tld/>i&gcirc;adis &ccirc;irka&ubreve; la rondaj &scirc;ultroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kampo <tld/>i&gcirc;adis la greno simile al movi&gcirc;anta maro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kanoj en la mar&ccirc;o <tld/>i&gcirc;adis tien kaj reen
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivero ankora&ubreve; furioze <tld/>is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aero komencis <tld/>i, kvaza&ubreve; skuata de traflugantaj nevideblaj birdoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
