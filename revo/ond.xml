<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ond</rad>/o
</kap>

<drv mrk="ond.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ond.0o.akva">
    <dif>
      Akvomaso, kiu levi&gcirc;as kaj refalas &ccirc;e supra&jcirc;o de maro, lago a&ubreve;
      rivero
      blovata de vento; akvo&scirc;velo:
      <ekz>
        la <tld/>oj alfrapas, alpla&ubreve;das la rokojn;
      </ekz>
      <ekz>
        la barketo estis glutita de terura <tld/>ego;
      </ekz>
      <ekz>
        la &scirc;a&ubreve;mantaj <tld/>oj.
      </ekz>
    </dif>
  <trd lng="es">ola</trd>
  </snc>
  <snc mrk="ond.0o.forma">
    <dif>
      Tio, kio levi&gcirc;as kaj mallevi&gcirc;as kiel <tld/>oj:
      <ekz>
        la <tld/>oj de flagranta flamo;
      </ekz>
      <ekz>
        la <tld/>oj de virina hararo;
      </ekz>
      <ekz>
        lumaj <tld/>oj;
      </ekz>
      <ekz>
        tritikaj spikoj rulis <tld/>ojn da oro
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  <trd lng="es">onda</trd>
  </snc>
  <snc mrk="ond.0o.mova">
    <dif>
      Tio, kio progresas per interrompitaj masoj kaj iom, post iom
      disvasti&gcirc;as, kiel <tld/>oj:
      <ekz>
        <tld/>oj da promenantoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>o da varma vento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o da revoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la fremdaj elementoj, dronigis en siaj <tld/>oj la anta&ubreve;ajn
        lo&gcirc;antojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        atak<tld/>o;
      </ekz>
      <ekz>
        <tld/>o da simpatio ekruli&gcirc;is tra la lando;
      </ekz>
      <ekz>
        tiam aperis granda strik<tld/>o;
      </ekz>
      <ekz>
        protest<tld/>oj levi&gcirc;is;
      </ekz>
      <ekz>
        li fluigadis <tld/>ojn da elokventeco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.0o">fluo</ref>
    </refgrp>
  <trd lng="es">oleada</trd>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_a;&c_l;&c_ja;</trd>
  <trd lng="de">Welle</trd>
  <trd lng="en">wave</trd>
  <trdgrp lng="fr">
    <trd>onde</trd>,
    <trd>vague<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">hull&aacute;m</trd>
  <trd lng="nl">golf</trd>
  <trd lng="pt">onda</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_n;&c_a;</trd>
  <trd lng="sv">v&aring;g</trd>
  <trd lng="uk">&c_h;&c_v;&c_i;&c_l;&c_ja;</trd>
</drv>

<drv mrk="ond.0a">
  <kap><tld/>a</kap>
  <snc mrk="ond.0a.KOMUNE">
    <dif>
      Karakterizata per <tld/>oj:
      <ekz>
        <tld/>a movo, maro, hararo, flamo;
      </ekz>
      <ekz>
        la <tld/>aj konturoj de drapira&jcirc;o;
      </ekz>
      <ekz>
        <tld/>e rampi, movi&gcirc;i.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_v;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>wellenf&ouml;rmig</trd>,
     <trd>wellig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fluctuant</trd>,
    <trd>ondoyant</trd>,
    <trd>ondul&eacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ondular</trd>,
    <trd>ondulado, -a</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hull&aacute;m-</trd>,
    <trd>hull&aacute;mos</trd>
  </trdgrp>
  <trd lng="nl">golvend</trd>
  <trd lng="pt">ondulado</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">v&aring;gig</trd>
</drv>

<drv mrk="ond.0igxi">
  <kap><tld/>i<fnt>Z</fnt>, <tld/>adi<fnt>Z</fnt>,
  <tld/>i&gcirc;i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ond.0igxi.KOMUNE">
    <dif>
      Levi&gcirc;i kaj mallevi&gcirc;i a&ubreve; iom post iom disvasti&gcirc;i, kiel
      <tld/>oj:
      <ekz>
        <tld/>as flamo
        (<ref tip="vid" cel="flagr.0i">flagri</ref>);
      </ekz>
      <ekz>
        la <tld/>anta amaso<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>is seninterrompe &ccirc;eno da veturiloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la popolo en senordaj grupoj <tld/>adis tien kaj
        reen<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el centoj da gor&gcirc;oj <tld/>as la konataj sonoj de l'
        himno;
      </ekz>
      <ekz>
        en mia brusto <tld/>as malkviete kura&gcirc;a
        entrepreno<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tra la tuta lando ek<tld/>as granda intereso por tiu
        afero;
      </ekz>
      <ekz>
        melodioj fluas trans la korton kaj &ccirc;irka&ubreve;<tld/>as
        nin;
      </ekz>
      <ekz>
        la plorego de l' <tld/>i&gcirc;antaj lagoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la spikoj <tld/>i&gcirc;as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en la profundo <tld/>i&gcirc;adis palmaj arbaroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;iaj longaj haroj <tld/>i&gcirc;adis &ccirc;irka&ubreve;
        la kapo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;antaj &scirc;tofoj;
      </ekz>
      <ekz>
        kontra&ubreve;aj sentoj <tld/>i&gcirc;is en li
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="lev.0igxi">levi&gcirc;i</ref>,
      <ref cel="plauxd.0i">pla&ubreve;di</ref>,
      <ref cel="rul.0i">ruli</ref>,
      <ref cel="sxauxm.0i">&scirc;a&ubreve;mi</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_a;&c_p;&c_r;., &c_p;&c_r;&c_a;
  &c_m;&c_o;&c_r;&c_a;)</klr></trd>
  <trd lng="de"><klr>sich </klr>heben und <ind>senken</ind></trd>
  <trd lng="fr">fluctuer</trd>
  <trd lng="hu">hull&aacute;mzik</trd>
  <trdgrp lng="nl">
    <trd>fluctueren</trd>,
    <trd>golven</trd>
  </trdgrp>
  <trd lng="pt">ondular</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_o; &c_m;&c_o;&c_r;&c_je; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="es">
    <trd>ondular</trd>,
    <trd>fluctuar</trd>,
    <trd>dispersarse</trd>
  </trdgrp>
</drv>

<drv mrk="ond.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="ond.0igxo.FIG">
    <dif>
      Sinsekva levi&gcirc;o kaj mallevi&gcirc;o de <tld/>o:
      <ekz>
        la <tld/>i&gcirc;oj de la akvo, <uzo tip="stl">FIG</uzo>
        de la prezoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Heben und <ind>Senken</ind></trd>
  <trdgrp lng="fr">
    <trd>ondoiement</trd>,
    <trd>ondulation</trd>
  </trdgrp>
  <trd lng="hu">hull&aacute;mz&aacute;s</trd>
  <trd lng="pt">ondula&ccedil;&atilde;o</trd>
  <trdgrp lng="es">
    <trd>ondulaci&oacute;n</trd>,
    <trd>fluctuaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>op- en neergaan</ind></trd>,
    <trd>fluctuatie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_n;&c_a;&c_p;&c_r;. &c_n;&c_a;
    &c_m;&c_o;&c_r;&c_je;)</klr></trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="ond.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ond.0umi.KOMUNE">
    <dif>
      Doni al io <tld/>oformon:
      <ekz>
        <tld/>umi la harojn;
      </ekz>
      <ekz>
        <tld/>umita lado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_h;&c_v;&c_a;&c_l;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_u;&c_ju;</ind>
  &c_f;&c_o;&c_r;&c_m;&c_u;</trd>
  <trdgrp lng="de">
     <trd><klr>in </klr>Wellen <ind>legen</ind></trd>,
     <trd>ondulieren</trd>
  </trdgrp>
  <trd lng="fr">faire <ind>onduler</ind></trd>
  <trdgrp lng="hu">
    <trd>hull&aacute;mos&iacute;t</trd>,
    <trd>ondol&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>golven</ind></trd>
  <trd lng="pt">ondular</trd>
  <trd lng="es">ondular</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_d;&c_a;&c_t;&c_mol; <ind>&c_v;&c_o;&c_l;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_u;&c_ju;</ind>
  &c_f;&c_o;&c_r;&c_m;&c_u;</trd>
</drv>

<drv mrk="ond.0obanejo">
  <kap><tld/>obanejo</kap>
  <fnt><bib>KrED</bib></fnt>
  <snc mrk="ond.0obanejo.KOMUNE">
    <dif>
      Banejo, en kiu aparato produktas ondojn por la plezuro de la
      banantoj.
    </dif>
  </snc>
  <trd lng="de">Wellenbad</trd>
  <trd lng="fr">bain &agrave; remous</trd>
  <trd lng="hu">hull&aacute;mf&uuml;rd&odblac;</trd>
  <trd lng="nl">golfbad</trd>
</drv>

<drv mrk="ond.0obrila">
  <kap><tld/>obrila</kap>
  <snc mrk="ond.0obrila.KOMUNE">
    <dif>
      Prezentanta kvaza&ubreve; brilajn <tld/>ojn:
      <ekz>
        <tld/>obrila &scirc;tofo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="muar.0a">muara</ref>
    </refgrp>
  </snc>
  <trd lng="de">moiriert</trd>
  <trd lng="fr">chatoyant</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  <trd lng="hu">sz&iacute;nj&aacute;tsz&oacute;</trd>
</drv>

<drv mrk="ond.0olinia">
  <kap><tld/>olinia</kap>
  <snc mrk="ond.0olinia.KOMUNE">
    <dif>
      Havanta <tld/>an konturon:
      <ekz>
        <tld/>olinia lando<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>wellenf&ouml;rmig</trd>,
     <trd>wellig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ondul&eacute;</trd>,
    <trd>vallonn&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hull&aacute;mvonal&uacute;</trd>,
    <trd>k&iacute;gy&oacute;z&oacute;</trd>
  </trdgrp>
  <trd lng="nl">gemoireerd</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ond.dis0o">
  <kap>dis<tld/>o</kap>
  <snc mrk="ond.dis0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Sulko, kiun &scirc;ipo lasas malanta&ubreve; si.
    </dif>
  </snc>
  <trd lng="de">Kielwasser</trd>
  <trd lng="fr">sillage</trd>
  <trdgrp lng="hu">
    <trd>haj&oacute;sodor</trd>,
    <trd>nyomdokv&iacute;z</trd>
  </trdgrp>
  <trd lng="nl">kielzog</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_mol;&c_v;&c_a;&c_t;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_r;&c_u;&c_ja;</trd>
</drv>
</art>
