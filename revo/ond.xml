<drv mrk="ond.0igxi">
  <kap>
    <tld/>i, 
    <var><kap><tld/>adi</kap></var>,
    <var><kap><tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ond.0igxi.KOMUNE">
    <dif>
      Levi&gcirc;i kaj mallevi&gcirc;i a&ubreve; &scirc;vele-mal&scirc;vele disvasti&gcirc;i, kiel
      <tld/>oj:
      <ekz>
        l torentoj, en kiuj <tld/>i&gcirc;as la akvo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 46:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la profundo <tld/>i&gcirc;adis palmaj arbaroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj longaj belegaj haroj senorde <tld/>i&gcirc;adis &ccirc;irka&ubreve; la bela kapo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        abundaj bukloj <tld/>i&gcirc;adis &ccirc;irka&ubreve; la rondaj &scirc;ultroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kampo <tld/>i&gcirc;adis la greno simile al movi&gcirc;anta maro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kanoj en la mar&ccirc;o <tld/>i&gcirc;adis tien kaj reen
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivero ankora&ubreve; furioze <tld/>is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aero komencis <tld/>i, kvaza&ubreve; skuata de traflugantaj nevideblaj birdoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la strato estis plena <klr>[&#8230;]</klr> en la mezo <tld/>is seninterrompa &ccirc;eno de
        komerca&jcirc;oj, portataj de homoj, azenoj a&ubreve; bovoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        sentoj <tld/>is kaj luktis en li
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[liaj]</klr> viza&gcirc;aj linioj <klr>[&#8230;]</klr> <tld/>i&gcirc;adis, kiel okazas
        ordinare &ccirc;e homoj kun impresebla naturo, kiam ili subite estas ekscititaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en mia brusto <tld/>as malkviete kura&gcirc;a entrepreno
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia brusto <tld/>adis, kvaza&ubreve; &gcirc;i volas krevi
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balanc1.0igxi">balanci&gcirc;i</ref>,
      <ref cel="bol.0i">boli</ref>,
      <ref cel="fluktu.0i.KOMUNE">fluktui<sncref/></ref>,
      <ref cel="lev.0igxi">levi&gcirc;i</ref>,
      <ref cel="plauxd.0i">pla&ubreve;di</ref>,
      <ref cel="rul.0i">ruli</ref>,
      <ref cel="sxauxm.0i">&scirc;a&ubreve;mi</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>,
      <ref cel="vibr.0i">vibri</ref>
    </refgrp>
  </snc>
