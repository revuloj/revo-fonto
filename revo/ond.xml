    </dif>
  </snc>
  <trd lng="be">&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_k;&c_a;&c_r;&c_o;&c_t;&c_k;&c_a;&c_ja; <ind>&c_h;&c_v;&c_a;&c_l;&c_ja;</ind></trd>
