  </trdgrp>
  <trd lng="sv">v&aring;gig</trd>
</drv>

<drv mrk="ond.0igxi">
  <kap><tld/>i<fnt>Z</fnt>, <var><kap><tld/>adi<fnt>Z</fnt></kap></var>,
  <var><kap><tld/>i&gcirc;i<fnt>Z</fnt></kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ond.0igxi.KOMUNE">
    <dif>
      Levi&gcirc;i kaj mallevi&gcirc;i a&ubreve; iom post iom disvasti&gcirc;i, kiel
      <tld/>oj:
      <ekz>
        <tld/>as flamo
        (<ref tip="vid" cel="flagr.0i">flagri</ref>);
      </ekz>
      <ekz>
        la <tld/>anta amaso<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>is seninterrompe &ccirc;eno da veturiloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la popolo en senordaj grupoj <tld/>adis tien kaj
        reen<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el centoj da gor&gcirc;oj <tld/>as la konataj sonoj de l'
        himno;
      </ekz>
      <ekz>
        en mia brusto <tld/>as malkviete kura&gcirc;a
        entrepreno<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tra la tuta lando ek<tld/>as granda intereso por tiu
        afero;
      </ekz>
      <ekz>
        melodioj fluas trans la korton kaj &ccirc;irka&ubreve;<tld/>as
        nin;
      </ekz>
      <ekz>
        la plorego de l' <tld/>i&gcirc;antaj lagoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la spikoj <tld/>i&gcirc;as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en la profundo <tld/>i&gcirc;adis palmaj arbaroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;iaj longaj haroj <tld/>i&gcirc;adis &ccirc;irka&ubreve;
        la kapo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;antaj &scirc;tofoj;
      </ekz>
      <ekz>
        kontra&ubreve;aj sentoj <tld/>i&gcirc;is en li
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="lev.0igxi">levi&gcirc;i</ref>,
      <ref cel="plauxd.0i">pla&ubreve;di</ref>,
      <ref cel="rul.0i">ruli</ref>,
      <ref cel="sxauxm.0i">&scirc;a&ubreve;mi</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_a;&c_p;&c_r;., &c_p;&c_r;&c_a;
  &c_m;&c_o;&c_r;&c_a;)</klr></trd>
  <trd lng="de"><klr>sich </klr>heben und <ind>senken</ind></trd>
  <trdgrp lng="en">
    <trd>to ripple</trd>,
    <trd>wave</trd>, <trd>undulate</trd>, <trd>swell</trd>
  </trdgrp>
  <trd lng="fr">fluctuer</trd>
  <trd lng="hu">hull&aacute;mzik</trd>
  <trdgrp lng="nl">
    <trd>fluctueren</trd>,
    <trd>golven</trd>
  </trdgrp>
  <trd lng="pt">ondular</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_o; &c_m;&c_o;&c_r;&c_je; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="es">
    <trd>ondular</trd>,
    <trd>fluctuar</trd>,
    <trd>dispersarse</trd>
  </trdgrp>
</drv>

