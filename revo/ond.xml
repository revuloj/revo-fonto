<drv mrk="ond.0a">
  <kap><tld/>a</kap>
  <snc mrk="ond.0a.KOMUNE">
    <dif>
      Karakterizata per <tld/>oj:
      <ekz>
        <tld/>a movo, maro, hararo, flamo;
      </ekz>
      <ekz>
        la <tld/>aj konturoj de drapira&jcirc;o;
      </ekz>
      <ekz>
        <tld/>e rampi, movi&gcirc;i.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_v;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vlnit&yacute;</trd>,
    <trd>vln&iacute;c&iacute; se</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>wellenf&ouml;rmig</trd>,
     <trd>wellig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wavy</trd>,
    <trd>rippling</trd>, <trd>waving</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ondular</trd>,
    <trd>ondulado, -a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fluctuant</trd>,
    <trd>ondoyant</trd>,
    <trd>ondul&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hull&aacute;m-</trd>,
    <trd>hull&aacute;mos</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ber<ind>gelombang</ind></trd>,
    <trd>ber<ind>ombak</ind></trd>
  </trdgrp>
  <trd lng="nl">golvend</trd>
  <trd lng="pt">ondulado</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_o;&c_j;</trd>,
