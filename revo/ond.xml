          <aut>GrizaLeono</aut>
          <vrk><url ref="https://tatoeba.org/epo/sentences/show/988569">
          Tatoeba, frazo 988569a</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_n;&c_a;&c_m;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>ona molt alta</trd>,
