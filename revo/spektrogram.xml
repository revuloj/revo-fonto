<kap>
  <rad>spektrogram</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="spektrogram.0o">
  <kap><tld/>o</kap>
  <snc mrk="spektrogram.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Grafiko prezentanta la
      <ref tip="vid" cel="spektr.0o">spektron</ref>
      de iu fizika apera&jcirc;o:
      <ekz>
        la unua <tld/>o havis la hidrogenan linion, la dua &mdash; la
        helian
        <fnt>
         <aut>V. &Jcirc;uravleva, trad. A. I. Ver&scirc;inin</aut>,
         <vrk><url ref="http://www.autodidactproject.org/other/rapsodio.html">
         Starluma Rapsodio</url></vrk>,
         <lok>Norda Prismo, 1961:3, p. 86a-90a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_t;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="en">spectrogram</trd>
  <trd lng="es">espectrograma</trd>
  <trd lng="fr">spectrogramme</trd>
  <trd lng="it">spettrogramma</trd>
  <trd lng="ja">&#x5206;&#x5149;&#x5199;&#x771F;  <pr>&#x3076;&#x3093;&#x3072;&#x304B;&#x308A;&#x3057;&#x3083;&#x3057;&#x3093;</pr></trd>
  <trd lng="pl">spektrogram</trd>
  <trd lng="uk">&c_s;&#x43F;&c_je;&#x43A;&c_t;&c_r;&#x43E;&c_g;&c_r;&c_a;&#x43C;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x8B5C;&#x5716; <pr>gu&#x101;ngp&#x1D4;t&#xFA;</pr></trd>,
    <trd>&#x5149;&#x8C31;&#x56FE; <pr>gu&#x101;ngp&#x1D4;t&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>

