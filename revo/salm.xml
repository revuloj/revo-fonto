<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>salm</rad>/o
</kap>
<drv mrk="salm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="salm.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Salmo</trd>), mara fi&scirc;o kun rozkolora bongusta viando,
      kiu kontra&ubreve;fluen migras en la riverojn por demeti frajon.
    </dif>
  </snc>
  <trd lng="de">Lachs</trd>
  <trd lng="fr">saumon</trd>
  <trd lng="nl">zalm</trd>
</drv>
<drv mrk="salm.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="salm.0oformaj"><tld/>oformaj
      fi&scirc;oj</ref> (<trd lng="la">Salmonidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Lachse</trd>
</drv>
<drv mrk="salm.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Salmoniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Lachsfische</trd>
</drv>
</art>
