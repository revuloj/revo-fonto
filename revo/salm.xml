<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salm.xml,v 1.34 2023/09/03 12:09:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salm</rad>/o
</kap>
<drv mrk="salm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Mara fi&scirc;o</ref> 
      kun rozkolora bongusta viando,
      kiu kontra&ubreve;fluen migras en la riverojn por fraji;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="salmed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Salmo</trd>):
      <ekz>
        estis efektive stranga ideo doni al ni pun&ccirc;on kaj varman <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rakontis pri la <tld/>o, kiu saltas e&ccirc; kontra&ubreve; la falantan akvon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la man&gcirc;o&ccirc;ambro hodia&ubreve; matene du iaj mallongaj homoj man&gcirc;is <tld/>on kaj
        ankora&ubreve; multe da alia
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre dekoltita vesto, el <tld/>okolora atlaso kaj brodita silko
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        grandaj <tld/>okoloraj rozoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/9c/Salmo_salar-Atlantic_Salmon-Atlanterhavsparken_Norway_(cropped).JPG/640px-Salmo_salar-Atlantic_Salmon-Atlanterhavsparken_Norway_(cropped).JPG" prm="CC BY-SA 2.5">Atlantika <tld/>o (Salmo salar), Atlanterhavsparken, &Aring;lesund, Norvegujo
        <fnt>
          <aut>Hans-Petter Fjeld, propra verko</aut>
          <lok><url ref="&WCW;/File:Salmo_salar-Atlantic_Salmon-Atlanterhavsparken_Norway_(cropped).JPG">&WCW;/File:Salmo_salar-Atlantic_Salmon-Atlanterhavsparken_Norway_(cropped).JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_o;&c_s;&c_mol;</trd>; 
    <trd>&c_s;&c_jo;&c_m;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_mol;&c_o;&c_m;&c_g;&c_a;</trd>
  <trd lng="ca">salm&oacute;</trd>
  <trd lng="cs">losos</trd>
  <trd lng="de">Lachs</trd>
  <trd lng="en">salmon</trd>
  <trd lng="es">salm&oacute;n</trd>
  <trd lng="fr">saumon</trd>
  <trd lng="hu">lazac</trd>
  <trd lng="id">salmon</trd>
  <trd lng="it">salmone</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30A4;&#x30BB;&#x30A4;&#x30E8;&#x30A6;&#x30B5;&#x30B1;</trd>,
    <trd>&#x30B5;&#x30B1;</trd>
  </trdgrp>
  <trd lng="nl">zalm</trd>
  <trd lng="pl">&lstroke;oso&sacute;<klr> szlachetny</klr></trd>
  <trdgrp lng="pt">
   <trd>salm&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;
    <ind>&c_l;&c_o;&c_s;&c_o;&c_s;&c_mol;</ind> <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>,
    <trd>&c_l;&c_o;&c_s;&c_o;&c_s;&c_mol; <klr>(unu el
    specioj)</klr></trd>,
    <trd>&c_s;&c_jo;&c_m;&c_g;&c_a; <klr>(unu el specioj, =
    &c_l;&c_o;&c_s;&c_o;&c_s;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="sk">losos</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&c_s;&#x43E;&c_s;&#x44C;</trd>,
    <trd>&c_s;&#x44C;&#x43E;&#x43C;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="salm.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de
      <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Salmoniformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_o;&c_s;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">salmoniformes</trd>
  <trd lng="de">Lachsfische</trd>
  <trd lng="fr">salmoniformes</trd>
  <trd lng="hu">lazacalak&uacute;ak</trd>
  <trd lng="it">salmoniformi</trd>
  <trd lng="pl">&lstroke;ososiokszta&lstroke;tne</trd>
  <trd lng="ru">&c_l;&c_o;&c_s;&c_o;&c_s;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
