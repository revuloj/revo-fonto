<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>narcis</rad>/o
</kap>
<drv mrk="narcis.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super"
       cel="amaril1.0oj">amarilidacoj</ref>
       (<trd lng="la">Narcissus</trd>), kies floroj havas pokalforman
       kromkoronon kaj intensan bonodoron.
     </dif>
   </snc>
   <trd lng="be">&c_n;&c_a;&c_r;&c_c;&c_y;&c_s;</trd>
   <trd lng="ca">narc&iacute;s</trd>
   <trd lng="de">Narzisse</trd>
   <trd 
lng="el">&ny;&alfa_ton;&rho;&kappa;&jota;&sigma;&sigma;&omikron;&sigma_fina;</trd>
   <trdgrp lng="en">
     <trd>narcissus</trd>,
     <trd>dafodil</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>narcis</trd>,
     <trd>paasbloem</trd>
   </trdgrp>
   <trd lng="es">narciso</trd>
   <trd lng="fr">narcisse</trd>
   <trd lng="hu">n&aacute;rcisz</trd>
   <trd lng="pt">narciso</trd>
   <trd lng="ru">&c_n;&c_a;&c_r;&c_c;&c_i;&c_s;&c_s;</trd>
   <trd lng="sv">narciss</trd>
</drv>
</art>
