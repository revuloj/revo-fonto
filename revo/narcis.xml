<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: narcis.xml,v 1.33 2024/01/29 15:21:35 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>narcis</rad>/o 
</kap>
<drv mrk="narcis.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="narcis.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
       Vegeta&jcirc;o <ref tip="lst" cel="plant.floro0o" lst="voko:asparagoparencoj">asparagoparenca</ref> el la
       mediteranea zono, kies floroj havas pokalforman
       kromkoronon kaj intensan bonodoron;
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio
       <ref tip="malprt" cel="amaril1.0oj">amarilidacoj</ref>
       (<trd lng="la">Narcissus</trd>), i.a. kun la specio
      <ref tip="prt" cel="jonkvi.0o">jonkvilo</ref>:
      <ekz>
        la tero balda&ubreve; kovri&gcirc;is de smeralda verda&jcirc;o, kiun superstaris <tld/>oj kaj violoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        miloj da diversaj <tld/>oj malfermi&gcirc;as meze de majo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php">Tammisaari, urbeto &ccirc;e Balta Maro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
         legendo diras, ke <tld/>o estas transformita el akva feino
         <fnt>
         <aut>-</aut>,
         <vrk><url ref="http://esperanto.china.org.cn/EL/EL/ElPopolaCxinio/9905/bd99-5-5.html">
         El &Ccirc;inaj Gazetoj</url></vrk>,
         <lok>El popola &Ccirc;inio, 1999-05</lok>
         </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/79/Narcissus_poeticus_Spechtensee_01.JPG/320px-Narcissus_poeticus_Spechtensee_01.JPG" prm="CC BY-SA 4.0">floro de blanka <tld/>o (Narcissus poeticus) &ccirc;e lago Spechtensee, Stirio, A&ubreve;strujo
        <fnt>
          <aut>Uoaei1, propra verko</aut>
          <lok><url ref="&WCW;/File:Narcissus_poeticus_Spechtensee_01.JPG">&WCW;/File:Narcissus_poeticus_Spechtensee_01.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="narcis1.0o">Narciso (mita heroo)</ref>
  <trd lng="be">&c_n;&c_a;&c_r;&c_c;&c_y;&c_s;</trd>
  <trd lng="br">rozenn-gamm</trd>
  <trd lng="ca">narc&iacute;s</trd>
  <trd lng="cs">narcis</trd>
  <trd lng="de">Narzisse</trd>
  <trd lng="el">&ny;&alfa_ton;&rho;&kappa;&jota;&sigma;&sigma;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
     <trd>narcissus</trd>,
     <trd>daffodil</trd>
   </trdgrp>
  <trd lng="es">narciso</trd>
  <trd lng="fr">narcisse</trd>
  <trd lng="hu">n&aacute;rcisz</trd>
  <trd lng="id">narsis</trd>
  <trd lng="ja">&#x30B9;&#x30A4;&#x30BB;&#x30F3;</trd>
  <trdgrp lng="nl">
     <trd>narcis</trd>,
     <trd>paasbloem</trd>
   </trdgrp>
  <trd lng="pl">narcyz bia&lstroke;y</trd>
  <trd lng="pt">narciso</trd>
  <trd lng="ro">narcis&abreve;</trd>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_c;&c_i;&c_s;&c_s;</trd>
  <trd lng="sk">narcis</trd>
  <trd lng="sv">narciss</trd>
  <trd lng="uk">&#x43D;&#x430;&#x440;&#x446;&#x438;&#x441;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x4ED9; <pr>shu&#x1D0;xi&#x101;n</pr></trd>,
    <trd>&#x6C34;&#x4ED9;&#x82B1; <pr>shu&#x1D0;xi&#x101;nhu&#x101;</pr></trd>,
    <trd>&#x5587;&#x53ED;&#x6C34;&#x4ED9; <pr>l&#x101;b&#x101;shu&#x1D0;xi&#x101;n</pr></trd>
  </trdgrp>
</drv>

</art>
