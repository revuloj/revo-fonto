<kap>
<ofc>2</ofc>
<rad>bisturi</rad>/o
</kap>
<drv mrk="bistur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="skalpe.0o">Skalpelo</ref>:
      <ekz>
        robota protezo, kreita en Italio, kies aplikado ne postulas la uzadon de kirurga
        <tld/>o kaj kies kosto estas relative malalta
        <fnt>
        <bib>Monato</bib>
        <aut>Cristina Casella</aut>
          <vrk><url ref="&Monato;/2018/012268.php">Noveca artefarita mano prezentita en
          Romo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_s;&c_t;&c_u;&c_r;&c_y; <klr>(&c_m;&c_a;&c_l;&c_y; &c_n;&c_o;&c_zh; &c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;&c_a;)</klr></trd>
    <trd lng="lat">scalprum</trd>
    <trd lng="bg">&c_l;&c_a;&c_n;&c_c;&c_je;&c_t;</trd>
    <trd lng="ca">bistur&iacute;</trd>
    <trdgrp lng="cs">
      <trd>b&rcaron;itvice</trd>,
      <trd>chirurgick&yacute; no&zcaron;&iacute;k</trd>
    </trdgrp>
    <trd lng="de">Skalpell</trd>
    <trdgrp lng="en"><trd>bistoury</trd><trd>lancet</trd></trdgrp>
    <trd lng="es">bistur&iacute;</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&f_y;&a_gh; &a_j;&a_r;&a_A;&a_H;&f_y;</trd>
      <trd>&a_n;&f_y;&a_sh;&a_t;&a_r;</trd>
    </trdgrp>
    <trd lng="fr">bistouri</trd>
    <trd lng="hu">bistouri</trd>
    <trd lng="it">bisturi</trd>
    <trd lng="la">scalprum</trd>
    <trdgrp lng="nl">
      <trd>scalpel</trd>,
      <trd>lancet</trd>,
      <trd>bistouri</trd>,
      <trd>operatiemes</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bistur</trd>,
      <trd>lancet</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_i;&c_s;&c_t;&c_u;&c_r;&c_i;</trd>,
      <trd>&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
      &c_n;&c_o;&c_zh;</trd>
    </trdgrp>
    <trd lng="sk">chirurgick&yacute; no&zcaron;&iacute;k</trd>
  </snc>
  <trd lng="he">&#x5E1;&#x5DB;&#x5D9;&#x5DF;
  &#x5DB;&#x5D9;&#x5E8;&#x5D5;&#x5E8;&#x5D2;&#x5D9;&#x5EA;</trd>
  <trd lng="ja">&#x67F3;&#x8449;&#x5200;  <pr>&#x308A;&#x3085;&#x3046;&#x3088;&#x3046;&#x3068;&#x3046;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_s;&c_t;&c_u;&c_r;&c_ib;</trd>,
    <trd>&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;&c_ib;&c_ch;&#x43D;&c_i;&c_j; &#x43D;&c_ib;&c_zh;</trd>
  </trdgrp>
</drv>
</art>
