<art mrk="$Id$">
<kap>
  <rad>euxsk</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="euxsk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="vask.0o">vasko</ref>
    </dif>
  </snc>
  <trd lng="en">Basque</trd>
  <trd lng="fr">Basque<klr> (subst.)</klr></trd>
