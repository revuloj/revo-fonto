<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>slovak</rad>/o
</kap>

<drv mrk="slovak.0ujo">
  <kap>
    <tld lit="S"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="slovak.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra E&ubreve;ropo kun &ccirc;efurbo Bratislavo.
      Teritorio de Respubliko Slovakujo.
      ISO-3166 kodo: SK.
      Iam parto de <ref tip="malprt"
      cel="cxehx.CX0oslovakujo">&Ccirc;e&hcirc;oslovakujo</ref>.
      &Gcirc;i limas kun
      <ref tip="vid" cel="auxstr.0ujo.GEOG">A&ubreve;strujo</ref>
      <ref tip="vid" cel="cxehx.CX0ujo">&Ccirc;ehujo</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="pol.P0lando">Pollando</ref> kaj
      <ref tip="vid" cel="ukrain.U0ujo">Ukrainujo</ref>.
    </dif>
    <ref tip="sin" cel="slovaki.0o.GEOG">Slovakio</ref>
    <trd lng="bg">&c_S;&c_l;&c_o;&c_v;&c_a;&c_k;&c_i;&c_ja;</trd>
    <trd lng="de">Slowakei</trd>
    <trd lng="en">Slovakia</trd>
    <trdgrp lng="pl">
      <trd>S&lstroke;owacja</trd>,
      <trd>Republika <ind>S&lstroke;owacji</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

