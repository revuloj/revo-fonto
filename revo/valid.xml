<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>valid</rad>/a
</kap>
<drv mrk="valid.0a">
  <kap><tld/>a</kap>
  <snc mrk="valid.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Prezentanta la la&ubreve;le&gcirc;ajn kondi&ccirc;ojn postulatajn por regule efiki:
      <ekz>
        <tld/>a bileto, testamento,
      </ekz>
      <ekz>
        la kontrakto definitive <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_t;&c_o; &c_m;&c_a;&c_je;
    <ind>&c_s;&c_ib;&c_l;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="ca">v&agrave;lid</trd>
  <trdgrp lng="cs">
    <trd>platný</trd>,
    <trd>právoplatný</trd>,
    <trd>validní</trd>
  </trdgrp>
  <trd lng="de">g&uuml;ltig</trd>
  <trd lng="en">valid</trd>
  <trd lng="es">v&aacute;lido</trd>
  <trdgrp lng="fr">
    <trd>en cours</trd>,
    <trd>valable</trd>,
    <trd>valide</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tav;&qof;&fpe;</trd>,
    <trd>&bet;&resh;-&tav;&vav;&qof;&fpe;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rv&eacute;nyes</trd>,
    <trd>hat&aacute;lyos</trd>
  </trdgrp>
  <trd lng="nl">geldig</trd>
  <trd lng="pt">v&aacute;lido</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_s;&c_i;&c_l;&c_u;)</klr></trd>,
    <trd>&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_s;&c_i;&c_l;&c_u; <klr tip="amb">(&c_ju;&c_r;.)</klr></trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>platný</trd>,
    <trd>právoplatný</trd>
  </trdgrp>
  <trd lng="sv">giltlig</trd>
</drv>

<drv mrk="valid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="valid.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Valori la&ubreve; certaj kondi&ccirc;oj pri le&gcirc;eco, templimo ktp.:
      <ekz>
        la vizo de la pasporto <tld/>os por 60-taga restado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_m;&c_o;&c_c;</ind></trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_s;&c_ib;&c_l;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="ca">&eacute;sser v&agrave;lid</trd>
  <trdgrp lng="cs">
    <trd>být platný</trd>,
    <trd>mít platnost</trd>,
    <trd>platit</trd>
  </trdgrp>
  <trd lng="de">gelten</trd>
  <trdgrp lng="es">
    <trd>tener vigor</trd>,
    <trd>tener vigencia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avoir <ind>cours</ind></trd>,
    <trd>&ecirc;tre <ind>valable</ind></trd>,
    <trd>&ecirc;tre <ind>valide</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&yod;&vav;&tav; <ind>&tav;&qof;&fpe;</ind></trd>
  <trdgrp lng="hu">
    <trd>&eacute;rv&eacute;nyben van</trd>,
    <trd>hat&aacute;lyos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geldig zijn</trd>,
    <trd>gelden</trd>
  </trdgrp>
  <trd lng="pt">ser v&aacute;lido</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; &c_s;&c_i;&c_l;&c_u; <klr tip="amb">(&c_ju;&c_r;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>mať platnosť</trd>,
    <trd>platiť</trd>
  </trdgrp>
  <trd lng="sv">g&auml;lla</trd>
</drv>

<drv mrk="valid.eks0igxi">
  <kap>eks<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="valid.eks0igxi.JUR">
    <dif>
      Ne plu <tld/>i pro kaduki&gcirc;o, pro paso de iu templimo:
      <ekz>
        mi sugestas ke oni akiru malnovajn filmojn kies kopirajtoj
        eks<tld/>i&gcirc;is
        <fnt>
        <aut>KriZe</aut>,
        <vrk><url ref="http://gxangalo.com/">Esperanto-Televido -
        Forumo</url></vrk>,
        <lok>2003-12-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol; <ind>&c_m;&c_o;&c_c;</ind></trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_ib;&c_l;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>caducar</trd>,
    <trd>perdre la <ind>validesa</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devenir <ind>caduc</ind></trd>,
    <trd>perdre sa <ind>validit&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rv&eacute;nytelenn&eacute; v&aacute;lik</trd>,
    <trd>&eacute;rv&eacute;ny&eacute;t veszti</trd>,
    <trd>hat&aacute;ly&aacute;t veszti</trd>,
    <trd>lej&aacute;r<klr tip="ind">(hat&aacute;rid&odblac;)</klr></trd>
  </trdgrp>  
</drv>
</art>
