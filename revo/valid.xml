<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: valid.xml,v 1.24 2017/02/11 14:10:13 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>valid</rad>/a
</kap>
<drv mrk="valid.0a">
  <kap><tld/>a</kap>
  <snc mrk="valid.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Prezentanta la laŭleĝajn kondiĉojn postulatajn
      por regule efiki:
      <ekz>
        <tld/>a bileto, testamento,
      </ekz>
      <ekz>
        la kontrakto definitive <tld/>iĝis.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_t;&c_o; &c_m;&c_a;&c_je;
    <ind>&c_s;&c_ib;&c_l;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="ca">vàlid</trd>
  <trdgrp lng="cs">
    <trd>platný</trd>,
    <trd>právoplatný</trd>,
    <trd>validní</trd>
  </trdgrp>
  <trd lng="de">gültig</trd>
  <trd lng="en">valid</trd>
  <trd lng="es">válido</trd>
  <trdgrp lng="fr">
    <trd>en cours</trd>,
    <trd>valable</trd>,
    <trd>valide</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tav;&qof;&fpe;</trd>,
    <trd>&bet;&resh;-&tav;&vav;&qof;&fpe;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>érvényes</trd>,
    <trd>hatályos</trd>
  </trdgrp>
  <trd lng="nl">geldig</trd>
  <trd lng="pt">válido</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr
    tip="ind"> (&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j;
    &c_s;&c_i;&c_l;&c_u;)</klr></trd>,
    <trd>&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_s;&c_i;&c_l;&c_u;
    <klr tip="amb">(&c_ju;&c_r;.)</klr></trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>platný</trd>,
    <trd>právoplatný</trd>
  </trdgrp>
  <trd lng="sv">giltlig</trd>
  <trd lng="zh">有效的</trd>
</drv>

<drv mrk="valid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="valid.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Valori laŭ certaj kondiĉoj pri leĝeco, templimo
      ktp.:
      <ekz>
        la vizo de la pasporto <tld/>os por 60-taga restado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_m;&c_o;&c_c;</ind></trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_s;&c_ib;&c_l;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="ca">ésser vàlid</trd>
  <trdgrp lng="cs">
    <trd>být platný</trd>,
    <trd>mít platnost</trd>,
    <trd>platit</trd>
  </trdgrp>
  <trd lng="de">gelten</trd>
  <trdgrp lng="es">
    <trd>tener vigor</trd>,
    <trd>tener vigencia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avoir <ind>cours</ind></trd>,
    <trd>être <ind>valable</ind></trd>,
    <trd>être <ind>valide</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&yod;&vav;&tav;
  <ind>&tav;&qof;&fpe;</ind></trd>
  <trdgrp lng="hu">
    <trd>érvényben van</trd>,
    <trd>hatályos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geldig zijn</trd>,
    <trd>gelden</trd>
  </trdgrp>
  <trd lng="pt">ser válido</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; &c_s;&c_i;&c_l;&c_u; <klr
  tip="amb">(&c_ju;&c_r;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>mať platnosť</trd>,
    <trd>platiť</trd>
  </trdgrp>
  <trd lng="sv">gälla</trd>
  <trdgrp lng="zh">
    <trd>有效</trd>,
    <trd>生效</trd>
  </trdgrp>
</drv>

<drv mrk="valid.eks0igxi">
  <kap>eks<tld/>iĝi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="valid.eks0igxi.JUR">
    <dif>
      Ne plu <tld/>i pro kadukiĝo, pro paso de iu templimo:
      <ekz>
        mi sugestas ke oni akiru malnovajn filmojn kies kopirajtoj
        eks<tld/>iĝis
        <fnt>
        <aut>KriZe</aut>,
        <vrk><url ref="http://gxangalo.com/">Esperanto-Televido -
        Forumo</url></vrk>,
        <lok>2003-12-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;
    <ind>&c_m;&c_o;&c_c;</ind></trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;
    <ind>&c_s;&c_ib;&c_l;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>caducar</trd>,
    <trd>perdre la <ind>validesa</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devenir <ind>caduc</ind></trd>,
    <trd>perdre sa <ind>validité</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>érvénytelenné válik</trd>,
    <trd>érvényét veszti</trd>,
    <trd>hatályát veszti</trd>,
    <trd>lejár<klr tip="ind">(határidő)</klr></trd>
  </trdgrp>
  <trd lng="zh">失效</trd>
</drv>
</art>
