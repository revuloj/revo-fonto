<drv mrk="sku.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sku.for0i.KOMUNE">
    <dif>
      Rapide kaj krude <ref cel="jxet.for0i">for&jcirc;eti</ref>, forpu&scirc;i:
      <ekz>
        oni for<tld/>as min kiel akridon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 109:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        li for<tld/>is la beston en la fajron
        <fnt><bib>NT</bib>, <lok>&Ago; 28:5</lok></fnt>;
      </ekz>
      <ekz>
        for<tld/>u de vi la polvon, levi&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 52:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        li for<tld/>is &scirc;in per forta brakmovo kaj fari&gcirc;is ankora&ubreve; pli kolera
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento for<tld/>is de la betuloj la flavi&gcirc;intajn foliojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sku.for0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="liber.0igxi">Liberi&gcirc;i</ref> de io stre&ccirc;ante 
      sin plurdirekte:
      <ekz>
        <klr>[li]</klr> volis for<tld/>i de si la influon de la plej alta kolegio
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> iom post iom for<tld/>is la unuajn impresojn kaj komencis pli
        atente rigardi &ccirc;irka&ubreve;e
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi devas for<tld/>i la kvietan, mediteman vivon en nia urbeto, kaj ser&ccirc;i en pli
        &gcirc;ojaj &ccirc;irka&ubreve;a&jcirc;oj novajn, pli feli&ccirc;ajn, pli lumajn pensojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i tamen balda&ubreve; for<tld/>is de si la kontra&ubreve;volan enpensi&gcirc;on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi devas for<tld/>i vian pezan humoron, diris la patrino, provante vigligi lin
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        diable! li diras interne kaj perforte for<tld/>as la malgajajn pensojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_e;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="cs">set&rcaron;&aacute;st</trd>
  <trdgrp lng="de">
    <trd>absch&uuml;tteln</trd>
  </trdgrp>
  <trd lng="en">to cast off</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;barrasser</ind> de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elvet</trd>,
    <trd>eldob</trd>,
    <trd>ler&aacute;z</trd>
  </trdgrp>
  <trd lng="nl">afschudden</trd>
  <trd lng="pl">odrzuca&cacute;</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_ja;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">strias&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_r;&c_ja;&c_s;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_k;&c_i;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sku.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="sku.ne0ebla.KOMUNE">
    <dif>
      <ref cel="firm.0a">Firma</ref>, <ref cel="fidel.0a.iu">fidela</ref>, 
      ke &gcirc;in oni ne renversas a&ubreve; detruas:
      <ekz>
        la ne<tld/>eblaj restu
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 12:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        ricevante regnon ne<tld/>eblan, ni havu dankemecon
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 12:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia koro estas pura, miaj intencoj ne<tld/>eblaj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; &ccirc;i tiu ne<tld/>ebla certeco &ccirc;io alia perdis la signifon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXIV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_a;&c_h;&c_ib;&c_s;&c_n;&c_y;</trd>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_o;&c_k;&c_l;&c_a;&c_t;&c_i;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>neochv&ecaron;jn&yacute;</trd>,
    <trd>neot&rcaron;esn&yacute;</trd>
  </trdgrp>
  <trd lng="de">unersch&uuml;tterlich</trd>
  <trd lng="en">unshakeable</trd>
  <trd lng="fr">in&eacute;branlable</trd>
  <trd lng="hu">megingathatatlan</trd>
  <trd lng="nl">onverstoorbaar</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_o;&c_k;&c_o;&c_l;&c_je;&c_b;&c_i;&c_m;&c_y;&c_j;
</trd>
  <trdgrp lng="sk">
    <trd>neochvejn&yacute;</trd>,
    <trd>neotrasite&lcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_o;&c_h;&c_i;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_h;&c_i;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_r;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sku.sen0e">
  <kap>sen<tld/>e</kap>
  <snc mrk="sku.sen0e.KOMUNE">
    <dif>
      Malabrupte, kun glata, 
      <ref cel="konsta.0e">konstanta</ref> iro:
      <ekz>
        &ccirc;io fari&gcirc;as kviete, sen<tld/>e kaj e&ccirc; nerimarkeble
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista 
          en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>e kaj regule da&ubreve;ras kaj disvolvi&gcirc;as tiu
        proceso de la homa movi&gcirc;ado, pensado kaj sentado,
        kiu estas nomata vivo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la vivo en Marino ruli&gcirc;is ne sen<tld/>e, kaj la kompatinda Nikolao <nom>Petrovi&ccirc;</nom>
        havis multe da &ccirc;agrenoj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_w;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_y;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_l;&c_a;&c_d;&c_k;&c_o;</trd>
  <trd lng="cs">bez ot&rcaron;es&uring;</trd>
  <trdgrp lng="de">
    <trd>gleichm&auml;&szlig;ig</trd>,
    <trd>ungest&ouml;rt</trd>
  </trdgrp>
  <trd lng="en">unwaveringly</trd>
  <trdgrp lng="fr">
    <trd>continument</trd>,
    <trd>sans secousses</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megr&aacute;zk&oacute;dtat&aacute;sok n&eacute;lk&uuml;l</trd>,
    <trd>sim&aacute;n <klr tip="ind">(&aacute;tv.)</klr></trd>
  </trdgrp>
  <trd lng="pl">bez <ind>wstrz&aogonek;s&oacute;w</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_v;&c_n;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_z;
    <ind>&c_p;&c_o;&c_t;&c_r;&c_ja;&c_s;&c_je;&c_n;&c_i;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="sk">bez otrasov</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_o;&c_r;&c_u;&c_sh;&c_n;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_h;&c_i;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="sku.anim0a">
  <kap>
    anim<tld/>a
  </kap>
  <snc mrk="sku.anim0a.PSI">
    <dif>
      Kiu <tld/>as la animon, tre <ref cel="emoci.0a">emocia</ref>, tre
      <ref tip="vid" cel="afekci.0a.anima">afekcia<sncref/></ref>,
      <ref tip="vid" cel="konste1.0a">konsterna</ref>:
       <ekz>
        anim<tld/>a bildo de malsata infano
        <fnt>
          <aut>-</aut>
          <vrk><url ref="http://www.esperokatolika.org/ek19811985/ek1984_03.htm">[kovrilo]</url></vrk>
          <lok>Espero Katolika, 1984:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la anim<tld/>a, sufoketa kaj malproksimatinga mu&gcirc;ado venis &scirc;ajne el sub la tero
        <fnt>
          <aut>Pang Naiying</aut>
          <vrk><url ref="http://esperanto.china.org.cn/EL/EL/ElPopolaCxinio/2000-06/bd2000-6-11.html">La
          lastaj arbohakistoj</url></vrk>
          <lok>El Popola &Ccirc;inio, 2000:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_sh;&c_a;&c_l;&c_a;&c_m;&c_l;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufw&uuml;hlend</trd>,
    <trd>ersch&uuml;tternd</trd>,
    <trd>bewegend</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bouleversant</trd>,
    <trd>poignant</trd>
  </trdgrp>
</drv>
