<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akompa.xml,v 1.42 2023/10/28 08:33:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>akompan</rad>/i
</kap>
<drv mrk="akompa.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akompa.0i.kunesti">
    <dif>
      Esti a&ubreve; iri kun iu a&ubreve; io:
      <ekz>
        mi <tld/>is miajn gastojn &gcirc;is ilia domo;
      </ekz>
      <ekz>
        &ccirc;iun sian vorton li <tld/>is per premo sur la brakon de
        sia najbaro;
      </ekz>
      <ekz>
        tiaj malfacila&jcirc;oj <tld/>as &ccirc;iujn kongresojn;
      </ekz>
      <ekz>
        la filantropio ne &ccirc;iam <tld/>as la ri&ccirc;econ;
      </ekz>
      <ekz>
        tiun teatran pozon <tld/>is viza&gcirc;esprimo, kiu signifis
        ravitecon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        aprobo havas valoron nur se &gcirc;in <tld/>as konformaj agoj;
      </ekz>
      <ekz>
        tiu &ccirc;i ideo <tld/>adis Esperantismon de &gcirc;ia naski&gcirc;o
        &gcirc;is la nuna tempo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en
          la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni promenis en <tld/>o de nia kuzo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>delc&#x2019;her <ind>kompagnunezh</ind> da<klr> [esti]</klr></trd>,
      <trd>beza&ntilde; <ind>gant</ind><klr> [esti]</klr></trd>,
      <trd>mont <ind>gant</ind><klr> [iri]</klr></trd>,
      <trd>ambroug<klr> [iri]</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">odprevadi&tcaron;</trd>
    <trdgrp lng="sv">
      <trd><ind>f&ouml;lja</ind> med</trd>,
      <trd>ledsaga</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4F34;&#x53D1; <pr>b&#xE0;nf&#x101;</pr></trd>,
    <trd>&#x4F34;&#x767C; <pr>b&#xE0;nf&#x101;</pr></trd>,
    <trd>&#x4F34;&#x968F;&#x7684; <pr>b&#xE0;nsu&#xED;d&#xED;</pr></trd>,
    <trd>&#x4F34;&#x96A8;&#x7684; <pr>b&#xE0;nsu&#xED;d&#xED;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="akompa.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kun<ref tip="super" cel="lud.0i.MUZ">ludi</ref>:
      <ekz>
        mia fratino <tld/>is min per la piano;
      </ekz>
      <ekz>
        la kantisto <tld/>is sin mem;
      </ekz>
      <ekz>
        stereotipa a&ubreve;tomata <tld/>ado helpe de <klr>[...]</klr>
        sonsinteziloj
        <fnt>
          <aut>Grigori Arosev</aut>
          <vrk><url ref="&Monato;/2001/007106.php">&Ccirc;armaj kantoj por
          infanoj</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 16</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="br">
      <trd>eila&ntilde;<klr tip="ind"> (sonerezh)</klr></trd>,
      <trd>seni&ntilde; da heul</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_k;&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">sprev&aacute;dza&tcaron;</trd>
    <trd lng="sv">ackompanjera</trd>
    <trd lng="zh">&#x4F34;&#x594F; <pr>b&#xE0;nz&#xF2;u</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>doprov&aacute;zet</trd>,
    <trd>doprov&aacute;zet <klr>(n&ecaron;koho)</klr></trd>
  </trdgrp>
  <trd lng="de">begleiten</trd>
  <trd lng="en">accompany</trd>
  <trd lng="es">acompa&ntilde;ar</trd>
  <trd lng="fr">accompagner</trd>
  <trd lng="he">&lamed;&he;&tav;&lamed;&vav;&vav;&tav;</trd>
  <trdgrp lng="id">
    <trd>men<ind>damping</ind>i</trd>,
    <trd>meng<ind>iring</ind>i</trd>,
    <trd>[<ind>serta</ind>] menyertai</trd>,
    <trd>[<ind>teman</ind>] menemani</trd>
  </trdgrp>
  <trd lng="it">accompagnare</trd>
  <trdgrp lng="ja">
    <trd>&#x540C;&#x884C;&#x3059;&#x308B;  <pr>&#x3069;&#x3046;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4F34;&#x594F;&#x3059;&#x308B;  <pr>&#x3070;&#x3093;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">begeleiden</trd>
  <trdgrp lng="pt">
    <trd>acompanhar</trd>,
    <trd>seguir</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sprev&aacute;dza&tcaron; <klr>(hud)</klr></trd>,
    <trd>sprev&aacute;dza&tcaron;, sprev&aacute;dza&tcaron; <klr>(hud)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="vo">kopan&ouml;n</trd>
</drv>

<drv mrk="akompa.0e_al">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kun.0.kie">Kun</ref>, agante helpe a&ubreve;
      akorde:
      <ekz>
        la &ccirc;eestantoj drinkis, babilis kaj &scirc;ercis, <tld/>e al &scirc;i
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 76a, p. 598a</lok></fnt>;
      </ekz>
      <ekz>
        &hcirc;oro <klr>[...]</klr> akompane al <klr>[...]</klr> plena
        orkestro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48938">Kantado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a; <ind>&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ju;</ind></trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">de <ind>compagnie</ind> avec</trd>
  <trdgrp lng="it">
    <trd>con</trd>,
    <trd><ind>assieme</ind> a</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a; <ind>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ju;</ind> &c_s;</trd>
</drv>

<drv mrk="akompa.0atede">
  <kap><tld/>ate de, <var><kap>en <tld/>o de</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="kun.0.kie">Kun</ref>:
      <ekz>
        aperis unu el la pordistoj de la instituto, <tld/>ate de
        oficisto el la re&gcirc;a palaco
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 15</lok></fnt>;
      </ekz>
      <ekz>
        unu el &scirc;iaj laboristinoj pasas la korton en <tld/>o de
        la nevenkebla Ale&ccirc;jo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kompan.en0ode">en kompanio de</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ib;</ind>
    <klr>(&c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_u;
    <ind>&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>e-ser<klr tip="ind"> (ub.)</klr></trd>,
    <trd>e <ind>kompagnunezh</ind><klr tip="ind"> (ub.)</klr></trd>,
    <trd>e <ind>kavandenn</ind><klr tip="ind"> (ub.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avec<klr tip="ind"> (en compagnie de)</klr></trd>,
    <trd>en <ind>compagnie</ind> de</trd>,
    <trd>accompagn&eacute; de</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>con</trd>,
    <trd>in <ind>compagnia</ind> di</trd>,
    <trd>accompagnato da</trd>
  </trdgrp>
  <trd lng="ru">&c_v; <ind>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_i;</ind> &c_s;</trd>
</drv>

<drv mrk="akompa.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Iu <tld/>anta:
      <ekz>
        li <klr>[...]</klr> ordonis al siaj <tld/>antoj atendi ekstere
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne plu bezonas <tld/>anton, &ccirc;ar la ser&ccirc;ekspedicio ne okazos
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="br">
    <trd>kompagnun<klr tip="ind"> (a zalc'h kompagnunezh)</klr></trd>,
    <trd>kavandad</trd>
    <trd>ambrouger</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pr&uring;vodce</trd>,
    <trd>spole&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Begleiter</trd>,
    <trd>Gefolgsmann</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>iu akompananta</trd>,
    <trd>companion</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compagnon<klr tip="ind"> (personne qui accompagne)</klr></trd>,
    <trd>accompagnateur</trd>
    <trd>membre de la <ind>suite</ind></trd>
  </trdgrp>
  <trd lng="he">&bet;&fnun; &lamed;&vav;&vav;&yod;&he;</trd>
  <trdgrp lng="id">
    <trd>pen<ind>damping</ind></trd>,
    <trd>peng<ind>iring</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accompagnatore</trd>,
    <trd>compagno<klr> (accompagnante)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x540C;&#x4F34;&#x8005;  <pr>&#x3069;&#x3046;&#x306F;&#x3093;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x968F;&#x884C;&#x8005;  <pr>&#x305A;&#x3044;&#x3053;&#x3046;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x4F34;&#x594F;&#x8005;  <pr>&#x3070;&#x3093;&#x305D;&#x3046;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spolo&ccaron;n&iacute;k</trd>,
    <trd>sprievodca</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_u;&c_p;&c_r;&c_o;&c_v;&c_ib;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="akompa.0antaro">
  <kap><tld/>antaro</kap>
  <snc>
    <dif>
      Tuto de la personoj, kiuj <tld/>as iun, ekz-e por honoro a&ubreve; gardo:
      <ekz>
        la juna re&gcirc;o elrajdis kun granda <tld/>antaro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kaj lia tuta <tld/>antaro <klr>[...]</klr> stari&gcirc;is anta&ubreve; li
        <fnt>
          <bib>MT</bib>
          <lok>Ii. re&gcirc;oj 5:15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="eskort.0o">eskorto</ref>
    <ref tip="sin" cel="kompan.0o.akompanantaro">kompanio</ref>
    <rim>
      Trovi&gcirc;as en <fnt><bib>PV</bib></fnt> mencio pri alternativa
      formo <ctl><tld/>aro</ctl>, kiu tamen ne &scirc;ajnas uzata.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ja; <klr tip="amb">(&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="br">
    <trd>koskoriad</trd>,
    <trd>ambrougadeg</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>doprovod</trd>,
    <trd>dru&zcaron;ina</trd>,
    <trd>pr&uring;vod</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gefolge</trd>
     <trd>Gefolgschaft</trd>
  </trdgrp>
  <trd lng="en">entourage</trd>
  <trd lng="es">compa&ntilde;&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>suite<klr tip="ind"> (personnes qui accompagnent)</klr></trd>,
    <trd>compagnie<klr tip="ind"> (entourage)</klr></trd>,
    <trd>entourage</trd>,
    <trd>escorte</trd>
  </trdgrp>
  <trd lng="he">&pe;&#x5BC;&#x5B8;&mem;&#x5B7;&lamed;&#x5B4;&yod;&#x5B8;&he;</trd>
  <trdgrp lng="id">
    <trd><ind>iring</ind>-iringan</trd>,
    <trd><ind>iring</ind>an</trd>,
    <trd><ind>rombong</ind>an</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>seguito</trd>,
    <trd>entourage</trd>,
    <trd>scorta</trd>,
    <trd>compagnia<klr> (gruppo)</klr></trd>,
    <trd>comitiva</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x968F;&#x54E1;  <pr>&#x305A;&#x3044;&#x3044;&#x3093;</pr></trd>,
    <trd>&#x968F;&#x884C;&#x56E3;  <pr>&#x305A;&#x3044;&#x3053;&#x3046;&#x3060;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cortejo</trd>,
    <trd>comitiva</trd>,
    <trd>s&eacute;quito</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dru&zcaron;ina</trd>,
    <trd>sprievod</trd>
  </trdgrp>
   <trdgrp lng="ms">
    <trd>mengiringi</trd>,
    <trd>berteman</trd>
  </trdgrp>
</drv>
</art>
