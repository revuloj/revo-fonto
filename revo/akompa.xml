<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>akompan</rad>/i
</kap>
<drv mrk="akompa.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akompa.0i.1">
    <dif>
      Esti a&ubreve; iri kun iu a&ubreve; io:
      <ekz>
        mi <tld/>is miajn gastojn &gcirc;is ilia domo;
      </ekz>
      <ekz>
        &ccirc;iun sian vorton li <tld/>is per premo sur la brakon de
        sia najbaro;
      </ekz>
      <ekz>
        tiaj malfacila&jcirc;oj <tld/>as &ccirc;iujn kongresojn;
      </ekz>
      <ekz>
        la filantropio ne &ccirc;iam <tld/>as la ri&ccirc;econ;
      </ekz>
      <ekz>
        tiun teatran pozon <tld/>is viza&gcirc;esprimo, kiu signifis
        ravitecon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        aprobo havas valoron nur se &gcirc;in <tld/>as konformaj agoj;
      </ekz>
      <ekz>
        tiu &ccirc;i ideo <tld/>adis Esperanton de &gcirc;ia naski&gcirc;o
        &gcirc;is la nuna tempo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni promenis en <tld/>o de nia kuzo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>delc&#8217;her <ind>kompagnunezh</ind> da<klr> [esti]</klr></trd>,
      <trd>beza&ntilde; <ind>gant</ind><klr> [esti]</klr></trd>,
      <trd>mont <ind>gant</ind><klr> [iri]</klr></trd>,
      <trd>ambroug<klr> [iri]</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">odprevadi&tcaron;</trd>
    <trdgrp lng="sv">
      <trd><ind>f&ouml;lja</ind> med</trd>,
      <trd>ledsaga</trd>
    </trdgrp>
  </snc>
  <snc mrk="akompa.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kun<ref tip="super" cel="lud.0i.MUZ">ludi</ref>:
      <ekz>
        mia fratino <tld/>is min per la piano;
      </ekz>
      <ekz>
        la kantisto <tld/>is sin mem.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="br">
      <trd>eila&ntilde;<klr tip="ind"> (sonerezh)</klr></trd>,
      <trd>seni&ntilde; da heul</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_k;&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">sprev&aacute;dza&tcaron;</trd>
    <trd lng="sv">ackompanjera</trd>
  </snc>
  <trd lng="cs">doprov&aacute;zet</trd>
  <trd lng="de">begleiten</trd>
  <trd lng="en">accompany</trd>
  <trd lng="es">acompa&ntilde;ar</trd>
  <trd lng="fr">accompagner</trd>
  <trd lng="it">accompagnare</trd>
  <trd lng="nl">begeleiden</trd>
  <trdgrp lng="pt">
    <trd>acompanhar</trd>,
    <trd>seguir</trd>
  </trdgrp>
  <trd lng="vo">kopan&ouml;n</trd>
</drv>

<drv mrk="akompa.0e.al">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kun.0.kie">Kun</ref>, agante helpe a&ubreve;
      akorde:
      <ekz>
        la &ccirc;eestantoj drinkis, babilis kaj &scirc;ercis, <tld/>e al &scirc;i
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 76a, p. 598a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">de <ind>compagnie</ind> avec</trd>
  <trdgrp lng="it">
    <trd>con</trd>,
    <trd><ind>assieme</ind> a</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a; <ind>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ju;</ind> &c_s;</trd>
</drv>

<drv mrk="akompa.0atede">
  <kap><tld/>ate de, en <tld/>o de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <ref tip="dif" cel="kun.0.kie">kun</ref>
    <ekz>
      aperis unu el la pordistoj de la instituto, <tld/>ate de
      oficisto el la re&gcirc;a palaco
      <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 15</lok></fnt>;
    </ekz>
    <ekz>
      unu el &scirc;iaj laboristinoj pasas la korton en <tld/>o de
      la nevenkebla Ale&ccirc;jo
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
    <ref tip="sin" cel="kompan.en0ode">en kompanio de</ref>
  </snc>
  <trdgrp lng="br">
    <trd>e-ser<klr tip="ind"> (ub.)</klr></trd>,
    <trd>e <ind>kompagnunezh</ind><klr tip="ind"> (ub.)</klr></trd>,
    <trd>e <ind>kavandenn</ind><klr tip="ind"> (ub.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avec<klr tip="ind"> (en compagnie de)</klr></trd>,
    <trd>en <ind>compagnie</ind> de</trd>,
    <trd>accompagn&eacute; de</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>con</trd>,
    <trd>in <ind>compagnia</ind> di</trd>,
    <trd>accompagnato da</trd>
  </trdgrp>
  <trd lng="ru">&c_v; <ind>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_i;</ind> &c_s;</trd>
</drv>

<drv mrk="akompa.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Iu <tld/>anta.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>kompagnun<klr tip="ind"> (a zalc'h kompagnunezh)</klr></trd>,
    <trd>kavandad</trd>
    <trd>ambrouger</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compagnon<klr tip="ind"> (personne qui accompagne)</klr></trd>,
    <trd>accompagnateur</trd>
    <trd>membre de la <ind>suite</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accompagnatore</trd>,
    <trd>compagno<klr> (accompagnante)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="akompa.0antaro">
  <kap><tld/>antaro</kap>
  <snc>
    <dif>
      Tuto de la personoj, kiuj <tld/>as iun, ekz-e por honoro a&ubreve; gardo.
    </dif>
    <ref tip="vid" cel="eskort.0o">eskorto</ref>
    <ref tip="sin" cel="kompan.0o.akompanantaro">kompanio</ref>
    <rim>
      Trovi&gcirc;as en <fnt><bib>PV</bib></fnt> mencion pri alternativa
      formo <ctl><tld/>aro</ctl>, kiu tamen ne &scirc;ajnas uzata.
    </rim>
  </snc>
  <trd lng="de">Begleiter</trd>
  <trd lng="en">entourage</trd>
  <trdgrp lng="br">
    <trd>koskoriad</trd>,
    <trd>ambrougadeg</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite<klr tip="ind"> (personnes qui accompagnent)</klr></trd>,
    <trd>compagnie<klr tip="ind"> (entourage)</klr></trd>,
    <trd>entourage</trd>,
    <trd>escorte</trd>
  </trdgrp>
  <trd lng="es">compa&ntilde;&iacute;a</trd>
  <trdgrp lng="it">
    <trd>seguito</trd>,
    <trd>entourage</trd>,
    <trd>scorta</trd>,
    <trd>compagnia<klr> (gruppo)</klr></trd>,
    <trd>comitiva</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cortejo</trd>,
    <trd>comitiva</trd>,
    <trd>s&eacute;quito</trd>
  </trdgrp>
  <trd lng="sk">sprievod</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
