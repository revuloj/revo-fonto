<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>akompan</rad>/i
</kap>

<drv mrk="akompa.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1" mrk="akompa.0i.1">
    <dif>
      Esti a&ubreve; iri kun iu a&ubreve; io:
      <ekz>mi <tld/>is miajn gastojn &gcirc;is ilia domo;</ekz>
      <ekz>
        &ccirc;iun sian vorton li <tld/>is per premo sur la brakon de 
        sia najbaro;
      </ekz>
      <ekz>tiaj malfacila&jcirc;oj <tld/>as &ccirc;iujn  kongresojn;</ekz>
      <ekz>la filantropio ne &ccirc;iam <tld/>as la ri&ccirc;econ;</ekz>
      <ekz>
        tiun teatran pozon <tld/>is viza&gcirc;esprimo, kiu signifis ravitecon<fnt>Z</fnt>;
      </ekz>
      <ekz>aprobo havas valoron nur se &gcirc;in <tld/>as konformaj agoj;</ekz>
      <ekz>
        tiu &ccirc;i ideo <tld/>adis Esp-on de &gcirc;ia naski&gcirc;o 
        &gcirc;is la nuna tempo<fnt>Z</fnt>;
      </ekz>
      <ekz>ni promenis en <tld/>o de nia kuzo.</ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>, 
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd><ind>f&ouml;lja</ind> med</trd>,
      <trd>ledsaga</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kunludi:
      <ekz>mia fratino <tld/>is min  per la piano;</ekz>
      <ekz>la kantisto <tld/>is sin mem.</ekz>
    </dif>
    <trd lng="be">&c_a;&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ru">&c_a;&c_k;&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">ackompanjera</trd>
  </snc>
  <trd lng="cs">doprov&aacute;zet</trd>
  <trd lng="de">begleiten</trd>
  <trd lng="en">accompany</trd>
  <trd lng="es">acompa&ntilde;ar</trd>
  <trd lng="fr">accompagner</trd>
  <trd lng="nl">begeleiden</trd>
  <trdgrp lng="pt">
    <trd>acompanhar</trd>,
    <trd>seguir</trd>
  </trdgrp>
  <trd lng="sk">sprev&aacute;dza&tcaron;</trd>
</drv>

<drv mrk="akompa.0antaro">
  <kap><tld/>(ant)aro</kap>
  <snc>
    <dif>
      Grupo da personoj, kiuj <tld/>as iun por honoro a&ubreve; gardo.
    </dif>
  </snc>
  <trd lng="de">Begleiter</trd>
  <trd lng="en">entourage</trd>
  <trd lng="fr">suite</trd>
  <trd lng="es">compa&ntilde;&iacute;a</trd>
  <trdgrp lng="pt">
    <trd>cortejo</trd>,
    <trd>comitiva</trd>,
    <trd>s&eacute;quito</trd>
  </trdgrp>
  <trd lng="sk">sprievod</trd>
</drv>
</art>
