<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aligat1.xml,v 1.11 2023/10/28 08:33:05 revo Exp $">
<kap>
  <rad>aligatored</rad>/oj
</kap>
<drv mrk="aligat1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="aligat1.0oj.ZOO">
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="krokod.0uloj">krokodiluloj</ref>
      (<trd lng="la">Alligatoridae</trd>), &ccirc;e kiuj la kvara dento estas
      ne videbla &ccirc;e fermita bu&scirc;o:
      <ekz>
        la <tld/>oj estas familio al kiu apartenas la <ref tip="prt" cel="aligat.0o">aligatoroj</ref>
        kaj <ref tip="prt" cel="kajman.0o">kajmanoj</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=161588">Aligatoredoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x77ED;&#x543B;&#x9C77; <pr>du&#x1CE;nw&#x11B;n&#xE8;</pr></trd>,
    <trd>&#x77ED;&#x543B;&#x9CC4; <pr>du&#x1CE;nw&#x11B;n&#xE8;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_g;&c_a;&c_t;&c_a;&c_r;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Alligatoren</trd>
  <trdgrp lng="en">
     <trd>alligators</trd>,
     <trd>Alligatoridae</trd>
  </trdgrp>
  <trd lng="fr">alligatorid&eacute;s</trd>
  <trd lng="he">&#x5EA;&#x5E0;&#x5D9;&#x5E0;&#x5D9;&#x5DD;</trd>
  <trd lng="pl">aligatorowate</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_i;&c_g;&c_a;&c_t;&c_o;&c_r;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

