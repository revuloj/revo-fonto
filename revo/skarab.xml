<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skarab</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="skarab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skarab.0o.koleoptero">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>scarab</trd>,
      <trd>beetle</trd>
    </trdgrp>
    <trd lng="hu">bog&aacute;r</trd>
    <trd lng="ru">&c_zh;&c_u;&c_k;</trd>
  </snc>
  <snc mrk="skarab.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="skarab1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Scarabaeus</trd>),
      karakterizata per lamenformaj kornoj kaj metanta la
      ovojn en sterkoglobetojn.
    </dif>
    <trd lng="de">Skarab&auml;us</trd>
    <trd lng="fr">scarab&eacute;e</trd>
    <trd lng="hu">galacsinhajt&oacute;</trd>
    <trd lng="ru">&c_s;&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_j;</trd>
  </snc>
  <trd lng="nl">kever</trd>
</drv>

<drv mrk="skarab.cervo0o">
  <kap>cervo<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="skarab.cervo0o.ZOO">
    <dif>
      Granda koleoptero, kies vira genro posedas fortajn
      prenilojn sur la kapo, genro el la familio
      <ref tip="super" cel="lukane.0oj">lukanedoj</ref>
      (<trd lng="la">Lucanus</trd>).
    </dif>    
    <ref tip="sin" cel="lukan.0o.ZOO">lukano</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>cerf-volant<klr tip="ind"> (insecte)</klr></trd>,
    <trd>lucane</trd>
  </trdgrp>
  <trd lng="be">&c_zh;&c_u;&c_k;-&c_a;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Hirschk&auml;fer</trd>
  <trd lng="en">stag beetle</trd>
  <trd lng="hu">szarvasbog&aacute;r</trd>
  <trd lng="nl">vliegend <ind>hert</ind></trd>
  <trd lng="pt">lucan&iacute;deo</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_u;&c_k;-&c_o;&c_l;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_g;&c_a;&c_ch; <klr tip="amb">(&c_zh;&c_u;&c_k;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="skarab.gren0o">
  <kap>gren<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="skarab.gren0o.ZOO">
    <dif>
      Genro el familio <ref tip="super"
      cel="kurkul1.0oj">kurkuliedoj</ref>
      (<trd lng="la">Sitophilus</trd>), kies larvo kreskas ene de
      grengrajnoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="kaland2.0o">kalandrao</ref>,
      <ref cel="kaland.0o.ZOO">kalandro</ref>
    </refgrp>
  </snc>
  <trd lng="de">Kornk&auml;fer</trd>
  <trd lng="en">corn weevil</trd>
  <trdgrp lng="fi">
    <trd>jyv&auml;k&auml;rs&auml;k&auml;s</trd>,
    <trd>(Sitophilus-suvun) <ind>k&auml;rs&auml;k&auml;s</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>calandre<klr tip="ind"> (charan&ccedil;on)</klr></trd>,
    <trd>charan&ccedil;on</trd>
  </trdgrp>
  <trd lng="hu">zsizsik</trd>
  <trd lng="nl">kalander</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j; &c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;</trd>
</drv>

<drv mrk="skarab.ligno0o">
  <kap>ligno<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="skarab.ligno0o.ZOO">
    <dif>
      Diversaj koleopteroj, kies larvoj kreskas en viva ligno de arboj,
      en putranta ligno a&ubreve; en ligna&jcirc;oj, ilin interne man&gcirc;ante:
      <ekz>
        la insekto brile nigra-blanka kun longaj striitaj antenoj estis
        azia ligno<tld/>o kun latina nomo <ctl>Anoplophora
        glabripennis</ctl>
        <fnt>
          <aut>S. Maul</aut>,
          <vrk>Surprizo el ligna kesto</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 1a, p. 21a</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Holzk&auml;fer</trd>
  <trd lng="en">woodboring beetle</trd>
  <trdgrp lng="fr">
    <trd>bupreste</trd>,
    <trd>ver de bois</trd>,
    <trd>vrillette</trd>
  </trdgrp>
  <trd lng="hu">sz&uacute;</trd>
  <trd lng="nl">houtkever</trd>
</drv>

<drv mrk="skarab.maj0o">
  <kap>maj<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="skarab.maj0o.ZOO">
    <dif>
      Herboman&gcirc;a koleoptero, tre malutila same kiel &gcirc;ia
      larvo, tielnomata blanka vermo, genro el familio <ref tip="super" 
      cel="skarab1.0oj">skarabedoj</ref>
      (<trd lng="la">Melolontha</trd>):
      <ekz>
        &scirc;i kantis tre bele: &leftquot;Maj<tld/>o, flugu!&rightquot;
        <fnt><bib>Fab1</bib>, <lok>Elinjo-fingreto</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="melolo.0o.ZOO">melolonto</ref>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_u;&c_sh;&c_ch;</trd>
  <trd lng="de">Maik&auml;fer</trd>
  <trd lng="en">may bug</trd>
  <trdgrp lng="fr">
    <trd>hanneton</trd>,
    <trd>ver blanc<klr tip="ind"> (insecte)</klr></trd>
  </trdgrp>
  <trd lng="hu">cserebog&aacute;r</trd>
  <trd lng="nl">meikever</trd>
  <trd lng="pl">chrab&aogonek;szcz majowy</trd>
  <trdgrp lng="pt">
    <trd>melolonta</trd>,
    <trd>besouro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_zh;&c_u;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_h;&c_r;&c_u;&c_shch;</trd>
  </trdgrp>
  <trd lng="sk">chr&uacute;st</trd>
</drv>

<drv mrk="skarab.mari0o">
  <kap>mari<tld/>o</kap>
  <snc mrk="skarab.mari0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Koleoptero, kun duonsfera korpo kaj beletaj rondaj makuletoj sur 
      &gcirc;ia kiraso, nutrantanta sin de afidoj,
      genro el familio <ref tip="super"
      cel="kokcin1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Coccinella</trd>):
      <ekz>
        mari<tld/>o / tra inter herba valo / glite rapidas
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url
          ref="http://www.vastalto.com/blog/?p=259">Mariskarabo</url></vrk>,
          <lok>2006-04-08</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kokcin.0o.ZOO">kokcinelo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_w;&c_k;&c_a;-&c_b;&c_a;&c_g;&c_o;&c_w;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>buoc'h-Doue</trd>,
    <trd>c'hwilig-Doue</trd>,
    <trd>yarig-Doue</trd>
  </trdgrp>
  <trd lng="de">Marienk&auml;fer <klr tip="ind">(Gattung)</klr></trd>
  <trdgrp lng="en">
    <trd>ladybird</trd>,
    <trd>ladybird beetle</trd>,
    <trd>lady beetle</trd>,
    <trd>ladybug</trd>
  </trdgrp>
  <trd lng="fr">coccinelle</trd>
  <trd lng="hu">katicabog&aacute;r</trd>
  <trd lng="nl">lieveheersbeestje</trd>
  <trd lng="ru">&c_b;&c_o;&c_zh;&c_mol;&c_ja; &c_k;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>

<drv mrk="skarab.sterko0o">
  <kap>sterko<tld/>o, <var><kap>sterk<tld/>o</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="skarab.sterko0o.ZOO">
    <dif>
      Koleoptero nigre, blue a&ubreve; verde brila kaj
      vivanta en sterka&jcirc;oj, genro el la familio <ref tip="super"
      cel="geotru1.0oj">geotrupedoj</ref>
      (<trd lng="la">Geotrupes</trd>).
      <ekz>
        elrampis la sterk<tld/>o
        <fnt><bib>Fab4</bib>, <lok>Sterkskarabo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="geotru.0o.ZOO">geotrupo</ref>
  </snc>
  <trd lng="de">Mistk&auml;fer</trd>
  <trd lng="en">dung beetle</trd>
  <trdgrp lng="fr">
    <trd>bousier</trd>,
    <trd>g&eacute;otrupe</trd>
  </trdgrp>
  <trd lng="hu">ganajt&uacute;r&oacute; bog&aacute;r</trd>
  <trd lng="nl">mestkever</trd>
  <trd lng="pt">escaravelho</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_z;&c_n;&c_y;&c_j; &c_zh;&c_u;&c_k;</trd>
</drv>
</art>
