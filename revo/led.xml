<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: led.xml,v 1.27 2024/01/15 16:14:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>led</rad>/o
</kap>

<drv mrk="led.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="led.0o.LED">
    <dif>
      Felo preparita por industriaj uzoj: &scirc;uoj, rimenoj kaj aliaj:
      <ekz>
        el fremda <tld/>o oni tran&ccirc;as lar&gcirc;e
        <klr>(oni estas malavara se oni ne pagas la kostojn.
        <ref tip="vid" cel="posx.0o">po&scirc;o</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fel.0o">felo</ref>,
      <ref cel="hauxt.0o">ha&ubreve;to</ref>,
      <ref cel="pelt.0o">pelto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_u;&c_r;&c_a; <klr>(&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_je;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_o;&c_zh;&c_a;</trd>
  <trd lng="bo">&t_ka;&t_naro;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="br">l&ecirc;r</trd>
  <trd lng="ca">cuir</trd>
  <trdgrp lng="cs">
    <trd>k&uring;&zcaron;e vy&ccaron;in&ecaron;n&aacute;</trd>,
    <trd>use&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Leder</trd>
  <trd lng="el">&delta;&epsilon_ton;&rho;&my;&alfa;</trd>
  <trd lng="en">leather</trd>
  <trd lng="es">cuero</trd>
  <trd lng="fr">cuir</trd>
  <trd lng="hu">b&odblac;r <klr>(kidolgozott)</klr></trd>
  <trd lng="it">cuoio</trd>
  <trdgrp lng="ja">
    <trd>&#x306A;&#x3081;&#x3057;&#x9769;  <pr>&#x306A;&#x3081;&#x3057;&#x304C;&#x308F;</pr></trd>,
    <trd>&#x9769;  <pr>&#x3064;&#x304F;&#x308A;&#x304B;&#x308F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>leder</trd>,
    <trd>leer</trd>
  </trdgrp>
  <trd lng="pl">sk&oacute;ra <klr tip="amb">wyprawiona</klr></trd>
  <trd lng="pt">couro</trd>
  <trd lng="ru">&c_k;&c_o;&c_zh;&c_a; <klr>(&c_v;&c_y;&c_d;&c_je;&c_l;&c_a;&c_n;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">ko&zcaron;a <klr>(vypracovan&aacute;)</klr> reme&ncaron;</trd>
  <trd lng="uk">&#x448;&#x43A;&#x456;&#x440;&#x430; (&#x432;&#x438;&#x440;&#x43E;&#x431;&#x43B;&#x435;&#x43D;&#x430;)</trd>
  <trdgrp lng="zh">
    <trd>&#x76AE;&#x9769; <pr>p&#xED;g&#xE9;</pr></trd>,
    <trd>&#x76AE; <pr>p&#xED;</pr></trd>,
    <trd>&#x9769; <pr>g&#xE9;</pr></trd>,
    <trd>&#x97CB; <pr>w&#xE9;i</pr></trd>,
    <trd>&#x97E6; <pr>w&#xE9;i</pr></trd>,
    <trd>&#x97B9; <pr>ku&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="led.grajno0o">
  <kap>grajno<tld/>o</kap>
  <snc mrk="led.grajno0o.LED">
    <dif>
      Malglata <tld/>o, kvaza&ubreve; kovrita per grajnoj:
      <ekz>
        &scirc;i &jcirc;etis anta&ubreve; si <klr>[&#x2026;]</klr> sakon el grajno<tld/>o, kiun &scirc;i portis
        &scirc;ultrozone
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_g;&c_r;&c_y;&c_n;</trd>
  <trd lng="ca">xagr&iacute;</trd>
  <trd lng="cs">zrnit&aacute; k&uring;&zcaron;e</trd>
  <trd lng="de">Chagrinleder</trd>
  <trdgrp lng="fr">
    <trd>cuir grenu</trd>,
    <trd>chagrin<klr tip="ind"> (cuir)</klr></trd>
  </trdgrp>
  <trd lng="hu">szemcs&eacute;s <ind>b&odblac;r</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x7C92;&#x8D77;&#x306A;&#x3081;&#x3057;&#x9769;  <pr>&#x3064;&#x3076;&#x304A;&#x3053;&#x306A;&#x3081;&#x3057;&#x304C;&#x308F;</pr></trd>,
    <trd>&#x30B7;&#x30E3;&#x30B0;&#x30EA;&#x30FC;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">chagrijnleer</trd>
  <trd lng="pt">chagr&eacute;m</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_g;&c_r;&c_je;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_zh;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_g;&c_r;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zrnit&aacute; ko&zcaron;a <klr>(&scaron;agr&eacute;n)</klr></trd>
</drv>
</art>
