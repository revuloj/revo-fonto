<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>led</rad>/o
</kap>
<drv mrk="led.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="led.0o.LED">
    <dif>
      Felo preparita por industriaj uzoj: &scirc;uoj, rimenoj kaj aliaj:
      <ekz>
        el fremda <tld/>o oni tran&ccirc;as lar&gcirc;e
        <klr>(oni estas malavara se oni ne pagas la kostojn.
        <ref tip="vid" cel="posx.0o">po&scirc;o</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fel.0o">felo</ref>,
      <ref cel="hauxt.0o">ha&ubreve;to</ref>,
      <ref cel="pelt.0o">pelto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_u;&c_r;&c_a; <klr>(&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_je;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="br">l&ecirc;r</trd>
  <trd lng="de">Leder</trd>
  <trd lng="el">&delta;&epsilon_ton;&rho;&my;&alfa;</trd>
  <trd lng="en">leather</trd>
  <trd lng="es">cuero</trd>
  <trd lng="fr">cuir</trd>
  <trd lng="hu">b&odblac;r <klr>(kidolgozott)</klr></trd>
  <trd lng="it">cuoio</trd>
  <trdgrp lng="nl">
    <trd>leder</trd>,
    <trd>leer</trd>
  </trdgrp>
  <trd lng="pt">couro</trd>
  <trd lng="ru">&c_k;&c_o;&c_zh;&c_a; <klr>(&c_v;&c_y;&c_d;&c_je;&c_l;&c_a;&c_n;&c_n;&c_a;&c_ja;)</klr></trd>
</drv>

<drv mrk="led.grajno0o">
  <kap>grajno<tld/>o</kap>
  <snc mrk="led.grajno0o.LED">
    <dif>
      Malglata <tld/>o, kvaza&ubreve; kovrita per <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_g;&c_r;&c_y;&c_n;</trd>
  <trd lng="de">Chagrinleder</trd>
  <trdgrp lng="fr">
    <trd>cuir grenu</trd>,
    <trd>chagrin<klr tip="ind"> (cuir)</klr></trd>
  </trdgrp>
  <trd lng="hu">szemcs&eacute;s <ind>b&odblac;r</ind></trd>
  <trd lng="nl">chagrijnleer</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_g;&c_r;&c_je;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_zh;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_g;&c_r;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
