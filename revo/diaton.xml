    <trd lng="ja">&#20840;&#38899;&#38542;&#12398;  <pr>&#12380;&#12435;&#12362;&#12435;&#12363;&#12356;&#12398;</pr></trd>
  <trd lng="pt">diat&ocirc;nico</trd>
