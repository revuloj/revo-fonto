<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lameli.xml,v 1.18 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>lamelibrank</rad>/oj
</kap>
<drv mrk="lameli.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="valv.du0uloj">Duvalvuloj</ref>:
      <ekz>
        <tld/>oj nuntempe estas nomataj duvalvuloj, al ili apartenas ekz.
        <ref tip="ekz" cel="mitul.0o">mitulo</ref>
        kaj <ref tip="ekz" cel="ostr.0o">ostro</ref>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="konk.klap0oj">klapkonkoj</ref>
  </refgrp>
  <trd lng="be">&c_d;&c_v;&c_u;&c_h;&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;&c_v;&c_y;&c_ja; <ind>&c_m;&c_a;&c_l;&c_ju;&c_s;&c_k;&c_ib;</ind></trd>
  <trdgrp lng="ca">
    <trd>lamel&middot;libranquis</trd>,
    <trd>pelec&iacute;podes</trd>,
    <trd>ac&egrave;fals</trd>,
    <trd>bivalves</trd>
  </trdgrp>
  <trd lng="de">Muscheltiere</trd>
  <trd lng="es">lamelibranquios</trd>
  <trdgrp lng="fr">
    <trd>lamellibranches</trd>
  </trdgrp>
  <trd lng="hu">lemezeskopolty&uacute;sok</trd>
  <trd lng="la">Lamellibranchiata</trd>
  <trd lng="nl">plaatkieuwigen</trd>
  <trd lng="pl">ma&lstroke;&zdot;e</trd>
  <trdgrp lng="pt">
    <trd>lamelibr&acirc;nquios</trd>,
    <trd>pelec&iacute;podes</trd>
  </trdgrp>
  <trd lng="ro">molu&#x219;te comestibile</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_zh;&c_a;&c_b;&c_je;&c_r;&c_n;&c_y;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#x74E3;&#x9C13;&#x7DB1; <pr>b&#xE0;ns&#x101;ig&#x101;ng</pr></trd>,
    <trd>&#x74E3;&#x9CC3;&#x7EB2; <pr>b&#xE0;ns&#x101;ig&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
