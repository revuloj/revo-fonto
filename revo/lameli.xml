<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lamelibrank</rad>/oj
</kap>
<drv mrk="lameli.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_klasoj" cel="klas.0o.BIO">Klaso</ref>
      de <ref tip="super" cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Bivalvia</trd>)
      kun duvalva konko kaj kies granda nombro estas
      man&gcirc;ebla:
      <ekz>
        <tld/>oj nuntempe estas nomataj duvalvuloj, al ili apartenas ekz.
        <ref tip="ekz" cel="mitul.0o">mitulo</ref>
        kaj <ref tip="ekz" cel="ostr.0o">ostro</ref>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="konk.klap0oj">klapkonkoj</ref>
  </refgrp>
  <trdgrp lng="ca">
    <trd>lamel&middot;libranquis</trd>,
    <trd>pelec&iacute;podes</trd>,
    <trd>ac&egrave;fals</trd>,
    <trd>bivalves</trd>
  </trdgrp>
  <trd lng="de">Muscheltiere</trd>
  <trd lng="es">lamelibranquios</trd>
  <trdgrp lng="fr">
    <trd>lamellibranches</trd>
  </trdgrp>
  <trd lng="hu">lemezeskopolty&uacute;sok</trd>
  <trd lng="la">Lamellibranchiata</trd>
  <trd lng="nl">plaatkieuwigen</trd>
  <trd lng="pl">ma&lstroke;&zdot;e</trd>
  <trdgrp lng="pt">
    <trd>lamelibr&acirc;nquios</trd>,
    <trd>pelec&iacute;podes</trd>
  </trdgrp>
  <trd lng="ro">molu&#537;te comestibile</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_zh;&c_a;&c_b;&c_je;&c_r;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
