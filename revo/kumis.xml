<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kumis</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kumis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentinta acida &ccirc;evalina lakto, uzata kiel trinka&jcirc;o.  
    </dif>
    <ref tip="vid" cel="kefir.0o">kefiro</ref>
  </snc>
  <trd lng="de">Kumyss</trd>
  <trdgrp lng="en">
    <trd>koumiss</trd>,
    <trd>kumiss</trd>
  </trdgrp>
  <trd lng="nl">koemis</trd>
  <trd lng="ru">&c_k;&c_u;&c_m;&c_y;&c_s;</trd>
</drv>
</art>
