<drv mrk="scienc.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="scienc.0ulo.SCI">
    <dif>
      Esploranto pri fenomenoj de iu fako, pri ties devenoj, evoluoj kaj
      klarigoj:
      <ekz>
       la fondintoj kunvokis al Parizo komitaton de <tld/>uloj
       diverslandaj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        li estis lerta <tld/>ulo koncerne la instruon de Moseo
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 7:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj, apartenantaj al la pli bonstataj klasoj, maristoj, Kopenhagaj bur&gcirc;oj kaj
        anka&ubreve; kelke da <tld/>uloj sidis en vigla interparolado &ccirc;e siaj kru&ccirc;etoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        apogis lin <klr>[&#x2026;]</klr> hungaraj aktoroj, sportistoj kaj
        <tld/>uloj
        <fnt>
          <aut>K. Juh&aacute;sz</aut>,
          <vrk>Apena&ubreve; venko</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 6a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne demandu <tld/>ulon, demandu spertulon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>home de ci&egrave;ncia</trd>,
    <trd>savi</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>u&ccaron;enec</trd>,
    <trd>v&ecaron;dec</trd>
  </trdgrp>
