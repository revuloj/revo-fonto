<drv mrk="scienc.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj metodoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a nomenklaturo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        fari <tld/>ajn eksperimentojn kaj observojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pritraktado de <tld/>aj demandoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la profesoro de botaniko skribis <tld/>an verkon
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni turnas nin al diversaj <tld/>aj societoj kaj kongresoj
        <fnt>
          <bib>EE</bib>
          <lok>VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_y;</trd>
  <trd lng="de">wissenschaftlich</trd>
  <trd lng="en">scientific</trd>
  <trd lng="fr">scientifique</trd>
  <trdgrp lng="id">
    <trd>ilmiah</trd>,
    <trd>saintifik</trd>
  </trdgrp>
  <trdgrp lng="ja">
