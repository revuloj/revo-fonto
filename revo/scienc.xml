    <trd>&#31185;&#23398;&#30340;&#12394;  <pr>&#12363;&#12364;&#12367;&#12390;&#12365;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="pl">naukowy</trd>
  <trdgrp lng="zh">
    <trd>&#31185;&#23398; <pr>k&emacron;xu&eacute;</pr></trd>,
    <trd>&#31185;&#23416; <pr>k&emacron;xu&eacute;</pr></trd>
