<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magnez.xml,v 1.19 2025/03/06 19:29:32 revo Exp $">
<kap>
  <rad>magnez</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="magnez.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="magnez.0o.KEM">
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      <frm>MgO</frm>, <ref tip="super" cel="oksid.0o">oksido</ref> de <ref tip="prt" cel="magnez1.0o">magnezio</ref>:
      <ekz>
        <tld/>o estas blanka, mola, tre malpeza pulvoro, uzata en
        medicino;
      </ekz>
      <ekz>
        malica <klr>[&#x2026;]</klr> &gcirc;i fiksi&gcirc;is sub la koro <klr>[&#x2026;]</klr> kontra&ubreve; tio helpas
        neniu <tld/>o
        <fnt>
          <aut>Henrik Ibsen, trad. Odd Tangerud</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0309/0006.php">Popolmalamiko</url></vrk>
          <lok>5a akto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_e;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_k;&c_ib;&c_s; &c_m;&c_a;&c_g;&c_n;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trd lng="de">Magnesia</trd>
  <trdgrp lng="en">
     <trd>magnesia</trd>,
     <trd>magnesium oxide</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_n;&f_y;&a_z;&f_y;</trd>
  <trd lng="fr">magn&eacute;sie</trd>
  <trd lng="hu">magn&eacute;zia</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30B0;&#x30CD;&#x30B7;&#x30A2;</trd>,
    <trd>&#x82E6;&#x571F;  <pr>&#x304F;&#x3069;</pr></trd>
  </trdgrp>
  <trd lng="nl">magnesia</trd>
  <trdgrp lng="pl">
    <trd>tlenek <ind>magnezu</ind></trd>,
    <trd>magnezja <klr>palona</klr></trd>
  </trdgrp>
  <trd lng="pt">magn&eacute;sia</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s;&c_mol; &c_m;&c_a;&c_g;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s; &c_m;&c_a;&c_g;&c_n;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C27;&#x5316;&#x9382; <pr>y&#x1CE;nghu&agrave;m&ecaron;i</pr></trd>,
    <trd>&#x6C27;&#x5316;&#x9541; <pr>y&#x1CE;nghu&agrave;m&ecaron;i</pr></trd>
  </trdgrp>
</drv>
</art>
