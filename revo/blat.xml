<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>blat</rad>/o
</kap>
<drv mrk="blat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el la ordo de insektoj, al kiu apartenas
      <tld/>o <sncref ref="blat.0o.ZOO"/>.
    </dif>
    <trd lng="de">Schabe</trd>
    <trd lng="es">cucaracha</trd>
    <trd lng="fr">blatte</trd>
    <trdgrp lng="hu">
      <trd>cs&oacute;t&aacute;ny</trd>,
      <trd>sv&aacute;bbog&aacute;r</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_k;&c_a;&c_n;</trd>
  </snc>
  <snc mrk="blat.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <tld/>edoj (<trd lng="la">Blatta</trd>),
      ofte vivanta en kuirejoj, bakejoj ktp.
    </dif>
    <trd lng="de">K&uuml;chenschabe</trd>
    <trd lng="es">cucaracha</trd>
    <trdgrp lng="fr">
      <trd>blatte</trd>,
      <trd>cafard <klr tip="ind">(blatte)</klr></trd>,
      <trd>cancrelat</trd></trdgrp> 
    <trd lng="hu">cs&oacute;t&aacute;ny</trd>
    <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_k;&c_a;&c_n;</trd>
  </snc>
  <snc mrk="blat.0o.ic">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">KOMP</uzo>
    <fnt>SPIV</fnt>
    <dif>
      <ref cel="integr.0a.cirkvito">Integra cirkvito</ref>
      provizita je tega&jcirc;o kaj malgrandaj konektiloj por muntado.
    </dif>
    <trd lng="en">chip</trd>
    <trd lng="es">circuito integrado</trd>
    <trdgrp lng="fr">
      <trd>pastille<klr tip="ind"> (de circuit
int&eacute;gr&eacute;)</klr></trd>, 
      <trd>puce<klr tip="amb"> (&eacute;lectronique)</klr></trd></trdgrp>
    <trd lng="hu">chip</trd>
    <trd lng="ru">&#x447;&#x438;&#x43f;</trd>
  </snc>
</drv>
</art>
