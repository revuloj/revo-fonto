<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>blat</rad>/o
</kap>
<drv mrk="blat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el la ordo de insektoj, al kiu apartenas
      <tld/>o <sncref ref="blat.0o.ZOO"/>.
    </dif>
    <trd lng="de">Schabe</trd>
  </snc>
  <snc mrk="blat.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <tld/>edoj (<trd lng="la">Blatta</trd>),
      ofte vivanta en kuirejoj, bakejoj ktp.
    </dif>
    <trd lng="de">K&uuml;chenschabe</trd>
  </snc>
</drv>
</art>
