<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lakon.xml,v 1.16 2022/08/16 20:29:46 revo Exp $">
<kap>
  <rad>lakon</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lakon.0a">
  <kap>
    <tld/>a
  </kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lakon.0a.KONUNE">
    <uzo tip="klr">(pri stilo a&ubreve; homo)</uzo>
    <dif>
      Malmultvorta, konciza:
      <ekz>
        ni multe admiris la <tld/>econ de lia respondo;
      </ekz>
      <ekz>
        oni cetere povas observi iun <klr>[&#8230;]</klr> analogion inter la manifestoj
        ra&ubreve;mistaj, sennaciistaj kaj komunistaj; la tri estas skizaj, e&ccirc; <tld/>aj
        <fnt>
          <aut>D. Kessous</aut>
          <vrk><url ref="https://satesperanto.org/spip.php?article2269"
              >Ra&ubreve;mismo kaj novliberalismo</url></vrk>
          <lok>Sennaciulo, 2013-01/04 (1291-1294)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_o;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&kappa;&omega;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="de">
     <trd>lakonisch</trd>,
     <trd>kurz und b&uuml;ndig</trd>,
     <trd>knapp</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>laconic</trd>,
    <trd>brief</trd>,
    <trd>concise</trd>,
    <trd>sententious</trd>,
    <trd>succinct</trd>
  </trdgrp>
  <trd lng="es">lac&oacute;nico</trd>
  <trdgrp lng="hu">
     <trd>lakonikus</trd>,
     <trd>t&ouml;m&ouml;r</trd>
  </trdgrp>
  <trd lng="fr">laconique</trd>
  <trd lng="nl">laconiek</trd>
  <trd lng="pl">lakoniczny</trd>
  <trd lng="pt">lac&ocirc;nico</trd>
  <trd lng="ro">laconic</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_zh;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
