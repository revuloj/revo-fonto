<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lakon</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="lakon.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Malmultvorta, konciza (pp homo a&ubreve; stilo):
      <ekz>
        ni multe admiris la <tld/>econ de lia respondo.
      </ekz>
    </dif>
  </snc>
  <trd lng="el">&lambda;&alfa;&kappa;&omega;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="de">
     <trd>lakonisch</trd>,
     <trd>kurz und b&uuml;ndig</trd>,
     <trd>knapp</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>laconic</trd>,
    <trd>brief</trd>,
    <trd>concise</trd>,
    <trd>sententious</trd>,
    <trd>succinct</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>lakonikus</trd>,
     <trd>t&ouml;m&ouml;r</trd>
  </trdgrp>
  <trd lng="nl">laconiek</trd>
  <trd lng="pt">lac&ocirc;nico</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_zh;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
