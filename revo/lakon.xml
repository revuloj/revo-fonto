     <trd>knapp</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>laconic</trd>,
    <trd>brief</trd>,
    <trd>concise</trd>,
    <trd>sententious</trd>,
    <trd>succinct</trd>
  </trdgrp>
  <trd lng="es">lac&oacute;nico</trd>
  <trdgrp lng="hu">
     <trd>lakonikus</trd>,
     <trd>t&ouml;m&ouml;r</trd>
  </trdgrp>
  <trd lng="fr">laconique</trd>
  <trdgrp lng="ja">
    <trd>&#12521;&#12467;&#12491;&#12450;&#12398;</trd>,
