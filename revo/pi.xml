<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pi</rad>/a
</kap>
<drv mrk="pi.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Vive, sincere kaj ame respektoplena al Dio kaj religia&jcirc;oj:
      <ekz>
        <tld/>e vivi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se ezoko <tld/>i&gcirc;is, gobio ne dormu
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al la ombroj de &ccirc;iuj mortintaj esperantistoj la kongreso esprimas
        sian respekton kaj <tld/>an saluton<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>fromm</trd>,
    <trd>gottesf&uuml;rchtig</trd>
  </trdgrp>
  <trd lng="en">pious</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;vot</trd>,
    <trd>pieux</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&dalet;&vav;&qof;</trd>,
    <trd>&dalet;&tav;&yod;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>j&aacute;mbor</trd>,
    <trd>istenf&eacute;l&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vroom</trd>,
    <trd>godvruchtig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_ch;&c_je;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">from</trd>
</drv>

<drv mrk="pi.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Sincera, volonta praktikado de 
      <ref tip="malprt" cel="religi.0o">religio</ref>:
      <ekz>
        peli la <tld/>econ &gcirc;is bigoteco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;votion</trd>,
    <trd>pi&eacute;t&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_ch;&c_je;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="pi.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo kun <tld/>a spirito kaj konduto.
    </dif>
    <ref tip="vid" cel="religi.0ulo">religiulo</ref>
  </snc>
  <trd lng="fr">d&eacute;vot<klr> (subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_g;&c_o;&c_m;&c_o;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_o;&c_zh;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_sh;&c_a;</trd>
  </trdgrp>
</drv>
</art>
