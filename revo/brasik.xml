<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brasik.xml,v 1.56 2022/04/26 15:00:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brasik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="brasik.0o">
  <kap><tld/>o</kap>
  <snc mrk="brasik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="brasik1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Brassica</trd>):
      <ekz>
        <ind>blanka <tld/>o</ind>;
        <trd lng="br">kaol gwenn</trd>
        <trd lng="de">Wei&szlig;kraut</trd>
        <trd lng="es">col blanca</trd>
        <trd lng="fr">chou blanc</trd>
        <trd lng="nl">witte <ind>kool</ind></trd>
        <trdgrp lng="hu">
          <trd>feh&eacute;r <ind>k&aacute;poszta</ind></trd>,
          <trd>fejes <ind>k&aacute;poszta</ind></trd>
        </trdgrp>
        <trd lng="pl">bia&lstroke;a <ind>kapusta</ind></trd>
      </ekz>
      <ekz>
        <ind>ru&gcirc;a <tld/>o</ind>;
        <trd lng="br">kaol ruz</trd>
        <trd lng="de">Rotkraut</trd>
        <trd lng="es">lombarda</trd>
        <trd lng="fr">chou rouge</trd>
        <trd lng="nl">rode <ind>kool</ind></trd>
        <trd lng="hu">v&ouml;r&ouml;s <ind>k&aacute;poszta</ind></trd>
        <trd lng="pl">czerwona <ind>kapusta</ind></trd>
      </ekz>
      <ekz>
        &ccirc;ina <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2008/009624.php">Veneno frostigita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>krispa <tld/>o</ind>;
        <trd lng="br">kaol-dantek</trd>
        <trdgrp lng="de">
          <trd>Wirsingkohl</trd>,
          <trd>Welschkraut</trd>
        </trdgrp>
        <trd lng="hu">kelk&aacute;poszta</trd>
      </ekz>
      <ekz>
        <ind>tigo<tld/>o</ind>
        <klr>(kun dika tigo)</klr>.
        <trd lng="br">kaol-rabez</trd>
        <trd lng="de">Kohlrabi</trd>
        <trdgrp lng="es">
          <trd>col r&aacute;bano</trd>,
          <trd>colirr&aacute;bano</trd>
        </trdgrp>
        <trd lng="hu">karal&aacute;b&eacute;</trd>
        <trd lng="pl">kalarepa</trd>
      </ekz>
      <ekz>
        <ind>acida <tld/>o</ind>
        <klr>(fermentinta blanka <tld/>o)</klr>;
        <trd lng="br">kaol-go</trd>
        <trd lng="de">Sauerkraut</trd>
        <trdgrp lng="es">
          <trd>chucrut</trd>,
          <trd>col agria</trd>
        </trdgrp>
        <trd lng="fr">choucroute</trd>
        <trd lng="nl">zuurkool</trd>
        <trd lng="hu">savany&uacute; <ind>k&aacute;poszta</ind></trd>
        <trd lng="pl"><ind>kapusta</ind> kiszona</trd>
      </ekz>
      <ekz>
        unu kapo de <tld/>o aspektas tute simile al la alia
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        diversaj for&jcirc;eta&jcirc;oj: trunketoj de <tld/>o, balaa&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Gefian&ccirc;oj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la sablo floris malgranda mara <tld/>o
        <klr>(vd <ref cel="kramb.0o">krambo</ref>)</klr>
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la leporeto man&gcirc;is <tld/>on el iliaj manoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iuj koridoroj estas tia odoro de <tld/>o, ke oni devas gardi
        sian nazon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="nap.0o">napo</ref>,
       <ref cel="rap.0o">rapo</ref>,
       <ref cel="sabeli.0o">sabeliko (savoj<tld/>o)</ref> 
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
    <trd lng="bg">&c_z;&c_je;&c_l;&c_je;</trd>
    <trd lng="br">kaol(enn)</trd>
    <trd lng="cs">zel&iacute;</trd>
    <trd lng="de">Kohl</trd>
    <trd lng="es">col</trd>
    <trdgrp lng="en">
      <trd>brassica</trd>,
      <trd>cabbage</trd>
    </trdgrp>
    <trd lng="fr">chou</trd>
    <trd lng="hu">k&aacute;poszta</trd>
    <trd lng="id">kol</trd>
    <trd lng="nl">kool</trd>
    <trd lng="pl">kapusta</trd>
    <trd lng="pt">couve</trd>
    <trd lng="ro">varz&abreve;</trd>
    <trd lng="ru">&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
    <trd lng="sk">kapusta</trd>
    <trd lng="sv">k&aring;l</trd>
  </snc>
  <trd lng="bo">&#xF63;&#xF7C;&#xF44;&#xF0B;&#xF40;&#xF7C;&#xF62;&#xF0B;&#xF54;&#xF51;&#xF0B;&#xF5A;&#xF63;&#xF0B;</trd>
  <trd lng="he">&#x5DB;&#x5E8;&#x5D5;&#x5D1;</trd>
  <trd lng="uk">&#x43A;&#x430;&#x43F;&#x443;&#x441;&#x442;&#x430;</trd>
</drv>

<drv mrk="brasik.flor0o">
  <kap>flor<tld/>o</kap>
  <snc mrk="brasik.flor0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Varia&jcirc;o de
      <ref tip="super" cel="brasik.gxarden0o.BOT">&gcirc;arden<tld/>o</ref>
      (<trd lng="la">Brassica oleracea var. botrytis</trd>), kies
      florbur&gcirc;onoj formas densan kapon:
      <ekz>
        komisiite a&ccirc;eti flor<tld/>on, &scirc;i perdis la moneron, donitan al
        &scirc;i tiucele
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/009002.php">Memoroj neniam perdas sian
          mal&gcirc;ojon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mia edzino faras bongustegajn pladojn aldonante <klr>[&#x2026;]</klr>
        froma&gcirc;sa&ubreve;con (kun
        flor<tld/>o)
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2003/008020.html">Man&gcirc;u por sano,
          samideano!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="brokol.0o">brokolo</ref>
    <bld lok="&WCU;/thumb/0/08/Bloemkool.jpg/640px-Bloemkool.jpg" prm="CC-BY-SA-3.0">flor<tld/>o
        <fnt>
          <aut>Rasbak, propra verko</aut>
          <lok><url ref="&WCW;/File:Bloemkool.jpg">&WCW;/File:Bloemkool.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_ja;&c_t;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_f;&c_i;&c_o;&c_l;</trd>
  <trd lng="bo">&#xF58;&#xF7A;&#xF0B;&#xF4F;&#xF7C;&#xF42;&#xF0B;&#xF54;&#xF51;&#xF0B;&#xF5A;&#xF63;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>kaol-fleur</trd>,
    <trd>kaol-ble&ubreve;&ntilde;v</trd>,
    <trd>brikoli</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>karfiol</trd>,
    <trd>kv&ecaron;t&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Blumenkohl</trd>
  <trd lng="en">cauliflower</trd>
  <trd lng="es">coliflor</trd>
  <trd lng="fr">chou-fleur</trd>
  <trd lng="hi">&#x917;&#x94B;&#x92D;&#x940; [<ind>g&#x14D;bh&#x12B;</ind>]</trd>
  <trdgrp lng="hu">
    <trd>kelvir&aacute;g</trd>,
    <trd>karfiol</trd>
  </trdgrp>
  <trd lng="id"><ind>kembang</ind> kol</trd>
  <trd lng="nl">bloemkool</trd>
  <trd lng="pl">kalafior</trd>
  <trd lng="ro">conopid&abreve;</trd>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
  <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="sk">karfiol</trd>
  <trd lng="sv">blomk&aring;l</trd>
  <trd lng="uk">&#x446;&#x432;&#x456;&#x442;&#x43D;&#x430; &#x43A;&#x430;&#x43F;&#x443;&#x441;&#x442;&#x430;</trd>
</drv>

<drv mrk="brasik.gxarden0o">
  <kap>&gcirc;arden<tld/>o</kap>
  <snc mrk="brasik.gxarden0o.BOT">
    <dif>
      Specio de <ref tip="super" cel="brasik.0o.BOT"><tld/>o</ref>
      (<trd lng="la">Brassica oleracea</trd>), kies varia&jcirc;oj
      estas kulturataj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_a;&c_k;&c_a;&c_ch;&c_a;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="de">Gem&uuml;sekohl</trd>
  <trd lng="en">cabbage</trd>
  <trdgrp lng="es">
    <trd>col</trd>,
    <trd>repollo</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_o;&c_k;&c_o;&c_ch;&c_a;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="pl">kalafior</trd>
  <trd lng="sv">k&aring;l</trd>
  <trd lng="uk">&#x43A;&#x430;&#x43F;&#x443;&#x441;&#x442;&#x430; &#x433;&#x43E;&#x440;&#x43E;&#x434;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="brasik.burgxon0o">
  <kap>bur&gcirc;on<tld/>o, <var><kap>brusel<tld/>o</kap></var></kap>
  <snc mrk="brasik.burgxon0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Varia&jcirc;o de
      <ref tip="super" cel="brasik.gxarden0o.BOT">&gcirc;arden<tld/>o</ref>
      (<trd lng="la">Brassica oleracea var. gemmifera</trd>),
      kun man&gcirc;eblaj bur&gcirc;onoj:
      <ekz>
        bur&gcirc;on<tld/>o a&ubreve; brusel<tld/>o estis en 1821 por la unua fojo
        kultivita apud
        Bruselo, Belgio kaj rapide fari&gcirc;is grava vintrolegomo en E&ubreve;ropo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=259909">Bur&gcirc;onbrasiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/92/M&uuml;nster%2C_Domplatz%2C_Wochenmarkt_--_2019_--_2659.jpg/320px-M&uuml;nster%2C_Domplatz%2C_Wochenmarkt_--_2019_--_2659.jpg" prm="CC BY-SA 4.0">bur&gcirc;onoj de brusel<tld/>o, en bazaro de M&uuml;nster, Germanujo
        <fnt>
          <aut>Dietmar Rabich, propra verko</aut>
          <lok><url ref="&WCW;/File:M%C3%BCnster,_Domplatz,_Wochenmarkt_--_2019_--_2659.jpg">&WCW;/File:M&uuml;nster,_Domplatz,_Wochenmarkt_--_2019_--_2659.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_s;&c_e;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="br">kaol-Brusel</trd>
  <trd lng="de">Rosenkohl</trd>
  <trd lng="en">Brussels sprout</trd>
  <trd lng="es">col de Bruselas</trd>
  <trd lng="fr">chou de Bruxelles</trd>
  <trd lng="he">&#x5DB;&#x5E8;&#x5D5;&#x5D1; &#x5E0;&#x5D9;&#x5E6;&#x5E0;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
    <trd>bimb&oacute;skel</trd>,
    <trd>r&oacute;zsakel</trd>
  </trdgrp>
  <trd lng="nl">spruitje</trd>
  <trd lng="pl">brukselka</trd>
  <trd lng="ru">&c_b;&c_r;&c_ju;&c_s;&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="sv">brysselk&aring;l</trd>
  <trd lng="uk">&#x431;&#x440;&#x44E;&#x441;&#x435;&#x43B;&#x44C;&#x441;&#x44C;&#x43A;&#x430; &#x43A;&#x430;&#x43F;&#x443;&#x441;&#x442;&#x430;</trd>
</drv>

<drv mrk="brasik.tigo0o">
  <kap>tigo<tld/>o</kap>
  <snc mrk="brasik.tigo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Varia&jcirc;o de
      (<trd lng="la">Brassica oleracea var. gongylodes</trd>),
      kun mallonga karneca tigo tuberi&gcirc;inta iom super la grundo:
      <ekz>
        tigo<tld/>o estas dujara planto kiu apartenas al la brasika
        familio, &gcirc;i estis
        konata jam en antikva Romo sub la nomo &leftquot;<nac>caulorapa</nac>&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=107223">Tigobrasiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="brasik.gxarden0o">&gcirc;arden<tld/>o</ref>
    <ref tip="sin" cel="brasik.0rapo"><tld/>rapo</ref>
    <bld lok="&WCU;/thumb/6/6f/Koolrabi_(Brassica_oleracea_convar._acephala_alef._var._gongylodes).jpg/497px-Koolrabi_(Brassica_oleracea_convar._acephala_alef._var._gongylodes).jpg" prm="CC-BY-SA-3.0">tigo<tld/>o
        <fnt>
          <aut>Rasbak, propra verko</aut>
          <lok><url ref="&WCW;/File:Koolrabi_(Brassica_oleracea_convar._acephala_alef._var._gongylodes).jpg">&WCW;/File:Koolrabi_(Brassica_oleracea_convar._acephala_alef._var._gongylodes).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_r;&c_a;&c_b;&c_ib;</trd>
  <trd lng="de">Kohlrabi</trd>
  <trd lng="en">kohlrabi</trd>
  <trdgrp lng="es">
    <trd>col r&aacute;bano</trd>,
    <trd>colirr&aacute;bano</trd>
  </trdgrp>
  <trd lng="pl">kalarepa</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x44C;&#x440;&#x430;&#x431;&#x456;</trd>
</drv>

<drv mrk="brasik.0rapo">
  <kap><tld/>rapo</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="brasik.0rapo.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="brasik.tigo0o.BOT">tigobrasiko</ref>
    <rim mrk="brasik.0rapo.RIM_FNT">
      Evitinda la&ubreve; <fnt><bib>PIV1</bib></fnt> kaj
      <fnt><bib>PIV2</bib></fnt>.
      Tamen trovebla en aliaj vortaroj,
      ekzemple polaj (<fnt><aut>Antoni Grabowski</aut>
      <vrk>S&lstroke;ownik J&eogonek;zyka Esperanto</vrk>
      <lok>Nowe Wydawnictwo Polskie, Warszawa 1990, originale 1910</lok>
      </fnt>,
      <fnt>
      <aut>Tadeusz J. Michalski</aut>
      <vrk>S&lstroke;ownik esperancko-polski</vrk>
      <lok>Wiedza Powszechna, Warszawa, 1991</lok>
      </fnt>)
      kaj anglaj (<fnt><bib>WED</bib></fnt>, <fnt><bib>CEE</bib></fnt>).
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_r;&c_a;&c_b;&c_ib;</trd>
  <trd lng="cs">kedluben</trd>
  <trd lng="he">&#x5DB;&#x5E8;&#x5D5;&#x5D1;</trd>
  <trd lng="sk">kaler&aacute;b</trd>
</drv>

<drv mrk="brasik.acida_0o">
  <kap>
    acida <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="sauxrk.0o">Sa&ubreve;rkra&ubreve;to</ref>:
      <ekz>
        acida <tld/>o venas ver&scirc;ajne el norda &Ccirc;inio kaj tra mongolaj
        triboj al E&ubreve;ropo
        <klr>[&#x2026;]</klr>, kie mankis la vintra C-vitaminfonto el
        citracoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31881">Acida brasiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>,
    <trd>&c_k;&c_v;&c_a;&c_sh;&c_a;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Sauerkohl</trd>
  <trd lng="he">&#x5DB;&#x5E8;&#x5D5;&#x5D1; &#x5D7;&#x5DE;&#x5D5;&#x5E5;</trd>
</drv>


