<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brasik</rad>/o
</kap>
<drv mrk="brasik.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Legoma veg. el fam. de kruciferoj (<trd lng="la">Brassica</trd>). 
</dif>
</snc>
<trd lng="cs">zel&iacute;</trd>
<trd lng="de">Kohl</trd>
</drv>
<drv mrk="brasik.flor0o">
<kap>flor<tld/>o</kap>
<snc>
<dif>
Speco de <tld/>o kun densaj man&gcirc;eblaj floroj 
(<trd lng="la">Brassica botrytis</trd>)  
</dif>
<refgrp tip="sin">
<ref cel="brokol.0o">brokolo</ref>.
</refgrp>
</snc>
<trd lng="de">Blumenkohl</trd>
</drv>
<drv mrk="brasik.0napo">
<kap><tld/>napo</kap>
<snc>
<dif>
Speco de <tld/>o kun tubera man&gcirc;ebla radiko 
(<trd lng="la">Brassica napobrassica</trd>).  </dif>
</snc>
<trd lng="de">Kohlr&uuml;be</trd>
</drv>
<drv mrk="brasik.0rapo">
<kap><tld/>rapo</kap>
<snc>
<dif>
Speco de <tld/>o kun tubera man&gcirc;ebla trunko similanta rapon 
(<trd lng="la">Brassica rapa</trd>). 
</dif>
</snc>
<trd lng="de">wei&szlig;e R&uuml;be</trd>
</drv>
<drv mrk="brasik.burgxon0obrusel0o">
<kap>bur&gcirc;on<tld/>o, brusel<tld/>o</kap>
<snc>
<dif>
Speco de <tld/>o kun man&gcirc;eblaj bur&gcirc;onoj 
(<trd lng="la">Brassica gemmifera</trd>).   </dif>
</snc>
<trd lng="de">Rosenkohl</trd>
</drv>
</art>
