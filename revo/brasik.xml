<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brasik</rad>/o
</kap>
<drv mrk="brasik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="brasik.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Brassica</trd>):
      <ekz>
        blanka, ru&gcirc;a, acida <klr>(fermentinta blanka <tld/>o)</klr>,
        krispa <tld/>o;
      </ekz>
      <ekz>
        tigo<tld/>o <klr>(kun dika tigo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zel&iacute;</trd>
  <trd lng="de">Kohl</trd>
  <adm>Metu la ekzemplojn foje en proprajn deriva&jcirc;ojn, por ebligi tradukadon</adm>
</drv>
<drv mrk="brasik.flor0o">
  <kap>flor<tld/>o</kap>
  <snc>
    <dif>
      Varia&jcirc;o de
      <ref tip="super" cel="brasik.gxarden0o">&gcirc;arden<tld/>o</ref>
      (<trd lng="la">Brassica oleracea var. botrytis</trd>), kies
      florbur&gcirc;onoj formas densan kapon.
    </dif>
    <ref tip="sub" cel="brokol.0o">brokolo</ref>
  </snc>
  <trd lng="de">Blumenkohl</trd>
</drv>
<drv mrk="brasik.gxarden.0o">
  <kap>&gcirc;arden<tld/>o</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="brasik.0o"><tld/>o</ref>
      (<trd lng="la">Brassica oleracea</trd>), kies varia&jcirc;oj
      estas kulturataj.
    </dif>
  </snc>
</drv>
<drv mrk="brasik.0napo">
  <kap><tld/>napo</kap>
  <snc>
    <dif>
      Varia&jcirc;o de <ref tip="super" cel="nap.0o">napo</ref>
      (<trd lng="la">Brassica napus var. napobrassica</trd>),
      kun tubera man&gcirc;ebla radiko.
    </dif>
  </snc>
  <trd lng="de">Kohlr&uuml;be</trd>
  <adm>&Scirc;ovu al napo</adm>
</drv>
<drv mrk="brasik.burgxon0obrusel0o">
  <kap>bur&gcirc;on<tld/>o, brusel<tld/>o</kap>
  <snc>
    <dif>
      Varia&jcirc;o de
      <ref tip="super" cel="brasik.gxarden.0o">&gcirc;arden<tld/>o</ref>
      (<trd lng="la">Brassica oleracea var. gemmifera</trd>),
      kun man&gcirc;eblaj bur&gcirc;onoj.
    </dif>
  </snc>
  <trd lng="de">Rosenkohl</trd>
</drv>
</art>
