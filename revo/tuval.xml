<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tuval</rad>/o
</kap>

<drv mrk="tuval.0o">
  <kap>
    <tld lit="T"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="tuval.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sud<ref tip="vid" cel="pacifik.0o">pacifika</ref> insularo.
      Teritorio de &scirc;tato Tuvalo.
    </dif>
    <trdgrp lng="pl">
      <trd>Wyspy <ind>Lagunowe</ind></trd>,
      <trd>Wyspy <ind>Ellice</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="tuval.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la samnoma insularo kun &ccirc;efurbo Funufatio.
      ISO-3166 kodo: TV.
    </dif>
    <trdgrp lng="pl">
      <trd>Tuvalu</trd>,
      <trd>Wyspy <ind>Ellice</ind><klr> (przest.)</klr></trd>,
      <trd>Terytorium <ind>Tuvalu</ind><klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
</drv>

</art>

