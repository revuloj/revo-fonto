<art mrk="$Id: radiog.xml,v 1.15 2022/07/20 16:48:47 revo Exp $">
<kap>
  <rad>radiogram</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="radiog.0o">
  <kap><tld/>o</kap>
  <snc mrk="radiog.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Telegramo ekspedata per radiotelegrafio:
      <ekz>
        li trarigardis la <tld/>ojn alvenintajn
        dumnokte
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_jo;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="cs">radiogram</trd>
  <trd lng="de">Funktelegramm</trd>
  <trd lng="fr">radiogramme</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;di&oacute;s&uuml;rg&ouml;ny</trd>,
    <trd>r&aacute;di&oacute;telegram</trd></trdgrp>
  <trd lng="nl">radiogram</trd>
  <trd lng="pl">radiogram</trd>
  <trd lng="pt">radiograma</trd>
  <trd lng="ro">radiogram&abreve;</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
  <trd lng="sk">r&#xE1;diogram</trd>
  <trd lng="uk">&c_r;&c_a;&c_d;&c_ib;&#x43E;&c_g;&c_r;&c_a;&#x43C;&c_a;</trd>
</drv>
</art>
