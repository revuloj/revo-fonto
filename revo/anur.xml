<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anur.xml,v 1.8 2023/10/28 08:33:07 revo Exp $">
<kap>
  <rad>anur</rad>/oj <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="anur.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="anur.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="vost.sen0uloj">Senvostuloj</ref>:
      <ekz>
        ne estas klara scienca distingo inter ranoj kaj bufoj, unu familio de <tld/>oj,
        la diskolanguloj, havas ecojn de amba&ubreve;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=42664">Anuroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ja;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
    <trd lng="de">Froschlurche</trd>
    <trdgrp lng="zh">
    <trd>&#x65E0;&#x5C3E;&#x76EE; <pr>w&#xFA;w&#x11B;im&#xF9;</pr></trd>,
    <trd>&#x7121;&#x5C3E;&#x76EE; <pr>w&#xFA;w&#x11B;im&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">&zcaron;&aacute;by</trd>
  <trd lng="sk">bezchvost&eacute; oboj&zcaron;iveln&iacute;ky</trd>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_h;&c_v;&#x43E;&c_s;&c_t;&c_ib; &c_z;&c_je;&#x43C;&#x43D;&#x43E;&c_v;&#x43E;&c_d;&#x43D;&c_ib;</trd>
</drv>
</art>
