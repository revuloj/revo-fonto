<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: epizod.xml,v 1.22 2023/11/07 20:34:11 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>epizod</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="epizod.0o">
  <kap><tld/>o</kap>
  <snc mrk="epizod.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Akcesora agado, ne stre&ccirc;e ligita kun la &ccirc;efa temo de
      rakonto:
      <ekz>
        la ses lastaj libroj de Eneido estas plenaj je <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Nebenhandlung</trd>,
      <trd>Zwischenhandlung</trd>
    </trdgrp>
    <trd lng="pl">epizod</trd>
  </snc>
  <snc mrk="epizod.0o.KOMUNE">
    <dif>
      Aparta kurioza fakto en historia a&ubreve; grava okazinta&jcirc;o:
      <ekz>
        <tld/>oj de kongreso;
      </ekz>
      <ekz>
        kortu&scirc;a <tld/>o de la Revolucio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="incide.0o">incidento</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Zwischenfall</trd>,
      <trd>Episode</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>event</trd>,
      <trd>occurrence</trd>
    </trdgrp>
    <trd lng="pl">anegdota</trd>
    <trd lng="zh">&#x610F;&#x5916;&#x4E8B;&#x4EF6; <pr>y&#xEC;w&#xE0;ish&#xEC;ji&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_z;&c_o;&c_d;</trd>
  <trd lng="cs">epizoda</trd>
  <trd lng="en">episode</trd>
  <trd lng="fr">&eacute;pisode</trd>
  <trd lng="hu">epiz&oacute;d</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30D4;&#x30BD;&#x30FC;&#x30C9;  <pr>&#x3048;&#x3074;&#x305D;&#x30FC;&#x3069;</pr></trd>,
    <trd>&#x633F;&#x8A71;  <pr>&#x305D;&#x3046;&#x308F;</pr></trd>,
    <trd>&#x633F;&#x8A71;&#x7684;&#x306A;&#x51FA;&#x6765;&#x4E8B;  <pr>&#x305D;&#x3046;&#x308F;&#x3066;&#x304D;&#x306A;&#x3067;&#x304D;&#x3054;&#x3068;</pr></trd>,
    <trd>&#x9038;&#x8A71;  <pr>&#x3044;&#x3064;&#x308F;</pr></trd>
  </trdgrp>
  <trd lng="pt">epis&oacute;dio</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_z;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;beh</trd>,
    <trd>pr&iacute;hoda</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43F;&#x456;&#x437;&#x43E;&#x434;</trd>
</drv>

<drv mrk="epizod.0a">
  <kap><tld/>a</kap>
  <snc mrk="epizod.0a.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Mal&ccirc;efa, akcesora: 
      <ekz>
        <ind><tld/>a rolo</ind>, tasko, okaza&jcirc;o.
        <trd lng="hu">epiz&odblac;dszerep</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_z;&c_a;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>epizodick&yacute;</trd>,
    <trd>epizodn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fl&uuml;chtig</trd>,
    <trd>eingeschaltet</trd>,
    <trd>nebenher</trd>,
    <trd>episodisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>episodal</trd>,
    <trd>incidental</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accessoire<klr tip="ind"> (anecdotique)</klr></trd>,
    <trd>anecdotique</trd>,
    <trd>&eacute;pisodique<klr tip="ind">
    (accessoire)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>epiz&oacute;d-</trd>,
    <trd>epizodikus</trd>,
    <trd>mell&eacute;kes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x633F;&#x8A71;&#x7684;&#x306A;  <pr>&#x305D;&#x3046;&#x308F;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x4ED8;&#x968F;&#x7684;&#x306A;  <pr>&#x3075;&#x305A;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="pl">epizodyczny</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_z;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">epiz&oacute;dny</trd>
  <trd lng="uk">&#x435;&#x43F;&#x456;&#x437;&#x43E;&#x434;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
