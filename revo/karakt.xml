<drv mrk="karakt.0a">
  <kap><tld/>a</kap>
  <snc mrk="karakt.0a.propra">
    <dif>
      Propra, apartenanta al ies <tld/>o<sncref ref="karakt.0o.ecaro"/>:
      <ekz>
        kunmetado de vortoj estas <tld/>a eco de la lingvo germana
        <fnt><bib>LR</bib>,
        <lok>La Esperantisto, 1891, p. 23</lok></fnt>;
      </ekz>
      <ekz>
        tia konduto estas por li <tld/>a.
      </ekz>
    </dif>
  <trd lng="id"><baz>karakter</baz>berkarakter</trd>
  </snc>
  <snc mrk="karakt.0a.specifa">
    <dif>
      Specifa, prezentanta tre rimarkeblan, originalajn kaj
      frapajn trajtojn:
      <ekz>
        <tld/>a vesto;
      </ekz>
      <ekz>
        <tld/>a viza&gcirc;o;
      </ekz>
      <ekz>
        efektive la subjektivaj simptomoj pleje estas tiel
        <tld/>aj, ke el ili oni facile povas meti la diagnozon
        de kronika kataro de la konjunktivo
        <fnt>
        <aut>E. Fuchs, tr. &Z;</aut>:
        <vrk>Kronika katara konjunktivito</vrk>.
        <lok>Fundamenta Krestomatio</lok>.
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="id">
    <trd>khas</trd>,
    <trd>distingtif</trd>
  </trdgrp>
  </snc>
  <snc mrk="karakt.0a.homtipa">
    <dif>
      Propra al iu homgrupo, etno, socia medio, epoko; esprimanta
      iun mensan tipon:
      <ekz>
        <tld/>a danco;
      </ekz>
      <ekz>
        <ind><tld/>a rolo</ind>
        <klr>(en teatro, rolo prezentanta homtipon)</klr>.
        <trd lng="fr">r&ocirc;le de caract&egrave;re</trd>
        <trd lng="ru">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_n;&c_a;&c_ja;
        &c_r;&c_o;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
  <trd lng="id">karakteristik</trd>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_e;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">caracter&iacute;stic</trd>
  <trd lng="en">characteristic</trd>
  <trd lng="es">caracter&iacute;stica<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>caract&eacute;ristique<klr tip="ind"> (adj.)</klr></trd>,
    <trd>de <ind>caract&egrave;re</ind></trd>,
    <trd>distinctif</trd>,
    <trd>typique</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24615;&#26684;&#12398;  <pr>&#12379;&#12356;&#12363;&#12367;&#12398;</pr></trd>,
    <trd>&#20491;&#24615;&#30340;&#12394;  <pr>&#12371;&#12379;&#12356;&#12390;&#12365;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>karakter-</trd>,
    <trd>karakteristiek</trd>
  </trdgrp>
  <trd lng="pl">charakterowy</trd>
  <trd lng="ro">caracteristic&abreve;</trd>
  <trd lng="ru">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="zh">&#29305;&#26377; <pr>t&egrave;y&#466;u</pr></trd>
</drv>

<drv mrk="karakt.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="karakt.0a.senvola">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Senvola, cedema, facile subi&gcirc;anta al la opinioj kaj influoj
      de la aliaj, al kiu mankas propra
      <tld/>o<sncref ref="karakt.0o.PSI"/>:
      <ekz>
        mia patro estis tre ri&ccirc;a homo, natura aristokrato,
        bonege edukita, sed beda&ubreve;rinde sen<tld/>a
        <fnt>
        <aut>V. Devjatnin</aut>: <vrk>Arturo</vrk>.
        <lok>Fundamenta Krestomatio</lok>.
        </fnt>;
      </ekz>
      <ekz>
        certe vi, kiu legas tiujn &ccirc;i liniojn, ne apartenas al tiaj
        senpripensaj, facilanimaj kaj sen<tld/>aj homoj, kiuj
        turni&gcirc;as la&ubreve; la vento
        <fnt>
        <aut>H. Seppik</aut>: <vrk>La tuta Esperanto</vrk>.
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">characterless</trd>
    <trdgrp lng="fr">
      <trd>sans <ind>caract&egrave;re</ind><klr tip="ind"> (qui manque
de volont&eacute;)</klr></trd>,
      <trd>faible<klr tip="ind"> (qui manque de
      volont&eacute;)</klr></trd>,
      <trd>falot<klr tip="ind"> (adj.)</klr></trd>,
      <trd>mou<klr tip="ind"> (qui manque de volont&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="de">charakterlos</trd>
  <trdgrp lng="id">
    <trd>tanpa karakter</trd>,
    <trd>tidak berkarakter</trd>
  </trdgrp>
  </snc>
  <snc mrk="karakt.0a.senkolora">
    <dif>
      Al kiu mankas propraj trajtoj
      <tld/>aj<sncref ref="karakt.0a.specifa"/>,
      <refgrp tip="sin">
        <ref cel="banal.0a">banala</ref>,
        <ref cel="plat.0a">plata</ref>
      </refgrp>.
    </dif>
    <trdgrp lng="fr">
      <trd>sans <ind>caract&egrave;re</ind><klr tip="ind"> (qui manque
d'originalit&eacute;)</klr></trd>,
      <trd>incolore<klr tip="ind"> (qui manque
d'originalit&eacute;)</klr></trd>,
      <trd>insipide<klr tip="ind"> (qui manque
d'originalit&eacute;)</klr></trd>,
      <trd>p&acirc;le<klr tip="ind"> (qui manque
d'originalit&eacute;)</klr></trd>,
      <trd>plat<klr tip="ind"> (adj., qui manque
d'originalit&eacute;)</klr></trd>,
      <trd>terne<klr tip="ind"> (qui manque
      d'originalit&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="de">charakterlos</trd>
  <trd lng="en">insipid</trd>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>insuls</trd>,
    <trd>dessaborit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>insulso</trd>,
    <trd>falto de car&aacute;cter</trd>,
    <trd>desaborido</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#29305;&#24615;&#12398;&#28961;&#12356;  <pr>&#12392;&#12367;&#12379;&#12356;&#12398;&#12394;&#12356;</pr></trd>,
    <trd>&#24179;&#20961;&#12394;  <pr>&#12408;&#12356;&#12412;&#12435;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">karakterloos</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;aby</trd>,
    <trd>bez wyrazu</trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>slab</trd>,
    <trd>neexpresiv</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">bezcharaktern&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je; &c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je; &c_o;&c_r;&c_i;&c_gu;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
