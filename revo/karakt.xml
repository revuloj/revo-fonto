<drv mrk="karakt.skrib0ero">
  <kap>skrib<tld/>o</kap>
  <snc mrk="karakt.skrib0ero.mana">
    <dif>
      Ies persona stilo manskribi (precipe pri la aspekto de
      la literoj):
      <ekz>
        gracie &gcirc;i estis skribita, tio estis skrib<tld/>o de
        sinjorino
        <fnt><bib>Fab2</bib>,
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi konas la skriban <tld/>on de l&#8217; letero?
        &#8208; &gcirc;i estas mano de Hamleto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
         la skrib<tld/>o de mia frato!
          <fnt>
            <aut>&Scirc;ilero, tr. &Z;</aut>: <vrk>La rabistoj</vrk>.
            <lok><url
            ref="https://books.google.ru/books?id=Re_FBgAAQBAJ&amp;pg=PA23&amp;lpg=PA23&amp;dq=a+skribkaraktero+de+mia+frato&amp;source=bl&amp;ots=FMEQ_GQVyF&amp;sig=lhuaxdxditFVOOZku79ZTe1MK1Y&amp;hl=en&amp;sa=X&amp;ei=UaDuVM-jOam9ygPUhoKwDw&amp;ved=0CCIQ6AEwAA#v=onepage&amp;q&amp;f=false">1:2</url></lok>
          </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_ch;&c_y;&c_r;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_e;&c_r;&c_a; &c_p;&c_ib;&c_s;&c_mol;&c_m;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>charakter p&iacute;sma</trd>,
    <trd>rukopis</trd>,
    <trd>zp&uring;sob psan&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schreibweise</trd>
  </trdgrp>
  <trd lng="en">handwriting</trd>
  <trd lng="id">tulisan tangan</trd>
