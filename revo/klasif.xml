<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>klasifik</rad>/i
</kap>
<drv mrk="klasif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Aran&gcirc;i, grupigi la&ubreve; klasoj:
      <ekz>
        <tld/>i siajn notojn, kolekton;
      </ekz>
      <ekz>
        la agadon de la firmo oni povis <tld/>i sub tiu
        speco de skermado<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_a;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">klasifikovat</trd>
  <trd lng="de">klassifizieren</trd>
  <trdgrp lng="en">
    <trd>classify</trd>,
    <trd>reduce to order</trd>,
    <trd>sort out</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>classifier</trd>,
    <trd>arranger</trd>,
    <trd>cat&eacute;goriser</trd>,
    <trd>classer</trd>,
    <trd>ordonner</trd>,
    <trd>trier</trd>
    <trd>ranger</trd>
  </trdgrp>
  <trd lng="es">clasificar</trd>
  <trdgrp lng="hu">
    <trd>oszt&aacute;lyoz</trd>,
    <trd>kategoriz&aacute;l</trd>,
    <trd>rendez</trd>
  </trdgrp>
  <trd lng="nl">classificeren</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="klasif.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metoda disdonado la&ubreve; klasoj:
      <ekz>
        botanika, kemia <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_a;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Klassifizierung</trd>
  <trd lng="en">classification</trd>
  <trd lng="fr">classification</trd>
  <trd lng="es">clasificaci&oacute;n</trd>
  <trdgrp lng="hu">
    <trd>oszt&aacute;lyoz&aacute;s</trd>,
    <trd>kategoriz&aacute;l&aacute;s</trd>,
    <trd>rendez&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">classificatie</trd><trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
