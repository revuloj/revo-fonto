<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>klasifik</rad>/i
</kap>
<drv mrk="klasif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Aran&gcirc;i, grupigi la&ubreve; klasoj:
      <ekz><tld/>i siajn notojn, kolekton;</ekz>
      <ekz>
        la agadon de la firmo oni povis <tld/>i sub tiu
        speco de skermado...<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>classify</trd>,
    <trd>reduce to order</trd>,
    <trd>sort out</trd>
  </trdgrp>
  <trd lng="cs">klasifikovat</trd>
   <trd lng="nl">classificeren</trd>
<trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="klasif.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metoda disdonado la&ubreve; klasoj:
      <ekz>botanika, kemia <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="en">classification</trd>
   <trd lng="nl">classificatie</trd><trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
