<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>antarkt</rad>/a<fnt>Z</fnt>
</kap>
<drv mrk="antark.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Sudpolusa.
    </dif>
  </snc>
  <trd lng="de">antarktisch</trd>
  <trd lng="en">antarctic</trd>
  <trd lng="fr">antarctique</trd>
  <trd lng="nl">antarctisch</trd>
  <trd
lng="ru">&#1072;&#1085;&#1090;&#1072;&#1088;&#1082;&#1090;&#1080;&#1095;&#1077;&#1089;&#1082;&#1080;&#1081;</trd>
</drv>
<drv mrk="antark.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La <tld/>a regiono.
    </dif>
  </snc>
  <trd lng="de">Antarktis</trd>
  <trd lng="nl">Antarctis</trd>
</drv>
<drv mrk="antark.A0o">
  <kap><tld lit="A"/>o, <tld lit="A"/>ido</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La <ref tip="super" cel="kontin.0o.terkuno">kontinento</ref>
      situanta &ccirc;e la suda poluso.
    </dif>
  </snc>
  <rim>
    Oni konsideras <tld lit="A"/>on anka&ubreve; sesa mondoparto.
  </rim>
  <trd lng="de">Antarktika</trd>
  <trd lng="en">Antarctica</trd>
  <trd lng="nl">Antarctica</trd>
</drv>
<drv mrk="antark.A0aOceano">
  <kap><tld lit="A"/>a Oceano</kap>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      &ccirc;irka&ubreve; la <tld/>a kontinento.
    </dif>
  </snc>
  <trd lng="de">Antarktischer Ozean</trd>
  <trdgrp lng="en">
    <trd>Antarctic Ocean</trd>,
    <trd>Southern Ocean</trd>
  </trdgrp>
  <trd lng="nl">Antarctische Oceaan</trd>
</drv>
</art>
