<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>impoz</rad>/i
</kap>
<drv mrk="impoz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Aran&gcirc;i tiamaniere la diversajn enpa&gcirc;igitajn 
      komposta&jcirc;ojn de folio,
      ke ili estu la&ubreve;vice, kiam la folio estas faldita.  
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>ausschie&szlig;en</trd>,
     <trd>umbrechen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>effectuer l'<ind>imposition</ind><klr tip="ind"> (typo.)</klr></trd>,
    <trd>imposer<klr tip="ind"> (typo.)</klr></trd></trdgrp> 
  <trd lng="hu">kil&odblac;<klr tip="ind"> (nyomd&aacute;szat)</klr></trd>
  <trd lng="nl">opmaken</trd>
  <trd lng="pt">impor<klr tip="amb"> (livros, tipografia)</klr></trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
