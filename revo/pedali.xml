<art mrk="$Id$">
<kap>
<rad>pedaliac</rad>/oj
</kap>
<drv mrk="pedali.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Pedaliaceae</trd>), ekz.
      <ref tip="sub" cel="sezam.0o">sezamo</ref>.
