<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: malvac.xml,v 1.18 2024/01/25 20:10:33 revo Exp $">
<kap>
  <rad>malvac</rad>/oj
</kap>
<drv mrk="malvac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="malvac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Malvaceae</trd>):
      <ekz>
        <tld/>oj estas arboj (tropike) a&ubreve; moltigaj plantoj
        (&gcirc;enerala en la kontinenta klimato), kun segmentumitaj folioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24379">Malvacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        abutilo estas genro el la familio de la <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=298842">Abutilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="malv.0o">malvo</ref>,
      <ref cel="hibisk.0o">hibisko</ref>,
      <ref cel="kaka.0ujo">kakaujo</ref>,
      <ref cel="kola.0ujo">kolaujo</ref>,
      <ref cel="sterku1.0o">sterkulio</ref>,
      <ref cel="duri.0arbo">duriujo</ref>,
      <ref cel="alte.0o">alteo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_l;&c_je;&c_z;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">malv&agrave;cies</trd>
  <trd lng="cs">sl&eacute;zovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Malvengew&auml;chse</trd>
  <trd lng="en">mallows</trd>
  <trd lng="fr">malvac&eacute;es</trd>
  <trd lng="hu">m&aacute;lyvaf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30A2;&#x30AA;&#x30A4;&#x79D1;  <pr>&#x30A2;&#x30AA;&#x30A4;&#x304B;</pr></trd>
  <trd lng="nl">kaasjeskruidfamilie</trd>
  <trd lng="pl">&sacute;lazowate</trd>
  <trd lng="pt">malv&aacute;ceas</trd>
  <trd lng="ro">malvaceae</trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">slezovit&eacute;</trd>
  <trd lng="uk">&#x43C;&#x430;&#x43B;&#x44C;&#x432;&#x43E;&#x432;&#x456;</trd>
  <trdgrp lng="zh">
    <trd>&#x9326;&#x8475;&#x79D1; <pr>j&#x1D0;nku&#xED;k&#x113;</pr></trd>,
    <trd>&#x9526;&#x8475;&#x79D1; <pr>j&#x1D0;nku&#xED;k&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
