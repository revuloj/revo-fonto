<art mrk="$Id: seksologi.xml,v 1.9 2024/03/24 20:00:00 revo Exp $">
<kap>
  <rad>seksologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="seksologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="seksologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Studo de homaj kondutoj kaj problemoj rilataj al sekso:
      <ekz>
        <tld/>o estas la la&ubreve;sistema studado de homa sekseco
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Seksologio">seksologio</url>,
        2010-01-25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_s;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">sexuologie</trd>
  <trd lng="en">sexology</trd>
  <trd lng="fr">sexologie</trd>
  <trd lng="pl">seksuologia</trd>
  <trd lng="sk">sexuol&oacute;gia</trd>
  <trd lng="uk">&c_s;&c_je;&#x43A;&c_s;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x6027;&#x5B66; <pr>x&#xEC;ngxu&#xE9;</pr></trd>,
    <trd>&#x6027;&#x5B78; <pr>x&#xEC;ngxu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="seksologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="seksologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="seksologi.0o.MED"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_s;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="en">sexologist</trd>
  <trd lng="fr">sexologue</trd>
  <trd lng="pl">seksuolog</trd>
</drv>
</art>

