<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>seksologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="seksologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="seksologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Studo de homaj kondutoj kaj problemoj rilataj al sekso:
      <ekz>
        <tld/>o estas la la&ubreve;sistema studado de homa sekseco
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Seksologio">seksologio</url>, 
        2010-01-25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sexologie</trd>
</drv>

<drv mrk="seksologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="seksologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="seksologi.0o.MED"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="fr">sexologue</trd>
</drv>
</art>

