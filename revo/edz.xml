<drv mrk="edz.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="edz.0igxo.lauxlegxa">
    <dif>
      La&ubreve;le&gcirc;a ligo de du homoj:
      <ekz>
        <tld/>i&gcirc;o la&ubreve; koro, la&ubreve; la kvanto da oro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li faris al la re&gcirc;idino <tld/>i&gcirc;opeton, kaj &scirc;i tuj diris jes
        (<ref tip="vid" cel="man.0o">mano</ref>)
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;o pro amo flamanta al sako sonanta
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fian&ccirc;i&gcirc;o ne estas <tld/>i&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        de <tld/>i&gcirc;o tro malfrua orfoj naski&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        malsamranga <tld/>i&gcirc;o
        (<ref tip="sin" cel="mezali.0o">mezalianco</ref>);
      </ekz>
      <ekz>
        <ind><tld/>i&gcirc;ofesto</ind>
        <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
        <trd lng="en">wedding</trd>
        <trd lng="fr">noce</trd>
        <trd lng="es">fiesta de bodas</trd>
        <trd lng="hu">h&aacute;zasod&aacute;s</trd>
        <trd lng="id">[<ind>nikah</ind>] pernikahan</trd>
        <trd lng="nl">huwelijksfeest</trd>
        <trd lng="pl">wesele</trd>
        <trd lng="ru">&c_s;&c_v;&c_a;&c_d;&c_mol;&c_b;&c_a;</trd>
        <trd lng="tr">d&uuml;&gbreve;&uuml;n</trd>
        <fnt><bib>MT</bib>, <lok>&Alt; 3:11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="edz.0igxo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Sakramento, kiu sanktigas la unuigon de du homoj kaj havigas al
      ili dian helpon por kristane vivi.
    </dif>
    <ref tip="sin" cel="matrim.0o.edzigxo">matrimonio</ref>
    <trd lng="en">Holy Matrimony</trd>
    <trd lng="id">[<ind>nikah</ind>] Pernikahan Kudus</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Heirat</trd>,
    <trd>Verm&auml;hlung</trd>
  </trdgrp>
  <trd lng="en">Holy Matrimony</trd>
  <trdgrp lng="fr">
    <trd>mariage</trd>,
    <trd>noces<klr tip="ind"> (mariage)</klr></trd>
  </trdgrp>
  <trd lng="hu">h&aacute;zass&aacute;g<klr tip="ind"> 
  (szents&eacute;g)</klr></trd>
  <trd lng="nl">huwelijk</trd>
  <trd lng="pl">&sacute;lub</trd>
  <trdgrp lng="es">
    <trd>casamiento (desde la perspectiva del hombre)</trd>,
    <trd>hacerse esposo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je; &c_v; &c_b;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;&c_o;&c_s;&c_o;&c_ch;&c_je;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0eco">
  <kap><ofc>*</ofc><tld/>eco<fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5013">edz'ec'i</url></lok></fnt></kap>
  <snc mrk="edz.0eco.stato">
    <dif>
      Stato de ge<tld/>oj:
      <ekz>
        <klr>[la]</klr> ligo de <tld/>eco
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta nia <tld/>eco estis neinterrompita dancado
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bela junulino trovi&gcirc;is en la plena a&ubreve;reolo de la <tld/>eco
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la formo lin senprudentigis, li rigardis la &scirc;elon kaj ne la
        kernon, kaj tio
        estas <klr>[&#8230;]</klr> granda malfeli&ccirc;o en la <tld/>eco
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ju;&c_b;</trd>
  <trd lng="cs">man&zcaron;elstv&iacute;</trd>
  <trd lng="de">Ehe</trd>
  <trd lng="en">marriage</trd>
  <trd lng="es">matrimonio (estado)</trd>
  <trdgrp lng="fr">
    <trd>mariage<klr> (&eacute;tat))</klr></trd>,
    <trd>union<klr> (conjugale)</klr></trd>
  </trdgrp>
  <trd lng="hu">h&aacute;zass&aacute;g<klr tip="ind"> 
  (&aacute;llapot)</klr></trd>
  <trd lng="pl">ma&lstroke;&zdot;e&nacute;stwo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">man&zcaron;elstvo</trd>
</drv>

<drv mrk="edz.0eca">
  <kap><tld/>eca</kap>
  <snc mrk="edz.0eca.rilata">
    <dif>
      Rilata al <tld/>eco:
      <ekz>
        la <tld/>eca &jcirc;uro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Stella</nom>, vi estis do la serpento en mia <tld/>eca
        paradizo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        <tld/>eca <klr>(la&ubreve;le&gcirc;a)</klr> infano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>
  <trd lng="cs">man&zcaron;elsk&yacute; <klr>(d&iacute;t&ecaron;)</klr></trd>
  <trd lng="de">ehelich</trd>
  <trd lng="en">marital</trd>
  <trd lng="es">matrimonial</trd>
  <trd lng="fr">conjugal</trd>
  <trd lng="hu">h&aacute;zass&aacute;gi</trd>
  <trd lng="nl">huwelijks-</trd>
  <trdgrp lng="pl">
    <trd>ma&lstroke;&zdot;e&nacute;ski</trd>,
    <trd>matrymonialny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_k;&c_i;&c_j;
    </trd>,
    <trd>&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">man&zcaron;elsk&eacute;</trd>
</drv>

<drv mrk="edz.ge0oj">
  <kap>ge<tld/>oj</kap>
  <snc mrk="edz.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="E"/>o(j) kaj <tld/>ino(j):
      <ekz>
        ili estas <tld/>o kaj <tld/>ino (ge<tld/>oj);
      </ekz>
      <ekz>
        se ge<tld/>oj sin batas, fremdulo restu flanke
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la maljunaj ge<tld/>oj sidis &ccirc;e sia vesperman&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        ge<tld/>a paro
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007013.html">A&ubreve;strio:
          Ekspozicio de famaj artistoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        devoj kaj rajtoj de la ge<tld/>a stato
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007192.html">Edzi&gcirc;oj
          anka&ubreve; por samseksaj paroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">husbands and wifes</trd>
    <trd lng="io">gespozi</trd>
  </snc>
<trd lng="de">Eheleute</trd>
</drv>

<drv mrk="edz.ge0o">
  <kap>ge<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="edz.ge0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Homo la&ubreve;le&gcirc;e ligita kun alia homo por kunvivado kaj fondo de
      familio; <ref tip="sin" cel="spoz.0o.GEN">spozo</ref>:
      <ekz>
        la unua afero, kiun faras &ccirc;iu sklavino kiam &scirc;i ali&gcirc;as al nova
        domanaro, estas informi&gcirc;i, kiu estas ge<tld/>o de kiu
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &Scirc;tona Urbo</vrk>
          <lok>jaro 1999a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel perdo de ge<tld/>o pro morto
        <fnt>
          <vrk>Aflikto</vrk>
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Aflikto">Vikipedio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;i tiu uzo devias de la tradicia uzo, kaj ne &ccirc;iuj esperantistoj
      akceptas &gcirc;in. Vidu la <ref cel="ge.0.ununombro">duan sencon de
      ge-</ref>.
    </rim>
  </snc>
  <trd lng="da">&aelig;gtef&aelig;lle</trd>
  <trd lng="en">spouse</trd>
  <trd lng="fa">&a_h;&a_m;&a_s;&a_r;</trd>
  <trdgrp lng="fi">
    <trd>puoliso</trd>,
    <trd>aviopuoliso</trd>
  </trdgrp>
  <trdgrp lng="fy">
    <trd>houlikspartner</trd>,
    <trd>oarehelte</trd>
  </trdgrp>
  <trd lng="hu">h&aacute;zast&aacute;rs</trd>
  <trd lng="io">spozo</trd>
  <trd lng="ko">&#48176;&#50864;&#51088;</trd>
  <trdgrp lng="nl">
    <trd>huwelijkspartner</trd>,
    <trd>echtgenoot</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>ektefelle</trd>,
    <trd>ektemake</trd>
  </trdgrp>
  <trd lng="tr">e&scedil;</trd>
  <trd lng="vo">matan</trd>
</drv>

<drv mrk="edz.ge0igxi">
  <kap>ge<tld/>i&gcirc;i,
  <var><kap>kun<tld/>i&gcirc;i</kap></var>,
  <var><kap>inter<tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="edz.ge0igxi.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      I&gcirc;i ge<tld/>oj:
      <ekz>
        &scirc;i ne volas inter<tld/>i&gcirc;i kun la enuiga talpo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        1105 paroj ge<tld/>i&gcirc;is sub &scirc;ia svatado
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato1/monato-9309-noticoj.txt">Esperanto
          sen peno</url></vrk>
          <lok>93/09</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne gravas kiom malproksimaj ili estas, <klr>[&#8230;]</klr> fine 
        ili havas la &scirc;ancon ge<tld/>i&gcirc;i
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 57a, p. 270a
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">heiraten</trd>
  <trd lng="en">get married</trd>
  <trdgrp lng="es">
    <trd>casarse (desde la perspectiva de la pareja)</trd>,
    <trd>hacerse esposos</trd>
  </trdgrp>
  <trd lng="fr">se <ind>marier</ind></trd>
  <trd lng="hu">&ouml;sszeh&aacute;zasodik</trd>
  <trd lng="id">[<ind>nikah</ind>] menikah</trd>
  <trd lng="nl">huwen</trd>
  <trdgrp lng="pl">
    <trd>pobiera&cacute; si&eogonek;</trd>,
    <trd>bra&cacute; &sacute;lub</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="edz.eks0igxi">
