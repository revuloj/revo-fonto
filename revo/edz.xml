  <trd lng="fr">c&eacute;libat</trd>
  <trd lng="id">selibat</trd>
  <trd lng="nl">celibaat</trd>
  <trdgrp lng="pl">
    <trd>bez&zdot;e&nacute;stwo</trd>,
    <trd>celibat</trd>,
    <trd><klr>(staro)</klr>panie&nacute;stwo</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_b;&c_r;&c_a;&c_ch;&c_i;&c_je;</trd>
  <trd lng="es">solter&iacute;a</trd>
</drv>
