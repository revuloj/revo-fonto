<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>edz</rad>/o
    </kap>

    <drv mrk="edz.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
        <dif>
          Viro la&ubreve;le&gcirc;e ligita kun virino por kunvivado kaj fondo de
          familio:
          <ekz>
            sed al sia <tld/>o Nabal &scirc;i nenion diris
            <fnt><bib>MT</bib>, <lok>&Sa1; 25:19</lok></fnt>.</ekz>
        </dif>
      </snc>
      <trd lng="be">&c_m;&c_u;&c_zh;</trd>
      <trd lng="cs">man&zcaron;el</trd>
      <trdgrp lng="de">
        <trd>Ehemann</trd>,
        <trd>Gatte</trd>,
        <trd>Gemahl</trd>,
        <trd>Mann</trd>
      </trdgrp>
      <trd lng="en">husband</trd>
      <trdgrp lng="fr">
        <trd>&eacute;poux<klr tip="ind"> (mari)</klr></trd>,
        <trd>mari</trd>
      </trdgrp>
      <trd lng="hu">f&eacute;rj</trd>
      <trdgrp lng="nl">
        <trd>echtgenoot</trd>,
        <trd>man<klr tip="ind"> (echtgenoot)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_m;&c_u;&c_zh;</trd>,
        <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_g;</trd>
      </trdgrp>
      <trd lng="tp">mije</trd>
    </drv>

    <drv mrk="edz.0ino">
      <kap><tld/>ino</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
        <dif>
          Virino la&ubreve;le&gcirc;e ligita kun viro por kunvivado kaj fondo de
          familio:
          <ekz>
            Re&hcirc;abeam prenis al si kiel <tld/>inon Ma&hcirc;alaton
            <fnt><bib>MT</bib>, <lok>&Kr2; 11:18</lok></fnt>;
          </ekz><ekz>
            <tld/>o kaj <tld/>ino estas ge<tld/>oj.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_zh;&c_o;&c_n;&c_k;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Ehefrau</trd>,
        <trd>Gattin</trd>,
        <trd>Gemahlin</trd>,
        <trd>Frau</trd>
      </trdgrp>
      <trd lng="en">wife</trd>
      <trdgrp lng="fr">
        <trd>&eacute;pouse</trd>,
        <trd>femme<klr tip="ind"> (&eacute;pouse)</klr></trd>
      </trdgrp>
      <trd lng="hu">feles&eacute;g</trd>
      <trdgrp lng="nl">
        <trd>echtgenote</trd>,
        <trd>vrouw<klr tip="ind"> (echtgenote)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_zh;&c_je;&c_n;&c_a;</trd>,
        <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_g;&c_a;</trd>
      </trdgrp>
      <trd lng="tp">meli</trd>
    </drv>

    <drv mrk="edz.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Fari iun <tld/>o per la&ubreve;le&gcirc;a a&ubreve; religia ligo:
          <ekz>
            la gepatroj <tld/>igis la filon<fnt>B</fnt>;
          </ekz><ekz>
            se li estas <tld/>igita, tiam lia <tld/>ino
            eliru kune kun li
            <fnt><bib>MT</bib>, <lok>&Eli; 21:3</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_u;&c_z;&c_ja;&c_c;&c_mol; &c_z;&c_a; &c_m;&c_u;&c_zh;&c_a;</trd>
        <trdgrp lng="fr">
          <trd>marier<klr tip="ind"> (prendre en mariage)</klr></trd>,
          <trd>&eacute;pouser</trd>,
          <trd>prendre en <ind>mariage</ind></trd>
        </trdgrp>
        <trd lng="de">heiraten</trd>
        <trd lng="hu">elvesz<klr tip="ind"> (f&eacute;rj&uuml;l/feles&eacute;g&uuml;l)</klr></trd>
      </snc>
      <snc>
        <dif>
          Doni kiel <tld/>on:
          <ekz>
            la familio volis <tld/>igi lin al unu kuzino, kies
            firma karaktero estis bone konata
            <fnt>
              <aut>Gaston WARINGHIEN</aut>: <vrk>Pa&ubreve;lo Verlaine</vrk>.
              <lok>La nica literatura revuo, 3:2</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
        <trdgrp lng="de">
          <trd>verheiraten</trd>,
          <trd>verm&auml;hlen</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>marier<klr tip="ind"> (donner en mariage)</klr></trd>,
          <trd>donner en <ind>mariage</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>megh&aacute;zas&iacute;t<klr tip="ind"> (f&eacute;rfit/n&odblac;t)</klr></trd>,
          <trd>hozz&aacute;ad<klr tip="ind"> (f&eacute;rj&uuml;l/n&odblac;&uuml;l)</klr></trd>
        </trdgrp>
      </snc>
      <trd lng="en">marry</trd>
      <trd lng="nl">uithuwelijken</trd>
      <trd lng="ru">&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="edz.0iga">
      <kap><tld/>iga</kap>
      <snc>
        <dif>
          Rilata al <tld/>igo:
          <ekz>
            <tld/>iga promeso<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="edz.0iga.geedzigxa">
        <uzo tip="stl">EVI</uzo>
        <dif>
          <ref tip="sin" cel="edz.ge0igxi">Ge<tld/>i&gcirc;a</ref>:
          <ekz>
            <tld/>iga ceremonio<fnt>Z</fnt>,
            formulo<fnt>Z</fnt>, ringo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="edz.0inigi">
      <kap><tld/>inigi</kap>
      <snc mrk="edz.0inigi.doni">
        <dif>
          Fari iun ies <tld/>ino per la&ubreve;le&gcirc;a a&ubreve; religia ligo:
          <ekz>
            tial la onklino konstante paroladis: al Eminjo mi
            donos bonan edukon kaj poste mi &scirc;in bone <tld/>inigos...
            <fnt><bib>Marta</bib></fnt>;
          </ekz><ekz>
            &scirc;iaj gepatroj volis &scirc;in <tld/>inigi kun alia persono
            <fnt><bib>F</bib>, <lok>&FE;39.</lok></fnt>;
          </ekz><ekz>
            li havis tridek filojn, kaj tridek filinojn li
            <tld/>inigis eksteren, kaj tridek filinojn li prenis
            el ekstere por siaj filoj
            <fnt><bib>MT</bib>, <lok>&Jug; 12:9</lok></fnt>;
          </ekz><ekz>
            prenu edzinojn por viaj filoj kaj <tld/>inigu viajn filinojn,
            por ke ili nasku filojn kaj filinojn
	    <fnt><bib>MT</bib>, <lok>&Jer; 29:6</lok></fnt>;
          </ekz><ekz>
            Temistoklo <tld/>inigis sian filinon je homo tre bona,
            sed malri&ccirc;a <fnt><bib>FK</bib></fnt>;
          </ekz><ekz>
            <ctl>Kaj la plej alta konsilantaro de la tiraj pastroj?...</ctl>
            &dash; &leftquot;&Gcirc;i povas nur <tld/>inigi min...&rightquot;
	    <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 9a.</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_v;&c_y;&c_d;&c_a;&c_c;&c_mol; &c_z;&c_a;&c_m;&c_u;&c_zh;</trd>
        <trdgrp lng="de">
          <trd>verheiraten</trd>,
          <trd>verm&auml;hlen</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>marier<klr tip="ind"> (donner en mariage)</klr></trd>,
          <trd>donner en <ind>mariage</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>megh&aacute;zas&iacute;t<klr tip="ind"> (f&eacute;rfit/n&odblac;t)</klr></trd>,
          <trd>hozz&aacute;ad<klr tip="ind"> (f&eacute;rj&uuml;l/n&odblac;&uuml;l)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_y;&c_d;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_m;&c_u;&c_zh;</trd>
      </snc>
      <snc mrk="edz.0inigi.preni">
        <dif>
          <tld lit="E"/>i&gcirc;i al iu:
          <ekz>
            donu mian edzinon Mi&hcirc;al, kiun mi <tld/>inigis al mi per
            cent prepucioj de Fili&scirc;toj
            <fnt><bib>MT</bib>, <lok>&Sa2; 3:14</lok></fnt>;
          </ekz><ekz>
            memkompreneble mi <tld/>inigis &scirc;in al mi
	    <fnt>
              <aut>Szathm&aacute;ri</aut>: <vrk>Tria pre&gcirc;o de Pygmalion</vrk>
            </fnt>;
          </ekz><ekz>
            mi <tld/>inigis al mi tri re&gcirc;idinojn de orientaj regnoj
            <fnt>
              <aut>Karel &Ccaron;apek, tr. Josef Vondrou&scaron;ek</aut>:
              <vrk>Aleksandro la Granda</vrk>,
              <lok>Libro de apokrifoj</lok>
            </fnt>.
          </ekz>
        </dif>
        <rim>
          Tia uzo postulas malimplican refleksivan komplementon:
          <tld/>inigi iun <em>al si</em> kaj povas havi aktivan,
          eventuale e&ccirc; iom perfortan nuancon.  Pli normala dirmaniero
          estas la pli simpla &leftquot;<tld/>i&gcirc;i al iu&rightquot;.
        </rim>
        <trd lng="be">&c_u;&c_z;&c_ja;&c_c;&c_mol; &c_z;&c_a; &c_zh;&c_o;&c_n;&c_k;&c_u;</trd>
        <trdgrp lng="fr">
          <trd>marier<klr tip="ind"> (prendre en mariage)</klr></trd>,
          <trd>&eacute;pouser</trd>,
          <trd>prendre en <ind>mariage</ind></trd>
        </trdgrp>
        <trd lng="de">heiraten</trd>
        <trd lng="hu">elvesz<klr tip="ind"> (f&eacute;rj&uuml;l/feles&eacute;g&uuml;l)</klr></trd>
      </snc>
      <trd lng="en">marry</trd>
      <trd lng="nl">uithuwelijken</trd>
      <trd lng="ru">&c_v;&c_z;&c_ja;&c_t;&c_mol; &c_v; &c_zh;&c_jo;&c_n;&c_y;</trd>
    </drv>

    <drv mrk="edz.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <uzo tip="klr">(kun, al)</uzo>
      <snc>
        <dif>
          I&gcirc;i <tld/>o:
          <ekz>
            la re&gcirc;ido <klr>...</klr> forkondukis &scirc;in
            al la palaco de sia patro <klr>...</klr>, kie li <tld/>i&gcirc;is
            je &scirc;i
            <fnt><bib>F</bib>, <lok>&FE; 23</lok></fnt>.</ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_p;&c_a;&c_b;&c_r;&c_a;&c_c;&c_c;&c_a; <klr>(&c_z;
            &c_k;&c_ib;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
      </trdgrp>
      <trd lng="de">heiraten</trd>
      <trd lng="en">get married</trd>
      <trdgrp lng="fr">
        <trd>&eacute;pouser</trd>,
        <trd>se <ind>marier</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>hozz&aacute;megy<klr tip="ind"> (f&eacute;rj&uuml;l/n&odblac;&uuml;l)</klr></trd>,
        <trd>megn&odblac;s&uuml;l</trd>,
        <trd>f&eacute;rjhez megy</trd>,
        <trd>megh&aacute;zasodik</trd>,
      </trdgrp>
      <trdgrp lng="nl">
        <trd>huwen</trd>,
        <trd>trouwen</trd>
      </trdgrp>
      <trd lng="ru">&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="edz.0inigxi">
      <kap><tld/>ini&gcirc;i</kap>
      <uzo tip="klr">(kun, al)</uzo>
      <snc>
        <dif>
          I&gcirc;i <tld/>ino:
          <ekz>
            &scirc;i <tld/>ini&gcirc;is kun sia kuzo
            <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>.</ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_z;&c_a;&c_m;&c_u;&c_zh;</trd>,
        <trd>&c_p;&c_a;&c_b;&c_r;&c_a;&c_c;&c_c;&c_a; <klr>(&c_z;
            &c_k;&c_ib;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
      </trdgrp>
      <trd lng="de">heiraten</trd>
      <trd lng="en">get married</trd>
      <trdgrp lng="fr">
        <trd>&eacute;pouser</trd>,
        <trd>se <ind>marier</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>hozz&aacute;megy<klr tip="ind"> (f&eacute;rj&uuml;l/n&odblac;&uuml;l)</klr></trd>,
        <trd>megn&odblac;s&uuml;l</trd>,
        <trd>f&eacute;rjhez megy</trd>,
        <trd>megh&aacute;zasodik</trd>,
      </trdgrp>
      <trdgrp lng="nl">
        <trd>huwen</trd>,
        <trd>trouwen</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_z;&c_a;&c_m;&c_u;&c_zh;</trd>
    </drv>

    <drv mrk="edz.0igxo">
      <kap><tld/>i&gcirc;o</kap>
      <snc>
        <dif>
          La&ubreve;le&gcirc;a ligo de viro kun virino:
          <ekz>
            <tld/>i&gcirc;o la&ubreve; koro, la&ubreve; la kvanto da oro<fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            fari <tld/>i&gcirc;opeton al knabino<fnt>Z</fnt>
            (<ref tip="vid" cel="man.0o">mano</ref>);
          </ekz><ekz>
            <tld/>i&gcirc;o pro amo flamanta al sako sonanta
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            malsamranga <tld/>i&gcirc;o
            (<ref tip="sin" cel="mezali.0o">mezalianco</ref>);
          </ekz><ekz>
            <ind><tld/>i&gcirc;ofesto</ind>
            <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
            <trd lng="en">wedding</trd>
            <trd lng="fr">noce</trd>
            <trd lng="hu">h&aacute;zasod&aacute;s</trd>
            <trd lng="nl">huwelijksfeest</trd>
            <trd lng="ru">&c_s;&c_v;&c_a;&c_d;&c_mol;&c_b;&c_a;</trd>
            <fnt><bib>MT</bib>, <lok>Alta kanto 3:11</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <snc>
        <uzo tip="fak">KRI</uzo>
        <dif>
          Sakramento, kiu sanktigas la unuigon de viro kun virino kaj havigas
          al ili dian helpon por kristane vivi.
        </dif>
        <trd lng="en">Holy Matrimony</trd>
      </snc>
      <trdgrp lng="de">
        <trd>Heirat</trd>,
        <trd>Verm&auml;hlung</trd>
      </trdgrp>
      <trd lng="en">Holy Matrimony</trd>
      <trdgrp lng="fr">
        <trd>mariage</trd>,
        <trd>noces<klr tip="ind"> (mariage)</klr></trd>
      </trdgrp>
      <trd lng="hu">h&aacute;zass&aacute;g<klr tip="ind"> (szents&eacute;g)</klr></trd>
      <trd lng="nl">huwelijk</trd>
      <trdgrp lng="ru">
        <trd>&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_b;&c_a;</trd>,
        <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je; &c_v;
          &c_b;&c_r;&c_a;&c_k;</trd>,
        <trd>&c_b;&c_r;&c_a;&c_k;&c_o;&c_s;&c_o;&c_ch;&c_je;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
      </trdgrp>
    </drv>

    <drv mrk="edz.0eco">
      <kap><tld/>eco</kap>
      <snc>
        <dif>
          Stato de ge<tld/>oj:
          <ekz>
            la ligo de <tld/>eco<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng ="be">&c_sh;&c_l;&c_ju;&c_b;</trd>
      <trd lng="de">Ehe</trd>
      <trd lng="en">marriage</trd>
      <trdgrp lng="fr">
        <trd>mariage<klr> (&eacute;tat))</klr></trd>,
        <trd>union<klr> (conjugale)</klr></trd>
      </trdgrp>
      <trd lng="hu">h&aacute;zass&aacute;g<klr tip="ind"> (&aacute;llapot)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
        <trd>&c_b;&c_r;&c_a;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="edz.0eca">
      <kap><tld/>eca</kap>
      <snc>
        <dif>
          Rilata al <tld/>eco:
          <ekz>
            <tld/>eca juro;
          </ekz><ekz>
            <tld/>ece feli&ccirc;a;
          </ekz><ekz>
            <tld/>eca <klr>(la&ubreve;le&gcirc;a)</klr> infano.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>
      <trd lng="de">ehelich</trd>
      <trd lng="en">marital</trd>
      <trd lng="fr">conjugal</trd>
      <trd lng="hu">h&aacute;zass&aacute;gi</trd>
      <trd lng="nl">huwelijks-</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="edz.ge0igxi">
      <kap>ge<tld/>i&gcirc;i</kap>
      <uzo tip="fak">GEN</uzo>
      <snc>
        <dif>
          I&gcirc;i ge<tld/>oj:
          <ekz>
            ne gravas kiom malproksimaj ili estas, <klr>...</klr> fine ili
            havas la &scirc;ancon ge<tld/>i&gcirc;i
            <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 57a, p. 270a</lok></fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="edz.inter0igxi">inter<tld/>i&gcirc;i</ref>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
      <trd lng="de">heiraten</trd>
      <trd lng="en">get married</trd>
      <trd lng="fr">se <ind>marier</ind></trd>
      <trd lng="hu">&ouml;sszeh&aacute;zasodik</trd>
      <trd lng="nl">huwen</trd>
      <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="edz.eks0igxi">
      <kap>eks<tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          La&ubreve;le&gcirc;e rompi la <tld/>i&gcirc;an ligon.
        </dif>
      </snc>
      <trdgrp lng ="be">
        <trd>&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
