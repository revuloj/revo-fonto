<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>edz</rad>/o
</kap>
<drv mrk="edz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Viro la&ubreve;le&gcirc;e ligita kun virino por kunvivado kaj fondo de
      familio.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_zh;</trd>
  <trd lng="cs">man&zcaron;el</trd>
  <trdgrp lng="de">
    <trd>Ehemann</trd>,
    <trd>Gatte</trd>,
    <trd>Gemahl</trd>,
    <trd>Mann</trd>
  </trdgrp>
  <trd lng="en">matrimony</trd>
  <trdgrp lng="fr">
    <trd>&eacute;poux<klr tip="ind"> (mari)</klr></trd>,
    <trd>mari</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>echtgenoot</trd>,
    <trd>man<klr tip="ind"> (echtgenoot)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_zh;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_g;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Virino la&ubreve;le&gcirc;e ligita kun viro por kunvivado kaj fondo de
      familio:
      <ekz>
        preni al si kiel <tld/>inon
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>II. Kroniko 11, 18</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj <tld/>ino estas ge<tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Ehefrau</trd>,
    <trd>Gattin</trd>,
    <trd>Gemahlin</trd>,
    <trd>Frau</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pouse</trd>,
    <trd>femme<klr tip="ind"> (&eacute;pouse)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>echtgenote</trd>,
    <trd>vrouw<klr tip="ind"> (echtgenote)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0igi">
  <kap><tld/>(in)igi</kap>
  <snc>
    <dif>
      Fari iun <tld/>(in)on per la&ubreve;le&gcirc;a a&ubreve; religia ligo:
      <ekz>
        li <tld/>inigis &scirc;in al si<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>iga ceremonio<fnt>Z</fnt>, promeso<fnt>Z</fnt>
        (<ref tip="vid" cel="fiancx">fian&ccirc;i&gcirc;o</ref>),
        formulo<fnt>Z</fnt>, ringo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>marier<klr tip="ind"> (prendre en mariage)</klr></trd>,
      <trd>&eacute;pouser</trd>,
      <trd>prendre en <ind>mariage</ind></trd>
    </trdgrp>
    <trd lng="de">heiraten</trd>
  </snc>
  <snc>
    <dif>
      Doni kiel <tld/>(in)on:
      <ekz>
        la gepatroj <tld/>igis la filon<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>verheiraten</trd>,
      <trd>verm&auml;len</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>marier<klr tip="ind"> (donner en mariage)</klr></trd>,
      <trd>donner en <ind>mariage</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_a;&c_c;&c_mol; &c_z;&c_a; &c_m;&c_u;&c_zh;</trd>
  </trdgrp>
  <trd lng="nl">uithuwelijken</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_m;&c_u;&c_zh;</trd>,
    <trd>&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0igxi">
  <kap><tld/>(in)i&gcirc;i</kap>
  <uzo tip="klr">(kun, al)</uzo>
  <snc>
    <dif>
      I&gcirc;i <tld/>(in)o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_b;&c_r;&c_a;&c_c;&c_c;&c_a; &c_sh;&c_l;&c_ju;&c_b;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="de">heiraten</trd>
  <trdgrp lng="fr">
    <trd>&eacute;pouser</trd>,
    <trd>se <ind>marier</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huwen</trd>,
    <trd>trouwen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_j;&c_t;&c_i; &c_z;&c_a;&c_m;&c_u;&c_zh;</trd>,
    <trd>&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      La&ubreve;le&gcirc;a ligo de viro kun virino:
      <ekz><tld/>i&gcirc;o la&ubreve; koro, la&ubreve; la kvanto da oro<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        fari <tld/>i&gcirc;opeton al knabino<fnt>Z</fnt>
        (<ref tip="vid" cel="man.0o">mano</ref>);
      </ekz>
      <ekz>
        <tld/>i&gcirc;o pro amo flamanta al sako sonanta
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        malsamranga <tld/>i&gcirc;o
        (<ref tip="sin" cel="mezali.0o">mezalianco</ref>);
      </ekz>
      <ekz>
        <ind><tld/>i&gcirc;ofesto</ind>
        <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
        <trd lng="fr">noce</trd>
        <trd lng="nl">huwelijksfeest</trd>
        <trd lng="ru">&c_s;&c_v;&c_a;&c_d;&c_mol;&c_b;&c_a;</trd>
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Alta kanto 3, 11</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Sakramento, kiu sanktigas la unuigon de viro kun virino kaj havigas
      al ili dian helpon por kristane vivi.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Heirat</trd>,
    <trd>Verm&auml;hlung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mariage</trd>,
    <trd>noces<klr tip="ind"> (mariage)</klr></trd>
  </trdgrp>
  <trd lng="nl">huwelijk</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je; &c_v; &c_b;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;&c_o;&c_s;&c_o;&c_ch;&c_je;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de ge<tld/>oj:
      <ekz>
        la ligo de <tld/>eco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng ="be">&c_sh;&c_l;&c_ju;&c_b;</trd>
  <trd lng="de">Ehe</trd>
  <trdgrp lng="fr">
    <trd>mariage<klr> (&eacute;tat))</klr></trd>,
    <trd>union<klr tip="ind"> (conjugale)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="edz.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Rilata al <tld/>eco:
      <ekz>
        <tld/>eca juro;
      </ekz>
      <ekz>
        <tld/>ece feli&ccirc;a;
      </ekz>
      <ekz>
        <tld/>eca <klr>(la&ubreve;le&gcirc;a)</klr> infano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>
  <trd lng="de">ehelich</trd>
  <trd lng="fr">conjugal</trd>
  <trd lng="nl">huwelijks-</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="edz.ge0igxi">
  <kap>ge<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i ge<tld/>oj:
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">heiraten</trd>
  <trd lng="fr">se <ind>marier</ind></trd>
  <trd lng="nl">huwen</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="edz.eks0igxi">
  <kap>eks<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      La&ubreve;le&gcirc;e rompi la <tld/>i&gcirc;an ligon.
    </dif>
  </snc>
  <trdgrp lng ="be">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="fr">divorcer</trd>
  <trd lng="nl">scheiden<klr tip="ind"> (huwelijk)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i;&c_s;&c_mol;</trd>
</drv>

<drv mrk="edz.inter0igxi">
  <kap>inter<tld/>i&gcirc;i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="edz.ge0igxi">Ge<tld/>i&gcirc;i</ref>:
    </dif>
    <ekz>
      inter<tld/>i&gcirc;i kun iu<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">heiraten</trd>
  <trd lng="fr">se <ind>marier</ind></trd>
  <trd lng="nl">huwen</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="edz.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="edz.ge0igxi">Ge<tld/>i&gcirc;i</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">heiraten</trd>
  <trd lng="fr">se <ind>marier</ind></trd>
  <trd lng="nl">huwen</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="edz.sen0ineco">
  <kap>sen<tld/>(in)eco</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="frauxl.0eco">Fra&ubreve;leco</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="fr">c&eacute;libat</trd>
  <trd lng="nl">celibaat</trd>
</drv>
<adm>
  Eviti la cirklon : senedz(in)eco = frauxleco = needzeco... <aut>MB</aut>
</adm>
</art>
