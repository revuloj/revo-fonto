      </trdgrp>
    </drv>

    <drv mrk="edz.nov0ino">
      <kap>nov<tld/>ino</kap>
      <snc>
        <dif>
          &Jcirc;us <tld/>ini&gcirc;inta ino:
          <ekz>
            &leftquot;Kisu la novedzinon!&rightquot; Vilcjo tondris, kaj
            Sturges Owen ne havis la forton malobei
            <fnt>
              <aut>Jack LONDON, tr. Reto Rossetti</aut>:
              <vrk>Dio de liaj prapatroj</vrk>.
              <lok>La nica literatura revuo, 1:4</lok>.
            </fnt>
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_a;&c_ja;</trd>,
        <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_a;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="edz.novge0oj">
      <kap>novge<tld/>oj</kap>
      <snc>
        <dif>
          &Jcirc;use ge<tld/>i&gcirc;inta paro:
          <ekz>
            du monatojn post tio la novge<tld/>oj
            forveturis el Ruslando
            <fnt>
              <aut>A. Kor&jcirc;enkov</aut>:
              <vrk>Enkonduko al &leftquot;Krimo kaj puno&rightquot; de F. Dostojevskij</vrk>.
              <lok>Sezonoj, 1993</lok>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ru">
