</drv>

<drv mrk="edz.nov0o">
  <kap>nov<tld/>o</kap>
  <snc mrk="edz.nov0o.MOR">
    <dif>
      &Jcirc;us <tld/>i&gcirc;inta viro:
      <ekz>
        la gepatroj de la nov<tld/>o <klr>...</klr> devas 
        alporti ora&jcirc;ojn kaj vesta&jcirc;ojn por sia bofilino
        <fnt>
          <aut>Saeed</aut>,
          <vrk>Silenti signifas konsenti</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">novoman&zcaron;el</trd>
  <trd lng="de">Br&auml;utigam</trd>
  <trd lng="es">reci&eacute;n casado</trd>
  <trd lng="id"><ind>pengantin</ind> baru</trd>
  <trdgrp lng="pl">
    <trd>pan m&lstroke;ody</trd>,
    <trd>nowo&zdot;eniec</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_o;&c_zh;&c_jo;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>novo &zcaron;en&aacute;&ccaron;</trd>,
    <trd>novoman&zcaron;el</trd>
  </trdgrp>
</drv>

<drv mrk="edz.nov0ino">
  <kap>nov<tld/>ino</kap>
  <snc mrk="edz.nov0ino.MOR">
    <dif>
      &Jcirc;us <tld/>ini&gcirc;inta virino:
      <ekz>
        &leftquot;kisu la nov<tld/>inon!&rightquot; Vil&ccirc;jo tondris, kaj
        <nom>Sturges Owen</nom> ne havis la forton malobei
        <fnt>
          <aut>J. London, trad. R. Rossetti</aut>:
          <vrk>Dio de liaj prapatroj</vrk>.
          <lok>Nica Literatura Revuo, 1956-03 &gcirc;is 04 (1:4)</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">novoman&zcaron;elka</trd>
  <trd lng="de">Braut</trd>
  <trd lng="es">reci&eacute;n casada</trd>
  <trd lng="id"><ind>pengantin</ind> baru</trd>
  <trd lng="pl">panna m&lstroke;oda</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">novoman&zcaron;elka</trd>
</drv>

<drv mrk="edz.novge0oj">
  <kap>novge<tld/>oj</kap>
  <snc mrk="edz.novge0oj.MOR">
    <dif>
      &Jcirc;use ge<tld/>i&gcirc;inta paro,
      <ref tip="sub" cel="edz.nov0o.MOR">nov<tld/>o</ref> kaj
      <ref tip="sub" cel="edz.nov0ino.MOR">nov<tld/>ino</ref>:
      <ekz>
        du monatojn post tio la novge<tld/>oj
        forveturis el Ruslando
        <fnt>
          <aut>A. Kor&jcirc;enkov</aut>:
          <vrk>Enkonduko al &leftquot;Krimo kaj puno&rightquot; de 
          F. Dostojevskij</vrk>.
          <lok>1993</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">novoman&zcaron;el&eacute;</trd>
  <trd lng="de">Neuverm&auml;hlte</trd>
  <trd lng="es">reci&eacute;n casados</trd>
  <trd lng="id"><ind>pengantin</ind> baru</trd>
  <trdgrp lng="pl">
    <trd>pa&nacute;stwo m&lstroke;odzi</trd>,
    <trd>nowo&zdot;e&nacute;cy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_o;&c_zh;&c_jo;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="sk">novoman&zcaron;elia</trd>
</drv>

<drv mrk="edz.plur0eco">
  <kap>plur<tld/>eco</kap>
  <snc mrk="edz.plur0eco.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="dif" cel="poliandri.0o.GEN">Poliandrio</ref>:
      <ekz>
        kvankam multaj sektoj subtenas plur<tld/>inecon, ili ne 
        subtenas plur<tld/>econ        
        <fnt>
          <aut>Jonatan</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/esperanto-l/message/73317">
          Viaj infanoj estas niaj</url></vrk>,
          <lok>2008-04-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mnohomu&zcaron;stv&iacute;</trd>,
    <trd>polyandrie</trd>
  </trdgrp>
  <trd lng="es">poliandria</trd>
  <trd lng="fr">polyandrie</trd>
  <trd lng="id">poliandri</trd>
  <trdgrp lng="sk">
    <trd>mnohomu&zcaron;stvo</trd>,
    <trd>plyandria</trd>
  </trdgrp>
</drv>

<drv mrk="edz.plur0ineco">
  <kap>plur<tld/>ineco</kap>
  <snc mrk="edz.plur0ineco.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="dif" cel="poligini.0o.GEN">Poliginio</ref>:
      <ekz>
        plur<tld/>ineco ekzistas en muslimaj landoj
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url ref="http://www.literaturo.dk/tradukado.htm">
          Pri la tradukado de poezio</url></vrk>,
          <lok>Heroldo de Esperanto, 1964-10 &gcirc;is 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mnoho&zcaron;enstv&iacute;</trd>,
    <trd>polygamie</trd>
  </trdgrp>
  <trd lng="es">poliginia</trd>
  <trd lng="fr">polygynie</trd>
  <trd lng="id">poligini</trd>
  <trdgrp lng="sk">
    <trd>mnoho&zcaron;enstvo</trd>,
    <trd>polygamia</trd>
  </trdgrp>
</drv>

<drv mrk="edz.plurge0eco">
  <kap>plurge<tld/>eco</kap>
  <snc mrk="edz.plurge0eco.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="dif" cel="poliga.0o.GEN">Poligamio</ref>:
      <ekz>
        la katolikisma eklezio permesas la <tld/>econ de pastroj kaj la 
        plurge<tld/>econ de konvertitoj en esceptaj okazoj
        <fnt>
          <aut>M. Rauhamaa</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/esperanto-l/message/60972">
          Hipokritado</url></vrk>,
          <lok>2006-02-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mehrehe</trd>
  <trd lng="fr">polygamie</trd>
  <trd lng="id">poligami</trd>
  <trd lng="es">poligamia</trd>
</drv>

<drv mrk="edz.prov0igxo">
  <kap>prov<tld/>i&gcirc;o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="edz.prov0igxo.antaux">
    <dif>
      Tempo de seksa kunvivado anta&ubreve; la <tld/>i&gcirc;o:
      <ekz>
        prov<tld/>i&gcirc;on oni celebras dum komuna e&ubreve;karistio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=319693">Franco
          Barbero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="fiancx.0igxo">fian&ccirc;i&gcirc;o</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Probeehe</trd>,
    <trd>Wilde Ehe</trd>
  </trdgrp>
  <trd lng="fr">mariage &agrave; l'essai</trd>
  <trdgrp lng="pl">
    <trd>&zdot;ycie przedma&lstroke;&zdot;e&nacute;skie</trd>,
    <trd>&zdot;ycie na "koci&aogonek; &lstroke;ap&eogonek;"</trd>
  </trdgrp>
</drv>

<drv mrk="edz.sen0ineco">
  <kap>sen<tld/>eco, <var><kap>sen<tld/>ineco</kap></var></kap>
  <snc mrk="edz.sen0ineco.frauxleco">
    <dif>
      Stato de homo vivanta sen edzo a&ubreve; edzino;
      <ref tip="dif" cel="frauxl.0eco">fra&ubreve;leco</ref>:
      <ekz>
        la sen<tld/>eco de kristanaj pastroj
        <fnt>
          <vrk><url
          ref="https://neciklopedio.org/wiki/Aleister_Crowley">Aleister
          Crowley &#8208; Neciklopedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Ledigkeit</trd>
  <trd lng="fr">c&eacute;libat</trd>
  <trd lng="id">selibat</trd>
  <trd lng="nl">celibaat</trd>
  <trdgrp lng="pl">
    <trd>bez&zdot;e&nacute;stwo</trd>,
    <trd>celibat</trd>,
    <trd><klr>(staro)</klr>panie&nacute;stwo</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_b;&c_r;&c_a;&c_ch;&c_i;&c_je;</trd>
  <trd lng="es">solter&iacute;a</trd>
</drv>
