<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>edz</rad>/o
</kap>
<drv mrk="edz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Viro la&ubreve;le&gcirc;e ligita kun virino por kunvivado k fondo de
      familio.
    </dif>
  </snc>
  <trd lng="cs">man&zcaron;el</trd>
  <trdgrp lng="de">
    <trd>Ehemann</trd>,
    <trd>Gatte</trd>,
    <trd>Gemahl</trd>,
    <trd>Mann</trd>
  </trdgrp>
  <trd lng="en">matrimony</trd>
</drv>
<drv mrk="edz.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Same ligita virino:
      <ekz>
        preni al si kiel <tld/>inon
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>II. Kroniko 11, 18</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>o k <tld/>ino estas ge<tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ehefrau</trd>,
    <trd>Gattin</trd>,
    <trd>Gemahlin</trd>,
    <trd>Frau</trd>
  </trdgrp>
</drv>
<drv mrk="edz.0igi">
  <kap><tld/>(in)igi</kap> 
  <snc num="1">
    <dif>
      Fari iun <tld/>(in)on per la&ubreve;le&gcirc;a a&ubreve; religia ligo:
      <ekz>
        li <tld/>inigis &scirc;in al si<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>iga ceremonio<fnt>Z</fnt>, promeso<fnt>Z</fnt>
        (<ref tip="sin" cel="fiancx">fian&ccirc;i&gcirc;o</ref>),
        formulo<fnt>Z</fnt>, ringo<fnt>Z</fnt>. 
      </ekz>
    </dif>
    <trd lng="de">heiraten</trd>
  </snc>
  <snc num="2">
    <dif>
      Doni kiel <tld/>(in)on:
      <ekz>
        la gepatroj <tld/>igis la filon<fnt>B</fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="de"><trd>verheiraten</trd>, <trd>verm&auml;len</trd></trdgrp>
  </snc>
</drv>
<drv mrk="edz.0igxi">
  <kap><tld/>(in)i&gcirc;i</kap> 
  <uzo tip="klr">(kun, al)</uzo>
  <snc>
    <dif>
      I&gcirc;i <tld/>(in)o. 
    </dif>
  </snc>
  <trd lng="de">heiraten</trd>
</drv>
<drv mrk="edz.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc num="1">
    <dif>
      La&ubreve;le&gcirc;a ligo de viro kun virino:
      <ekz>
        <tld/>i&gcirc;o la&ubreve; koro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        fari <tld/>i&gcirc;opeton al knabino<fnt>Z</fnt>
        (<ref tip="vid" cel="man.0o">mano</ref>);
      </ekz>
      <ekz>
        <tld/>i&gcirc;o pro amo flamanta al sako sonanta
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        malsamranga <tld/>i&gcirc;o 
        (<ref tip="sin" cel="mezali.0o">mezalianco</ref>);
      </ekz>
      <ekz>
        <tld/>i&gcirc;ofesto
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Alta kanto 3, 11</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Sakramento, kiu sanktigas la unuigon de viro kun virino k havigas al ili
      dian helpon por kristane vivi.
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Heirat</trd>, <trd>Verm&auml;hlung</trd></trdgrp>
</drv>
<drv mrk="edz.ge0igxikun0igxi">
  <kap>inter<tld/>i&gcirc;i<fnt>Z</fnt>, ge<tld/>i&gcirc;i,
  kun<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i ge<tld/>oj:
      <ekz>
        inter<tld/>i&gcirc;i kun iu<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">heiraten</trd>
</drv>
<drv mrk="edz.eks0igxi">
  <kap>eks<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      La&ubreve;le&gcirc;e rompi la <tld/>i&gcirc;an ligon. 
    </dif>
  </snc>
</drv>
<drv mrk="edz.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de ge<tld/>oj:
      <ekz>
        la ligo de <tld/>eco<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">Ehe</trd>
</drv>
<drv mrk="edz.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Rilata al <tld/>eco:
      <ekz>
        <tld/>eca juro;
      </ekz>
      <ekz>
        <tld/>ece feli&ccirc;a;
      </ekz>
      <ekz>
        <tld/>eca <klr>(la&ubreve;le&gcirc;a)</klr> infano.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ehelich</trd>
</drv>
<drv mrk="edz.sen0ineco">
  <kap>sen<tld/>(in)eco</kap>
  <snc>
    <dif>
      Fra&ubreve;leco.  
    </dif>
  </snc>
</drv>
</art>
