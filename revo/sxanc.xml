<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;anc</rad>/o
</kap>
<drv mrk="sxanc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ebleco de sukceso:
      <ekz>
        havi plej multe da <tld/>oj por esti elektita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        profiti la <tld/>on;
      </ekz>
      <ekz>
        diversaj lingvoj havas por tio &ccirc;i plimalpli egalajn
        <tld/>ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        niaj <tld/>oj alti&gcirc;is;
      </ekz>
      <ekz>
        ne havi <tld/>ojn al venko;
      </ekz>
      <ekz>
        <ind>bon<tld/>o</ind>;
        <trdgrp lng="he">
          <trd>&mem;&zayin;&lamed;</trd>,
          <trd>&gimel;&dalet;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>malbon<tld/>o</ind>.
        <trdgrp lng="he">
          <trd>&bet;&yod;&shin; &mem;&zayin;&lamed;</trd>,
          <trd>&bet;&yod;&shin; &gimel;&dalet;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hazard.0o">hazardo</ref>,
      <ref cel="sort.0o">sorto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_n;&c_je;&c_c;</trd>, 
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>    
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gelegenheit</trd>,
    <trd>Chance</trd>
  </trdgrp>
  <trd lng="en">chance</trd>
  <trd lng="fr">chance<klr tip="amb"> (possibilit&eacute;, probabilit&eacute;)</klr></trd>
  <trd lng="he">&samekh;&yod;&kaf;&vav;&yod;</trd>
  <trdgrp lng="hu">
    <trd>es&eacute;ly</trd>,
    <trd>sansz</trd>
  </trdgrp>
  <trd lng="nl">kans</trd>
  <trd lng="pt">chance</trd>
  <trd lng="ru">&c_sh;&c_a;&c_n;&c_s;</trd>
</drv>

<drv mrk="sxanc.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="sxanc.bon0a.KOMUNE">
    <dif>
      Hazarde trovanta multajn feli&ccirc;igajn okaza&jcirc;ojn:
      <ekz>
        mi estis bon<tld/>a, mi vidis lin &gcirc;ustatempe
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 15a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="felicx.0a.sukcesanta">feli&ccirc;a <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>chanceux</trd>,
    <trd>heureux<klr tip="amb"> (chanceux)</klr></trd>
  </trdgrp>
</drv>
</art>
