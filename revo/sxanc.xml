<vortaro>
<art mrk="$Id: sxanc.xml,v 1.72 2021/06/18 18:20:14 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;anc</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxanc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ebleco de sukceso:
      <ekz>
        havi plej multe da <tld/>oj por esti elektita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        profiti la <tld/>on;
      </ekz>
      <ekz>
        diversaj lingvoj havas por tio &ccirc;i plimalpli egalajn
        <tld/>ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        niaj <tld/>oj alti&gcirc;is;
      </ekz>
      <ekz>
        ne havi <tld/>ojn al venko;
      </ekz>
      <ekz>
        <ind>bon<tld/>o</ind>;
        <trdgrp lng="he">
          <trd>&mem;&zayin;&lamed;</trd>,
          <trd>&#x2137;&dalet;</trd>
        </trdgrp>
        <trd lng="nl">geluk</trd>
      </ekz>
      <ekz>
        <ind>malbon<tld/>o</ind>.
        <trdgrp lng="he">
          <trd>&bet;&yod;&shin; &mem;&zayin;&lamed;</trd>,
          <trd>&bet;&yod;&shin; &#x2137;&dalet;</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>ongeluk</trd>,
          <trd>pech</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hazard.0o">hazardo</ref>,
      <ref cel="sort.0o">sorto</ref>
    </refgrp>
  </snc>
  <trd lng="af">kans</trd>
  <trd lng="am">&#x12D5;&#x12F5;&#x120D;</trd>
  <trd lng="ar">&#x641;&#x631;&#x635;&#x629;</trd>
  <trd lng="az">&#x15F;ans</trd>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_n;&c_je;&c_c;</trd>, 
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>    
  </trdgrp>
  <trd lng="bn">&#x9B8;&#x9C1;&#x9AF;&#x9CB;&#x997;</trd>
  <trd lng="bo">&#xF66;&#xF9F;&#xF56;&#xF66;&#xF0B;&#xF60;&#xF42;&#xFB2;&#xF72;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="bs">&#x161;anse</trd>
  <trd lng="co">casu</trd>
  <trd lng="da">mulighed</trd>
  <trdgrp lng="de">
    <trd>Gelegenheit</trd>,
    <trd>Chance</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chance</trd>,
    <trd>luck</trd>,
    <trd>prospect<klr> (chance of success)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>oportunidad</trd>,
    <trd>suerte<klr> (en la expresi&oacute;n "Bonan
    &scirc;ancon!")</klr></trd>
  </trdgrp> 
  <trd lng="et">v&#xF5;imalus</trd>
  <trd lng="eu">aukera</trd>
  <trd lng="fr">chance<klr tip="amb"> (possibilit&eacute;, 
probabilit&eacute;)</klr></trd>
  <trd lng="fy">k&#xE2;ns</trd>
  <trd lng="gl">oportunidade</trd>
  <trd lng="gu">&#xAA4;&#xA95;</trd>
  <trd lng="ha">dama</trd>
  <trd lng="he">&samekh;&yod;&kaf;&vav;&yod;</trd>
  <trd lng="hi">&#x938;&#x902;&#x92F;&#x94B;&#x917;</trd>
  <trd lng="hr">&#x161;anse</trd>
  <trdgrp lng="hu">
    <trd>es&eacute;ly</trd>,
    <trd>sansz</trd>
  </trdgrp>
  <trd lng="hy">&#x577;&#x561;&#x576;&#x57D;</trd>
  <trd lng="is">t&#xE6;kif&#xE6;ri</trd>
  <trdgrp lng="it">
    <trd>opportunit&agrave;</trd>, 
    <trd>possibilit%agrave;<klr> (opportunit&agrave;)</klr></trd>, 
    <trd>occasione<klr> (chance)</klr></trd>, 
    <trd>chance</trd>    
  </trdgrp>
  <trd lng="ja">&#x30C1;&#x30E3;&#x30F3;&#x30B9;</trd>
  <trd lng="jw">kasempatan</trd>
  <trd lng="ka">&#x10E8;&#x10D0;&#x10DC;&#x10E1;&#x10D8;</trd>
  <trd lng="km">&#x17B1;&#x1780;&#x17B6;&#x179F;</trd>
  <trd lng="ko">&#xAE30;&#xD68C;</trd>
  <trd lng="lo">&#xEC2;&#xEAD;&#xE81;&#xEB2;&#xE94;</trd>
  <trd lng="ml">&#xD05;&#xD35;&#xD38;&#xD30;&#xD02;</trd>
  <trd lng="mr">&#x938;&#x902;&#x927;&#x940;</trd>
  <trd lng="my">&#x1021;&#x1001;&#x103D;&#x1004;&#x1037;&#x103A;&#x1021;&#x101C;&#x1019;&#x103A;&#x1038;</trd>
  <trd lng="ne">&#x92E;&#x94C;&#x915;&#x93E;</trd>
  <trd lng="nl">kans</trd>
  <trd lng="ny">mwayi</trd>
  <trd lng="pa">&#xA2E;&#xA4C;&#xA15;&#xA3E;</trd>
  <trd lng="pl">szansa</trd>
  <trd lng="ps">&#x686;&#x627;&#x646;&#x633;</trd>
  <trdgrp lng="pt">
    <trd>chance</trd>,
    <trd>oportunidade</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_n;&c_s;</trd>
  <trd lng="sd">&#x627;&#x62A;&#x641;&#x627;&#x642;</trd>
  <trd lng="si">&#xD85;&#xDC0;&#xDC3;&#xDCA;&#xDAE;&#xDCF;&#xDC0;&#xD9A;&#xDCA;</trd>
  <trd lng="sq">shans</trd>
  <trd lng="ta">&#xBB5;&#xBBE;&#xBAF;&#xBCD;&#xBAA;&#xBCD;&#xBAA;&#xBC1;</trd>
  <trd lng="te">&#xC15;&#xC4D;&#xC30;&#xC40;&#xC21;&#xC32;&#xC4D;&#xC32;&#xC4B; &#xC05;&#xC35;&#xC15;&#xC3E;&#xC36;&#xC3E;&#xC32;&#xC41;</trd>
  <trd lng="th">&#xE42;&#xE2D;&#xE01;&#xE32;&#xE2A;</trd>
  <trd lng="tl">pagkakataon</trd>
  <trd lng="ur">&#x645;&#x648;&#x642;&#x639;</trd>
  <trd lng="vi">c&#x1A1; h&#x1ED9;i</trd>
  <trd lng="yi">&#x5D2;&#x5E2;&#x5DC;&#x5E2;&#x5D2;&#x5E0;&#x5D4;&#x5D9;&#x5D9;&#x5B7;&#x5D8;</trd>
</drv>

<drv mrk="sxanc.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="sxanc.bon0a.KOMUNE">
    <dif>
      Hazarde trovanta multajn feli&ccirc;igajn okaza&jcirc;ojn:
      <ekz>
        mi estis bon<tld/>a, mi vidis lin &gcirc;ustatempe
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 15a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="felicx.0a.sukcesanta">feli&ccirc;a<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_l;&c_y;</trd>
  <trd lng="bo">&#xF56;&#xF66;&#xF7C;&#xF51;&#xF0B;&#xF53;&#xF58;&#xF66;&#xF0B;&#xF45;&#xF53;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>gl&uuml;cklich</trd>,
    <trd>Gl&uuml;ck haben</trd>
  </trdgrp>
  <trd lng="en">lucky</trd>
  <trdgrp lng="es">
    <trd>afortunado</trd>,
    <trd>afortunada</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chanceux</trd>,
    <trd>heureux<klr tip="amb"> (chanceux)</klr></trd>
  </trdgrp>
  <trd lng="nl">gelukkig<klr tip="ind"> (geluk hebbend)</klr></trd>
  <trd lng="pl">maj&aogonek;cy <ind>szcz&eogonek;&sacute;cie</ind></trd>
  <trd lng="it">fortunato</trd>
</drv>

<drv mrk="sxanc.mis0a">
  <kap>mis<tld/>a, <var><kap>malbon<tld/>a</kap></var></kap>
  <snc mrk="sxanc.mis0a.KOMUNE">
    <dif>
      Hazarde trafata de malfeli&ccirc;a&jcirc;oj,
      de malfavora sorto:
      <ekz>
        Jozefino libertempumas sola &ccirc;e la Franca Riviero,
        sed estas vere mis<tld/>a, &ccirc;ar la pluvo ne &ccirc;esas
        dum tuta semajno
        <fnt>
          <aut>R. Cash</aut>,
          <vrk><url ref="http://miresperanto.narod.ru/humuro/rikardo-cash-6.htm">
          &Scirc;ercoj 6</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>;
      </ekz>
      <ekz>
        malbon<tld/>a junulino delasis nesekuran tenon &ccirc;e alta
        bran&ccirc;o
        kaj kra&scirc;falis teren
        <fnt>
          <aut>E. R. Burroughs, trad. D. J. Harlow</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Tarzan/tarzanu04.html">
          Tarzan de la simioj</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>unlucky</trd>,
    <trd>luckless</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desafortunado</trd>,
    <trd>desafortunada</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>malchanceux</trd>,
    <trd>malheureux<klr> (malchanceux)</klr></trd>
  </trdgrp>
  <trd lng="pl">maj&aogonek;cy <ind>pecha</ind></trd>
</drv>

<drv mrk="sxanc.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="sxanc.sen0a.KOMUNE">
    <dif>
      Kiu ne havas &scirc;ancojn de sukceso,
      <ref tip="vid" cel="perspe.sen0a.FIG">senperspektiva</ref>,
      <ref tip="vid" cel="esper.sen0a">senespera</ref>:
      <ekz>
        se ekzistus ia Baghy-a skolo, certe &gcirc;i ne estus vana kaj 
        sen<tld/>a
        <fnt>
          <aut>K. Pi&ccirc;</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr65/baghy.html">
          Julio Baghy</url></vrk>,
          <lok>Nica Literatura Revuo, 1961-05 &gcirc;is 06 (6/5), p.
          189a-194a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">hopeless <klr>(no chance)</klr></trd>
  <trd lng="fr">qui n'a <ind>aucune chance</ind></trd>
  <trd lng="pl">bez <ind>szans</ind></trd>
</drv>
</art>
