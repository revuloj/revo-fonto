<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kontramark</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kontra6.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Dua marko stampita sur paka&jcirc;o, varo, sur objektoj el ar&gcirc;ento kaj
      oro.
    </dif>
    <trd lng="de">Kontrollmarke</trd>
    <trdgrp lng="hu">
      <trd>m&aacute;sodik <ind>jelzet</ind></trd>,
      <trd>ellenjelz&eacute;s</trd>,
      <trd>m&aacute;sodik <ind>f&eacute;mjelz&eacute;s</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>controlestempel</trd>,
      <trd>tweede merk</trd>
    </trdgrp>
    <trd lng="pt">contramarca</trd>
  </snc>
  <snc>
    <dif>
      Dua orda numero skribita sur fakturo a&ubreve; mendilo.
    </dif>
    <trd lng="hu">ellen&odblac;rz&odblac; <ind>sorsz&aacute;m</ind> <klr
    tip="ind">(sz&aacute;ml&aacute;n)</klr></trd>
    <trdgrp lng="nl">
      <trd>controlestempel</trd>,
      <trd>tweede merk</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Bileto liverata en teatroj, kinejoj k. a. al tiuj, kiuj momente
      eliras, kaj donanta la rajton denove eniri.
    </dif>
    <trd lng="de">Kontrollkarte</trd>
    <trdgrp lng="hu">
      <trd>visszat&eacute;r&odblac;jegy</trd>,
      <trd>kil&eacute;p&odblac;jegy</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>sortie</trd>,
      <trd>contramerk</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_m;&c_a;&c_r;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kontramarka</trd>,
    <trd>kontrolní značka</trd>
  </trdgrp>
  <trd lng="fr">contremarque</trd>
  <trd lng="sk">vstupenka pre návrat <klr>(po prestávke)</klr></trd>
</drv>
</art>
