<drv mrk="harmon1.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="harmon1.0igi.KOMUNE">
    <dif>
      <ref tip="super" cel="arangx.0i.ordigi">Aran&gcirc;i</ref> la&ubreve; <tld/>o, krei <tld/>on inter:
      <ekz>
        komponisto <tld/>igas sonojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        oratoro nur klopodu <tld/>igi la stilon kaj tonon de sia parolado al la jam
        ekzistanta spiritostato de la a&ubreve;skultantoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom malfacile estas <tld/>igi la etikajn principojn kun la bezonoj de la realo
        <fnt>
          <bib>CKv</bib>
          <lok>12</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>igi la agadon, UN fondis apartan Preparkomitaton
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devas <tld/>igi la interesojn de 15 landoj de EU
        <fnt><aut>Bardhyl Selimi</aut>:
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia korpo tamen estis skulptita de talenta artisto, kiu sciis <tld/>igi la
        partojn
        <fnt>
          <bib>ChM</bib>
          <lok>1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0igi">akordigi</ref>,
      <ref cel="ord.kun0igi.partojn">kunordigi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_m;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">harmonitzar<klr> (tr.)</klr></trd>
  <trd lng="cs">harmonizovat</trd>
  <trdgrp lng="de">
    <trd>harmonisieren</trd>,
    <trd>in <ind>Einklang</ind> bringen</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&ny;&alfa;&rho;&my;&omikron;&ny;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">to harmonize</trd>
  <trd lng="es">armonizar</trd>
  <trdgrp lng="fr">
    <trd>harmoniser<klr tip="ind"> (mettre en harmonie)</klr></trd>,
    <trd>accorder<klr tip="amb"> (harmoniser)</klr></trd>,
    <trd>mettre en <ind>accord</ind></trd>,
    <trd>faire s'<ind>entendre</ind></trd>,
    <trd>allier<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>assembler<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>assortir<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>associer<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>marier<klr tip="ind"> (harmoniser)</klr></trd>
  </trdgrp>
  <trd lng="hu">&ouml;sszhangba hoz</trd>
  <trd lng="it">armonizzare</trd>
  <trd lng="ja">&#x8ABF;&#x548C;&#12373;&#x305B;&#x308B;  <pr>&#12385;&#12423;&#12358;&#x308F;&#12373;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">harmoni&euml;ren</trd>
  <trd lng="pl">harmonizowa&cacute;</trd>
  <trd lng="pt">harmonizar</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zharmonizova&tcaron;</trd>
