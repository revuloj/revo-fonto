<kap>
  <rad>diferenci</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="difere1.0i">
  <kap><tld/>i<fnt><bib>MatTerm</bib>, <lok>p. 20</lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="difere1.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <refgrp tip="dif">
        <ref cel="deriv.0i.MAT">Derivi</ref> a&ubreve;
        <ref cel="difere2.0i">diferenciali</ref>
      </refgrp>:
      <ekz>
        <nom>G&ouml;sta Mittag-Leffler</nom> <klr>[&#x2026;]</klr> okupi&gcirc;is pri limvaloro, <tld/>a kaj
        integrala kalkulado, analitika geometrio kaj probablokalkulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=208852">G&ouml;sta Mittag-Leffler</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="uk">&#x434;&#x438;&#x444;&#x435;&#x440;&#x435;&#x43D;&#x446;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>
</art>
