<art mrk="$Id$">
<kap>
  <rad>diferenci</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="difere1.0i">
  <kap><tld/>i<fnt><bib>MatTerm</bib>, <lok>p. 20</lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="difere1.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <refgrp tip="dif">
      <ref cel="deriv.0i.MAT">derivi</ref> a&ubreve;
      <ref cel="difere2.0i">diferenciali</ref>.
    </refgrp>
  </snc>
</drv>
</art>
