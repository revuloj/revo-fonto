<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>glir</rad>/o
</kap>
<drv mrk="glir.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="glir.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Glis</trd>).
    </dif>
  </snc>
  <trd lng="de">Siebenschl&auml;fer</trd>
</drv>
<drv mrk="glir.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="rod1.0uloj">roduloj</ref>
      (<trd lng="la">Gliridae</trd>), kies anoj dormas dum malvarmaj a&ubreve;
      sekaj periodoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bilche</trd>,
    <trd>Schl&auml;fer <klr tip="ind">(Nagerfamilie)</klr></trd>
  </trdgrp>
  <trd lng="en">dormice</trd>
</drv>
</art>
