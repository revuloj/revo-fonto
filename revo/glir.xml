  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="glired.0oj"><tld/>edoj</ref>
      (<trd lng="la">Glis</trd>).
    </dif>
  </snc>
  <trd lng="de">Siebenschl&auml;fer</trd>
  <trd lng="fr">loir</trd>
  <trdgrp lng="nl">
    <trd>relmuis</trd>,
    <trd>zevenslaper</trd>,
    <trd>tuinslaper</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_n;&c_ja;-&c_p;&c_o;&c_l;&c_ch;&c_o;&c_k;</trd>
</drv>

</art>
