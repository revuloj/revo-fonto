<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ojstred</rad>/oj
</kap>
<drv mrk="ojstre.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Oestridae</trd>), grandaj haraj insektoj, kies larvoj
      parazitas en hufuloj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dasselfliegen</trd>,
    <trd>Biesfliegen</trd>
  </trdgrp>
  <trd lng="nl">horzels</trd>
  <trd lng="ru">&c_o;&c_v;&c_o;&c_d;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
