<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>akord</rad>/o
</kap>
<drv mrk="akord.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Sameco de sentoj a&ubreve; opinioj inter pluraj personoj, a&ubreve;
       &gcirc;usta konveneco inter pluraj aferoj:
       <ekz>
         Ili estas en <tld/>o kiel peto kaj mordo
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
         <klr>(tre malkonkordaj.
         <ref tip="sin" cel="kat.0o">kato</ref>)</klr>;
       </ekz>
       <ekz>
         kontrakti <tld/>on
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="konsen.0o">konsento</ref>,
       <ref cel="koncer1.0o">koncerto 2</ref>.
     </refgrp>
     <trd lng="fr">accord</trd>
     <trdgrp lng="ru">
       <trd>&#1089;&#1086;&#1075;&#1083;&#1072;&#1089;&#1080;&#1077;</trd>,
       <trd>&#1089;&#1086;&#1075;&#1083;&#1072;&#1089;&#1086;&#1074;&#1072;&#1085;&#1085;&#1086;&#1089;&#1090;&#1100;</trd>
     </trdgrp>
   </snc>
   <snc>
     <dif>
       Samtempa harmonia sonado de kelkaj tonoj;
       <ref tip="sin" cel="harmon1.0o.sonoj">harmonio</ref>:
       <ekz>
         perfekta <tld/>o.
       </ekz>
     </dif>
     <trd lng="de">Akkord</trd>
     <trd lng="fr">accord</trd>
     <trd lng="ru">&#1072;&#1082;&#1082;&#1086;&#1088;&#1076;</trd>
   </snc>
</drv>
<drv mrk="akord.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <dif>
     Esti en <tld/>o.
   </dif>
   <snc>
     <ekz>
       <tld/>i  kun iu.
     </ekz>
   </snc>
   <snc>
     <ekz>
       tiuj koloroj bone <tld/>as.
     </ekz>
   </snc>
   <trdgrp lng="fr">
     <trd>s'<ind>accorder</ind></trd>,
     <trd>s'<ind>assortir</ind></trd>,
     <trd>&ecirc;tre en <ind>accord</ind></trd></trdgrp>
   </drv>
<drv mrk="akord.0igi">
   <kap><tld/>igi</kap>
   <snc>
     <dif>
       Fari, ke du a&ubreve; pluraj personoj a&ubreve; aferoj i&gcirc;u en
       <tld/>on:
       <ekz>
         kaze <tld/>igi vorton kun alia<fnt>Z</fnt>;
       </ekz>
       <ekz>
         peni por <tld/>igi la naciajn fierecojn
         (<ref tip="sin" cel="pac.0igi">pacigi</ref>);
       </ekz>
       <ekz>
         <tld/>igi horlo&gcirc;on.
       </ekz>
     </dif>
     <trdgrp lng="fr">
       <trd>mettre d'<ind>accord</ind></trd>,
       <trd>harmoniser</trd></trdgrp>
     <trdgrp lng="ru">
       <trd>&#1087;&#1088;&#1080;&#1074;&#1086;&#1076;&#1080;&#1090;&#1100;
       &#1082; &#1089;&#1086;&#1075;&#1083;&#1072;&#1089;&#1080;&#1102;
       <klr>(&#1083;&#1102;&#1076;&#1077;&#1081;)</klr></trd>,

       <trd>&#1089;&#1086;&#1075;&#1083;&#1072;&#1089;&#1086;&#1074;&#1099;&#1074;&#1072;&#1090;&#1100;
       <klr>(&#1076;&#1077;&#1081;&#1089;&#1090;&#1074;&#1080;&#1103;)</klr></trd>
     </trdgrp>
   </snc>
</drv>
<drv mrk="akord.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <snc>
     <dif>
       Veni en <tld/>on:
       <ekz>
         ili <tld/>i&gcirc;as por vin trompi<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kun la kreskado de la tempo &ccirc;io
         <tld/>i&gcirc;os<fnt>B</fnt>;
       </ekz>
       <ekz>
         <tld/>i&gcirc;ema <klr>(konsentema)</klr> karaktero.
       </ekz>
     </dif>
     <trd lng="fr">se mettre d'<ind>accord</ind></trd>
     <trd
     lng="ru">&#1087;&#1088;&#1080;&#1093;&#1086;&#1076;&#1080;&#1090;&#1100;
     &#1082; &#1089;&#1086;&#1075;&#1083;&#1072;&#1089;&#1080;&#1102;</trd>
   </snc>
   <snc>
     <dif>
       = <tld/>i:
       <ekz>
         la adjektivo <tld/>i&gcirc;as kun la substantivo;
       </ekz>
       <ekz>
         la privata intereso tute <tld/>i&gcirc;is kun  la
         &gcirc;enerala.
       </ekz>
     </dif>
     <trd lng="fr">s'<ind>accorder</ind></trd>
     <trd
     lng="ru">&#1089;&#1086;&#1075;&#1083;&#1072;&#1089;&#1086;&#1074;&#1099;&#1074;&#1072;&#1090;&#1100;&#1089;&#1103;</trd>
   </snc>
</drv>
</art>
