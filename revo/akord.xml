<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>akord</rad>/o
</kap>
<drv mrk="akord.0o">
   <kap><tld/>o</kap>
   <snc mrk="akord.0o.konsento">
     <dif>
       Sameco de sentoj a&ubreve; opinioj inter pluraj personoj, a&ubreve;
       &gcirc;usta konveneco inter pluraj aferoj:
       <ekz>
         <ind>Ili estas en <tld/>o kiel peto kaj mordo</ind>
         <klr>(ili estas tre malkonkordaj.
         <ref tip="vid" cel="kat.0o">kato</ref>)</klr>
         <trd lng="fr">ils s'entendent comme <ind>chien</ind> et chat</trd>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         kontrakti <tld/>on
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="konsen.0o">konsento</ref>,
       <ref cel="koncer1.0o.FIG">koncerto <sncref/></ref>.
     </refgrp>
     <trdgrp lng="be">
       <trd>&c_s;&c_u;&c_l;&c_a;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>,
       <trd>&c_z;&c_l;&c_a;&c_d;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
       <trd>&c_u;&c_z;&c_g;&c_o;&c_d;&c_n;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
       <trd>&c_z;&c_g;&c_o;&c_d;&c_a;</trd>,
       <trd>&c_u;&c_g;&c_o;&c_d;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>Gleichklang</trd>,
       <trd>&Uuml;bereinstimmung</trd>
     </trdgrp>
     <trd lng="es">accord</trd>
     <trd lng="es">acuerdo</trd>
     <trdgrp lng="pt">
       <trd>acordo</trd>,
       <trd>concord&acirc;ncia</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>,
       <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
     </trdgrp>
     <trd lng="sk">s&uacute;lad</trd>
   </snc>
   <snc mrk="akord.0o.MUZ">
     <uzo tip="fak">MUZ</uzo>
     <dif>
       <ref tip="vid" cel="harmon1.0o.MUZ">Harmonia</ref> songrupo:
       <ekz>
         perfekta <tld/>o.
       </ekz>
     </dif>
     <trd lng="be">&c_a;&c_k;&c_o;&c_r;&c_d;</trd>
     <trd lng="de">Akkord</trd>
     <trd lng="en">chord</trd>
     <trd lng="es">acorde</trd>
     <trd lng="nl">overeenstemming</trd>
     <trd lng="pt">acorde</trd>
     <trd lng="ru">&c_a;&c_k;&c_k;&c_o;&c_r;&c_d;</trd>
     <trd lng="sk">akord</trd>
     <trdgrp lng="sv">
       <trd>ackord</trd>,
       <trd>samklang</trd>
     </trdgrp>
   </snc>
   <trd lng="fr">accord</trd>
</drv>

<drv mrk="akord.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Esti en <tld/>o.
     </dif>
     <ekz>
       <tld/>i kun iu;
     </ekz>
     <ekz>
       tiuj koloroj bone <tld/>as.
     </ekz>
   </snc>
   <trdgrp lng="be">
     <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_z;&c_g;&c_o;&c_d;&c_z;&c_je;</ind></trd>,
     <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_s;&c_u;&c_l;&c_a;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_ib;</ind></trd>
     <trd>&c_a;&c_d;&c_p;&c_a;&c_v;&c_ja;&c_d;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_a;&c_d;&c_n;&c_o; &c_a;&c_d;&c_n;&c_a;&c_m;&c_u;)</klr></trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd><klr>im </klr>Gleichklang sein</trd>,
     <trd>&uuml;bereinstimmen</trd>
   </trdgrp>
   <trd lng="en">accord</trd>
   <trdgrp lng="fr">
     <trd>s'<ind>accorder</ind></trd>,
     <trd>s'<ind>assortir</ind></trd>,
     <trd>&ecirc;tre en <ind>accord</ind></trd>
   </trdgrp>
   <trd lng="nl">overeenstemmen</trd>
   <trdgrp lng="pt">
     <trd>acordar</trd>,
     <trd>estar de acordo</trd>
   </trdgrp>
   <trd lng="sk">by&tcaron; v <ind>s&uacute;lade</ind></trd>
   <trd lng="sv">&ouml;verensst&auml;mma</trd>
</drv>

<drv mrk="akord.0igi">
   <kap><tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Fari, ke du a&ubreve; pluraj personoj a&ubreve; aferoj i&gcirc;u en
       <tld/>on:
       <ekz>
         kaze <tld/>igi vorton kun alia<fnt>Z</fnt>;
       </ekz>
       <ekz>
         peni por <tld/>igi la naciajn fierecojn
         (<ref tip="vid" cel="pac.0igi">pacigi</ref>);
       </ekz>
       <ekz>
         <tld/>igi horlo&gcirc;on.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_u;&c_z;&c_g;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
       <trd>&c_p;&c_r;&c_y;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_d;&c_a; <ind>&c_z;&c_g;&c_o;&c_d;&c_y;</ind></trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>Gleichklang <ind>hervorrufen</ind></trd>,
       <trd>&Uuml;bereinstimmung <ind>bewirken</ind></trd>,
       <trd>&Uuml;bereinstimmung <ind>herbeif&uuml;hren</ind></trd>
     </trdgrp>
     <trd lng="en">place in accord</trd>
     <trdgrp lng="fr">
       <trd>mettre d'<ind>accord</ind></trd>,
       <trd>harmoniser</trd>
     </trdgrp>
     <trd lng="nl">doen <ind>overeenstemmen</ind></trd>
     <trdgrp lng="pt">
       <trd>p&ocirc;r de acordo</trd>,
       <trd>concordar</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_k; <ind>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_ju;</ind>
       <klr>(&c_l;&c_ju;&c_d;&c_je;&c_j;)</klr></trd>,
       <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_ja;)</klr></trd>
     </trdgrp>
     <trd lng="sk">zladi&tcaron;</trd>
   </snc>
</drv>

<drv mrk="akord.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="akord.0igxi.ekkonsenti">
     <dif>
       Veni en <tld/>on:
       <ekz>
         ili <tld/>i&gcirc;as por vin trompi<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kun la kreskado de la tempo &ccirc;io
         <tld/>i&gcirc;os<fnt>B</fnt>;
       </ekz>
       <ekz>
         <tld/>i&gcirc;ema <klr>(konsentema)</klr> karaktero.
       </ekz>
     </dif>
     <trd lng="be">&c_p;&c_r;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_d;&c_a; <ind>&c_z;&c_g;&c_o;&c_d;&c_y;</ind></trd>
     <trdgrp lng="de">
       <trd><klr>in </klr>Gleichklang kommen</trd>,
       <trd>untereinander <ind>&Uuml;bereinstimmung erreichen</ind></trd>
     </trdgrp>
     <trd lng="en">come in accordance</trd>
     <trd lng="fr">se mettre d'<ind>accord</ind></trd>
     <trd lng="pt">entrar em acordo</trd>
     <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_k; 
<ind>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_ju;</ind></trd>
     <trd lng="sk">zladi&tcaron; sa</trd>
   </snc>
   <snc mrk="akord.0igxi.akordi">
     <ref tip="dif" cel="akord.0i"><tld/>i</ref>
     <ekz>
       la adjektivo <tld/>i&gcirc;as kun la substantivo;
     </ekz>
     <ekz>
       la privata intereso tute <tld/>i&gcirc;is kun la
       &gcirc;enerala.
     </ekz>
     <trd lng="be">&c_u;&c_z;&c_g;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
     <trd lng="en">accord</trd>
     <trd lng="fr">s'<ind>accorder</ind></trd>
     <trd lng="ru">&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
   </snc>
</drv>
</art>
