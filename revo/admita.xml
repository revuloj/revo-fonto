<art mrk="$Id: admita.xml,v 1.22 2023/08/30 18:23:30 revo Exp $">
<kap>
  <rad>admitanc</rad>/o
</kap>
<drv mrk="admita.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      inversa al la <ref cel="impeda.0o">impedanco</ref> 
      de elektra cirkvito:
      <ekz>
        la reela parto de <tld/>o estas la konduktanco, &gcirc;ia imaginara parto estas la
        susceptanco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=652623">Admitanco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_w;&c_n;&c_a;&c_ja; <ind>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  <trd lng="cs">admitance</trd>
  <trd lng="de">Admittanz</trd>
  <trd lng="en">admittance</trd>
  <trd lng="es">admitancia</trd>
  <trd lng="fr">admittance</trd>
  <trdgrp lng="hu">
    <trd>admittancia</trd>,
    <trd>komplex <ind>vezet&odblac;k&eacute;pess&eacute;g</ind></trd>
  </trdgrp>
  <trd lng="it">ammettenza</trd>
  <trd lng="ja">&#x30A2;&#x30C9;&#x30DF;&#x30BF;&#x30F3;&#x30B9;</trd>
  <trd lng="nl">admittantie</trd>
  <trd lng="pl">admitancja</trd>
  <trd lng="pt">admit&acirc;ncia</trd>
  <trd lng="ru">
  &c_p;&c_o;&c_l;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;
  </ind>
  </trd>
  <trdgrp lng="sk">
    <trd>admitancia</trd>,
    <trd>admitancia <klr>(el. tech.)</klr></trd>,
    <trd>zdanliv&aacute; <ind>vodivos&tcaron;</ind></trd>,
    <trd>zdanliv&aacute; vodivos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_d;&c_m;&c_ib;&c_t;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_a; &c_p;&c_r;&c_o;&c_v;&c_ib;&c_d;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
