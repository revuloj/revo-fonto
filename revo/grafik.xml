<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grafik.xml,v 1.40 2022/05/27 08:38:31 revo Exp $">
<kap>
  <ofc>9</ofc><rad>grafik</rad>/o 
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="grafik.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="grafik.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="lst" lst="voko:artoj" cel="art.0o.tehxniko">Arto</ref>
      prezenti objektojn per desegnaj linioj:
      <ekz>
        li okupis sin anka&ubreve; pri ar&hcirc;itekturo, <tld/>o, ilustrado de tekstoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011335.php">Pentristo de la homo kaj de la
          naturo</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi ne planas labori pri <tld/>o, sed &ccirc;efe pri tekstoj a&ubreve; aliaj aferoj, kiuj
        ne postulas altkvalitan bildon, <nom>DSTN</nom>-ekrano povas ta&ubreve;gi
        <fnt>
          <bib>Monato</bib>
          <aut>Zoran &Cacute;iri&cacute;</aut>
          <vrk><url ref="&Monato;/2003/008165.html">A&ccirc;eti tekokomputilon</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la portebla dokumentformo PDF, <klr>[&#8230;]</klr> ja ebligis modernan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2010/010009.php">Survoje al iu
          libro-normo</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Grafik</trd>,
      <trd>Graphik</trd>,
    </trdgrp>
    <trd lng="es">arte gr&aacute;fico</trd>
    <trd lng="fr">art graphique</trd>
    <trd lng="hu">grafika</trd>
    <trdgrp lng="nl">
      <trd>grafiek<klr tip="ind"> (kunst)</klr></trd>,
      <trd>grafische kunst</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>grafika</trd>,
      <trd>rysunek</trd>
    </trdgrp>
    <trd lng="pt">arte gr&aacute;fica</trd>
  </snc>
  <snc mrk="grafik.0o.LIN">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="grafis.0o">Grafismo</ref>:
      <ekz>
        la alfabeto de Esperanto plej bone konservas la <tld/>on kaj
        la fonetikon de l' internaciaj vortoj
        <fnt><bib>VdE</bib></fnt>;
      </ekz>
      <ekz>
        devas esti eble elpensi skribadon, kiu permesus, &leftquot;pensadi sur la papero&rightquot;, t. e.
        skribadi kun la rapideco de la pensado, &ndash; sekve &leftquot;tempo&scirc;parantan ideoskribadon
        a&ubreve; pensodesegnadon&rightquot;, kiun li nomis &leftquot;Ideo<tld/>o&rightquot;
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">grafika</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_k;&c_a;</trd>
  <trd lng="cs">grafika</trd>
  <trd lng="en">graphics</trd>
  <trd lng="es">gr&aacute;fico<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>grafikai rendszer</trd>,
    <trd>bet&udblac;rendszer</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_k;&c_a;</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_f;&c_ib;&#x43A;&c_a;</trd>
</drv>

<drv mrk="grafik.0a">
  <kap><tld/>a</kap>
  <snc mrk="grafik.0a.BELA">
    <dif>
      Koncernanta la prezentadon de <tld/>o:
      <ekz>
        la <tld/>aj artoj estas skribo, desegno, gravuro...;
      </ekz>
      <ekz>
        mi eklaboris en la tiama <tld lit="G"/>a Centro de UEA
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007988.html">Gerd Jacques</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        prezento teknika kaj <tld/>a
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007002.html">Muziko: Kanzonoj por
          interna konsumo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la tuta kajero estas bunta, ne nur pro koloraj fotoj sed anka&ubreve; pro <tld/>a
        aspekto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MAUL</aut>
          <vrk><url ref="&Monato;/2004/008384.html">Alveno al la moderna
          vivo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Atari</nom>-komputiloj estis speciale interesaj por <tld/>aj laboroj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Peeraerts</aut>
          <vrk><url ref="&Monato;/2004/008280.html">Komposta revolucio kaj presada
          stabileco</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a karto, al kiu eblas konekti plurajn ekranojn
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2008/009467.php">Pli komforte labori per
          komputilo</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">grafick&yacute;</trd>
  <trdgrp lng="de">
    <trd>grafisch</trd>,
    <trd>graphisch</trd>,
    <trd>zeichnerisch</trd>
  </trdgrp>
  <trd lng="en">graphic</trd>
  <trd lng="es">gr&aacute;fico<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="fr">graphique<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>grafikai</trd>,
    <trd>grafikus</trd>
  </trdgrp>
  <trd lng="nl">grafisch</trd>
  <trdgrp lng="pl">
    <trd>graficzny</trd>,
    <trd>rysunkowy</trd>
  </trdgrp>
  <trd lng="pt">gr&aacute;fico<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">grafick&yacute;</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="grafik.0ajxo">
  <kap><ofc>9</ofc><tld/>a&jcirc;o <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="grafik.0ajxo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Arta <ref tip="super" cel="desegn.0o.desegnajxo">desegno<sncref/></ref>:
      <ekz>
        valoraj arta&jcirc;oj &dash; inkluzive <tld/>a&jcirc;ojn kaj pentra&jcirc;ojn &dash; estas a&ubreve;kciitaj por
        lan&ccirc;i la iniciaton
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006940.html">Pollando: Korczak
          ridetanta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ludoj kun fajnaj kaj movi&gcirc;antaj tridimensiaj <tld/>a&jcirc;oj estas la &ccirc;efaj aferoj,
        kiuj povas utiligi la potencon de novaj procesoroj
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="&Steloj;monato2/007114.html">Intel alfrontas
          konkurencon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Grafik</trd>
  </snc>
  <snc mrk="grafik.0ajxo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Desegno abstrakta, servanta por ilustri te&hcirc;nikan a&ubreve; sciencan aferon:
      <ekz>
        la artikolo estas ri&ccirc;igita per statistikoj kaj <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2006/008820.php">Esperantologio reaperas</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o montras, por &ccirc;iu lando, la elcenton de junuloj, kiuj asertas paroli
        minimume unu fremdan lingvon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007130.html">Junuloj kaj
          lingvoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la tria plej granda grupo estas la liberaluloj kun 9,15&#x202F;%, iom anta&ubreve; la
        &leftquot;aliuloj&rightquot; kun 9,02&#x202F;% (vidu la apudan <tld/>a&jcirc;on)
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008365.html">Civitanoj perdas intereson pri
          E&ubreve;ropo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diagra.0o">diagramo</ref>,
      <ref cel="skem.0o">skemo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Grafik</trd>,
      <trd>grafische <ind>Darstellung</ind></trd>,
      <trd>grafisches <ind>Erzeugnis</ind></trd>
    </trdgrp>
    <trd lng="es">gr&aacute;fico<klr tip="ind"> (diagrama)</klr></trd>
    <trd lng="fr">graphique<klr tip="ind"> (sch&eacute;ma)</klr></trd>
    <trd lng="hu">grafikon</trd>
    <trd lng="nl">grafiek<klr tip="ind"> (schema)</klr></trd>
    <trdgrp lng="pl">
      <trd>graf</trd>,
      <trd>wykres</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_a;&c_f;&c_i;&c_k;</trd>,
      <trd>&c_ch;&c_je;&c_r;&c_t;&c_jo;&c_zh;</trd>,
      <trd>&c_s;&c_h;&c_je;&c_m;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grafik.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;152</lok></fnt>
    <dif>
      <klr>(de <ref cel="bild.0igo.MAT">bildigo</ref>
      <frm am="f"><k>f</k></frm>)</klr>
      Aro, konsistanta el la paroj
      <frm am="(x,f(x))">(<k>x</k>,<k>f</k>(<k>x</k>))</frm>
      por &ccirc;iu <frm am="x"><k>x</k></frm> en la fonto-aro de
      <frm am="f"><k>f</k></frm>.
    </dif>
    <ref tip="sin" cel="graf1.0o.grafikajxo">grafo</ref>
    <ref tip="vid" cel="prezen1.grafika0o"><tld/>a prezento</ref>
    <rim num="1">
      La&ubreve; la donita difino de <ref cel="bild.0igo.MAT">bildigo</ref>
      ne ekzistas esenca diferenco inter funkcio kaj &gcirc;ia <tld/>a&jcirc;o.
      Oni tamen konservas amba&ubreve; esprimojn pro la malsamaj intuiciaj
      kampoj, al kiuj ili referencas: <ctl>funkcio</ctl>
      parolas pri la maniero asocii du elementojn kaj
      <ctl><tld/>a&jcirc;o</ctl> referencas al eventuala <ref cel="kurb.0o.MAT">kurbo</ref>, figuranta la
      funkcion.
    </rim>
    <rim num="2" mrk="grafik.0ajxo.Sergio">
      La senco de la arteoria koncepto <ctl><tld/>a&jcirc;o</ctl> estas
      pli profunda ol la &ccirc;i-supra klarigo lerneja; <tld/>a&jcirc;o servas
      por prezenti funkcion a&ubreve; rilaton kiel unu pretan, tutan
      objekton
      (tio gravas ekz-e en kontrastigo de <ctl>aktuale
      malfinia</ctl> 
      sinuso kontraste al <ctl>potenciale malfinia</ctl>,
      la&ubreve;bezone komputebla sinuso).
      Pli matematikece oni devus paroli pri <tld/>a&jcirc;o de rilato,
      kio estas pli &gcirc;enerala koncepto; ekz-e anka&ubreve; la ekvacio
      <frm am="x^2+y^2=1">x&quadrat;+y&quadrat; = 1</frm> estas prezentebla arteorie
      per karteziaj
      duopoj, kvankam &gcirc;i ne difinas funkcion.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trd lng="de">Graph</trd>
    <trd lng="es">gr&aacute;fico<klr tip="ind"> (de una
    funci&oacute;n)</klr></trd>
    <trd lng="fr">graphe<klr tip="ind"> (d'une fonction)</klr></trd>
    <trd lng="hu">f&uuml;ggv&eacute;nyg&ouml;rbe</trd>
    <trd lng="nl">graaf<klr tip="ind"> (wisk.)</klr></trd>
    <trd lng="pl">wykres<klr tip="ind"> (funkcji)</klr></trd>
    <trd lng="pt">gr&aacute;fico<klr tip="amb"> (de uma
        fun&ccedil;&atilde;o)</klr></trd>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_k;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_f;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_a;&c_je; &c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>graf</trd>,
    <trd>diagram</trd>,
    <trd>grafick&eacute; zn&aacute;zorn&ecaron;n&iacute;</trd>,
    <trd>grafika</trd>
  </trdgrp>
  <trd lng="en">graph</trd>
  <trd lng="pt">gr&aacute;fico</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_a;&c_f;&c_ib;&#x43A;</trd>,
    <trd>&#x43A;&c_r;&c_je;&c_s;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="grafik.0isto">
  <kap><ofc>9</ofc><tld/>isto <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="grafik.0isto.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <tld lit="G"/>a <ref tip="super" cel="art.0isto">artisto</ref>; profesiulo pri <tld/>o:
      <ekz>
        la <tld/>isto &scirc;atas bildigi stratojn, parkojn, placojn de
        Sofio
        <fnt>
          <bib>Monato</bib>,
          <lok><url
          ref="http://tekstaro.com/pl/prenitutantekston.pl?nomo=artikoloj-el-monato&amp;tipo=&amp;sekcio=monatotri-007483&amp;antauasekcio=1&amp;postasekcio=1&amp;id=c10947&amp;ignorifremdajhojn=1&amp;uskle=1&amp;serchesprimo=grafikisto#c10947">Bulgario.
  Rava poezia mondo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Julia</nom> jam de kelkaj jaroj laboras kiel <tld/>istino
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2008/009487.php">Pri paeljoj kaj
          pentra&jcirc;oj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri tiu estetika malavanta&gcirc;o konsciis la <tld/>isto Franko <nom>Luin</nom>, kiu denove
        skanis multajn esperantlingvajn librojn
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2010/010009.php">Parto 1: Survoje al iu
          libro-normo</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto de la po&scirc;tmarko <klr>[&#8230;]</klr> gajnis la internacian <tld/>an
        konkurson
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010200.php">Nova Esperanto-po&scirc;tmarko</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Grafiker</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_k; <klr>(&c_m;&c_a;&c_s;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">grafik</trd>
  <trd lng="es">dise&ntilde;ador gr&aacute;fico</trd>
  <trd lng="fr">graphiste</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_f;&c_ib;&#x43A;&#xA0;(&c_h;&c_u;&c_d;&#x43E;&c_zh;&#x43D;&c_i;&#x43A;)</trd>
</drv>
