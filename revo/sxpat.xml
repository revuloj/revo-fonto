<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxpat.xml,v 1.30 2024/04/01 15:23:34 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>&scirc;pat</rad>/o  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxpat.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxpat.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="fos.0ilo">Fosilo</ref> konsistanta el lar&gcirc;a,
      plata klingo, havanta longan tenilon kaj uzata por tran&ccirc;i pecojn
      da tero dum &ccirc;irka&ubreve;fosado:
      <ekz>
        li revenis kun pio&ccirc;o kaj <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>22</lok>
        </fnt>;
      </ekz>
      <ekz>
        arkaika kultivmetodo, kies &ccirc;efa grundplugilo estas <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2006/008754.php"
              >Rizo: senelira problemo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="plug.0ileto.sxpato">plugileto</ref>
    <bld
      lok="&WCU;/thumb/0/0a/Historischer_Spaten_&quot;Eckardts_IDEAL_Kieler_Form&quot;%2C_Moderner_Spaten_&quot;IDEAL-Spaten_G&auml;rtner_Form&quot;.jpg/320px-Historischer_Spaten_&quot;Eckardts_IDEAL_Kieler_Form&quot;%2C_Moderner_Spaten_&quot;IDEAL-Spaten_G&auml;rtner_Form&quot;.jpg"
      prm="CC BY-SA 4.0"
      >Historia kaj moderna <tld/>o
        <fnt>
          <aut>Tanja M&uuml;nch</aut>
          <lok><url
          ref="&WCW;/File:Historischer_Spaten_%22Eckardts_IDEAL_Kieler_Form%22,_Moderner_Spaten_%22IDEAL-Spaten_G%C3%A4rtner_Form%22.jpg"
              >https://commons.wikimedia.org/wiki/File:Historischer_Spaten_&quot;Eckardts_IDEAL_Kieler_Form&quot;,_Moderner_Spaten_&quot;IDEAL-Spaten_G&auml;rtner_Form&quot;.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_d;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_ja;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Spaten</trd>
  <trd lng="el">&sigma;&kappa;&alfa;&pi;&alfa_ton;&ny;&jota;</trd>
  <trd lng="en">spade</trd>
  <trd lng="es">laya</trd>
  <trd lng="fr">b&ecirc;che</trd>
  <trd lng="hu">&aacute;s&oacute;</trd>
  <trd lng="it">vanga</trd>
  <trdgrp lng="ja">
    <trd>&#12377;&#12365;</trd>,
    <trd>&#39376;&#37604;  <pr>&#12385;&#12421;&#12358;&#12377;&#12365;</pr></trd>,
    <trd>&#37672;&#29226;  <pr>&#12356;&#12363;&#12426;&#12388;&#12417;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>spade</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szpadel</trd>,
    <trd>&lstroke;opata</trd>,
    <trd>rydel</trd>
  </trdgrp>
  <trd lng="pt">enxada</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_a; (&c_sh;&c_t;&c_i;&c_k;&c_o;&c_v;&c_a;)</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_a; (&c_ja;&c_k;&c_o;&c_r;&c_ja;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37740; <pr>qi&amacron;o</pr></trd>,
    <trd>&#38201; <pr>qi&amacron;o</pr></trd>,
    <trd>&#37732; <pr>ch&amacron;</pr></trd>,
    <trd>&#38200; <pr>ch&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxpat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxpat.0i.AGR">
    <dif>
      <ref tip="super" cel="fos.0i.tero">Fosi</ref> per <tld/>o:
      <ekz>
        la <tld/>ita grundo de la &gcirc;ardeno havis fortan odoron
        <fnt>
          <aut>Alain Fournier, trad. Roger Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 112</lok>
        </fnt>;
      </ekz>
      <ekz>
        fosante sian sulkon li ne rimarkis, ke li <tld/>as propran tombon
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fos.cxirkaux0i">&ccirc;irka&ubreve;fosi</ref>,
      <ref cel="fos.dis0i">disfosi</ref>,
      <ref cel="plug.0i">plugi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol; &c_r;&c_y;&c_d;&c_l;&c_jo;&c_w;&c_k;&c_a;&c_j;</trd>
  <trd lng="de">umgraben</trd>
  <trd lng="fr">b&ecirc;cher</trd>
  <trdgrp lng="ja">
    <trd>&#12377;&#12367;</trd>,
    <trd>&#12383;&#12364;&#12420;&#12377;</trd>
  </trdgrp>
  <trd lng="pl">kopa&cacute; <klr>  (np.&lstroke;opat&aogonek;</klr></trd>
  <trd lng="uk">&c_k;&c_o;&c_p;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
