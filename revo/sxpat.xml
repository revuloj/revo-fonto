<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>&scirc;pat</rad>/o  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxpat.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxpat.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Ilo konsistanta el lar&gcirc;a, plata kaj tran&ccirc;a lameno el 
      fero, havanta longan tenilon kaj uzata por plugi la teron per 
      homa brako;
      <ref tip="sin" cel="plug.0ileto.sxpato">plugileto</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_d;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>, 
    <trd>&c_l;&c_a;&c_p;&c_a;&c_t;&c_a;</trd>, 
    <trd>&c_zh;&c_a;&c_l;&c_ja;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>  
  <trd lng="de">Spaten</trd>
  <trd lng="el">&sigma;&kappa;&alfa;&pi;&alfa_ton;&ny;&jota;</trd>
  <trd lng="en">spade</trd>
  <trd lng="es">laya</trd>
  <trd lng="fr">b&ecirc;che</trd>
  <trd lng="hu">&aacute;s&oacute;</trd>
  <trd lng="it">vanga</trd>
  <trdgrp lng="nl">
    <trd>spade</trd>
  </trdgrp>
  <trd lng="pt">enxada</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
