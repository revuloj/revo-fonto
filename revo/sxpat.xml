<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>&scirc;pat</rad>/o
</kap>
<drv mrk="sxpat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Ilo konsistanta el lar&gcirc;a, plata kaj tran&ccirc;a lameno el fero, havanta
      longan tenilon kaj uzata por plugi la teron per homa brako;
      plugileto.
    </dif>
  </snc>
  <trd lng="de">Spaten</trd>
  <trdgrp lng="nl">
    <trd>spade</trd>
  </trdgrp>
  <trd lng="fr">b&ecirc;che</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
