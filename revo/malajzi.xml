<kap>
  <rad>Malajzi</rad>/o
</kap>

<drv mrk="malajzi.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">MY</mlg>
  <snc mrk="malajzi.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la sudorienta 
      <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kun &ccirc;efurbo Kuala-Lumpuro; federacio
      <tld/>o, limanta kun
      <ref tip="vid" cel="brunej.B0o.GEOG">Brunejo</ref>,
      <ref tip="vid" cel="indonezi.I0o.GEOG">Indonezio</ref>
      kaj <ref tip="vid" cel="taj.T0lando.GEOG">Tajlando</ref>:
      <ekz>
         <tld/>o estas lando kie lo&gcirc;as diversaj rasoj/etnoj, kaj la lando agnoskas &ccirc;ies rajtojn pri
         sia lingvo, religio, kulturo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9402-malajzio.txt">Malajzio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dum la vintraj monatoj (je Kristnasko kaj Silvestro) multaj homoj el
        meza kaj norda E&ubreve;ropo, kie mankas varmaj pla&gcirc;oj, fu&gcirc;as al sunaj
        marbordoj de ekzemple Tajlando, <tld/>o kaj Sri-Lanko
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008551.php">Ondego da helpemo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_a;&c_l;&c_a;&c_j;&c_z;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_M;&c_a;&c_l;&c_a;&c_j;&c_z;&c_i;&c_ja;</trd>
    <trd lng="ca">Mal&agrave;isia</trd>
    <trd lng="de">Malaysia</trd>
    <trd lng="en">Malaysia</trd>
    <trd lng="es">Malasia</trd>
    <trd lng="fr">Malaisie</trd>
    <trd lng="id">Malaysia</trd>
    <trd lng="nl">Maleisi&euml;</trd>
    <trdgrp lng="pl">
      <trd>Malezja</trd>,
      <trd>Federacja <ind>Malezji</ind></trd>
    </trdgrp>
    <trd lng="pt">Mal&aacute;sia</trd>
  </snc>
  <trd lng="uk">&#x41C;&#x430;&#x43B;&#x430;&#x439;&#x437;&#x456;&#x44F;</trd>
</drv>

</art>

