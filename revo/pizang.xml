<art mrk="$Id: pizang.xml,v 1.16 2023/09/03 12:45:08 revo Exp $">
  <kap>
    <rad>pizang</rad>/o <fnt>Z</fnt>
  </kap>
<drv mrk="pizang.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malaja nomo de la <ref tip="sin" cel="banan.0o">banano</ref>:
      <ekz>
         la deserta banano (kun multe da kulturataj varioj) kaj la <tld/>o, anka&ubreve; nomata &leftquot;amela
         banano&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27293">Banano</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <rim>
    La&ubreve; Vikipedio <tld/>o estas aparte la nedol&ccirc;aj, amelri&ccirc;aj specoj.
  </rim>
  <trd lng="af">piesang</trd>
  <trd lng="cs">ban&aacute;n obecn&yacute; <klr>(palma)</klr></trd>
  <trdgrp lng="de">
    <trd>Banane</trd>,
    <trd>Kochbanane</trd>,
    <trd>Pisangfeige</trd>
  </trdgrp>
  <trd lng="en">piesang</trd>
  <trdgrp lng="es">
    <trd>banana</trd>,
    <trd>pl&aacute;tano</trd>
  </trdgrp>
  <trd lng="fr">pisang</trd>
  <trd lng="hu">piz&aacute;ng</trd>
  <trd lng="id">pisang</trd>
  <trd lng="ja">&#x30D0;&#x30CA;&#x30CA;&#x306E;&#x4E00;&#x7A2E;  [<ind>&#x3070;&#x306A;&#x306A;&#x306E;&#x3044;&#x3063;&#x3057;&#x3085;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>banaan</trd>,
    <trd>pisang</trd>
  </trdgrp>
  <trd lng="pl">banan</trd>
  <trd lng="pt">pisang</trd>
  <trd lng="ro">banan&abreve;</trd>
  <trd lng="sk">ban&aacute;n oby&ccaron;ajn&yacute;
  <klr>(rastlina)</klr></trd>
  <trd lng="uk">&#x43C;&#x430;&#x43B;&#x430;&#x439;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x431;&#x430;&#x43D;&#x430;&#x43D;</trd>
</drv>
