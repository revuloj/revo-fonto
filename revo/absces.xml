<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>absces</rad>/o
</kap>
<drv mrk="absces.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Amaso da puso, kolekti&gcirc;inta en kava&jcirc;o
      meze de &ccirc;irka&ubreve;antaj histoj.
    </dif>
    <refgrp tip="sub">
      <ref cel="furunk.0o">furunko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>abces</trd>,
    <trd>etterbuil</trd>,
    <trd>ettergezwel</trd>
  </trdgrp>
  <trd lng="de">Abszess</trd>
  <trd lng="fr">abc&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;lyog</trd>,
    <trd>kel&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_r;&c_y;&c_v;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_c;&c_je;&c_s;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abscesso</trd>,
    <trd>apostema</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>b&ouml;ld</trd>,
    <trd>bulnad</trd>
  </trdgrp>
</drv>
</art>
