<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: absces.xml,v 1.35 2024/01/06 10:30:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>absces</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="absces.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="absces.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Amaso da <ref tip="prt" cel="pus.0o">puso</ref>, kolekti&gcirc;inta en kava&jcirc;o
      meze de &ccirc;irka&ubreve;antaj histoj:
      <ekz>
        sur la loko de la <tld/>o estos blanka &scirc;vela&jcirc;o a&ubreve; ru&gcirc;ete-blanka makulo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas infekta&jcirc;o de lepro, &gcirc;i ekfloris en la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas fermita ankora&ubreve; tubero kun puso, <ref tip="vid" cel="ulcer.0o">ulcero</ref>
        estas malfermita vundo, kiu pusas kaj ne povas rapide sani&gcirc;i,
        <ref tip="vid" cel="furunk.0o">furunko</ref>
        estas malgranda tubereto sur la ha&ubreve;to kun iom da enhavo simila al puso
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE1D;&#xE35;</trd>
    <trdgrp lng="zh">
    <trd>&#x6E83;&#x75A1; <pr>ku&#xEC;y&#xE1;ng</pr></trd>,
    <trd>&#x6F70;&#x760D; <pr>ku&#xEC;y&#xE1;ng</pr></trd>,
    <trd>&#33043;&#x75AE; <pr>n&#xF3;ngchu&amacron;ng</pr></trd>,
    <trd>&#x81BF;&#30241; <pr>n&#xF3;ngchu&amacron;ng</pr></trd>,
    <trd>&#33043;&#x80BF; <pr>n&#xF3;ngzh&#x1D2;ng</pr></trd>,
    <trd>&#x81BF;&#x816B; <pr>n&#xF3;ngzh&#x1D2;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_c;&c_e;&c_s;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_y;&c_w;</trd>
  </trdgrp>
  <trd lng="br">gor</trd>
  <trdgrp lng="cs">
    <trd>absces</trd>,
    <trd>bol&aacute;k</trd>,
    <trd>hl&iacute;za</trd>,
    <trd>ne&zcaron;it</trd>,
    <trd>v&rcaron;ed</trd>
  </trdgrp>
  <trd lng="de">Abszess</trd>
  <trd lng="en">abcess</trd>
  <trd lng="es">absceso</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_s;&a_h;</trd>,
    <trd>&a_d;&a_m;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">abc&egrave;s</trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5BC;&#x5E8;&#x5B0;&#x5E1;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;lyog</trd>,
    <trd>kel&eacute;s</trd>
  </trdgrp>
  <trd lng="it">ascesso</trd>
  <trdgrp lng="ja">
    <trd>&#x81BF;&#x760D;  <pr>&#x306E;&#12358;&#12424;&#12358;</pr></trd>,
    <trd/>,
    <trd>&#12391;&#x304D;&#12418;&#x306E;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>abces</trd>,
    <trd>etterbuil</trd>,
    <trd>ettergezwel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ropie&nacute;</trd>,
    <trd>wrz&oacute;d</trd>
 </trdgrp>
  <trdgrp lng="pt">
    <trd>abscesso</trd>,
    <trd>apostema</trd>
  </trdgrp>
  <trd lng="ro">peptic</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_r;&c_y;&c_v;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_c;&c_je;&c_s;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lo&zcaron;isko hnisu</trd>,
    <trd>vred</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>b&ouml;ld</trd>,
    <trd>bulnad</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_r;&c_i;&c_v;</trd>,
    <trd>&c_g;&#x43D;&#x43E;&#x44F;&#x43A;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_c;&c_je;&c_s;</trd>
  </trdgrp>
</drv>
