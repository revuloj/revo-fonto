<kap>
<rad>filokser</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="filoks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Malgranda insekto el la familio <ref tip="malprt" cel="filoks1.0oj"><tld/>edoj</ref> de la
      samnoma genro kaj parencaj, i.a.
      <trd lng="la">Daktulosphaira vitifoliae</trd>, sin. <trd lng="la">Viteus vitifoliae</trd>,
      kiu detruas vinarbedojn:
      <ekz>
        &gcirc;is anta&ubreve; du jarcentoj anka&ubreve; en la regiono de <nom>Buda</nom> estis kultivataj vinberujoj:
        epidemio <klr>[de]</klr> <tld/>o ilin detruis
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2007/009351.php">Je via sano!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/75/Reblaus_Blattgallen.JPG/640px-Reblaus_Blattgallen.JPG" prm="CC BY 3.0 de">gajloj (ovujoj) de <tld/>o sur malsupra flanko de vinfolioj
        <fnt>
          <aut>Joachim Schmid, propra foto</aut>
          <lok><url ref="&WCW;/File:Reblaus_Blattgallen.JPG">&WCW;/File:Reblaus_Blattgallen.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_k;&c_s;&c_e;&c_r;&c_a;</trd>
  <trd lng="ca">fil&middot;loxera</trd>
  <trdgrp lng="cs">
    <trd>fyloxera</trd>,
    <trd>r&eacute;vokaz</trd>
  </trdgrp>
  <trd lng="de">Reblaus</trd>
  <trd lng="fr">phyllox&eacute;ra<klr tip="ind"> (insecte)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&odblac;l&odblac;tet&udblac;</trd>,
    <trd>filox&eacute;ra</trd></trdgrp>
  <trd lng="ja">&#x30CD;&#x30A2;&#x30D6;&#x30E9;&#x30E0;&#x30B7;</trd>
  <trd lng="pt">filoxera</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_l;&c_o;&c_k;&c_s;&c_je;&c_r;&c_a;</trd>
  <trd lng="sk">fylox&eacute;ra</trd>
  <trd lng="uk">&#x444;&#x456;&#x43B;&#x43E;&#x43A;&#x441;&#x435;&#x440;&#x430;</trd>
</drv>

