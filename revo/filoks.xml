<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filokser</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="filoks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="filoks.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Phylloxera</trd>, <trd lng="la">Viteus</trd>),
      insekto detruanta vinarbedojn.
    </dif>
  </snc>
  <trd lng="de">Reblaus</trd>
</drv>
<drv mrk="filoks.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="homopt.0oj">homopteroj</ref>
      (<trd lng="la">Phylloxeridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Zwergl&auml;se</trd>
</drv>
</art>
