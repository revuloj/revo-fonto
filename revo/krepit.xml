<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: krepit.xml,v 1.4 2025/04/01 15:31:44 afido Exp $">
    <kap>
      <rad>krepit</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="krepit.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="krak.0eti">Kraketadi</ref>:
      <ekz>
        malanta&ubreve;e sur kredenco fritujoj <tld/>is kaj &ccirc;ino gutigis grason sur la viandon
        <fnt>
          <vrk><url ref="https://www.rulit.me/books/kiuj-inter-read-258425-7.html">Kiuj inter&scirc;an&gcirc;is
          vivon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pulma kongesteco, li diris, sin rektigante, <tld/>antaj bruoj en la bazo
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sibl.0i.brue">sibli<sncref/></ref>,
      <ref cel="susur.0i">susuri</ref>,
      <ref cel="sxusx.0i">&scirc;u&scirc;i</ref>
    </refgrp>
    <trd lng="de">knistern</trd>
    <trd lng="en">crackle</trd>
    <trd lng="fr">cr&eacute;piter</trd>
    <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kraketi</ke>.
  </rim>

  <trd lng="be">&c_h;&c_r;&c_a;&c_b;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&#1086;&c_t;&c_r;&c_ib;&c_s;&#1082;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_r;&c_u;&#1084;&c_t;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

  </art>

