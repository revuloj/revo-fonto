<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: krepit.xml,v 1.2 2022/03/22 15:44:13 afido Exp $">
    <kap>
      <rad>krepit</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="krepit.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="krak.0eti">Kraketadi</ref>:
      <ekz>
        malanta&ubreve;e sur kredenco fritujoj <tld/>is kaj &ccirc;ino gutigis grason sur la viandon
        <fnt>
          <vrk><url ref="https://www.rulit.me/books/kiuj-inter-read-258425-7.html">Kiuj inter&scirc;an&gcirc;is vivon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pulma kongesteco, li diris, sin rektigante, <tld/>antaj bruoj en la bazo
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sibl.0i.brue">sibli<sncref/></ref>,
      <ref cel="susur.0i">susuri</ref>,
      <ref cel="sxusx.0i">&scirc;u&scirc;i</ref>
    </refgrp>
    <trd lng="de">knistern</trd>
    <trd lng="en">crackle</trd>
    <trd lng="fr">cr&eacute;piter</trd>
    <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x442;&#x440;&#x456;&#x441;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x445;&#x440;&#x443;&#x43C;&#x442;&#x456;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

  </art>

