<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tempe.xml,v 1.2 2023/03/27 11:24:45 revo Exp $">
    <kap>
      <rad>tempe</rad>/o
    </kap>

<drv mrk="tempe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref>
      el fermentintaj <ref cel="soj.0o">sojfabo</ref>j:
      <ekz>
        vegetaranoj, aparte la pli rigoraj, ofte uzas sojofabojn kaj diversajn
        produkta&jcirc;ojn el tiuj faboj (ekz. tofuo, <tld/>o, mizo).
       <fnt>
          <aut>Krys Ungar</aut>
          <vrk><url ref="&Monato;/0000/004583.php">
          Vivu vigle kaj vegetare!</url></vrk>
          <bib>Monato</bib>
          <lok>1995:7, p. 18a</lok>
        </fnt>;
      </ekz>
      <ekz>
        fakte <tld/>o estas speco de <ref cel="tofu.0o">tofuo</ref>
        adaptita al la tropika klimato de Indonezio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=79374"
              >Indonezia kuirarto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tempeh</trd>
    <trd lng="en">tempeh</trd>
    <trd lng="id">tempe</trd>
  </snc>
</drv>

  </art>

