<vortaro>
<art mrk="$Id$">
<kap>
<rad>akonit</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="akonit.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="ranunk.0acoj">ranunkolacoj</ref>
       (<trd lng="la">Aconitum</trd>), tre venena herbo kun bluaj a&ubreve;
       flavaj kaskoformaj floroj.
     </dif>
   </snc>
   <trd lng="de">Eisenhut</trd>
   <trd lng="en">aconite, friars's caps monk's hood, wolf's bane</trd>
   <trdgrp lng="en">
     <trd>aconite</trd>,
     <trd>friars's caps</trd>,
     <trd>monk's hood</trd>,
     <trd>wolf's bane</trd>
   </trdgrp>
   <trd lng="fr">aconit</trd>
   <trdgrp lng="ru">
      <trd>&#1072;&#1082;&#1086;&#1085;&#1080;&#1090;</trd>,
      <trd>&#1073;&#1086;&#1088;&#1077;&#1094;</trd>
   </trdgrp>
</drv>
</art>
