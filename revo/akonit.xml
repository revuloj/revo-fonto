<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akonit.xml,v 1.27 2023/08/30 18:23:31 revo Exp $">
<kap>
  <rad>akonit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="akonit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Aconitum</trd>); tre venena 
      <ref tip="lst" cel="herb.0o.BOT" lst="voko:ranunkoloparencoj">herbo</ref> 
      kun bluaj, flavaj a&ubreve; blankaj kaskoformaj floroj kaj
      alterne a&ubreve; baze sidantaj ordinare manforme dividitaj folioj:
      <ekz>
        la blua <tld/>o a&ubreve; blua kaskofloro estas <klr>[&#x2026;]</klr> tradicia ornama kaj
        medicina planto, &ccirc;iuj partoj estas forte venenaj, jam du gramoj de la radiko
        estas mortiga
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=592834">Aconitum napellus</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj konataj alkaloidoj: akonitino de <tld/>o, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19017">Alkaloido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/70/Aconitum_japonicum_var._montanum_01.jpg/386px-Aconitum_japonicum_var._montanum_01.jpg" prm="CC BY 4.0">Japana <tld/>o (Aconitum japonicum var. montanum)
        <fnt>
          <aut>&Sigma;64, propra verko</aut>
          <lok><url ref="&WCW;/File:Aconitum_japonicum_var._montanum_01.jpg">&WCW;/File:Aconitum_japonicum_var._montanum_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_n;&c_ib;&c_t;</trd>
  <trd lng="de">Eisenhut</trd>
  <trdgrp lng="en">
    <trd>aconite</trd>,
    <trd>friars's caps</trd>,
    <trd>monk's hood</trd>,
    <trd>wolf's bane</trd>
  </trdgrp>
  <trd lng="fr">aconit</trd>
  <trdgrp lng="hu">
    <trd>farkasgy&ouml;k&eacute;r</trd>,
    <trd>sisakvir&aacute;g</trd>
  </trdgrp>
  <trd lng="ja">&#x30C8;&#x30EA;&#x30AB;&#x30D6;&#x30C8;</trd>
  <trd lng="nl">akoniet</trd>
  <trd lng="pt">ac&ocirc;nito</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_o;&c_n;&c_i;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_je;&c_c; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sv">stormhatt</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43A;&#x43E;&#x43D;&#x456;&#x442;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x442;&#x43E;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
