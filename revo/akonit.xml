<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akonit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="akonit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Aconitum</trd>), tre venena herbo kun bluaj a&ubreve;
      flavaj kaskoformaj floroj.
    </dif>
  </snc>
  <trd lng="de">Eisenhut</trd>
  <trdgrp lng="en">
    <trd>aconite</trd>,
    <trd>friars's caps</trd>,
    <trd>monk's hood</trd>,
    <trd>wolf's bane</trd>
  </trdgrp>
  <trd lng="fr">aconit</trd>
  <trdgrp lng="hu">
    <trd>farkasgy&ouml;k&eacute;r</trd>,
    <trd>sisakvir&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">akoniet</trd>
  <trd lng="pt">ac&ocirc;nito</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_o;&c_n;&c_i;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_je;&c_c; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sv">stormhatt</trd>
</drv>
</art>
