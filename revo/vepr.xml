<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vepr</rad>/o
</kap>
<drv mrk="vepr.0o">
  <kap><tld/>o</kap>
  <snc mrk="vepr.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Arbustaro, arbustotufo, dornejo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_s;&c_t;&c_y;
    <ind>&c_h;&c_m;&c_y;&c_z;&c_mol;&c_n;&c_ja;&c_k;</ind></trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>esbarzer</trd>,
    <trd>bardissa</trd>,
    <trd>romeguera</trd>
  </trdgrp>
  <trd lng="de">Gestr&uuml;pp</trd>
  <trd lng="el">&theta;&alfa_ton;&my;&ny;&omikron;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>broussaille</trd>,
    <trd>fourr&eacute;</trd>,
    <trd>halliers</trd>,
    <trd>ronceraie</trd>,
    <trd>taillis</trd>
  </trdgrp>
  <trd lng="hu">boz&oacute;t</trd>
  <trdgrp lng="la">
    <trd>dumetum</trd>,
    <trd>frutectum</trd>,
    <trd>vepres</trd>
  </trdgrp>
  <trd lng="nl">doornbos</trd>
  <trd lng="pt">moita</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_s;&c_t;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_s;&c_l;&c_i;</trd>,
    <trd>&c_ch;&c_a;&c_shch;&c_a;</trd>
  </trdgrp>
</drv>
</art>
