<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vepr.xml,v 1.27 2023/10/21 16:50:36 revo Exp $">
<kap>
  <rad>vepr</rad>/o
</kap>
<drv mrk="vepr.0o">
  <kap><tld/>o</kap>
  <snc mrk="vepr.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="super" cel="dens.0ajxo.HOR">Densa&jcirc;o</ref> de kelkaj sova&gcirc;aj, dornaj arbustoj:
      <ekz>
        ka&scirc;ita sub teraso, jen tie sub densa <tld/>o
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        li falis de proksimume 150-metra alteco, kaj neniu arbo, neniu <tld/>o, neniu
        herbejo povis moligi la baton
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_s;&c_t;&c_y;
    <ind>&c_h;&c_m;&c_y;&c_z;&c_mol;&c_n;&c_ja;&c_k;</ind></trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>esbarzer</trd>,
    <trd>bardissa</trd>,
    <trd>romeguera</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hou&scaron;tina</trd>,
    <trd>hou&scaron;t&iacute;</trd>,
    <trd>k&rcaron;ov&iacute;</trd>,
    <trd>trn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gestr&uuml;pp</trd>,
    <trd>Geb&uuml;sch</trd>
  </trdgrp>
  <trd lng="el">&theta;&alfa_ton;&my;&ny;&omikron;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;pineux<klr> (subst.)</klr></trd>,
    <trd>fourr&eacute;<klr> (subst.)</klr></trd>,
    <trd>buisson<klr> (&eacute;pineux)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scrub</trd>,
    <trd>brush</trd>,
    <trd>copse</trd>
  </trdgrp>  
  <trd lng="hu">boz&oacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x8302;&#x307F;  <pr>&#x3057;&#x3052;&#x307F;</pr></trd>,
    <trd>&#x3084;&#x3076;</trd>
  </trdgrp>
  <trdgrp lng="la">
    <trd>dumetum</trd>,
    <trd>frutectum</trd>,
    <trd>vepres</trd>
  </trdgrp>
  <trd lng="nl">doornbos</trd>
  <trdgrp lng="pl">
    <trd>zaro&sacute;la</trd>,
    <trd>chaszcze</trd>
  </trdgrp>
  <trd lng="pt">moita</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_s;&c_t;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_s;&c_l;&c_i;</trd>,
    <trd>&c_ch;&c_a;&c_shch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krovie</trd>,
    <trd>t&racute;nie</trd>,
    <trd>t&racute;nina</trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_a;&c_g;&c_a;&c_r;&#x43D;&c_i;&#x43A;&#x43E;&c_v;&c_i;&c_j; &#x43A;&c_u;&c_shch;</trd>
</drv>

<drv mrk="vepr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="vepr.0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Loko dense surkreskata de <tld/>oj:
      <ekz>
        <klr>[mi]</klr> pasigis horojn ka&scirc;ita en <tld/>ejo por foti sova&gcirc;ajn bestojn
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        aperas anta&ubreve; vi malalta dornoza <tld/>ejo kaj leporoj
        eksaltas inter viaj piedoj
        <fnt>
          <aut>V. Glazunov</aut>,
          <vrk><url ref="http://www.geocities.com/noveloj/17.html">
           Belulino Ajsul&uacute;</url></vrk>,
          <lok>en: La sfinkso de Steplando, 1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="dens.0ejo">densejo</ref>
    <ref tip="sub" cel="makis.0o.GEOG">makiso</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_sh;&c_ch;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>esbarzerar</trd>,
    <trd>bardissar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hou&scaron;tina</trd>,
    <trd>k&rcaron;ovina</trd>,
    <trd>k&rcaron;ovi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unterholz</trd>,
    <trd>Buschwerk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>broussaille</trd>,
    <trd>ronceraie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boz&oacute;tos</trd>,
    <trd>csalit</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>h&uacute;&scaron;tina</trd>,
    <trd>krovie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_s;&c_t;&c_i;&c_j; &c_ch;&c_a;&c_g;&c_a;&c_r;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&c_z;&c_a;&c_r;&#x43E;&c_s;&c_t;&c_ib;</trd>,
    <trd>&c_h;&c_a;&c_shch;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
