<kap>
  <ofc>2</ofc>
  <rad>a&ubreve;tobiografi</rad>/o
</kap>
<drv mrk="auxtob.0o">
  <kap><ofc>2</ofc> <tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc mrk="auxtob.0o.membiografio">
    <dif>
      <ref tip="dif" cel="biogra1.mem0o">Membiografio</ref>:
      <ekz>
        intertempe jam aperis du pliaj <tld/>oj de la sama
        a&ubreve;toro
        <fnt><aut>Julian Modest</aut>: <vrk><url ref="&Monato;/1995/monato199507.pdf#page=22">Sincere kaj
          honeste pri si
          mem (Gerrit Berveling: La unuaj 25 jaroj de mia memoro)</url></vrk>,
          <bib>Monato</bib>
          <lok>1995/07, p. 22</lok></fnt>;
      </ekz>
      <ekz>
        la libro enhavas elementon de <tld/>o: <klr>[&#x2026;]</klr> la verkisto skribis pri la
        spertoj de li a&ubreve; de liaj pacientoj
        <fnt>

          <aut>Miland Joshi</aut>
          <vrk><url ref="&Monato;/2008/009520.php">Nevideblaj mondoj</url></vrk>
          <bib>Monato</bib>
          <lok>2008/07, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ui belan artisman <tld/>an romanon
        <fnt>

          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2005/008629.php">Ne nur intimaj temoj</url></vrk>
          <bib>Monato</bib>
          <lok>2005/07, p. 32</lok>
        </fnt>;
      </ekz>
      <ekz>
        surbaze de <tld/>a materialo
        <fnt>

          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/007930.html">Interna pejza&gcirc;o de alia virino</url></vrk>
           <bib>Monato</bib>
          <lok>2004/05, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4EB2;&#x7B14; <pr>q&#x12B;nb&#x1D0;</pr></trd>,
    <trd>&#x89AA;&#x7B46; <pr>q&#x12B;nb&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_b;&c_ib;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
