<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>korekt</rad>/i
</kap>
<drv mrk="korekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Re&gcirc;ustigi la erarojn a&ubreve; rebonigi la difektojn de iu a&ubreve; io:
      <ekz>
        sin <tld/>i;
      </ekz>
      <ekz>
        <tld/>i taskon de lernanto, presprova&jcirc;on, la prononcon de
        fremdulo, kalkulon, fotografian kopion;
      </ekz>
      <ekz>
        pli facile estas malbonon eviti ol
        <tld/>i<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plia esploro <tld/>is mian opinion.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bon.re0igi">rebonigi</ref>,
      <ref cel="tusx.re0i">retu&scirc;i</ref>,
      <ref cel="amend">amendamenti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>berichtigen</trd>,
     <trd>verbessern</trd>,
     <trd>korrigieren</trd>
  </trdgrp>
  <trd lng="en">correct</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago, rimarko de <tld/>anto:
      <ekz>
        <tld/>oj en presprova&jcirc;o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Berichtigung</trd>,
     <trd>Verbesserung</trd>,
     <trd>Korrektur</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Enhavanta neniun eraron a&ubreve; difekton:
      <ekz>
        <tld/>a eldono, teksto, desegno;
      </ekz>
      <ekz>
        la <tld/>eco de la lingvo devus esti la unua zorgo de &ccirc;iu verkisto;
      </ekz>
      <ekz>
        por<tld/>a tabelo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>richtig</trd>,
     <trd>fehlerfrei</trd>,
     <trd>korrekt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Enhavanta erarojn
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="makul.0i">makuli</ref>,
      <ref cel="bel">malbeligi</ref>,
      <ref cel="bon.mal0igi">malbonigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>falsch</trd>,
     <trd>verkehrt</trd>,
     <trd>fehlerhaft</trd>,
     <trd>inkorrekt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i preserarojn en presejo.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Korrektor</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
</drv>

<drv mrk="korekt.pun0i">
  <kap>pun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provi plibonigi ies konduton per puno; edukpuni:
      <ekz>
        pun<tld/>i malican infanon;
      </ekz>
      <ekz>
        pun<tld/>ejo.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="korekt.sur0i">
  <kap>sur<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="K"/>i skribante &gcirc;ustan formon sur ne&gcirc;ustan:
      <ekz>
        estas malpermesite sur<tld/>i oficialajn dokumentojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>ausbessern</trd>,
     <trd>&uuml;berschreiben</trd>
  </trdgrp>
</drv>
</art>
