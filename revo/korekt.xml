<kap><ofc>*</ofc>
<rad>korekt</rad>/i</kap>
<drv mrk="korekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="sin" cel="gxust.0igi">&Gcirc;ustigi</ref> la erarojn 
      a&ubreve; <ref cel="bon.re0igi">rebonigi</ref> la difektojn de iu a&ubreve; io:
      <ekz>
        la plua interparolado iris ne tiel bone, &ccirc;iuminute oni penis <tld/>i unu la alian
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi permesis al mi nur <tld/>i la preserarojn
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi scias desegni, diris Marta, tio estas, &scirc;i <tld/>is 
        <klr>(<ref cel="preciz.0igi">precizigi</ref>)</klr> sin rapide, mi scias la
        desegnadon en sufi&ccirc;a grado, por povi pretigi <klr>[&#8230;]</klr> modelojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li vidis presi en unu loko, <tld/>i en alia, komposti en tiu &ccirc;i, reformi en tiu
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        nesufi&ccirc;an la&ubreve;tecon povas <tld/>i la la&ubreve;tigilo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli facile estas malbonon eviti ol <tld/>i
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amend">amendamenti</ref>,
      <ref cel="tusx.re0i">retu&scirc;i</ref>,
      <ref cel="revizi.0i">revizii</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_ya;&t_naro;&t_tsheg;&t_ba;&t_sa;&t_rata;&t_nga;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trd lng="ca">corregir</trd>
  <trdgrp lng="cs">
    <trd>korigovat</trd>,
    <trd>napravovat</trd>,
    <trd>opravit</trd>,
    <trd>opravovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>berichtigen</trd>,
    <trd>verbessern</trd>,
    <trd>korrigieren</trd>
  </trdgrp>
  <trd lng="en">correct</trd>
  <trd lng="es">corregir</trd>
  <trdgrp lng="fr">
    <trd>corriger</trd>,
    <trd>rectifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kijav&iacute;t</trd>,
    <trd>helyesb&iacute;t</trd>
    <trd>korrig&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbeteren</trd>,
    <trd>corrigeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>korygowa&cacute;</trd>,
    <trd>poprawia&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>corrigir</trd>,
    <trd>retificar</trd>,    
    <trd>emendar</trd>
  </trdgrp>
  <trd lng="ro">corecta</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,  
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korigova&tcaron;</trd>,
    <trd>opravi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago, rimarko de <tld/>anto:
      <ekz>
        <tld/>o de kelkaj eraroj de la unua eldono
        <fnt>
          <bib>LR</bib>
          <lok>Tria Parto &ndash; Jurisprudenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> kritikis, postulante <tld/>ojn a&ubreve; poluron de kelkaj versoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj eventualaj <tld/>oj eble ne &ccirc;iujn kontentigos
        <fnt>
          <bib>Zam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de kroataj limoj rilatis ne al serboj sed <klr>[&#8230;]</klr> al tiu
        parto, kiu inter la du mondomilitoj apartenis al Italio
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Steloj;monato2/006811.html"
              >Reago: Nekredeblaj malveroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu skriba&jcirc;o inspirita de Dio estas anka&ubreve; utila por instruo, por admono, por
        <tld/>o, por disciplino
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 3:16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_r;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>correcci&oacute;</trd>,
    <trd>rectificaci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korekce</trd>,
    <trd>korektura</trd>,
    <trd>oprava</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Berichtigung</trd>,
    <trd>Verbesserung</trd>,
    <trd>Korrektur</trd>
  </trdgrp>
  <trd lng="es">correcci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>correction</trd>,
    <trd>rectification</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kijav&iacute;t&aacute;s</trd>,
    <trd>helyesb&iacute;t&eacute;s</trd>,
    <trd>korrekci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbetering</trd>,
    <trd>correctie</trd>
  </trdgrp>
  <trd lng="pl">korekta</trd>     
  <trd lng="ro">corectare</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korekt&uacute;ra</trd>,
    <trd>oprava</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0a">
  <kap><tld/>a</kap>
  <snc mrk="korekt.0a.gxustiga">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Rilata al <tld/>(ad)o:
      <ekz>
        por<tld/>a tabelo;
      </ekz>
      <ekz>
        se <klr>[&#8230;]</klr> instruisto faras <tld/>an laboron, tio signifas, ke li
        okupi&gcirc;as pri <tld/>ado de taskoj faritaj de siaj lernantoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj reguloj de presejo
        <fnt><bib>PIV3</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a domo (establejo, kie oni provas <tld/>i deliktulojn)
        <fnt><bib>PIV3</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>correctiu</trd>,
      <trd>correccional</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>verbessernd</trd>,
      <trd>korrigierend</trd>
    </trdgrp>
    <trd lng="en">corrective</trd>
    <trdgrp lng="fr">
      <trd>correctionnel</trd>,
      <trd>correctif</trd>
    </trdgrp>
    <trdgrp lng="nl">
    <trd>corrigerend</trd>,
    <trd>correctioneel</trd>
  </trdgrp>
  </snc>
  <snc mrk="korekt.0a.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Enhavanta plu neniun eraron a&ubreve; difekton; 
      <ref tip="dif" cel="gxust.0a">&gcirc;usta</ref>:
      <ekz>
        <tld/>a prononcado de la vokaloj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        via ortografio ne estas absolute <tld/>a
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        via desegno <klr>[&#8230;]</klr> ne estas sufi&ccirc;e <tld/>a, nek sufi&ccirc;e preciza, ne
        redonas sufi&ccirc;e la ideon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la intervidi&gcirc;o estis <tld/>a <klr>(kp <ref cel="formal.0a.lauxkutima">formala</ref>)</klr>
        sed malvarma
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i a&ubreve;skultis kun rideto iom senmova la <tld/>ajn amkonfesojn, kiujn la juna duko
        sin kredis devigata fari al &scirc;i
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor.0a.KOMUNE">konforma<sncref/></ref>,
      <ref cel="regul.laux0a">la&ubreve;regula</ref>,
      <ref cel="erar.sen0a">senerara</ref>,
      <ref cel="mank.sen0a">senmanka</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>corregit</trd>,
      <trd>revisat</trd>,      
      <trd>lliure d'errates</trd>
    </trdgrp>
      <trdgrp lng="de">
      <trd>richtig</trd>,
      <trd>fehlerfrei</trd>,
      <trd>korrekt</trd>
    </trdgrp>
    <trd lng="es">correcto</trd>
    <trd lng="nl">correct<klr tip="ind"> (ontr.)</klr></trd>
    <adm>
      Tiu &ccirc;i malkonsekvenca uzo de &leftquot;korekta&rightquot; estas tiel vasti&gcirc;inta, ke efektive oni devus krei apartan
      artikolon kun homonimo. Oni apena&ubreve; povas esperi, ke la parolantoj iam
      rezignos tiun konfuzan dunaturecon de korekt/
      <aut>WD</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezvadn&yacute;</trd>,
    <trd>korek&ccaron;n&iacute;</trd>,
    <trd>n&aacute;pravn&yacute;</trd>,
    <trd>opravn&yacute;</trd>,
    <trd>spr&aacute;vn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helyes</trd>,
    <trd>korrekt</trd>
  </trdgrp>
  <trd lng="pl">bezb&lstroke;&eogonek;dny</trd>     
  <trd lng="ro">corect</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezchybn&yacute;</trd>,
    <trd>korektn&yacute;</trd>,
    <trd>spr&aacute;vny</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Enhavanta erarojn, <ref tip="dif" cel="gxust.mal0a">mal&gcirc;usta</ref>:
      <ekz>
        formo <klr>[&#8230;]</klr> gramatike mal<tld/>a
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        li volis <tld/>i la tekstojn de la eposoj de Homero, &ccirc;ar li opiniis ilin
        mal<tld/>aj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        historiaj falsa&jcirc;oj, <klr>[&#8230;]</klr> mal<tld/>aj informoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002,  &#8470; 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="makul.0i">makuli</ref>,
      <ref cel="bel">malbeligi</ref>,
      <ref cel="bon.mal0igi">malbonigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_y;&c_l;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>erroni</trd>,
    <trd>incorrecte</trd>
  </trdgrp>
  <trd lng="cs">nespr&aacute;vn&yacute;</trd>
  <trdgrp lng="de">
    <trd>falsch</trd>,
    <trd>verkehrt</trd>,
    <trd>fehlerhaft</trd>,
    <trd>inkorrekt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>err&oacute;neo</trd>,
    <trd>equivocado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>incorrect</trd>,
    <trd>erron&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helytelen</trd>,
    <trd>hib&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fout<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>niet <ind>gecorrigeerd</ind><klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&lstroke;&eogonek;dny</trd>,
    <trd>niepoprawny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>gre&#537;it</trd>,
    <trd>incorect</trd>,
    <trd>fals</trd>
  </trdgrp>   
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">nespr&aacute;vny</trd>
</drv>

<drv mrk="korekt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i komposterarojn:
      <ekz>
        mi fari&gcirc;is <tld/>isto en presejo
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur provpresa&jcirc;o oni devis denove <tld/>i erarojn, kion tamen en grandaj
        entreprenoj kutime faris profesiaj <tld/>istoj (reviziantoj)
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2009/009793.php"
              >Kompatindaj redaktoroj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unusola homo kiu legas la revuon anta&ubreve; ol mi sendas &gcirc;in <klr>[&#8230;]</klr> al la
        presejo <klr>[&#8230;]</klr> estas tre honesta esperantisto kaj <tld/>isto <nom>Rob
        Moerbeek</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">corrector</trd>
  <trdgrp lng="cs">
    <trd>korektor</trd>,
    <trd>opr&aacute;vce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Korrektor</trd>
  </trdgrp>
  <trd lng="es">corrector</trd>
  <trd lng="fr">correcteur</trd>
  <trd lng="hu">korrektor</trd>
  <trd lng="nl">corrector</trd>
  <trd lng="pl">korektor</trd>     
  <trd lng="ro">lector</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">korektor</trd>
</drv>

<drv mrk="korekt.pun0i">
  <kap>pun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provi plibonigi ies konduton per puno; edukpuni:
      <ekz>
        kiun la Eternulo amas, tiun Li pun<tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 3:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam fu&scirc;as kondukistoj, tiujn atendas severa pun<tld/>a kurso
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2005/008674.php"
              >Profitoj, punoj kaj privatigo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pun<tld/>ejo</ind>;
        <trd lng="ca">reformatori</trd>
        <trd lng="hu">jav&iacute;t&oacute;int&eacute;zet</trd>
        <trd lng="nl">strafinstelling</trd>
      </ekz>
      <ekz>
        <ind>pun<tld/>a tribunalo</ind>.
        <trd lng="ca">Tribunal Correccional</trd>
        <trd lng="de">Strafgericht</trd>
        <trd lng="fr">tribunal correctionnel</trd>
        <trd lng="nl">correctionele <ind>rechtbank</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">punir</trd>
  <trd lng="fr">corriger (en punissant)</trd>
  <trd lng="hu">b&uuml;ntetve <ind>jav&iacute;t</ind></trd>
  <trd lng="nl">bestraffen</trd>
  <trd lng="pl">ukara&cacute;</trd>     
  <trd lng="ro">pedepsi</trd>
</drv>

<drv mrk="korekt.sur0i">
  <kap>sur<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
