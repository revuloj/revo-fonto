<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxinz.xml,v 1.23 2023/11/11 11:36:38 revo Exp $">
<kap>
  <rad>&gcirc;inz</rad>/o,
  <var><kap><rad var="&jcirc;">&jcirc;inz</rad>/o  <fnt><bib>NeoGlo</bib></fnt></kap></var>,
  <var><kap><rad var="s">&jcirc;ins</rad>/o</kap></var>
</kap>
<drv mrk="gxinz.0o">
  <kap><tld/>o,
  <var><kap><tld var="&jcirc;"/>o</kap></var>, 
  <var><kap><tld var="s"/>o</kap></var></kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="gxinz.0o.TEKS">
    <dif>
      <ref tip="super" cel="pantal.0o">Pantalono</ref>
      de okcident-usona origino, el
      <ref tip="prt" cel="drelik.0o">dreliko</ref>, ofte blua:
      <ekz>
        sur la planko ku&scirc;is malnovaj gazetoj kaj kovertoj, sur
        la ununura se&gcirc;o ies <tld/>o, &scirc;trumpetoj, mamzono
        <fnt>
        <aut>Saliko</aut>, <vrk>En fremda lando</vrk>. <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        li portis nur grizan someran jakon kaj <tld/>on
        <fnt>
        <aut>Pejno Simono</aut>, <vrk>La profeta kurso</vrk>.
        <bib>Monato</bib>
        </fnt>;
      </ekz>
     <ekz>
        ni denove vidas ilin &ccirc;iujn, la dancistojn kaj kantistojn kaj
        iliajn inojn, en trivitaj <tld var="&jcirc;"/>oj, kun violkoloraj pluvmanteloj
        <fnt><aut>Cezar</aut>, <vrk><url ref="http://www.cezarkulturo.de/197985.html">Cezar-prozpoemoj
        05</url></vrk>, <lok>7, Adia&ubreve;, rusoj!</lok></fnt>;
      </ekz>
      <ekz>
        <tld var="s"/>o sen po&scirc;oj
        <fnt>
          <vrk><url ref="https://lernu.net/de/forumo/temo/25353">Okaze de la Tago de Virinoj</url></vrk>
          <lok>lernu.net</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n;&c_s;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_malmol;&c_n;&c_k;&c_i;</trd>,
    <trd>&c_d;&c_zh;&c_i;&c_n;&c_s;&c_i;</trd>
  </trdgrp>
  <trd lng="bo">&#xF47;&#xF72;&#xF53;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Jeans</trd>,
    <trd>Nietenhose</trd>
  </trdgrp>
  <trd lng="cs">d&zcaron;in(s)y</trd>
  <trd lng="en">jeans</trd>
  <trdgrp lng="es">
    <trd>(pantalones) <ind>vaqueros</ind></trd>,
    <trd>jeans</trd>,
    <trd>pantal&oacute;n de <ind>mezclilla</ind></trd>
  </trdgrp>
  <trd lng="fr">jeans</trd>
  <trd lng="he">&gimel;&#x5F3;&yod;&nun;&samekh;</trd>
  <trd lng="hu">farmernadr&aacute;g</trd>
  <trdgrp lng="id">
    <trd>jeans</trd>,
    <trd>jins</trd>
  </trdgrp>
  <trd lng="it">blue-jeans</trd>
  <trd lng="ja">&#x30B8;&#x30FC;&#x30F3;&#x30BA;  <pr>&#x3058;&#x30FC;&#x3093;&#x305A;</pr></trd>
  <trd lng="nl">jeans</trd>
  <trd lng="pl">d&zdot;insy</trd>
  <trd lng="pt">jeans</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;&c_s;&c_y;</trd>
  <trd lng="uk">&#x434;&#x436;&#x438;&#x43D;&#x441;</trd>
  <trd lng="zh">&#x725B;&#x4ED4;&#x88E4; <pr>ni&#xFA;z&#x1CE;ik&#xF9;</pr></trd>
</drv>
</art>
