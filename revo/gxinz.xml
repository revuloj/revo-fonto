<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&gcirc;inz</rad>/o
</kap>
<drv mrk="gxinz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="gxinz.0o.TEKS">
    <dif>
      <ref tip="super" cel="pantal.0o">Pantalono</ref>
      de okcident-usona origino, el
      <ref tip="prt" cel="drelik.0o">dreliko</ref>, ofte blua:
      <ekz>
        sur la planko ku&scirc;is malnovaj gazetoj kaj kovertoj, sur
        la ununura se&gcirc;o ies <tld/>o, &scirc;trumpetoj, mamzono
        <fnt>
        <aut>Saliko</aut>, <vrk>En fremda lando</vrk>. <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        li portis nur grizan someran jakon kaj <tld/>on
        <fnt>
        <aut>PEJNO Simono</aut>, <vrk>La profeta kurso</vrk>.
        <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n;&c_s;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Jeans</trd>,
    <trd>Nietenhose</trd>
  </trdgrp>
  <trd lng="cs">d&zcaron;in(s)y</trd>
  <trd lng="en">jeans</trd>
  <trdgrp lng="es">
    <trd>(pantalones) <ind>vaqueros</ind></trd>,
    <trd>jeans</trd>,
    <trd>pantal&oacute;n de <ind>mezclilla</ind></trd>
  </trdgrp>
  <trd lng="hu">farmernadr&aacute;g</trd>
  <trd lng="it">blue-jeans</trd>
  <trd lng="fr">jeans</trd>
  <trd lng="nl">jeans</trd>
  <trd lng="pl">d&zdot;insy</trd>
  <trd lng="pt">jeans</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;&c_s;&c_y;</trd>
</drv>
</art>
