<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>maraved</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="maraved.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Iama <ref tip="super" cel="unu.monasub0o">mona subunuo</ref>
      hispana, depende je provincoj ono de <ref tip="malprt"
      cel="real1.0o">realo</ref> a&ubreve; de <ref tip="malprt"
      cel="piastr.0o">piastro</ref>:
      <ekz>
        li ne donus e&ccirc; unu <tld/>on por ajna
        aventuro<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 23a, p.
        164a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Maraved&iacute;</trd>
  <trd lng="es">maraved&iacute;</trd>
  <trd lng="fr">marav&eacute;dis</trd>
  <trd lng="hu">marav&eacute;di</trd>
  <trdgrp lng="pt">
    <trd>morabitino</trd>,
    <trd>maravedi</trd>,
    <trd>almorabitino</trd>,
    <trd>maravidiol</trd>,
    <trd>mauro <klr tip="ind">(antiga moeda)</klr></trd>    
  </trdgrp>
</drv>
</art>

