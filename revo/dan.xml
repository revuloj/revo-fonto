<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>dan</rad>/o
</kap>
<drv mrk="dan.0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="dan.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref>, &scirc;tatanino de
      <ref tip="malprt" cel="dan.D0ujo"><tld lit="D"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="dan.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_t;&c_ch;&c_a;&c_n;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_d;&c_a;&c_t;&c_ch;&c_a;&c_n;&c_i;&c_n;</trd>
  <trd lng="ca">dan&eacute;s<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>dan</trd>,
    <trd>danien</trd>
  </trdgrp>
  <trd lng="de">D&auml;ne</trd>
  <trd lng="en">Dane</trd>
  <trd lng="es">dan&eacute;s</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;&f_y;</trd>
  <trd lng="fr">Danois<klr> (subst.)</klr></trd>
  <trd lng="nl">Deen</trd>
  <trd lng="no">danske</trd>
  <trd lng="pl">Du&nacute;czyk</trd>
  <trd lng="sk">danien</trd>
  <trd lng="sv">dansk</trd>
</drv>

<drv mrk="dan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
   <trd lng="fr">danois<klr> (adj.)</klr></trd>
   <trd lng="en">Danish</trd>
  </snc>
  <snc>
    <dif>
      "la dana" = la nacia lingvo de Danujo
      <ekz><fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Dana lingvo">Dana
        lingvo</url></lok></fnt></ekz>.
    </dif>
   <trd lng="fr">danois<klr> (langue)</klr></trd>
   <trd lng="en">Danish<klr> (language)</klr></trd>
  </snc>  <trd lng="be">&c_d;&c_a;&c_c;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_d;&c_a;&c_t;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">dan&eacute;s <klr>(adj.)</klr></trd>
  <trd lng="de">d&auml;nisch</trd>
  <trd lng="es">dan&eacute;s</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;&f_y;</trd>
  <trd lng="nl">Deens</trd>
  <trd lng="no">dansk</trd>
  <trd lng="pl">du&nacute;ski</trd>
  <trd lng="sv">dansk</trd>
</drv>

<drv mrk="dan.D0ujo">
  <kap><tld lit="D"/>ujo
  (<var><kap><tld lit="D"/>/i/o</kap></var>),
  <var><kap><tld lit="D"/>lando</kap></var>
   <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
   <fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Danio">Danio</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> norde de
      Germanio (la nura tera najbaro),
       sudokcidente de Svedio, kaj sude de Norvegio, kun &ccirc;efurbo <ref
       tip="vid" cel="kopenhag.0o">Kopenhago</ref>.
    </dif>
    <ref tip="sin" cel="dani.D0o">Danio</ref>
  </snc>
    <rim mrk="dan.0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la   responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan
  diferencon en la uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
  <trd lng="be">&c_D;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_D;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Dinamarca</trd>,
    <trd>Regne de <ind>Dinamarca</ind></trd>
  </trdgrp>
  <trd lng="de">D&auml;nemark</trd>
  <trd lng="en">Denmark</trd>
  <trd lng="es">Dinamarca</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>Danemark</trd>,
    <trd>Royaume du <ind>Danemark</ind></trd>
  </trdgrp>
  <trd lng="id">Denmark</trd>
  <trd lng="nl">Denemarken</trd>
  <trd lng="no">Danmark</trd>
  <trd lng="pl">Dania</trd>
  <trd lng="pt">Dinamarca</trd>
  <trd lng="sv">Danmark</trd>
</drv>

</art>

