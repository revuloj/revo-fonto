<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dan</rad>/o
</kap>

<drv mrk="dan.0o">
  <kap><tld/>o</kap>
  <snc mrk="dan.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="dan.D0ujo"><tld lit="D"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="dan.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_a;&c_t;&c_ch;&c_a;&c_n;&c_i;&c_n;</trd>
  <trd lng="de">D&auml;ne</trd>
</drv>

<drv mrk="dan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_a;&c_t;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">d&auml;nisch</trd>
</drv>

<drv mrk="dan.D0ujo">
  <kap><tld lit="A"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> norde de Germanio (la nura tera najbaro),
       sudokcidente de Svedio, kaj sude de Norvegio.
    </dif>
  </snc>
  <trd lng="bg">&c_D;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">D&auml;nemark</trd>
  <trd lng="en">Denmark</trd>
</drv>

</art>

