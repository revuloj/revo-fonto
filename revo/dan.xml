<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dan.xml,v 1.26 2021/03/06 20:19:45 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>dan</rad>/o <fnt><bib>Hamlet</bib></fnt>
</kap>
<drv mrk="dan.0o">
  <kap><ofc>8</ofc><tld/>o</kap>
  <snc mrk="dan.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de <ref tip="malprt" cel="dan.D0ujo"><tld lit="D"/>ujo</ref>:
      <ekz>
        la plej feli&ccirc;aj homoj estas <tld/>oj, maltanoj kaj svisoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008566.php">Frando-lando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        germano edzi&gcirc;inta al <tld/>ino, kreis esperantistan familion kun
        du denaskuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2011/010149.php">Ho, belaj spritaj
          tempoj!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="dan.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        brava juna <tld/>o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la volumojn verkis inter la jaroj 1932 kaj 1960 la <tld/>o
        Martinus
        <fnt>
          <bib>Monato</bib>
          <aut>H&aring;kan Lundberg</aut>
          <vrk><url ref="&Monato;/2005/008703.php">La libro de la
          vivo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efredaktoro de la dana gazeto <nom>Jyllands-Posten</nom>
        permesis eldoni 12 karikaturojn pri la profeto Muhamado
        <klr>[&#x2026;]</klr> islamanoj postulis, ke la redaktoro pardonpetu
        <klr>[&#x2026;]</klr> malgra&ubreve; tio, ke la situacio en Danio
        trankvili&gcirc;is, la etnaj <tld/>oj kaj la 200 000 islamanoj en la
        lando timas la sekvojn
        <fnt>
          <bib>Monato</bib>
          <aut>Betty Chatterjee</aut>
          <vrk><url ref="&Monato;/2006/008923.php">La karikaturoj:
          kalumnio, kolero kaj iom da kompreno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_t;&c_ch;&c_a;&c_n;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_d;&c_a;&c_t;&c_ch;&c_a;&c_n;&c_i;&c_n;</trd>
  <trd lng="ca">dan&eacute;s<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>dan</trd>,
    <trd>danien</trd>
  </trdgrp>
  <trd lng="da">dansker</trd>
  <trdgrp lng="de">
    <trd>D&auml;ne</trd>,
    <trd>D&auml;nin</trd>
  </trdgrp>
  <trd lng="en">Dane</trd>
  <trd lng="es">dan&eacute;s</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;&f_y;</trd>
  <trd lng="fr">Danois<klr> (subst.)</klr></trd>
  <trd lng="fy">Deen</trd>
  <trd lng="he">&#x5D3;&#x5E0;&#x5D9;</trd>
  <trd lng="nl">Deen</trd>
  <trd lng="no">danske</trd>
  <trd lng="pl">Du&nacute;czyk</trd>
  <trd lng="sk">danien</trd>
  <trd lng="sv">dansk</trd>
  <trd lng="vo">Dan&auml;nan</trd>
  <trd lng="tp">jan pi ma Tansi</trd>
</drv>

<drv mrk="dan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        la <tld/>a le&gcirc;aro
        <fnt>
          <bib>Monato</bib>
          <aut>Bjarke Nielsen</aut>
          <vrk><url ref="https://www.monato.be/2004/008416.html">Per la
          le&gcirc;o la lando estos konstruita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj flagoj
        <fnt>
          <bib>Monato</bib>
          <aut>Betty Chatterjee</aut>
          <vrk><url ref="&Monato;/2006/008923.php">La karikaturoj:
          kalumnio, kolero kaj iom da kompreno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
   <trd lng="fr">danois<klr> (adj.)</klr></trd>
   <trd lng="en">Danish</trd>
   <trd lng="tp">pi ma Tansi</trd>
   <trd lng="vo">Dan&auml;nik</trd>
  </snc>
  <snc>
    <dif>
      La <tld/>a lingvo:
      <ekz>
        kelkaj nekeltaj e&ubreve;ropaj lingvoj, ekzemple en la franca kaj
        <tld/>a
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9311-matematiko.txt">Matematiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">danois<klr> (langue)</klr></trd>
    <trd lng="en">Danish<klr> (language)</klr></trd>
    <trd lng="tp">toki Tansi</trd>
    <trd lng="vo">Dan&auml;nap&uuml;k</trd>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_c;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_d;&c_a;&c_t;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">dan&eacute;s <klr>(adj.)</klr></trd>
  <trd lng="da">dansk</trd>
  <trd lng="de">d&auml;nisch</trd>
  <trd lng="es">dan&eacute;s</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;&f_y;</trd>
  <trd lng="fy">Deensk</trd>
  <trd lng="he">&#x5D3;&#x5E0;&#x5D9;</trd>
  <trd lng="nl">Deens</trd>
  <trd lng="no">dansk</trd>
  <trd lng="pl">du&nacute;ski</trd>
  <trd lng="sv">dansk</trd>
</drv>

<drv mrk="dan.D0ujo">
  <kap><tld lit="D"/>ujo
    <fnt><bib>Fab2</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">DK</mlg>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> norde
      de Germanujo (la nura tera najbaro),
      sudokcidente de Svedujo, kaj sude de Norvegujo; kun &ccirc;efurbo <ref tip="prt" cel="kopenhag.0o">Kopenhago</ref>:
      <ekz>
        Hamleto, re&gcirc;ido de <tld lit="D"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8807">William
          Shakespeare</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld lit="D"/>ujo staras malnova kastelo, nomata
        <nom>Kronborg</nom>, kiu trovi&gcirc;as tute &ccirc;e la Sundo, kie la
        grandaj &scirc;ipoj &ccirc;iutage grandanombre preterveturas
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kontra&ubreve;-idista movado en <tld lit="D"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26535">Margrethe
          Noll</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="dani.D0o">Danio</ref>
  </snc>
  <trd lng="be">&c_D;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_D;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Dinamarca</trd>,
    <trd>Regne de <ind>Dinamarca</ind></trd>
  </trdgrp>
  <trd lng="da">Danmark</trd>
  <trd lng="de">D&auml;nemark</trd>
  <trd lng="en">Denmark</trd>
  <trd lng="es">Dinamarca</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>Danemark</trd>,
    <trd>Royaume du <ind>Danemark</ind></trd>
  </trdgrp>
  <trd lng="fy">Denemark</trd>
  <trd lng="id">Denmark</trd>
  <trd lng="nl">Denemarken</trd>
  <trd lng="no">Danmark</trd>
  <trd lng="pl">Dania</trd>
  <trd lng="pt">Dinamarca</trd>
  <trd lng="sv">Danmark</trd>
  <trd lng="tp">ma Tansi</trd>
  <trd lng="vo">Dan&auml;n</trd>
</drv>

</art>
