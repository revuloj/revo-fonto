<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>procesi</rad>/o
</kap>
<drv mrk="proces1.0o">
  <kap><tld/>o</kap>
  <snc mrk="proces1.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Festa mar&scirc;ado sur vojoj por elmontrado de sankta&jcirc;oj:
      <ekz>
	mar&scirc;i kiel <tld/>o
	<klr>(malrapide kaj solene)</klr>;
      </ekz>
      <ekz>
	<ind>funebra <tld/>o</ind>.
	<trd lng="hu">gy&aacute;szmenet</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="de">Prozession</trd>
  <trd lng="en">procession</trd>
  <trdgrp lng="fr">
    <trd>cort&egrave;ge</trd>,
    <trd>procession</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;rmenet</trd>
  <trdgrp lng="nl">
    <trd>processie</trd>,
    <trd>stoet</trd>,
    <trd>optocht</trd>
  </trdgrp>
  <trd lng="pt">prociss&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j; &c_h;&c_o;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="proces1.0i">
  <kap><tld/>i</kap>
  <snc mrk="proces1.0i.KOMUNE">
    <dif>
      Pa&scirc;i grupe, solene sur vojoj, kiel <tld/>o:
      <ekz>
	la homoj de la vitra urbo faris spacon por ili kaj <tld/>is post ili
	<fnt><bib>DOz</bib>, <lok>&leftquot;la vitra urbo&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="defil.0i.MIL">defili <sncref/></ref>,
      <ref cel="defil.0i.modo">defili <sncref/></ref>,
      <ref cel="manife1.0o">manifestacii</ref>
    </refgrp>
  </snc>
  <trd lng="en">go in procession</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;filer</trd>,
    <trd>faire une <ind>procession</ind></trd>
  </trdgrp>
</drv>
</art>
