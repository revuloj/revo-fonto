<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>procesi</rad>/o
</kap>
<drv mrk="proces1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ceremonia mar&scirc;ado de klerikaro kaj popolo, kantantaj himnojn, okaze
      de religia festo:
      <ekz>
        mar&scirc;i kiel <tld/>o
        <klr>(malrapide kaj solene)</klr>;
      </ekz>
      <ekz>
        funebra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Prozession</trd>
  <trdgrp lng="nl">
    <trd>processie</trd>,
    <trd>stoet</trd>,
    <trd>optocht</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j; &c_h;&c_o;&c_d;</trd>
  </trdgrp>
</drv>
</art>
