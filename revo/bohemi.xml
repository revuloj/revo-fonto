<drv mrk="bohemi.0o">
  <kap><tld/>o</kap>
  <snc mrk="bohemi.0o.bohemujo">
    <dif>
      <ref tip="dif" cel="bohem.0ujo">Bohemujo</ref>:
      <ekz>
        1412 <nom>Hus</nom> forlasis Pragon kaj foriris al
        <klr>[&#x2026;]</klr> suda <tld/>o kaj
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        la germanaj nomoj estis reuzataj en lokoj, kie tiu lingvo iam
        estis parolata:
        <tld/>o, Sud-Tirolo, Alzaco, Loreno
        <fnt>

          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2011/010353.php">La ultradimensio</url></vrk>
          <bib>Monato</bib>
          <lok>2011/10, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        menciita en dokumento el la jaro 1271, kiam la <tld lit="b"/>a
        re&gcirc;o Otokaro 2a
        transprenis la fortika&jcirc;on
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007044.php">De perforto al paco</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 12</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="de">B&ouml;hmen</trd>
    <trd lng="en">Bohemia</trd>
    <trd lng="it">Boemia</trd>
    <trd lng="nl">Bohemen</trd>
    <trd lng="pl">Bohemia</trd>
    <trd lng="ro">Bo&eacute;mia</trd>
    <trd lng="zh">&#x6CE2;&#x5E0C;&#x7C73;&#x4E9A; <pr>b&#x14D;x&#x12B;m&#x1D0;y&#xE0;</pr></trd>
</snc>
  <trd lng="be">&c_B;&c_a;&c_g;&c_je;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="he">&#x5D1;&#x5D5;&#x5D4;&#x5DE;&#x5D9;&#x5D4;</trd>
  <trd lng="ja">&#x30DC;&#x30D8;&#x30DF;&#x30A2;</trd>
  <trd lng="uk">&c_B;&#x43E;&c_g;&c_je;&#x43C;&c_ib;&#x44F;</trd>
