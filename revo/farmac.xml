<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farmac.xml,v 1.35 2025/10/20 18:44:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>farmaci</rad>/o
<fnt><bib>JED</bib></fnt></kap>

<drv mrk="farmac.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref> pri
      medikamentoj kaj ilia preparado:
      <ekz>
        la organiza&jcirc;o prizorgas anka&ubreve; prisanajn aferojn kaj respondecas pri <tld/>o kaj
        konstruas klinikon
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Vital Mondengi/pg</aut>
          <vrk><url ref="&Monato;/2005/008460.php">Semante esperon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;o starigis modernan industrian komplekson de 26 fabrikoj koncernaj al
        man&gcirc;a&jcirc;oj, kondimentoj, <tld/>o, trinka&jcirc;oj, biero, presado, pakado ktp
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010245.php">Floras enklavo komunista</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="farmak.0o">farmakologio</ref>
    <rim>
      Ne konfuzu kun
      <ref cel="apotek.0o">apoteko</ref>.
    </rim>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri  <ke>apotekofako/scienco</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_e;&c_w;&c_t;&c_y;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">farm&agrave;cia<klr> (ci&egrave;ncia)</klr></trd>
  <trdgrp lng="de">
    <trd>Arzneimittelkunde</trd>,
    <trd>Pharmazie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pharmaceutics</trd>,
    <trd>pharmacy<klr tip="amb"> (science, not store)</klr></trd>
  </trdgrp>
  <trd lng="es">farmacia<klr> (ciencia)</klr></trd>
  <trd lng="fr">pharmacie<klr tip="ind"> (science)</klr></trd>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer&eacute;szet</trd>,
    <trd>gy&oacute;gyszertan</trd>
  </trdgrp>
  <trd lng="id">farmasi</trd>
  <trd lng="ja">&#34220;&#23398;  <pr>&#12420;&#12367;&#12364;&#12367;</pr></trd>
  <trdgrp lng="nl">
    <trd>farmaceutica</trd>,
    <trd>farmacie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>farmacja</trd>,
    <trd>farmaceutyka</trd>
  </trdgrp>
  <trd lng="pt">farm&aacute;cia <klr tip="amb">(ci&ecirc;ncia)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#33647;&#29289;&#23398; <pr>y&agrave;ow&ugrave;xu&eacute;</pr></trd>
</drv>

<drv mrk="farmac.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="medika1.0o">Medikamento</ref>:
      <ekz>
        beladono <klr>[&#8230;]</klr> estas kuracplanto, kies folio, radiko uzatas por
        diversaj <tld/>a&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18620">Beladono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kurac.0ilo">kuracilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_d;&c_y;&c_k;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">f&agrave;rmac</trd>
  <trdgrp lng="en">
    <trd>pharmaceutical</trd>,
    <trd>medicine</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Arzneimittel</trd>,
    <trd>Pharmakum</trd>
  </trdgrp>
  <trd lng="es">f&aacute;rmaco</trd>
  <trd lng="fr">m&eacute;dicament</trd>
  <trd lng="hu">gy&oacute;gyszer</trd>
  <trdgrp lng="nl">
    <trd>medicijn</trd>,
    <trd>medicament</trd>,
    <trd>geneesmiddel</trd>
  </trdgrp>
  <trd lng="pl">farmaceutyk</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_i;&c_k;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="uk">&c_l;&c_ib;&c_k;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#33647;&#21697; <pr>y&agrave;op&#464;n</pr></trd>,
    <trd>&#33647;&#21058; <pr>y&agrave;oj&igrave;</pr></trd>,
    <trd>&#33647;&#29289; <pr>y&agrave;ow&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="farmac.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Tiu, kiu praktikas <tld/>on:
      <ekz>
        &scirc;i estis <tld/>istino, &scirc;i sciis, ke stranga nevuso nek jukanta nek doloriga
        a&ubreve;guras problemon
        <fnt>
          <bib>Monato</bib>
          <aut>Lin Sheng</aut>
          <vrk><url ref="&Monato;/2006/008670.php">Forpasis mia karulino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_e;&c_w;&c_t;</trd>
  <trd lng="ca">farmac&egrave;utic</trd>
  <trd lng="de">Pharmazeut</trd>
  <trdgrp lng="en">
    <trd>pharmacist <klr>(usone)</klr></trd>,
    <trd>chemist <klr>(brite)</klr></trd>,
    <trd>apothecary</trd>,
    <trd>druggist</trd>
  </trdgrp>
  <trd lng="es">farmac&eacute;utico</trd>
  <trd lng="fr">pharmacien</trd>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer&eacute;sz</trd>,
    <trd>patikus</trd>
  </trdgrp>
  <trd lng="id">farmakolog</trd>
  <trd lng="ja">&#34220;&#21092;&#24107;  <pr>&#12420;&#12367;&#12374;&#12356;&#12375;</pr></trd>
  <trdgrp lng="nl">
    <trd>farmaceut</trd>,
    <trd>apotheker</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>farmaceuta</trd>,
    <trd>aptekarz</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;</trd>
  <trd lng="uk">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;</trd>
</drv>
</art>
