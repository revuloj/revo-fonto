<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>farmaci</rad>/o
</kap>
<drv mrk="farmac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
     Scienco pri medikamentoj k ilia preparado. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Arzneimittelkunde</trd>,
     <trd>Pharmazie</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="farmac.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu praktikas <tld/>on. 
    </dif>
    <refgrp tip="sin">
      <ref cel="apotek.0o">apoteko</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Pharmazeut</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;</trd>
</drv>
</art>

