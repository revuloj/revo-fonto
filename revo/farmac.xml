<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>farmaci</rad>/o
</kap>
<drv mrk="farmac.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri medikamentoj
      kaj ilia preparado.
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="apotek.0o">apoteko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_e;&c_w;&c_t;&c_y;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">farm&agrave;cia<klr> (ci&egrave;ncia)</klr></trd>
  <trdgrp lng="de">
    <trd>Arzneimittelkunde</trd>,
    <trd>Pharmazie</trd>
  </trdgrp>
  <trd lng="en">pharmaceutics</trd>
  <trd lng="es">farmacia<klr> (ciencia)</klr></trd>
  <trd lng="fr">pharmacie<klr tip="ind"> (science)</klr></trd>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer&eacute;szet</trd>,
    <trd>gy&oacute;gyszertan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>farmaceutica</trd>,
    <trd>farmacie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="farmac.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="medika1.0o">Medikamento</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_d;&c_y;&c_k;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">f&agrave;rmac</trd>
  <trdgrp lng="en">
    <trd>pharmaceutical</trd>,
    <trd>medicine</trd>
  </trdgrp>
  <trd lng="es">f&aacute;rmaco</trd>
  <trd lng="hu">gy&oacute;gyszer</trd>
  <trdgrp lng="nl">
    <trd>medicijn</trd>,
    <trd>medicament</trd>,
    <trd>geneesmiddel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_i;&c_k;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="farmac.0isto">
  <kap>
    <tld/>isto
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Tiu, kiu praktikas <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_e;&c_w;&c_t;</trd>
  <trd lng="ca">farmac&egrave;utic</trd>
  <trd lng="de">Pharmazeut</trd>
  <trdgrp lng="en">
    <trd>pharmacist <klr>(usone)</klr></trd>,
    <trd>chemist <klr>(brite)</klr></trd>,
    <trd>apothecary</trd>,
    <trd>druggist</trd>
  </trdgrp>
  <trd lng="es">farmac&eacute;utico</trd>
  <trd lng="fr">pharmacien</trd>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer&eacute;sz</trd>,
    <trd>patikus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>farmaceut</trd>,
    <trd>apotheker</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;</trd>
</drv>
</art>
