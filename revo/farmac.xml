<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>farmaci</rad>/o
</kap>
<drv mrk="farmac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
     <ref tip="super" cel="scienc.0o">Scienco</ref> pri medikamentoj kaj ilia 
     preparado. 
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun: 
      <ref cel="apotek.0o">apoteko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Arzneimittelkunde</trd>,
     <trd>Pharmazie</trd></trdgrp>
  <trd lng="fr">pharmacie<klr tip="ind"> (science)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="farmac.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Tiu, kiu praktikas <tld/>on. 
    </dif>
  </snc>
  <trd lng="de">Pharmazeut</trd>
  <trd lng="fr">pharmacien</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_je;&c_v;&c_t;</trd>
</drv>
</art>

