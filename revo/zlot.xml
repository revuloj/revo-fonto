<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>zlot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="zlot.0o">
   <kap><tld/>o</kap>
   <snc mrk="zlot.0o.EKON">
   <uzo tip="fak">EKON</uzo>
     <dif>
       <ref tip="super" cel="unu.mon0o">Monunuo</ref> de Pollando.
       <ekz>
         bova&jcirc;o, kiu kostis 30000 <tld/>ojn anta&ubreve; du
jaroj, nun kostas 69 000 <tld/>ojn
         <fnt><bib>HomojMan&gcirc;antaj</bib>,
<lok>&leftquot;man&gcirc;i la nigron&rightquot;, p. 80a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fi">zloty</trd>
   <trd lng="fr">zloty</trd>
   <trd lng="hu">zloty</trd>
   <trd lng="pl">z&lstroke;oty</trd>
   <trdgrp lng="it">
   <trd>zloty</trd>, 
   <trd>sloto</trd>, 
   <trd>sloti</trd>
   </trdgrp>
</drv>
</art>

