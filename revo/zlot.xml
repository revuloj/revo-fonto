<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zlot.xml,v 1.14 2023/09/05 09:43:57 revo Exp $">
<kap>
   <rad>zlot</rad>/o
   <fnt>
     <bib>PIV1</bib>
   </fnt>
</kap>
<drv mrk="zlot.0o">
   <kap><tld/>o</kap>
   <snc mrk="zlot.0o.EKON">
   <uzo tip="fak">EKON</uzo>
   <mlg kod="ISO-4217">PLN</mlg>
     <dif>
       <ref tip="super" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref> de
       Pollando:
       <ekz>
         bova&jcirc;o, kiu kostis 30000 <tld/>ojn anta&ubreve; du
         jaroj, nun kostas 69 000 <tld/>ojn
         <fnt><bib>HomojMan&gcirc;antaj</bib>,
         <lok>&leftquot;man&gcirc;i la nigron&rightquot;, p.
         80a</lok></fnt>
       </ekz>;
      <ekz>
        1 zloto = 100 <ref tip="sub" cel="grosx.0o">gro&scirc;oj</ref>
        <fnt>
          <vrk>
            <url ref="&Monato;/landoj.php">La
            landoj de l' tero</url>
          </vrk>
       </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_z;&c_l;&c_o;&c_t;&c_y;</trd>
  <trd lng="ca">zloty</trd>
  <trd lng="cs">zlot&yacute;</trd>
  <trd lng="fi">zloty</trd>
  <trd lng="fr">zloty</trd>
  <trd lng="hu">zloty</trd>
  <trdgrp lng="it">
     <trd>zloty</trd>,
     <trd>sloto</trd>,
     <trd>sloti</trd>
   </trdgrp>
   <trd lng="ja">&#x30BA;&#x30AA;&#x30C1;</trd>
  <trd lng="nl">zloty</trd>
  <trd lng="pl">z&lstroke;oty</trd>
  <trd lng="ru">&c_z;&c_l;&c_o;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">zlot&yacute;</trd>
  <trd lng="uk">&c_z;&c_l;&c_o;&c_t;&c_i;&c_j;</trd>
</drv>
</art>

