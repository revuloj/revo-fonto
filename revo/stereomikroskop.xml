<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stereomikroskop.xml,v 1.5 2022/02/05 07:41:15 revo Exp $">
<kap>
  <rad>stereomikroskop</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="stereomikroskop.0o">
  <kap><tld/>o</kap>
  <snc mrk="stereomikroskop.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="mikros.0o.FOT">Mikroskopo</ref>
      kiu vidigas la reliefon de objektoj, ordinare per alrigardo
      tra du lornoj:
      <ekz>
        &ccirc;iuj registritaj taksonoj estis enlaboratorie kontrolataj per 
        <klr>...</klr> <tld/>o kaj lummikroskopo
        <fnt>
          <aut>C. Roux</aut>,
          <vrk><url
          ref="http://lichenologue.org/fichiers/docs/2004RouxArbolimo.pdf">
          Likenkuna&jcirc;oj kaj supra arbarlimo en la kalkaj Alpoj de suda
          Francio</url></vrk>,
          <lok>Bull. Soc. linn. Provence, 2004 (55)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stereo4.0.TEK">stereo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;&c_m;&c_ib;&c_k;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="ca">microscopi estereosc&ograve;pic</trd>
  <trdgrp lng="en">
    <trd>stereomicroscope</trd>,
    <trd>stereoscopic microscope</trd>,
    <trd>dissection microscope</trd>
  </trdgrp>
  <trd lng="fr">loupe binoculaire</trd>
  <trd lng="pl">stereomikroskop</trd>
</drv>
</art>

