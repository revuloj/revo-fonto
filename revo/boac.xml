<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>boac</rad>/o
</kap>
<drv mrk="boac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cerved.0oj">cervedoj</ref>
      (<trd lng="la">Rangifer</trd>), vivanta en nordaj regionoj.
      Amba&ubreve; seksoj portas kornojn:
      <ekz>
        la <tld/>o estas la sola hejmbesta specio el cervedoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="cerv.norda0o">norda cervo</ref>
  </snc>
  <trd lng="de">Rentier</trd>
  <trd lng="nl">rendier</trd>
  <trd lng="fr">renne</trd>
  <trdgrp lng="fr">
    <trd>tar&aacute;ndszarvas</trd>,
    <trd>r&eacute;nszarvas</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_mol;
  <klr>(&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;)</klr></trd>
</drv>
</art>
