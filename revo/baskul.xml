<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>baskul</rad>/o
</kap>
<drv mrk="baskul.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato, konsistanta esence el trabo, stango ks, kies unu ekstrema&jcirc;o
      levi&gcirc;as, kiam oni surpezas sur la alia.     
    </dif>
    <trdgrp lng="ru">
      <trd>&c_r;&c_y;&c_ch;&c_a;&c_g;</trd>,
      <trd>&c_k;&c_o;&c_r;&c_o;&c_m;&c_y;&c_s;&c_l;&c_o;</trd>,
      <trd>&c_v;&c_je;&c_s;&c_y; <klr>(&c_r;&c_y;&c_ch;&c_a;&c_zh;&c_n;&c_y;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Longa tabulo metita sur &scirc;tupo, tiel ke unu persono &ccirc;e &ccirc;iu ekstremo povas
      levi&gcirc;i k mallevi&gcirc;i alterne.
    </dif>
    <trd lng="ru">&c_k;&c_a;&c_ch;&c_je;&c_l;&c_i;</trd>
  </snc>
  <snc num="3">
    <uzo tip="fak">KOMP</uzo>
    <fnt>(<lok><url ref="&KompLeks;/B.HTM#BASKULO">KompLeks</url></lok>)</fnt>
    <dif>
      Flago kun operacio, &scirc;altanta &gcirc;ian valoron al la malo de
      la kuranta; tiu operacio. 
    </dif>
    <trd lng="en">toggle</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_t;&c_r;&c_i;&c_g;&c_g;&c_je;&c_r;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="baskul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Movi&gcirc;i tiamaniere.
    </dif>
  </snc>  
</drv>
<drv mrk="baskul.0ludo">
  <kap><tld/>ludo</kap>
  <snc num="1">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Infana ludo per <tld/>o.
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Politika manovro, kiu konsistas en la alterna favorado de du aliaj,
      inter si malamikaj partioj, landoj ktp.
    </dif>
  </snc>
</drv>
<drv mrk="baskul.0ponto">
  <kap><tld/>ponto</kap>
  <snc>
    <dif>
      Ponto, kies planko povas levi&gcirc;i k mallevi&gcirc;i danke al <tld/>a aparato.
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_m;&c_o;&c_s;&c_t;</trd>
</drv>
<drv mrk="baskul.0sxaltilo">
  <kap><tld/>&scirc;altilo</kap>
  <snc>
    <dif>
      &Scirc;altilo, kiu funkcias per <tld/>a movo.
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_m;&c_b;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>

