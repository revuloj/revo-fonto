<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kariotip.xml,v 1.10 2023/12/03 08:00:23 revo Exp $">
<kap>
  <rad>kariotip</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kariotip.0o">
  <kap><tld/>o</kap>
  <snc mrk="kariotip.0o.GNT">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="dif" cel="kromos.0aro.FIZL">Kromosomaro</ref>:
      <ekz>
        la <tld/>o ebligas a&ubreve; malebligas la krucadon inter specioj a&ubreve; 
        rasoj
        <fnt>
          <aut>C. St&oslash;p-Bowitz, M. B&eacute;la</aut>,
          <vrk><url ref="http://www.fw.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm">
          Enkonduko en la biologion</url></vrk>,
          <lok>Fokuso, 1987-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        analizoj de <tld/>o indikas ke <klr>[&#x2026;]</klr> la Akcipitredoj staras aparte el
        &ccirc;iuj aliaj falkoformaj birdoj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Falkoformaj_birdoj">Falkoformaj birdoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie estas alia <tld/>o, tutcerte alia
        <fnt>
          <aut>K. Buli&ccirc;ov</aut>,
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_ja;&c_t;&c_y;&c_p;</trd>
  <trd lng="ca">cariotip</trd>
  <trd lng="de">Karyotyp</trd>
  <trd lng="en">karyotype</trd>
  <trdgrp lng="fr">
  <trd>caryotype</trd>,
  <trd>caryogramme</trd>
  </trdgrp>
  <trd lng="pl">kariotyp</trd>     
  <trd lng="ro">cariotip</trd>
  <trd lng="uk">&#x43A;&#x430;&#x440;&#x456;&#x43E;&#x442;&#x438;&#x43F;</trd>
  <trd lng="zh">&#x6838;&#x578B; <pr>h&#xE9;x&#xED;ng</pr></trd>
</drv>
</art>

