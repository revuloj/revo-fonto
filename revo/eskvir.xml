<art mrk="$Id: eskvir.xml,v 1.11 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>eskvir</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="eskvir.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Nobela akompananto kaj servanto de pli altranga nobelo:
      <ekz>
        mi disponos, ke li sekvu min, kiel <tld/>o
        grandulon<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 21a, p.
        151a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pagxi.0o.junulo">pa&gcirc;io</ref>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_v;&c_a;&c_j;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>eunuch</trd>,
    <trd>kle&scaron;t&ecaron;nec</trd>
  </trdgrp>
  <trd lng="de">Knappe</trd>
  <trd lng="fr">&eacute;cuyer<klr> (servant)</klr></trd>
  <trdgrp lng="hu">
    <trd>csatl&oacute;s</trd>,
    <trd>fegyvern&ouml;k</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>giermek</trd>,
    <trd>pa&zacute;</trd>
  </trdgrp>
  <trd lng="pt">escudeiro</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_s;&c_k;&c_v;&c_a;&c_j;&c_r;</trd>,
    <trd>&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_i;&c_n; &c_n;&c_a;
    &c_s;&c_l;&c_u;&c_zh;&c_b;&c_je; &c_u;
    &c_s;&c_je;&c_n;&c_mol;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">zbrojno&scaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x441;&#x43A;&#x432;&#x430;&#x439;&#x440;</trd>,
    <trd>&#x441;&#x43A;&#x432;&#x430;&#x439;&#x440;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62A4;&#x82B1;&#x4F7F;&#x8005; <pr>h&#xF9;hu&#x101;sh&#x1D0;zh&#x11B;</pr></trd>,
    <trd>&#x4E61;&#x7EC5; <pr>xi&#x101;ngsh&#x113;n</pr></trd>
  </trdgrp>
</drv>
</art>

