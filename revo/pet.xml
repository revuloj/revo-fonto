<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pet.xml,v 1.38 2021/05/29 14:33:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pet</rad>/i
</kap>
<drv mrk="pet.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="pet.0i.KOMUNE">
    <dif>
      Sciigi al iu, ke oni deziras ion de li:
      <ekz>
        <tld/>i informon, helpon de iu;
      </ekz>
      <ekz>
        <tld/>i iun pri helpo, konsilo, permeso, pardono;
      </ekz>
      <ekz>
        sinjorino <klr>[&#x2026;]</klr> <tld/>is de &scirc;i trinki
        <fnt>
          <bib>F</bib>
          <lok>&FE; 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as, ke mi estu atenta
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as vin, rekonscii&gcirc;u kaj restu
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as vin akcepti de mi &ccirc;i tiun malgrandan donacon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u lin al mi!
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>
        <klr>(<tld/>u, ke li venu al mi)</klr>;
      </ekz>
      <ekz>
        sidi&gcirc;u, mi <tld/>as
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="postul.0i">postuli</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="invit.0i">inviti</ref>,
      <ref cel="dezir.0i">deziri</ref>,
      <ref cel="vol.bon0i">bonvoli</ref>,
      <ref cel="propon.0i">proponi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xF99;&#xF53;&#xF0B;&#xF5E;&#xF74;&#xF0B;&#xF60;&#xF56;&#xF74;&#xF63;&#xF0B;</trd>
  <trd lng="br">goulenn<klr tip="ind"> (v.)</klr></trd>
  <trdgrp lng="cs">
    <trd>po&zcaron;&aacute;dat</trd>,
    <trd>prosit</trd>,
    <trd>&zcaron;&aacute;dat</trd>
  </trdgrp>
  <trd lng="de">bitten</trd>
  <trdgrp lng="en">
    <trd>ask for</trd>,
    <trd>request</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>pedir</trd>,
     <trd>solicitar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>demander<klr> (objet, renseignement...)</klr></trd>,
    <trd>prier</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&bet;&qof;&shin;</ind></trd>
  <trd lng="hu">k&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>vragen<klr tip="ind"> (verzoeken)</klr></trd>,
    <trd>verzoeken</trd>
  </trdgrp>
  <trd lng="pl">prosi&cacute;</trd>
  <trd lng="pt"> pedir</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>poprosi&tcaron;</trd>,
    <trd>prosi&tcaron;</trd>,
    <trd>&zcaron;iada&tcaron;</trd>
  </trdgrp>
  <trd lng="zh">&#x8BF7;&#x6C42; [<ind>q&#x1D0;ngqi&uacute;</ind>]</trd>
</drv>

<drv mrk="pet.0o">
  <kap><tld/>o</kap>
  <snc mrk="pet.0o.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o de barono estas ordono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne direktu por ili supren <tld/>on kaj pre&gcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 11:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi turnas min al vi <klr>[&#x2026;]</klr> kun <tld/>o, kiun mi jam anta&ubreve; longe volis
        direkti al vi
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        virino, por kiu mi faris al vi <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        parencoj de malliberuloj prezentis <tld/>ojn pri malgrandigo de la puno
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i plenumis volonte mian <tld/>on
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pardon<tld/>o pro la pekoj
        <fnt>
          <bib>Monato</bib>
          <aut>Antonio Cappello</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007300.html">Antisemitismo kaj antijudismo</url></vrk>
        </fnt>;
        <trd lng="de">Entschuldigung</trd>
      </ekz>
      <ekz>
        <ind>oficiala <tld/>o</ind>
        <trd lng="de">Antrag</trd>
      </ekz>
      <ekz>
        <ind>deponi <tld/>on</ind> en oficiala lingvo de &scirc;tato
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007177.html">E&ubreve;ropa Unio: Patenta (mal)sukceso</url></vrk>
        </fnt>.
        <trd lng="de">einen <ind>Antrag</ind> stellen</trd>
      </ekz>
    </dif>
    <ref tip="lst" lst="voko:komunikoj" cel="komuni1.0o">komuniko</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_s;&c_mol;&c_b;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xF99;&#xF53;&#xF0B;&#xF5E;&#xF74;&#xF0B;</trd>
  <trd lng="br">goulenn<klr tip="ind"> (ak.)</klr></trd>
  <trdgrp lng="cs">
    <trd>prosba</trd>,
    <trd>&zcaron;&aacute;dost</trd>
  </trdgrp>
  <trd lng="de">Bitte</trd>
  <trd lng="en">request</trd>
  <trdgrp lng="es">
     <trd>petici&oacute;n</trd>,
     <trd>solicitud</trd>
  </trdgrp>
  <trd lng="fr">demande<klr> (objet, renseignement...)</klr></trd>
  <trd lng="he">&bet;&qof;&shin;&he;</trd>
  <trd lng="hu">k&eacute;r&eacute;s</trd>
  <trdgrp lng="nl">
    <trd>vraag<klr tip="ind"> (verzoek)</klr></trd>,
    <trd>verzoek</trd>
  </trdgrp>
  <trd lng="pl">pro&sacute;ba</trd>
  <trd lng="pt"> pedido</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_mol;&c_b;&c_a;</trd>
  <trd lng="sk">suplika</trd>
</drv>

<drv mrk="pet.0egi">
  <kap><ofc>*</ofc><tld/>egi
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -eg'</lok></fnt></kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="pet.0egi.KOMUNE">
    <dif>
      Insiste kaj humile <tld/>i:
      <ekz>
        li <tld/>egis nin, sed ni ne a&ubreve;skultis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 42:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>egu min, ke mi vin forlasu
        <fnt>
          <bib>MT</bib>
          <lok>Rut 1:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &scirc;iaj vortoj estis <tld/>ego de malespero
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_m;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>aspedi&ntilde;</trd>,
    <trd>azgoulenn</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nal&eacute;hav&ecaron; prosit</trd>,
    <trd>sna&zcaron;n&ecaron; prosit</trd>,
    <trd>&uacute;p&ecaron;nliv&ecaron; prosit</trd>,
    <trd>&scaron;kemrat  <klr>(o n&ecaron;co)</klr></trd>,
    <trd>&zcaron;adonit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>flehen</trd>,
    <trd>betteln <klr>(flehen)</klr></trd>,
    <trd>beknien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>beg</trd>,
    <trd>beseech</trd>,
    <trd>entreat</trd>,
    <trd>plead</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rogar</trd>,
    <trd>suplicar</trd>
  </trdgrp>
  <trd lng="fr">supplier</trd>
  <trd lng="he">&lamed;&he;&tav;<ind>&het;&nun;&fnun;</ind></trd>
  <trd lng="hu">k&ouml;ny&ouml;r&ouml;g</trd>
  <trd lng="nl">smeken</trd>
  <trd lng="pl">b&lstroke;aga&cacute;</trd>
  <trdgrp lng="pt">
    <trd>rogar</trd>,
     <trd>implorar</trd>,
    <trd>suplicar</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_m;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">naliehavo prosi&tcaron;</trd>
</drv>

<drv mrk="pet.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pet.for0i.KOMUNE">
    <dif>
      <tld lit="P"/>i de iu, ke li permesu foriri:
      <ekz>
        mi for<tld/>is min de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 13:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="permes.for0i">forpermesi</ref>
    <trd lng="de">um <ind>Entlassung</ind> bitten</trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">demander <ind>cong&eacute;</ind></trd>
  <trd lng="pt">pedir licen&ccedil;a</trd> 
  <trd lng="ru">&c_o;&c_t;&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="pet.pro0i">
  <kap>pro<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pet.pro0i.KOMUNE">
    <dif>
      Interhelpi por havigi al iu pardonon, favoron...:
      <ekz>
        li &ccirc;iam vivas, por pro<tld/>i pro ili
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 7:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;o Venceslao <klr>[&#x2026;]</klr> pledis por <nom>Hus</nom> kaj kun multaj nobeloj pro<tld/>is
        por li &ccirc;e la papo
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007031.html">Katolikismo: Johano Hus -- &ccirc;u ne plu herezulo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi konsilas do, anta&ubreve; &ccirc;io, fari petegojn, pre&gcirc;ojn, pro<tld/>adojn, dankojn por
        &ccirc;iuj
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 2:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>f&uuml;rbitten</trd>,
      <trd>sich f&uuml;r jemanden <ind>einsetzen</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol; <klr>(&c_z;&c_a;
  &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>intervenovat</trd>,
    <trd>orodovat</trd>,
    <trd>p&rcaron;imlouvat se</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>intercede</trd>,
    <trd>intermediate</trd>,
    <trd>mediate</trd>
  </trdgrp>
  <trd lng="es">interceder</trd>
  <trdgrp lng="fr">
    <trd>demander la <ind>gr&acirc;ce</ind> de</trd>,
    <trd>s'<ind>entremettre</ind> en faveur de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;r&ouml;g &eacute;rte</trd>,
    <trd>k&ouml;zbenj&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">voorspreken</trd>
  <trdgrp lng="pl">
    <trd>wstawia&cacute; si&eogonek; <klr>za kim&sacute;</klr></trd>,
    <trd>prosi&cacute; <klr>za kim&sacute;</klr></trd>
  </trdgrp>
  <trd lng="pt">interceder</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_z;&c_a; &c_k;&c_o;&c_g;&c_o;-&c_t;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>orodova&tcaron;</trd>,
    <trd>prihov&aacute;ra&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="pet.0skribo">
  <kap><tld/>skribo</kap>
  <snc mrk="pet.0skribo.POL">
    <dif>
      Skribita <tld/>o al oficialulo:
      <ekz>
        akcepti <tld/>oskribon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis prezentinta <tld/>skribon al la re&gcirc;o
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 19a, p.
        52a</lok></fnt>;
      </ekz>
      <ekz>
        neniu <tld/>skribo, peticio, provo akceptigi rekomendojn pri Esperanto valoras
        la perdon de tempo
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2005/008479.php">Rekomendoj plejparte senefikaj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>skribo estas lan&ccirc;ita: post unu semajno, &gcirc;i ricevis jam pli ol 15&#x202F;000
        subskribojn
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Spanjaard kaj Esperanto-Marseille</aut>
          <vrk><url ref="https://www.monato.be/2013/010999.php">Publika mono por privata koncerto</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="petici.0o">peticio</ref>
    <trd lng="de"><klr>(schriftlicher)</klr> <ind>Antrag</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_mol;&c_b;&c_a;
    <klr>(&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_z;&c_a;&c_ja;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>petice</trd>,
    <trd>p&iacute;semn&aacute; &zcaron;&aacute;dost</trd>,
    <trd>suplika</trd>,
    <trd>&zcaron;&aacute;dost</trd>
  </trdgrp>
  <trd lng="en">petition</trd>
  <trdgrp lng="fr">
    <trd>lettre de <ind>dol&eacute;ances</ind></trd>,
    <trd>p&eacute;tition</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;rv&eacute;ny</trd>,
    <trd>pet&iacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verzoekschrift</trd>,
    <trd>petitie</trd>
  </trdgrp>
  <trd lng="pl">petycja</trd>
  <trdgrp lng="pt">
      <trd>abaixo-assinado</trd>
      <trd>peti&ccedil;&atilde;o</trd>
   </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pet&iacute;cia</trd>,
    <trd>p&iacute;somn&aacute; &zcaron;iados&tcaron;</trd>,
    <trd>&zcaron;iados&tcaron;</trd>
  </trdgrp>
</drv>
</art>
