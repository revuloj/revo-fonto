<drv mrk="pet.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="pet.0i.KOMUNE">
    <dif>
      Sciigi al iu, ke oni deziras ion de li:
      <ekz>
        <tld/>i informon, helpon de iu;
      </ekz>
      <ekz>
        <tld/>i iun pri helpo, konsilo, permeso, pardono;
      </ekz>
      <ekz>
        sinjorino <klr>[&#x2026;]</klr> <tld/>is de &scirc;i trinki
        <fnt>
          <bib>F</bib>
          <lok>&FE; 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as, ke mi estu atenta
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as vin, rekonscii&gcirc;u kaj restu
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as vin akcepti de mi &ccirc;i tiun malgrandan donacon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u lin al mi!
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>
        <klr>(<tld/>u, ke li venu al mi)</klr>;
      </ekz>
      <ekz>
        sidi&gcirc;u, mi <tld/>as
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="postul.0i">postuli</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="invit.0i">inviti</ref>,
      <ref cel="dezir.0i">deziri</ref>,
      <ref cel="vol.bon0i">bonvoli</ref>,
      <ref cel="propon.0i">proponi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_nyata;&t_na;&t_tsheg;&t_zha;&t_shabchu;&t_tsheg;&t_-a;&t_ba;&t_shabchu;&t_la;&t_tsheg;</trd>
  <trd lng="br">goulenn<klr tip="ind"> (v.)</klr></trd>
  <trdgrp lng="cs">
    <trd>po&zcaron;&aacute;dat</trd>,
    <trd>prosit</trd>,
    <trd>&zcaron;&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>bitten</trd>,
      <trd>erbitten</trd>,
      <trd>beantragen</trd>,
      <trd>anfordern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ask for</trd>,
    <trd>request</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>pedir</trd>,
     <trd>solicitar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>demander<klr> (objet, renseignement...)</klr></trd>,
    <trd>prier</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&bet;&qof;&shin;</ind></trd>
  <trd lng="hu">k&eacute;r</trd>
  <trd lng="id">me<ind>minta</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x983C;&#x3080;  <pr>&#x305F;&#x306E;&#x3080;</pr></trd>,
    <trd>&#x8ACB;&#x3046;  <pr>&#x3053;&#x3046;</pr></trd>,
    <trd>&#x9858;&#x3046;  <pr>&#x306D;&#x304C;&#x3046;</pr></trd>,
    <trd>&#x4F9D;&#x983C;&#x3059;&#x308B;  <pr>&#x3044;&#x3089;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8ACB;&#x9858;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x304C;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x547C;&#x3076;  <pr>&#x3088;&#x3076;</pr></trd>,
