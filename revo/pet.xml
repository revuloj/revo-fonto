<drv mrk="pet.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pet.for0i.KOMUNE">
    <dif>
      <tld lit="P"/>i de iu, ke li permesu foriri:
      <ekz>
        mi for<tld/>is min de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 13:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="permes.for0i">forpermesi</ref>
    <trd lng="de">um <ind>Entlassung</ind> bitten</trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">demander <ind>cong&eacute;</ind></trd>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x4F11;&#x6687;&#x3092;&#x9858;&#x3044;&#x51FA;&#x308B;
  </ind><pr>&#x306E;&#x304D;&#x3085;&#x3046;&#x304B;&#x3092;&#x306D;&#x304C;&#x3044;&#x3067;&#x308B;</pr></trd>
  <trd lng="pt">pedir licen&ccedil;a</trd>
  <trd lng="ru">&c_o;&c_t;&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x8ACB;&#x5047; <pr>q&#x1D0;ngji&#xE0;</pr></trd>,
    <trd>&#x8BF7;&#x5047; <pr>q&#x1D0;ngji&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pet.pro0i">
  <kap>pro<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pet.pro0i.KOMUNE">
    <dif>
      Interhelpi por havigi al iu pardonon, favoron...:
      <ekz>
        li &ccirc;iam vivas, por pro<tld/>i pro ili
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 7:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;o Venceslao <klr>[&#x2026;]</klr> pledis por <nom>Hus</nom> kaj kun multaj nobeloj pro<tld/>is
        por li &ccirc;e la papo
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        mi konsilas do, anta&ubreve; &ccirc;io, fari petegojn, pre&gcirc;ojn, pro<tld/>adojn, dankojn por
        &ccirc;iuj
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 2:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>f&uuml;rbitten</trd>,
      <trd>sich f&uuml;r jemanden <ind>einsetzen</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol; <klr>(&c_z;&c_a;
  &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>intervenovat</trd>,
    <trd>orodovat</trd>,
    <trd>p&rcaron;imlouvat se</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>intercede</trd>,
    <trd>intermediate</trd>,
    <trd>mediate</trd>
  </trdgrp>
  <trd lng="es">interceder</trd>
  <trdgrp lng="fr">
    <trd>demander la <ind>gr&acirc;ce</ind> de</trd>,
    <trd>s'<ind>entremettre</ind> en faveur de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;r&ouml;g &eacute;rte</trd>,
    <trd>k&ouml;zbenj&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>me<ind>mediasi</ind>kan</trd>,
    <trd><baz>tengah</baz>menengahi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3068;&#x308A;&#x306A;&#x3059;</trd>,
    <trd>&#x4EF2;&#x306B;&#x5165;&#x3063;&#x3066;&#x5606;&#x9858;&#x3059;&#x308B;
    <pr>&#x306A;&#x304B;&#x306B;&#x306F;&#x3044;&#x3063;&#x3066;&#x305F;&#x3093;&#x304C;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorspreken</trd>
  <trdgrp lng="pl">
    <trd>wstawia&cacute; si&eogonek; <klr>za kim&sacute;</klr></trd>,
    <trd>prosi&cacute; <klr>za kim&sacute;</klr></trd>
  </trdgrp>
  <trd lng="pt">interceder</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_z;&c_a; &c_k;&c_o;&c_g;&c_o;-&c_t;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>orodova&tcaron;</trd>,
    <trd>prihov&aacute;ra&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_i; &c_z;&c_a; &c_k;&c_o;&c_g;&c_o;-&c_n;. (&c_z; &c_m;&c_je;&c_t;&c_o;&c_ju;
    &c_v;&c_i;&c_b;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_l;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="pet.mon0o">
  <kap>
    mon<tld/>o
  </kap>
  <snc>
    <dif>
      El<tld/>o de mono, ordinare por iu bonafara celo, projekto ks:
      <ekz>
        mon<tld/>o por la tanzaniaj lernejoj
        <fnt>
          <aut>Saliko</aut>
            <vrk><url ref="&Monato;/2001/007297.html">Kolekti sonorilojn</url></vrk>
