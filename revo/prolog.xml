<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>prolog</rad>/o
</kap>
<drv mrk="prolog.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Parto de teatra&jcirc;o, kiu anoncas kaj preparas la agon.
    </dif>
    <refgrp tip="sin">
      <ref cel="parol.antaux0o">anta&ubreve;parolo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_jo;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Vorrede</trd>,
    <trd>Vorspiel</trd>,
    <trd>Prolog</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;hang</trd>,
    <trd>el&odblac;j&aacute;t&eacute;k</trd>,
    <trd>el&odblac;verseng&eacute;s</trd>,
    <trd>prol&oacute;gus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorrede</trd>,
    <trd>proloog</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sv">prolog</trd>
</drv>
</art>
