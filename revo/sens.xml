<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>sens</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sens.0o">
  <kap><tld/>o</kap>
  <snc mrk="sens.0o.FIZL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">FIZL</uzo>
    <ref tip="dif" cel="sent.0umo">Sentumo</ref>
    <ekz>
      voja&gcirc;oj instigas anka&ubreve; ordinarajn homojn al pli intensa
      perceptado
      de la okaza&jcirc;oj, des pli poetojn, homojn kun viglaj, fajnaj
      <tld/>oj
      <fnt>
      <aut>F.-G. R&ouml;ssler</aut>
      <vrk>Profite voja&gcirc;u kun Edwin</vrk>
      <bib>Monato</bib>
      <lok>1994:6, p. 21a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>schopnost vn&iacute;mat</trd>,
    <trd>smysl <klr>(vn&iacute;mac&iacute;)</klr></trd>
  </trdgrp>
  <trd lng="de">Sinn</trd>
  <trd lng="en">sense</trd>
  <trd lng="fr">sens<klr> (perceptif)</klr></trd>
  <trd lng="hu">&eacute;rz&eacute;kszerv</trd>
  <trd lng="it">senso<klr> (percezione)</klr></trd>
  <trd lng="nl">zintuig</trd>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;
  &c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_ja;:
  &c_s;&c_l;&c_u;&c_h;, &c_z;&c_r;&c_je;&c_n;&c_i;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>zmyslov&yacute; org&aacute;n</trd>,
    <trd>&uacute;stroj</trd>
  </trdgrp>
</drv>

<drv mrk="sens.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      La kvin sentumoj.
    </dif>
  </snc>
  <trd lng="cs">vn&iacute;mac&iacute; smysly</trd>
  <trd lng="fr">les cinq <ind>sens</ind></trd>
  <trd lng="hu">az &ouml;t <ind>&eacute;rz&eacute;kszerv</ind></trd>
  <trd lng="it">i cinque <ind>sensi</ind></trd>
  <trd lng="nl">zintuigen</trd>
  <trd lng="sk">vn&iacute;macie zmysly</trd>
</drv>

<drv mrk="sens.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <ref tip="dif" cel="sent.0umajxo">Sentuma&jcirc;o</ref>
  </snc>
  <trd lng="fr">sensation</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;kel&eacute;s</trd>,
    <trd>&eacute;rz&eacute;klet</trd>
  </trdgrp>
  <trd lng="it">sensazione</trd>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
</drv>

<drv mrk="sens.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="sens.0ilo.TEK">
    <ref tip="dif" cel="sent.0ilo.TEK">sentilo<sncref/></ref>
    <ekz>
      malgrandaj <tld/>iloj kontrolos la staton de paciento dum kirurgia
      operacio
      <fnt>
      <aut>G. Cazzaro</aut>
      <vrk>Mikroskope vastaj horizontoj</vrk>
      <bib>Monato</bib>
      <lok>1997:2, p. 12a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>bu&ncaron;ka p&rcaron;&iacute;jmaj&iacute;c&iacute; vjemy</trd>,
    <trd>senzila</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Geberger&auml;t</trd>;
      <trd>F&uuml;hler</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>sensor</trd>, <trd>sensing device</trd>, <trd>pickup</trd>,
      <trd>transducer</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>capteur</trd>,
    <trd>senseur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;kel&odblac;</trd>,
    <trd>szenzor</trd>
  </trdgrp>
  <trd lng="it">sensore</trd>
  <trd lng="pl">czujnik</trd>
  <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_t;&c_ch;&c_i;&c_k;</trd>;
      <trd>&c_shch;&c_u;&c_p;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>bunka prij&iacute;maj&uacute;ca vnemy</trd>,
    <trd>senzila</trd>
  </trdgrp>
</drv>

<drv mrk="sens.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="sens.0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Doktrino, la&ubreve; kiu &ccirc;ia kono venas de <tld/>a&jcirc;oj kaj nur de
      ili; <ref tip="sin" cel="sensua.0o">sensualismo</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="empiri.0ismo">empiriismo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">senzualizmus</trd>
  <trd lng="de">Sensualismus</trd>
  <trd lng="fr">sensualisme<klr> (philosophique)</klr></trd>
  <trd lng="hu">szenzualizmus</trd>
  <trdgrp lng="it">
    <trd>sensismo<klr> (fil.)</klr></trd>, 
    <trd>sensualismo<klr> (fil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">sensualisme</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_s;&c_u;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">senzualizmus</trd>
</drv>

<drv mrk="sens.elektro0o">
  <kap>elektro<tld/>o</kap>
  <snc mrk="sens.elektro0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="sens.0o.FIZL"><tld lit="S"/>o</ref>,
      per kiu oni perceptas elektrajn kampojn:
      <ekz>
        elektro<tld/>o estas kutime nur utila por &scirc;arkoj
        kaj aliaj subakvaj bestoj, pro tio ke la sala akvo
        pli bone tralasas la elektrajn signalojn ol aero
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url
          ref="https://scivolemo.wordpress.com/2018/05/02/mamuloj-kiuj-venas-el-ovoj-la-monotremoj/">
          Mamuloj kiuj venas el ovoj: la&nbsp;monotremoj</url></vrk>
          <lok>Scivolemo, 2018-05-02</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;lectroperception</trd>
</drv>
</art>
