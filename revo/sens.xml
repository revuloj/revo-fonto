    <dif>
      <ref tip="dif" cel="vid.0povo.SEN">Vidpovo</ref>:
      <ekz>
        la lumintensa funkcio <klr>[&#x2026;]</klr> priskribas la mezan
        spektran <tld/>a&jcirc;on de homa vid<tld/>o de brileco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=480167">Lumintensa
          funkcio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>B&eacute;la Julesz</nom> <klr>[&#x2026;]</klr> faris gravajn kontribuojn
        al la teorio de homa vid<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=479066">B&eacute;la
          Julesz</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_o;&c_k;</trd>
  <trd lng="cs">zrak</trd>
  <trdgrp lng="de">
    <trd>Sehf&auml;higkeit</trd>,
    <trd>Gesichtssinn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ability to see</trd>,
    <trd>sight</trd>
  </trdgrp>
  <trd lng="es">vista<klr> (sentido)</klr></trd>
