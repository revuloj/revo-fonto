  <trd lng="pt">vis&atilde;o<klr> (sentido)</klr></trd>
  <trd lng="ru">&c_z;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>schopnos&tcaron; vidie&tcaron;</trd>,
    <trd>videnie</trd>,
    <trd>zrak</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_ib;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x8996;&#x89BA; <pr>sh&#xEC;ju&#xE9;</pr></trd>,
