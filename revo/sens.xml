</drv>

<drv mrk="sens.auxd0o">
  <kap>a&ubreve;d<tld/>o</kap>
  <snc mrk="sens.auxd0o.SEN">
    <dif>
      <ref tip="dif" cel="auxd.0povo.SEN">A&ubreve;dpovo</ref>:
      <ekz>
        cirklodancoj <klr>[povas]</klr> plibonigi la kunordigon inter la
        vidsenso, la a&ubreve;d<tld/>o kaj la korpmovado
        <fnt>
          <aut>Danka Leyk</aut>
          <vrk><url ref="http://gresillon.org/IMG/rtf/cirklodancoj.rtf">Cirklodancoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        beleco alparolas &ccirc;efe al la vidsenso, sed ekzistas beleco por la
        a&ubreve;d<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=137234">Belo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;</trd>
  <trdgrp lng="de">
    <trd>Geh&ouml;r</trd>,
    <trd>H&ouml;rverm&ouml;gen</trd>
  </trdgrp>
  <trd lng="en">sense of hearing</trd>
  <trdgrp lng="es">
    <trd>audici&oacute;n<klr> (sentido)</klr></trd>,
    <trd>o&iacute;do<klr> (sentido)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>audition<klr> (sens)</klr></trd>,
    <trd>ou&iuml;e<klr> (sens)</klr></trd>
  </trdgrp>
  <trd lng="it">udito<klr> (senso)</klr></trd>
  <trd lng="nl">gehoor</trd>
  <trd lng="no">h&oslash;rsel</trd>
  <trd lng="pl">s&lstroke;uch</trd>
  <trd lng="pt">audi&ccedil;&atilde;o<klr> (sentido)</klr></trd>
  <trd lng="uk">&c_s;&c_l;&c_u;&c_h;</trd>
  <trdgrp lng="zh">
    <trd>&#x542C;&#x89C9; <pr>t&#x12B;ngju&#xE9;</pr></trd>,
    <trd>&#x807D;&#x89BA; <pr>t&#x12B;ngju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sens.elektro0o">
  <kap>elektro<tld/>o</kap>
  <snc mrk="sens.elektro0o.SEN">
    <dif>
      <ref tip="super" cel="sens.0o.FIZL"><tld lit="S"/>o</ref>,
      per kiu iuj bestoj perceptas elektrajn kampojn:
      <ekz>
        elektro<tld/>o estas kutime nur utila por &scirc;arkoj
        kaj aliaj subakvaj bestoj, pro tio ke la sala akvo
        pli bone tralasas la elektrajn signalojn ol aero
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2018/05/02/mamuloj-kiuj-venas-el-ovoj-la-monotremoj/">
          Mamuloj kiuj venas el ovoj: la&nbsp;monotremoj</url></vrk>
          <lok>Scivolemo, 2018-05-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_r;&c_e;&c_c;&c_e;&c_p;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Elektrosinn</trd>
  <trd lng="en">electroreception</trd>
  <trd lng="es">electrorrecepci&oacute;n</trd>
  <trd lng="fr">&eacute;lectroperception</trd>
  <trd lng="pl">zmys&lstroke; elektryczny</trd>
  <trdgrp lng="pt">
     <trd>eletrorrecep&ccedil;&atilde;o</trd>
    <trd>eletropercep&ccedil;&atilde;o</trd>
  </trdgrp>
</drv>

<drv mrk="sens.flar0o">
  <kap>flar<tld/>o</kap>
  <snc mrk="sens.flar0o.SEN">
    <dif>
      <ref tip="dif" cel="flar.0povo.SEN">Flarpovo</ref>:
      <ekz>
        krom spira organo, nazo anka&ubreve; estas la organo de la flar<tld/>o,
        kiu intime ligi&gcirc;as kun la gustumado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=226599">Nazo
          (organo)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        flar<tld/>o <klr>[de &ccirc;evalo]</klr> kvankam multe pli bona ol tiu
        de homoj, ne estas tiom bona kiom tiu de hundo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=774">&Ccirc;evalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        murenoj <klr>[&#x2026;]</klr> &ccirc;asas aliajn bestojn per tre fajna
        flar<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=131721">Mureno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ju;&c_h;</trd>
  <trdgrp lng="de">
    <trd>Riechsinn</trd>,
    <trd>Geruchssinn</trd>
  </trdgrp>
  <trd lng="en">sense of smell</trd>
  <trd lng="es">olfato<klr> (sentido)</klr></trd>
  <trdgrp lng="fr">
    <trd>flair</trd>,
    <trd>odorat</trd>
  </trdgrp>
  <trd lng="hu">szagl&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x81ED;&#x899A;  <pr>&#x3057;&#x3085;&#x3046;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x55C5;&#x899A;  <pr>&#x304D;&#x3085;&#x3046;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">reukzin</trd>
  <trdgrp lng="pl">
    <trd>w&eogonek;ch</trd>,
    <trd>powonienie</trd>
  </trdgrp>
  <trd lng="pt">olfato</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_ju;&c_h;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_n;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_ch;&c_u;&c_t;&c_mol;&c_jo;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_ju;&c_h;</trd>
  <trdgrp lng="zh">
    <trd>&#x55C5;&#x89BA; <pr>xi&#xF9;ju&#xE9;</pr></trd>,
    <trd>&#x55C5;&#x89C9; <pr>xi&#xF9;ju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sens.gusto0o">
  <kap>gusto<tld/>o</kap>
  <snc mrk="sens.gusto0o.SEN">
    <dif>
      <ref tip="super" cel="sens.0o.FIZL"><tld lit="S"/>o</ref>,
      per kiu oni perceptas salecon, dol&ccirc;econ, amarecon, acidecon
      en la bu&scirc;o:
      <ekz>
        iuj el ili iam, ekzemple unufoje dum kelkaj semajnoj,
        por kontentigi gusto<tld/>on, man&gcirc;as ion a&ubreve; trinkas
        <fnt>
          <aut>J. M. Werdin</aut>,
          <vrk><url ref="http://inedia.info/esperant/">
          Senman&gcirc;a vivstilo</url></vrk>,
          <lok>2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;e la proteo]</klr> la bur&gcirc;onoj de la gusto<tld/>o estas
        en la muka
        epitelio de la bu&scirc;o, plej parto &ccirc;e la supra flanko de la lango
        kaj en
        la enirejo al la truoj de la brankoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=208425">Proteo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_k; <klr tip="amb">(&c_p;&c_a;&c_ch;&c_u;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
  <trd lng="de">Geschmackssinn</trd>
  <trd lng="en">sense of taste</trd>
  <trd lng="es">gusto<klr> (sentido)</klr></trd>
  <trd lng="fr">gout<klr> (sens)</klr></trd>
  <trd lng="ja">&#x5473;&#x899A;  <pr>&#x307F;&#x304B;&#x304F;</pr></trd>
  <trd lng="pt">paladar</trd>
  <trd lng="pl">smak<klr> (zmys&lstroke;)</klr></trd>
  <trd lng="uk">&c_s;&c_m;&c_a;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x5473;&#x89BA; <pr>w&#xE8;iju&#xE9;</pr></trd>,
    <trd>&#x5473;&#x89C9; <pr>w&#xE8;iju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sens.magneto0o">
  <kap>magneto<tld/>o</kap>
  <snc mrk="sens.magneto0o.SEN">
    <dif>
      <ref tip="super" cel="sens.0o.FIZL"><tld lit="S"/>o</ref>,
      per kiu iuj bestoj perceptas magnetajn kampojn
      de la tero:
      <ekz>
        eksperimentaj teknikoj kiel genaj noka&ubreve;toj
        permesis pli detalan ekzamenon de ebla
        magneto<tld/>ado en <klr>...</klr> bananmu&scirc;oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Magnetosensivo">
          magnetosensivo</url>, 2017-08-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_a;&c_r;&c_e;&c_c;&c_e;&c_p;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Magnetsinn</trd>
  <trd lng="en">magnetoreception</trd>
  <trd lng="es">magnetorrecepci&oacute;n</trd>
  <trd lng="fr">magn&eacute;toception</trd>
  <trd lng="pl">zmys&lstroke; magnetyczny</trd>
</drv>

<drv mrk="sens.tusxo0o">
  <kap>tu&scirc;o<tld/>o,
  <var><kap>tu&scirc;<tld/>o</kap></var></kap>
  <snc mrk="sens.tusxo0o.SEN">
    <dif>
      <ref tip="super" cel="sens.0o.FIZL"><tld lit="S"/>o</ref>,
      per kiu oni perceptas kontra&ubreve;premon, glatecon, varmecon ktp.
      de tu&scirc;ata&jcirc;o:
      <ekz>
        je la tu&scirc;o<tld/>o &gcirc;i estas glata, malvarma kaj malmola
        <fnt>
          <aut>B. Russell, trad. J. Alkasaro</aut>,
          <vrk><url ref="http://www.enxarxa.com/esperanto/RUSSELL%20La%20problemoj%20de%20la%20filozofio.pdf">
           La problemoj de la filozofio</url></vrk>,
          <lok>[2004?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        maketo uzata de blinduloj por kompreni pere de la tu&scirc;o<tld/>o
        kia estas &ccirc;u konstrua&jcirc;o &ccirc;u urbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=262514">Maketo
          (arkitekturo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_t;&c_y;&c_k; <klr tip="amb">(&c_p;&c_a;&c_ch;&c_u;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
  <trd lng="de">Tastsinn</trd>
  <trdgrp lng="en">
    <trd>haptic perception</trd>,
    <trd>sense of touch</trd>
  </trdgrp>
  <trd lng="es">tacto<klr> (sentido)</klr></trd>
  <trd lng="fr">toucher<klr> (sens)</klr></trd>
  <trd lng="ja">&#x89E6;&#x899A;  <pr>&#x3057;&#x3087;&#x3063;&#x304B;&#x304F;</pr></trd>
  <trd lng="pl">dotyk<klr> (zmys&lstroke;)</klr></trd>
  <trd lng="pt">tato<klr> (sentido)</klr></trd>
  <trd lng="uk">&c_d;&c_o;&c_t;&c_i;&c_k; <klr>(&c_ch;&c_u;&c_t;&c_t;&c_ja;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x89E6;&#x89C9; <pr>ch&#xF9;ju&#xE9;</pr></trd>,
    <trd>&#x89F8;&#x89BA; <pr>ch&#xF9;ju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sens.vid0o">
