  <trdgrp lng="ja">
    <trd>&#x611F;&#x3058;&#x308B;  <pr>&#x304B;&#x3093;&#x3058;&#x308B;</pr></trd>,
    <trd>&#x77E5;&#x899A;&#x3059;&#x308B;  <pr>&#x3061;&#x304B;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odczuwa&cacute;</trd>,
