<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>sens</rad>/o
</kap>
<drv mrk="sens.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="sent.0umo">Sentumo</ref>.
    </dif>
  </snc>
  <trd lng="de">Sinn</trd>
  <trd lng="en">sense</trd>
  <trd lng="nl">zintuig</trd>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_ja;:
  &c_s;&c_l;&c_u;&c_h;, &c_z;&c_r;&c_je;&c_n;&c_i;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
</drv>

<drv mrk="sens.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      La kvin sentumoj.
    </dif>
  </snc>
  <trd lng="nl">zintuigen</trd>
</drv>

<drv mrk="sens.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="sent.0umajxo">Sentuma&jcirc;o</ref>, sensacio 2.
    </dif>
    <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  </snc>
</drv>

<drv mrk="sens.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, la&ubreve; kiu &ccirc;ia kono venas de la <tld/>a&jcirc;oj kaj nur de ili.
    </dif>
    <refgrp tip="sin">
      <ref cel="sensua.0o">sensualismo</ref>,
      <ref cel="empiri.0ismo">empiriismo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">sensualisme</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_s;&c_u;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
