<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buhxt1.xml,v 1.4 2022/05/13 06:43:00 revo Exp $">
<kap>
  <rad>bu&hcirc;t</rad>/o
</kap>
<drv mrk="buhxt1.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="buhxt1.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Tre malgranda <ref tip="super" cel="golf.0o.GEOG">golf</ref>o:
      <ekz>
        la norda marbordo de la <klr>[&#8230;]</klr> <tld/>o
        <fnt>
        <aut>D. J. Harlow</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/tobias_e.html">
        El la Taglibro de Tobias Finch</url></vrk>,
        <lok>1986</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni e&ccirc; sukcesis okupi etan <tld/>on duone malanta&ubreve; roko, kun
        bona sablo
        <fnt>
        <aut>-</aut>
        <vrk><url ref="http://dunudaj.blogspot.fr/2005/08/someraj-tagoj-nudbrustaj.html">
        Someraj tagoj, nudbrustaj esperantistinoj</url></vrk>,
        <lok>Du nudaj amikinoj, 2005-08-05</lok>
        </fnt>;
      </ekz>
      <ekz>
         la rifto dividi&gcirc;as en nordorienta kaj nordokcidenta bran&ccirc;oj, al la nordokcidenta bran&ccirc;o
         apartenas la malsupra rejna <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=432533">Mediteraneo-Mjoso-zono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bucht</trd>
    <trd lng="en">bay</trd>
  </snc>
  <rim>
    Anka&ubreve; ia far&ccirc;ita pasta&jcirc;o estas nomata
    <ref tip="hom" cel="buhxt.0o.KUI">bu&hcirc;to</ref>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_h;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">anse<klr> (marine)</klr></trd>
  <trd lng="uk">&#x43F;&c_i;&c_r;&c_ib;&c_zh;&#x43E;&#x43A; &c_z; &#x43D;&c_a;&c_ch;&c_i;&#x43D;&#x43A;&#x43E;&#x44E;</trd>
</drv>
</art>

