<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kamizo.xml,v 1.16 2020/12/13 22:43:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kamizol</rad>/o
</kap>
<drv mrk="kamizo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Manikhava <ref tip="super" cel="vest.sub0o">subvesto</ref>
      el lano a&ubreve; kotono, kovranta la supran parton
      de la korpo, &gcirc;is la lumboj:
      <ekz>
        &scirc;i estis vestita per strikta <tld/>o
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">camisola</trd>
    <trdgrp lng="en">
      <trd>undershirt</trd>,
      <trd>camisole</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>maillot de corps</trd>,
      <trd>chemise<klr tip="ind"> (sous-v&ecirc;tement)</klr></trd>
    </trdgrp>
    <trd lng="hu">ujjas</trd>
  </snc>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="jak.0o">jako</ref>:
      <ekz>
        la nigra <tld/>o de la patricioj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la flavleda <tld/>o de la rajdoservisto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tunik.0o">tuniko</ref>,
      <ref cel="jxak.0o">&jcirc;ako</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>jac</trd>,
      <trd>jaqueta</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>jerkin</trd>,
      <trd>doublet</trd>
    </trdgrp>
    <trd lng="fr">camisole</trd>
    <trdgrp lng="hu">
      <trd>zeke</trd>,
      <trd>zubbony</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_z;&c_o;&c_l;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_z;&c_e;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kamizola</trd>,
    <trd>kamizolka</trd>,
    <trd>kazajka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Weste</trd>,
    <trd>Wams [ark]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>borstrok</trd>,
    <trd>buis<klr tip="ind"> (borstrok)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kaftan</trd>,
    <trd>kamizelka</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>caftan </trd>,
    <trd>jiletc&abreve;</trd>,
    <trd>vest&abreve;</trd>
  </trdgrp>   
  <trd lng="ru">&c_k;&c_a;&c_m;&c_z;&c_o;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>kamizola</trd>,
    <trd>kazajka</trd>
  </trdgrp>
</drv>
</art>
