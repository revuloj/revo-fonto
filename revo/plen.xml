  <trd lng="hu">&uuml;res</trd>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3089;&#x306E;</trd>,
    <trd>&#x7A7A;&#x3044;&#x3066;&#x3044;&#x308B;  [<ind>&#x3042;&#x3044;&#x3066;&#x3044;&#x308B;</ind>]</trd>,
    <trd>&#x6B20;&#x3051;&#x305F;  [<ind>&#x304B;&#x3051;&#x305F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">leeg</trd>
