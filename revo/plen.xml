    <trd>&#x9055;&#x53CD;&#x3059;&#x308B;  [<ind>&#x3044;&#x306F;&#x3093;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nie dope&lstroke;ni&cacute;</trd>,
