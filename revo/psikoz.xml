<vortaro>
<art mrk="$Id: psikoz.xml,v 1.20 2026/02/24 22:51:43 revo Exp $">
<kap>
  <rad>psikoz</rad>/o <fnt><bib>SPV</bib></fnt>,
  <var><kap><rad var="&hcirc;">psi&hcirc;oz</rad>/o</kap></var>
</kap>
<drv mrk="psikoz.0o">
  <kap><tld/>o, <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <snc mrk="psikoz.0o.PSI">
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="lst" cel="san.mensmal0o.MED" lst="voko:malsanoj">Mensmalsano</ref> ne ka&ubreve;zata
      de organa  lezo a&ubreve; difekti&gcirc;o, kontraste al <ref tip="vid" cel="neuxro1.0o">ne&ubreve;rozo</ref>:
      <ekz>
        mania-deprimiga <tld/>o;
      </ekz>
      <ekz>
         kaj <tld/>o de tiu batalludo kaptis la tutan mondon
          <fnt>
            <aut>I. Lejzerowicz</aut>
            <vrk>El la "Verda Biblio"</vrk>
            <lok>1935</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi trovis medicinan konkludon, atestantan &ccirc;e la &scirc;ipanaro de &#8218;<nom>Ramfornicus</nom>&#8216;
          kolektivan halucinogenan <tld var="&hcirc;"/>on
          <fnt>
            <aut>S. Lem, trad. J. Finkel</aut>
            <vrk><url ref="https://www.esperanto.mv.ru/Lem/wl_2a.html">
            Surloka esploro</url></vrk>
            <lok>2003-2014</lok>
          </fnt>.
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_o;&c_z;</trd>
  <trd lng="cs">psych&oacute;za</trd>
  <trd lng="en">psychosis</trd>
  <trd lng="fa">&a_r;&a_w;&a_A;&a_n;&#8204;&f_p;&a_r;&f_y;&a_sh;&f_y;</trd>
  <trd lng="fr">psychose</trd>
  <trdgrp lng="hu">
    <trd>elmebaj</trd>,
    <trd>elmebetegs&eacute;g</trd>,
    <trd>pszich&oacute;zis</trd>
  </trdgrp>
  <trd lng="id">psikosis</trd>
  <trd lng="it">psicosi</trd>
  <trdgrp lng="ja">
    <trd>&#x7CBE;&#x795E;&#x75C5;  <pr>&#x305B;&#12356;&#12375;&#12435;&#12403;&#12423;&#12358;</pr></trd>,
    <trd>&#x7CBE;&#x795E;&#x969C;&#x5BB3;  <pr>&#x305B;&#12356;&#12375;&#12435;&#12375;&#12423;&#12358;&#x304C;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">psychose</trd>
  <trd lng="pl">psychoza</trd>
  <trd lng="pt">psicose</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_o;&c_z;</trd>
  <trd lng="sk">psych&oacute;za</trd>
  <trd lng="uk">&#x43F;&c_s;&c_i;&c_h;&#x43E;&c_z;</trd>
  <trdgrp lng="zh">
    <trd>&#x7CBE;&#x795E;&#x75C5; <pr>j&#x12B;ngsh&#xE9;nb&#xEC;ng</pr></trd>,
    <trd>&#x601D;&#x89BA;&#22833;&#x8ABF; <pr>s&#x12B;ju&#xE9;sh&#x12B;ti&#xE1;o</pr></trd>,
    <trd>&#x601D;&#x89C9;&#22833;&#x8C03; <pr>s&#x12B;ju&#xE9;sh&#x12B;ti&#xE1;o</pr></trd>
  </trdgrp>
</drv>
