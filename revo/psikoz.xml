<art mrk="$Id: psikoz.xml,v 1.17 2023/09/03 12:45:13 revo Exp $">
<kap>
  <rad>psikoz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="psikoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="psikoz.0o.PSI">
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
    <dif>
      Mensa <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref> ne ka&ubreve;zata, kiel la
      <ref tip="vid" cel="neuxro1.0o">ne&ubreve;rozo</ref>, de organa
      lezo a&ubreve; difekti&gcirc;o:
      <ekz>
        mania-deprimiga <tld/>o;
      </ekz>
      <ekz>
         kaj <tld/>o de tiu batalludo kaptis la tutan mondon
          <fnt>
            <aut>Izrael Lejzerowicz</aut>,
            <vrk>El la "Verda Biblio"</vrk>,
            <lok><url ref="https://www.fadedpage.com/showbook.php?pid=20180733">
          Fadedpage</url>
            </lok>
          </fnt>.
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_o;&c_z;</trd>
  <trd lng="cs">psych&oacute;za</trd>
  <trd lng="en">psychosis</trd>
  <trd lng="fa">&a_r;&a_w;&a_A;&a_n;&zwnj;&f_p;&a_r;&f_y;&a_sh;&f_y;</trd>
  <trd lng="fr">psychose</trd>
  <trdgrp lng="hu">
    <trd>elmebaj</trd>,
    <trd>elmebetegs&eacute;g</trd>,
    <trd>pszich&oacute;zis</trd>
  </trdgrp>
  <trd lng="id">psikosis</trd>
  <trd lng="it">psicosi</trd>
  <trdgrp lng="ja">
    <trd>&#x7CBE;&#x795E;&#x75C5;  [<ind>&#x305B;&#x3044;&#x3057;&#x3093;&#x3073;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x7CBE;&#x795E;&#x969C;&#x5BB3;  [<ind>&#x305B;&#x3044;&#x3057;&#x3093;&#x3057;&#x3087;&#x3046;&#x304C;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">psychose</trd>
  <trd lng="pl">psychoza</trd>
  <trd lng="pt">psicose</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_o;&c_z;</trd>
  <trd lng="sk">psych&oacute;za</trd>
  <trd lng="uk">&#x43F;&#x441;&#x438;&#x445;&#x43E;&#x437;</trd>
</drv>
