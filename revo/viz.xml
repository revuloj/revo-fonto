<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: viz.xml,v 1.30 2025/10/03 20:30:19 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>viz</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="viz.0o">
  <kap><tld/>o</kap>
  <snc mrk="viz.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="permes.0o">Permeso</ref> transiri limon de iu lando, enskribita en pasporton:
      <ekz>
        vi ne rajtas restadi &ccirc;i tie en Nederlando sen paperoj kun la <tld/>o de niaj
        instancoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi volas vojagi al Tajvano, vi devas havi Tajvanan
        <klr>[&#x2026;]</klr> <tld/>on, kiu ne validas en aliaj partoj de Tero
        <fnt>
        <aut>J. M. Werdin, Wang C.-Y.</aut>,
        <vrk>Tajvano au &Ccirc;inujo</vrk>,
        <lok>soc.culture.esperanto, 2004-02-16</lok></fnt>.
      </ekz>
      <ekz>
        Aniko proponis organizi &ccirc;ion: la akiron de la <tld/>oj, la luon de la hotelo, ktp
        <fnt>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_z;&c_a;</trd>
  <trd lng="bo">&t_la;&t_ga;&t_tsheg;&t_-a;&t_kha;&t_yata;&t_drengbu;&t_ra;&t_tsheg;</trd>
  <trd lng="ca">visat</trd>
  <trd lng="cs">v&iacute;zum</trd>
  <trd lng="de">Visum</trd>
  <trd lng="el">&beta;&jota_ton;&zeta;&alfa;</trd>
  <trd lng="en">visa</trd>
  <trd lng="es">visa</trd>
  <trd lng="fr">visa</trd>
  <trd lng="hu">v&iacute;zum</trd>
  <trdgrp lng="ja">
    <trd>&#x30D3;&#x30B6;  <pr>&#x3073;&#x3056;</pr></trd>,
    <trd>&#x67FB;&#x8A3C;  <pr>&#x3055;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">visum</trd>
  <trd lng="pl">wiza</trd>
  <trd lng="pt">visto</trd>
  <trd lng="ru">&c_v;&c_i;&c_z;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>vidovanie</trd>,
    <trd>v&iacute;zum</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_z;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7B7E;&#x8BC1; <pr>qi&amacron;nzh&egrave;ng</pr></trd>,
    <trd>&#x7C3D;&#x8B49; <pr>qi&amacron;nzh&egrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="viz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="viz.0i.POL">
    <dif>
      Enskribi en pasporton, permeson transiri limon:
      <ekz>
        dokumento, kiun <tld/>as la doganisto
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>aldonoj, malgranda marista
        terminaro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">visar</trd>
  <trd lng="cs">vlo&zcaron;it v&iacute;zum do pasu <klr>(raz&iacute;tkem)</klr></trd>
  <trd lng="de">ein Visum <ind>ausstellen</ind></trd>
  <trd lng="en">to visa</trd>
  <trd lng="es">visar</trd>
  <trd lng="fr">viser<klr tip="amb"> (un document)</klr></trd>
  <trd lng="hu">v&iacute;zummal ell&aacute;t</trd>
  <trd lng="ja">&#x67FB;&#x8A3C;&#x3059;&#x308B;  <pr>&#x3055;&#x3057;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">viseren</trd>
  <trdgrp lng="pl">
    <trd>wizowa&cacute;</trd>,
    <trd>przyk&lstroke;ada&cacute; <ind>wiz&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>da&tcaron; v&iacute;zum</trd>,
    <trd>vidova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="viz.labor0o">
  <kap>labor<tld/>o,
  <var><kap>labora <tld/>o</kap></var></kap>
  <snc mrk="viz.labor0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="viz.0o.POL"><tld lit="V"/>o</ref>
      permesanta restadon en la koncerna lando por laborado:
      <ekz>
        Budio estas indoneziano, kaj tial bezonas
        labor<tld/>on por labori tie &ccirc;i
        <fnt>
          <aut>A. &Ubreve;esto</aut>,
          <vrk>mia ofico</vrk>,
          <lok><url ref="http://www.andywest.org/eo/numeroj/a05p2.html">
          La Po&scirc;to Dismeo</url>, 2005-08-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_a;&c_ja; <ind>&c_v;&c_ib;&c_z;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Arbeitsvisum</trd>,
    <trd>Gesch&auml;ftsvisum</trd>
  </trdgrp>
  <trd lng="en">work visa</trd>
  <trd lng="fr">visa de travail</trd>
  <trd lng="nl">arbeidsvisum</trd>
  <trdgrp lng="pl">
    <trd>wiza pracownicza</trd>,
    <trd>wiza z pozwoleniem na prac&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="viz.stud0o">
  <kap>stud<tld/>o,
  <var><kap>studenta <tld/>o</kap></var></kap>
  <snc mrk="viz.stud0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="viz.0o.POL"><tld lit="V"/>o</ref>
      permesanta restadon en la koncerna lando por studado:
      <ekz>
        studenta <tld/>o eldonata surbaze de invito de
        rusia lernejo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://vojagxo.info/vizo-kaj-doganejo-de-rusio/">
          Vizo kaj Doganejo de Rusio</url></vrk>,
          <lok>La Voja&gcirc;o, 2013-11-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_u;&c_d;&c_e;&c_n;&c_c;&c_k;&c_a;&c_ja; <ind>&c_v;&c_ib;&c_z;&c_a;</ind></trd>
  <trd lng="de">Studentenvisum</trd>
  <trd lng="en">student visa</trd>
  <trd lng="fr">visa d'&eacute;tudes</trd>
  <trd lng="nl">studentenvisum</trd>
  <trd lng="pl">wiza studencka</trd>
</drv>

<drv mrk="viz.transita0o">
  <kap>transita <tld/>o</kap>
  <snc mrk="viz.transita0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="viz.0o.POL"><tld lit="V"/>o</ref>
      permesanta mallongtempan trairon de
      koncerna lando dum voja&gcirc;o al transa loko:
      <ekz>
        mi ricevis mian norvegan <tld/>on kaj svedan
        transitan <tld/>on
        <fnt>
          <aut>Eduard</aut>,
          <vrk>Kiam mi povos esti normala homo</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_z;&c_y;&c_t;&c_n;&c_a;&c_ja; <ind>&c_v;&c_ib;&c_z;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>pr&uring;choz&iacute; v&iacute;zum</trd>,
    <trd>tranzitn&iacute; v&iacute;zum</trd>
  </trdgrp>
  <trd lng="de">Durchreisevisum</trd>
  <trd lng="en">transit visa</trd>
  <trd lng="fr">visa de transit</trd>
  <trd lng="nl">transitvisum</trd>
  <trdgrp lng="pl">
    <trd>wiza przejazdowa</trd>,
    <trd>wiza tranzytowa</trd>
  </trdgrp>
  <trd lng="sk">tranzitn&eacute; v&iacute;zum</trd>
</drv>

<drv mrk="viz.turisma0o">
  <kap>turisma <tld/>o,
  <var><kap>turista <tld/>o</kap></var></kap>
  <snc mrk="viz.turisma0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="viz.0o.POL"><tld lit="V"/>o</ref>
      permesanta restadon en la koncerna lando dum turisma vizitado:
      <ekz>
        vi petas turisman <tld/>on kaj invitanta organizo
        estas <ctl><nom>Ecca-Inter</nom></ctl>
        <fnt>
          <aut>Vitalik</aut>,
          <vrk><url ref="https://groups.yahoo.com/neo/groups/pri-ijk/conversations/topics/198">
          [pri IJK] Vizo-akirado kaj...</url></vrk>,
          <lok>Pri IJK por kiu ajn!, 2004-06-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_y;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_v;&c_ib;&c_z;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Reisevisum</trd>,
    <trd>Touristenvisum</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>visitor visa</trd>,
    <trd>tourist visa</trd>
  </trdgrp>
  <trd lng="fr">visa de tourisme</trd>
  <trd lng="nl">toeristisch <ind>visum</ind></trd>
  <trd lng="pl">wiza turystyczna</trd>
</drv>
</art>
