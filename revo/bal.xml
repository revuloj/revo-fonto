<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bal</rad>/o
</kap>
<drv mrk="bal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Pompa kunveno, en kiu oni dancas:
      <ekz>
        postmorga&ubreve; la guberniestro donos grandan <tld/>on
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 12a, p.
        64a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;</trd>
  <trd lng="br">bal</trd>
  <trd lng="ca">ball</trd>
  <trdgrp lng="cs">
    <trd>bál</trd>,
    <trd>ples</trd>,
    <trd>taneční zábava</trd>
  </trdgrp>
  <trd lng="de">Ball</trd>
  <trd lng="en">ball</trd>
  <trd lng="es">baile</trd>
  <trd lng="fr">bal</trd>
  <trd lng="hu">b&aacute;l</trd>
  <trd lng="nl">bal<klr tip="ind"> (danspartij)</klr></trd>
  <trd lng="pl">bal</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>b&aacute;l</trd>,
    <trd>ples</trd>,
    <trd>tanečná zábava</trd>
  </trdgrp>
  <trd lng="sv">bal</trd>
</drv>
</art>
