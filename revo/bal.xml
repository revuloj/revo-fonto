  <trd lng="ru">&c_b;&c_a;&c_l;</trd>
  <trd lng="sk">b&aacute;l</trd>
  <trd lng="sv">bal</trd>
</drv>
<drv mrk="bal.0eto">
<kap><tld/>eto<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="balet.0o">baleto</ref>.
    </dif>
  </snc>
  <rim>
   Ne temas pri deriva&jcirc;o.
  </rim>
</drv>
</art>
