    <dif>
      Kunveno, en kiu oni dancas.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Ball</trd>
  <trd lng="en">ball</trd>
  <trd lng="fr">bal</trd>
  <trd lng="hu">b&aacute;l</trd>
