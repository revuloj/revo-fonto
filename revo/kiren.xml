<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kiren.xml,v 1.1 2020/12/27 18:59:14 afido Exp $">
    <kap>
      <rad>Kiren</rad>/o <fnt><bib>NT</bib></fnt>,
      <var><kap><rad var="C">Ciren</rad>/o</kap></var>
    </kap>

<drv mrk="kiren.0o">
  <kap><tld/>o, <var><kap><tld var="C"/>o</kap></var></kap>
  <uzo tip="fak">HIS</uzo><uzo tip="fak">ARKE</uzo>
  <snc mrk="kiren.0o.antikva">
    <dif>
      Antikva greka <ref tip="super" cel="urb.0o">urbo</ref>,
      situanta en la hodia&ubreve;a <ref tip="malprt" cel="libi.L0o">Libio</ref>
      proksime de la urbeto <nom>Shahat</nom>:
      <ekz>
        la partoj de Libio apud <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kaptis Simonon, <tld/>anon, venantan de la kamparo, kaj metis sur lin la
        krucon, por porti &gcirc;in post Jesuo
        <fnt>
          <bib>NT</bib>
          <lok>Luko 23:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        Eratosteno de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=151"
              >Arkimedo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la kribrilo de Eratosteno <klr>[&#8230;]</klr> de <tld var="C"/>o por trovi serion da primoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=187204"
              >Kribrilo de Eratosteno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kyrene</trd>
    <trd lng="en">Cyrene</trd>
    <trd lng="fr">Cyr&egrave;ne</trd>
  </snc>
</drv>

  </art>

