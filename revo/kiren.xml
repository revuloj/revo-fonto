<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kiren.xml,v 1.7 2023/12/03 09:58:49 afido Exp $">
    <kap>
      <rad>Kiren</rad>/o <fnt><bib>NT</bib></fnt>,
      <var><kap><rad var="C">Ciren</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
    </kap>

<drv mrk="kiren.0o">
  <kap><tld/>o, <var><kap><tld var="C"/>o</kap></var></kap>
  <snc mrk="kiren.0o.antikva">
    <uzo tip="fak">ARKE</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva greka <ref tip="lst" lst="voko:urboj" cel="urb.0o">urbo</ref>,
      situanta en la hodia&ubreve;a <ref tip="malprt" cel="libi.L0o">Libio</ref>
      proksime de la urbeto <nom>Shahat</nom>:
      <ekz>
        la partoj de Libio apud <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>&Ago; 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kaptis Simonon, <tld/>anon, venantan de la kamparo, kaj metis sur lin la
        krucon, por porti &gcirc;in post Jesuo
        <fnt>
          <bib>NT</bib>
          <lok>&Luk; 23:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        Eratosteno de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=151">Arkimedo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la kribrilo de Eratosteno <klr>[&#x2026;]</klr> de <tld var="C"/>o por trovi serion da primoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=187204">Kribrilo de Eratosteno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_ib;&c_r;&c_e;&c_n;&c_a;</trd>
  <trd lng="de">Kyrene</trd>
  <trd lng="en">Cyrene</trd>
  <trd lng="fr">Cyr&egrave;ne</trd>
  <trd lng="ja">&#x30AD;&#x30EC;&#x30FC;&#x30CD;</trd>
  <trd lng="uk">&#x41A;&c_ib;&c_r;&c_je;&#x43D;&c_a;</trd>
  <trd lng="zh">&#x6614;&#x5170;&#x5C3C; <pr>x&#x12B;l&#xE1;nn&#xED;</pr></trd>
</drv>
</art>

