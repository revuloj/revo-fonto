<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>&gcirc;ig</rad>/o <fnt><bib>MuzTerm</bib></fnt></kap>
<drv mrk="gxig.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="gxig.0o.danco">
    <dif>
      Vigla, angla-devena <ref tip="lst" cel="danc.0o"
      lst="voko:dancoj">danco</ref>
      tri-takta, kun multaj saltaj pa&scirc;oj, &scirc;atata en klasika
      E&ubreve;ropo kaj plu populara kiel popola danco.
    </dif>
    <ref tip="sin" cel="jxig.0o">&jcirc;igo</ref>
  </snc>
  <trd lng="pt">giga <klr tip="amb">(dan&ccedil;a)</klr></trd>
  <snc mrk="gxig.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco
      ofte uzata kiel parto de <ref tip="malprt"
      cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_g;&c_a;</trd>
  <trd lng="de">Gigue</trd>
  <trd lng="en">jig</trd>
  <trd lng="es">giga</trd>
  <trd lng="fa">&f_zh;&f_y;&f_g;</trd>
  <trdgrp lng="fr">
    <trd>gigue</trd>
  </trdgrp>
  <trd lng="hu">gigue</trd>
  <trd lng="it">giga</trd>
  <trd lng="nl">gigue</trd>
  <trd lng="pt">giga <klr tip="amb">(composi&ccedil;&atilde;o musical)</klr></trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_g;&c_a;</trd>
</drv>
</art>
