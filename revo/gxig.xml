<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&gcirc;ig</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="gxig.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="gxig.0o.danco">
    <dif>
      Vigla, angla-devena <ref tip="super" cel="danc.0o">danco</ref>
      tri-takta, kun multaj saltaj pa&scirc;oj, &scirc;atata en klasika
E&ubreve;ropo
      kaj plu populara kiel popola danco.
    </dif>
  </snc>
  <snc mrk="gxig.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco
      ofte uzata kiel parto de <ref tip="malprt" cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gigue</trd>
  </trdgrp>
</drv>
</art>

