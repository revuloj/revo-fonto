<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ofend.xml,v 1.29 2021/05/29 14:32:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ofend</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ofend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofend.0i.iun">
    <dif>
      Kolerigi a&ubreve; &ccirc;agreni iun malrespektante ties dignon:
      <ekz>
        vi <tld/>is lin per via rido;
      </ekz>
      <ekz>
        pardonu, se mi vin nevole <tld/>is
        <fnt>
          <aut>V. Devjatnin</aut>,
          <vrk>Arturo</vrk>,
          <lok>en: Fundamenta Krestomatio de la lingvo Esperanto,
          1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas <tld/>ata per plej malgranda nepruda&jcirc;o;
      </ekz>
      <ekz>
        <tld/>i noblan senton;
      </ekz>
      <ekz>
        muso la katon &ccirc;iam <tld/>is
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indign.0igi">indignigi</ref>,
      <ref cel="skanda.0i">skandali</ref>,
      <ref cel="sxok.0i">&scirc;oki</ref>,
      <ref cel="vund.0i">vundi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beleidigen</trd>,
      <trd>verletzen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megb&aacute;nt</trd>,
      <trd>megs&eacute;rt</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obra&zdot;a&cacute;</trd>,
      <trd>ura&zdot;a&cacute;</trd>,
      <trd>ubli&zdot;a&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ofend.0i.ion">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      Malobei sciencan regulon, paroli malkonforme al komuna scio:
      <ekz>
        <tld/>i fonetikan le&gcirc;on;
      </ekz>
      <ekz>
        tio <tld/>as nian lingvan senton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi ankora&ubreve; <tld/>is la geografion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontraux.0i">kontra&ubreve;i</ref>,
      <ref cel="obe">malobei</ref>,
      <ref cel="romp.0i">rompi</ref>,
      <ref cel="pasx.super0itrans0i">transpa&scirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>verletzen</trd>
    </trdgrp>
    <trd lng="hu">s&eacute;rt</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_w;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF53;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>potupit</trd>,
    <trd>ubl&iacute;&zcaron;it na cti</trd>,
    <trd>urazit</trd>
  </trdgrp>
  <trd lng="el">
  &pi;&rho;&omikron;&sigma;&beta;&alfa_ton;&lambda;&lambda;&omega;</trd>
  <trd lng="en">offend</trd>
  <trd lng="es">ofender</trd>
  <trdgrp lng="fr">
    <trd>f&acirc;cher</trd>,
    <trd>offenser</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beledigen</trd>,
    <trd>krenken</trd>
  </trdgrp>
  <trd lng="pt">ofender</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_r;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ubl&iacute;&zcaron;i&tcaron; na cti</trd>,
    <trd>urazi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="ofend.0a">
  <kap><tld/>a</kap>
  <snc mrk="ofend.0a.PSI">
    <dif>
      &Ccirc;agrena, <ref tip="super" cel="respek.mal0a.PSI">malrespekta</ref>
      al ies digno:
      <ekz>
        la kuracisto <klr>[...]</klr> ekatakis per la plej <tld/>aj
        vortoj
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>,
          <lok>La fatala sa&gcirc;eco</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>potupn&yacute;</trd>,
    <trd>ur&aacute;&zcaron;liv&yacute;</trd>
  </trdgrp>
  <trd lng="es">ofensivo</trd>
  <trdgrp lng="fr">
    <trd>blessant<klr tip="amb"> (moralement)</klr></trd>,
    <trd>offensant</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beledigend</trd>,
    <trd>krenkend</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>obra&zacute;liwy</trd>,
      <trd>obra&zdot;aj&aogonek;cy</trd>,
      <trd>ura&zdot;aj&aogonek;cy</trd>,
      <trd>ubli&zdot;aj&aogonek;cy</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_r;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ur&aacute;&zcaron;liv&yacute;</trd>
</drv>

<drv mrk="ofend.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio per kio oni <tld/>as:
      <ekz>
        pardoni, ka&scirc;i, ven&gcirc;i <tld/>on;
      </ekz>
      <ekz>
        <tld/>aj paroloj, gestoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bat.0o">bato</ref>,
      <ref cel="vund.0o">vundo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_w;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xFA8;&#xF51;&#xF0B;&#xF62;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>ubl&iacute;&zcaron;en&iacute; na cti</trd>,
    <trd>ur&aacute;&zcaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Beleidigung</trd>,
    <trd>Kr&auml;nkung</trd>
  </trdgrp>
  <trd lng="el">
  &pi;&rho;&omikron;&sigma;&beta;&omikron;&lambda;&eta_ton;</trd>
  <trd lng="es">ofensa</trd>
  <trdgrp lng="fr">
    <trd>offense<klr tip="amb"> (acte offensant)</klr></trd>,
    <trd>vexation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&eacute;rt&eacute;s</trd>,
    <trd>b&aacute;nt&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">belediging</trd>
  <trdgrp lng="pl">
      <trd>obraza</trd>,
      <trd>uraza</trd>
    </trdgrp>
  <trd lng="pt">ofensa</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_r;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">ur&aacute;&zcaron;ka</trd>
</drv>

<drv mrk="ofend.0eti">
  <kap><tld/>eti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofend.0eti.PSI">
    <dif>
      &Ccirc;agreni iun, malrespektante ties guston a&ubreve; pla&ccirc;on;
      <ref tip="vid" cel="incit.0i.kolerigi">inciti<sncref/></ref>,
      <ref tip="vid" cel="pik.0i.inciti">piki<sncref/></ref>:
      <ekz>
        tiu vorto <tld/>etas miajn orelojn;
      </ekz>
      <ekz>
        <tld/>eta koloro, vo&ccirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>kr&auml;nken</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>froisser<klr tip="amb"> (quelqu'un)</klr></trd>,
    <trd>vexer</trd>
  </trdgrp>
  <trd lng="hu">b&aacute;nt</trd>
  <trd lng="nl">krenken</trd>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_je;&c_t;&c_mol; <klr tip="amb">
  (&c_o;&c_b;&c_i;&c_d;&c_je;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="ofend.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="ofend.0igxema.PSI">
    <dif>
      Facile <tld/>i&gcirc;anta, koleri&gcirc;anta pro <tld/>oj;
      <ref tip="vid" cel="pik.0igxema.PSI">piki&gcirc;ema</ref>:
      <ekz>
        juneco estas arda, arda kaj <tld/>i&gcirc;ema
        <fnt>
          <aut>J. German, trad. J. Finkel</aut>,
          <vrk><url ref="https://esperanto-mv.pp.ru/German/afero-08-3.html">
          La afero, al kiu vi servas</url></vrk>,
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">susceptible</trd>
</drv>

<drv mrk="ofend.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ofend.0igxi.PSI">
    <dif>
      Malkontenti, <ref tip="super" cel="koler.0i">koleri</ref> pro ies
      malrespekta ago:
      <ekz>
        mi esperas, ke vi ne <tld/>i&gcirc;os, se mi tion faras;
      </ekz>
      <ekz>
        mi <tld/>i&gcirc;is de tio, kion vi diris al mi kaj mi respondis per
        mal&gcirc;entila&jcirc;o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">ofenderse</trd>
  <trdgrp lng="fr">
    <trd>se <ind>f&acirc;cher</ind></trd>,
    <trd>s'<ind>offenser</ind></trd>
  </trdgrp>
  <trd lng="hu">megs&eacute;rt&odblac;dik</trd>
  <trd lng="nl">beledigd worden</trd>  
  <trd lng="pl">obrazi&cacute; si&eogonek;</trd>  
  <trd lng="ru">&c_o;&c_b;&c_i;&c_d;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ofend.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="ofend.0igxo.PSI">
    <dif>
      <ref tip="super" cel="koler.0o">Kolero</ref> pro ies maldeca,
      malrespekta ago:
      <ekz>
        vo&ccirc;o, en kiu oni sentis <tld/>i&gcirc;on
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">offense<klr tip="amb"> (ressentie)</klr></trd>
  <trd lng="de">Beleidigung</trd>
  <trd lng="es">ofensa</trd>
  <trd lng="hu">s&eacute;rt&odblac;d&eacute;s</trd>
  <trd lng="nl">belediging</trd>
  <trdgrp lng="pl">
      <trd>obraza</trd>,
      <trd>uraza</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>ofensa</trd>,
   <trd>aborrecimento</trd>,
   <trd>desgosto</trd>,
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_i;&c_d;&c_a; <klr tip="amb"> 
  (&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>

<drv mrk="ofend.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="ofend.sen0a.KOMUNE">
    <dif>
      Kiu ne celas, ne ka&ubreve;zas <tld/>on:
      <ekz>
        la blondulino anka&ubreve; &gcirc;uis la situacion; &scirc;iaj vangosulkoj 
        pliprofundi&gcirc;is, kaj &scirc;i rigardadis Donaldon senembarase kaj 
        sen<tld/>e
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>innocent<klr> (inoffensif)</klr></trd>,
    <trd>inoffensif</trd>
  </trdgrp>
</drv>
</art>
