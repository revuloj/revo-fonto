<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ofend</rad>/i
</kap>
<drv mrk="ofend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofend.0i.iun">
    <dif>
      Kolerigi a&ubreve; &ccirc;agreni iun malrespektante ties dignon:
      <ekz>
        vi <tld/>is lin per via rido;
      </ekz>
      <ekz>
        pardonu, se mi vin nevole <tld/>is
        <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
        Devjatnin), 2</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i estas <tld/>ata per plej malgranda nepruda&jcirc;o;
      </ekz>
      <ekz>
        <tld/>i noblan senton;
      </ekz>
      <ekz>
        muso la katon &ccirc;iam <tld/>is
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indign.0igi">indignigi</ref>,
      <ref cel="skanda.0i">skandali</ref>,
      <ref cel="sxok.0i">&scirc;oki</ref>,
      <ref cel="vund.0i">vundi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beleidigen</trd>,
      <trd>verletzen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megb&aacute;nt</trd>,
      <trd>megs&eacute;rt</trd>
    </trdgrp>
  </snc>
  <snc mrk="ofend.0i.ion">
    <dif>
      Malrespekti, malobservi sciencan regulon a&ubreve; veron:
      <ekz>
        <tld/>i fonetikan le&gcirc;on;
      </ekz>
      <ekz>
        tio <tld/>as nian lingvan senton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi ankora&ubreve; <tld/>is la geografion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontraux.0i">kontra&ubreve;i</ref>,
      <ref cel="obe">malobei</ref>,
      <ref cel="romp.0i">rompi</ref>,
      <ref cel="pasx.super0itrans0i">transpa&scirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>verletzen</trd>
    </trdgrp>
    <trd lng="hu">s&eacute;rt</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_w;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">offend</trd>
  <trd lng="es">ofender</trd>
  <trdgrp lng="fr">
    <trd>f&acirc;cher</trd>,
    <trd>offenser</trd>
  </trdgrp>
  <trd lng="el">&pi;&rho;&omikron;&sigma;&beta;&alfa_ton;&lambda;&lambda;&omega;</trd>
  <trdgrp lng="nl">
    <trd>beledigen</trd>,
    <trd>krenken</trd>
  </trdgrp>
  <trd lng="pt">ofender</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_r;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="ofend.0a">
  <kap><tld/>a</kap>
  <snc mrk="ofend.0a.PSI">
    <dif>
      &Ccirc;agrena, <ref tip="super" cel="respek.mal0a.PSI">malrespekta</ref>
      al ies digno:
      <ekz>
        la kuracisto <klr>[...]</klr> ekatakis per la plej <tld/>aj vortoj
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova &scirc;akproblemo, (tria skizo
        el la &ccirc;uk&ccirc;a vivo), 4, La fatala sa&gcirc;eco</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">ofensivo</trd>
  <trdgrp lng="fr">
    <trd>blessant<klr tip="amb"> (moralement)</klr></trd>,
    <trd>offensant</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_r;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ofend.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio per kio oni <tld/>as:
      <ekz>
        pardoni, ka&scirc;i, ven&gcirc;i <tld/>on;
      </ekz>
      <ekz>
        <tld/>aj paroloj, gestoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bat.0o">bato</ref>,
      <ref cel="vund.0o">vundo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_w;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Beleidigung</trd>,
    <trd>Kr&auml;nkung</trd>
  </trdgrp>
  <trd lng="el">&pi;&rho;&omikron;&sigma;&beta;&omikron;&lambda;&eta_ton;</trd>
  <trd lng="es">ofensa</trd>
  <trdgrp lng="fr">
    <trd>offense<klr tip="amb"> (acte offensant)</klr></trd>,
    <trd>vexation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&eacute;rt&eacute;s</trd>,
    <trd>b&aacute;nt&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">belediging</trd>
  <trd lng="pt">ofensa</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_r;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="ofend.0eti">
  <kap><tld/>eti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofend.0eti.PSI">
    <dif>
      &Ccirc;agreni iun, malrespektante ties guston a&ubreve; pla&ccirc;on:
      <ekz>
        tiu vorto <tld/>etas miajn orelojn;
      </ekz>
      <ekz>
        <tld/>eta koloro, vo&ccirc;o
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="incit.0i.kolerigi"> inciti <sncref/></ref>,
      <ref cel="pik.0i.inciti">piki <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>kr&auml;nken</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>froisser<klr tip="amb"> (quelqu'un)</klr></trd>,
    <trd>vexer</trd>
  </trdgrp>
  <trd lng="hu">b&aacute;nt</trd>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_je;&c_t;&c_mol; <klr tip="amb">(&c_o;&c_b;&c_i;&c_d;&c_je;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="ofend.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ofend.0igxi.PSI">
    <dif>
      Malkontenti, <ref tip="super" cel="koler.0i">koleri</ref> pro ies
      malrespekta ago:
      <ekz>
        mi esperas, ke vi ne <tld/>i&gcirc;os, se mi tion faras;
      </ekz>
      <ekz>
        mi <tld/>i&gcirc;is de tio, kion vi diris al mi kaj mi respondis per
        mal&gcirc;entila&jcirc;o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">ofenderse</trd>
  <trdgrp lng="fr">
    <trd>se <ind>f&acirc;cher</ind></trd>,
    <trd>s'<ind>offenser</ind></trd>
  </trdgrp>
  <trd lng="hu">megs&eacute;rt&odblac;dik</trd>  
  <trd lng="ru">&c_o;&c_b;&c_i;&c_d;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ofend.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="ofend.0igxo.PSI">
    <dif>
      <ref tip="super" cel="koler.0o">Kolero</ref> pro ies maldeca,
      malrespekta ago:
      <ekz>
        vo&ccirc;o, en kiu oni sentis <tld/>i&gcirc;on
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">offense<klr tip="amb"> (ressentie)</klr></trd>
  <trd lng="de">Beleidigung</trd>
  <trd lng="es">ofensa</trd>
  <trd lng="hu">s&eacute;rt&odblac;d&eacute;s</trd>  
  <trd lng="ru">&c_o;&c_b;&c_i;&c_d;&c_a; <klr tip="amb"> (&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
