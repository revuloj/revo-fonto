<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>&gcirc;eneral</rad>/a
</kap>
<drv mrk="gxener.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta senescepte al tuto a&ubreve; al la plej granda parto el personoj
      a&ubreve; objektoj; neaparta, neindividua:
      <ekz>
        <tld/>a kunveno, regulo,
        opinio;
      </ekz>
      <ekz>
        <tld/>aj ideoj;
      </ekz>
      <ekz>
        <tld/>a malarmado;
      </ekz>
      <ekz>
        mi sendas al vi mian <tld/>an prezaron;
      </ekz>
      <ekz>
        <tld/>a pardono.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="specia.0a">speciala</ref>
    </refgrp>
  </snc>
  <trd lng="cs">v&scaron;eobecn&yacute;</trd>
  <trdgrp lng="de">
    <trd>allgemein</trd>,
    <trd>generell</trd>
  </trdgrp>
  <trd lng="el">&gamma;&epsilon;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">general</trd>
  <trd lng="es">general</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;n&eacute;ral</trd>,
    <trd>commun</trd>,
    <trd>global</trd>,
    <trd>ordinaire</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ltal&aacute;nos</trd>
  <trdgrp lng="it">
    <trd>generale</trd>
    <trd>generico</trd>
  </trdgrp>
  <trd lng="nl">algemeen</trd>
  <trdgrp lng="pt">
    <trd>geral</trd>,
    <trd>gen&eacute;rico</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>generell</trd>,
    <trd>allm&auml;n</trd>
  </trdgrp>
</drv>

<drv mrk="gxener.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io havu <tld/>an amplekson:
      <ekz>
        <tld/>igi regulon, doktrinon,
        le&gcirc;on, kutimon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>verallgemeinern</trd>,
    <trd>generalisieren</trd>
  </trdgrp>
  <trd lng="el">&gamma;&epsilon;&ny;&jota;&kappa;&epsilon;&ypsilon_ton;&omega;</trd>
  <trd lng="en">generalize</trd>
  <trd lng="es">generalizar</trd>
  <trd lng="fr">g&eacute;n&eacute;raliser</trd>
  <trd lng="hu">&aacute;ltal&aacute;nos&iacute;t</trd>
  <trd lng="it">generalizzare</trd>
  <trd lng="nl">veralgemenen</trd>
  <trd lng="pt">generalizar</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_b;&c_shch;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
