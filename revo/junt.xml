<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>junt</rad>/o
    </kap>
    <drv mrk="junt.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">TEK</uzo>
      <snc mrk="junt.0o..ilo">
        <dif>
          Me&hcirc;anika a&ubreve; elektra
          <ref tip="super" cel="fac.inter0o">interfaco</ref>,
          kunigilo de cirkvitoj, kabloj, tuboj a&ubreve; interspaco inter du
          kunigitaj &scirc;tonaj, brikaj, metalaj a&ubreve; lignaj pecoj, kiun oni
          plenigas per gluo, mastiko, mortero.
        </dif>
        <refgrp tip="sub">
          <ref cel="cxarni.0o">&ccirc;arniro</ref>,
          <ref cel="born.0o">borno</ref>,
          <ref cel="bolt.0o">bolto</ref>,
          <ref cel="klem.0o">klemo</ref>,
          <ref cel="sxraux.0o">&scirc;ra&ubreve;bo</ref>.
        </refgrp>
      </snc>
      <snc mrk="junt.0o.kunigo">
        <dif>
          La kunigo mem.
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="fold.0o">foldo</ref>.
      </refgrp>
      <trdgrp lng="de">
        <trd>Fuge</trd>,
        <trd>Junktur</trd>,
        <trd>Naht</trd>,
        <trd>Sto&szlig;</trd></trdgrp>
      <trd lng="es">junta</trd>
      <trdgrp lng="nl">
       <trd>voeg</trd>,
       <trd>naad</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>csatlakoz&aacute;s</trd>,
        <trd>csatlakoz&oacute;</trd>,
        <trd>illeszked&eacute;s</trd>,
        <trd>k&ouml;t&eacute;s</trd>,
        <trd>ereszt&eacute;k</trd>
      </trdgrp>
      <trd lng="pt">junta</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_malmol;&#x451;&c_m;</trd>,
        <trd>&c_s;&c_t;&c_y;&c_k;</trd>
      </trdgrp>
    </drv>
  </art>
