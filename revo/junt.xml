<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: junt.xml,v 1.30 2025/03/30 18:49:52 revo Exp $">
<kap>
  <rad>junt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="junt.0o">
  <kap><tld/>o</kap>
  <snc mrk="junt.0o.ilo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Me&hcirc;anika a&ubreve; elektra
      <ref tip="super" cel="fac.inter0o">interfaco</ref>,
      kunigilo de cirkvitoj, kabloj, tuboj a&ubreve; interspaco inter du
      kunigitaj &scirc;tonaj, brikaj, metalaj a&ubreve; lignaj pecoj,
      kiun oni plenigas per gluo, mastiko, mortero:
      <ekz>
        li rigardis la oblikvajn tabulojn de la tegmento, kiuj
        iom sinkis inter la &ccirc;evronoj kaj kaj jam ekputris &ccirc;e
        la <tld/>oj
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bolt.0o">bolto</ref>,
      <ref cel="cxarni.0o">&ccirc;arniro</ref>,
      <ref cel="lut.0o">luto</ref>,
      <ref cel="nit.0junto">nitjunto</ref>,
      <ref cel="sxraux.0o">&scirc;ra&ubreve;bo</ref>,
      <ref cel="veld.0o">veldo</ref>
    </refgrp>
  </snc>
  <snc mrk="junt.0o.kunigo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      La kunigo mem:
      <ekz>
        <uzo tip="fak">KON</uzo>
        te&hcirc;nikoj <klr>[de]</klr> <tld/>ado: per <ref cel="fold.0o">foldo</ref>
        kaj <ref cel="lang.0o.FIG">lango</ref>,
        per <ref cel="tenon.0o">tenono</ref>
        kaj <ref cel="mortez.0o">mortezo</ref>,
        per plurdenta <tld/>o, per hirundvosta <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32560">Ligna&jcirc;isto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ELE</uzo>
        kunigo de diversspecaj duonkonduktantoj: pn-<tld/>o (p = pozitiva regiono, n =
        negativa regiono) formas diodon, duo da <tld/>oj npn a&ubreve; pnp faras transistoron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=63478">Junto (elektro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kunig(a&#309;)o</ke>, <ke>interfiks(a&#309;)o</ke>.
  </rim>

  <refgrp tip="vid">
    <ref cel="fold.0o">foldo</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_y;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>junta</trd>,
    <trd>juntura</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fuge</trd>,
    <trd>Junktur</trd>,
    <trd>Naht</trd>,
    <trd>Sto&szlig;</trd>,
    <trd>Verbindung</trd>,
    <trd>Schnittstelle</trd>
  </trdgrp>
  <trd lng="es">junta</trd>
  <trdgrp lng="fr">
    <trd>jonction<klr tip="ind"> (interface)</klr></trd>,
    <trd>joint<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatlakoz&aacute;s</trd>,
    <trd>csatlakoz&oacute;</trd>,
    <trd>illeszked&eacute;s</trd>,
    <trd>k&ouml;t&eacute;s</trd>,
    <trd>ereszt&eacute;k</trd>
  </trdgrp>
  <trd lng="it">giunto</trd>
  <trdgrp lng="ja">
    <trd>&#32153;&#12366;&#30446;  <pr>&#12388;&#12366;&#12417;</pr></trd>,
    <trd>&#25509;&#21512;&#31623;&#25152;  <pr>&#12379;&#12388;&#12372;&#12358;&#12363;&#12375;&#12423;</pr></trd>,
    <trd>&#32153;&#25163;  <pr>&#12388;&#12366;&#12390;</pr></trd>,
    <trd>&#20181;&#21475;  <pr>&#12375;&#12367;&#12385;</pr></trd>,
    <trd>&#25509;&#32154;  <pr>&#12379;&#12388;&#12382;&#12367;</pr></trd>,
    <trd>&#12472;&#12519;&#12452;&#12531;&#12488;  <pr>&#12376;&#12423;&#12356;&#12435;&#12392;</pr></trd>,
    <trd>&#31680;&#29702;  <pr>&#12379;&#12388;&#12426;</pr></trd>,
    <trd>&#36899;&#25509;  <pr>&#12428;&#12435;&#12379;&#12388;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voeg</trd>,
    <trd>naad</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z&lstroke;&aogonek;cze</trd>,
    <trd>po&lstroke;&aogonek;czenie</trd>,
    <trd>spojenie</trd>
  </trdgrp>
  <trd lng="pt">junta</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_jo;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fuga</trd>,
    <trd>spoj</trd>,
    <trd>&scaron;k&aacute;ra</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>spoj</trd>,
    <trd>sp&aacute;ra</trd>,
    <trd>&scaron;ev</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_a;&c_r;&#1085;&c_ib;&c_r;</trd>,
    <trd>&c_sh;&c_a;&c_r;&#1085;&c_ib;&c_r;&#1085;&c_je; &c_z;&#8217;&c_jeu;&c_d;&#1085;&c_a;&#1085;&#1085;&#1103;</trd>,
    <trd>&c_s;&c_t;&c_i;&#1082;</trd>,
    <trd>&#1087;&c_a;&c_z;</trd>,
    <trd>(&c_z;&#8217;&c_jeu;&c_d;&#1085;&c_u;&c_v;&c_a;&#1083;&#1100;&#1085;&c_i;&c_j;) &c_sh;&#1086;&c_v;</trd>,
    <trd>&c_z;&#8217;&c_jeu;&c_d;&#1085;&c_a;&#1085;&#1085;&#1103;</trd>,
    <trd>&c_s;&#1087;&#1086;&#1102;&c_v;&c_a;&#1085;&#1085;&#1103;</trd>,
    <trd>&c_z;&c_r;&#1086;&c_shch;&c_u;&c_v;&c_a;&#1085;&#1085;&#1103; (&c_s;&c_t;&#1086;&#1083;&#1103;&c_r;&#1085;&c_i;&c_h; &c_v;&c_i;&c_r;&#1086;&c_b;&c_ib;&c_v;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25340;&#25509; <pr>p&#299;nji&#275;</pr></trd>,
    <trd>&#20132;&#30028;&#22788; <pr>ji&#257;oji&#232;ch&#249;</pr></trd>
  </trdgrp>
</drv>
</art>
