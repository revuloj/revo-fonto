<art mrk="$Id: tild.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tild</rad>/o
</kap>
<drv mrk="tild.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Signotipo ~ simila al ku&scirc;anta litero S:
      <ekz>
        n kun <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_l;&c_mol;&c_d;&c_a;</trd>
  <trd lng="bg">&c_t;&c_i;&c_l;&c_d;&c_a;</trd>
  <trd lng="ca">titlla</trd>
  <trdgrp lng="cs">
    <trd>ležatá vlnovka</trd>,
    <trd>tilda</trd>
  </trdgrp>
  <trd lng="de">Tilde</trd>
  <trd lng="en">tilde</trd>
  <trd lng="fr">tilde</trd>
  <trdgrp lng="hu">
    <trd>tilde</trd>,
    <trd>hull&aacute;mjel</trd>,
    <trd>ism&eacute;tl&odblac;jel</trd>
  </trdgrp>
  <trd lng="nl">tilde</trd>
  <trd lng="pl">tylda</trd>
  <trdgrp lng="pt">
    <trd>til</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_l;&c_mol;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>tilda</trd>,
    <trd>vežatá vlnovka</trd>
  </trdgrp>
</drv>
</art>
