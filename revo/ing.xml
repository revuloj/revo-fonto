<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>ing</rad>
</kap>
<subart>
  <dif>
    Sufikso signanta objekton, en kiu unu alia objekto a&ubreve; membro
    estas parte enigita kaj fiksita, &gcirc;enerale per sia
    ekstrema&jcirc;o:
  </dif>
  <snc>
    <ekz>
      bastoneto en kiu oni tenas plumon por skribado
      estas <ind>plum<tld/>o</ind>
      <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 40</lok></fnt>;
      <trd lng="de">Federhalter</trd>
    </ekz>
    <ekz>
      tubeto en kiun oni metas cigaron kiam oni &gcirc;in fumas 
      estas <ind>cigar<tld/>o</ind>
      <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 40</lok></fnt>;
      <trd lng="ru">&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;</trd>
    </ekz>
    <ekz>
      <ind>kandel<tld/>o</ind><fnt>Z</fnt>;
      <trd lng="de">Kerzenst&auml;nder</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_d;&c_s;&c_v;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>,
        <trd>&c_k;&c_a;&c_n;&c_d;&c_je;&c_l;&c_ja;&c_b;&c_r;</trd>
      </trdgrp>
    </ekz>
    <ekz>
      <ind>fingr<tld/>o</ind> <klr>(por gardi la fingron &ccirc;e kudrado)</klr><fnt>Z</fnt>;
      <trd lng="de">Fingerhut</trd>
      <trd lng="ru">&c_n;&c_a;&c_p;&c_jo;&c_r;&c_s;&c_t;&c_o;&c_k;</trd>
    </ekz>
    <ekz>
      <ind>pied<tld/>o</ind><fnt>Z</fnt>,
      <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_m;&c_ja;</trd>
    </ekz>
    <ekz>
      <ind>flam<tld/>o</ind><fnt>Z</fnt> ktp.
      <trd lng="de">Brenner</trd>
      <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
    </ekz>
    <ref tip="vid" cel="uj">uj</ref>
  </snc>
</subart>
<subart>
  <dif>
    Samsignifa memstara vortero:
  </dif>
  <drv mrk="ing.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Objekto, kiu servas por surkovri, &scirc;irmi alian objekton parte
        enigitan en &gcirc;i:
        <ekz>
          <ind><tld/>o de glavo</ind>.
          <trd lng="ru">&c_n;&c_o;&c_zh;&c_n;&c_y; &c_m;&c_je;&c_ch;&c_a;</trd>
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>Halter</trd>,
      <trd>St&auml;nder</trd>,
      <trd>Scheide <klr>(glavo)</klr> <klr tip="ind">(Schwert)</klr></trd>
    </trdgrp>
  </drv>
  <drv mrk="ing.en0igi">
    <kap>en<tld/>igi</kap>
    <snc>
      <dif>
        Meti ion en <klr>(&gcirc;ian)</klr> <tld/>on.
      </dif>
    </snc>
    <trdgrp lng="ru">
      <trd>&c_v;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
  <drv mrk="ing.el0igi">
    <kap>el<tld/>igi</kap>
    <snc>
      <dif>
        Eligi ion el (&gcirc;ia) <tld/>o:
        <ekz>
          <ind>el<tld/>igi glavon</ind>.
          <trd lng="de">ein <ind>Schwert</ind> ziehen</trd>
          <trd lng="ru">&c_v;&c_y;&c_n;&c_u;&c_t;&c_mol; &c_m;&c_je;&c_ch; &c_i;&c_z; &c_n;&c_o;&c_zh;&c_je;&c_n;</trd>
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_t;&c_a;&c_shch;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
</subart>
</art>

