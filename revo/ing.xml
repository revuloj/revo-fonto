<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
-<rad>ing</rad></kap>
<subart>
  <dif>
    Sufikso signanta objekton, en kiu unu alia objekto a&ubreve; membro
    estas parte enigita kaj fiksita, &gcirc;enerale per sia
    ekstrema&jcirc;o:
  </dif>
  <snc>
    <ekz>
      bastoneto en kiu oni tenas plumon por skribado
      estas <ind>plum<tld/>o</ind>
      <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 40</lok></fnt>;
      <trd lng="de">Federhalter</trd>
    </ekz>
    <ekz>
      tubeto en kiun oni metas cigaron kiam oni &gcirc;in fumas 
      estas cigar<tld/>o
      <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 40</lok></fnt>;
    </ekz>
    <ekz>
      <ind>kandel<tld/>o</ind><fnt>Z</fnt>;
      <trd lng="de">Kerzenst&auml;nder</trd>
    </ekz>
    <ekz>
      <ind>fingr<tld/>o</ind> <klr>(por gardi la fingron &ccirc;e kudrado)</klr><fnt>Z</fnt>;
      <trd lng="de">Fingerhut</trd>
    </ekz>
    <ekz>
      pied<tld/>o<fnt>Z</fnt>,
    </ekz>
    <ekz>
      <ind>flam<tld/>o</ind><fnt>Z</fnt> ktp.
      <trd lng="de">Brenner</trd>
    </ekz>
    <ref tip="vid" cel="uj">uj</ref>
  </snc>
</subart>
<subart>
  <dif>
    Samsignifa memstara vortero:
  </dif>
  <drv mrk="ing.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Objekto, kiu servas por surkovri, &scirc;irmi alian objekton parte
        enigitan en &gcirc;i:
        <ekz>
          <tld/>o de glavo.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>Halter</trd>,
      <trd>St&auml;nder</trd>,
      <trd>Scheide <klr>(glavo)</klr> <klr tip="ind">(Schwert)</klr></trd>
    </trdgrp>
  </drv>
  <drv mrk="ing.en0igi">
    <kap>eningigi</kap>
    <snc>
      <dif>
        Meti ion en <klr>(&gcirc;ian)</klr> <tld/>on.
      </dif>
    </snc>
  </drv>
  <drv mrk="ing.el0igi">
    <kap>elingigi</kap>
    <snc>
      <dif>
        Eligi ion el (&gcirc;ia) <tld/>o:
        <ekz>
          <ind>el<tld/>igi glavon</ind>.
          <trd lng="de">ein <ind>Schwert</ind> ziehen</trd>
        </ekz>
      </dif>
    </snc>
  </drv>
</subart>
</art>

<!--
$Log$
-->
</vortaro>
