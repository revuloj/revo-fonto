<kap>
  <ofc>*</ofc>
  <rad>influ</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="influ.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="influ.0i.KOMUNE">
    <dif>
      Morale a&ubreve; materie
      <ref tip="vid" cel="efik.0i.KOMUNE">efiki</ref>
      al iu a&ubreve; io, iagrade
      <ref tip="vid" cel="modif.0i">modifi</ref>
      &gcirc;ian funkciadon:
      <ekz>
        la varioj de vetero <tld/>as al la nervoj;
      </ekz>
      <ekz>
        la malsano <tld/>is lian humoron;
      </ekz>
      <ekz>
        mi volas neniel <tld/>i vin;
      </ekz>
      <ekz>
        bonaj ekzemploj pli forte <tld/>as ol admonoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu argumento tre forte <tld/>os sur mia(n) decido(n);
      </ekz>
      <ekz>
        <tld/>i la sorton de la lando<fnt>B</fnt>;
      </ekz>
      <ekz>
        li estas &ccirc;iam <tld/>ata de sia plia&gcirc;a frato;
      </ekz>
      <ekz>
        tiuj gramatikaj formoj <tld/>as unu la alian;
      </ekz>
      <ekz>
        la homa volo sen&ccirc;ese <tld/>as sur la lingvaj(n) aferoj(n);
      </ekz>
      <ekz>
        oni kredis iam, ke la astroj <tld/>as super la sorto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_p;&c_l;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>influir</trd>,
    <trd>influenciar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t vliv</trd>,
    <trd>ovlivnit</trd>,
    <trd>ovliv&ncaron;ovat</trd>,
    <trd>p&uring;sobit na n&ecaron;co</trd>,
    <trd>&uacute;&ccaron;inkovat na n&ecaron;co</trd>
  </trdgrp>
  <trd lng="de">beeinflussen</trd>
  <trdgrp lng="en"><trd>affect</trd>, <trd>influence</trd></trdgrp>
  <trd lng="es">influir</trd>
  <trd lng="fr">influencer</trd>
  <trdgrp lng="hu">
    <trd>befoly&aacute;sol</trd>,
    <trd>hat r&aacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F71;&#x97FF;&#12434;&#x4E0E;&#12360;&#x308B;  <pr>&#12360;&#12356;&#x304D;&#12423;&#12358;&#12434;&#12354;&#x305F;&#12360;&#x308B;</pr></trd>,
    <trd>&#x611F;&#21270;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#x304B;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5DE6;&#x53F3;&#12377;&#x308B;  <pr>&#12373;&#12422;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">be&iuml;nvloeden</trd>
  <trd lng="pt">influir</trd>
  <trd lng="ru">&c_v;&c_l;&c_i;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; vplyv</trd>,
    <trd>&uacute;&ccaron;inkova&tcaron; na nie&ccaron;o</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43F;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i; (&#x43D;&c_a; &c_shch;&#x43E;-&#x43D;.</trd>,
    <trd>&#x43A;&#x43E;&c_g;&#x43E;-&#x43D;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F71;&#x54CD; <pr>y&#x1D0;ngxi&#x1CE;ng</pr></trd>,
    <trd>&#x611F;&#x67D3; <pr>g&#x1CE;nr&#x1CE;n</pr></trd>,
    <trd>&#x718F;&#38518; <pr>x&#x16B;nt&#xE1;o</pr></trd>,
    <trd>&#29301;&#x52A8; <pr>qi&amacron;nd&#xF2;ng</pr></trd>,
    <trd>&#x8AAA;&#x670D; <pr>shu&#x14D;f&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="influ.0a">
  <kap><tld/>a,
  <var><kap><tld/>hava</kap></var></kap>
  <snc mrk="influ.0a.KOMUNE">
    <dif>
      Havanta <tld/>on:
      <ekz>
        <tld/>a oratoro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_p;&c_l;&c_y;&c_v;&c_o;&c_v;&c_y;</trd>
  <trd lng="ca">influent</trd>
  <trd lng="cs">vlivn&yacute;</trd>
  <trd lng="de">einflussreich</trd>
  <trd lng="en">influential</trd>
  <trd lng="es">influyente</trd>
  <trd lng="fr">influent</trd>
  <trdgrp lng="hu">
    <trd>befoly&aacute;sos</trd>,
    <trd>hat&aacute;sos</trd>
  </trdgrp>
  <trd lng="ja">&#x5F71;&#x97FF;&#x529B;&#x306E;&#12354;&#x308B; 
  <pr>&#12360;&#12356;&#x304D;&#12423;&#12358;&#12385;&#x304B;&#12425;&#x306E;&#12354;&#x308B;</pr></trd>
  <trd lng="nl">invloedrijk</trd>
  <trd lng="pt">influente</trd>
  <trd lng="ru">&c_v;&c_l;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">vplyvn&yacute;</trd>
  <trd lng="uk">&c_v;&#x43F;&#x43B;&c_i;&c_v;&#x43E;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#26377;&#x5F71;&#x54CD; <pr>y&#x1D2;uy&#x1D0;ngxi&#x1CE;ng</pr></trd>,
    <trd>&#x4E3E;&#x8DB3;&#x8F7B;&#x91CD; <pr>j&#x1D4;z&#xFA;q&#x12B;ngzh&#xF2;ng</pr></trd>,
    <trd>&#x663E;&#x8FBE; <pr>xi&#x1CE;nd&#xE1;</pr></trd>,
    <trd>&#x4F54;&#x4E0A;&#x98A8; <pr>zh&#xE0;nsh&#xE0;ngf&emacron;ng</pr></trd>,
    <trd>&#x4F54;&#x512A;&#x52E2; <pr>zh&#xE0;ny&#xF2;ush&#xEC;</pr></trd>,
    <trd>&#x5F88;&#26377;&#x5F71;&#x54CD; <pr>h&#x11B;ny&#x1D2;uy&#x1D0;ngxi&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="influ.0o">
  <kap><tld/>o</kap>
  <snc mrk="influ.0o.KOMUNE">
    <dif>
      Efiko al iu a&ubreve; io:
      <ekz>
        la <tld/>o de la majstro super liaj dis&ccirc;iploj;
      </ekz>
      <ekz>
        havi <tld/>on sur iun<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sub la <tld/>o de agitantojn<fnt>Z</fnt>.
      </ekz>
      <ekz>
        <tld/>o de la puno al la kulpinto;
      </ekz>
      <ekz>
        posedi grandan politikan <tld/>on;
      </ekz>
      <ekz>
        &gcirc;ui grandan <tld/>on &ccirc;e la ministro;
      </ekz>
      <ekz>
        la <tld/>o de la luno al la tajdoj;
      </ekz>
      <ekz>
        <tld/>o de varmo kaj pluvo sur la rikoltoj;
      </ekz>
      <ekz>
        <tld/>o de tro brila lumo sur la okulo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_p;&c_l;&c_y;&c_w;</trd>
  <trd lng="ca">influ&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>influence</trd>,
    <trd>p&uring;soben&iacute;</trd>,
    <trd>vliv</trd>
  </trdgrp>
  <trd lng="de">Einfluss</trd>
  <trd lng="en">influence</trd>
  <trd lng="es">influencia</trd>
  <trd lng="fr">influence</trd>
  <trdgrp lng="hu">
    <trd>befoly&aacute;s</trd>,
    <trd>r&aacute;hat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F71;&#x97FF;  <pr>&#12360;&#12356;&#x304D;&#12423;&#12358;</pr></trd>,
    <trd>&#x611F;&#21270;  <pr>&#x304B;&#12435;&#x304B;</pr></trd>,
    <trd>&#x5F71;&#x97FF;&#x529B;  <pr>&#12360;&#12356;&#x304D;&#12423;&#12358;&#x308A;&#12423;&#x304F;</pr></trd>,
    <trd>&#x52E2;&#x529B;  <pr>&#x305B;&#12356;&#x308A;&#12423;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">invloed</trd>
  <trd lng="pt">influ&ecirc;ncia</trd>
  <trd lng="ru">&c_v;&c_l;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">vplyv</trd>
  <trd lng="uk">&c_v;&#x43F;&#x43B;&c_i;&c_v;</trd>
  <trdgrp lng="zh">
    <trd>&#x5F71;&#x54CD; <pr>y&#x1D0;ngxi&#x1CE;ng</pr></trd>,
    <trd>&#x611F;&#x67D3; <pr>g&#x1CE;nr&#x1CE;n</pr></trd>,
    <trd>&#x5F71;&#x54CD;&#x529B; <pr>y&#x1D0;ngxi&#x1CE;ngl&#xEC;</pr></trd>,
    <trd>&#x718F;&#38518; <pr>x&#x16B;nt&#xE1;o</pr></trd>,
    <trd>&#26435;&#x52BF; <pr>qu&#xE1;nsh&#xEC;</pr></trd>,
    <trd>&#x5A01;&#x52BF; <pr>w&emacron;ish&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="influ.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="influ.0ebla.KOMUNE">
    <dif>
      Kiu iel povas suferi <tld/>on:
      <ekz>
        facile <tld/>ebla homo;
      </ekz>
      <ekz>
        la elektron-metalo tute ne estas <tld/>ebla de alkaloj kaj
        lesivoj
        <fnt>
          <aut>tr</aut>
          <vrk>Elektron-metalo</vrk>
          <lok>Esperanto Triumfonta, 1920:1, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a; &c_ja;&c_k;&c_o;&c_g;&c_a; &c_l;&c_jo;&c_g;&c_k;&c_a; <ind>&c_w;&c_p;&c_l;&c_y;&c_v;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">influen&ccedil;able</trd>
  <trd lng="ja">&#x5F71;&#x97FF;&#12373;&#x308C;&#12420;&#12377;&#12356;  <pr>&#12360;&#12356;&#x304D;&#12423;&#12358;&#12373;&#x308C;&#12420;&#12377;&#12356;</pr></trd>
  <trd lng="uk">&#x44F;&#x43A;&c_i;&c_j; &#x43F;&c_ib;&c_d;&c_d;&c_a;&c_jeu;&c_t;&#x44C;&c_s;&#x44F; &c_v;&#x43F;&#x43B;&c_i;&c_v;&c_u;</trd>
</drv>

<drv mrk="influ.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="influ.0igxi.KOMUNE">
    <dif>
      Ricevi <tld/>on de...:
      <ekz>
        li <tld/>i&gcirc;as sole de kalkulo pri eventualaj perdoj a&ubreve; gajnoj
        financaj
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_p;&c_a;&c_d; <ind>&c_u;&c_p;&c_l;&c_y;&c_v;&c_a;&c_m;</ind></trd>
  <trd lng="fr">&ecirc;tre <ind>influenc&eacute;</ind> par</trd>
</drv>

<drv mrk="influ.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="influ.kontraux0o.KOMUNE">
    <dif>
      <tld lit="I"/>o kun mala efiko kompare kun alia <tld/>o:
      <ekz>
        &ccirc;ar mankis &scirc;ia kontra&ubreve;<tld/>o, lia frenezo
        <klr>...</klr> grade forpelis la klientojn
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_l;&c_y;&c_w; &c_u; &c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_b;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="fr">contre-influence</trd>
  <trdgrp lng="zh">
    <trd>&#x611F;&#21270; <pr>g&#x1CE;nhu&#xE0;</pr></trd>,
    <trd>&#25239;&#34913; <pr>k&#xE0;ngh&#xE9;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="influ.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="influ.sen0a.KOMUNE">
    <dif>
      Kiu ne <tld/>as, ne pezas, ne efikas:
      <ekz>
        sen<tld/>eco de la gramatiko sur la vortaron
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        e&ccirc; se sen<tld/>aj, ni estas almena&ubreve; propagandemaj
        <fnt>
          <aut>R. F. Vaughan</aut>
          <vrk>La Malaja Lingvo</vrk>
          <lok>The British Esperantist, 1914-10 (118), p. 200a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_w;&c_p;&c_l;&c_y;&c_v;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>neutre<klr> (sans influence)</klr></trd>,
    <trd>sans influence</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F71;&#x97FF;&#x529B;&#x306E;&#x306A;&#12356; 
    <pr>&#12360;&#12356;&#x304D;&#12423;&#12358;&#12385;&#x304B;&#12425;&#x306E;&#x306A;&#12356;</pr></trd>,
    <trd>&#x52E2;&#x529B;&#12434;&#25345;&#x305F;&#x306A;&#12356; 
    <pr>&#x305B;&#12356;&#x308A;&#12423;&#x304F;&#12434;&#12418;&#x305F;&#x306A;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x43A;&c_i;&c_j; &#x43D;&c_je; &#x43C;&c_a;&c_jeu; &c_v;&#x43F;&#x43B;&c_i;&c_v;&c_u;</trd>,
    <trd>&#x43D;&c_je;&c_v;&#x43F;&#x43B;&c_i;&c_v;&#x43E;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E2D;&#24615; <pr>zh&#x14D;ngx&#xEC;ng</pr></trd>,
    <trd>&#x4E2D;&#x7ACB; <pr>zh&#x14D;ngl&#xEC;</pr></trd>,
    <trd>&#x5C0F;&#x53EF; <pr>xi&#x1CE;ok&#x11B;</pr></trd>
  </trdgrp>
</drv>
</art>
