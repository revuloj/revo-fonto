<drv mrk="alarm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="alarm.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="anonc.0i.publike">Anonci</ref> minacantan <ref
      cel="dangxe.0o">dan&gcirc;ero</ref>n
      a&ubreve; voki al batalo a&ubreve; helpo, per <ref tip="prt" cel="signal.0o">signalo</ref> a&ubreve; krioj:
      <ekz>
        oni <tld/>is la gvardion kaj okupis &ccirc;iujn enirojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta vila&gcirc;o estas <tld/>ita
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="avert.0i.pridangxero">averti<sncref/></ref>
    <trdgrp lng="zh">
    <trd>&#22823;&#22768;&#30142;&#21628; <pr>d&agrave;sh&emacron;ngj&iacute;h&#363;</pr></trd>,
    <trd>&#22823;&#32882;&#30142;&#21628; <pr>d&agrave;sh&emacron;ngj&iacute;h&#363;</pr></trd>,
    <trd>&#22577;&#35686; <pr>b&agrave;oj&#464;ng</pr></trd>,
    <trd>&#25253;&#35686; <pr>b&agrave;oj&#464;ng</pr></trd>,
    <trd>&#24779; <pr>w&#462;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
  &c_t;&c_r;&c_y;&c_v;&c_o;&c_g;&c_u;</trd>
  <trdgrp lng="ca">
    <trd>alarmar</trd>,
    <trd>alertar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>alarmovat</trd>,
    <trd>volat do zbran&ecaron;</trd>,
    <trd>vyhl&aacute;sit poplach</trd>
  </trdgrp>
  <trd lng="de">alarmieren</trd>
  <trd lng="en">alarm</trd>
  <trdgrp lng="es">
    <trd>alarmar</trd>,
    <trd>alertar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>alarmer</trd>,
    <trd>alerter</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&zayin;&ayin;&yod;&qof;</trd>
  <trdgrp lng="hu">
    <trd>riaszt</trd>,
    <trd>riad&oacute;ztat</trd>
  </trdgrp>
  <trd lng="ja">&#35686;&#22577;&#12434;&#20253;&#12360;&#12427;  <pr>&#12369;&#12356;&#12411;&#12358;&#12434;&#12388;&#12383;&#12360;&#12427;</pr></trd>
  <trd lng="nl">alarmeren</trd>
  <trdgrp lng="pl">
    <trd>alarmowa&cacute;</trd>,
    <trd>ostrzega&cacute;</trd>,
    <trd>trwo&zdot;y&cacute;</trd>,
    <trd>zaniepokoi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alarmar</trd>,
    <trd>dar <ind>alarme</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol;
  &c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>alarmova&tcaron;</trd>,
    <trd>robi&tcaron; poplach</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_n;&c_ja;&c_t;&c_i; &c_p;&c_o; &c_t;&c_r;&c_i;&c_v;&c_o;&c_z;&c_ib;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i; &c_t;&c_r;&c_i;&c_v;&c_o;&c_g;&c_u;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i; &c_n;&c_a; &c_s;&c_p;&c_o;&c_l;&c_o;&c_h;</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0o">
  <kap><tld/>o</kap>
  <snc mrk="alarm.0o.KOMUNE">
    <dif>
      Signalo por anonci minacantan dan&gcirc;eron a&ubreve; voki al
      batalo a&ubreve; helpo:
      <ekz>
        kriu <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Joel 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne a&ubreve;das la <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>?
      </ekz>
      <ekz>
        oni a&ubreve;das kriojn de <tld/>o, mortangorajn vokojn pri helpo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni pendigis supre la flagon de <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj principaj kontra&ubreve;uloj, kiuj anta&ubreve; ne longe mal&scirc;ate rigardis nin de alte,
        nun jam krias <tld/>on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3626;&#3633;&#3597;&#3597;&#3634;&#3603;&#3648;&#3605;&#3639;&#3629;&#3609;&#3616;&#3633;&#3618;</trd>
    <trdgrp lng="zh">
    <trd>&#22577;&#35686;&#20449;&#34399; <pr>b&agrave;oj&#464;ngx&igrave;nh&agrave;o</pr></trd>,
    <trd>&#25253;&#35686;&#20449;&#21495; <pr>b&agrave;oj&#464;ngx&igrave;nh&agrave;o</pr></trd>,
    <trd>&#21361;&#38505;&#20449;&#21495; <pr>w&eacute;ixi&#462;nx&igrave;nh&agrave;o</pr></trd>,
    <trd>&#21361;&#38570;&#20449;&#34399; <pr>w&eacute;ixi&#462;nx&igrave;nh&agrave;o</pr></trd>,
    <trd>&#35686;&#22577;&#20449;&#34399; <pr>j&#464;ngb&agrave;ox&igrave;nh&agrave;o</pr></trd>,
    <trd>&#35686;&#25253;&#20449;&#21495; <pr>j&#464;ngb&agrave;ox&igrave;nh&agrave;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_v;&c_o;&c_g;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>alarma<klr tip="amb"> (av&iacute;s)</klr></trd>,
    <trd>alerta</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>alarm</trd>,
    <trd>poplach</trd>
  </trdgrp>
  <trd lng="de">Alarm</trd>
  <trd lng="en">alarm</trd>
  <trdgrp lng="es">
    <trd>alarma<klr tip="amb"> (aviso)</klr></trd>,
    <trd>alerta</trd>
  </trdgrp>

  <trdgrp lng="fr">
    <trd>alarme</trd>,
    <trd>alerte<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="he">&alef;&zayin;&ayin;&qof;&he;</trd>
  <trdgrp lng="hu">
    <trd>riad&oacute;</trd>,
    <trd>riaszt&aacute;s</trd>
  </trdgrp>
  <trd lng="id">alarm</trd>
  <trd lng="it">allarme</trd>
  <trd lng="ja">&#35686;&#22577;  <pr>&#12369;&#12356;&#12411;&#12358;</pr></trd>
  <trd lng="nl">alarm</trd>
  <trd lng="pl">alarm</trd>
  <trdgrp lng="pt">
    <trd>alarme</trd>,
    <trd>alarma</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>alarm</trd>,
    <trd>poplach</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_i;&c_v;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_a;&c_l;&c_ja;&c_r;&c_m;</trd>,
    <trd>&c_gu;&c_v;&c_a;&c_l;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0a">
  <kap><tld/>a</kap>
  <snc mrk="alarm.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, maltrankviliga:
      <ekz>
        la <tld/>a stato de la regno
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <klr>[&#8230;]</klr> <tld/>aj trumpetoj estis en lia mano
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 31:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trumpetoj ludis <tld/>an alvokon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brul<tld/>aj sonoriloj komencas brui, la pulvoturo eksplodas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_v;&c_o;&c_zh;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>d'<ind>alarma</ind></trd>,
    <trd>alarmant</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poplachov&yacute;</trd>,
    <trd>popla&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">alarmierend</trd>
  <trd lng="en">alarming</trd>
  <trd lng="fr">alarmant</trd>
  <trd lng="hu">riaszt&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#35686;&#22577;&#12398;  <pr>&#12369;&#12356;&#12411;&#12358;&#12398;</pr></trd>,
    <trd>&#21361;&#24613;&#12434;&#21578;&#12370;&#12427;  <pr>&#12365;&#12365;&#12421;&#12358;&#12434;&#12388;&#12370;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">alarmerend</trd>
  <trd lng="pl">alarmowy</trd>
  <trd lng="pt">alarmante</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">popla&scaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_i;&c_v;&c_o;&c_zh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&c_o;&c_z;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="alarm.0ilo.KOMUNE">
    <dif>
      <ref tip="lst" lst="voko:iloj" cel="aparat.0o.TEK">Aparato</ref> uzata por sonigi <tld/>on,
      okaze de dan&gcirc;ero a&ubreve; &scirc;telo:
      <ekz>
        sur la muroj videblas <tld/>ilo kontra&ubreve; dom&scirc;telistoj, kiu konstante sonas
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2013/010862.php">Katobleka mondo</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne pagas e&ccirc; unu cendon por
        kontra&ubreve;&scirc;telistaj <tld/>iloj
        <fnt>
          <aut>M. Twain, trad. R. Williams</aut>,
          <vrk><url ref="&Sezonoj;2014/05/twain/">
          La familio McWilliams kaj la kontra&ubreve;&scirc;telista
          alarmilo</url></vrk>,
          <lok>La Ondo de Esperanto, 2014:3 (234)</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="avert.0ilo">avertilo</ref>
    <ref tip="sub" cel="siren.0o.FIG">sireno<sncref/></ref>
    <trdgrp lng="zh">
    <trd>&#22577;&#35686;&#31995;&#32113; <pr>b&agrave;oj&#464;ngx&igrave;t&#466;ng</pr></trd>,
    <trd>&#25253;&#35686;&#31995;&#32479; <pr>b&agrave;oj&#464;ngx&igrave;t&#466;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_v;&c_a;&c_r;&c_y;&c_j;&c_n;&c_a;&c_ja;
    <ind>&c_s;&c_y;&c_g;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</ind></trd>,
    <trd>&c_s;&c_y;&c_r;&c_e;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_d;&c_o;&c_k; <klr>(&c_a;&c_v;&c_a;&c_r;&c_y;&c_j;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="ca">alarma<klr tip="amb"> (dispositiu)</klr></trd>
  <trd lng="cs">popla&scaron;n&eacute;
  za&rcaron;&iacute;zen&iacute;</trd>
  <trdgrp lng="de">
    <trd>Alarmanlage</trd>,
    <trd>Sirene</trd>
  </trdgrp>
  <trd lng="en">alarm</trd>
  <trd lng="es">alarma<klr tip="amb"> (dispositivo)</klr></trd>
  <trd lng="fr">alarme<klr> (dispositif)</klr></trd>
  <trdgrp lng="hu">
    <trd>riaszt&oacute;berendez&eacute;s</trd>,
    <trd>riaszt&oacute;k&eacute;sz&uuml;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35686;&#37912;  <pr>&#12369;&#12356;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#35686;&#22577;&#22120;  <pr>&#12369;&#12356;&#12411;&#12358;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="nl">alarm</trd>
  <trdgrp lng="pl">
    <trd>syrena <ind>alarmowa</ind></trd>,
    <trd>system <ind>alarmowy</ind></trd>,
    <trd>ostrzegacz</trd>
  </trdgrp>
  <trd lng="pt">alarme</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_r;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>popla&scaron;n&eacute; zariadenie</trd>,
    <trd>alarm</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_n;&c_a;&c_r;&c_ja;&c_d;&c_d;&c_ja; &c_p;&c_o;&c_d;&c_a;&c_ch;&c_ib; &c_t;&c_r;&c_i;&c_v;&c_o;&c_g;&c_i;
    (&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_g;&c_u;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_d;&c_z;&c_v;&c_ib;&c_n; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="alarm.0isto.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pesimista homo, kiu maltrankviligas la publikon, konstante
      anoncante minacantajn, plej ofte fantaziajn, dan&gcirc;erojn:
      <ekz>
        panjo, ne estu <tld/>isto
        <fnt>
          <aut>Mao Zifu</aut>
          <vrk><url ref="http://www.elerno.cn/originala/orig013.htm">Allogo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Panikmacher</trd>
    <trd lng="en">alarmist</trd>
    <trd lng="fr">alarmiste</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;szmad&aacute;r</trd>,
      <trd>r&eacute;mh&iacute;rterjeszt&odblac;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>alarmista</trd>,
      <trd>sygna&lstroke;owy</trd>
    </trdgrp>
    <trd lng="pt">alarmista</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_n;&c_i;&c_k;&c_jo;&c_r;</trd>,
      <trd>&c_a;&c_l;&c_a;&c_r;&c_m;&c_i;&c_s;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ja;&c_r;&c_m;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">alarmista</trd>
  <trd lng="cs">panika&rcaron;</trd>
  <trd lng="es">alarmista</trd>
  <trdgrp lng="ja">
    <trd>&#35686;&#22577;&#20418;  <pr>&#12369;&#12356;&#12411;&#12358;&#12363;&#12363;&#12426;</pr></trd>,
    <trd>&#19990;&#12398;&#19981;&#23433;&#12434;&#12363;&#12365;&#12383;&#12390;&#12427;&#20154;&#39442;&#12364;&#12379;&#12394;&#20154;
    <pr>&#12424;&#12398;&#12405;&#12354;&#12435;&#12434;&#12363;&#12365;&#12383;&#12390;&#12427;&#12402;&#12392;&#12373;&#12431;&#12364;&#12379;&#12394;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="sk">panik&aacute;r</trd>
  <trd lng="uk">&c_p;&c_a;&c_n;&c_ib;&c_k;&c_je;&c_r;</trd>
</drv>
