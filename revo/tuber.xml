<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tuber</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tuber.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tuber.0o.sxvelajxo">
    <dif>
      &Ccirc;iuspeca bulforma elstara&jcirc;o a&ubreve;
      &scirc;vela&jcirc;o
      sur a&ubreve; en iu objekto, korpo a&ubreve; organo:
      <ekz>
        <tld/>plena saliko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la plej supre elstaranta roka <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oplena dorso de krokodilo<fnt>B</fnt>;
      </ekz>
      <ekz>
        bati al si <tld/>on sur la frunto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>estas <tld/>o en la afero</ind>
        <klr>(la afero ne iras glate)</klr>
        <trd lng="fr">il y a un os</trd>
        <trd lng="pl">i w tym ca&lstroke;y <ind>s&eogonek;k</ind></trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxib.0o">&gcirc;ibo</ref>,
      <ref cel="sxvel.0ajxo">&scirc;vela&jcirc;o</ref>,
      <ref cel="veruk.0o">veruko</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Beule</trd>,
      <trd>Buckel</trd>,
      <trd>Knolle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>dudor</trd>,
      <trd>g&ouml;b</trd>,
      <trd>b&uuml;ty&ouml;k</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>knobbel</trd>,
      <trd>knoest</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>guz</trd>,
      <trd>guze&lstroke;</trd>,
      <trd>s&eogonek;k</trd>,
      <trd>wypuk&lstroke;o&sacute;&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_i;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_u;&c_g;&c_o;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_p;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tuber.0o.BOT">
    <dif>
      Dika firma &scirc;vela&jcirc;o de
      <ref tip="malprt" cel="tig.0o">tigo</ref> a&ubreve;
      <ref tip="malprt" cel="radik.0o.BOT">radiko</ref>, ordinare
      subtera a&ubreve; rekte super la tero:
      <ekz>
        <ind>radika <tld/>o</ind>;
        <trd lng="de">Wurzelknolle</trd>
        <trd lng="hu">gy&ouml;k&eacute;rgum&oacute;</trd>
        <trd lng="pl">bulwa</trd>
      </ekz>
      <ekz>
        <ind>tiga <tld/>o</ind>;
        <trd lng="de">Sprossknolle</trd>
        <trd lng="hu">sz&aacute;rcsom&oacute;</trd>
        <trd lng="nl">knoop</trd>
        <trd lng="pl">s&eogonek;k</trd>
      </ekz>
      <ekz>
        terpomo estas <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o de orkideo, de tigobrasiko.
      </ekz>
    </dif>
    <trd lng="de">Knolle</trd>
    <trd lng="hu">gum&oacute;</trd>
    <trdgrp lng="pl">
      <trd>bulwa</trd>,
      <trd>s&eogonek;k</trd>,
      <trd>cebulka</trd>
    </trdgrp>
    <trd lng="nl">knol</trd>
    <trd lng="ru">&c_k;&c_l;&c_u;&c_b;&c_je;&c_n;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="tuber.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="tuber.har0o.KOS">
    <dif>
      Torda&jcirc;o da haroj diversmaniere kunvolvitaj super la nuko:
      <ekz>
        oni tiom tedis &scirc;in, por ke &scirc;i fortondu la kaskadon
        de siaj 
        haroj jam longaj &gcirc;is la genuoj, kaj faru al si
        har-<tld/>on
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dutt</trd>
  <trd lng="hu">konty</trd>  
  <trd lng="nl">haarknoet</trd>
  <trd lng="pl">kok</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_ch;&c_o;&c_k; <klr
    tip="amb">(&c_p;&c_r;&c_i;&c_ch;&c_jo;&c_s;&c_k;&c_a;)</klr></trd>,
    <trd>&c_u;&c_z;&c_je;&c_l; <klr
    tip="amb">(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </trdgrp>
</drv>
</art>
