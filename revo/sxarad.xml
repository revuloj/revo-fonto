<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxarad.xml,v 1.21 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;arad</rad>/o
</kap>
<drv mrk="sxarad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="enigm.0o.SPO">Enigmo</ref>, en kiu oni devas
      diveni vorton danke al sinsekvaj difinoj de &ccirc;iu el
      &gcirc;iaj silaboj konsiderataj kiel memstaraj vortoj:
      <ekz>
        Du &Scirc;aradoj <klr>(en &leftquot;La stranga butiko&rightquot;)</klr>
        <fnt>
          <bib>LSB</bib>
          <lok>Du &Scirc;aradoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        studado de vortoj efektivi&gcirc;as per allogaj rimedoj: ludoj,
        <tld/>oj,
        facilaj klarigoj, krucvortenigmoj<fnt><aut>Boris Kolker</aut>:
        <vrk>Kreskigi la vortprovizon (Christiane De Vleminck kaj Emile
        Van Damme, Leksikaj ekskursoj, unua volumo)</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 6a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rebus.0o">rebuso</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Scharade</trd>,
    <trd>Silbenr&auml;tsel</trd>
  </trdgrp>
  <trd lng="be">&c_sh;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="en">charade</trd>
  <trd lng="fr">charade</trd>
  <trd lng="hu">sz&oacute;tagrejtv&eacute;ny</trd>
  <trd lng="it">sciarada</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E3;&#x30EC;&#x30FC;&#x30C9;</trd>,
    <trd>&#x5B57;&#x8B0E;  <pr>&#x3042;&#x3056;&#x306A;&#x305E;</pr></trd>
  </trdgrp>
  <trd lng="nl">charade</trd>
  <trd lng="pl">szarada</trd>
  <trd lng="pt">charada</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="uk">&c_sh;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
</drv>
</art>
