  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ok</rad>
    </kap>
    <drv mrk="ok.0">
      <kap><ofc>*</ofc><tld/></kap>
      <snc>
        <dif>
          Numeralo: duoble kvar.
        </dif>
      </snc>
      <trd lng="bg">&c_o;&c_s;&c_je;&c_m;</trd>
      <trd lng="cs">osm</trd>
      <trd lng="de">acht</trd>
      <trd lng="en">eight</trd>
      <trd lng="es">ocho</trd>
      <trd lng="fr">huit</trd>
      <trd lng="nl">acht</trd>
      <trd lng="pl">osiem</trd>
      <trd lng="ru">&#x432;&#x43e;&#x441;&#x435;&#x43c;&#x44c;</trd>
      <trd lng="tr">sekiz</trd>
    </drv>
    <drv mrk="ok.0uma">
      <kap><tld/>uma</kap>
      <snc>
        <uzo tip="fak">MAT</uzo>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Tia ke la bazo de &gcirc;ia nombrosistemo estas 8: 
          <ekz>la 8 <tld/>umaj ciferoj estas 01234567.</ekz>
        </dif>
        <trd lng="en">octal</trd>
        <trd lng="pl">&oacute;siemkowy</trd>
        <trd lng="ru">&#x432;&#x43e;&#x441;&#x44c;&#x43c;&#x435;&#x440;&#x438;&#x447;&#x43d;&#x44b;&#x439;</trd>
      </snc>
    </drv>
  </art>
