<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hacx.xml,v 1.5 2022/04/30 08:42:41 revo Exp $">
<kap>
  <rad>ha&ccirc;</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hacx.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="desegn.0i.KOMUNE">Desegni</ref>
      per densaj paralelaj strekoj signantaj plenigon de surfaco:
      <ekz>
        oblikvaj paraleloj <tld/>itaj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Optika_iluzio">
          </url>Optika iluzio, 2017-03-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schraffieren</trd>,
      <trd>stricheln</trd>
    </trdgrp>
    <trd lng="en">hatch (drawing)</trd>
    <trd lng="fr">hachurer</trd>
  </snc>
  <snc mrk="hacx.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="masagx.0i.MED">Masa&gcirc;i</ref>
      per iom rapida, densa batado pere de la eksteraj randoj
      de la manoj:
      <ekz>
        zorge <tld/>u &scirc;iajn gambojn
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 62a, p. 337a</lok>
        </fnt>.
       </ekz>
    </dif>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x448;&#x442;&#x440;&#x438;&#x445;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43C;&#x430;&#x441;&#x430;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43A;&#x43E;&#x440;&#x43E;&#x442;&#x43A;&#x438;&#x43C;&#x438; &#x441;&#x438;&#x43B;&#x44C;&#x43D;&#x438;&#x43C;&#x438; &#x443;&#x434;&#x430;&#x440;&#x430;&#x43C;&#x438; &#x440;&#x435;&#x431;&#x440;&#x43E;&#x43C; &#x434;&#x43E;&#x43B;&#x43E;&#x43D;&#x456;</trd>
  </trdgrp>
</drv>
</art>

