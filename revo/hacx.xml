<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hacx.xml,v 1.8 2025/03/30 10:24:54 revo Exp $">
<kap>
  <rad>ha&ccirc;</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="hacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hacx.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="desegn.0i.KOMUNE">Desegni</ref>
      per densaj paralelaj strekoj signantaj plenigon de surfaco:
      <ekz>
        oblikvaj paraleloj <tld/>itaj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Optika_iluzio">
          </url>Optika iluzio, 2017-03-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_t;&c_r;&c_y;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>schraffieren</trd>,
      <trd>stricheln</trd>
    </trdgrp>
    <trd lng="en">hatch (drawing)</trd>
    <trd lng="fr">hachurer</trd>
  </snc>
  <snc mrk="hacx.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="masagx.0i.MED">Masa&gcirc;i</ref>
      per iom rapida, densa batado pere de la eksteraj randoj
      de la manoj:
      <ekz>
        zorge <tld/>u &scirc;iajn gambojn
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 62a, p. 337a</lok>
        </fnt>.
       </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_m;&c_a;&c_s;&c_a;&c_zh;</ind> &c_r;&c_u;&c_b;&c_a;&c_m; &c_d;&c_a;&c_l;&c_o;&c_n;&c_ib;</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>streketumi</ke>.
  </rim>

  <trdgrp lng="uk">
    <trd>&c_sh;&c_t;&c_r;&c_i;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1084;&c_a;&c_s;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i; &#1082;&#1086;&c_r;&#1086;&c_t;&#1082;&c_i;&#1084;&c_i; &c_s;&c_i;&#1083;&#1100;&#1085;&c_i;&#1084;&c_i;
    &c_u;&c_d;&c_a;&c_r;&c_a;&#1084;&c_i; &c_r;&c_je;&c_b;&c_r;&#1086;&#1084; &c_d;&#1086;&#1083;&#1086;&#1085;&c_ib;</trd>
  </trdgrp>
</drv>
</art>

