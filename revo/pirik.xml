<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pirik.xml,v 1.13 2024/02/17 21:20:29 revo Exp $">
<kap>
  <rad>pirik</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="pirik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du mallongaj a&ubreve; senakcentaj silaboj; simb. /&ypsilon;&ypsilon;/:
      <ekz>
        <klr>[pro la <ref cel="sxveb.0o.licenco">&scirc;vebo</ref>]</klr>
        anstata&ubreve; jambo a&ubreve; trokeo oni ricevas <tld/>on
        <fnt><bib>PGl</bib>, <lok>p. 21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_r;&c_y;&c_h;&c_ib;&c_j;</trd>
  <trd lng="de">Pyrrhichius</trd>
  <trdgrp lng="en">
    <trd>pyrrhic</trd>,
    <trd>dibrach</trd>
  </trdgrp>
  <trd lng="fr">pyrrhique</trd>
  <trd lng="hu">pirrichius</trd>
  <trd lng="nl">pyrricus</trd>
  <trd lng="pl">pirychej</trd>
  <trdgrp lng="pt">
    <trd>pirr&iacute;quio</trd>,
    <trd>d&iacute;braco</trd>,
    <trd>pariambo</trd>,
    <trd>periambo</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_r;&c_i;&c_h;&c_i;&c_j;</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_r;&c_i;&c_h;&c_ib;&c_j; (&c_s;&c_t;&#x43E;&#x43F;&c_a; &c_z; &c_d;&c_v;&#x43E;&c_h; &#x43A;&#x43E;&c_r;&#x43E;&c_t;&#x43A;&c_i;&c_h;
  &c_s;&#x43A;&#x43B;&c_a;&c_d;&c_ib;&c_v;)</trd>
</drv>
</art>
