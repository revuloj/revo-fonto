<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: admir.xml,v 1.39 2026/02/02 07:17:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>admir</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="admir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="admir.0i.PSI">
    <dif>
      Esti <ref tip="malprt" cel="rav.0i">ravita</ref> per la beleco a&ubreve; supereco:
      <ekz>
        <tld/>i pejza&gcirc;on, virinon, statuon, muzikon,
        heroa&jcirc;on, virton;
      </ekz>
      <ekz>
        <klr>[&ccirc;iuj]</klr> <tld/>is &scirc;ian gracian flugetantan iradon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ramzes</nom> <tld/>is la malavarecon de l' nomar&hcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        du pentra&jcirc;oj <klr>[&#x2026;]</klr> kolektis &ccirc;irka&ubreve; si amason da <tld/>antoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen kia bravulo vi estas! li diris al si mem <tld/>ante sian kura&gcirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; monstron <tld/>as, kiu ame deliras
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ador.0i">adori</ref>,
      <ref cel="envi.0i.ion">envii</ref>,
      <ref cel="rigard.gxu0i">&gcirc;urigardi</ref>
    </refgrp>
    <trdgrp lng="zh">
      <trd>&#x4EF0;&#x6155; <pr>y&#x1CE;ngm&ugrave;</pr></trd>,
      <trd>&#x4F69; <pr>p&egrave;i</pr></trd>,
      <trd>&#x4F69;&#x670D; <pr>p&egrave;ifu</pr></trd>,
      <trd>&#x5FC3;&#x4EEA; <pr>x&imacron;ny&iacute;</pr></trd>,
      <trd>&#x5FC3;&#x5100; <pr>x&imacron;ny&iacute;</pr></trd>,
      <trd>&#x611F;&#x5230;&#x5F88;&#x656C;&#x4F69;
      <pr>g&#x1CE;nd&agrave;oh&ecaron;nj&igrave;ngp&egrave;i</pr></trd>,
      <trd>&#x6155; <pr>m&ugrave;</pr></trd>,
      <trd>&#x62DC; <pr>b&agrave;i</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_v;&c_a;&c_zh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trd lng="bo">&t_ba;&t_sa;&t_tata;&t_naro;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>bama&ntilde; ouzh</trd>,
    <trd>estlammi&ntilde; ouzh</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obdivovat <klr>(n&ecaron;koho, n&ecaron;co)</klr></trd>,
    <trd>obdivovat se <klr>(n&ecaron;komu, n&ecaron;&ccaron;emu)</klr></trd>
  </trdgrp>
  <trd lng="de">bewundern</trd>
  <trd lng="en">admire</trd>
  <trd lng="es">admirar</trd>
  <trd lng="he">&lamed;&#x5B0;&he;&#x5B4;&tav;&#x5B0;&pe;&#x5BC;&#x5B7;&ayin;&#x5B5;&lamed;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#x5606;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x305F;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8CDB;&#x7F8E;&#x3059;&#x308B;  <pr>&#x3055;&#x3093;&#x3073;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x656C;&#x670D;&#x3059;&#x308B;  <pr>&#x3051;&#x3044;&#x3075;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x611F;&#x5FC3;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ms">mengagumi</trd>
  <trdgrp lng="fr">
    <trd>admirer</trd>,
    <trd>s'<ind>enthousiasmer</ind> (pour)</trd>,
    <trd>s'<ind>emmerveiller</ind> (de)</trd>,
    <trd>s'<ind>extasier</ind> (de)</trd>
  </trdgrp>
  <trd lng="hu">csod&aacute;l</trd>
  <trd lng="it">ammirare</trd>
  <trd lng="nl">bewonderen</trd>
  <trd lng="pl">podziwia&cacute;</trd>
  <trdgrp lng="pt">
    <trd>admirar</trd>,
    <trd>apreciar</trd>
  </trdgrp>
  <trd lng="ro">admira</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  </trd>
  <trd lng="sk">obdivova&tcaron;</trd>
  <trd lng="sv">beundra</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_ju;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="admir.0a">
  <kap><tld/>a</kap>
  <snc mrk="admir.0a.PSI">
    <dif>
      Sentanta, esprimanta <tld/>on:
      <ekz>
        <klr>[&scirc;ia vesto]</klr> altiris <tld/>ajn rigardojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        estro <klr>[&#x2026;]</klr> kontenta pri sia dungito, kaj
        e&ccirc; <tld/>a
        <fnt>
          <aut>D. Hromada-Kishegyi, trad. J. Lazert</aut>,
          <vrk><url ref="http://eo.hades-presse.com/n1.99/charpentisto-filozofo.shtml">
          &Ccirc;arpentista filozofio</url></vrk>,
          <lok>Hades, 1999:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujare okazis la Universalaj Kongresoj en atmosfero de senlima entuziasmo,
        <klr>[&#x2026;]</klr> kaj <tld/>a a&ubreve;skultado al la vortoj de la &leftquot;Majstro&rightquot;
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">obdivn&yacute;</trd>
  <trd lng="de">bewundernd</trd>
  <trd lng="fr">admiratif</trd>
  <trd lng="pl">wzbudzaj&aogonek;cy  <ind>podziw</ind></trd>
  <trd lng="sk">obdivn&yacute;</trd>
</drv>

<drv mrk="admir.0o">
  <kap><tld/>o</kap>
  <snc mrk="admir.0o.PSI">
    <dif>
      Sento ricevata de io bela, alta, impresa, iel
      eminenta:
      <ekz>
        li eldiris la&ubreve;te sian <tld/>on pri la fajraj
        &ccirc;evaloj
        <fnt>
          <bib>Fab4</bib>,
          <vrk>La vento rakontas pri Valdemaro Doe kaj pri
          liaj filinoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o al levi&gcirc;o de la suno
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>Kruko kaj Baniko el Bervalo</vrk>,
          <lok>1970</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> indiferente akceptis la signojn de la <tld/>o de l' publiko
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> konstruis kolosajn monumentojn, kies resta&jcirc;oj naskas <tld/>on
        de la nuntempaj teknikistoj
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>obdiv</trd>,
    <trd>odiv</trd>,
    <trd>podiv</trd>
  </trdgrp>
  <trd lng="de">Bewunderung</trd>
  <trd lng="fr">admiration</trd>
  <trd lng="he">&he;&ayin;&resh;&tsadi;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#x5606;  <pr>&#x304B;&#x3093;&#x305F;&#x3093;</pr></trd>,
    <trd>&#x8CDB;&#x7F8E;  <pr>&#x3055;&#x3093;&#x3073;</pr></trd>,
    <trd>&#x656C;&#x670D;  <pr>&#x3051;&#x3044;&#x3075;&#x304F;</pr></trd>,
    <trd>&#x611F;&#x5FC3;  <pr>&#x304B;&#x3093;&#x3057;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podziw</trd>,
    <trd>zachwyt</trd>,
    <trd>admiracja</trd>,
    <trd>mir</trd>
  </trdgrp>
  <trd lng="sk">obdiv</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_m;&c_i;&c_l;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="admir.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="admir.0inda.PSI">
    <dif>
      Kiu meritas, kiu ka&ubreve;zas <tld/>on:
      <ekz>
        birdo <klr>[&#x2026;]</klr> <tld/>inda pro siaj koloroj
        <fnt>
          <bib>Fab3</bib>
          <lok>Birdo Fenikso</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>inda ma&scirc;ino
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ion li man&gcirc;as kun <tld/>inda apetito
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen unu patrino <tld/>inda, kiu sin donas tute al la edukado de
        sia filino
        <fnt>
          <aut>&Eacute;. Javal</aut>,
          <vrk>Inter blinduloj</vrk>,
          <lok>1907</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">obdivuhodn&yacute;</trd>
  <trd lng="de">bewundernswert</trd>
  <trd lng="fr">admirable</trd>
  <trd lng="he">&resh;&alef;&vav;&yod; &lamed;&he;&ayin;&resh;&tsadi;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#x5606;&#x3059;&#x3079;&#x304D;  <pr>&#x304B;&#x3093;&#x305F;&#x3093;&#x3059;&#x3079;&#x304D;</pr></trd>,
    <trd>&#x3059;&#x3070;&#x3089;&#x3057;&#x3044;</trd>,
    <trd>&#x5B9F;&#x306B;&#x307F;&#x3054;&#x3068;&#x306A;  <pr>&#x3058;&#x3064;&#x306B;&#x307F;&#x3054;&#x3068;&#x306A;</pr></trd>,
    <trd>&#x656C;&#x670D;&#x3059;&#x3079;&#x304D;  <pr>&#x3051;&#x3044;&#x3075;&#x304F;&#x3059;&#x3079;&#x304D;</pr></trd>,
    <trd>&#x3042;&#x3063;&#x3071;&#x308C;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>godny <ind>podziwu</ind></trd>,
    <trd>wzbudzaj&aogonek;cy <ind>podziw</ind></trd>,
    <trd>zachwycaj&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hodn&yacute; obdivu</trd>,
    <trd>ob  divuhodn&yacute;</trd>,
    <trd>podivuhodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_d;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_d;&c_je;&c_s;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
