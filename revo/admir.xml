<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: admir.xml,v 1.37 2024/10/24 19:25:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>admir</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="admir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="admir.0i.PSI">
    <dif>
      Esti ravita per la beleco a&ubreve; supereco:
      <ekz>
        <tld/>i pejza&gcirc;on, virinon, statuon, muzikon,
        heroa&jcirc;on, virton;
      </ekz>
      <ekz>
        stultulo trovas &ccirc;iam pli grandan stultulon, kiu lin <tld/>as.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4EF0;&#24917; <pr>y&#x1CE;ngm&#xF9;</pr></trd>,
    <trd>&#x4F69; <pr>p&#xE8;i</pr></trd>,
    <trd>&#x4F69;&#x670D; <pr>p&#xE8;ifu</pr></trd>,
    <trd>&#x5FC3;&#x4EEA; <pr>x&#x12B;ny&#xED;</pr></trd>,
    <trd>&#x5FC3;&#20736; <pr>x&#x12B;ny&#xED;</pr></trd>,
    <trd>&#x611F;&#21040;&#x5F88;&#x656C;&#x4F69;
    <pr>g&#x1CE;nd&#xE0;oh&#x11B;nj&#xEC;ngp&#xE8;i</pr></trd>,
    <trd>&#24917; <pr>m&#xF9;</pr></trd>,
    <trd>&#x62DC; <pr>b&#xE0;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_v;&c_a;&c_zh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trd lng="bo">&#xF56;&#xF66;&#xF9F;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>bama&ntilde; ouzh</trd>,
    <trd>estlammi&ntilde; ouzh</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obdivovat <klr>(n&ecaron;koho, n&ecaron;co)</klr></trd>,
    <trd>obdivovat se <klr>(n&ecaron;komu, n&ecaron;&ccaron;emu)</klr></trd>
  </trdgrp>
  <trd lng="de">bewundern</trd>
  <trd lng="en">admire</trd>
  <trd lng="es">admirar</trd>
  <trd
  lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B4;&#x5EA;&#x5B0;&#x5E4;&#x5BC;&#x5B7;&#x5E2;&#x5B5;&#x5DC;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#22022;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#x305F;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x8CDB;&#x7F8E;&#12377;&#x308B;  <pr>&#12373;&#12435;&#12403;&#12377;&#x308B;</pr></trd>,
    <trd>&#x656C;&#x670D;&#12377;&#x308B;  <pr>&#12369;&#12356;&#12405;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x611F;&#x5FC3;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#12375;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ms">mengagumi</trd>
  <trdgrp lng="fr">
    <trd>admirer</trd>,
    <trd>s'<ind>enthousiasmer</ind> (pour)</trd>,
    <trd>s'<ind>emmerveiller</ind> (de)</trd>,
    <trd>s'<ind>extasier</ind> (de)</trd>
  </trdgrp>
  <trd lng="hu">csod&aacute;l</trd>
  <trd lng="it">ammirare</trd>
  <trd lng="nl">bewonderen</trd>
  <trd lng="pl">podziwia&cacute;</trd>
  <trdgrp lng="pt">
    <trd>admirar</trd>,
    <trd>apreciar</trd>
  </trdgrp>
  <trd lng="ro">admira</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  </trd>
  <trd lng="sk">obdivova&tcaron;</trd>
  <trd lng="sv">beundra</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44E;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43C;&c_i;&#x43B;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_a;&c_h;&#x43E;&#x43F;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="admir.0a">
  <kap><tld/>a</kap>
  <snc mrk="admir.0a.PSI">
    <dif>
      Sentanta, esprimanta <tld/>on:
      <ekz>
        estro <klr>...</klr> kontenta pri sia dungito, kaj
        e&ccirc; <tld/>a
        <fnt>
          <aut>D. Hromada-Kishegyi, trad. J. Lazert</aut>,
          <vrk><url ref="http://eo.hades-presse.com/n1.99/charpentisto-filozofo.shtml">
          &Ccirc;arpentista filozofio</url></vrk>,
          <lok>Hades, 1999:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">obdivn&yacute;</trd>
  <trd lng="fr">admiratif</trd>
  <trd lng="pl">wzbudzaj&aogonek;cy  <ind>podziw</ind></trd>
  <trd lng="sk">obdivn&yacute;</trd>
</drv>

<drv mrk="admir.0o">
  <kap><tld/>o</kap>
  <snc mrk="admir.0o.PSI">
    <dif>
      Sento ricevata de io bela, alta, impresa, iel
      eminenta:
      <ekz>
        li eldiris la&ubreve;te sian <tld/>on pri la fajraj
        &ccirc;evaloj
        <fnt>
          <bib>Fab4</bib>,
          <vrk>La vento rakontas pri Valdemaro Doe kaj pri
          liaj filinoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o al levi&gcirc;o de la suno
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>Kruko kaj Baniko el Bervalo</vrk>,
          <lok>1970</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>obdiv</trd>,
    <trd>odiv</trd>,
    <trd>podiv</trd>
  </trdgrp>
  <trd lng="fr">admiration</trd>
  <trd lng="he">&#x5D4;&#x5E2;&#x5E8;&#x5E6;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#22022;  <pr>&#x304B;&#12435;&#x305F;&#12435;</pr></trd>,
    <trd>&#x8CDB;&#x7F8E;  <pr>&#12373;&#12435;&#12403;</pr></trd>,
    <trd>&#x656C;&#x670D;  <pr>&#12369;&#12356;&#12405;&#x304F;</pr></trd>,
    <trd>&#x611F;&#x5FC3;  <pr>&#x304B;&#12435;&#12375;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podziw</trd>,
    <trd>zachwyt</trd>,
    <trd>admiracja</trd>,
    <trd>mir</trd>
  </trdgrp>
  <trd lng="sk">obdiv</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43C;&c_i;&#x43B;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&c_a;&c_h;&#x43E;&#x43F;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="admir.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="admir.0inda.PSI">
    <dif>
      Kiu meritas, kiu ka&ubreve;zas <tld/>on:
      <ekz>
        jen unu patrino <tld/>inda, kiu sin donas tute al la edukado de
        sia filino
        <fnt>
          <aut>&Eacute;. Javal</aut>,
          <vrk>Inter blinduloj</vrk>,
          <lok>1907</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">obdivuhodn&yacute;</trd>
  <trd lng="fr">admirable</trd>
  <trd lng="he">&#x5E8;&#x5D0;&#x5D5;&#x5D9; &#x5DC;&#x5D4;&#x5E2;&#x5E8;&#x5E6;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#22022;&#12377;&#12409;&#x304D;  <pr>&#x304B;&#12435;&#x305F;&#12435;&#12377;&#12409;&#x304D;</pr></trd>,
    <trd>&#12377;&#12400;&#12425;&#12375;&#12356;</trd>,
    <trd>&#x5B9F;&#x306B;&#x307F;&#12372;&#12392;&#x306A;  <pr>&#12376;&#12388;&#x306B;&#x307F;&#12372;&#12392;&#x306A;</pr></trd>,
    <trd>&#x656C;&#x670D;&#12377;&#12409;&#x304D;  <pr>&#12369;&#12356;&#12405;&#x304F;&#12377;&#12409;&#x304D;</pr></trd>,
    <trd>&#12354;&#12387;&#12401;&#x308C;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>godny <ind>podziwu</ind></trd>,
    <trd>wzbudzaj&aogonek;cy <ind>podziw</ind></trd>,
    <trd>zachwycaj&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hodn&yacute; obdivu</trd>,
    <trd>ob  divuhodn&yacute;</trd>,
    <trd>podivuhodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_d;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_d;&c_je;&c_s;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
