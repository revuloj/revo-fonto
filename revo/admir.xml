<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>admir</rad>/i
</kap>
<drv mrk="admir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti ravita per la beleco a&ubreve; supereco:
      <ekz>
        <tld/>i pejza&gcirc;on, virinon, statuon, muzikon, heroa&jcirc;on, virton;
      </ekz>
      <ekz>
        stultulo trovas &ccirc;iam pli grandan stultulon, kiu lin <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;a&c_h;a&c_p;&c_l;&c_ja;&c_c;&c_c;a</trd>,
    <trd>&c_l;&c_ju;&c_b;a&c_v;a&c_c;&c_c;a</trd>
  </trdgrp>
  <trd lng="cs">obdivovat</trd>
  <trd lng="en">admire</trd>
  <trd lng="fr">admirer</trd>
  <trd lng="it">ammirare</trd>
  <trd lng="nl">bewonderen</trd>
  <trdgrp lng="pt">
    <trd>admirar</trd>,
    <trd>apreciar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">beundra</trd>
</drv>
</art>
