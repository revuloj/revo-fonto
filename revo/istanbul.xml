<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Istanbul</rad>/o
    </kap>
    <drv mrk="istanbul.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  La plej granda urbo kaj eksa &ccirc;efurbo de
          <ref tip="malprt" cel="turk.T0ujo">Turkujo</ref>,
          havenurbo &ccirc;e Bosporo (28&ring;57&#x2032;E, 41&ring;N).
	</dif>
      </snc>
      <refgrp tip="sin">
        <ref cel="bizanc.0o">Bizanco</ref>,
        <ref cel="konstantinopol.0o">Konstantinopolo</ref>
      </refgrp>
      <trd lng="de">Istanbul</trd>
      <trd lng="en">Istanbul</trd>
      <trd lng="el">&Kappa;&omega;&ny;&sigma;&tau;&alfa;&ny;&tau;&jota;&ny;&omikron;&ypsilon_ton;&pi;&omikron;&lambda;&eta;</trd>
      <trd lng="hu">Isztambul</trd>
      <trd lng="nl">Istamboel</trd>
      <trd lng="ru">&c_S;&c_t;&c_a;&c_m;&c_b;&c_u;&c_l;</trd>
      <trd lng="tr">&Idot;stanbul</trd>
    </drv>

  </art>

