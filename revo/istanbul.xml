<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Istanbul</rad>/o
</kap>
<drv mrk="istanbul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La plej granda urbo kaj eksa &ccirc;efurbo de
      <ref tip="malprt" cel="turk.T0ujo">Turkujo</ref>,
      havenurbo &ccirc;e Bosporo (28&ring;57&#8242;E, 41&ring;N):
      <ekz>
        la ortodoksa patriarkejo en <tld/>o, Turkio
        <fnt><aut>Evgeni Georgiev</aut><vrk>Papo Johano Pa&ubreve;lo 
        la 2a en Rumanio</vrk><bib>Monato</bib><lok>jaro 1999a, numero 
        9a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="bizanc.0o">Bizanco</ref>,
    <ref cel="konstantinopol.0o">Konstantinopolo</ref>
  </refgrp>
  <trd lng="be">&c_S;&c_t;&c_a;&c_m;&c_b;&c_u;&c_l;</trd>
  <trd lng="de">Istanbul</trd>
  <trd lng="en">Istanbul</trd>
  <trd lng="el">&Kappa;&omega;&ny;&sigma;&tau;&alfa;&ny;&tau;&jota;&ny;&omikron;&ypsilon_ton;&pi;&omikron;&lambda;&eta;</trd>
  <trdgrp lng="fr">
    <trd>Istanbul</trd>
  </trdgrp>
  <trd lng="hu">Isztambul</trd>
  <trd lng="nl">Istamboel</trd>
  <trd lng="pt">Istambul</trd>
  <trd lng="ru">&c_S;&c_t;&c_a;&c_m;&c_b;&c_u;&c_l;</trd>
  <trd lng="tr">&Idot;stanbul</trd>
</drv>
</art>
