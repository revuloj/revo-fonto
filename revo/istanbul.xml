<vortaro>
<art mrk="$Id: istanbul.xml,v 1.18 2024/09/24 07:09:21 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Istanbul</rad>/o
</kap>
<drv mrk="istanbul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La plej granda <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      kaj iama &ccirc;efurbo de
      <ref tip="malprt" cel="turk.T0ujo">Turkujo</ref>,
      havenurbo &ccirc;e Bosporo (28&#x2DA;57&minute;E, 41&#x2DA;N):
      <ekz>
        iun nokton en aprilo 1915 en <tld/>o la polico arestis 800 intelektulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Susanna Gevorgjan</aut>
          <vrk><url ref="&Monato;/2006/008828.php">Krimo forgesita</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        transe&ubreve;ropa transporto-koridoro, kiu ligas
        Dresdenon kun la havenurbo Tesaloniko &ccirc;e Egea Maro kaj kun <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/011089.php">Ponta ponton malfermas</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ortodoksa patriarkejo en <tld/>o
        <fnt><aut>Evgeni Georgiev</aut><vrk>Papo Johano Pa&ubreve;lo 
        la 2a en Rumanio</vrk><bib>Monato</bib><lok>jaro 1999a, numero 
        9a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bizanc.0o">Bizanco</ref>,
      <ref cel="konstantinopol.0o">Konstantinopolo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_S;&c_t;&c_a;&c_m;&c_b;&c_u;&c_l;</trd>
  <trd lng="de">Istanbul</trd>
  <trd lng="en">Istanbul</trd>
  <trd lng="el">&Kappa;&omega;&ny;&sigma;&tau;&alfa;&ny;&tau;&jota;&ny;&omikron;&ypsilon_ton;&pi;&omikron;&lambda;&eta;</trd>
  <trdgrp lng="fr">
    <trd>Istanbul</trd>
  </trdgrp>
  <trd lng="es">Estambul</trd>
  <trd lng="hu">Isztambul</trd>
  <trd lng="ja">&#x30A4;&#x30B9;&#x30BF;&#x30F3;&#x30D6;&#x30FC;&#x30EB;</trd>
  <trd lng="nl">Istamboel</trd>
  <trd lng="pt">Istambul</trd>
  <trd lng="ru">&c_S;&c_t;&c_a;&c_m;&c_b;&c_u;&c_l;</trd>
  <trd lng="tr">&Idot;stanbul</trd>
  <trd lng="sk">Istanbul</trd>
  <trdgrp lng="cs">
    <trd>Ca&rcaron;ihrad</trd>,
    <trd>Istanbul</trd>
  </trdgrp>
  <trd lng="uk">&#x421;&#x442;&#x430;&#x43C;&#x431;&#x443;&#x43B; </trd>
  <trdgrp lng="zh">
    <trd>&#x4F0A;&#x65AF;&#x5766;&#x5E03;&#x5C14; <pr>y&#x12B;s&#x12B;t&#x1CE;nb&#xF9;&#x11B;r</pr></trd>,
    <trd>&#x4F0A;&#x65AF;&#x5766;&#x5821; <pr>Y&#x12B;s&#x12B;t&#x1CE;nb&#x1CE;o</pr></trd>,
    <trd>&#x4F0A;&#x65AF;&#x5766;&#x5E03;&#x5C14; <pr>Y&#x12B;s&#x12B;t&#x1CE;nb&#xF9;&#x11B;r</pr></trd>,
    <trd>&#x4F0A;&#x65AF;&#x5766;&#x5821; <pr>y&#x12B;s&#x12B;t&#x1CE;nb&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
</art>
