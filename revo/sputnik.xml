<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>sputnik</rad>/o</kap>

    <drv mrk="sputnik.0o">
      <kap><tld/>o</kap>
      <snc>
	<uzo tip="fak">TEK</uzo>
	<dif>
	  Kosma aparato rivoluanta la&ubreve; orbito &ccirc;irka&ubreve; Tero a&ubreve; alia
	  astro kiel ties artefarita
	  <ref tip="super" cel="sateli.0o">satelito</ref>:
	  <ekz>
	    oni ne povas enorbitigi <tld/>ojn pere de pulvo!
	    <fnt>
	      <aut>Donald J. HARLOW</aut>:
	      <vrk>Noveveninto</vrk>.
	      <lok><url
 ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np6301/novevenintou.html"
		  >Norda Prismo, 1963:1</url></lok>
	    </fnt>
	  </ekz><ekz>
	    en la lastaj 10 jaroj &Ccirc;inio jam sukcese pafis 27 <tld/>ojn
	    por pli ol 10 landoj
	    <fnt>
	      <lok><url
	      ref="http://esperanto.cri.com.cn/esperanto/2002/Oct/90548.htm"
		  >&Ccirc;ina Radio Internacia, 2002-11-08</url></lok>
	    </fnt>;
	  </ekz><ekz>
	    komunika, esplora, spiona <tld/>o.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en">satellite</trd>
      <trd lng="ru">&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;<klr> (&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;)</klr></trd>
    </drv>

    <drv mrk="sputnik.0a">
      <kap><tld/>a</kap>
      <snc>
	<uzo tip="fak">TEK</uzo>
	<dif>
	  Rilata al <tld/>o, funkcianta per <tld/>o:
	  <ekz>
	    <tld/>a anteno,
	  </ekz><ekz>
	    <tld/>a telefono,
	  </ekz><ekz>
	    <tld/>a televido,
	  </ekz><ekz>
	    en 1985 oficisto de Unesko rimarkis la miraklan regionon
	    pere de <tld/>aj fotoj kaj malsekretigis &gcirc;in al la mondo
	    <fnt>
	      <aut>Xin Hua</aut>:
	      <vrk>Regiono Paralele Trafluata de Tri Riveroj</vrk>.
	      <lok><url
	      ref="http://esperanto.cri.com.cn/esperanto/2002/Oct/90548.htm"
		  >EP&Ccirc;</url></lok>
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
    </drv>

  </art>

<!--
$Log$
-->
</vortaro>