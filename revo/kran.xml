<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kran.xml,v 1.19 2021/05/13 13:13:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kran</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kran.0o.AKV">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato adaptita al ekstrema&jcirc;o de tubo kaj provizita per
      movebla fermilo, kiu la&ubreve;vole ellasas a&ubreve; detenas elfluadon
      de gaso a&ubreve; flua&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kohout</trd>,
    <trd>kohoutek</trd>,
    <trd>ventil</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hahn</trd>,
    <trd>Zapfen</trd>,
    <trd>Wasserhahn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tap</trd>,
    <trd>cock</trd>,
    <trd>faucet</trd>,
    <trd>spigot</trd>
  </trdgrp>
  <trd lng="fr">robinet</trd>
  <trd lng="hu">csap <klr tip="ind">(v&iacute;z-, g&aacute;z-)</klr></trd>
  <trd lng="nl">kraan</trd>
  <trdgrp lng="pl">
    <trd>kurek</trd>,
    <trd>kran</trd>
  </trdgrp>
  <trd lng="pt">torneira</trd>
  <trd lng="ro">robinet</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_n; <klr>(&c_n;&c_a; &c_t;&c_r;&c_u;&c_b;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>koh&uacute;tik</trd>,
    <trd>p&iacute;pa</trd>
  </trdgrp>
  <trd lng="sv">kran</trd>
</drv>

<drv mrk="kran.el0i">
  <kap>el<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elver&scirc;i per <tld/>o:
      <ekz>
        oni &ccirc;i tie elkranis bieron, enver&scirc;is vinon, komercis
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.retour.cz/muzeum-sumavy/esperanto/susice.htm">
          Muzeo de Sumava</url></vrk>,
          <lok>2003</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nato&ccaron;it tekutinu z kohoutu</trd>,
    <trd>na&ccaron;epovat tekutinu z kohoutu</trd>,
    <trd>st&aacute;&ccaron;et tekutinu z p&iacute;py</trd>
  </trdgrp>
  <trd lng="de">zapfen</trd>
  <trd lng="en">draw off</trd>
  <trd lng="hu">kiereszt <klr tip="ind">(csapb&oacute;l)</klr></trd>
  <trd lng="nl">aftappen</trd>
  <trd lng="pl">czerpa&cacute;</trd>     
  <trd lng="ro">trage</trd>
  <trd lng="sk">na&ccaron;apova&tcaron; tekutinu z koh&uacute;tika</trd>
</drv>

<drv mrk="kran.miks0o">
  <kap>miks<tld/>o</kap>
  <snc mrk="kran.miks0o.AKV">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="kran.0o.AKV"><tld lit="K"/>o</ref>
      per kiu oni povas miksi kaj dozi plurajn alfluojn,
      ekzemple varmakvan kaj malvarmakvan:
      <ekz>
        kuirejaj lavkuvo, miks<tld/>o kaj vazlavilo
        <fnt>
          <aut>WE</aut>,
          <vrk>Ni solvas termin-vaka&jcirc;ojn</vrk>,
          <lok><url
          ref="http://fabien.galerio.org/kofo/cirkuleroj/kofo14.pdf">
          Konstru-Forumo, 2009-01 (14)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">umyvadlov&aacute; baterie</trd>
  <trd lng="fr">robinet m&eacute;langeur</trd>
  <trd lng="pl">bateria umywalkowa</trd>     
  <trd lng="ro">bazin</trd>
  <trd lng="sk">um&yacute;vadlov&aacute; bat&eacute;ria</trd>
</drv>
</art>
