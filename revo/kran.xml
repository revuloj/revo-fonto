<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kran</rad>/o
</kap>

<drv mrk="kran.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Aparato adaptita al ekstrema&jcirc;o de tubo, kaj provizita per
      turnebla
      fermilo, kiu povas la&ubreve; la direkto de sia turni&gcirc;o ebligi a&ubreve;
      neebligi
      elfluadon de flua&jcirc;o a&ubreve; gaso.
    </dif>
    <trd lng="be">&c_k;&c_r;&c_a;&c_n;</trd>
    <trd lng="bg">&c_k;&c_r;&c_a;&c_n;</trd>
    <trd lng="nl">kraan</trd>
    <trdgrp lng="en">
      <trd>tap</trd>,
      <trd>cock</trd>,
      <trd>faucet</trd>,
      <trd>spigot</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Hahn</trd>,
      <trd>Zapfen</trd>,
      <trd>Wasserhahn</trd>
    </trdgrp>
    <trd lng="fr">robinet</trd>
    <trd lng="hu">csap <klr tip="ind">(v&iacute;z-, g&aacute;z-)</klr></trd>
    <trd lng="pt">torneira</trd>
    <trd lng="ru">&c_k;&c_r;&c_a;&c_n; <klr>(&c_n;&c_a; &c_t;&c_r;&c_u;&c_b;&c_je;)</klr></trd>
    <trd lng="sv">kran</trd>
  </snc>
</drv>

<drv mrk="kran.el0i">
  <kap>
    el<tld/>i<fnt>Z</fnt>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elver&scirc;i per <tld/>o.
    </dif>
    <trd lng="nl">aftappen</trd>
    <trd lng="en">draw off</trd>
  </snc>
  <trd lng="de">zapfen</trd>
  <trd lng="hu">kiereszt <klr tip="ind">(csapb&oacute;l)</klr></trd>
</drv>
</art>
