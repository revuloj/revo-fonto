<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kran.xml,v 1.26 2023/12/03 09:59:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kran</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kran.0o.AKV">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato adaptita al ekstrema&jcirc;o de tubo kaj provizita per
      movebla <ref tip="prt" cel="ferm.0ilo">fermilo</ref>, 
      kiu la&ubreve;vole ellasas a&ubreve; detenas elfluadon
      de gaso a&ubreve; flua&jcirc;o:
      <ekz>
        la <tld/>oj de flamingoj estis lutitaj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        malfermante la <tld/>on, mi fluigis en mian manplaton la fre&scirc;an akvon
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;oforo descendis, malfermis ian <tld/>on, starantan apud la vojo kaj tiris
        flekseblan tubon de tie &gcirc;is la benzinujo de la a&ubreve;to
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu ferm&scirc;ra&ubreve;bu la <tld/>on? kompreneble la subhejtisto
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 40</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxan.0o">&ccirc;ano</ref>,
      <ref cel="spil.0ilo">spililo</ref>,
      <ref cel="sxtop.0ilo.ferm">&scirc;topilo<sncref/></ref>,
      <ref cel="valv.0o.TEK">valvo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kohout</trd>,
    <trd>kohoutek</trd>,
    <trd>ventil</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hahn <klr>(Wasser)</klr></trd>,
    <trd>Zapfen <klr>(Hahn)</klr></trd>,
    <trd>Wasserhahn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tap</trd>,
    <trd>cock</trd>,
    <trd>faucet</trd>,
    <trd>spigot</trd>
  </trdgrp>
  <trd lng="fr">robinet</trd>
  <trd lng="hu">csap <klr tip="ind">(v&iacute;z-, g&aacute;z-)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6813;  <pr>&#x305B;&#x3093;</pr></trd>,
    <trd>&#x30B3;&#x30C3;&#x30AF;  <pr>&#x3053;&#x3063;&#x304F;</pr></trd>,
    <trd>&#x86C7;&#x53E3;  <pr>&#x3058;&#x3083;&#x3050;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="nl">kraan</trd>
  <trdgrp lng="pl">
    <trd>kurek</trd>,
    <trd>kran</trd>
  </trdgrp>
  <trd lng="pt">torneira</trd>
  <trd lng="ro">robinet</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_n; <klr>(&c_n;&c_a; &c_t;&c_r;&c_u;&c_b;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>koh&uacute;tik</trd>,
    <trd>p&iacute;pa</trd>
  </trdgrp>
  <trd lng="sv">kran</trd>
  <trd lng="uk">&c_k;&c_r;&c_a;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x9F8D;&#x982D; <pr>shu&#x1D0;l&#xF3;ngt&#xF3;u</pr></trd>,
    <trd>&#x9F8D;&#x982D; <pr>l&#xF3;ngt&#xF3;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kran.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="versx.el0i">Elver&scirc;i</ref> per <tld/>o:
      <ekz>
        oni &ccirc;i tie el<tld/>is bieron, enver&scirc;is vinon, komercis
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.retour.cz/muzeum-sumavy/esperanto/susice.htm">
          Muzeo de Sumava</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spil.0i">spili</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; &c_z; &c_k;&c_r;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>nato&ccaron;it tekutinu z kohoutu</trd>,
    <trd>na&ccaron;epovat tekutinu z kohoutu</trd>,
    <trd>st&aacute;&ccaron;et tekutinu z p&iacute;py</trd>
  </trdgrp>
  <trd lng="de">zapfen</trd>
  <trd lng="en">draw off</trd>
  <trd lng="hu">kiereszt <klr tip="ind">(csapb&oacute;l)</klr></trd>
  <trd lng="ja">&#x86C7;&#x53E3;&#x304B;&#x3089;&#x6CE8;&#x3050;  <pr>&#x3058;&#x3083;&#x3050;&#x3061;&#x304B;&#x3089;&#x305D;&#x305D;&#x3050;</pr></trd>
  <trd lng="nl">aftappen</trd>
  <trd lng="pl">czerpa&cacute;</trd>     
  <trd lng="ro">trage</trd>
  <trd lng="sk">na&ccaron;apova&tcaron; tekutinu z koh&uacute;tika</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i;/&c_n;&c_a;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i; &c_z; &c_k;&c_r;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x653E;&#x6389; <pr>f&#xE0;ngdi&#xE0;o</pr></trd>
</drv>

<drv mrk="kran.miks0o">
  <kap>miks<tld/>o</kap>
  <snc mrk="kran.miks0o.AKV">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="kran.0o.AKV"><tld lit="K"/>o</ref>
      per kiu oni povas miksi kaj dozi plurajn alfluojn,
      ekzemple varmakvan kaj malvarmakvan:
      <ekz>
        kuirejaj lavkuvo, miks<tld/>o kaj vazlavilo
        <fnt>
          <aut>WE</aut>,
          <vrk>Ni solvas termin-vaka&jcirc;ojn</vrk>,
          <lok><url ref="http://fabien.galerio.org/kofo/cirkuleroj/kofo14.pdf">
          Konstru-Forumo, 2009-01 (14)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_je;&c_sh;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; <klr>(&c_s;&c_a;&c_n;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="cs">umyvadlov&aacute; baterie</trd>
  <trd lng="de">Mischbatterie</trd>
  <trd lng="fr">robinet m&eacute;langeur</trd>
  <trd lng="pl">bateria umywalkowa</trd>     
  <trd lng="ro">bazin</trd>
  <trd lng="sk">um&yacute;vadlov&aacute; bat&eacute;ria</trd>
</drv>
</art>
