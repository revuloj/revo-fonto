<drv mrk="rok.0o">
  <kap><tld/>o</kap>
  <snc mrk="rok.0o.KOMUNE">
    <dif>
      Granda &scirc;tono nature firme fiksita en iu loko:
      <ekz>
        tie ku&scirc;is grandaj blokoj da <tld/>o, &ccirc;irka&ubreve;kreskitaj de
        musko
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sidis sur <tld/>o anta&ubreve; granda kaverno
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;hakitaj pecegoj da granito, &ccirc;iu el ili estas peco da
        <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &ccirc;i tiu <tld/>o mi konstruos mian eklezion
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 16:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie staras rokoj, kies strangajn formojn donis la ventoj kaj
        tajdoj
        <fnt>
          <bib>Monato</bib>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006851.html">Francio:
          Quimper</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        multaj devis foriri al la montaro, kie inter altaj <tld/>oj ili
        &gcirc;isvivis 
        la 17an jarcenton, miksi&gcirc;inte kun la indi&gcirc;ena lo&gcirc;antaro
        <fnt>
          <bib>Monato</bib>
          <aut>Vja&ccirc;eslav (Slavik) Ivanov</aut>
          <vrk><url ref="https://www.monato.be/2003/007865.html">La oseta
          lingvo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konstrui sur <tld/>o
        <klr>(fari ion solidan, fortikan)</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:24</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Fels</trd>
    <trd lng="ru">&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="rok.0o.GEOL">
    <uzo tip="stl">EVI</uzo>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="dif" cel="rok.0ajxo.GEOL"><tld lit="R"/>a&jcirc;o<sncref/></ref>:
      <ekz>
        la akvo malfortigas la <tld/>ojn kaj faciligas
        ilian diseri&gcirc;on
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="af">rock</trd>
  <trd lng="am">&#x12D0;&#x1208;&#x1275;</trd>
  <trd lng="ar">&#x635;&#x62E;&#x631;&#x629;</trd>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  <trd lng="bg">&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  <trd lng="bn">&#x9B6;&#x9BF;&#x9B2;&#x9BE;</trd>
  <trd lng="bo">&#xF62;&#xFA1;&#xF7C;&#xF0B;</trd>
  <trd lng="co">scogliu</trd>
  <trdgrp lng="cs">
    <trd>rock</trd>,
    <trd>rockov&aacute; hudba</trd>
  </trdgrp>
  <trd lng="da">sten</trd>
  <trd lng="en">rock</trd>
