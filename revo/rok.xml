<drv mrk="rok.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>oj:
      <ekz>
        vi vidas tie la <tld/>an muregon kaj la grandan kavernon
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la forta <tld/>a insulo staras meze de la bruanta maro
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>a golf&#x2019; la &scirc;ipon ili ka&scirc;is
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> kastelo &ccirc;iam ankora&ubreve; staris sen&scirc;an&gcirc;e alte supre
        sur sia <tld/>a alta&jcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">felsig</trd>
  <trdgrp lng="en">
    <trd>rock</trd>,
    <trd>rocky</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5CA9;&#x306E;  <pr>&#x3044;&#x308F;&#x306E;</pr></trd>,
    <trd>&#x5CA9;&#x77F3;&#x304B;&#x3089;&#x306A;&#x308B;  <pr>&#x304C;&#x3093;&#x305B;&#x304D;&#x304B;&#x3089;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x5CA9;&#x306B;&#x8986;&#x308F;&#x308C;&#x305F;  <pr>&#x3044;&#x308F;&#x306B;&#x304A;&#x304A;&#x308F;&#x308C;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skalisty</trd>,
    <trd>skalny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_je;&c_l;&c_ja;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_t;&c_r;&c_u;&c_b;&c_a;&c_t;&c_i;&c_j;</trd>
