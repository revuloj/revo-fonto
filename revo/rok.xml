  <trd lng="jw">watu</trd>
  <trd lng="ka">&#x10E0;&#x10DD;&#x10D9;&#x10D8;</trd>
  <trd lng="km">&#x179A;&#x17C9;&#x17BB;&#x1780;</trd>
  <trd lng="ko">&#xBC14;&#xC704;</trd>
  <trd lng="lo">&#xEAB;&#xEB5;&#xE99;</trd>
  <trd lng="ml">&#xD2A;&#xD3E;&#xD31;</trd>
  <trd lng="mr">&#x930;&#x949;&#x915;</trd>
  <trd lng="my">&#x1000;&#x103B;&#x1031;&#x102C;&#x1000;&#x103A;&#x101C;&#x102F;&#x1036;&#x1038;&#x1000;&#x103C;&#x102E;&#x1038;</trd>
  <trd lng="ne">&#x91A;&#x91F;&#x94D;&#x91F;&#x93E;&#x928;</trd>
  <trd lng="nl">rots</trd>
  <trd lng="ny">thanthwe</trd>
