<vortaro>
<art mrk="$Id$">
<kap>
  <rad>romboedr</rad>/o
</kap>
<drv mrk="romboe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="parale1.0o">Paralelepipedo</ref>, kies &ccirc;iuj
      facoj estas <ref cel="romb.0o.figuro">romboj</ref>:
      <ekz>
        la facoj de <tld/>o estas egalaj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_b;&c_o;&c_e;&c_d;&c_r;</trd>
  <trd lng="de">Rhomboeder</trd>
  <trd lng="en">rhombohedron</trd>
  <trd lng="fr">rhombo&egrave;dre</trd>
  <trd lng="hu">rombo&eacute;der</trd>
  <trd lng="pl">romboedr</trd>
  <trd lng="pt">romboedro</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_b;&c_o;&c_e;&c_d;&c_r;</trd>
</drv>
</art>

