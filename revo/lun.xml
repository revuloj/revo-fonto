<drv mrk="lun.kreska0o">
  <kap>kreska <tld/>o,
  <var><kap>kreskanta <tld/>o</kap></var></kap>
  <snc mrk="lun.kreska0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Fazo kaj aspekto de la <tld/>o,
      kiam &gcirc;ia luma parto de tago al tago grandi&gcirc;as:
      <ekz>
        sikula flago estas blua kun okradia suno <klr>[&#x2026;]</klr> kaj 
        kreskanta <tld/>o
        <fnt>
          <aut>O. Arno&scaron;t Fischer, G. Stier</aut>,
          <vrk><url ref="http://www.literatura.bucek.name/brno/fischer_sikuloj.pdf">
          En sikula lando</url></vrk>,
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kresce.0o">krescento</ref>
    <rim>
      La &gcirc;iba parto vidi&gcirc;as tiam
      dekstra en la norda duonglobo de la tero,
      maldekstra en la suda duonglobo.
    </rim>
  </snc>
   <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_k; <klr>(&c_p;&c_je;&c_r;&c_sh;&c_a;&c_ja; &c_k;&c_v;&c_a;&c_d;&c_r;&c_a; &c_m;&c_je;&c_s;&c_ja;&c_c;&c_a;)</klr></trd>
  <trd lng="de">zunehmender <ind>Mond</ind></trd>
  <trd lng="fr">lune croissante</trd>
  <trd lng="ja">&#x4E0A;&#x5F26;&#x306E;&#x6708;  <pr>&#x3058;&#x3087;&#x3046;&#x3052;&#x3093;&#x306E;&#x3064;&#x304D;</pr></trd>
  <trd lng="nl">wassende <ind>maan</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x6708;&#x7259; <pr>yu&#xE8;y&#xE1;</pr></trd>,
    <trd>&#x76C8;&#x6708; <pr>y&#xED;ngyu&#xE8;</pr></trd>,
    <trd>&#x670F; <pr>f&#x11B;i</pr></trd>
  </trdgrp>
