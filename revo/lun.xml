    <trd>&c_r;&c_a;&c_v;&c_je;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_i;&c_j; &c_p;&c_ib;&c_v;&c_m;&c_ib;&c_s;&c_ja;&c_c;&c_mol; &c_n;&c_a; &c_n;&c_ib;&c_g;&c_t;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
