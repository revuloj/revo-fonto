  <snc mrk="lun.miel0o.MOR">
    <dif>
      <ref tip="dif" cel="monat.miel0o.EDZ">Mielmonato</ref>:
      <ekz>
        &leftquot;en bonaj familioj la novaj geedzoj ne pasigas la unuan nokton
        sub la kontrolo de la najbaroj, kaj&rightquot; &ndash; tion &scirc;i aldonis
        pli faciltone &ndash; &leftquot;tial oni ja inventis la miel<tld/>an
        voja&gcirc;on&rightquot;
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_d;&c_o;&c_v;&c_y; &c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
