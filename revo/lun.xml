<drv mrk="lun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lun.0o.AST_tera">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="astr.0o.AST">Astro</ref>,
      kiu estas <ref tip="super" cel="sateli.0o.planedo">satelito</ref>
      de la <ref tip="vid" cel="ter.0o.planedo">Tero</ref>
      kaj rondiras &ccirc;irka&ubreve; &gcirc;i:
      <ekz>
        la kvar <ind>fazoj de la <tld/>o</ind>
        estas: nov<tld/>o, kreska <tld/>o,
        plen<tld/>o, malkreska <tld/>o;
        <trdgrp lng="pl">
          <trd>fazy <ind>ksi&eogonek;&zdot;yca</ind></trd>,
          <trd>kwadry <ind>ksi&eogonek;&zdot;yca</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>kvarono</ind>;
        <trd lng="it"><ind>quarto</ind> di Luna</trd>
        <trd lng="nl">kwartier <klr>(v. maan)</klr></trd>
      </ekz>
      <ekz>
        levinte viajn okulojn al la &ccirc;ielo kaj vidinte la sunon kaj la <tld/>on kaj la
        stelojn
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 4:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i aspektas <klr>[&#8230;]</klr> bela kiel la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 6:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o fari&gcirc;os sanga
        <fnt>
          <bib>MT</bib>
          <lok>Joel 2:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta <tld/>o fari&gcirc;is kiel sango
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estis grandega nokta lampo, alte supre en la blua aero
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o ne a&ubreve;skultas, kiam hundo &gcirc;in insultas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se forestas la suno, sufi&ccirc;as la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o influas la <ref cel="tajd.0o.MAR">tajdo</ref>jn, tirante akvon trans oceanojn
        <fnt>
       <aut>Will Green</aut>
          <vrk><url ref="&Monato;/2000/006869.php">La luna ludo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o &ccirc;irka&ubreve;iras la mondon en da&ubreve;ro de 25 horoj
        <fnt>
       <aut>Will Green</aut>
          <vrk><url ref="&Monato;/2000/006869.php">La luna ludo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        prezidanto <nom>Bush</nom> favoris usonan <tld/>programon, tiel ke spacorganiza&jcirc;o NASA
        devis duonigi sian bu&gcirc;eton por ISS
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009681.php">Politika sukceso, scienca
          fiasko</url></vrk>
          <bib>Monato</bib>
          <lok>2009/01, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_je;&c_s;&c_ja;&c_c;</trd>
    <trd lng="br">loar</trd>
    <trd lng="en">Moon</trd>
    <trd lng="es">Luna</trd>
    <trd lng="he">&lamed;&bet;&nun;&he;</trd>
    <trd lng="hu">Hold</trd>
    <trd lng="it">Luna</trd>
    <trd lng="ja">&#26376; <pr>&#12388;&#12365;</pr></trd>
    <trdgrp lng="pl">
      <trd>ksi&eogonek;&zdot;yc</trd>,
      <trd>miesi&aogonek;c<klr> (przest.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_L;&c_u;&c_n;&c_a;</trd>
    <trd lng="sk">Mesiac</trd>
    <trdgrp lng="zh">
      <trd>&#26376;<klr> [yu&egrave;]</klr></trd>,
      <trd>&#26376;&#20142;<klr> [yu&egrave;liang]</klr></trd>,
      <trd>&#26376;&#29699;<klr> [yu&egrave;qi&uacute;]</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="lun.0o.AST_planeda">
    <uzo tip="fak">AST</uzo>
    <dif>
      Natura <ref tip="sin" cel="sateli.0o.planedo">satelito</ref> de planedo:
      <ekz>
        la <tld/>oj de Jupitero;
      </ekz>
      <ekz>
        <klr>[la]</klr> interplaneda sondilo <nom>Huygens</nom> <klr>[&#8230;]</klr> sur<tld/>i&gcirc;is
        sur
        Titano, la plej granda el la multaj <tld/>oj de Saturno
        <fnt>
          <aut>Eskil Svane</aut>
          <vrk><url ref="&Monato;/2005/008552.php">Ankora&ubreve; pri EDE</url></vrk>
          <bib>Monato</bib>
          <lok>2005/03, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;
      </trd>
    </trdgrp>
    <trd lng="br">loarenn</trd>
    <trd lng="en">moon</trd>
    <trd lng="es">luna</trd>
    <trd lng="hu">hold</trd>
