<drv mrk="lun.0a">
  <kap><tld/>a</kap>
  <snc mrk="lun.0a.AST">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a nokto
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a lumo, en kiu disfluis nigraj ombroj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj radioj, penetrantaj tra la fenestro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;inoj festas novjaron la&ubreve; la malnova <tld/>a kalendaro
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006852.php">&Ccirc;u vere kristana erao?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 17</lok>
          </fnt>;
      </ekz>
      <ekz>
        ha&ubreve;to <tld/>a
        <fnt>
          <aut>Alejandro Cossavella</aut>
          <vrk><url ref="&Monato;/2006/008766.php">Poemoj de Alejandro
          Cossavella</url></vrk>
          <bib>Monato</bib>
          <lok>2006/04, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_c;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="br">-<ind>loar</ind></trd>
  <trdgrp lng="cs">
    <trd>lun&aacute;rn&iacute;</trd>,
    <trd>m&ecaron;s&iacute;&ccaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mond-</trd>,
    <trd>lunar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lunar</trd>,
    <trd>moonly</trd>
  </trdgrp>
  <trd lng="es">lunar</trd>
  <trd lng="fr">lunaire</trd>
  <trd lng="he">&yod;&resh;&het;&yod;</trd>
  <trd lng="hu">hold-</trd>
  <trd lng="id">lunar</trd>
  <trd lng="io">lunala</trd>
  <trdgrp lng="it">
    <trd>lunare</trd>,
    <trd>selenico</trd>,
    <trd>selenitico</trd>
  </trdgrp>
  <trd lng="ja">&#26376;&#12398;  <pr>&#12370;&#12388;&#12398;</pr></trd>
  <trd lng="nl">maan-</trd>
  <trd lng="pl">ksi&eogonek;&zdot;ycowy</trd>
