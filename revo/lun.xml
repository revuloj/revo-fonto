<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lun</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="lun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lun.0o.AST_tera">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="astr.0o.AST">Astro</ref>,
      kiu estas <ref tip="super" cel="sateli.0o.planedo">satelito</ref>
      de la <ref tip="vid" cel="ter.0o.planedo">Tero</ref>
      kaj rondiras &ccirc;irka&ubreve; &gcirc;i:
      <ekz>
        la kvar <ind>fazoj de la <tld/>o</ind>
        estas: nov<tld/>o, kreska <tld/>o,
        plen<tld/>o, malkreska <tld/>o;
        <trdgrp lng="pl">
          <trd>fazy <ind>ksi&eogonek;&zdot;yca</ind></trd>,
          <trd>kwadry <ind>ksi&eogonek;&zdot;yca</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>kvarono</ind>.
        <trd lng="it"><ind>quarto</ind> di Luna</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_je;&c_s;&c_ja;&c_c;</trd>
    <trd lng="br">loar</trd>
    <trd lng="en">Moon</trd>
    <trd lng="es">Luna</trd>
    <trd lng="he">&lamed;&bet;&nun;&he;</trd>
    <trd lng="hu">Hold</trd>
    <trd lng="it">Luna</trd>
    <trd lng="ja">&#26376; [<ind>&#12388;&#12365;</ind>]</trd>
    <trdgrp lng="pl">
      <trd>ksi&eogonek;&zdot;yc</trd>,
      <trd>miesi&aogonek;c<klr> (przest.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_L;&c_u;&c_n;&c_a;</trd>
    <trd lng="sk">Mesiac</trd>
    <trdgrp lng="zh">
      <trd>&#26376;<klr> [yu&egrave;]</klr></trd>,
      <trd>&#26376;&#20142;<klr> [yu&egrave;liang]</klr></trd>,
      <trd>&#26376;&#29699;<klr> [yu&egrave;qi&uacute;]</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="lun.0o.AST_planeda">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nomo donita al naturaj <ref tip="sin" 
      cel="sateli.0o.planedo">satelitoj
      </ref> de aliaj planedoj ol Tero:
      <ekz>
        la <tld/>oj de Jupitero.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;
      </trd>
    </trdgrp>
    <trd lng="br">loarenn</trd>
    <trd lng="en">moon</trd>
    <trd lng="es">luna</trd>
    <trd lng="hu">hold</trd>
    <trdgrp lng="it">
      <trd><ind>satellite</ind> naturale</trd>
      <trd>luna <klr>(nome comune)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ksi&eogonek;&zdot;yc</trd>,
      <trd>satelita</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_u;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sk">mesiac <klr>(nebesk&eacute; teleso)</klr></trd>
  </snc>
  <trd lng="bg">&c_l;&c_u;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>luna</trd>,
    <trd>m&ecaron;s&iacute;c</trd>
  </trdgrp>
  <trd lng="af">maan</trd>
  <trd lng="de">Mond</trd>
  <trd lng="fr">lune</trd>
  <trd lng="fy">moanne</trd>
  <trd lng="he">&yod;&resh;&het;</trd>
  <trd lng="ia">luna</trd>
  <trd lng="id">bulan</trd>
  <trd lng="io">luno</trd>
  <trd lng="jbo">lunra</trd>
  <trd lng="nl">maan</trd>
  <trd lng="pt">lua</trd>
  <trdgrp lng="sk">
    <trd>luna</trd>,
    <trd>mesiac</trd>
  </trdgrp>
  <trd lng="sv">m&aring;ne</trd>
  <trd lng="tp">mun</trd>
  <trd lng="vo">mun</trd>
</drv>

<drv mrk="lun.0a">
  <kap><tld/>a</kap>
  <snc mrk="lun.0a.AST">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a lumo, helo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_c;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="br">-<ind>loar</ind></trd>
  <trdgrp lng="cs">
    <trd>lun&aacute;rn&iacute;</trd>,
    <trd>m&ecaron;s&iacute;&ccaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mond-</trd>,
    <trd>lunar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lunar</trd>,
    <trd>moonly</trd>
  </trdgrp>
  <trd lng="es">lunar</trd>
  <trd lng="fr">lunaire</trd>
  <trd lng="he">&yod;&resh;&het;&yod;</trd>
  <trd lng="hu">hold-</trd>
  <trd lng="id">lunar</trd>
  <trd lng="io">lunala</trd>
  <trdgrp lng="it">
    <trd>lunare</trd>,
    <trd>selenico</trd>,
    <trd>selenitico</trd>
  </trdgrp>
  <trd lng="nl">maan-</trd>
  <trd lng="pl">ksi&eogonek;&zdot;ycowy</trd>
  <trd lng="pt">lunar</trd>
  <trd lng="ru">&c_l;&c_u;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">mesa&ccaron;n&yacute;</trd>
  <trd lng="sv">m&aring;n-</trd>
  <trd lng="tp">mun</trd>
  <trd lng="vo">munik</trd>
</drv>

<drv mrk="lun.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="lun.duon0o.FORM">
    <dif>
      Formo de la <tld/>o, kiam videblas proksimume
      de kvarono &gcirc;is duono de &gcirc;ia surfaco:
      <ekz>
        duon<tld/>o levi&gcirc;os super la horizonton nur post unu a&ubreve; du 
        horoj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 12a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ark.lun0o.AST">lunarko</ref>
    </refgrp>
  </snc>
  <trd lng="cs">p&uring;lm&ecaron;s&iacute;c</trd>
  <trd lng="fr">demi-lune</trd>
  <trd lng="it">mezzaluna</trd>
  <trd lng="nl">halve maan</trd>
  <trd lng="sk">polmesiac</trd>
  <trd lng="sv">halvm&aring;ne</trd>
</drv>

<drv mrk="lun.kreska0o">
  <kap>kreska <tld/>o,
  <var><kap>kreskanta <tld/>o</kap></var></kap>
  <snc mrk="lun.kreska0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Fazo kaj aspekto de la <tld/>o,
      kiam &gcirc;ia luma parto de tago al tago grandi&gcirc;as:
      <ekz>
        sikula flago estas blua kun okradia suno <klr>...</klr> kaj 
        kreskanta <tld/>o
        <fnt>
          <aut>O. Arno&scaron;t Fischer, G. Stier</aut>,
          <vrk><url
          ref="http://www.literatura.bucek.name/brno/fischer_sikuloj.pdf">
          En sikula lando</url></vrk>,
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La &gcirc;iba parto estas tiam
    dekstra la norda duonglobo de la tero,
    maldekstra en la suda duonglobo.
  </rim>
  <trd lng="fr">lune croissante</trd>
</drv>

<drv mrk="lun.malkreska0o">
  <kap>malkreska <tld/>o,
  <var><kap>malkreskanta <tld/>o</kap></var></kap>
  <snc mrk="lun.malkreska0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Fazo kaj aspekto de la <tld/>o,
      kiam &gcirc;ia luma parto de tago al tago malgrandi&gcirc;as:
      <ekz>
        venis la malkreska <tld/>o flava-vanga
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url
          ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La &gcirc;iba parto estas tiam
    maldekstra la norda duonglobo de la tero,
    dekstra en la suda duonglobo.
  </rim>
  <trd lng="fr">lune d&eacute;croissante</trd>
</drv>

<drv mrk="lun.miel0o">
  <kap>miel<tld/>o</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="lun.miel0o.MOR">
    <ref tip="dif" cel="monat.miel0o.EDZ">mielmonato</ref>
    <ekz>
      &leftquot;en bonaj familioj la novaj geedzoj ne pasigas la unuan nokton 
      sub la kontrolo de la najbaroj, kaj&rightquot; &ndash; tion &scirc;i aldonis 
      pli faciltone &ndash; &leftquot;tial oni ja inventis la miel<tld/>an 
      voja&gcirc;on&rightquot;
      <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="bg">&c_m;&c_je;&c_d;&c_je;&c_n; &c_m;&c_je;&c_s;&c_je;&c_c;</trd>
  <trd lng="de">Flitterwochen</trd>
  <trd lng="en">honeymoon</trd>
  <trd lng="fr">lune de miel</trd>
  <trd lng="id"><ind>bulan</ind> madu</trd>
  <trd lng="it">luna di miele</trd>
  <trd lng="nl">wittebroodsweken</trd>
  <trd lng="sv">smekm&aring;nad</trd>
</drv>

<drv mrk="lun.nov0o">
  <kap>nov<tld/>o,
  <var><kap>nova <tld/>o</kap></var>,
  <var><kap>malplena <tld/>o</kap></var></kap>
  <snc mrk="lun.nov0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Fazo, aspekto de la <tld/>o, fine de ciklo,
      komence de nova ciklo, kiam  
      videblas ankora&ubreve; eta parto de &gcirc;ia surfaco a&ubreve; tute
      nenio:
      <ekz>
        la nov<tld/>o staris sur la &ccirc;ielo
        <fnt><bib>Fab3</bib>, <lok>Kolo de botelo</lok></fnt>;
      </ekz>
      <ekz>
        li for&jcirc;etis &ccirc;ion kontra&ubreve; kelkaj guta&ccirc;oj da vino, kiuj estos
        for anta&ubreve; la venonta nov<tld/>o
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>, 
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        ar&gcirc;enta son&gcirc;oherbaro /
        kovras la malplenan <tld/>on
        <fnt>
          <aut>F. Garc&iacute;a Lorca, trad. A. Alonso Nu&ntilde;ez</aut>,
          <vrk>Al la urbo Santiago</vrk>,
          <lok><url
          ref="http://esperanto.es:8080/jspui/bitstream/11013/1853/3/Kajeroj%20el%20la%20Sudo%2C%201992%2C%20n.%20014.pdf">
          Kajeroj el la Sudo, 1992 (14)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd></trd>,
    <trd>nov</trd>,
    <trd>novolun&iacute;</trd>,
    <trd>nov&yacute; m&ecaron;s&iacute;c</trd>
  </trdgrp>
  <trd lng="fr">nouvelle <ind>lune</ind></trd>
  <trd lng="fy">nijmoanne</trd>
  <trd lng="id"><ind>bulan</ind> baru</trd>
  <trd lng="it"><ind>luna</ind> nuova</trd>
  <trd lng="nl">nieuwe maan</trd>
  <trdgrp lng="sk">
    <trd>nov</trd>,
    <trd>nov&yacute; mesiac</trd>
  </trdgrp>
  <trd lng="sv">nym&aring;ne</trd>
</drv>

<drv mrk="lun.plen0o">
  <kap>plen<tld/>o,
  <var><kap>plena <tld/>o</kap></var></kap>
  <snc mrk="lun.plen0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Fazo, aspekto de la luno, kiam videblas la plena disko
      de &gcirc;ia surfaco:
      <ekz>
        la plen<tld/>o disvastigis lumon preska&ubreve; tagan
        <fnt><bib>Fab3</bib>, <lok>Supo el kolbasaj 
        bastonetoj</lok></fnt>;
      </ekz>
      <ekz>
        li revenos hejmen je la plen<tld/>o
        <fnt><bib>MT</bib>, <lok>&Sen; 7:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&uacute;pln&ecaron;k</trd>
  <trd lng="fr">pleine <ind>lune</ind></trd>
  <trd lng="id"><ind>bulan</ind> purnama</trd>
  <trdgrp lng="it">
    <trd>luna piena</trd>,
    <trd>plenilunio</trd>
  </trdgrp>
  <trd lng="nl">volle maan</trd>
  <trd lng="pl">n&oacute;w</trd>
  <trdgrp lng="sk">
    <trd>pln&yacute; mesiac</trd>,
    <trd>spln <klr>(Mesiaca)</klr></trd>
  </trdgrp>
  <trd lng="sv">fullm&aring;ne</trd>
  <trd lng="vo">fulamun</trd>
</drv>

<drv mrk="lun.0dauxro">
  <kap><tld/>da&ubreve;ro, <var><kap><tld/>periodo</kap></var></kap>
  <snc mrk="lun.0dauxro.AST">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Tempo de tuta ciklo de la <tld/>aj fazoj,
      proksimume 29,5 tagoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="monat.0o.AST">monato</ref>
    </refgrp>
  </snc>
  <trd lng="br">loariad</trd>
  <trd lng="cs">m&ecaron;s&iacute;&ccaron;n&iacute; f&aacute;ze</trd>
  <trd lng="de">Lunation</trd>
  <trd lng="fr">lunaison</trd>
  <trdgrp lng="hu">
    <trd>holdh&oacute;nap</trd>,
    <trd>lun&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="it">lunazione</trd>
  <trd lng="nl">maanmaand</trd>
  <trdgrp lng="pl">
    <trd>miesi&aogonek;c <ind>ksi&eogonek;&zdot;ycowy</ind></trd>,
    <trd>lunacja</trd>
  </trdgrp>
  <trd lng="ru">
  &c_l;&c_u;&c_n;&c_n;&c_y;&c_j; &c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
  <trd lng="sk">f&aacute;za mesiaca</trd>
</drv>
</art>
