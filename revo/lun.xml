<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lun</rad>/o
</kap>
<drv mrk="lun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc num="1">
    <dif>
      <ref tip="super" cel="astr.0o.1">Astro</ref>,
      kiu estas satelito de la tero kaj rondiras &ccirc;irka&ubreve; &gcirc;i:
      <ekz>fazoj de la <tld/>o;</ekz>
      <ekz><tld/>arko;</ekz>
      <ekz><tld/>kvarono;</ekz>
      <ekz>plen<tld/>o;</ekz>
      <ekz>la nov<tld/>o staris sur la &ccirc;ielo<fnt>Z</fnt>.</ekz>
    </dif>
    <trd lng="en">Moon</trd>
    <trd lng="es">Luna</trd>
    <trd lng="fr">lune</trd>
    <trd lng="he">&lamed;&bet;&nun;&he;</trd>
    <trd lng="hu">Hold</trd>
    <trd lng="nl">maan</trd>
    <trd lng="ru">&c_L;&c_u;&c_n;&c_a;</trd>
    <trd lng="sk">Mesiac</trd>
    <trdgrp lng="zh">
      <trd>&#26376;<klr> [yu&egrave;]</klr></trd>,
      <trd>&#26376;&#20142;<klr> [yu&egrave;liang]</klr></trd>,
      <trd>&#26376;&#29699;<klr> [yu&egrave;qi&uacute;]</klr></trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Nomo donita al la satelitoj de aliaj planedoj ol tero:
      <ekz>la <tld/>oj de Jupitero.</ekz>
    </dif>
    <trd lng="en">moon</trd>
    <trd lng="es">luna</trd>
    <trd lng="fr">lune</trd>
    <trd lng="hu">hold</trd>
    <trd lng="nl">maan</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_u;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sk">mesiac <klr>(nebesk&eacute; teleso)</klr></trd>
  </snc>
  <trd lng="de">Mond</trd>
  <trd lng="he">&yod;&resh;&het;</trd>
  <trd lng="pt">lua</trd>
</drv>

<drv mrk="lun.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz><tld/>a lumo, helo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Mond-</trd>,
     <trd>lunar</trd>
  </trdgrp>
  <trd lng="es">lunar</trd>
  <trd lng="fr">lunaire</trd>
  <trd lng="he">&yod;&resh;&het;&yod;</trd>
  <trd lng="hu">hold-</trd>
  <trd lng="pt">lunar</trd>
  <trd lng="ru">&c_l;&c_u;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">mesa&ccaron;n&yacute;</trd>
</drv>

<drv mrk="lun.0dauxro0periodo">
  <kap><tld/>da&ubreve;ro, <tld/>periodo</kap>
  <snc>
    <dif>
      La sinsekvo de la <tld/>fazoj, komencante de la nov<tld/>o.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>holdh&oacute;nap</trd>,
    <trd>lun&aacute;ci&oacute;</trd></trdgrp>
  <trd lng="nl">maanmaand</trd>
  <trd lng="ru">&c_l;&c_u;&c_n;&c_n;&c_y;&c_j; &c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
</drv>

<drv mrk="lun.0arko">
  <kap><tld/>arko</kap>
  <snc>
    <dif>
      <refgrp tip="sin"><ref cel="kresce.0o">Krescento</ref>.</refgrp>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Mondsichel</trd>,
     <trd>Halbmond</trd>
  </trdgrp>
  <trd lng="fr">croissant<ind> de lune</ind></trd>
  <trd lng="hu">holdsarl&oacute;</trd>
  <trd lng="ru">&c_l;&c_u;&c_n;&c_n;&c_y;&c_j; &c_s;&c_je;&c_r;&c_p;</trd>
</drv>
</art>
