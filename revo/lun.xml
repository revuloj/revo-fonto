        <fnt>
          <aut>Eskil Svane</aut>
          <vrk><url ref="&Monato;/2005/008552.php">Ankora&ubreve; pri EDE</url></vrk>
          <bib>Monato</bib>
          <lok>2005/03, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
