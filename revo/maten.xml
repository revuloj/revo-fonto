<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>maten</rad>/o
</kap>
<drv mrk="maten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Parto de la tago ekde sunlevi&gcirc;o &gcirc;is tagmezo.
    </dif>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>r&aacute;no</trd>,
    <trd>jitro</trd>
  </trdgrp>
  <trd lng="de">Morgen</trd>
  <trd lng="en">morning</trd>
  <trdgrp lng="fr">
    <trd>matin</trd>,
    <trd>matin&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>reggel</trd>,
    <trd>d&eacute;lel&odblac;tt</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>ochtend</trd>,
    <trd>morgen <klr tip="ind">(ochtend)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rano</trd>,
    <trd>poranek</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_r;&c_o;</trd>
  <trd lng="sv">morgon</trd>
  <trd lng="vo">g&ouml;del</trd>
</drv>

<drv mrk="maten.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>o.
    </dif>
    <trdgrp lng="hu">
      <trd>reggel van</trd>,
      <trd>d&eacute;lel&odblac;tt van</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="maten.0rugxo">
  <kap><tld/>ru&gcirc;o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Matena krepusko:
      <ekz>
        &ccirc;e la <tld/>ru&gcirc;o Don Quijote ekrajdis de la
        gastejo<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 4a, p.
        45a</lok></fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="auxror.0o">a&ubreve;roro</ref>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_l;&c_a;&c_k;</trd>
  <trd lng="de">Morgenrot</trd>
  <trdgrp lng="fr">
    <trd>aube<klr> (aurore)</klr></trd>,
    <trd>aurore</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajnalp&iacute;r</trd>,
    <trd>pirkadat</trd>
  </trdgrp>
  <trd lng="nl">ochtendrood</trd>
  <trd lng="pl">zorza <ind>poranna</ind></trd>
  <trd lng="ru">&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja; &c_z;&c_a;&c_r;&c_ja;</trd>
  <trd lng="sv">morgonrodnad</trd>
</drv>
</art>
