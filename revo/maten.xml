<drv mrk="maten.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="maten.0i.KAL">
    <dif>
      Esti <tld/>a tempo:
      <ekz>
        <tld/>as, vesperas &#8208; kaj tago malaperas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_n;&c_ib;&c_c;&c_a; <klr>(&c_ja;&c_k;
  &c_v;&c_y;&c_k;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_k;)</klr></trd>
  <trd lng="ca">ser de mat&iacute;</trd>
  <trd lng="cs">sv&iacute;tat</trd>
  <trd lng="de"><ind>Morgen</ind> werden</trd>
  <trd lng="en">be <ind>morning</ind></trd>
  <trdgrp lng="hu">
    <trd>reggel van</trd>,
    <trd>d&eacute;lel&odblac;tt van</trd>
  </trdgrp>
  <trd lng="ja">&#26397;&#12391;&#12354;&#12427;  <pr>&#12354;&#12373;&#12391;&#12354;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd><ind>morgen</ind> zijn</trd>,
    <trd><ind>ochtend</ind> zijn</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>brie&zcaron;di&tcaron; sa</trd>,
    <trd>svita&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="maten.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="maten.0igxi.KAL">
    <dif>
      Ekesti <tld/>o; <ref tip="super" cel="komenc.0igxi">komenci&gcirc;i</ref>
      <klr>(pri hela tago)</klr>:
      <ekz>
        <tld/>i&gcirc;is, trumpetis ligna korno de la pa&scirc;tisto
        <fnt>
          <aut>A. Gajdar, trad. E. S. Pervertajlo kaj R. Grin&scirc;pan</aut>,
          <vrk><url ref="http://miresperanto.com/biblioteko/timur/04.htm">
          Timur kaj lia Ta&ccirc;mento</url></vrk>,
          <lok>1982</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mateni&gcirc;is, li estis vekita de malla&ubreve;ta ronkado
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <tld/>i&gcirc;is, lia malforteco estis tia, ke li falis sur apogse&gcirc;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam <tld/>i&gcirc;is, kiam la el&ccirc;erpi&gcirc;o de mia forto min dronigis en dormon
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">amanecer</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_d;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_d;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a;
    <ind>&c_s;&c_mol;&c_v;&c_je;&c_t;</ind></trd>
  </trdgrp>
  <trd lng="de">anbrechen <klr>(Tag)</klr></trd>
  <trd lng="en">dawn</trd>
  <trd lng="pt">amanhecer</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_v;&c_i;&c_d;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_i;&c_d;&c_n;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="maten.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="maten.0igxo.KAL">
    <dif>
      Tempo, kiam esti&gcirc;as <tld/>o, tempo de
      matena <ref tip="vid" cel="krepus.0o.KAL">krepusko</ref>:
      <ekz>
        anta&ubreve; la <tld/>i&gcirc;o ni lin mortigos
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lampo restis neestingita &gcirc;is la <tld/>i&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        longaj sociaj noktoj &gcirc;is preska&ubreve; <tld/>i&gcirc;o
        <fnt>
          <aut>A. Wandel</aut>,
          <vrk><url ref="http://www.internacia-festivalo.de/21-rap-Wandel.html">
          21-a Internacia Festivalo en Magdeburg</url></vrk>,
          <lok>[2006?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_n;&c_a;&c_k;</trd>
  <trd lng="de">Tagesanbruch</trd>
  <trd lng="en">dawn</trd>
  <trd lng="fa">&a_s;&f_p;&f_y;&a_d;&a_h;&#8204;&a_d;&a_m;</trd>
  <trd lng="fr">aube</trd>
  <trd lng="es">amanecer</trd>
