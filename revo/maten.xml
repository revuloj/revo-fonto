<drv mrk="maten.0rugxo">
  <kap><tld/>ru&gcirc;o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Matena tempo, kiam la suno ru&gcirc;igas la &ccirc;ielon;
      <ref tip="sin" cel="auxror.0o">a&ubreve;roro</ref>:
      <ekz>
        balda&ubreve; la <tld/>ru&gcirc;o ver&scirc;os sian koloron sur la maron
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili forliberigis &scirc;in, kiam levi&gcirc;is la <tld/>ru&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 19:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        jam mi ekvidas <tld/>ru&gcirc;on, la tagi&gcirc;on de nova glora epoko
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Fernando <klr>[estis]</klr> refari&gcirc;inta tia, kia li estis en la <tld/>ru&gcirc;o de
        iliaj amrilatoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lev.sun0igxo">sunlevi&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="br">
    <trd>goulo&ugrave;-deiz</trd>,
    <trd>tarzh-deiz</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>alba</trd>,
    <trd>aurora</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Morgenrot</trd>,
    <trd>Morgenr&ouml;te</trd>
  </trdgrp>
  <trd lng="es">aurora</trd>
  <trd lng="fr">aurore</trd>
  <trdgrp lng="hu">
    <trd>hajnalp&iacute;r</trd>,
    <trd>pirkadat</trd>
  </trdgrp>
  <trd lng="id">fajar</trd>
  <trd lng="ja">&#x671D;&#x713C;&#x3051;  <pr>&#x3042;&#x3055;&#x3084;&#x3051;</pr></trd>
  <trd lng="nl">ochtendrood</trd>
  <trd lng="pl">zorza <ind>poranna</ind></trd>
  <trd lng="ru">&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja;
&c_z;&c_a;&c_r;&c_ja;</trd>
  <trd lng="sv">morgonrodnad</trd>
  <trd lng="uk">(&c_r;&c_a;&c_n;&c_k;&c_o;&c_v;&c_a;) &c_z;&c_o;&c_r;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x671D;&#x971E; <pr>zh&#x101;oxi&#xE1;</pr></trd>,
