    <trd>&#x660E;&#x3051;&#x65B9;  <pr>&#x3042;&#x3051;&#x304C;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_v;&c_ib;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x65E6; <pr>d&#xE0;n</pr></trd>,
    <trd>&#x9ECE;&#x660E;&#x65F6;&#x5206; <pr>l&#xED;m&#xED;ngsh&#xED;f&#x113;n</pr></trd>,
    <trd>&#x9ECE;&#x660E;&#x6642;&#x5206; <pr>l&#xED;m&#xED;ngsh&#xED;f&#x113;n</pr></trd>,
    <trd>&#x9ECE;&#x660E; <pr>l&#xED;m&#xED;ng</pr></trd>,
    <trd>&#x6612; <pr>h&#x16B;</pr></trd>,
    <trd>&#x5929;&#x4EAE; <pr>ti&#x101;nli&#xE0;ng</pr></trd>,
