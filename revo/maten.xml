<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>maten</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="maten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="maten.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Parto de tago
      (de <ref tip="malprt" cel="diurn.0o">diurno</ref>)
      ekde sunlevi&gcirc;o &gcirc;is tagmezo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>beure</trd>,
    <trd>mintin</trd>
  </trdgrp>
  <trd lng="ca">mat&iacute;</trd>
  <trdgrp lng="cs">
    <trd>r&aacute;no</trd>,
    <trd>jitro</trd>
  </trdgrp>
  <trd lng="de">Morgen</trd>
  <trd lng="en">morning</trd>
  <trd lng="es">ma&ntilde;ana <klr tip="amb">(parte del 
d&iacute;a)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_m;&a_d;&a_A;&a_d;</trd>,
    <trd>&a_S;&a_b;&a_H;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>matin</trd>,
    <trd>matin&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>reggel</trd>,
    <trd>d&eacute;lel&odblac;tt</trd>
  </trdgrp>
  <trd lng="id">pagi</trd>
  <trd lng="ku">sibe</trd>
  <trdgrp lng="nl">
    <trd>ochtend</trd>,
    <trd>morgen <klr tip="ind">(ochtend)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rano</trd>,
    <trd>poranek</trd>
  </trdgrp>
  <trd lng="pt">manh&atilde;</trd>
  <trdgrp lng="ro">
    <trd>de diminea&tcedil;&abreve;</trd>,
    <trd>devreme</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_r;&c_o;</trd>
  <trd lng="sk">ráno</trd>
  <trd lng="sv">morgon</trd>
  <trd lng="vo">g&ouml;del</trd>
  <trdgrp lng="zh">
    <trd>&#26216; [<ind>ch&eacute;n</ind>]</trd>
    <trd>&#26089;&#19978; [<ind>z&#462;o shang</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="maten.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="maten.0i.KAL">
    <dif>
      Esti <tld/>a tempo.
    </dif>
  </snc>
  <trd lng="ca">ser de mat&iacute;</trd>
  <trd lng="cs">svítat</trd>
  <trdgrp lng="hu">
    <trd>reggel van</trd>,
    <trd>d&eacute;lel&odblac;tt van</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>morgen</ind> zijn</trd>,
    <trd><ind>ochtend</ind> zijn</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>brieždiť sa</trd>,
    <trd>svitať</trd>
  </trdgrp>
</drv>

<drv mrk="maten.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="maten.0igxi.KAL">
    <dif>
      I&gcirc;i <tld/>o, komenci&gcirc;i
      <klr>(pri hela tago)</klr>:
      <ekz>
        <tld/>i&gcirc;is; trumpetis ligna korno de la pa&scirc;tisto
        <fnt>
          <aut>A. Gajdar, trad. E. S. Pervertajlo kaj R. Grin&scirc;pan</aut>,
          <vrk><url
          ref="http://miresperanto.com/biblioteko/timur/04.htm">
          Timur kaj lia Ta&ccirc;mento</url></vrk>,
          <lok>1982</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">amanecer</trd>
  </snc>
</drv>

<drv mrk="maten.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="maten.0igxo.KAL">
    <dif>
      Tempo, kiam esti&gcirc;as <tld/>o, tempo de
      matena <ref tip="vid" cel="krepus.0o.KAL">krepusko</ref>:
      <ekz>
        longaj sociaj noktoj &gcirc;is preska&ubreve; <tld/>i&gcirc;o
        <fnt>
          <aut>A. Wandel</aut>,
          <vrk><url
          ref="http://www.internacia-festivalo.de/21-rap-Wandel.html">
          21-a Internacia Festivalo en Magdeburg</url></vrk>,
          <lok>[2006?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fa">&a_s;&f_p;&f_y;&a_d;&a_h;&zwnj;&a_d;&a_m;</trd>
  <trd lng="fr">aube</trd>
  <trd lng="es">amanecer</trd>
</drv>

<drv mrk="maten.0rugxo">
  <kap><tld/>ru&gcirc;o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Matena tempo, kiam la suno ru&gcirc;igas la &ccirc;ielon;
      <ref tip="sin" cel="auxror.0o">a&ubreve;roro</ref>:
      <ekz>
        &ccirc;e la <tld/>ru&gcirc;o Don Quijote ekrajdis de la
        gastejo<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 4a, p.
        45a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_l;&c_a;&c_k;</trd>  
  <trdgrp lng="br">
    <trd>goulo&ugrave;-deiz</trd>,
    <trd>tarzh-deiz</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>alba</trd>,
    <trd>aurora</trd>
  </trdgrp>
  <trd lng="de">Morgenrot</trd>
  <trd lng="es">aurora</trd>
  <trd lng="fr">aurore</trd>
  <trdgrp lng="hu">
    <trd>hajnalp&iacute;r</trd>,
    <trd>pirkadat</trd>
  </trdgrp>
  <trd lng="id">fajar</trd>
  <trd lng="nl">ochtendrood</trd>
  <trd lng="pl">zorza <ind>poranna</ind></trd>
  <trd lng="ru">&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja; 
&c_z;&c_a;&c_r;&c_ja;</trd>
  <trd lng="sv">morgonrodnad</trd>
</drv>
</art>
