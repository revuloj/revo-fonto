<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: istri.xml,v 1.1 2023/08/03 15:24:27 afido Exp $">
    <kap>
      <rad>Istri</rad>/o
    </kap>

<drv mrk="istri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="istri.0o.duoninsulo">
    <dif>
      <ref tip="super" cel="insul.duon0o">Duoninsulo</ref> 100&#8239;km oriente de Venecio,
      &ccirc;e la kontra&ubreve;a bordo de <ref cel="adriatik.0o">Adriatiko</ref>:
      <ekz>
        en <tld/>o la limon difinas rivero <nom>Dragonja</nom>, sed iam oni fosis kanalon
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="https://www.monato.be/2004/008454.html"
              >Malfermi pandoran skatolon</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilirianoj regis longatempe Adriatikon de <tld/>o &gcirc;is Ambrakio, anta&ubreve; la tri
        iliriaj-romiaj militoj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2007/009244.php"
              >Navige tra prahistorio</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Istrien</trd>
    <trd lng="en">Istria</trd>
    <trd lng="fr">Istrie</trd>
  </snc>
</drv>

<drv mrk="istri.0a">
  <kap>
    <tld lit="i"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        oratoro plenvo&ccirc;a, lar&gcirc;vokala, kun ege pla&ccirc;a <tld lit="i"/>a prononcmaniero
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2004/008066.html"
              >Retrorigardo al intensa vivo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        pitoreska <tld lit="i"/>a urbeto <nom>Motovun</nom> (Kroatio) <klr>[&#8230;]</klr> gastigos Konferencon de
        <nom>OSIEK</nom> kaj, paralele, la 6ajn Somerajn Kursojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 6 (68)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">istrisch</trd>
</drv>

  </art>

