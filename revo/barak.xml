<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: barak.xml,v 1.17 2017/05/30 16:10:21 revo Exp $">
  <kap>
    <ofc>2</ofc>
    <rad>barak</rad>/o
  </kap>
  <drv mrk="barak.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Provizora konstruo, plej ofte el ligno, por lo&gcirc;igi
        soldatojn, laboristojn, malsanulojn, rifu&gcirc;intojn:
        <ekz>
          en longa vico mi staras kun la tuta familio anta&ubreve;
          <tld/>o
          <fnt>
          <aut>Stefan Maul</aut>,
          <vrk>El verva vivo &jcirc;urnalista</vrk>,
          <lok>p. 18</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="bud.0o">budo</ref>,
        <ref cel="kaban.0o">kabano</ref>,
        <ref cel="kajut.0o">kajuto</ref>.
      </refgrp>
      <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_k;</trd>
      <trd lng="de">Baracke</trd>
      <trd lng="es">barraca</trd>
      <trd lng="en">barracks</trd>
      <trd lng="fr">baraque <klr tip="ind">(construction
      provisoire)</klr></trd>
      <trd lng="hu">barakk</trd>
      <trd lng="nl">barak</trd>
      <trd lng="pl">barak</trd>
      <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_k;</trd>
      <trd lng="sv">barack</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>bar&aacute;k</trd>,
    <trd>provizorn&iacute; staven&iacute;</trd>,
    <trd>ubytovna</trd>
  </trdgrp>
  <trd lng="sk">barak</trd>
    <trd lng="th">&#3648;&#3614;&#3636;&#3591;&#3629;&#3634;&#3624;&#3633;&#3618;&#3594;&#3633;&#3656;&#3623;&#3588;&#3619;&#3634;&#3623;</trd>
</drv>
</art>
