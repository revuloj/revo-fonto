<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filozo1.xml,v 1.31 2023/11/09 20:48:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>filozofi</rad>/o
</kap>
<drv mrk="filozo1.0o">
  <kap><tld/>o</kap>
  <snc mrk="filozo1.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref> pri la esencaj
      plej &gcirc;eneralaj principoj kaj ka&ubreve;zoj de la estado.
    </dif>
    <trd lng="es">filosof&iacute;a</trd>
    <trd lng="pl">filozofia</trd>
  </snc>
  <snc mrk="filozo1.0o.sciencaro">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Tuto de &ccirc;iuj natursciencoj (fiziko, &hcirc;emio, zoologio,
      botaniko...)
      kaj kultursciencoj (historio, filologio, psikologio...).
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_z;&c_o;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="bo">&#xF58;&#xF5A;&#xF53;&#xF0B;&#xF49;&#xF72;&#xF51;&#xF0B;</trd>
  <trd lng="cs">filozofie</trd>
  <trd lng="de">Philosophie</trd>
  <trd lng="en">philosophy</trd>
  <trd lng="fr">philosophie</trd>
  <trdgrp lng="id">
    <trd>falsafah</trd>,
    <trd>filosofi</trd>,
    <trd>filsafat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x54F2;&#x5B66;  <pr>&#x3066;&#x3064;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x3082;&#x306E;&#x306E;&#x898B;&#x65B9;  <pr>&#x3082;&#x306E;&#x306E;&#x307F;&#x304B;&#x305F;</pr></trd>,
    <trd>&#x8003;&#x3048;&#x65B9;  <pr>&#x304B;&#x3093;&#x304C;&#x3048;&#x304B;&#x305F;</pr></trd>,
    <trd>&#x4EBA;&#x751F;&#x89B3;  <pr>&#x3058;&#x3093;&#x305B;&#x3044;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x4E16;&#x754C;&#x89B3;  <pr>&#x305B;&#x304B;&#x3044;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">filosofie</trd>
  <trd lng="pt">filosofia</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">filozofia</trd>
  <trd lng="sv">filosofi</trd>
  <trd lng="uk">&#x444;&#x456;&#x43B;&#x43E;&#x441;&#x43E;&#x444;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x54F2;&#x5B66;</trd>
</drv>

<drv mrk="filozo1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="filozo1.0i.FIL">
    <dif>
      Esplori kaj pritrakti la <tld/>on;
      <ref tip="sin" cel="filozo.0i.science">filozofi</ref>:
      <ekz>
        vi <tld/>as, post kiom da tempo ni komencos iri kvarpiede?
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>.
      </ekz>
    </dif>   
  </snc>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">filozofovat</trd>
  <trd lng="de">philosophieren</trd>
  <trdgrp lng="en">
    <trd>to philosophize <klr>(usone)</klr></trd>,
    <trd>philosophise <klr>(brite)</klr></trd>
  </trdgrp>
  <trd lng="es">filosofar</trd>
  <trd lng="fr">philosopher</trd>
  <trdgrp lng="id">
    <trd>ber<ind>falsafah</ind></trd>,
    <trd>ber<ind>filsafat</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x54F2;&#x5B66;&#x3059;&#x308B;  <pr>&#x3066;&#x3064;&#x304C;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x601D;&#x7D22;&#x3059;&#x308B;  <pr>&#x3057;&#x3055;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">filosoferen</trd>
  <trdgrp lng="ru">        
  <trd>&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <ind>&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_je;&c_j;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>filozofova&tcaron;</trd>,
    <trd>prem&yacute;&scaron;&lcaron;a&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">filosofera</trd>
  <trd lng="zh">&#x8FDB;&#x884C;&#x54F2;&#x5B66;&#x63A2;&#x8BA8; <pr>j&#xEC;nx&#xED;ngzh&#xE9;xu&#xE9;t&#xE0;nt&#x1CE;o</pr></trd>
</drv>
</art>
