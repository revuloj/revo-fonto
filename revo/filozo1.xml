<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>filozofi</rad>/o
</kap>
<drv mrk="filozo1.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Scienco pri la esencaj plej &gcirc;eneralaj principoj k ka&ubreve;zoj de la estado.
    </dif>
    <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  </snc>
  <snc num="2">
    <uzo tip="klr">(en germanlingvaj landoj)</uzo>
    <dif>
      Tuto de &ccirc;iuj natursciencoj (fiziko, &hcirc;emio, zoologio, botaniko kc)
      k moralaj sciencoj (historio, filologio, psikologio kc). 
    </dif>
  </snc>
</drv>
<drv mrk="filozo1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="filozo.0i">filozofi</ref>.  
    </dif>
  </snc>
</drv>
</art>
