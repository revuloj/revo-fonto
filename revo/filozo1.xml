<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>filozofi</rad>/o
</kap>
<drv mrk="filozo1.0o">
  <kap><tld/>o</kap>
  <snc mrk="filozo1.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako"
      lst="voko:sciencoj">Scienco</ref> pri la esencaj
      plej
      &gcirc;eneralaj principoj kaj ka&ubreve;zoj de la estado.
    </dif>
    <trd lng="es">filosof&iacute;a</trd>
  </snc>
  <snc mrk="filozo1.0o.sciencaro">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Tuto de &ccirc;iuj natursciencoj (fiziko, &hcirc;emio, zoologio,
      botaniko...)
      kaj kultursciencoj (historio, filologio, psikologio...).
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_z;&c_o;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="de">Philosophie</trd>
  <trd lng="en">philosophy</trd>
  <trd lng="fr">philosophie</trd>
  <trdgrp lng="id">
    <trd>falsafah</trd>,
    <trd>filosofi</trd>,
    <trd>filsafat</trd>
  </trdgrp>
  <trd lng="nl">filosofie</trd>
  <trd lng="pt">filosofia</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">filozofia</trd>
  <trd lng="sv">filosofi</trd>
</drv>

<drv mrk="filozo1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="filozo1.0i.FIL">
    <dif>
      Esplori kaj pritrakti la <tld/>on.
    </dif>
    <ref tip="sin" cel="filozo.0i.science">filozofi</ref>
  </snc>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>
  <trd lng="de">philosophieren</trd>
  <trd lng="es">filosofar</trd>
  <trd lng="fr">philosopher</trd>
  <trdgrp lng="id">
    <trd>[<ind>falsafah</ind>] berfalsafah</trd>,
    <trd>[<ind>filsafat</ind>] berfilsafat</trd>
  </trdgrp>
  <trd lng="nl">filosoferen</trd>
  <trdgrp lng="ru">         <trd>&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;&c_i;&c_je;&c_j;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>filozofovať</trd>,
    <trd>premýšľať</trd>
  </trdgrp>
  <trd lng="sv">filosofera</trd>
</drv>
</art>
