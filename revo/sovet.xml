<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sovet</rad>/o
</kap>
<drv mrk="sovet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Revolucia popolkonsilio memregada en
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>
      dum la revolucioj de 1905a kaj 1917a jaroj,
      kiun la <ref tip="vid" cel="bolsxe1.0o">bol&scirc;evistoj</ref>
      rigardis novtipa formo de &scirc;tatregado.
    </dif>
  </snc>
  <snc>
    <dif>
      En la <ref tip="vid" cel="sovet.S0unio">Unio de la Sovetaj
      Socialismaj Respublikoj</ref> (USSR),
      deputitaro formanta superan organon, kiu gvidas
      &scirc;taton, respublikon, gubernion a&ubreve; distrikton:
      <ekz>
        rebaloto al <tld/>oj <fnt><aut>V. Varankin</aut>:
        <vrk>Metropoliteno</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La vorto devenas de la rusa vorto &leftquot;&c_s;&c_o;&c_v;&c_je;&c_t;&rightquot; (konsilio).
  </rim>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_t;</trd>
  <trd lng="de">Sowjet</trd>
  <trdgrp lng="en">
    <trd>soviet</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soviet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szovjet</trd>,
    <trd>tan&aacute;cs</trd>
  </trdgrp>
  <trd lng="nl">sovjet</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;<klr> (&c_o;&c_r;&c_g;&c_a;&c_n; &c_v;&c_l;&c_a;&c_s;&c_t;&c_i; &c_v; &c_S;&c_S;&c_S;&c_R;)</klr></trd>
</drv>

<drv mrk="sovet.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="sovet.S0io">
  <kap><tld lit="S"/>io</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <ref tip="dif" cel="sovet.UniodeS0ajSocialismajRespublikoj">Unio de
  <tld lit="S"/>aj Socialismaj Respublikoj</ref>
  <ekz>
    tiel restis &gcirc;is la atako de Germanio kontra&ubreve; <tld lit="S"/>io, la 22an
    de junio 1941<fnt><aut>Zofia Banet-Fornalowa</aut>: <vrk>Kadi&scirc; por
    Jedwabne</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 1a, p.
    18a</lok></fnt>.
  </ekz>
  <ekz>
    <tld lit="S"/>io estas laborista &scirc;tato. <fnt><aut>V. Varankin</aut>:
    <vrk>Metropoliteno</vrk></fnt>.
  </ekz>
  <trd lng="be">&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib; &c_S;&c_a;&c_ju;&c_z;</trd>
  <trdgrp lng="ru">
    <trd>&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_S;&c_o;&c_ju;&c_z;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_a; &c_S;&c_o;&c_v;&c_je;&c_t;&c_o;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="sovet.S0unio">
  <kap><tld lit="S"/>unio</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <ref tip="dif" cel="sovet.UniodeS0ajSocialismajRespublikoj">Unio de
  <tld lit="S"/>aj Socialismaj Respublikoj</ref>
  <ekz>
    Lin delonge interesas Sovetunio. <fnt><aut>V. Varankin</aut>:
    <vrk>Metropoliteno</vrk></fnt>.
  </ekz>
  <trd lng="be">&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib; &c_S;&c_a;&c_ju;&c_z;</trd>
  <trd lng="de">Sowjetunion</trd>
  <trd lng="en">Soviet Union</trd>
  <trd lng="fr">Union sovi&eacute;tique</trd>
  <trd lng="hu">Szovjetuni&oacute;</trd>
  <trd lng="nl">Sovjetunie</trd>
  <trd lng="ru">&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_S;&c_o;&c_ju;&c_z;</trd>
  <trd lng="tr">Sovyetler Birli&gbreve;i</trd>
</drv>

<drv mrk="sovet.UniodeS0ajSocialismajRespublikoj">
  <kap>Unio de <tld lit="S"/>aj Socialismaj Respublikoj</kap>
  <mlg>USSR</mlg>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Federacia &scirc;tato, ekzistinta de 1922 &gcirc;is 1991 sur plej granda
      parto de la teritorio de eksa
      <ref tip="vid" cel="rus.R0aImperio">Ruslanda Imperio</ref>,
      kiu, en 1990, konsistis el
      <ref tip="prt" cel="rus.R0aFederacio">Ruslanda Federacio</ref>,
      Ukrainujo, Belorusujo, Moldavujo, Litovujo, Latvujo, Estonujo,
      Kartvelujo, Armenujo, Azerbaj&gcirc;ano, Kaza&hcirc;ujo, Uzbekujo,
      Turkmenujo, Ta&gcirc;ikujo, Kirgizujo:
      <ekz>
        Mi rakontas al ili pri konstruado de l' socialismo en USSR kaj oni
        a&ubreve;skultas min atenteme. <fnt><aut>V. Varankin</aut>:
        <vrk>Metropoliteno</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      <ref cel="azi.A0o">Azio</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib; &c_S;&c_a;&c_ju;&c_z;</trd>,
    <trd>&c_S;&c_a;&c_ju;&c_z; &c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;&c_h; &c_S;&c_a;&c_c;&c_y;&c_ja;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;&c_h; &c_R;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_S;&c_S;&c_S;&c_R;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sowjetunion</trd>,
    <trd>Union der sozialistischen Sowjetrepubliken</trd>,
    <trd>UdSSR</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Union of Soviet Socialist Republics</trd>,
    <trd>Soviet Union</trd>,
    <trd>USSR</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Union des r&eacute;publiques socialistes sovi&eacute;tiques</trd>,
    <trd>Union sovi&eacute;tique</trd>,
    <trd>URSS</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Szovjetuni&oacute;</trd>,
    <trd>Szovjet Szocialista K&ouml;zt&aacute;rsas&aacute;gok Sz&ouml;vets&eacute;ge</trd>,
    <trd>SZU</trd>
  </trdgrp>
  <trd lng="nl">Sovjetunie</trd>
  <trdgrp lng="ru">
    <trd>&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_S;&c_o;&c_ju;&c_z;</trd>
    <trd>&c_S;&c_o;&c_ju;&c_z; &c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_h; &c_S;&c_o;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_h;
    &c_R;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_S;&c_S;&c_S;&c_R;</trd>,
  </trdgrp>
  <trdgrp lng="tr">
    <trd>Sovyetler Birli&gbreve;i</trd>,
    <trd>Sovyet Sosyalist Cumhuriyetler Birli&gbreve;i</trd>,
    <trd>SSCB</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33487;&#32500;&#22467;</trd>,
    <trd><klr>tr. </klr>&#34311;&#32173;&#22467;<klr> [S&#363;w&eacute;i'&#257;i]</klr></trd>,
    <trd>&#33487;</trd>,
    <trd><klr>tr. </klr>&#34311;<klr> [s&#363;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sovet.0ia">
  <kap><tld/>ia</kap>
  <snc>
    <dif>
      Rilata al <tld lit="S"/>io.
      <ekz>
        Ili havas &ccirc;i tie pli, ol cent leterojn el USSR, po&scirc;tkartojn,
        amason da fotoj, <tld/>iajn librojn kaj gazetojn.
        <fnt><aut>V. Varankin</aut>: <vrk>Metropoliteno</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
