<vortaro>
<art mrk="$Id: sovet.xml,v 1.41 2024/09/30 18:35:44 revo Exp $">
<kap><ofc>10</ofc>
  <rad>sovet</rad>/o
</kap>
<drv mrk="sovet.0o">
  <kap><ofc>10</ofc> <tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Revolucia popolkonsilio memregada en
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>
      dum la revolucioj de 1905a kaj 1917a jaroj,
      kiun la <ref tip="vid" cel="bolsxe1.0o">bol&scirc;evistoj</ref>
      rigardis novtipa formo de &scirc;tatregado:
      <ekz>
        la 1a de marto de 1917, la <tld lit="S"/>o de Petrogrado aperigis ties
        Ordonon Numero 1a, kiu instigis la militistojn obei ties ordonojn anstata&ubreve; tiujn de la
        provizora registaro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=301170">Februara revolucio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      En la <ref tip="vid" cel="sovet.S0unio">Unio de la Sovetaj
      Socialismaj Respublikoj</ref> (USSR),
      deputitaro formanta superan organon, kiu gvidas
      &scirc;taton, respublikon, gubernion a&ubreve; distrikton:
      <ekz>
        rebaloto al <tld/>oj
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        la du&ccirc;ambra parlamento estis anstata&ubreve;igita fare de la supera
        <tld/>o de Estona <nom>SSR</nom> (25-a de a&ubreve;gusto 1940 &gcirc;is 1990)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=322725">Parlamento de Estonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La vorto devenas de la rusa vorto
    &leftquot;<nac>&c_s;&c_o;&c_v;&c_je;&c_t;</nac>&rightquot; (konsilio).
  </rim>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_t;</trd>
  <trd lng="de">Sowjet</trd>
  <trdgrp lng="en">
    <trd>soviet</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soviet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szovjet</trd>,
    <trd>tan&aacute;cs</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12477;&#12499;&#12456;&#12488;</trd>,
    <trd>&#21172;&#36786;&#35413;&#35696;&#20250;
    <pr>&#12429;&#12358;&#12398;&#12358;&#12402;&#12423;&#12358;&#12366;&#12363;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">sovjet</trd>
  <trdgrp lng="pt">
   <trd>soviete</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;<klr>
  (&c_o;&c_r;&c_g;&c_a;&c_n; &c_v;&c_l;&c_a;&c_s;&c_t;&c_i; &c_v;
  &c_S;&c_S;&c_S;&c_R;)</klr></trd>
  <trd lng="sk">soviet</trd>
  <trd lng="cs">sov&ecaron;t</trd>
  <trd lng="uk">&c_r;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#33487;&#32500;&#22467; <pr>s&umacron;w&eacute;i&amacron;i</pr></trd>,
    <trd>&#34311;&#32173;&#22467; <pr>s&umacron;w&eacute;i&amacron;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sovet.0a">
  <kap><ofc>10</ofc><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        dum la <tld/>a regado estis pli simple konstrui aparatojn, &ccirc;ar tiam estis pli facile akiri
        la necesajn partojn
        <fnt>
       <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006877.php">Nekutimaj transportiloj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la <tld/>a tempo estis milicistoj, kiuj arigis la ebriulojn kaj ilin akompanis al la
        sobrigejo
        <fnt>
          <aut>Antanas Grincevi&ccaron;ius</aut>
            <vrk><url ref="&Monato;/2000/007066.php">Sobriga propono</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 15</lok>
          </fnt>;
      </ekz>
      <ekz>
        dum <tld/>a okupacio Litovio perdis &ccirc;irka&ubreve; 800&nbsp;000 homojn
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
            <vrk><url ref="&Monato;/2000/007073.php">&Ccirc;u finfine kompenso?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 7</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>sov&ecaron;tov&yacute;</trd>,
    <trd>t&yacute;kaj&iacute;c&iacute; se sov&ecaron;tu</trd>
  </trdgrp>
  <trd lng="de">sowjetisch</trd>
  <trd lng="fr">sovi&eacute;tique</trd>
  <trd lng="ja">&#12477;&#12499;&#12456;&#12488;&#12398;  <pr>&#12381;&#12403;&#12360;&#12392;&#12398;</pr></trd>
  <trdgrp lng="pl">
    <trd>radziecki</trd>,
    <trd>sowiecki<klr> (pot., obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">sovietsky</trd>
  <trd lng="uk">&c_r;&c_a;&c_d;&c_ja;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#33487;&#32852; <pr>s&umacron;li&aacute;n</pr></trd>,
    <trd>&#34311;&#32879; <pr>s&umacron;li&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sovet.S0unio">
  <kap>
    <tld lit="S"/>unio,
    <var><kap><ofc>10</ofc><tld lit="S"/>-Unio</kap></var>,
    <var><kap><ofc>10</ofc><tld lit="S"/>a Unio</kap></var></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <dif>
    <ref tip="dif" cel="sovet.UniodeS0ajSocialismajRespublikoj">Unio de
    <tld lit="S"/>aj Socialismaj Respublikoj</ref>:
      <ekz>
        lin delonge interesas <tld lit="S"/>unio
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        superpotencoj kiaj Usono kaj <tld lit="S"/>unio ne sukcesis venki en Vjetnamio a&ubreve; en Afganio
       <fnt>
       <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2000/006953.php">Bezonata: nova &leftquot;fremdula
          legio&#8221;</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        fine de a&ubreve;gusto 2010 Moldavio festis 19 jarojn da sendependeco post la kolapso de <tld
        lit="S"/>unio
        <fnt>
          <bib>Monato</bib>
          <aut>Pan Gavrilan</aut>
          <vrk><url ref="&Monato;/2010/010201.php">En la lando de la flavaj bicikloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<ref tip="sin" cel="soveti.0o">Sovetio</ref>
  <trd lng="be">&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;
  &c_S;&c_a;&c_ju;&c_z;</trd>
  <trd lng="de">Sowjetunion</trd>
  <trd lng="en">Soviet Union</trd>
  <trd lng="fr">Union sovi&eacute;tique</trd>
  <trd lng="hu">Szovjetuni&oacute;</trd>
  <trd lng="nl">Sovjetunie</trd>
  <trdgrp lng="pl">
    <trd>Zwi&aogonek;zek Socjalistycznych Republik Radzieckich</trd>,
    <trd>Sowiety<klr> (pot., obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;
  &c_S;&c_o;&c_ju;&c_z;</trd>
  <trd lng="tr">Sovyetler Birli&gbreve;i</trd>
  <trdgrp lng="zh">
    <trd>&#33487;&#32852; <pr>S&umacron;li&aacute;n</pr></trd>,
    <trd>&#34311;&#32879; <pr>S&umacron;li&aacute;n</pr></trd>,
    <trd>&#33487;&#32852; <pr>s&umacron;li&aacute;n</pr></trd>,
    <trd>&#34311;&#32879; <pr>s&umacron;li&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sovet.UniodeS0ajSocialismajRespublikoj">
  <kap>Unio de <tld lit="S"/>aj Socialismaj Respublikoj</kap>
  <mlg>USSR</mlg>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
    <dif>
      Federacia &scirc;tato, ekzistinta de 1922 &gcirc;is 1991 sur plej
      granda
      parto de la teritorio de eksa
      <ref tip="vid" cel="imperi.Rusa0o">Ruslanda Imperio</ref>,
      kiu, en 1990, konsistis el
      <ref tip="prt" cel="rus.R0aFederacio">Ruslanda Federacio</ref>,
      Ukrainujo, Belorusujo, Moldavujo, Litovujo, Latvujo, Estonujo,
      Kartvelujo, Armenujo, Azerbaj&gcirc;ano, Kaza&hcirc;ujo, Uzbekujo,
      Turkmenujo, Ta&gcirc;ikujo, Kirgizujo:
      <ekz>
        Mi rakontas al ili pri konstruado de l' socialismo en USSR kaj
        oni a&ubreve;skultas min atenteme
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        nuntempe popolistoj en Rusio &scirc;atas malbeni la nomojn de la tiutempaj
        &leftquot;junaj reformistoj&#8221;, kaj kulpigi ilin, kune kun <nom>Mi&hcirc;ail Gorba&ccirc;ov</nom>, pri
        la kra&scirc;o de la imperio, kaj malanta&ubreve; ili, kompreneble, staris la malamikaj
        okcidentaj landoj, kiuj konspiris por pereigi la karmemoran Union de Sovetiaj Socialismaj
        Respublikoj
        <fnt>
          <bib>Monato</bib>
          <aut>Kalle Kniivil&auml;</aut>
          <vrk><url ref="&Monato;/2007/009159.php">15 jarojn post la pereo de la imperio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      <ref cel="azi.A0o">Azio</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;
    &c_S;&c_a;&c_ju;&c_z;</trd>,
    <trd>&c_S;&c_a;&c_ju;&c_z;
    &c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;&c_h;
    &c_S;&c_a;&c_c;&c_y;&c_ja;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;&c_h;
    &c_R;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_S;&c_S;&c_S;&c_R;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sowjetunion</trd>,
    <trd>Union der sozialistischen Sowjetrepubliken</trd>,
    <trd>UdSSR</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Union of Soviet Socialist Republics</trd>,
    <trd>Soviet Union</trd>,
    <trd>USSR</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Union des r&eacute;publiques socialistes
    sovi&eacute;tiques</trd>,
    <trd>Union sovi&eacute;tique</trd>,
    <trd>URSS</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Szovjetuni&oacute;</trd>,
    <trd>Szovjet Szocialista K&ouml;zt&aacute;rsas&aacute;gok
    Sz&ouml;vets&eacute;ge</trd>,
    <trd>SZU</trd>
  </trdgrp>
  <trd lng="nl">Sovjetunie</trd>
  <trdgrp lng="pt">
   <trd>Uni&atilde;o das Rep&uacute;blicas Socialistas
   Sovi&eacute;ticas</trd>,
   <trd>URSS</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Zwi&aogonek;zek Socjalistycznych Republik Radzieckich</trd>,
    <trd>Sowiety<klr> (pot., obra&zacute;l.)</klr></trd>,
    <trd>ZSRR</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;
    &c_S;&c_o;&c_ju;&c_z;</trd>,
    <trd>&c_S;&c_o;&c_ju;&c_z;
    &c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_h;
    &c_S;&c_o;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_h;
    &c_R;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_S;&c_S;&c_S;&c_R;</trd>,
  </trdgrp>
  <trdgrp lng="tr">
    <trd>Sovyetler Birli&gbreve;i</trd>,
    <trd>Sovyet Sosyalist Cumhuriyetler Birli&gbreve;i</trd>,
    <trd>SSCB</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33487;&#32500;&#22467;</trd>,
    <trd><klr>tr. </klr>&#34311;&#32173;&#22467;<klr>
    [S&umacron;w&eacute;i'&amacron;i]</klr></trd>,
    <trd>&#33487;</trd>,
    <trd><klr>tr. </klr>&#34311;<klr> [s&umacron;]</klr></trd>
  </trdgrp>
</drv>

