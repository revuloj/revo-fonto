<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skrofol</rad>/o
</kap>
<drv mrk="skrofo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Formo de tuberkulozo karakterizata per &scirc;velo de limfaj glandoj kaj
      inflamo de okuloj kaj artikoj.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_r;&c_o;&c_f;&c_u;&c_l;</trd>
  <trd lng="de">Skrofel</trd>
  <trd lng="nl">scrofulose</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_o;&c_f;&c_u;&c_l;&c_jo;&c_z;</trd>,
    <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_u;&c_h;&c_a;</trd>
  </trdgrp>
</drv>
</art>
