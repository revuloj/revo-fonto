<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ananas</rad>/o
</kap>
<drv mrk="ananas.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       &Scirc;ajnfrukto de <tld/>ujo.
     </dif>
   </snc>
   <trd lng="de">Ananas</trd>
   <trd lng="fr">ananas</trd>
   <trd lng="ru">&#1072;&#1085;&#1072;&#1085;&#1072;&#1089;</trd>
</drv>
<drv mrk="ananas.0ujo">
   <kap><tld/>ujo</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio
       <ref tip="super" cel="bromeli.0acoj">bromeliacoj</ref>
       (<trd lng="la">Ananas</trd>). Unu specio estas kulturata
       pro la &scirc;ajnfrukto.
     </dif>
   </snc>
   <trd lng="fr">ananas</trd>
</drv>
</art>
