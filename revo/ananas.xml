<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ananas</rad>/o
</kap>
<drv mrk="ananas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;ajnfrukto de <tld/>ujo.
    </dif>
  </snc>
  <trd lng="de">Ananas</trd>
</drv>
<drv mrk="ananas.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="bromeli.0acoj">bromeliacoj</ref> 
      (<trd lng="la">Ananas</trd>). Unu specio estas kulturata 
      pro la &scirc;ajnfrukto.
    </dif>
  </snc>
</drv>
</art>
