<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ananas.xml,v 1.31 2021/02/28 16:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ananas</rad>/o
</kap>
<drv mrk="ananas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;ebla &scirc;ajnfrukto de <tld/>ujo,
      unu- &gcirc;is tri-kilograma, konsistanta el la maturi&gcirc;inta infloresko, 
      kies tigo kaj beroj ktp kunkreskas en strobiloforman tuton:
      <ekz>
        en la mezo inter la folioj kronis la fruktan piramidon <tld/>o
        tute ora 
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        jam Kristoforo Kolumbo kunportis en 1493 la <tld/>on 
        de sia dua voja&gcirc;o al la Nova Mondo al Hispanio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=508727">Bromeliacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s;</trd>
  <trd lng="br">aval-ananaz</trd>
  <trdgrp lng="ca">
    <trd>pinya tropical</trd>,
    <trd>anan&agrave;s</trd>
  </trdgrp>
  <trd lng="cs">ananas</trd>
  <trd lng="de">Ananas</trd>
  <trd lng="en">pineapple</trd>
  <trdgrp lng="es">
    <trd>pi&ntilde;a tropical</trd>,
    <trd>anan&aacute;s</trd>
  </trdgrp>
  <trd lng="fa">&a_A_madda;&a_n;&a_A;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">ananas</trd>
  <trd lng="he">&#x5D0;&#x5E0;&#x5E0;&#x5E1;</trd>
  <trd lng="hu">anan&aacute;sz</trd>
  <trd lng="id">nanas</trd>
  <trd lng="nl">ananas</trd>
  <trd lng="pl">ananas</trd>
  <trdgrp lng="pt">
    <trd>anan&aacute;s</trd>,
    <trd>abacaxi</trd>
  </trdgrp>
  <trd lng="ro">ananas</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s;</trd>
  <trd lng="sk">anan&aacute;s</trd>
  <trd lng="sv">ananas</trd>
</drv>

<drv mrk="ananas.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="bromeli.0oj">bromeliacoj</ref>
      (<trd lng="la">Ananas</trd>), kies unu specio estas kulturata
      pro la man&gcirc;ebla &scirc;ajnfrukto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="br">
    <trd>avalenn-ananaz</trd>,
    <trd>plantenn <ind>ananaz</ind></trd>
  </trdgrp>
  <trd lng="ca">anan&agrave;s</trd>
  <trd lng="cs">ananas</trd>
  <trd lng="de">Ananasbaum</trd>
  <trd lng="en">pineapple</trd>
  <trd lng="es">anan&aacute;s</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_A_madda;&a_n;&a_A;&a_n;&a_A;&a_s;</ind></trd>
  <trd lng="fr">ananas</trd>
  <trd lng="he">&#x5E2;&#x5E5; &#x5D0;&#x5E0;&#x5E0;&#x5E1;</trd>
  <trd lng="hu">anan&aacute;sz <klr tip="amb">(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">anan&aacute;s</trd>
</drv>
</art>
