<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ananas</rad>/o
</kap>
<drv mrk="ananas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;ajnfrukto de <tld/>ujo.
    </dif>
  </snc>
  <trd lng="de">Ananas</trd>
  <trd lng="en">pineapple</trd>
  <trd lng="fr">ananas</trd>
  <trd lng="nl">ananas</trd>
  <trd lng="pl">ananas</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s;</trd>
  <trd lng="sv">ananas</trd>
</drv>

<drv mrk="ananas.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="bromeli.0oj">bromeliacoj</ref>
      (<trd lng="la">Ananas</trd>). Unu specio estas kulturata
      pro la &scirc;ajnfrukto.
    </dif>
  </snc>
  <trd lng="en">pineapple</trd>
  <trd lng="fr">ananas</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_n;&c_a;&c_s; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
