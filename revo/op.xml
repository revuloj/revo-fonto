      <trd>individueel</trd>,
      <trd>apart</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einzeln</trd>
    </trdgrp>
    <trd lng="pl">pojedynczy</trd>
    <trd lng="pt">individual</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_je;&c_d;&c_i;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
