  </trdgrp>
  <trd lng="uk">&#x43E;&c_d;&#x43D;&#x43E;&#x43E;&c_s;&c_ib;&c_b;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x5355;&#x4EBA; <pr>d&#x101;nr&#xE9;n</pr></trd>,
    <trd>&#x55AE;&#x4EBA; <pr>d&#x101;nr&#xE9;n</pr></trd>,
    <trd>&#x4E2A;&#x522B;&#x5730; <pr>g&#xE8;bi&#xE9;de</pr></trd>,
