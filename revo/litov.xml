<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>litov</rad>/o
</kap>

<drv mrk="litov.0o">
  <kap><tld/>o <fnt><bib>PIV1</bib></fnt></kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="litov.0ujo"><tld lit="L"/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="nl">Litouwer</trd>
</drv>


<drv mrk="litov.0ujo">
  <kap>
    <tld lit="L"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="litov.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la orienta E&ubreve;ropo &ccirc;e <ref tip="vid"
      cel="mar.BaltaM0o">Balta Maro</ref>
      kun &ccirc;efurbo Vilno.
      Teritorio de Respubliko Litovujo.
      ISO-3166 kodo: LT.
      &Gcirc;i limas kun
      <ref tip="vid" cel="belorus.0ujo.GEOG">Belorusujo</ref>,
      <ref tip="vid" cel="latv.0ujo">Latvujo</ref>,
      <ref tip="vid" cel="pol.P0lando">Pollando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref> (Kaliningrada regiono)
      kaj <ref tip="vid" cel="mar.BaltaM0o">Balta Maro</ref>.
    </dif>
    <ref tip="sin" cel="litovi.L0o.GEOG">Litovio</ref>
    <trd lng="fa">&a_l;&f_y;&a_t;&a_w;&a_A;&a_n;&f_y;</trd>
    <trd lng="nl">Litouwen</trd>
    <trdgrp lng="pl">
      <trd>Litwa</trd>,
      <trd>Republika <ind>Litewska</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

