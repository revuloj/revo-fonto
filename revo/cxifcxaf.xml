<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: cxifcxaf.xml,v 1.6 2024/05/30 22:29:38 afido Exp $">
    <kap>
      <rad>&ccirc;if&ccirc;af</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="cxifcxaf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="paser.0oformaj">Paseroforma</ref>
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      (<trd lng="la">Phylloscopus collybita</trd>),
      migrema, rekonebla per sia kanto:
      <ekz>
        la nomo de la birdoj fitiso kaj <tld/>o imitas la sonojn de iliaj kantoj
        <fnt>
          <vrk><url ref="https://www.esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_19.pdf">Ingrid
          Vallin trovis la birdojn &#8230;</url></vrk>
          <lok>Ora &Jcirc;urnalo, n-ro 14, jun-jul 2019</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2b/Phylloscopus_collybita_1.jpg/640px-Phylloscopus_collybita_1.jpg"
    prm="CC BY-SA 3.0"><tld/>o (Phylloscopus collybita)
        <fnt>
          <aut>Semprempe, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Phylloscopus_collybita_1.jpg">https://commons.wikimedia.org/wiki/File:Phylloscopus_collybita_1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Zilpzalp</trd>,
      <trd>Weidenlaubs&auml;nger</trd>
    </trdgrp>
    <trd lng="en">chiffchaff</trd>
    <trd lng="fr">pouillot v&eacute;loce</trd>
    <trd lng="nl">tsjiftsjaf</trd>
    <trd lng="pl">pierwiosnek<klr>  (ptak)</klr></trd>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_ch;&c_u;&c_r;&c_a;&c_w;&c_k;&c_a;-&c_c;&c_je;&c_n;&c_mol;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="zh">&#x67F3;&#x83BA;&#x5C5E; <pr>li&#x1D4;y&#x12B;ngsh&#x1D4;</pr></trd>
</drv>

  </art>

