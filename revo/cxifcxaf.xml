<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: cxifcxaf.xml,v 1.3 2023/09/01 18:29:27 afido Exp $">
    <kap>
      <rad>&ccirc;if&ccirc;af</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="cxifcxaf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="paser.0oformaj">Paseroforma</ref>
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      (<trd lng="la">Phylloscopus collybita</trd>),
      migrema, rekonebla per sia kanto:
      <ekz>
        la nomo de la birdoj fitiso kaj <tld/>o imitas la sonojn de iliaj kantoj
        <fnt>
          <vrk><url ref="https://www.esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_19.pdf"
              >Ingrid Vallin trovis la birdojn &#8230;</url></vrk>
          <lok>Ora &Jcirc;urnalo, n-ro 14, jun-jul 2019</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/2b/Phylloscopus_collybita_1.jpg/640px-Phylloscopus_collybita_1.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Phylloscopus collybita)
        <fnt>
          <aut>Semprempe, propra verko</aut>
          <lok><url ref="&WCW;/File:Phylloscopus_collybita_1.jpg"
              >https://commons.wikimedia.org/wiki/File:Phylloscopus_collybita_1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Zilpzalp</trd>,
      <trd>Weidenlaubs&auml;nger</trd>
    </trdgrp>
    <trd lng="en">chiffchaff</trd>
    <trd lng="fr">pouillot v&eacute;loce</trd>
    <trd lng="nl">tsjiftsjaf</trd>
  </snc>
</drv>

  </art>

