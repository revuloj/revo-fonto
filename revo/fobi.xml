<vortaro>
<art mrk="$Id$">
<kap>
<rad>fobi</rad>/o
</kap>
<drv mrk="fobi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kronika malsano karakterizata per timo:
      <ekz>
        <ind>aero<tld/>o</ind>,
        <trd lng="fr">a&eacute;rophobie</trd>
        <trdgrp lng="hu">
          <trd>l&eacute;giszony</trd>,
          <trd>aerof&oacute;bia</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>akvo<tld/>o</ind>.
        <trd lng="es">hidrofobia</trd>
        <trd lng="fr">hydrophobie</trd>
        <trdgrp lng="hu">
          <trd>v&iacute;ziszony</trd>,
          <trd>hidrof&oacute;bia</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Phobie</trd>
  <trd lng="es">fobia</trd>
  <trd lng="fr">phobie</trd>
  <trdgrp lng="hu">
    <trd>iszony</trd>,
    <trd>f&oacute;bia</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_b;&c_i;&c_ja;</trd>
</drv>
</art>
