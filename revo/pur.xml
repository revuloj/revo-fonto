<drv mrk="pur.0ema">
  <kap><ofc>*</ofc><tld/>ema</kap>
  <snc mrk="pur.0ema.KOMUNE">
    <dif>
      Kutime zorganta pri <tld/>eco:
      <ekz>
        li estas tre <tld/>ema, kaj e&ccirc; unu polveron
        vi ne trovos sur lia vesto
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_h;&c_a;&c_j;&c_n;&c_y;</trd>
  <trd lng="cs">&ccaron;istotn&yacute;</trd>
  <trd lng="de">reinlich</trd>
  <trd lng="en">tidy</trd>
  <trd lng="es">limpio</trd>
  <trd lng="fr">propre<klr tip="amb"> (aimant la
propret&eacute;)</klr></trd>
  <trd lng="hu">tisztas&aacute;gszeret&odblac;</trd>
  <trd lng="nl">op <ind>reinheid</ind> gesteld</trd>
  <trd lng="pl">schludny</trd>
  <trd lng="pt">asseado</trd>
  <trd lng="ro">curat</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_r;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_p;&c_l;&c_o;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;istotn&yacute;</trd>
  <trd lng="sv">renlig</trd>
  <trd lng="tr">temiz<klr>(temizli&gbreve;i seven)</klr></trd>
</drv>

<drv mrk="pur.0e_kaj_nure">
  <kap>
    <tld/>e kaj nure,
    <var><kap><tld/>e kaj simple</kap></var>
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="pur.0e_kaj_nure.FRAZ">
    <dif>
      &Gcirc;uste dirite, sen troigo a&ubreve; maltroigo:
      <ekz>
        mi min demandas &ccirc;u tiu afero pri la albana krimmetodo ne estas <tld/>e kaj nure
        anta&ubreve;ju&gcirc;o, kli&scirc;o
        <fnt>
          <aut>J. Valano</aut>
          <vrk>&Ccirc;u li bremsis sufi&ccirc;e?</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ni sen anta&ubreve;ju&gcirc;o konsideras la &ccirc;i-supran frazon,
        ni konstatas, ke &gcirc;i <tld/>e kaj simple esprimas
        unu pasintan okaza&jcirc;on
        <fnt>
          <aut>Akademio de Esperanto</aut>
          <vrk><url ref="https://www.akademio-de-esperanto.org/aktoj/aktoj1/pasivaj_participoj.html">Esploro de la Ekzercaro pri la formo &leftquot;estis -ita&rightquot;</url></vrk>
          <lok>Akademia Cirkulero 76a, 1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><ind>schlicht</ind> und einfach</trd>,
    <trd><ind>einfach</ind> gesagt</trd>,
    <trd>ohne zu <ind>besch&ouml;nigen</ind></trd>
  </trdgrp>
  <trd lng="fr">purement et simplement</trd>
</drv>

<drv mrk="pur.0igi">
  <kap><ofc>*</ofc><tld/>igi
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -ig'</lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
    Igi <tld/>a:
  </dif>
  <snc mrk="pur.0igi.neniu_fremda">
    <ekz>
      refandi kaj <tld/>igi ar&gcirc;enton
      <fnt><bib>MT</bib>, <lok>&Mal; 3:3</lok></fnt>;
    </ekz>
    <ekz>
      ne<tld/>igita ar&gcirc;ento, kiu kovras argila&jcirc;on
      <fnt><bib>MT</bib>, <lok>&Sen; 26:23</lok></fnt>.
    </ekz>
    <trdgrp lng="nl">
      <trd>zuiveren</trd>,
      <trd>raffineren</trd>,
      <trd>veredelen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>czy&sacute;ci&cacute;</trd>,
      <trd>sprz&aogonek;ta&cacute;</trd>
    </trdgrp>   
    <trd lng="pt">limpar</trd>
    <trd lng="ro">curat</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>reinigen</trd>
    </trdgrp>
  </snc>
  <snc mrk="pur.0igi.neniu_malbona">
    <ekz>
      <tld/>igi ies koron<fnt>Z</fnt>.
    </ekz>
    <trd lng="nl">zuiveren</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>reinigen</trd>
    </trdgrp>
    <trd lng="pt">limpar</trd>
  </snc>
  <snc mrk="pur.0igi.ne_makulita">
    <ekz>
      mi prenis broson kaj <tld/>igis la veston
      <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
    </ekz>
    <ekz>
      <tld/>igi la nazon de infano.
    </ekz>
    <trdgrp lng="nl">
      <trd>schoonmaken</trd>,
      <trd>reinigen</trd>,
      <trd>poetsen</trd>
    </trdgrp>
    <trd lng="pt">limpar</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>reinigen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xFA6;&#xFB1;&#xF7C;&#xF44;&#xF0B;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>pucovat <klr>(hovor.)</klr></trd>,
    <trd>vy&ccaron;istit</trd>,
    <trd>&ccaron;istit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>reinigen</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>clean</trd>, <trd>cleanse</trd></trdgrp>
  <trd lng="es">limpiar</trd>
  <trdgrp lng="fr">
    <trd>nettoyer</trd>,
    <trd>purifier</trd>
  </trdgrp>
  <trd lng="hu">tiszt&iacute;t</trd>
  <trdgrp lng="sk">
    <trd>o&ccaron;isti&tcaron;</trd>,
    <trd>pre&ccaron;isti&tcaron;</trd>,
    <trd>vy&ccaron;isti&tcaron;</trd>,
    <trd>&ccaron;isti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>rena</trd>,
    <trd>reng&ouml;ra</trd>
  </trdgrp>
  <trd lng="tr">temizlemek</trd>
</drv>

