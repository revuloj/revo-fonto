    <trd>&c_z;&c_a;&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pur.akvo0igejo">
  <kap>
    akvo<tld/>igejo
  </kap>
  <snc>
    <dif>
      Instala&jcirc;o, kie oni purigas akvon de malpura&jcirc;oj kaj ekskrementoj:
      <ekz>
        la unua akvo<tld/>igejo en E&ubreve;ropo ekfunkciis en 1882 en Frankfurto &ccirc;e Majno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=241310"
              >Akvopurigejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kl&auml;ranlage</trd>
  <trd lng="en">wastewater treatment plant</trd>
  <trd lng="fr">traitement des eaux us&eacute;es</trd>
</drv>


<drv mrk="pur.bril0a">
  <kap>bril<tld/>a</kap>
  <snc mrk="pur.bril0a.KOMUNE">
    <dif>
      Pro <tld/>eco reluma, brila:
      <ekz>
        <klr>[la]</klr> cervo <klr>[&#8230;]</klr> havis &ccirc;irka&ubreve; la kolo bril<tld/>an kupran
        ringon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bril<tld/>aj rikoltiloj de la rikoltistoj brilis sur la grenkampoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estis nova, bril<tld/>a kaj bela
        <fnt><bib>Fab4</bib>, <lok>&leftquot;Bela&rightquot;</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_n;&c_y; &c_d;&c_a; &c_b;&c_l;&c_ja;&c_s;&c_k;&c_u;</trd>
  <trd lng="cs">z&aacute;&rcaron;iv&ecaron; &ccaron;ist&yacute;</trd>
  <trdgrp lng="de">
    <trd>blitzsauber</trd>,
    <trd>blitzeblank</trd>
  </trdgrp>
  <trd lng="en">sparkling clean</trd>
  <trd lng="fr">reluisant<klr tip="amb"> (propre)</klr></trd>
  <trd lng="hu">ragyog&oacute;an <ind>tiszta</ind></trd>
  <trd lng="nl">blinkend<klr tip="ind"> (schoon)</klr></trd>
  <trd lng="pl">czysty do po&lstroke;ysku</trd>
  <trdgrp lng="pt">
    <trd>reluzentemente <ind>limpo</ind></trd>,
    <trd>resplandescente</trd>
  </trdgrp>
  <trd lng="ro">curat pentru a str&abreve;luci</trd>
  <trd lng="sk">&zcaron;iarivo &ccaron;ist&yacute;</trd>
  <trd lng="tr">p&inodot;r&inodot;l p&inodot;r&inodot;l</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_ch;&c_i;&c_shch;&c_je;&c_n;&c_i;&c_j; &c_d;&c_o; &c_b;&c_l;&c_i;&c_s;&c_k;&c_u;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_s;&c_k;&c_u;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="pur.frot0igi">
  <kap>frot<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.frot0igi.KOMUNE">
    <dif>
      <ref tip="super" cel="pur.0igi"><tld lit="P"/>igi</ref>
      forigante supran mal<tld/>on per
      <ref tip="vid" cel="frot.0i.MOV">frotado</ref>:
      <ekz>
        lia ora krono kaj la sceptro estis frot<tld/>igitaj tiel, ke &ccirc;io aspektis belege
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        frot<tld/>igita kaj bril<tld/>a la lanterno staris en angulo, kie &gcirc;i tuj faladis
        en la okulojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi same levadis la litojn, la akvo-sitelojn, portis malpurajn vesta&jcirc;ojn al la
        lavejo, frot<tld/>igis, gladis
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        frot<tld/>igita pargeta planko
        <fnt><bib>Fab4</bib>, <lok>La Vento Rakontas pri Valdemaro Doe
        kaj pri liaj Filinoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrap.0i">skrapi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_d;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;)</klr></trd>,
    <trd>&c_a;&c_d;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drhnout</trd>,
    <trd>vydrhnout</trd>,
    <trd>vyt&rcaron;&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schrubben</trd>,
    <trd>rubbeln</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>polish</trd>, <trd>scrub</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>astiquer</trd>,
    <trd>fourbir</trd>,
    <trd>r&eacute;curer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sik&aacute;l</trd>,
    <trd>s&uacute;rol</trd>
  </trdgrp>
  <trd lng="nl">schoonwrijven</trd>
  <trd lng="pl">szorowa&cacute;</trd>
  <trd lng="pt">esfregar</trd>
  <trd lng="ro">tuf&abreve;rie</trd>
  <trdgrp lng="sk">
    <trd>drhn&uacute;&tcaron;</trd>,
    <trd>&scaron;&uacute;cha&tcaron; <klr>(pri pran&iacute;)</klr></trd>
  </trdgrp>
  <trd lng="sv">skrubba</trd>
  <trd lng="uk">&c_v;&c_i;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="pur.naz0igi">
  <kap>blov<tld/>igi, <var><kap>naz<tld/>igi</kap></var></kap>
  <snc mrk="pur.naz0igi.KOS">
    <dif>
      <tld lit="P"/>igi per trablovo <klr>(de nazo ks)</klr>; senmukigi:
      <ekz>
        <klr>[li]</klr> blov<tld/>igas la nazon kaj volas denove preni la glason, kiam
        li sentas, ke li devas terni denove
        <fnt>
          <bib>VRA</bib>
          <lok>Moa Martinson</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Elvira</nom> prenis paperan po&scirc;tukon, brue naz<tld/>igis 
        en &gcirc;in, kaj ploris
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://www.esperanto.org.uk/eldonoj/piron/tekstoj/Ili_kaptis_Elzan.pdf">
          Ili kaptis Elzan!</url></vrk>,
          <lok>1985</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mung.0i.KOS">mungi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_r;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>smrkat</trd>,
    <trd>&ccaron;istit si nos</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnauben</trd>,
    <trd>schneuzen</trd>
  </trdgrp>
  <trd lng="fr">se <ind>moucher</ind></trd>
  <trd lng="pl">wyciera&cacute; nos</trd>     
  <trd lng="ro">&scedil;terge&tcedil;i nasul</trd>
  <trdgrp lng="sk">
    <trd>smrka&tcaron;</trd>,
    <trd>&ccaron;isti&tcaron; nos</trd>
  </trdgrp>
</drv>

<drv mrk="pur.ripoz0igi">
  <kap>ripoz<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.ripoz0igi.KOMUNE">
    <dif>
      <ref tip="super" cel="pur.0igi"><tld lit="P"/>igi</ref>
      per ripozigo, precipe likva&jcirc;on, kies malpura&jcirc;oj tiel
      alfundi&gcirc;as:
      <ekz>
        mi ripoz<tld/>igos iliajn akvojn
        <fnt><bib>MT</bib>, <lok>&Jeh; 32:14</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekant.0i">dekanti</ref>,
      <ref cel="precip1.0i.KEM">precipiti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_s;&c_t;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  <trdgrp lng="de">
    <trd>dekantieren</trd>,
    <trd><ind>absetzen</ind> lassen</trd>,
    <trd>ausf&auml;llen</trd>
  </trdgrp>
  <trd lng="en">decant</trd>
  <trd lng="fr">d&eacute;canter</trd>
  <trdgrp lng="hu">
    <trd>der&iacute;t</trd>,
    <trd>&uuml;lep&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">decanteren</trd>
  <trd lng="pl">scedzi&cacute;</trd>
  <trd lng="pt">decantar</trd>
  <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="pur.sek0igi">
  <kap>sek<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.sek0igi.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="klr">(pri tola&jcirc;oj)</uzo>
    <dif>
      <tld lit="P"/>igi per &hcirc;emia, senakva procedo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_h;&c_ib;&c_m;&c_ch;&c_y;&c_s;&c_t;&c_k;&c_u;</ind></trd>
  <trd lng="de">chemisch <ind>reinigen</ind></trd>
  <trd lng="fr">laver &agrave; sec</trd>
</drv>

<drv mrk="pur.sek0igejo">
  <kap>sek<tld/>igejo</kap>
  <snc mrk="pur.sek0igejo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Loko a&ubreve; butiko al kiu oni portas tola&jcirc;ojn
      por ilia sek<tld/>igo:
      <ekz>
        kiam Lidia estis en Lima unu el la lokaj bahaanoj,
        <nom>Glen Sealts</nom>, kiu posedis sek<tld/>igejon, ofertis
        senpage sek<tld/>igi &scirc;iajn vesta&jcirc;ojn
        <fnt>
          <aut>W. Heller, trad. B. Westerhoff</aut>
          <vrk>Lidia, la Vivo de Lidia Zamenhof, Filino de
          Esperanto</vrk>
          <lok>2007</lok>
        </fnt>.
    </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_ch;&c_y;&c_s;&c_t;&c_k;&c_a; <klr>(&c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>
  <trd lng="de">chemische <ind>Reinigung</ind></trd>
  <trdgrp lng="fr">
    <trd>blanchisserie</trd>,
    <trd>pressing</trd>
  </trdgrp>
</drv>

<drv mrk="pur.visx0igi">
  <kap>vi&scirc;<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.visx0igi.HEJM">
    <dif>
      <ref tip="super" cel="pur.0igi"><tld lit="P"/>igi</ref>
      per <ref tip="vid" cel="visx.0i.MOV">vi&scirc;ado</ref>:
      <ekz>
        ni demetis la bluzojn kaj per ili
        vi&scirc;<tld/>igis la plankon
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wischen</trd>,
    <trd>schrubben</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>astiquer</trd>,
    <trd>essuyer</trd>
  </trdgrp>
  <trd lng="pl">&sacute;ciera&cacute;</trd>     
  <trd lng="ro">&scedil;terge</trd>
</drv>


<drv mrk="pur.0e_kaj_nure">
  <kap>
    <tld/>e kaj nure,
    <var><kap><tld/>e kaj simple</kap></var>
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="pur.0e_kaj_nure.FRAZ">
    <dif>
      &Gcirc;uste dirite, sen troigo a&ubreve; maltroigo:
      <ekz>
        mi min demandas &ccirc;u tiu afero pri la albana krimmetodo ne estas <tld/>e kaj nure
        anta&ubreve;ju&gcirc;o, kli&scirc;o
        <fnt>
          <aut>J. Valano</aut>
          <vrk>&Ccirc;u li bremsis sufi&ccirc;e?</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ni sen anta&ubreve;ju&gcirc;o konsideras la &ccirc;i-supran frazon,
        ni konstatas, ke &gcirc;i <tld/>e kaj simple esprimas
        unu pasintan okaza&jcirc;on
        <fnt>
          <aut>Akademio de Esperanto</aut>
          <vrk><url
          ref="&AdE;/aktoj/aktoj1/pasivaj_participoj.html">Esploro
          de la Ekzercaro pri la formo &leftquot;estis -ita&rightquot;</url></vrk>
          <lok>Akademia Cirkulero 76a, 1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z; &c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_s;&c_a;&c_w;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_r;&c_o;&c_s;&c_t;&c_u; &c_k;&c_a;&c_zh;&c_u;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>schlicht</ind> und einfach</trd>,
    <trd><ind>einfach</ind> gesagt</trd>,
    <trd>ohne zu <ind>besch&ouml;nigen</ind></trd>
  </trdgrp>
  <trd lng="fr">purement et simplement</trd>
</drv>

<drv mrk="pur.etna0igo">
  <kap>etna <tld/>igo,
  <var><kap>etna <tld/>igado</kap></var></kap>
  <snc mrk="pur.etna0igo.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Forigo de &ccirc;iuj anoj de iu nacio el teritorio:
      <ekz>
        tiu milito de etna <tld/>igado pere de perforto 
        amarigas mian vivon
        <fnt>
          <aut>Ratimir</aut>,
          <vrk>Rat i Mir</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="genocid.0o">genocido</ref>,
      <ref cel="masakr.0o">masakro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_ch;&c_y;&c_s;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="de">etnische S&auml;uberung</trd>
  <trd lng="fr">nettoyage ethnique</trd>
  <trd lng="pl">czystka etniczna</trd>     
  <trd lng="ro">purificarea etnic&abreve;</trd>
</drv>

