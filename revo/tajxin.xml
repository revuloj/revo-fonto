<vortaro>
  <art mrk="$Id: tajxin.xml,v 1.2 2025/03/04 14:29:39 afido Exp $">
    <kap>
      <rad>ta&jcirc;in</rad>/o, <var><kap><rad var="&gcirc;">ta&gcirc;in</rad>/o</kap></var>
    </kap>

<drv mrk="tajxin.0o">
  <kap><tld/>o, <var><kap><tld var="&gcirc;"/>o</kap></var></kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Argila stuf<ref tip="super" cel="pot.0o">poto</ref> de la maroka kuirarto;
      <ref tip="super" cel="stuf.0ajxo">stufa&jcirc;o</ref> preparita en tia poto (t.e. <tld/>a&jcirc;o):
      <ekz>
        cepa kaj vinbera <tld/>o
        <fnt>
          <vrk><url ref="http://fagot.alain.free.fr/kuirado/Rpf12/html/VbS02.html"
              >Vinbera Semajnfino </url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        post la granda fasto, ni man&gcirc;is <tld var="&gcirc;"/>on de cidonioj kun juglandoj kaj
        migdaloj
        <fnt>
          <vrk><url ref="https://kajereto.com/tags/maroko"
              >La unua tagman&gcirc;o post la granda fasto</url></vrk>
          <lok>Kajereto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi anka&ubreve; sentas la alvokon <klr>[&#8230;]</klr> al tiuj marokaj nutra&jcirc;oj (kuskuso Kaj
        <tld/>o)
        <fnt>
          <aut>Francine Bohy, trad. Charles Norga</aut>
          <vrk><url ref="https://libro.ee/file.php?id=3184"
              >La Infanoj de la vidalvida domo</url></vrk>
          <lok>La ferioj &ccirc;e &Jcirc;o&jcirc;o onklino, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="marmit.0o">marmito</ref>
    <bld
      lok="&WCU;/thumb/7/78/Ali_Baba_Tajine%2C_offenes_Feuer.jpg/319px-Ali_Baba_Tajine%2C_offenes_Feuer.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o super fajro
        <fnt>
          <aut>Ali Baba Tajine, propra verko</aut>
          <lok><url ref="&WCW;/File:Ali_Baba_Tajine,_offenes_Feuer.jpg"
              >https://commons.wikimedia.org/wiki/File:Ali_Baba_Tajine,_offenes_Feuer.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ar">&a_T;&a_A;&a_j;&a_n;</trd>
    <trd lng="de">Tajine</trd>
    <trd lng="en">tagine</trd>
    <trd lng="fr">tajine</trd>
    <trd lng="he">&tet;&alef;&gimel;'&yod;&fnun;</trd>
    <trd lng="ru">&c_t;&c_a;&c_d;&c_zh;&c_i;&c_n;</trd>
    <trd lng="uk">&c_t;&c_a;&c_d;&c_zh;&c_i;&c_n;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_t;&c_a;&c_d;&c_zh;&c_y;&c_n;</trd>,
  <trd>&c_t;&c_a;&c_zh;&c_y;&c_n;</trd>
</trdgrp>
</drv>

