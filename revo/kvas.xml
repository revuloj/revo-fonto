<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>kvas</rad>/o</kap>

    <drv mrk="kvas.0o">
      <kap><tld/>o</kap>
      <snc>
	<uzo tip="fak">KUI</uzo>
	<dif>
	  Fermentita trinka&jcirc;o, farata el malto, akvo, hordeo kaj
	  diversaj panspecoj:
	  <ekz>
	    la junulo balda&ubreve; aperis kun granda blanka kru&ccirc;o, plena
	    de bona <tld/>o
	    <fnt>
	      <aut>I. Turgenev</aut>:
	      <vrk>&Hcirc;orj kaj Kalini&ccirc;</vrk>.
	      <lok>LOdE, 2003:7 (105)</lok>
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en">kvass</trd>
      <trd lng="ru">&c_k;&c_v;&c_a;&c_s;</trd>
    </drv>

    <drv mrk="kvas.0supo">
      <kap><tld/>supo</kap>
      <snc>
	<uzo tip="fak">KUI</uzo>
	<dif>
	  Man&gcirc;a&jcirc;o el <tld/>o kun haketitaj legomoj, spicherboj kaj viando:
	  <ekz>
	    <tld/>supon oni man&gcirc;as malvarma.
	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_o;&c_k;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
    </drv>

  </art>

