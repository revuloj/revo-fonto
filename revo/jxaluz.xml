<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&jcirc;aluz</rad>/a
</kap>
<drv mrk="jxaluz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Sentanta doloran maltrankvilon pro la timo pri perdo a&ubreve; difekto de
      iu sia poseda&jcirc;o, kiun oni pro tio fervore prizorgas:
      <ekz>
        <tld/>a pri sia famo, situacio, pri siaj rajtoj;
      </ekz>
      <ekz>
        nia Dio estas <tld/>a Dio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">ongerust</trd>
    <trdgrp lng="pt">
      <trd>cioso</trd>,
      <trd>zeloso</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Sentanta doloran suspektoplenan maltrankvilon pri la fideleco de
      amata persono:
      <ekz>
        <tld/>a kiel tigro;
      </ekz>
      <ekz>
        amo kaj <tld/>o estas gefratoj<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iu nacio <tld/>e volas konservi sian suverenecon.
      </ekz>
    </dif>
    <trd lng="nl">jaloers</trd>
    <trdgrp lng="pt">
      <trd>enciumado</trd>,
      <trd>ciumento</trd>
    </trdgrp>
    <trd lng="sv">svartsjuk</trd>
  </snc>
  <trd lng="de">eifers&uuml;chtig</trd>
  <trd lng="en">jealous</trd>
  <trd lng="es">celoso</trd>
  <trd lng="fr">jaloux</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_n;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="jxaluz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de iu <tld/>a.
    </dif>
  </snc>
  <trd lng="de">Eifersucht</trd>
  <trd lng="es">celos</trd>
  <trd lng="fr">jalousie</trd>
  <trd lng="nl">jalousie</trd>
  <trdgrp lng="pt">
    <trd>ci&uacute;me</trd>,
    <trd>zelo</trd>
  </trdgrp>
  <trd lng="sv">svartsjuka</trd>
</drv>

<drv mrk="jxaluz.0i">
  <kap><tld/>i</kap> <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        <tld/>i pri<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pro io, al, kontra&ubreve; iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">eifers&uuml;chtig sein</trd>
  <trdgrp lng="es">
    <trd>estar <ind>celoso</ind> (de)</trd>
    <trd>sentir <ind>celos</ind> (de)</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>jaloux</ind> (de)</trd>
  <trd lng="nl">jaloers zijn</trd>
  <trdgrp lng="pt">
    <trd>sentir ci&uacute;mes</trd>,
    <trd>ter ci&uacute;mes</trd>,
    <trd>mostrar zelo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">vara <ind>svartsjuk</ind></trd>
</drv>
</art>
