<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>frajt</rad>/i</kap>
<drv mrk="frajt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kontra&ubreve; pago ekspedi komerca&jcirc;on, &scirc;ar&gcirc;on
      per ajna transportilo:
      <ekz>
        oni <tld/>as &scirc;ipe, aeroplane, fervoje, omnibuse.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verfrachten</trd>
  <trd lng="en">freight</trd>
  <trdgrp lng="hu">
    <trd>elfuvaroz</trd>,
    <trd>elhaj&oacute;z</trd>
  </trdgrp>
  <trd lng="nl">bevrachten</trd>
  <trd lng="pt">fretar</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_h;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="frajt.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ekspedigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">als Fracht aufgeben</trd>
  <trdgrp lng="hu">
    <trd>fuvaroztat</trd>,
    <trd>fuvark&eacute;nt felad</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>bevrachten</ind></trd>
</drv>

<drv mrk="frajt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        ekzistas diversspecaj <tld/>oj: (fervoja) <tld/>o,
        omnibusa <tld/>o, aeroplana <tld/>o, &scirc;ip<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Fracht</trd>,
    <trd>Transportkosten</trd>
  </trdgrp>
  <trd lng="es">flete</trd>
  <trdgrp lng="fr">
    <trd>fret</trd>,
    <trd>prix de transport</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fuvar</trd>,
    <trd>fuvard&iacute;j</trd>
  </trdgrp>
  <trd lng="it">costo del trasporto</trd>
  <trdgrp lng="nl">
    <trd>vracht</trd>,
    <trd>transportkosten</trd>
  </trdgrp>
  <trd lng="pt">frete</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_a;&c_h;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_z;&c_k;&c_a; &c_g;&c_r;&c_u;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">frakt</trd>
</drv>

<drv mrk="frajt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Objekto ekspedata <tld/>e.
    </dif>
  </snc>
  <trd lng="de">Fracht</trd>
  <trd lng="hu">teher&aacute;ru</trd>  
  <trd lng="nl">vrachtgoed</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_z;&c_i;&c_m;&c_y;&c_j; &c_g;&c_r;&c_u;&c_z;</trd>
</drv>

<drv mrk="frajt.ordinara0ajxo">
  <kap>ordinara <tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld/>a&jcirc;o ekspedata per ordinara, kutima rapideco.
    </dif>
  </snc>
  <trd lng="de">Normalfracht</trd>
  <trdgrp lng="hu">
    <trd>szok&aacute;sos <ind>sz&aacute;ll&iacute;tm&aacute;ny</ind></trd>,
    <trd>norm&aacute;l <ind>fuvar&aacute;ru</ind></trd>
  </trdgrp>
</drv>

<drv mrk="frajt.rapida0ajxo">
  <kap>rapida <tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <tld/>a&jcirc;o ekspedata per pli rapida ol ordinara rapideco.
    </dif>
  </snc>
  <trd lng="de">Eilfracht</trd>
  <trdgrp lng="hu">
    <trd>gyors&aacute;ru</trd>,
    <trd>gyorssz&aacute;ll&iacute;tm&aacute;ny</trd>
  </trdgrp>
  <trd lng="nl">ijlgoed</trd>
  <trd lng="ru">&c_s;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_g;&c_r;&c_u;&c_z;</trd>
</drv>

<drv mrk="frajt.ekspresa0ajxo">
  <kap>ekspresa <tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <tld/>a&jcirc;o liverata al adresato tuj post &gcirc;ia alveno al destinita loko.
    </dif>
  </snc>
  <trd lng="de">Schnellfracht</trd>
  <trd lng="hu">expressz&aacute;ru</trd>  
</drv>

<drv mrk="frajt.0iganto">
  <kap><tld/>iganto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiu, kiu <tld/>igas per helpo de <tld/>isto.
    </dif>
  </snc>
  <trd lng="de">Verfrachter</trd>
  <trdgrp lng="hu">
    <trd>fuvaroztat&oacute;</trd>,
    <trd>sz&aacute;ll&iacute;ttat&oacute;</trd>
  </trdgrp>
  <trd lng="nl">bevrachter</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;&c_o;&c_o;&c_t;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="frajt.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiu, kies profesio estas <tld/>i.
    </dif>
  </snc>
  <trd lng="de">Frachtspediteur</trd>
  <trd lng="hu">sz&aacute;ll&iacute;tm&aacute;nyoz&oacute;</trd>  
  <trd lng="nl">expediteur</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_p;&c_je;&c_d;&c_i;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_h;&c_t;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="frajt.sxip0ajxo">
  <kap>&scirc;ip<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="karg.0o">Kargo</ref> de &scirc;ipo.
    </dif>
  </snc>
  <trd lng="de">Schiffsfracht</trd>
  <trdgrp lng="hu">
    <trd>haj&oacute;fuvar</trd>,
    <trd>haj&oacute;rakom&aacute;ny</trd>,
    <trd>tengeri <ind>sz&aacute;ll&iacute;tm&aacute;ny</ind></trd>
  </trdgrp>
  <trd lng="nl">scheepsvracht</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_d;&c_o;&c_v;&c_o;&c_j; &c_g;&c_r;&c_u;&c_z;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_h;&c_t;</trd>
  </trdgrp>
</drv>
</art>
