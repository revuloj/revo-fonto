  <trd lng="sk">p&iacute;sanie r&yacute;chlopisom</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x435;&#x43D;&#x43E;&#x442;&#x438;&#x43F;&#x456;&#x44F;</trd>,
