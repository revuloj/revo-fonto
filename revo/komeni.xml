<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komeni.xml,v 1.11 2023/09/01 15:46:41 revo Exp $">
<kap>
  <rad>Komeni</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="komeni.0o">
  <kap><tld/>o</kap>
  <snc mrk="komeni.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      &Ccirc;e&hcirc;a filozofo, edukisto kaj episkopo (1592&ndash;1670):
      <ekz>
        la principoj de la universala lingvo de <tld/>o
        estas identaj kun tiuj, en kiuj bazi&gcirc;as Esperanto
        <fnt>
          <aut>Jaroslav Mr&aacute;z</aut>,
          <vrk><url ref="http://www.volny.cz/toascii.cs/aeh-ikeh/komenio.html">Komenio &ndash;
an&gcirc;elo de la paco</url></vrk>,
          <lok>2002-09-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_m;&c_je;&c_n;&c_s;&c_k;&c_ib; <klr>(&c_Ja;&c_n; &c_A;&c_m;&c_a;&c_s;)</klr></trd>
  <trd lng="ja">&#x30B3;&#x30E1;&#x30CB;&#x30A6;&#x30B9;</trd>
  <trd lng="lat">Comenius</trd>
  <trdgrp lng="cs">
    <trd>Komensk&yacute;<klr tip="ind"> (Jean
&Aacute;mos)</klr></trd>,
    <trd>Komensk&yacute;</trd>
  </trdgrp>
  <trd lng="de">Comenius</trd>
  <trdgrp lng="fr">
    <trd>Comenius</trd>,
    <trd>Komensk&yacute;<klr tip="ind"> (Jean &Aacute;mos)</klr></trd>
  </trdgrp>
  <trd lng="it">Comenio</trd>
  <trdgrp lng="pl">
    <trd>Comenius</trd>
    <trd>Komensk&yacute;<klr tip="ind"> (Jan &Aacute;mos)</klr></trd>
    <trd>Kome&nacute;sky<klr tip="ind"> (Jan &Aacute;mos)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Comenius</trd>,
    <trd>Komensky</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_K;&c_o;&c_m;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;<klr tip="ind"> 
(&c_Ja;&c_n; &c_A;&c_m;&c_o;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="sk">Komensk&yacute;</trd>
</drv>
</art>

