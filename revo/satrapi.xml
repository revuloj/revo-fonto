<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: satrapi.xml,v 1.2 2023/05/16 09:25:54 revo Exp $">
<kap>
  <rad>satrapi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="satrapi.0o">
  <kap><tld/>o</kap>
  <snc mrk="satrapi.0o.POL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="provin.0o.POL">Provinco</ref>
      de la antikva persa imperio,
      regata de <ref tip="vid" cel="satrap.0o.HIS">satrapo</ref>:
      <ekz>
        de la Misianoj, la Lidianoj, la Lasonoj, la Kabalianoj
        kaj la Hitenoj li ricevadis kvincent talantojn, kaj
        tiu &ccirc;i estis la dua <tld/>o
        <fnt>
          <aut>Herodoto, trad. S. Sarafian</aut>,
          <vrk>Historio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_r;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="fr">satrapie</trd>
</drv>
</art>

