<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: imput.xml,v 1.20 2024/12/23 12:51:31 revo Exp $">
<kap>
  <rad>imput</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="imput.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="imput.0i.al_konto">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="atribu.0i.asigni">Atribui</ref> sumon al ia konto:
      <ekz>
        <tld/>i &scirc;uldon de societo sur la disponeblaj profitoj;
      </ekz>
      <ekz>
        tiu elspezo devas esti <tld/>ita sur la pasintjara bilanco;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; plej etan eventon oni <tld/>os debete al mia konto
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kalkul.al0i">alkalkuli</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anrechnen</trd>,
      <trd>zurechnen</trd>,
      <trd>zuweisen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>besz&aacute;m&iacute;t<klr tip="amb"> (vmit vmibe)</klr></trd>,
      <trd>terh&eacute;re &iacute;r</trd>
    </trdgrp>
    <trd lng="nl">op <ind>rekening</ind> schrijven</trd>
    <trd lng="ru">&c_o;&c_t;&c_n;&c_je;&c_s;&c_t;&c_i; <klr>(&c_s;&c_u;&c_m;&c_m;&c_u; &c_n;&c_a; &c_k;&c_a;&c_k;&c_o;&c_j;-&c_l;. &c_s;&c_ch;&c_jo;&c_t;)</klr></trd>
  </snc>
  <snc mrk="imput.0i.parton">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Atribui ion al la dividparto de iu:
      <ekz>
        <tld/>i meblojn, pentra&jcirc;on, domon en la heredan parton de iu
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jugx.al0i">alju&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zuteilen</trd>,
      <trd>zuschreiben</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jav&aacute;ra &iacute;r</trd>,
      <trd>neki &iacute;t&eacute;l</trd>,
      <trd>besz&aacute;m&iacute;t<klr tip="amb"> (vkinek vmit)</klr></trd>
    </trdgrp>
    <trd lng="nl">toeschrijven</trd>
    <trd lng="ru">&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o; &c_v; &c_ch;&c_mol;&c_jo;-&c_l;. &c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
  <snc mrk="imput.0i.kulpigi">
    <dif>
      <ref tip="super" cel="atribu.0i">Atribui</ref> al iu la prirespondecon de io:
      <ekz>
        al iliaj influoj oni ja atribuis la freneza&jcirc;ojn de Nero, al iliaj inspiroj oni
        <tld/>is &ccirc;iujn krimojn, kiujn li plenumis
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        majeste aspektantaj genioj <klr>[&#8230;]</klr> kies speciala, <tld/>ita devo estis
        forteni malbonajn spiritojn for de la re&gcirc;a lo&gcirc;ejo
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> kondukas en la kriminala proceso al mistakso de la datumoj kaj
        faktoj kaj al senbazeco de la <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2010/010065.php">Maljusta justico</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kulp.0igi">kulpigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>bezichtigen</trd>,
      <trd>unterstellen</trd>,
      <trd>zuschreiben</trd>,
      <trd>ungerechtfertigt <ind>beschuldigen</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tulajdon&iacute;t<klr tip="amb"> (vkinek vmit)</klr></trd>,
      <trd>rov&aacute;s&aacute;ra &iacute;r</trd>,
      <trd>betud<klr tip="amb"> (vkinek vmit)</klr></trd>
    </trdgrp>
    <trd lng="nl">wijten</trd>
    <trd lng="ru">&c_v;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol; &c_v; &c_v;&c_i;&c_n;&c_u;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_r;&c_a;&c_h;&c_u;&c_n;&c_a;&c_k;)</klr></trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_o;&c_s;&c_ib;&c_c;&c_mol; <klr>(&c_d;&c_a; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>imputovat</trd>,
    <trd>nepr&aacute;vem p&rcaron;i&ccaron;&iacute;tat</trd>,
    <trd>p&rcaron;isuzovat</trd>,
    <trd>sv&aacute;d&ecaron;t</trd>,
    <trd>sv&eacute;st</trd>,
    <trd>vnutit</trd>
  </trdgrp>
  <trd lng="es">imputar</trd>
  <trdgrp lng="fr">
    <trd>imputer</trd>,
    <trd>attribuer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A08;&#x4E0A;&#12377;&#x308B;  <pr>&#12369;&#12356;&#12376;&#12423;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x7E70;&#x308A;&#20837;&#x308C;&#x308B;  <pr>&#x304F;&#x308A;&#12356;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x8CAC;&#x4EFB;&#12434;&#x8CA0;&#x308F;&#x305B;&#x308B;
    <pr>&#x305B;&#x304D;&#x306B;&#12435;&#12434;&#x304A;&#x308F;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x305B;&#12356;&#x306B;&#12377;&#x308B;</trd>
  </trdgrp>
  <trd lng="pt">imputar</trd>
  <trdgrp lng="sk">
    <trd>pripo&ccaron;&iacute;ta&tcaron;</trd>,
    <trd>prip&iacute;sa&tcaron;</trd>,
    <trd>pris&uacute;di&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&#x43F;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &c_z;&c_a; &#x43F;&c_r;&#x43E;&c_v;&c_i;&#x43D;&c_u;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_a;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i; &c_z;..</trd>,
    <trd>&#x43D;&c_a;&c_r;&c_a;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a;..</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43D;&c_je;&c_s;&c_t;&c_i; &#x43D;&c_a; &c_r;&c_a;&c_h;&c_u;&#x43D;&#x43E;&#x43A; &c_v;&c_i;&c_d;&c_a;&c_t;&#x43A;&c_ib;&c_v; &c_z;&#8230;</trd>
  </trdgrp>
  <trd lng="zh">&#x5F52;&#x56E0; <pr>gu&#x12B;y&#x12B;n</pr></trd>
</drv>
</art>
