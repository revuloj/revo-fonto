<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rostbef</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="rostbe.0o">
  <kap><tld/>o</kap>
  <snc mrk="rostbe.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Peco da rostita <ref tip="super" cel="bov.0ajxo">bova&jcirc;o</ref>:
      <ekz>
        <tld/>o kun rostitaj boletoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=602354"
              >Speclo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_s;&c_t;&c_b;&c_ib;&c_f;</trd>
  <trd lng="bg">&c_p;&c_je;&c_ch;&c_je;&c_n;&c_o; <ind>&c_g;&c_o;&c_v;&c_je;&c_zh;&c_d;&c_o;</ind></trd>
  <trd lng="cs">roastbeef</trd>
  <trdgrp lng="de">
    <trd>Rostbeef</trd>,
    <trd>Grillsteak</trd>
  </trdgrp>
  <trd lng="en">roast <ind>beef</ind></trd>
  <trdgrp lng="fr">
    <trd>rosbeef</trd>,
    <trd>rosbif</trd>
  </trdgrp>
  <trd lng="hu">marhas&uuml;lt</trd>
  <trd lng="nl">rosbief</trd>
  <trd lng="pt">rosbife</trd>
  <trd lng="ru">&c_r;&c_o;&c_s;&c_t;&c_b;&c_i;&c_f;</trd>
  <trd lng="sk">roastbeef</trd>
  <trd lng="sv">rostbiff</trd>
</drv>
</art>
