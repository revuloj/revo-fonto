<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>damn</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="damn.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">REL</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kondamni al la suferoj de la infero.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_l;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">zatratit</trd>
  <trd lng="de">verdammen</trd>
  <trd lng="en">damn</trd>
  <trd lng="es">condenar</trd>
  <trd lng="fr">damner</trd>
  <trd lng="hu">k&aacute;rhozatra &iacute;t&eacute;l</trd>
  <trdgrp lng="nl">
    <trd>verdoemen</trd>,
    <trd>vervloeken</trd>,
    <trd>naar de <ind>hel</ind> verwijzen</trd>,
    <trd>verwensen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kl&aogonek;&cacute; si&eogonek;</trd>,
    <trd>zaklina&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>danar</trd>,
    <trd>tornar <ind>maldito</ind></trd>,
    <trd>condenar ao <ind>inferno</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_k;&c_l;&c_ja;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_k;&c_l;&c_i;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="damn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Kondamno je la infero; stato de <tld/>ito.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_k;&c_l;&c_jo;&c_n;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_l;&c_ja;&c_c;&c_mol;&c_c;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">zatracen&iacute;</trd>
  <trd lng="de">Verdammnis</trd>
  <trd lng="en">damnation</trd>
  <trd lng="es">condenaci&oacute;n</trd>
  <trd lng="hu">k&aacute;rhozat</trd>
  <trd lng="fr">damnation</trd>
  <trdgrp lng="nl">
    <trd>verdoeming</trd>,
    <trd>verdoemenis</trd>,
    <trd>vervloeking</trd>
  </trdgrp>
  <trd lng="pl">przekle&nacute;stwo</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_k;&c_l;&c_ja;&c_t;&c_mol;&c_je;</trd>
</drv>
<adm>
  Mi prenis inspiron de la PIV1-aj difinoj kaj reenkondukis la artikolon
  anta&ubreve; la vorto "infero", sed lasis la minusklon.
  <aut>MB</aut>
</adm>
</art>
