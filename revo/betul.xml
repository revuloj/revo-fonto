<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>betul</rad>/o
</kap>
<drv mrk="betul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="betul.0acoj">betulacoj</ref>
      (<trd lng="la">Betula</trd>), arboj kaj arbedoj kun hela mola ligno,
      kun tre flekseblaj bran&ccirc;oj, ofte kun blanka truk&scirc;elo.
    </dif>
  </snc>
  <trd lng="de">Birke</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&#1105;&c_z;&c_a;</trd>
</drv>
<drv mrk="betul.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Betulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Birkengew&auml;chse</trd>
</drv>
</art>
