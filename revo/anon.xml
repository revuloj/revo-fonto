<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="anon.0o">
  <kap><tld/>o</kap>
  <snc mrk="anon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <tld/>arbo, mezgranda, kun verda &scirc;elo kaj blanka karno rapide
      moli&gcirc;anta pro varmo.
    </dif>
  </snc>
  <trd lng="de">Cherimoya</trd>
  <trd lng="en">cherimoya</trd>
  <trd lng="fr">anone</trd>
  <trdgrp lng="es">
    <trd>anona</trd>,
    <trd>cherimoya</trd>
  </trdgrp>
  <trd lng="hu">ann&oacute;na</trd>
  <trd lng="nl">cherimoya<klr tip="ind"> (vrucht)</klr></trd>
</drv>

<drv mrk="anon.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="anon.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
       Monta arbo el Andoj, kultivata en duontropikaj
       landoj, el familio anonacoj
      (<trd lng="la">Annona cherimola</trd>):
      <ekz>
        &leftquot;mi fartas bone, Ariero,&rightquot; diras Tomegino, kiu sidas
        sur la <tld/>arbo
        <fnt>
        <aut>A. Lef&oacute;nst Sardi&ntilde;as, trad. J. de Jes&uacute;s Campos
        Pacheco</aut>,
        <vrk><url
        ref="http://membres.lycos.fr/vilhelmo/Kuba_infanliteraturo.html">
        Kuba infanliteraturo</url>, En la montaro</vrk>,
        <lok>1999-2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Cherimoyabaum</trd>
  <trd lng="fr">anonier</trd>
  <trdgrp lng="hu">
    <trd>cserimoj&oacute;fa</trd>,
    <trd>kr&eacute;malmafa</trd>
  </trdgrp>
  <trd lng="nl">cherimoya<klr tip="ind"> (boom)</klr></trd>
</drv>
</art>

