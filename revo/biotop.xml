<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: biotop.xml,v 1.7 2023/08/31 08:48:51 revo Exp $">
<kap>
  <rad>biotop</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="biotop.0o">
  <kap><tld/>o</kap>
  <snc mrk="biotop.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Aparta 
      <ref tip="vid" cel="medi.0o.EKOL">medio</ref>,
      natura vivejo de difinitaj specioj:
      <ekz>
        la Ukraina Registaro planis kaj konstruis kanalon 
        en la natura reservejo de la delto de la Danubo; 
        &gcirc;i detruas la ekologion de tiu <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk>Deklaracio de la Ekologiisma Frakcio de SAT kaj
          AVE</vrk>,
          <lok><url ref="http://www.westermayer.de/manfred/verduloj/29/aveno29bu.htm">
          Aveno, 2005-06-29</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_ja;&c_t;&c_o;&c_p;</trd>
  <trd lng="cs">biotop</trd>
  <trd lng="en">biotope</trd>
  <trd lng="fr">biotope</trd>
  <trd lng="he">&#x5D1;&#x5D9;&#x5D5;&#x5D8;&#x5D5;&#x5E4;</trd>
  <trdgrp lng="ja">
    <trd>&#x30D3;&#x30AA;&#x30C8;&#x30FC;&#x30D7;</trd>,
    <trd>&#x5C0F;&#x751F;&#x6D3B;&#x570F;  [<ind>&#x3057;&#x3087;&#x3046;&#x305B;&#x3044;&#x304B;&#x3064;&#x3051;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">biotop</trd>
  <trd lng="uk">&c_b;&c_ib;&#x43E;&c_t;&#x43E;&#x43F;</trd>
</drv>
</art>

