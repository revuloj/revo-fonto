<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nikodem.xml,v 1.18 2024/01/29 15:59:10 revo Exp $">
    <kap>
      <ofc>*</ofc> <ofc>7</ofc><rad>Nikodem</rad>/o
    </kap>

<drv mrk="nikodem.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena
      kaj aperanta en la Nova Testamento:
      <ekz>
        estis viro el la Fariseoj, nomata <tld/>o, &ccirc;efo inter la Judoj
        <fnt>
          <bib>NT</bib>
          <lok>Johano 3:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <tld/>o batas Jozefon, tiam <tld/>o estas la batanto
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Nicodemus</trd>
  <trd lng="be">&c_N;&c_ib;&c_k;&c_a;&c_d;&c_y;&c_m;</trd>
  <trd lng="ca">Nicodem</trd>
  <trd lng="cs">Nikodemus</trd>
  <trd lng="de">Nikodemus</trd>
  <trd lng="fr">Nicod&egrave;me</trd>
  <trd lng="hu">Nikod&eacute;musz</trd>
  <trd lng="ja">&#x30CB;&#x30B3;&#x30C7;&#x30E2;</trd>
  <trdgrp lng="nl">
    <trd>Nico</trd>,
    <trd>Nicodemus</trd>
  </trdgrp>
  <trd lng="pl">Nikodem</trd>
  <trd lng="pt">Nicodemos</trd>
  <trd lng="ru">&c_N;&c_i;&c_k;&c_o;&c_d;&c_i;&c_m;</trd>
  <trd lng="sk">Nikod&eacute;m</trd>
  <trd lng="uk">&#x41D;&#x438;&#x43A;&#x43E;&#x434;&#x438;&#x43C;</trd>
  <trd lng="zh">&#x5C3C;&#x54E5;&#x5E95;&#x6BCD; <pr>N&#xED;g&#x113;d&#x1D0;m&#x1D4;</pr></trd>
</drv>
  </art>

