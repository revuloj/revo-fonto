<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ripoz.xml,v 1.36 2023/11/27 09:27:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ripoz</rad>/i
</kap>
<drv mrk="ripoz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ripoz.0i.nenifari">
   <dif>
     <ref tip="super" cel="rest.0i.neforiri">Resti</ref> dum kelka tempo nelaborante,
     nenion farante, por refre&scirc;igi siajn fortojn:
      <ekz>
        Dio finis en la sepa tago Sian laboron, kiun Li faris, kaj Li <tld/>is en la
        sepa tago de la tuta laboro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ne <tld/>is e&ccirc; unu momenton, sed tuj komencis la preparadon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        fine la pli juna de la dancantaj fratinoj sin &jcirc;etis malfacile spirante
        <klr>[&#8230;]</klr> sur benkon, por ek<tld/>i
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la maldekstra bordo de Nilo ku&scirc;is granda urbo <nom>Siut</nom>, en kiu <nom>Ramzes</nom>
        <tld/>is
        kelke da tagoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas tiel laca, ke mi devas iom <tld/>i
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la barono el<tld/>is de la voja&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj <tld/>is sur la ekbrilinta &scirc;tono
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la korpo <tld/>is kaj la spirito voja&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lando <tld/>is <klr>(&gcirc;uis pacon)</klr> dum kvardek jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 3:11</lok>
        </fnt>.
      </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="cxes.0i">&ccirc;esi</ref>,
     <ref cel="halt.0i">halti</ref>,
     <ref cel="kviet.0a">kvieta</ref>,
     <ref cel="strecx.mal0igxi.PSI">malstre&ccirc;i&gcirc;i<sncref/></ref>,
     <ref cel="pauxz.0i">pa&ubreve;zi</ref>,
     <ref cel="gener.re0i">regeneri</ref>
   </refgrp>
   <trd lng="be">&c_a;&c_d;&c_p;&c_a;&c_ch;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trdgrp lng="de">
      <trd>ruhen</trd>,
      <trd>ausruhen</trd>,
      <trd>verschnaufen</trd>,
      <trd>rasten</trd>
    </trdgrp>
   <trd lng="fa">&a_A;&a_s;&a_t;&a_r;&a_A;&a_H;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
   <trdgrp lng="nl">
     <trd>uitrusten</trd>,
     <trd>pauzeren</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_o;&c_t;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_t;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
   </trdgrp>
 </snc>
 <snc mrk="ripoz.0i.kusxi">
   <dif>
     Senmove kviete <ref tip="sin" cel="kusx.0i.ripozi">ku&scirc;i</ref>:
      <ekz>
        estis alligita al la plafono lulilo, en kiu la infano de Doroteo <tld/>is per
        trankvila dormo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la tendo estis]</klr> provizita je la plej belaj kusenoj, sur kiuj devis
        ripozi la gefian&ccirc;oj en la trankvila malvarmeta nokto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        firme lia kapo <tld/>is sur &scirc;ia brusto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la koro de sa&gcirc;ulo <tld/>as <klr>(konstante
         trovi&gcirc;as, kp <ref cel="latent.0a.FIL">latenta<sncref/></ref>)</klr> sa&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>purigi akvon
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 32:14</lok>
        </fnt>.
     </ekz>
   </dif>
    <refgrp tip="vid">
      <ref cel="dorm.0i.ripozi">dormi</ref>
    </refgrp>
   <trd lng="be">&c_s;&c_p;&c_a;&c_ch;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trd lng="de">ruhen</trd>
   <trdgrp lng="fa">
     <trd>&a_A_madda;&a_r;&a_m;&f_y;&a_d;&a_n;</trd>,
     <trd>&a_A_madda;&a_r;&a_A;&a_m;&f_y;&a_d;&a_n;</trd>
   </trdgrp>
   <trd lng="nl">rusten</trd>
   <trd lng="ru">&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="ripoz.0i.mortinte">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Ku&scirc;i malvive en tombo, esti entombigita:
      <ekz>
        iam mi <tld/>os je eterne en trankvilo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
       la faraonoj, &ccirc;i tiuj dioj, kiuj estis anta&ubreve; ni, <tld/>as en siaj
       piramidoj
       <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 9&#xAA;.</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i dancis en la nefermitan tombejon, sed la mortintoj, kiuj tie <tld/>is, ne
        dancis
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu la mortintojn <tld/>i
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>!
      </ekz>
   </dif>
   <trd lng="be">&c_s;&c_p;&c_a;&c_ch;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trd lng="de">ruhen <klr>(im Grab)</klr></trd>
   <trd lng="fa">&a_A_madda;&a_r;&a_m;&f_y;&a_d;&a_n;</trd>
   <trd lng="nl">rusten</trd>
   <trd lng="ru">&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
 </snc>
 <trd lng="bo">&t_la;&t_hata;&t_naro;&t_da;&t_tsheg;&t_la;&t_hata;&t_naro;&t_da;&t_tsheg;</trd>
 <trdgrp lng="cs">
   <trd>b&yacute;t v klidu</trd>,
   <trd>oddechnout <klr>(si)</klr></trd>,
   <trd>odpo&ccaron;inout si</trd>,
   <trd>odpo&ccaron;&iacute;vat</trd>,
   <trd>ulevit si</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>descansar</trd>,
    <trd>reposar</trd>
   </trdgrp>
   <trd lng="en">rest</trd>
   <trdgrp lng="fr">
     <trd>reposer</trd>,
     <trd>se <ind>reposer</ind></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>pihen</trd>,
     <trd>nyugszik</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x4F11;&#x606F;&#12377;&#x308B;  <pr>&#x304D;&#12421;&#12358;&#x305D;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x4F11;&#x61A9;&#12377;&#x308B;  <pr>&#x304D;&#12421;&#12358;&#12369;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#38745;&#x304B;&#x306B;&#x6A2A;&#x305F;&#x308F;&#x308B; 
    <pr>&#12375;&#x305A;&#x304B;&#x306B;&#12424;&#12371;&#x305F;&#x308F;&#x308B;</pr></trd>,
    <trd>&#x6A2A;&#x306B;&#x306A;&#12387;&#12390;&#x4F11;&#12416;  <pr>&#12424;&#12371;&#x306B;&#x306A;&#12387;&#12390;&#12420;&#12377;&#12416;</pr></trd>,
    <trd>&#x6A2A;&#x305F;&#x308F;&#x308B;  <pr>&#12424;&#12371;&#x305F;&#x308F;&#x308B;</pr></trd>,
    <trd>&#x6C38;&#30496;&#12377;&#x308B;  <pr>&#12360;&#12356;&#x307F;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>odpoczywa&cacute;</trd>,
     <trd>wypoczywa&cacute;</trd>
   </trdgrp>
   <trd lng="pt">descansar</trd>
   <trdgrp lng="sk">
    <trd>odd&yacute;chnu&tcaron; si</trd>,
    <trd>odpo&ccaron;&iacute;va&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">vila</trd>
  <trd lng="tok">lape</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="ripoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="ripoz.0o.stato">
    <dif>
      Stato de iu <tld/>anta:
      <ekz>
        <tld/>o de sankta sabato
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        laborante sen <tld/>o 15 horojn &ccirc;iutage
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> migris <klr>[&#8230;]</klr> de urbo al urbo, sen halto kaj <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        havi momenton da amikeco kaj <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj fermi&gcirc;is por <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i templo devas donadi spiritan <tld/>on al la maljunuloj, konsolon al la
        suferantoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>laboro finita, <tld/>o merita</ind>
        <trd lng="fr">apr&egrave;s l'effort le <ind>r&eacute;confort</ind></trd>
        <trd lng="hu">j&oacute;l v&eacute;gzett munka ut&aacute;n meg&eacute;rdemelt
          <ind>pihen&eacute;s</ind></trd>
        <trd lng="pl">praca sko&nacute;czona, wypoczynek zas&lstroke;u&zdot;ony</trd>
        <trd lng="pt">trabalho conclu&iacute;do, descanso merecido.</trd>
        <trd lng="ru"><ind>&c_k;&c_o;&c_n;&c_ch;&c_i;&c_l; &c_d;&c_je;&c_l;&c_o;</ind> &dash; &c_g;&c_u;&c_l;&c_ja;&c_j; &c_s;&c_m;&c_je;&c_l;&c_o;</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_y;&c_n;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klid</trd>,
    <trd>oddech</trd>,
    <trd>odpo&ccaron;inek</trd>,
    <trd>pohov</trd>,
    <trd>rast <klr>(hovor.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erholung</trd>,
    <trd>Ruhe</trd>,
    <trd>Rast</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_t;&a_r;&a_A;&a_H;&a_t;</trd>,
    <trd>&a_A_madda;&a_r;&a_A;&a_m;&a_sh;</trd>,
    <trd>&a_A_madda;&a_s;&a_A;&f_y;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">repos</trd>
  <trdgrp lng="hu">
     <trd>pihen&eacute;s</trd>,
     <trd>nyugv&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4F11;&#x606F;  <pr>&#x304D;&#12421;&#12358;&#x305D;&#x304F;</pr></trd>,
    <trd>&#x4F11;&#x61A9;  <pr>&#x304D;&#12421;&#12358;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odpoczynek</trd>,
    <trd>wypoczynek</trd>,
    <trd>wytchnienie</trd>,
    <trd>popas</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_o;&c_t;&c_d;&c_y;&c_h;</trd>,
     <trd>&c_p;&c_o;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oddych</trd>,
    <trd>odpo&ccaron;inok</trd>,
    <trd>pokoj</trd>
  </trdgrp>
  <trd lng="tok">lape</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_ch;&c_i;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&c_ch;&c_i;&#x43D;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="ripoz.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="ripoz.0ejo.pauxzejo">
    <dif>
      Loko, en kiu oni <tld/>as:
      <ekz>
        la kolombo ne trovis <tld/>ejon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tio estas Mia <tld/>ejo por eterne, &ccirc;i tie Mi lo&gcirc;os
        <fnt><bib>MT</bib>, <lok>&Psa; 132:14</lok></fnt>;
      </ekz>
      <ekz>
        la Orienta Kafejo estis preferata renkontejo kaj <tld/>ejo dum laborpa&ubreve;zoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008487.php">Fina fakturo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj gefratoj el Kosovo preferas jam la marpla&gcirc;ojn kaj <tld/>ejojn de Albanio al
        tiuj de Montenegro kaj Bulgario
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo
          anta&ubreve;eniras</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
     <ref cel="haven.0o">haveno</ref>,
     <ref cel="oaz.0o">oazo</ref>
    </refgrp>
    <trd lng="be">&c_m;&c_je;&c_s;&c_c;&c_a; <ind>&c_a;&c_d;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_k;&c_u;</ind></trd>
    <trdgrp lng="cs">
     <trd>rekrea&ccaron;n&iacute; st&rcaron;edisko</trd>,
     <trd>zotavovna</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Erholungsst&auml;tte</trd>,
      <trd>Raststatt</trd>
    </trdgrp>
    <trd lng="fa">&a_A;&a_s;&a_t;&a_r;&a_A;&a_H;&a_t;&f_g;&a_A;&a_h;</trd>
    <trd lng="fr">lieu de <ind>repos</ind></trd>
    <trdgrp lng="hu">
      <trd>nyughely</trd>,
      <trd>pihen&odblac;hely</trd>
     </trdgrp>
     <trd lng="pl">zacisze</trd>
    <trdgrp lng="sk">
       <trd>rekrea&ccaron;n&eacute; stredisko</trd>,
       <trd>zotavov&ncaron;a</trd>
    </trdgrp>
  </snc>
  <snc mrk="ripoz.0ejo.tombo">
    <dif>
      <ref tip="dif" cel="tomb.0o">Tombo</ref>:
      <ekz>
        la pavimita strato kondukas nun super la <tld/>ejo <klr>(<ref
        cel="tomb.0o">tombo</ref>)</klr>
        de la maljuna <nom>Preben</nom> kaj de lia edzino, kaj neniu plu ilin memoras
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>!
      </ekz>
      <ekz>
        la artisto trovis sian lastan <tld/>ejon en la propra bieno
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006938.php">Friedensreich Hundertwasser 1928 -
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_g;&c_ib;&c_l;&c_a;</trd>
    <trd lng="de">Ruhest&auml;tte</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x4F11;&#x606F;&#25152;  <pr>&#x304D;&#12421;&#12358;&#x305D;&#x304F;&#12376;&#12423;</pr></trd>,
    <trd>&#x4F11;&#x61A9;&#25152;  <pr>&#x304D;&#12421;&#12358;&#12369;&#12356;&#12376;&#12423;</pr></trd>,
    <trd>&#x30EA;&#x30BE;&#x30FC;&#x30C8;  <pr>&#x308A;&#x305E;&#x30FC;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_ib;&c_s;&c_c;&c_je; (&c_d;&#x43B;&#x44F;) &c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_ch;&c_i;&#x43D;&#x43A;&c_u;</trd>
</drv>

<drv mrk="ripoz.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ripoz.0igi.pauxzigi">
   <dif>
     Havigi al iu <tld/>on:
      <ekz>
        facila lega&jcirc;o, kiu agrable <tld/>igas la cerbon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie &ccirc;i &scirc;i trovis alian vivmanieron, kiu &scirc;in <tld/>igis el la ekzistado unutona
        kaj mona&hcirc;a
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ku&scirc;i&gcirc;is en ombron por <tld/>igi mian korpon
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la unuaj momentoj e&ccirc; la kanapo ne povis <tld/>igi lin, sed balda&ubreve; li ekdormis
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
       sur verdaj herbejoj Li <tld/>igas min,
       apud trankvilaj akvoj Li kondukas min
       <fnt><bib>MT</bib>, <lok>&Psa; 23:2</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_d;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_k;</ind></trd>
    <trd lng="cs">nechat odpo&ccaron;&iacute;vat</trd>
    <trd lng="fa">&a_A;&a_s;&a_t;&a_r;&a_A;&a_H;&a_t; &a_d;&a_A;&a_d;&a_n;</trd>
    <trd lng="fr">faire <ind>reposer</ind></trd>
    <trdgrp lng="hu">
       <trd>nyugtat</trd>,
       <trd>pihentet</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>nakazywa&cacute;<ind>  odpoczynek</ind></trd>,
      <trd>pozwoli&cacute;<ind>  odpocz&aogonek;&cacute;</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
       <trd>&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;</trd>,
       <trd>&c_u;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;</trd>,
       <trd>&c_d;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_k;&c_o;&c_j;</trd>
    </trdgrp>
    <trd lng="sk">necha&tcaron; odpo&ccaron;&iacute;va&tcaron;</trd>
  </snc>
  <snc mrk="ripoz.0igi.entombigi">
    <dif>
      <ref tip="dif" cel="tomb.en0igi">Entombigi</ref>:
      <ekz>
        oni lin en lia malvasta &ccirc;erko forveturigis en la kamparon, por <tld/>igi lin tie
        en lia hereda tombo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni &jcirc;etis la korpon en profundan mar&ccirc;on, por ke la koto &gcirc;in kovru kaj <tld/>igu,
        sed la korpo restis super la mar&ccirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ter.en0igi.entombigi">enterigi</ref>,
       <ref cel="sepult.0i">sepulti</ref>
    </refgrp>
    <trd lng="be">&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u; &c_m;&c_a;&c_g;&c_ib;&c_l;&c_u;)</klr></trd>
    <trdgrp lng="de">
      <trd>zur letzten <ind>Ruhe</ind> betten</trd>,
      <trd>bestatten</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x4F11;&#x307E;&#x305B;&#x308B;  <pr>&#12420;&#12377;&#x307E;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x4F11;&#x606F;&#12373;&#x305B;&#x308B; 
    <pr>&#x304D;&#12421;&#12358;&#x305D;&#x304F;&#12373;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;
    &c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_ch;&c_i;&#x43D;&#x43E;&#x43A;/&c_s;&#x43F;&#x43E;&c_ch;&c_i;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;/&c_d;&#x43E;&c_z;&c_v;&#x43E;&#x43B;&#x44F;&c_t;&c_i; &c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_ch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="ripoz.sen0e">
  <kap>sen<tld/>e</kap>
  <snc>
    <dif>
      <ref tip="super" cel="dauxr.0e">Da&ubreve;re</ref>, sen pa&ubreve;zo a&ubreve; sufi&ccirc;a
      ripozo:
      <ekz>
        sen<tld/>e li iris al la montoj, iris supren kaj malsupren li estis jam tre laca
        kaj vidis ankora&ubreve; neniun vila&gcirc;on nek domon
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        lin la furioj pelas sen<tld/>e
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        la piedoj sen<tld/>e da&ubreve;rigis sian dancadon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
         la okuloj brilis kiel du helaj steloj, sed
         ili maltrankvile ruli&gcirc;adis tien kaj reen
         senhalte kaj sen<tld/>e
         <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
       </ekz>
     </dif>
    <refgrp tip="vid">
       <ref cel="cxes.sen0a">sen&ccirc;ese</ref>,
       <ref cel="halt.sen0a">senhalte</ref>,
       <ref cel="pauxz.sen0e">senpa&ubreve;ze</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z; <ind>&c_a;&c_d;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_k;&c_u;</ind></trd>,
    <trd>&c_b;&c_je;&c_z;&c_u;&c_p;&c_y;&c_n;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ruhelos</trd>,
    <trd>rastlos</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_w;&a_s;&a_t;&a_h;</trd>,
    <trd>&f_p;&f_y;&a_A;&f_p;&f_y;</trd>,
    <trd>&a_l;&a_A;&f_y;&a_n;&a_q;&a_T;&a_ayn;</trd>
  </trdgrp>
  <trd lng="fr">sans repos</trd>
  <trdgrp lng="hu">
   <trd>nyughatatlanul</trd>,
   <trd>pihen&eacute;s n&eacute;lk&uuml;l</trd>,
   <trd>szakadatlan</trd>
  </trdgrp>
  <trd lng="ja">&#x4F11;&#x307F;&#x306A;&#x304F;  <pr>&#12420;&#12377;&#x307F;&#x306A;&#x304F;</pr></trd>
  <trdgrp lng="pl">
    <trd>bez<ind>  odpoczynku</ind></trd>,
    <trd>bez<ind>  wytchnienia</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
   <trd>&c_n;&c_je;&c_u;&c_s;&c_t;&c_a;&c_n;&c_n;&c_o;</trd>,
   <trd>&c_b;&c_je;&c_s;&c_p;&c_r;&c_je;&c_s;&c_t;&c_a;&c_n;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">bez oddychu</trd>
  <trd lng="cs">bez oddechu</trd>
  <trd lng="uk">&c_b;&c_je;&c_z; &c_v;&c_ib;&c_d;&#x43F;&#x43E;&c_ch;&c_i;&#x43D;&#x43A;&c_u;</trd>
</drv>
