<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>planed</rad>/o
</kap>
<drv mrk="planed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="astr.0o.1">Astro</ref> ne memlumanta,
      rondiranta &ccirc;irka&ubreve; <ref tip="vid" cel="stel.0o.AST">stelo</ref>:
      <ekz>
        la pli frue ekkonita <tld/>o el gaso estas simila al nia Jupitero
        <fnt><aut>OH</aut>: <vrk>Du
        strangaj planedsistemoj</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero 7a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>
    <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_e;&c_t;&c_a;</trd>
    <trd lng="ca">planeta</trd>
    <trdgrp lng="cs">
      <trd>planeta</trd>,
      <trd>ob&ecaron;&zcaron;nice</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Planet</trd>,
      <trd>Wandelstern</trd>
    </trdgrp>
    <trd lng="en">planet</trd>
    <trd lng="es">planeta</trd>
    <trd lng="fr">plan&egrave;te</trd>
    <trdgrp lng="he">
      <trd>&kaf;&vav;&kaf;&bet; &lamed;&kaf;&tav;</trd>,
      <trd>&pe;&lamed;&nun;&tet;&he;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bolyg&oacute;</trd>,
      <trd>plan&eacute;ta</trd>
    </trdgrp>
    <trd lng="it">pianeta</trd>
    <trd lng="ja">&#12431;&#12367;&#12379;&#12356; &#24785;&#26143;
    &ubreve;akusei</trd>
    <trd lng="nl">planeet</trd>
    <trd lng="pl">planeta</trd>
    <trd lng="pt">planeta</trd>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_n;&c_je;&c_t;&c_a;</trd>
    <trd lng="sv">planet</trd>
    <trd lng="tr">gezegen</trd>
  </snc>
</drv>
</art>
