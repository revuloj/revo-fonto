    <trd lng="fa">&a_s;&f_y;&a_A;&a_r;&a_h;</trd>
    <trd lng="fr">plan&egrave;te</trd>
    <trdgrp lng="he">
      <trd>&kaf;&vav;&kaf;&bet; &lamed;&kaf;&tav;</trd>,
