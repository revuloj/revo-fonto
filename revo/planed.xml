  <trd lng="my">&#x1002;&#x103C;&#x102D;&#x102F;&#x101F;&#x103A;</trd>
  <trd lng="ne">&#x917;&#x94D;&#x930;&#x939;</trd>
  <trd lng="ny">dzikoli</trd>
  <trd lng="pa">&#xA17;&#xA4D;&#xA30;&#xA39;&#xA3F;</trd>
  <trd lng="ps">&#x633;&#x6CC;&#x627;&#x631;&#x647;</trd>
  <trd lng="sd">&#x633;&#x64A;&#x627;&#x631;&#x648;</trd>
