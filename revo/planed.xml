<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: planed.xml,v 1.29 2021/05/28 17:06:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>planed</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="planed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="planed.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="astr.0o.AST">Astro</ref> ne memlumanta,
      rondiranta &ccirc;irka&ubreve; <ref tip="vid" cel="stel.0o.AST">stelo</ref>
      kaj balaanta la &ccirc;irka&ubreve;a&jcirc;on de sia orbito:
      <ekz>
        la pli frue ekkonita <tld/>o el gaso estas simila al nia
        Jupitero
        <fnt>
        <aut>OH</aut>,
        <vrk>Du strangaj planedsistemoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 7a, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:planedoj"><tld/>oj</lstref>
    <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>
    <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_e;&c_t;&c_a;</trd>
    <trd lng="bg">&c_p;&c_l;&c_a;&c_n;&c_je;&c_t;&c_a;</trd>
    <trd lng="ca">planeta</trd>
    <trdgrp lng="cs">
      <trd>planeta</trd>,
      <trd>ob&ecaron;&zcaron;nice</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Planet</trd>,
      <trd>Wandelstern</trd>
    </trdgrp>
    <trd lng="en">planet</trd>
    <trd lng="es">planeta</trd>
    <trd lng="fa">&a_s;&f_y;&a_A;&a_r;&a_h;</trd>
    <trd lng="fr">plan&egrave;te</trd>
    <trdgrp lng="he">
      <trd>&kaf;&vav;&kaf;&bet; &lamed;&kaf;&tav;</trd>,
      <trd>&pe;&lamed;&nun;&tet;&he;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bolyg&oacute;</trd>,
      <trd>plan&eacute;ta</trd>
    </trdgrp>
    <trd lng="id">planet</trd>
    <trd lng="it">pianeta</trd>
    <trdgrp lng="ja">
	    <trd>&#x60D1;&#x661F; 
[<ind>&#x308F;&#x304F;&#x305B;&#x3044;</ind>]</trd>,
	    <trd>&#x904A;&#x661F;
[<ind>&#x3086;&#x3046;&#x305B;&#x3044;</ind>]</trd>
    </trdgrp>
    <trd lng="nl">planeet</trd>
    <trd lng="pl">planeta</trd>
    <trd lng="pt">planeta</trd>
    <trd lng="ro">planet&abreve;</trd>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_n;&c_je;&c_t;&c_a;</trd>
    <trd lng="sv">planet</trd>
    <trd lng="tr">gezegen</trd>
    <trd lng="zh">&#x884C;&#x661F; [<ind>x&iacute;ng x&imacron;ng</ind>]</trd>
  </snc>
  <trd lng="bo">&#xF42;&#xF5F;&#xF60;</trd>
  <trd lng="cs">planeta, ob&ecaron;&zcaron;nice</trd>
  <trd lng="ne">&#x917;&#x94D;&#x930;&#x939;</trd>
  <trdgrp lng="sk">
    <trd>obe&zcaron;nica</trd>,
    <trd>plan&eacute;ta</trd>
  </trdgrp>
</drv>

<drv mrk="planed.nan0o">
  <kap>nan<tld/>o</kap>
  <snc mrk="planed.nan0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="astr.0o.AST">Astro</ref> ne memlumanta,
      rondiranta &ccirc;irka&ubreve; <ref tip="vid" cel="stel.0o.AST">stelo</ref>,
      sed ne balaanta la &ccirc;irka&ubreve;a&jcirc;on de sia orbito:
      <ekz>
      Eriso, la plej granda nan<tld/>o de la sunsistemo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zwergplanet</trd>,
    </trdgrp>
    <trd lng="en">dwarf planet</trd>
    <trd lng="es">planeta enano</trd>
    <trd lng="fr">plan&egrave;te naine</trd>
    <trd lng="id">planet</trd>
    <trd lng="it">pianeta nano</trd>
    <trd lng="nl">dwergplaneet</trd>
    <trd lng="pl">planeta kar&lstroke;owata</trd>
    <trd lng="pt">planeta an&atilde;o</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;&c_o;&c_v;&c_a;&c_ja; 
&c_p;&c_l;&c_a;&c_n;&c_je;&c_t;&c_a;</trd>
    <trd lng="tr">c&uuml;ce gezegen</trd>
  </snc>
</drv>

</art>
