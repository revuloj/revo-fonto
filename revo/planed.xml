<drv mrk="planed.nan0o">
  <kap>nan<tld/>o</kap>
  <snc mrk="planed.nan0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="astr.0o.AST">Astro</ref> ne memlumanta,
      rondiranta &ccirc;irka&ubreve; <ref tip="vid" cel="stel.0o.AST">stelo</ref>,
      sed ne balaanta la &ccirc;irka&ubreve;a&jcirc;on de sia orbito:
      <ekz>
        <ref tip="sub" cel="ceres.0o">Cereso</ref> estas nan<tld/>o en la asteroida zono 
        inter Marso kaj Jupitero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127785"
              >Cereso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="pluton.0o.AST">Plutono</ref>
    <trd lng="be">&c_k;&c_a;&c_r;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_p;&c_l;&c_ja;&c_n;&c_e;&c_t;&c_a;</ind></trd>
    <trdgrp lng="de">
      <trd>Zwergplanet</trd>,
    </trdgrp>
    <trd lng="en">dwarf planet</trd>
    <trd lng="es">planeta enano</trd>
    <trd lng="fr">plan&egrave;te naine</trd>
    <trd lng="id">planet</trd>
    <trd lng="it">pianeta nano</trd>
    <trd lng="nl">dwergplaneet</trd>
    <trd lng="pl">planeta kar&lstroke;owata</trd>
    <trd lng="pt">planeta an&atilde;o</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_n;&c_je;&c_t;&c_a;</trd>
    <trd lng="tr">c&uuml;ce gezegen</trd>
  </snc>
