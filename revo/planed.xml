  <trd lng="ko">&#xD589;&#xC131;</trd>
  <trd lng="lo">&#xE94;&#xEB2;&#xEA7;</trd>
  <trd lng="ml">&#xD17;&#xD4D;&#xD30;&#xD39;&#xD02;</trd>
