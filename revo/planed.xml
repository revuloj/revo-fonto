  <trd lng="da">verden</trd>
  <trd lng="et">planeet</trd>
  <trd lng="eu">planeta</trd>
  <trd lng="fy">planeet</trd>
  <trd lng="gl">planeta</trd>
  <trd lng="gu">&#xA97;&#xACD;&#xAB0;&#xAB9;</trd>
  <trd lng="ha">duniya</trd>
  <trd lng="hi">&#x917;&#x94D;&#x930;&#x939;</trd>
