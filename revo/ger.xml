<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ger.xml,v 1.2 2023/10/27 07:22:53 afido Exp $">
    <kap>
      <rad>ger</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="ger.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      <ref tip="lst" lst="voko:kvantoj" cel="unu.mezur0o">Mezurunuo</ref> de
      pezo kaj mono, dudekono de <ref tip="malprt" cel="sikl.0o">siklo</ref> (0,6&#8239;g):
      <ekz>
        prenu ela&ccirc;eton la&ubreve; via takso, kvin siklojn da ar&gcirc;ento la&ubreve; la sankta siklo, kiu
        konsistas el dudek <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 18:16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_je;&c_r;&c_a; <klr tip="amb">(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="de">Gera <klr>(antikes Ma&szlig;)</klr></trd>
    <trd lng="en">Gerah</trd>
    <trd lng="he">&gimel;&resh;&he;</trd>
  </snc>
</drv>

  </art>

