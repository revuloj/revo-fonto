<drv mrk="neces.laux0e">
  <kap>la&ubreve;<tld/>e</kap>
  <snc mrk="neces.laux0e.KOMUNE">
    <dif>
      Tiam kaj tiom, kiom <tld/>as:
      <ekz>
        la&ubreve;necese la Akademio havas la rajton eldoni propran bultenon
        <fnt>
          <bib>EeP</bib>
          <lok>UNUA PARTO</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vicprezidantoj la&ubreve;<tld/>e anstata&ubreve;as la prezidanton
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la organiza kompanio scias, kiom da bicikloj trovi&gcirc;as en iu
        stacio, kaj la&ubreve;<tld/>e transportas rezervajn veturilojn, se en
        iu kvartalo ili mankas
        <fnt>
          <aut>I. Durwael</aut>,
          <vrk><url ref="&Monato;/2012/010599.php">
          Nova malnova trafikilo</url></vrk>,
          <bib>Monato</bib>,
          <lok>2012:3, p. 17a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>soweit <ind>erforderlich</ind></trd>,
    <trd>falls <ind>notwendig</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>seg&uacute;n sea necesario</trd>,
    <trd>en caso de necesidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en cas de besoin</trd>,
    <trd>en fonction du besoin</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zgodnie z potrzeb&aogonek;</trd>,
    <trd>wed&lstroke;ug potrzeb</trd>
  </trdgrp>
  <trd lng="ro">dup&abreve; cum este necesar</trd>
</drv>

<drv mrk="neces.ne0a">
  <kap>ne<tld/>a, <var><kap>sen<tld/>a</kap></var></kap>
  <snc mrk="neces.ne0a.KOMUNE">
    <dif>
      Kiu ne <tld/>as;
      <ref tip="vid" cel="flu.super0a.FIG">superflua</ref>:
      <ekz>
        ni tiel energie batalas kontra&ubreve; &ccirc;ia sen<tld/>a reformo
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la servistino fari&gcirc;is ne<tld/>a, tamen <nom>Sara</nom> restis
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu anta&ubreve;zorgo, tamen, estis tute ne<tld/>a, &ccirc;ar la azeno ne povus forkuri
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>aj kaj kruelaj humiligoj
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fakultativ.0a">fakultativa</ref>,
      <ref cel="vol.liber0a">libervola</ref>,
      <ref cel="bezon.sen0a">senbezona</ref>
    </refgrp>
  </snc>
  <trd lng="cs">nepot&rcaron;ebn&yacute;</trd>
  <trd lng="en">unnecessary</trd>
  <trdgrp lng="es">
    <trd>innecesario</trd>,
    <trd>in&uacute;til</trd>
  </trdgrp>
  <trd lng="fr">inutile</trd>
  <trdgrp lng="it">
    <trd>superfluo</trd>,
    <trd>inutile</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x7528;&#x306E;  [<ind>&#x3080;&#x3088;&#x3046;&#x306E;</ind>]</trd>,
    <trd>&#x7121;&#x99C4;&#x306A;  [<ind>&#x3080;&#x3060;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niekonieczny</trd>,
    <trd>zb&eogonek;dny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>inutil</trd>,
    <trd>nenecesar</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_n;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nepotrebn&yacute;</trd>
  <trd lng="uk">&c_n;&c_je;&c_p;&c_o;&c_t;&c_r;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="neces.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Ne<tld/>a kaj e&ccirc; malutila:
      <ekz>
        mal<tld/>a elspezo, hasto;
      </ekz>
      <ekz>
        interparolo pri mal<tld/>a temo
        <fnt>
          <bib>MkM</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas en la verko mirinde bele interligita kaj
        nenio estas en &gcirc;i mal<tld/>a
        <fnt>
          <aut>A.Grabowski</aut>:
          <vrk>Anta&ubreve;parolo al &leftquot;Sinjoro Tadeo&rightquot;</vrk>.
          <lok>En &leftquot;Sinjoro Tadeo&rightquot;, Varsovio, 1918. P.VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        strange aspektas la mal<tld/>aj pedanta&jcirc;oj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balast.0o.FIG">balasta</ref>,
      <ref cel="lok.mis0a">misloka</ref>,
      <ref cel="redund.0a">redunda</ref>,
      <ref cel="flu.super0a">superflua</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>unn&ouml;tig</trd>,
    <trd>entbehrlich</trd>
  </trdgrp>
  <trd lng="en">unnecessary</trd>
  <trd lng="es">inecesario</trd>
  <trdgrp lng="pl">
    <trd>zbyteczny</trd>,
    <trd>niepotrzebny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>superflu&abreve;</trd>,
    <trd>de prisos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_r;&c_ja;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_n;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

