<drv mrk="neces.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="neces.0ajxo.KOMUNE">
    <dif>
      Io nepre bezonata:
      <ekz>
        prepari diversan <tld/>a&jcirc;on por la edzi&gcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i a&ccirc;etis en la <ind>butiko de <tld/>a&jcirc;oj</ind> iom da sukero, teo, kelke da bulkoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
        <trd lng="de">Lebensmittelgesch&auml;ft</trd>
      </ekz>
      <ekz>
        tiu &ccirc;i ago <klr>[&#x2026;]</klr> ja estas nenio alia, ol fera <tld/>a&jcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu portis dorsosakon kaj estis bone ekipita per &ccirc;iuj <tld/>a&jcirc;oj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rapidis al la kuirejo kaj revenis kun kafpoto kaj aliaj <tld/>a&jcirc;oj, faris
        fajron <klr>[&#x2026;]</klr> kaj kuiris fortegan kafon
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        nutra&jcirc;oj kaj diversaj <tld/>a&jcirc;oj komencis malofti&gcirc;i, kaj multspecaj limigoj venis
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_n;&c_je;&c_a;&c_o;&c_b;&c_h;&c_o;&c_d;&c_n;&c_a;&c_je;</ind></trd>
    <trd lng="de">Notwendiges</trd>
    <trd lng="en">necessity</trd>
    <trd lng="es">necesidad<klr> (para un ser vivo)</klr></trd>
    <trd lng="fr">n&eacute;cessit&eacute;</trd>
    <trd lng="hu">sz&uuml;ks&eacute;glet</trd>
    <trd lng="it">necessit&agrave;<klr> (cosa di cui si ha bisogno)</klr></trd>
    <trd lng="nl">behoefte</trd>
    <trdgrp lng="pl">
      <trd>niezb&eogonek;dnik</trd>,
      <trd>rzecz potrzebna</trd>
    </trdgrp>
    <trd lng="pt">necessidade</trd>
    <trdgrp lng="ro">
      <trd>esen&#x21B;iale</trd>,
      <trd>kit</trd>,
      <trd>lucru necesar</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_shch;&c_o;-&c_n;. &c_n;&c_je;&c_o;&c_b;&c_h;&c_ib;&c_d;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_t;&c_r;&c_ib;&c_b;&c_n;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="neces.0ajxo.ekskremento">
    <dif>
      Feka&jcirc;o, urino:
      <ekz>
        ekster la tendaro, kien vi elirados por <tld/>a&jcirc;oj, fosileton
        vi devas havi &ccirc;e vi <klr>[&#x2026;]</klr> fosu per &gcirc;i kaj
        reen kovru vian elirinta&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 23:12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ekskre1.0o">Ekskremento</ref>
    <trd lng="de">Notdurft</trd>
  </snc>
  <trdgrp lng="ja">
