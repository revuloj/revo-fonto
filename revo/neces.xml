<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>neces</rad>/a <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="neces.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc>
	<dif>
	  Estanta nepre <ref tip="vid" cel="bezon.0i">bezonata</ref>;
          ne povanta esti malhavata a&ubreve; ne uzata:
	  <ekz>
	    la aero estas <tld/>a por &ccirc;iuj vivantaj esta&jcirc;oj<fnt>B</fnt>;
	  </ekz><ekz>
	    via &ccirc;eesto &ccirc;e la kunsido estas <tld/>a;
	  </ekz><ekz>
	    fari &ccirc;ion <tld/>an por sukcesi;
	  </ekz><ekz>
	    <tld/>a formala&jcirc;o;
	  </ekz><ekz>
	    la artikolo estas oportuna sed ne <tld/>a<fnt>Z</fnt>;
	  </ekz><ekz>
	    energie batali kontra&ubreve; &ccirc;ia sen<tld/>a reformo<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc>
	<uzo tip="fak">FIL</uzo>
	<dif>
	  La&ubreve;racie a&ubreve; la&ubreve;logike ne povanta esti alieca
          a&ubreve; ne esti; nepre okazonta, neevitebla:
	  <ekz>
	    varmo estas <tld/>a efiko de fajro;
	  </ekz><ekz>
	    <tld/>a konsekvenco de principo.
	  </ekz>
	</dif>
      </snc>
    <trdgrp lng="be">
	  <trd>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_y;</trd>,
	  <trd>&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_n;&c_y;</trd>
	</trdgrp>
      <trd lng="cs">nutn&yacute;</trd>
      <trdgrp lng="de">
	<trd>n&ouml;tig</trd>,
	<trd>notwendig</trd>
      </trdgrp>
      <trdgrp lng="el">
	<trd>&alfa;&ny;&alfa;&gamma;&kappa;&alfa;&jota_ton;&omikron;&sigma_fina;</trd>,
	<trd>&alfa;&pi;&alfa;&rho;&alfa;&jota_ton;&tau;&eta;&tau;&omikron;&sigma_fina;</trd>,
	<trd>&chi;&rho;&epsilon;&jota;&alfa;&zeta;&omikron_ton;&my;&epsilon;&ny;&omikron;&sigma_fina;</trd>
      </trdgrp>
      <trd lng="en">necessary</trd>
      <trd lng="es">necesario</trd>
      <trd lng="fr">n&eacute;cessaire</trd>
      <trd lng="hu">sz&uuml;ks&eacute;ges</trd>
      <trdgrp lng="nl">
	<trd>nodig</trd>,
	<trd>noodzakelijk</trd>
      </trdgrp>
      <trd lng="pt">necess&aacute;rio</trd>
      <trdgrp lng="ru">
	<trd>&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_y;&c_j;</trd>,
	<trd>&c_n;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    <trd lng="tp">wile</trd>
    </drv>

    <drv mrk="neces.0eco">
      <kap><tld/>eco, <var><kap><tld/>o</kap></var></kap>
      <snc>
	<dif>
	  Eco de tio, kio estas <tld/>a:
	  <ekz>
	    <tld/>o ne estas kareso
	    <fnt><bib>PrV</bib></fnt>;
          </ekz>
          <ekz>
	    <tld/>eco kontra&ubreve;vola estas le&gcirc;o malmola
	    <fnt><bib>PrV</bib></fnt>.
          </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="dev.0igo">devigo</ref>,
	  <ref cel="legx.0o">le&gcirc;o</ref>,
	  <ref cel="murmur">murmuro</ref>,
	  <ref cel="fort">perforto</ref>
	</refgrp>
      </snc>
      <trd lng="be">&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      <trd lng="de">Notwendigkeit</trd>
      <trd lng="el">&alfa;&ny;&alfa_ton;&gamma;&kappa;&eta;</trd>
      <trd lng="es">necesidad</trd>
      <trd lng="fr">n&eacute;cessit&eacute;</trd>
      <trd lng="hu">sz&uuml;ks&eacute;gess&eacute;g</trd>
      <trd lng="nl">noodzaak</trd>
      <trd lng="pt">necessidade</trd>
      <trd lng="ru">&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="neces.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
	<dif>
	  Io bezonata:
	  <ekz>
	    la <tld/>a&jcirc;oj de la vivo;
	  </ekz><ekz>
	    &ccirc;iuj <tld/>a&jcirc;oj mankas;
	  </ekz><ekz>
	    la Roma imperio estis tiutempe ekonomia <tld/>a&jcirc;o.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_n;&c_je;&c_a;&c_o;&c_b;&c_h;&c_o;&c_d;&c_n;&c_a;&c_je;</ind></trd>
      <trd lng="de">Notwendiges</trd>
      <trd lng="fr">n&eacute;cessit&eacute;</trd>
      <trd lng="hu">sz&uuml;ks&eacute;glet</trd>
      <trd lng="nl">behoefte</trd>
      <trd lng="pt">necessidade</trd>
    </drv>

    <drv mrk="neces.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
  	<dif>
  	  Loko destinita por feki kaj urini.
        </dif>
      </snc>
      <trd lng="be">&c_t;&c_u;&c_a;&c_l;&c_je;&c_t;</trd>
      <trd lng="bg">&c_t;&c_o;&c_a;&c_l;&c_je;&c_t;&c_n;&c_a;</trd>
      <trd lng="de">Toilette</trd>
      <trdgrp lng="en">
        <trd>toilet</trd>,
        <trd>WC</trd>
        <trd>bathroom</trd>,
        <trd>restroom</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>toilettes</trd>,
        <trd>WC</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>v&eacute;c&eacute;</trd>,
        <trd>WC</trd>
        <trd>toalett</trd>,
        <trd>illemhely</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>toilet</trd>,
        <trd>w.c.</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>banheiro <klr>(Braz.)</klr></trd>,
        <trd>toalete</trd>,
        <trd>sanit&aacute;rio</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_t;&c_u;&c_a;&c_l;&c_je;&c_t;</trd>,
        <trd>&c_u;&c_b;&c_o;&c_r;&c_n;&c_a;&c_ja;</trd>,
        <trd>&c_n;&c_u;&c_zh;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="neces.0igi">
      <kap><tld/>igi</kap>
      <snc>
	<dif>
	  Igi <tld/>a:
	  <ekz>
	    la entrepreno <tld/>igas multan monon;
	  </ekz><ekz>
	    la regulo <tld/>igas klarigon;
	  </ekz><ekz>
	    la prudento <tld/>igas nin venki la naturon<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_t;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_y;&c_m;</ind></trd>
      </trdgrp>
      <trdgrp lng="de">
	<trd>n&ouml;tig machen</trd>,
	<trd>notwendig machen</trd>
      </trdgrp>
      <trd lng="el">&chi;&rho;&epsilon;&jota;&alfa_ton;&zeta;&omikron;&my;&alfa;&jota;</trd>
      <trd lng="es">necesitar</trd>
      <trdgrp lng="fr">
        <trd>demander<klr> (n&eacute;cessiter)</klr></trd>,
        <trd>exiger<klr> (n&eacute;cessiter)</klr></trd>,
        <trd>n&eacute;cessiter</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>sz&uuml;ks&eacute;gess&eacute; tesz</trd>,
        <trd>sz&uuml;ks&eacute;gel</trd>
      </trdgrp>
      <trd lng="nl">noodzakelijk maken</trd>
      <trd lng="pt">necessitar</trd>
      <trdgrp lng="ru">
	<trd>&c_t;&c_r;&c_je;&c_b;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_y;&c_m;</ind></trd>
      </trdgrp>
    </drv>

    <drv mrk="neces.0ujo">
      <kap><tld/>ujo</kap>
      <snc  mrk="neces.0ujo.skatoleto">
	<dif>
	  Skatoleto por enteni konstante bezonatajn objektojn:
	  <ekz>
	    <tld/>ujo por tualeto, voja&gcirc;o;
	  </ekz><ekz>
	    <tld/>ujo de kudristino, de kuracisto.
	  </ekz>
	</dif>
	<trd lng="be">&c_n;&c_e;&c_s;&c_e;&c_s;&c_e;&c_r;</trd>
	<trd lng="de">Necessaire</trd>
        <trd lng="fr">n&eacute;cessaire<klr> (trousse)</klr></trd>
        <trd lng="hu">neszesszer</trd>	
	<trd lng="nl">necessaire</trd>
	<trd lng="pt">n&eacute;cessair</trd>
	<trd lng="ru">&c_n;&c_je;&c_s;&c_je;&c_s;&c_s;&c_je;&c_r;</trd>
      </snc>
      <snc  mrk="neces.0ujo.segxo">
	<ref tip="dif" cel="neces.0segxo">necesse&gcirc;o</ref>
	<rim>
	  La&ubreve; <fnt><bib>HejmVort</bib></fnt>, la
	  uzo de &leftquot;<tld/>ujo&rightquot; en &ccirc;i tiu signifo estas ne
	  malofta, sed la&ubreve; la Fundamento &gcirc;i havas la sencon
	  <tld/>ujo<sncref ref="neces.0ujo..skatoleto"/>.  Tamen tiu
	  senco &scirc;ajnas iom arkaiki&gcirc;inta, kaj &ccirc;iel ajn,
	  eblas anka&ubreve; rigardi la du &leftquot;<tld/>ujo&rightquot;n homonimoj.
	</rim>
        <trd lng="hu">v&eacute;c&eacute;kagyl&oacute;</trd>	
      </snc>
    </drv>

    <drv mrk="neces.0bezono">
      <kap><tld/>bezono<fnt>Z</fnt></kap>
      <snc>
	<dif>
	  Manko de &ccirc;io <tld/>a por la vivo; ekstrema senhaveco.
	</dif>
      </snc>
      <trd lng="be">&c_g;&c_a;&c_l;&c_je;&c_ch;&c_a;</trd>
      <trd lng="fr">besoin<klr> (&ecirc;tre dans le besoin etc.)</klr></trd>
     <trdgrp lng="hu">
       <trd>sz&uuml;ks&eacute;g<klr tip="amb">(&iacute;ns&eacute;g)</klr></trd>,
       <trd>sz&udblac;k&ouml;lk&ouml;d&eacute;s</trd>,
       <trd>&iacute;ns&eacute;g</trd>
     </trdgrp>
      <trd lng="nl">nooddruft</trd>
      <trd lng="ru">&c_n;&c_i;&c_shch;&c_je;&c_t;&c_a;</trd>
    </drv>

    <drv mrk="neces.0segxo">
      <kap><tld/>se&gcirc;o, <tld/>eja
      se&gcirc;o<fnt><bib>HejmVort</bib></fnt></kap>
      <snc>
	<dif>
	  <ref tip="malprt" cel="neces.0ejo"><tld lit="N"/>eja</ref>
	  instala&jcirc;o por feki a&ubreve; urini, ordinare kun akvo&scirc;prucigilo por
	  elpeli la ekskrementojn.
	</dif>
        <ref tip="sin" cel="neces.0ujo.segxo"><tld/>ujo <sncref/></ref>
      </snc>
      <trd lng="be">&c_u;&c_n;&c_ib;&c_t;&c_a;&c_z;</trd>
      <trdgrp lng="en">
	<trd>toilet seat</trd>,
	<trd>toilet bowl</trd>
      </trdgrp>
      <trd lng="fr">cuvette de toilettes</trd>
      <trd lng="hu">v&eacute;c&eacute;kagyl&oacute;</trd>
      <trd lng="ru">&c_u;&c_n;&c_i;&c_t;&c_a;&c_z;</trd>
    </drv>

<drv mrk="neces.viv0ajxo">
  <kap>viv<tld/>a&jcirc;o</kap>
  <snc mrk="neces.viv0ajxo.EKON">
    <dif>
      Afero esenca, havenda, bezonata por vivado:
      <ekz>
        per <klr>s</klr>ia &ccirc;iutaga laboro li povis akiri la 
        viv<tld/>a&jcirc;ojn: sufi&ccirc;an man&gcirc;a&jcirc;on, modestan vesta&jcirc;on,
        lo&gcirc;ejon
        <fnt>
          <aut>J. S&aacute;rk&ouml;zi</aut>,
          <vrk><url ref="http://mek.niif.hu/04100/04194/04194.htm">
          Filmo por pli ol 16 jaruloj kaj aliaj noveloj</url></vrk>, 
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grav.vivo0a.KOMUNE">vivograva</ref>
    </refgrp>
  </snc>
  <trd lng="fr">ressource vitale</trd>
</drv>

  </art>
