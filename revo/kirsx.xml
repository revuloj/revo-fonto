<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kirsx.xml,v 1.16 2023/09/01 15:07:13 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kir&scirc;</rad>/o
</kap>
<drv mrk="kirsx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="brand.cxeriz0o">&ccirc;erizbrando</ref>:
      <ekz>
        en oran&gcirc;osuko, pulvorsukero kaj <tld/>o marinitaj fragoj, surtabligita kun
        batita kremo kun vanilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39760">Auguste Escoffier</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_sh;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_n;&c_a;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>
  <trd lng="ca">kirsch</trd>
  <trd lng="de">Kirschwasser</trd>
  <trdgrp lng="en">
    <trd>kirsh</trd>,
    <trd>cherry brandy</trd>
  </trdgrp>
  <trd lng="fr">kirsch</trd>
  <trdgrp lng="hu">
    <trd>cseresznyep&aacute;linka</trd>,
    <trd>cherry brandy</trd>
  </trdgrp>
  <trd lng="ja">&#x30AD;&#x30EB;&#x30B7;&#x30E5;</trd>
  <trdgrp lng="nl">
    <trd>kirsch</trd>,
    <trd>kersenbrandewijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kirsch</trd>,
    <trd>wi&sacute;ni&oacute;wka</trd>
  </trdgrp>
  <trd lng="ro">kirsch</trd>
  <trd lng="ru">&c_v;&c_i;&c_sh;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_n;&c_a;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x448;&#x43D;&#x435;&#x432;&#x430; &#x43D;&#x430;&#x43B;&#x438;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x432;&#x438;&#x448;&#x43D;&#x456;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
