<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bandit.xml,v 1.26 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bandit</rad>/o
</kap>

<drv mrk="bandit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Strat<ref tip="super" cel="rab.0isto">rabisto</ref>, ordinare
      membro de <ref tip="malprt" cel="band.0o.bandacxo">bando<sncref/></ref>:
      <ekz>
        ricevis <tld/>o la&ubreve; sia merito
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la&ubreve;]</klr> via viza&gcirc;o, mi pensus, ke vi estas <klr>[&#x2026;]</klr> <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ojn kaj aventuristojn ili varbis en la armeon kaj faris ilin bonegaj
        soldatoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Brazilo, Kolombio, Peruo la &scirc;akristoj pri drogoj, la <tld/>oj, jam montras
        pli da povo ol la &scirc;tato, ol la polica aparato
        <fnt>
	<aut>Gilbert Ledon</aut>
	<vrk><url ref="&Monato;/2001/007283.php">Putraj demokratioj</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la e&ubreve;ropaj partizanoj estis teroristoj (<tld/>oj) la&ubreve; la naziaj okupantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2009/009725.php">Nu, karaj fratoj ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur naciaj vojoj, <tld/>oj atakas a&ubreve;tojn
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2012/010816.php">Restas nur espero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_n;&c_d;&c_y;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&#xF47;&#xF42;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="ca">bandit</trd>
  <trd lng="cs">gangster</trd>
  <trd lng="de">Bandit</trd>
  <trd lng="en">bandit</trd>
  <trd lng="es">bandido</trd>
  <trd lng="fr">bandit</trd>
  <trd lng="he">&#x5E9;&#x5C1;&#x5D5;&#x5B9;&#x5D3;&#x5B5;&#x5D3;</trd>
  <trd lng="hu">bandita</trd>
  <trd lng="it">bandito</trd>
  <trdgrp lng="ja">
    <trd>&#x5C71;&#x8CCA;  <pr>&#x3055;&#x3093;&#x305E;&#x304F;</pr></trd>,
    <trd>&#x76D7;&#x8CCA;  <pr>&#x3068;&#x3046;&#x305E;&#x304F;</pr></trd>,
    <trd>&#x8FFD;&#x3044;&#x306F;&#x304E;  <pr>&#x304A;&#x3044;&#x306F;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">bandiet</trd>
  <trdgrp lng="pl">
    <trd>bandyta</trd>,
    <trd>bandzior<klr> (pot.)</klr></trd>,
    <trd>rozb&oacute;jnik</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_b;&c_a;&c_n;&c_d;&c_i;&c_t;</trd>,
     <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bandita</trd>,
    <trd>l&uacute;pe&zcaron;n&iacute;k</trd>,
    <trd>zbojn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&#x43D;&c_d;&c_i;&c_t;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_b;&c_ib;&c_j;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
