<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lutr</rad>/o
</kap>
<drv mrk="lutr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Lutra</trd>), malgranda raba besto kun
      na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta &ccirc;e akvo
      kaj man&gcirc;anta fi&scirc;ojn.
    </dif>
  </snc>
  <trd lng="cs">vydra</trd>
  <trd lng="de">Otter</trd>
  <trd lng="hu">vidra</trd>
  <trd lng="en">otter</trd>
  <trd lng="nl">otter</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
</drv>
</art>
