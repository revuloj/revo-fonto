<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lutr.xml,v 1.34 2024/01/15 19:32:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lutr</rad>/o
</kap>
<drv mrk="lutr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lutr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref> kun
      na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta &ccirc;e akvo kaj man&gcirc;anta
      fi&scirc;ojn;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Lutra</trd>):
      <ekz>
        la e&ubreve;ropa <tld/>o vivas anka&ubreve; en la Induso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26794">Induso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ac/Lutra_lutra_qtl2.jpg/640px-Lutra_lutra_qtl2.jpg" prm="CC BY-SA     3.0">E&ubreve;ropa <tld/>o (Lutra lutra) en Sova&gcirc;parko L&uuml;neburg, Germanujo
        <fnt>
          <aut>Quartl, propra verko</aut>
          <lok><url ref="&WCW;/File:Lutra_lutra_qtl2.jpg">&WCW;/File:Lutra_lutra_qtl2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xFB2;&#xF58;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>dourgi</trd>,
    <trd>ki-dour</trd>
  </trdgrp>
  <trd lng="ca">ll&uacute;dria</trd>
  <trd lng="cs">vydra</trd>
  <trd lng="de">Otter</trd>
  <trd lng="en">otter</trd>
  <trd lng="es">nutria</trd>
  <trd lng="fr">loutre</trd>
  <trd lng="hu">vidra</trd>
  <trd lng="id">berang-berang</trd>
  <trd lng="it">lontra</trd>
  <trd lng="ja">&#x30AB;&#x30EF;&#x30A6;&#x30BD;</trd>
  <trd lng="nl">otter</trd>
  <trd lng="pl">wydra</trd>
  <trd lng="pt">lontra</trd>
  <trd lng="ro">vidr&abreve;</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
  <trd lng="sk">vydra</trd>
  <trd lng="uk">&c_v;&c_i;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x736D; <pr>t&#x1CE;</pr></trd>,
    <trd>&#x737A; <pr>t&#x1CE;</pr></trd>,
    <trd>&#x6C34;&#x736D; <pr>shu&#x1D0;t&#x1CE;</pr></trd>,
    <trd>&#x6C34;&#x737A; <pr>shu&#x1D0;t&#x1CE;</pr></trd>,
    <trd>&#x7335; <pr>bi&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lutr.mar0o">
  <kap>
    mar<tld/>o
  </kap>
  <snc>
    <dif>
      Samfamilia <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">specio</ref>,
      sed vivanta &ccirc;e marbordoj de la
      Norda Pacifiko, kie &gcirc;i man&gcirc;as moluskojn, krustacojn, kaj
      ekinoidojn
      (<trd lng="la">Enhydra lutris</trd>):
      <ekz>
        Bering-insulo estas parto de la rusa siberia marborda regiono, 1
660&#x202F;km&quadrat; granda,
        senarbara, sed ri&ccirc;a je fontoj kaj altvaloraj felbestoj, la
        arktaj vulpoj kaj
        mar-<tld/>oj estis rapide ekstermitaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=198275">Bering-insulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/bd/Enhydra_lutris_seashell.jpg/640px-Enhydra_lutris_seashell.jpg" prm="CC BY-SA 2.5">mar<tld/>o
        <fnt>
          <aut>J&uacute;lio Reis (Uzanto:Tintazul)</aut>
          <lok><url ref="&WCW;/File:Enhydra_lutris_seashell.jpg">&WCW;/File:Enhydra_lutris_seashell.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; <ind>&c_v;&c_y;&c_d;&c_r;&c_a;</ind></trd>,
    <trd>&c_k;&c_a;&c_m;&c_ch;&c_a;&c_c;&c_k;&c_ib; <ind>&c_b;&c_a;&c_b;&c_jo;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="de">Seeotter</trd>
  <trd lng="es">nutria marina</trd>
  <trd lng="ja">&#x30E9;&#x30C3;&#x30B3;</trd>
  <trdgrp lng="pl">
    <trd>wydra morska</trd>,
    <trd>ka&lstroke;an</trd>
  </trdgrp>
  <trd lng="ro">vidr&abreve; de mare</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43B;&c_a;&#x43D; &#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_a; &c_v;&c_i;&c_d;&c_r;&c_a;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j; &c_b;&#x43E;&c_b;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x736D; <pr>h&#x1CE;it&#xE0;</pr></trd>,
    <trd>&#x6D77;&#x737A; <pr>h&#x1CE;it&#xE0;</pr></trd>
  </trdgrp>
</drv>

</art>
