<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lutr</rad>/o
</kap>
<drv mrk="lutr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel.0edoj">musteledoj</ref>
      (<trd lng="la">Lutra</trd>), malgranda raba besto kun
      na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta &ccirc;e akvo
      kaj man&gcirc;anta fi&scirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Otter</trd>
  <trd lng="nl">otter</trd>
</drv>
</art>
