<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lutr</rad>/o
</kap>
<drv mrk="lutr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lutr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Lutra</trd>), malgranda raba besto kun
      na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta &ccirc;e akvo kaj man&gcirc;anta fi&scirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="br">
    <trd>dourgi</trd>,
    <trd>ki-dour</trd>
  </trdgrp>
  <trd lng="cs">vydra</trd>
  <trd lng="de">Otter</trd>
  <trd lng="en">otter</trd>
  <trd lng="fr">loutre</trd>
  <trd lng="it">lontra</trd>
  <trd lng="hu">vidra</trd>
  <trd lng="nl">otter</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
</drv>
</art>
