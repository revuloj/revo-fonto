<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ekstr</rad>/a
</kap>
<drv mrk="ekstr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="ekstr.0a.aldona">
    <dif>
      Aldona; eksterordinara; okazanta ekster la kutima vico:
      <ekz>
        <tld/>a eldona&jcirc;o<fnt>Z</fnt>,
      </ekz>
      <ekz>
        trajno,
      </ekz>
      <ekz>
        kelnero,
      </ekz>
      <ekz>
        laboro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.al0i">aldoni</ref>,
      <ref cel="etat.ekster0a.gxenerale">eksteretata</ref>,
      <ref cel="ordina.ekster0a">eksterordinara</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sonder-</trd>,
      <trd>Extra-</trd>,
      <trd>zus&auml;tzlich</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>additionnel</trd>,
      <trd>extraordinaire<klr tip="ind"> (additionnel)</klr></trd>,
      <trd>compl&eacute;mentaire<klr tip="ind"> (additionnel)</klr></trd>,
      <trd>suppl&eacute;mentaire<klr tip="ind"> (additionnel)</klr></trd>,
      <trd>en <ind>plus</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&uuml;l&ouml;n</trd>,
      <trd>extra</trd>,
      <trd>plusz</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_v;&c_y;&c_p;&c_u;&c_s;&c_k; &c_g;&c_a;&c_z;&c_je;&c_t;&c_y;)</klr></trd>,
      <trd>&c_ch;&c_r;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_j; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_z;&c_a;&c_s;&c_je;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
    </trdgrp>
    <trd lng="sv">extra</trd>
  </snc>
  <snc mrk="ekstr.0a.eksterordinara">
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Eksterordinare bonkvalita:
      <ekz>
        <tld/>a vino,
      </ekz>
      <ekz>
        &ccirc;okolado.
      </ekz>
    </dif>
    <rim>
      Tiu &ccirc;i senco aspektas kiel francismo. Ja en tiu lingvo 
      &leftquot;extra&rightquot; estas familiara mallongigo de 
      &leftquot;eksterordinara&rightquot;.
    </rim>
    <refgrp tip="vid">
      <ref cel="delika.0a">delikata</ref>,
      <ref cel="rafin">rafinita</ref>.
    </refgrp>
    <trd lng="be">&c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_a;&c_j; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>besonders</trd>,
      <trd>au&szlig;ergew&ouml;hnlich</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>excellent</trd>,
      <trd>extra<klr tip="ind"> (excellent)</klr></trd>,
      <trd>sup&eacute;rieur<klr tip="ind"> (de tr&egrave;s bonne qualit&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&uuml;l&ouml;nleges</trd>,
      <trd>extra</trd>,
      <trd>szuper</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_sh;&c_je;&c_g;&c_o; &c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;</trd>
  </snc>
  <trd lng="es">extra</trd>
</drv>
</art>
