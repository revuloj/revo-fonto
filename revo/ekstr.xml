<ekz><tld/>a eldona&jcirc;o<fnt>Z</fnt>,</ekz>
<ekz>trajno, kelnero, laboro.</ekz>
</dif>
<refgrp tip="sin">
<ref cel="don.al0i">aldoni</ref>.
</refgrp>
<trdgrp lng="ru">
  <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_v;&c_y;&c_p;&c_u;&c_s;&c_k; &c_g;&c_a;&c_z;&c_je;&c_t;&c_y;)</klr></trd>,
  <trd>&c_ch;&c_r;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_j; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_z;&c_a;&c_s;&c_je;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
</trdgrp>
</snc>
<snc num="2">
<dif>
Eksterordinare bonkvalita: <ekz><tld/>a vino, &ccirc;okolado.</ekz>
</dif>
<refgrp tip="sin">
<ref cel="delika.0a">delikata</ref>,
<ref cel="rafin">rafinita</ref>.
</refgrp>
<trd lng="ru">&c_v;&c_y;&c_s;&c_sh;&c_je;&c_g;&c_o; &c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;</trd>
</snc>
</drv>
