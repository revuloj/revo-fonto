<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekstr.xml,v 1.23 2023/11/07 19:22:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ekstr</rad>/a
</kap>
<drv mrk="ekstr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="ekstr.0a.aldona">
    <dif>
      Aldona; <ref cel="ordina.ekster0a">eksterordinara</ref>; 
      okazanta ekster la kutima vico:
      <ekz>
        vi salutas nur sole viajn fratojn, kion <tld/>an vi faras
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 5:47</lok>
        </fnt>?
      </ekz>
      <ekz>
        matene li per <tld/>a po&scirc;to forveturis al la diablo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun ni estas la mastroj en la domo, kaj nun ni havos <tld/>an festenon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &scirc;ipo trovi&gcirc;is kelke da <tld/>aj velstangoj
        <fnt>
          <bib>Rob</bib>
          <lok>Parto II</lok>
        </fnt>;
      </ekz>
      <ekz>
        honore al la konferenco, oni aran&gcirc;is <tld/>ajn prezentadojn de la cirka programo
        <fnt>
          <bib>CKv</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        gajni <tld/>an kurta&gcirc;eton de la kuna vendado
        <fnt>
          <bib>KrM</bib>
          <lok>6. Kiu Ne Riskas, Tiu Ne Gajnas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi plenumis la taskon perfekte, kun <tld/>a emfazo
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Korpogvardia veterano</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni en&scirc;ovis <tld/>ajn monatojn por ebligi reakordigon de monatoj kaj sezonoj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Sep</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akceso.0a">akcesora</ref>,
      <ref cel="don.al0ita.krome">aldonita<sncref/></ref>,
      <ref cel="ali.0a">alia</ref>,
      <ref cel="apart.0a">aparta</ref>,
      <ref cel="krom1.0a">kroma</ref>,
      <ref cel="pli.0a.aldona">plia<sncref/></ref>,
      <ref cel="suplem.0a">suplementa</ref>,
      <ref cel="suplem.0a.KOMUNE">suplementa</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sonder-</trd>,
      <trd>Extra-</trd>,
      <trd>zus&auml;tzlich</trd>,
      <trd>au&szlig;er der <ind>Reihe</ind></trd>,
      <trd>sonstige</trd>
    </trdgrp>
    <trd lng="en">extra</trd>
    <trdgrp lng="fr">
      <trd>additionnel</trd>,
      <trd>extraordinaire<klr tip="ind"> (additionnel)</klr></trd>,
      <trd>compl&eacute;mentaire<klr tip="ind">
      (additionnel)</klr></trd>,
      <trd>suppl&eacute;mentaire<klr tip="ind">
      (additionnel)</klr></trd>,
      <trd>en <ind>plus</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&uuml;l&ouml;n</trd>,
      <trd>extra</trd>,
      <trd>plusz</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ekstra</trd>,
      <trd>in <ind>plus</ind></trd>,
      <trd>dodatkowy</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>extraordin&aacute;rio</trd>,
      <trd>extra</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;
      <klr>(&c_n;&c_a;&c_p;&c_r;. &c_v;&c_y;&c_p;&c_u;&c_s;&c_k;
      &c_g;&c_a;&c_z;&c_je;&c_t;&c_y;)</klr></trd>,
      <trd>&c_ch;&c_r;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_j;
      <klr>(&c_n;&c_a;&c_p;&c_r;.
      &c_z;&c_a;&c_s;&c_je;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
    </trdgrp>
    <trd lng="sv">extra</trd>
    <trd lng="zh">&#x8FFD;&#x52A0; <pr>zhu&#x12B;ji&#x101;</pr></trd>
</snc>
  <snc mrk="ekstr.0a.eksterordinara">
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Eksterordinare bonkvalita, <ref tip="dif" cel="kvalit.alt0a">altkvalita</ref>:
      <ekz>
        <tld/>a vino, &ccirc;okolado.
      </ekz>
    </dif>
    <rim>
      Tiu &ccirc;i senco aspektas kiel francismo. Ja en tiu lingvo 
      &leftquot;<nac>extra</nac>&rightquot; estas familiara mallongigo de 
      &leftquot;eksterordinara&rightquot;.
    </rim>
    <refgrp tip="vid">
      <ref cel="delika.0a">delikata</ref>,
      <ref cel="rafin">rafinita</ref>
    </refgrp>
<!-- ne traduku evitindajn...
    <trd lng="be">&c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_a;&c_j;
    &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>besonders</trd>,
      <trd>au&szlig;ergew&ouml;hnlich</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>excellent</trd>,
      <trd>extra<klr tip="ind"> (excellent)</klr></trd>,
      <trd>sup&eacute;rieur<klr tip="ind"> (de tr&egrave;s bonne
      qualit&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&uuml;l&ouml;nleges</trd>,
      <trd>extra</trd>,
      <trd>szuper</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ekstra</trd>,
      <trd>super</trd>,
      <trd>superowy</trd>,
      <trd>prima</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_sh;&c_je;&c_g;&c_o;
    &c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;</trd>
-->
  </snc>
  <trdgrp lng="cs">
    <trd>odd&ecaron;len&yacute;</trd>,
    <trd>zvl&aacute;&scaron;tn&iacute;</trd>
  </trdgrp>
  <trd lng="es">extra</trd>
  <trdgrp lng="ja">
    <trd>&#x7279;&#x5225;&#x306E;  <pr>&#x3068;&#x304F;&#x3079;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x683C;&#x5225;&#x306E;  <pr>&#x304B;&#x304F;&#x3079;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x4F59;&#x5206;&#x306E;  <pr>&#x3088;&#x3076;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x81E8;&#x6642;&#x306E;  <pr>&#x308A;&#x3093;&#x3058;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mimoriadny</trd>,
    <trd>zvl&aacute;&scaron;tny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_t;&c_r;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_ib;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_shch;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_ja;&c_k;&c_ib;&c_s;&c_t;&c_mol;)</trd>
  </trdgrp>
</drv>
</art>
