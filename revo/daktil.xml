<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: daktil.xml,v 1.33 2020/05/10 10:10:12 revo Exp $">
<kap>
  <ofc>*</ofc><rad>daktil</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="daktil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="daktil.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longeca, dol&ccirc;a <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <ref tip="malprt" cel="daktil.0arbo"><tld/>arbo</ref>.
      <ekz>
        li man&gcirc;is tritikan platan kukon, <esc>plenmanon</esc> da
        <tld/>oj kaj trinkis pokalon da malforta biero
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        palmoj naskas dol&ccirc;ajn <tld/>ojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        figoj kaj <tld/>oj pruvas, ke oni importis nutra&jcirc;ojn el foraj
        regionoj, e&ccirc; el la norda Afriko
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2011/010488.php">Rubo de la
          romianoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim mrk="daktil.0o.RIM_HOM">
      <ref tip="hom" cel="daktil1.0o">Daktilo</ref>
      estas anka&ubreve; nomo de versa ritmo.      
    </rim>
    <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;</trd>
    <trd lng="br">datezenn<klr tip="ind"> (frouezhenn)</klr></trd>
    <trd lng="ca">d&agrave;til</trd>
    <trd lng="cs">datle</trd>
    <trd lng="da">daddel</trd>
    <trd lng="de">Dattel</trd>
    <trd lng="en">date<klr tip="amb"> (fruit)</klr></trd>
    <trdgrp lng="es">
      <trd>d&aacute;til</trd>,
      <trd>d&aacute;ctilo<klr> (poes&iacute;a)</klr></trd>
    </trdgrp>
    <trd lng="fr">datte</trd>
    <trd lng="he">&tav;&mem;&resh;</trd>
    <trd lng="hu">datolya</trd>
    <trd lng="id">kurma</trd>
    <trd lng="io">datelo</trd>
    <trdgrp lng="ja">
      <trd>&#12487;&#12540;&#12484; [<ind>&#12391;&#12540;&#12388;</ind>]</trd>,
      <trd>&#12490;&#12484;&#12513;&#12516;&#12471;&#12398;&#23455; [<ind>&#12394;&#12388;&#12417;&#12420;&#12375;&#12398;&#12415;</ind>]</trd>
    </trdgrp>
    <trd lng="nl">dadel<klr tip="ind"> (vrucht)</klr></trd>
    <trd lng="pl">daktyl</trd>
    <trd lng="pt">t&acirc;mara</trd>
    <trdgrp lng="ro">
      <trd>curmal&abreve;</trd>,
      <trd>vers dactil</trd>
    </trdgrp>
    <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_k;</trd>
  </snc>
  <trd lng="cs">datle <klr>(plod)</klr></trd>
  <trd lng="sk">dat&lcaron;a</trd>
</drv>

<drv mrk="daktil.0arbo">
  <kap><tld/>arbo, 
    <var><kap><tld/>ujo</kap></var>,
    <var><kap><tld/>palmo</kap></var>
  </kap>
  <snc mrk="daktil.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj"
      cel="genr.0o.BIO">Genro</ref>
      (<trd lng="la">Phoenix</trd>)
      el familio <ref tip="malprt" cel="palm.0oj">palmoj</ref>,
      de kiu unu specio estas multe kulturata pro la fruktoj:
      <ekz>
        sepdek <tld/>palmoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>palmo etendis sian &scirc;irman foliaron tre alte, kvaza&ubreve; &gcirc;i
        volus ludi la rolon de sunombrelo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        palmosukero estas farita batante flortigojn de
        diversaj palmoj por kolekti la sevon, la plej grava 
        specio estas la hinda <tld/>ujo (<nom>Phoenix sylvestris</nom>)
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=379026">Nerafinita
          dol&ccirc;iga&jcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en malproksimo oni vidis <klr>[&#8230;]</klr> dense dissemitajn
        <tld/>arbojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_ja;
  &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="br">datezenn<klr tip="ind"> (gwezenn)</klr></trd>
  <trd lng="ca">palmera datilera</trd>
  <trdgrp lng="cs">
    <trd>datlovn&iacute;k</trd>,
    <trd>datlovn&iacute;k prav&yacute;</trd>,
    <trd>datlov&aacute; palma</trd>
  </trdgrp>
  <trd lng="da">daddelpalme</trd>
  <trd lng="de">Dattelpalme</trd>
  <trdgrp lng="en">
    <trd>date<klr tip="amb"> (plant)</klr></trd>,
    <trd>date palm</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>datilera</trd>,
    <trd>palma com&uacute;n</trd>,
    <trd>f&eacute;nix</trd>,
    <trd>t&aacute;mara</trd>,
    <trd>datilero</trd>
  </trdgrp>
  <trd lng="fr">dattier</trd>
  <trdgrp lng="he">
    <trd>&dalet;&qof;&lamed;</trd>,
    <trd>&ayin;&ftsadi; &tav;&mem;&resh;</trd>
  </trdgrp>
  <trd lng="hu">datolyap&aacute;lma</trd>
  <trd lng="io">dateliero</trd>
  <trd lng="ja">&#12490;&#12484;&#12513;&#12516;&#12471; [<ind>&#12394;&#12388;&#12417;&#12420;&#12375;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>dadelpalm</trd>,
    <trd>dadel<klr tip="ind"> (dadelpalm)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>daktylowiec</trd>,
    <trd>palma <ind>daktylowa</ind></trd>
  </trdgrp>
  <trd lng="pt">tamareira</trd>
  <trd lng="ro">finic</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_k;&c_o;&c_v;&c_a;&c_ja;
  &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="sk">dat&lcaron;ov&aacute; palma</trd>
</drv>
</art>
