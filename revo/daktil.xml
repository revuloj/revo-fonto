<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>daktil</rad>/o
</kap>
<drv mrk="daktil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Longeca, dol&ccirc;a frukto de
      <ref tip="malprt" cel="daktil.0ujo"><tld/>ujo</ref>.
    </dif>
    <trd lng="de">Dattel</trd>
    <trd lng="en">date</trd>
    <trd lng="fr">datte</trd>
    <trd lng="hu">datolya</trd>
    <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="daktil.0o.poezia">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el tri silaboj, el kiuj la unua estas longa a&ubreve; akcenta, kaj la
      du ceteraj estas mallongaj a&ubreve; senakcentaj;
      simb. /&dash;&ypsilon;&ypsilon;/:
      <ekz>
        "tiu &ccirc;i" konsistigas <tld/>on.
      </ekz>
    </dif>
    <trd lng="de">Daktylus</trd>
    <trd lng="en">dactyl</trd>
    <trd lng="fr">dactyle<klr tip="ind"> (m&eacute;trique)</klr></trd>
    <trd lng="hu">daktilus</trd>
    <trd lng="ru">&c_d;&c_a;&c_k;&c_t;&c_i;&c_l;&c_mol;</trd>
  </snc>
</drv>
<drv mrk="daktil.0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="palm.0oj">palmoj</ref>
      (<trd lng="la">Phoenix</trd>), kies unu specio estas multe
      kulturata pro la fruktoj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>date</trd>,
    <trd>date palm</trd>
  </trdgrp>
  <trd lng="fr">dattier</trd>
  <trd lng="hu">datolyap&aacute;lma</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_k;&c_o;&c_v;&c_a;&c_ja;
  &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
</drv>
</art>
