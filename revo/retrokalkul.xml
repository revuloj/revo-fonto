<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: retrokalkul.xml,v 1.1 2025/11/13 02:24:49 afido Exp $">
    <kap>
      <rad>retrokalkul</rad>/i
    </kap>

<drv mrk="retrokalkul.0i">
  <kap>retro<tld/>i</kap>
  <snc mrk="retrokalkul.0o.MAT">
    <dif>
      <ref tip="super" cel="kalkul.0i.operacii">Kalkuli</ref>
      en la direkto kontra&ubreve;a al la normala:
      <ekz>
        por &ccirc;i tiu renkonti&gcirc;o, <nom>Alexandra</nom> <tld/>is la tagojn
        <fnt>
          <bib>EP&Ccirc;</bib>
          <vrk><url ref="http://www.espero.com.cn/2015-10/22/content_36860801.htm"
              >La mondo kortu&scirc;ita de &ccirc;inaj &gcirc;emeloj dise vivantaj</url></vrk>
          <lok>El Popola &Ccirc;inio, 2015-10-22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kalkul.de0i">dekalkuli</ref>,
      <ref cel="krement.de0i">dekrementi</ref>
    </refgrp>
  </snc>
<trd lng="de">r&uuml;ckw&auml;rts z&auml;hlen</trd>
<trd lng="en">count down</trd>
</drv>

<drv mrk="retrokalkul.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="kalkul.0o.aritmetika">Kalkulo</ref> en direkto kontra&ubreve;a al la normala:
      <ekz>
        &gcirc;iaj profesi-nivelaj stilo kaj ludado kompareblas kun tiuj de la kanto &leftquot;La fina
        <tld/>o&rightquot; de la svedoj <nom>Europe</nom>
        <fnt>
          <bib>Monato</bib>
          <vrk>Bona le&gcirc;era rokmuziko</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_l;&c_ib;&c_k; &c_z;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_b;&c_r;&c_o;&c_je;&c_n;&c_je; &c_n;&c_a;&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o; &c_b;&c_r;&c_o;&c_je;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">compte regressiva</trd>
  <trdgrp lng="de">
    <trd>R&uuml;ckw&auml;rtsz&auml;hlen</trd>,
    <trd>R&uuml;ckrechnung</trd>,
    <trd>Countdown</trd>
  </trdgrp>
  <trd lng="en">countdown</trd>
  <trd lng="fr">compte &agrave; rebours</trd>
  <trd lng="hu">visszasz&aacute;ml&aacute;l&aacute;s</trd>
  <trdgrp lng="id">
    <trd>hitung mundur</trd>,
    <trd><baz>hitung</baz>perhitungan mundur</trd>
  </trdgrp>
  <trd lng="nl">(het) <ind>aftellen</ind></trd>
  <trd lng="pl">odliczanie</trd>
  <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;
  &c_o;&c_t;&c_s;&c_ch;&c_je;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5012;&#x8A08;&#x6642; <pr>d&agrave;oj&igrave;sh&iacute;</pr></trd>,
    <trd>&#x5012;&#x6570;&#x8BA1;&#x65F6; <pr>d&agrave;osh&#x1D4;j&igrave;sh&iacute;</pr></trd>,
    <trd>&#x5012;&#x6570;&#x8BA1;&#x6570; <pr>d&agrave;osh&#x1D4;j&igrave;sh&ugrave;</pr></trd>,
    <trd>&#x8B80;&#x79D2; <pr>d&uacute;mi&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

  </art>

