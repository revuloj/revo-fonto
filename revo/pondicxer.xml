<vortaro>
  <art mrk="$Id: pondicxer.xml,v 1.5 2024/07/26 09:27:55 afido Exp $">
    <kap>
      <rad>Pondi&ccirc;er</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="pondicxer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la sudo de <ref tip="malprt" cel="barat.B0o">Barato</ref>,
      &ccirc;. 135&#x202F;km sude de &Ccirc;enajo:
      <ekz>
        la surskribo sur la bareloj estis farita de armilisto Benjameno
        <nom>Ellian</nom> en <tld/>o
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Puducherry</trd>,
      <trd>Pondicherry</trd>
    </trdgrp>
    <trd lng="en">Pondicherry</trd>
    <trd lng="fr">Pondich&eacute;ry</trd>
  <trdgrp lng="ta">
    <trd>&#xBAA;&#xBC1;&#xBA4;&#xBC1;&#xB9A;&#xBCD;&#xB9A;&#xBC7;&#xBB0;&#xBBF;</trd>,
    <trd>&#xBAA;&#xBBE;&#xBA3;&#xBCD;&#xB9F;&#xBBF;&#xB9A;&#xBCD;&#xB9A;&#xBC7;&#xBB0;&#xBBF;</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_P;&c_a;&c_n;&c_d;&c_y;&c_ch;&c_e;&c_r;&c_y;</trd>,
    <trd>&c_P;&c_u;&c_d;&c_u;&c_ch;&c_e;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="ja">&#x30DD;&#x30F3;&#x30C7;&#x30A3;&#x30B7;&#x30A7;&#x30EA;</trd>
  <trd lng="zh">&#x672C;&#22320;&#x6CBB;&#x91CC; <pr>b&#x11B;nd&#xEC;zh&#xEC;l&#x1D0;</pr></trd>
</drv>

