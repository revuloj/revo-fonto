        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Puducherry</trd>,
      <trd>Pondicherry</trd>
    </trdgrp>
    <trd lng="en">Pondicherry</trd>
    <trd lng="fr">Pondich&eacute;ry</trd>
  <trdgrp lng="ta">
    <trd>&#2986;&#3009;&#2980;&#3009;&#2970;&#3021;&#2970;&#3015;&#2992;&#3007;</trd>,
    <trd>&#2986;&#3006;&#2979;&#3021;&#2975;&#3007;&#2970;&#3021;&#2970;&#3015;&#2992;&#3007;</trd>
  </trdgrp>
  </snc>
</drv>

