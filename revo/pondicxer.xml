<vortaro>
  <art mrk="$Id: pondicxer.xml,v 1.2 2023/08/16 14:28:52 afido Exp $">
    <kap>
      <rad>Pondi&ccirc;er</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="pondicxer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la sudo de <ref tip="malprt" cel="barat.B0o">Barato</ref>,
      &ccirc;. 135&#8239;km sude de &Ccirc;enajo:
      <ekz>
        la surskribo sur la bareloj estis farita de armilisto Benjameno
