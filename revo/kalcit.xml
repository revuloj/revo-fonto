<vortaro>
  <art mrk="$Id$">
    <kap><rad>kalcit</rad>/o</kap>

    <drv mrk="kalcit.0o">
      <kap><tld/>o</kap>
      <snc>
	<uzo tip="fak">MIN</uzo>
	<dif>
          Diversdevena kristala mineralo <frm>CaCO<sub>3</sub></frm>,
          la &ccirc;efa komponanto de la
          <ref cel="kalk.0osxtono">kalko&scirc;tonoj</ref>.
	</dif>
      </snc>
      <trd lng="en">tiff</trd>
      <trdgrp lng="ru">
	<trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_k;&c_o;&c_v;&c_y;&c_j;
&c_sh;&c_p;&c_a;&c_t;</trd>,
	<trd>&c_k;&c_a;&c_l;&c_mol;&c_c;&c_i;&c_t;</trd>
      </trdgrp>
    </drv>

  </art>

