<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap><rad>kalcit</rad>/o</kap>
     <drv mrk="kalcit.0o">
       <kap><tld/>o</kap>
       <snc>
	    <uzo tip="fak">MIN</uzo>
	    <dif>
           Diversdevena kristala mineralo <frm>CaCO<sub>3</sub></frm>,
           la &ccirc;efa komponanto de la
           <ref cel="kalk.0osxtono">kalko&scirc;tonoj</ref>.
	    </dif>
       </snc>
  <trd lng="ca">calcita</trd>
  <trd lng="cs">kalcit</trd>
  <trdgrp lng="de">
    <trd>Kalzit</trd>,
    <trd>Calcit</trd>,
    <trd>Kalkspat</trd>
  </trdgrp>
  <trd lng="en">tiff</trd>
  <trd lng="fa">&f_k;&a_l;&a_s;&f_y;&a_t;</trd>
  <trdgrp lng="fi">
    <trd>kalsiitti</trd>,
    <trd>kalkkis&auml;lp&auml;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kalcit</trd>,
    <trd>m&eacute;szp&aacute;t</trd>
  </trdgrp>
  <trd lng="pl">kalcyt</trd>
  <trd lng="pt">calcita</trd>
  <trd lng="ro">calcit</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_k;&c_o;&c_v;&c_y;&c_j; &c_sh;&c_p;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_c;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">kalcit</trd>
</drv>
</art>

