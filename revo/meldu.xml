<art mrk="$Id: meldu.xml,v 1.6 2024/01/25 20:10:36 afido Exp $">
  <kap>
    <rad>meldu</rad>/o
    <fnt><bib>KrED</bib></fnt>
  </kap>

<drv mrk="meldu.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AGR</uzo><uzo tip="fak">BOT</uzo>
    <dif>
      Planta <ref tip="super" cel="san.mal0o">malsano</ref>, karakterizita de 
      blanketa folikovro, ka&ubreve;zita de diversaj parazitoj, i.a. 
      <ref tip="sub" cel="ojdi.0o">ojdio</ref> 
      (ordo <nom>Erysiphales</nom> de la filumo <ref cel="fung.sak0oj">sakfungoj</ref>)
      kaj aliaj (ordo <nom>Peronosporales</nom>):
      <ekz>
        fine de la 19-a jarcento <tld/>o kaj vinla&ubreve;sepedemio detruis la vitejojn, kio
        ka&ubreve;zis gravan krizon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=317988">Thalheim AG</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_u;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_ja; &c_r;&c_a;&c_s;&c_a;</trd>
    <trd lng="de">Mehltau</trd>
    <trd lng="en">mildew</trd>
    <trd lng="fr">moisissure</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x767D;&#x30AB;&#x30D3;  <pr>&#x3057;&#x308D;&#x30AB;&#x30D3;</pr></trd>,
    <trd>&#x3046;&#x3069;&#x3093;&#x7C89;&#x75C5;  <pr>&#x3046;&#x3069;&#x3093;&#x3053;&#x306A;&#x3073;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x767D;&#x6E0B;&#x75C5;  <pr>&#x3057;&#x308D;&#x3057;&#x3076;&#x3073;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x456;&#x43B;&#x44C;&#x434;&#x44C;&#x44E;</trd>,
    <trd>&#x43D;&#x435;&#x441;&#x43F;&#x440;&#x430;&#x432;&#x436;&#x43D;&#x44C;&#x43E;&#x43C;&#x443;&#x447;&#x43D;&#x438;&#x441;&#x442;&#x430; &#x440;&#x43E;&#x441;&#x430; (&#x445;&#x432;&#x43E;&#x440;&#x43E;&#x431;&#x430; &#x440;&#x43E;&#x441;&#x43B;&#x438;&#x43D;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9709; <pr>m&#xE9;i</pr></trd>,
    <trd>&#x9EF4; <pr>m&#xE9;i</pr></trd>,
    <trd>&#x9709; <pr>m&#xE9;i</pr></trd>,
    <trd>&#x9709;&#x75C5; <pr>m&#xE9;ib&#xEC;ng</pr></trd>,
    <trd>&#x9EF4; <pr>m&#xE9;i</pr></trd>
  </trdgrp>
</drv>

