<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alf.xml,v 1.22 2023/08/30 18:23:32 revo Exp $">
<kap>
  <rad>alf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alf.0o">
  <kap><tld/>o</kap>
  <snc mrk="alf.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="stip.0o.genro">stipo</ref>
      kies trunketojn oni uzas por fabriki &scirc;nurojn, mata&jcirc;ojn kaj
      tre molan, ne susurantan paperon
      (<trd lng="la">Stipa tenacissima</trd>):
      <ekz>
        foliofibroj de la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=306439">Alfo (botaniko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">kavyl nejtu&zcaron;&scaron;&iacute;</trd>
    <trdgrp lng="de">
        <trd>Espartogras</trd>,
        <trd>Halfagras</trd>
      </trdgrp>
    <trd lng="en">stipa grass</trd>
    <trd lng="fr">alfa</trd>
    <trdgrp lng="hu">
      <trd>eszpart&oacute;f&udblac;</trd>,
      <trd>halfaf&udblac;</trd>,
      <trd>alfaf&udblac;</trd>,
      <trd>l&oacute;s&ouml;r&eacute;nyf&udblac;</trd>
    </trdgrp>
    <trd lng="nl">espartogras</trd>
    <trdgrp lng="pl">
      <trd>esparto</trd>,
      <trd>alfa<klr> (ro&sacute;l.)</klr></trd>
    </trdgrp>
    <trd lng="pt">esparto</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_s;&c_p;&c_a;&c_r;&c_t;&c_o;</trd>,
      <trd>&c_a;&c_l;&c_mol;&c_f;&c_a;
      <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>esparto</trd>,
      <trd>halfa</trd>
    </trdgrp>
  </snc>
  <rim>
    <ref tip="hom" cel="alf1.0o">Alfo</ref> estas anka&ubreve; la unua litero de la greka alfabeto.
  </rim>
  <trd lng="ja">&#x30A2;&#x30D5;&#x30EA;&#x30AB;&#x30CF;&#x30CD;&#x30AC;&#x30E4;</trd>
  <trd lng="uk">&#x430;&#x43B;&#x44C;&#x444;&#x430;</trd>
</drv>
</art>
