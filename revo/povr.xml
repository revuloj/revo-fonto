<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>povr</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="povr.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">POE</uzo>
    <dif>
      Videble suferanta, senhelpa, kaj pro tio <ref tip="vid"
      cel="kompat.0inda">kompatinda</ref>:
      <ekz>
        kiam mi memoras mian <tld/>an edzon, la larmoj inundas miajn
        okulojn
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 48a, p.
        662a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">pauvre<klr> (&agrave; plaindre)</klr></trd>
  <trd lng="pt">pobre</trd>
</drv>
</art>

