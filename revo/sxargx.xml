<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxargx.xml,v 1.31 2021/05/28 12:53:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ar&gcirc;</rad>/i
</kap>
<drv mrk="sxargx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.0i.KOMUNE">
    <dif>
      Surmeti peza&jcirc;on sur iun a&ubreve; ion:
      <ekz>
        <tld/>i iun per sako da terpomoj;
      </ekz>
      <ekz>
        <tld/>u viajn brutojn kaj iru
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 45:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sendis dek azenojn, <tld/>itajn per bona&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 45:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patro <tld/>is vin per peza jugo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 12:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        venkinto, <tld/>ita per trezoroj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
      <ekz>
        <tld/>i bretaron per libroj;
      </ekz>
      <ekz>
        tabloj, <tld/>itaj per man&gcirc;a&jcirc;oj, vino kaj floroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita fruktarbo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        veturiloj <tld/>itaj per la mebla&jcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">EKON</uzo>
        &scirc;uldoj, <tld/>antaj la <klr>[&#x2026;]</klr> &scirc;taton
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj elspezoj peze <tld/>is la bud&gcirc;eton;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bonvolu <tld/>i mian konton per viaj elspezoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ita letero <klr>(enhavanta monon a&ubreve;
        valora&jcirc;on)</klr>;
      </ekz>
      <ekz>
        &scirc;ipo&scirc;ar&gcirc;isto
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beladen</trd>,
      <trd>belasten</trd>,
      <trd>befrachten</trd>,
      <trd>voll stellen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cargar</trd>,
      <trd>fletar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>berak</trd>,
      <trd>felrak</trd>,
      <trd>rakodik</trd>
      <trd>megrak</trd>,
      <trd>megterhel</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxargx.0i.FIG">
    <dif>
      Premi anime:
    </dif>
    <subsnc mrk="sxargx.0i.FIG_ajxon">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Trudi al iu ion penigan, spirite pezan:
        <ekz>
          pardonu, ke mi denove <tld/>as vin per
          peto<fnt>Z</fnt>;
        </ekz>
        <ekz>
          gravaj devoj <tld/>as viajn &scirc;ultrojn;
        </ekz>
        <ekz>
          la popolo estis <tld/>ita de malfacila laboro
          <fnt>
            <bib>MT</bib>
            <lok>Ne&hcirc;emja 5:18</lok>
          </fnt>;
        </ekz>
        <ekz>
          ili estis nur homoj kaj <tld/>itaj
          (<ref tip="vid" cel="sufer.0i">suferi</ref>)
          per homaj difektoj;
        </ekz>
        <ekz>
          ili ne <tld/>u sur sin la kulpon de la krimo
          <fnt>
            <bib>MT</bib>
            <lok>Levidoj 22:16</lok>
          </fnt>;
        </ekz>
        <ekz>
          ne <tld/>u lin per laborado sklava
          <fnt>
            <bib>MT</bib>
            <lok>Levidoj 25:39</lok>
          </fnt>;
        </ekz>
        <ekz>
          vi labor<tld/>is min per viaj pekoj
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 43:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          por ne <tld/>i la memoron, estas sufi&ccirc;e nur memori la <klr>[&#x2026;]</klr>
          <fnt>
            <bib>FK</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">
&c_a;&c_b;&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">bel&auml;stigen</trd>
      <trd lng="es">cargar<klr> (sobre la conciencia)</klr></trd>
      <trdgrp lng="hu">
        <trd>terhel <klr tip="ind">(&aacute;tv.)</klr></trd>,
        <trd>megterhel <klr tip="ind">(&aacute;tv.)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="sxargx.0i.FIG_respondecon">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Trudi al iu prirespondota&jcirc;on:
        <ekz>
          du krimoj <tld/>as lin;
        </ekz>
        <ekz>
          lia konscienco estas <tld/>ita
          <klr>(devas respondi pri, estas kulpa pri)</klr>
          de mortigo
          <fnt>
            <bib>MT</bib>
            <lok>Sentencoj 28:17</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>belasten</trd>,
        <trd>lasten (auf)</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>nyom</trd>,
        <trd>terhel</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="sxargx.0i.sxargi">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="sxarg.0i">&scirc;argi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_o;&c_v;&c_a;&c_r;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_o;&c_v;&c_a;&c_r;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">carregar</trd>
  <trdgrp lng="de">
    <trd>laden</trd>,
    <trd>best&uuml;cken</trd>
  </trdgrp>
  <trd lng="en">burden</trd>
  <trd lng="es">cargar</trd>
  <trd lng="fr">charger</trd>
  <trdgrp lng="nl">
    <trd>laden</trd>,
    <trd>belasten</trd>
  </trdgrp>
  <trd lng="oc">cargar</trd>
  <trdgrp lng="pt">
    <trd>carregar</trd>,
    <trd>p&ocirc;r peso em</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxargx.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxargx.0a.KOMUNE">
    <dif>
      Peza, prema:
      <ekz>
        rolo tre flata sed tre <tld/>a
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en Barcelona en la 6a de septembro 1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiel doloras mia malfeli&ccirc;a koro &ccirc;e tiu &ccirc;i <tld/>a rememoro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i volis ekdormi, dezirante almena&ubreve; per tio &ccirc;i sin liberigi de &scirc;in premanta <tld/>a impreso de morto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        promeso estas lar&gcirc;a, plenumo estas <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>belastend</trd>,
      <trd>schwer</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_n;&c_a;&c_t;&c_o;&c_v;&c_a;&c_r;&c_v;&c_a;&c_shch;</trd>
  <trd lng="es">pesado</trd>
  <trd lng="fr">lourd &agrave; porter</trd>
</drv>

<drv mrk="sxargx.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxargx.0o.KOMUNE">
    <dif>
      Peza&jcirc;o surprenita, portata de iu a&ubreve; per io (t.e. transportilo):
      <ekz>
        sur &ccirc;eval' de najbaro la <tld/>o ne pezas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu metis la <tld/>on sur sian azenon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 44:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oportistoj
        <fnt>
          <bib>MT</bib>
          <lok>I. re&gcirc;oj 5:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o&ccirc;evalo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>veturilo</ind>
        <klr>(vd <ref tip="sin" cel="kamion.0o">kamiono</ref>)</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>.
        <trdgrp lng="de">
          <trd>Lastwagen</trd>,
          <trd>LKW</trd>
      </trdgrp>
      </ekz>
    </dif>
    <ref tip="vid" cel="port.0ajxo.KOMUNE">porta&jcirc;o</ref>
    <trd lng="de">Ladung</trd>
    <trd lng="it">carico</trd>
  </snc>
  <snc mrk="sxargx.0o.FIG">
    <dif>
      Anima premo, spirita peno.
    </dif>
    <subsnc mrk="sxargx.0o.FIG_de_ajxo">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Peno trudata al iu:
        <ekz>
          &ccirc;iu havas sian <tld/>on;
          <fnt><bib>PrV</bib></fnt>
        </ekz>
        <ekz>
          kiu do en &scirc;vito kaj en &gcirc;emoj la <tld/>on de la vivo volus porti
          <fnt>
            <bib>Hamlet</bib>
          </fnt>.
        </ekz>
      </dif>
     <trdgrp lng="de">
        <trd>M&uuml;he</trd>,
        <trd>Belastung</trd>
      </trdgrp>
      <trdgrp lng="it">
     <trd>fardello<klr> (fig.)</klr></trd>, 
     <trd>pena<klr> (fig.)</klr></trd>, 
     <trd>croce<klr> (fig.)</klr></trd>
     </trdgrp>
    </subsnc>
    <subsnc mrk="sxargx.0o.FIG_de_respondeco">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Prirespondeco trudata al iu:
        <ekz>
          se vi iros kun mi, vi estos por mi <tld/>o
          <fnt>
            <bib>MT</bib>
            <lok>II. Samuel 15:33</lok>
          </fnt>;
        </ekz>
        <ekz>
          preni sur siajn &scirc;ultrojn tiun neesprimeblan <tld/>on de necerteco kaj nedifiniteco de situacio
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          granda <tld/>o falis sur lian koron
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XV</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="honor.0iga">honoriga</ref>.
      </refgrp>
     <trd lng="it">carica<klr tip="amb">
     (responsabilit&agrave;)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="sxargx.0o.sxargo">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="sxarg.0o">&scirc;argo</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;</trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_o;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_b;&c_r;&c_je;&c_m;&c_je;</trd>,
    <trd>&c_t;&c_je;&c_zh;&c_je;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF7C;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Last</trd>,
    <trd/>,
    <trd/>,
    <trd/>
  </trdgrp>
  <trd lng="es">carga</trd>
  <trd lng="fr">charge</trd>
  <trdgrp lng="hu">
    <trd>teher</trd>,
    <trd>terhel&eacute;s</trd>,
    <trd>rakom&aacute;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;</trd>
  <trd lng="nl">lading</trd>
  <trd lng="pt">carga</trd>
</drv>

<drv mrk="sxargx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxargx.0igxi.KOMUNE">
    <dif>
      Surpreni peza&jcirc;on, surpreni respondecon:
      <ekz>
        neniu estus preta <tld/>i&gcirc;i per la defendo por <nom>Hrafnkel</nom>
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Rakonto pri Hrafnkel</vrk>,
          <lok>en: Islandaj Pravo&ccirc;oj, 1964</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>belasten</ind></trd>
  </snc>
  <trd lng="bg">&c_n;&c_a;&c_t;&c_o;&c_v;&c_a;&c_r;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trd lng="es"><ind>cargar</ind>se<klr> (de)</klr></trd>
  <trd lng="fr">se <ind>charger</ind><klr> (de)</klr></trd>
</drv>

<drv mrk="sxargx.de0i">
  <kap>de<tld/>i, <var><kap>el<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.de0i.KOMUNE">
    <dif>
      Depreni de sur portanto:
      <ekz>
        la vidvino <klr>[&#x2026;]</klr> veki&gcirc;is
        &ccirc;iutage regule pro la bruo de l' de<tld/>ataj
        laktokru&ccirc;egoj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>;
      </ekz>
      <ekz>
        kiam la brulligno estis el<tld/>ita, la patro <klr>[&#x2026;]</klr> a&ccirc;etis al si grandan faskon da angiloj
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la rivero, kelkdek homoj el<tld/>is &ccirc;e
        tor&ccirc;oj &scirc;tonojn el grandega floso
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entladen</trd>,
      <trd>abladen</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_t;&c_o;&c_v;&c_a;&c_r;&c_v;&c_a;&c_m;</trd>
  <trd lng="es">descargar</trd>
  <trd lng="fr">d&eacute;charger</trd>
  <trd lng="it">scaricare<klr> (mettere gi&ugrave;)</klr></trd>
</drv>

<drv mrk="sxargx.mal0i">
  <kap>mal<tld/>i, <var><kap>sen<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.mal0i.KOMUNE">
    <dif>
      Senigi je <tld/>o, je peza&jcirc;o:
      <ekz>
        mal<tld/>i &scirc;ipojn
        <fnt>
        <aut>H. Vallienne</aut>,
        <vrk>&Ccirc;u li?</vrk>,
        <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;barkoj revenas kaj mal<tld/>as fi&scirc;ojn kaj krustacojn
        <fnt>
          <bib>Monato</bib>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006851.html">Francio: Quimper</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la projekto celas mal<tld/>i la a&ubreve;tovojon inter Munkeno kaj la flughaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard F&ouml;ssmeier</aut>
          <vrk><url ref="https://www.monato.be/2004/008200.html">&Ccirc;u iam rapidos Transrapid?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiam la Sa&gcirc;ulo regas, li sen<tld/>igas korojn
        <fnt>
          <aut>-, trad. S. E. Kisa</aut>,
          <vrk><url ref="http://www.kisa.ca/taoismo.html">
          Da&ubreve;de&gcirc;ingo</url></vrk>,
          <lok>2000-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        refoja ekskuo <klr>[&#x2026;]</klr> sen<tld/>igis lin de &ccirc;iu cetera pripensado
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entladen</trd>,
      <trd>abladen</trd>,
      <trd>erleichtern</trd>
    </trdgrp>
  </snc>
  <trd lng="es">descargar</trd>
  <trd lng="fr">d&eacute;charger</trd>
  <trdgrp lng="it">
  <trd>scaricare<klr> (liberare dal carico)</klr></trd>
  <trd>alleggerire<klr> (scaricare)</klr></trd>
  <trd>esonerare<klr> (liberare dal incarico/impegno)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxargx.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxargx.sen0igxi.KOMUNE">
    <dif>
      Seni&gcirc;i je peza&jcirc;o, prema&jcirc;o:
      <ekz>
        la &scirc;ipo devis sen<tld/>i&gcirc;i
        <fnt><bib>NT</bib>, <lok>&Ago; 21:3</lok></fnt>;
      </ekz>
      <ekz>
        sen<tld/>igi sian konsciencon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia koro sen<tld/>i&gcirc;is
        <fnt><bib>Fab4</bib>, <lok>Infano en la tombo</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">erleichtern</trd>
  </snc>
  <trd lng="es"><ind>descargar</ind>se<klr> (de)</klr></trd>
  <trd lng="fr">se <ind>d&eacute;charger</ind><klr> (de)</klr></trd>
</drv>

<drv mrk="sxargx.tro0i">
  <kap>tro<tld/>i, <var><kap>super<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.tro0i.KOMUNE">
    <dif>
      Tre <tld/>i kun risko de rompo, renverso, frakaso...:
      <ekz>
        super<tld/>ita per laboro, per &scirc;uldoj, per impostoj;
      </ekz>
      <ekz>
