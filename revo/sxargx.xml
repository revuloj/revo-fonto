<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ar&gcirc;</rad>/i
</kap>
<drv mrk="sxargx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti pezan portota&jcirc;on sur iun:
      <ekz>
        <tld/>i iun per sako da terpomoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beladen</trd>,
      <trd>belasten</trd>,
      <trd>befrachten</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>berak</trd>,
      <trd>felrak</trd>,
      <trd>rakodik</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Meti pezan objekton sur ion:
      <ekz>
        <tld/>i tablon per man&gcirc;a&jcirc;oj, bretaron per libroj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj elspezoj peze <tld/>is la bud&gcirc;eton;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bonvolu <tld/>i mian konton per viaj elspezoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ita <klr>(enhavanta monon a&ubreve; valora&jcirc;on)</klr> letero.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beladen</trd>,
      <trd>belasten</trd>,
      <trd>voll stellen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megrak</trd>,
      <trd>megterhel</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trudi al iu ion penigan:
      <ekz>
        pardonu, ke mi denove <tld/>as vin per
        peto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        gravaj devoj <tld/>as viajn &scirc;ultrojn;
      </ekz>
      <ekz>
        la popolo estis <tld/>ita de malfacila laboro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili estis nur homoj kaj <tld/>itaj
        (<ref tip="sin" cel="sufer.0i">suferi</ref>)
        per homaj difektoj;
      </ekz>
      <ekz>
        rolo tre flata sed tre <tld/>a<fnt>Z</fnt> <klr>(peza)</klr>;
      </ekz>
      <ekz>
        super<tld/>ita per laboro, per &scirc;uldoj, per impostoj;
      </ekz>
      <ekz>
        tro<tld/>i la memoron de infano;
      </ekz>
      <ekz>
        vi labor<tld/>is min per viaj pekoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_b;&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">bel&auml;stigen</trd>
  <trdgrp lng="hu">
    <trd>terhel <klr tip="ind">(&aacute;tv.)</klr></trd>,
    <trd>megterhel <klr tip="ind">(&aacute;tv.)</klr></trd>
  </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trudi al iu prirespondota&jcirc;on:
      <ekz>
        du krimoj <tld/>as lin;
      </ekz>
      <ekz>
        lia konscienco estas <tld/>ita
        <klr>(devas respondi pri, estas kulpa pri)</klr>
        de mortigo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>belasten</trd>,
      <trd>lasten (auf)</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>nyom</trd>,
      <trd>terhel</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      (Ev.) &Scirc;argi.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trdgrp lng="de">
    <trd>laden</trd>,
    <trd>best&uuml;cken</trd>
  </trdgrp>
  <trd lng="en">burden</trd>
  <trd lng="fr">charger</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>laden</trd>,
    <trd>belasten</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>carregar</trd>,
    <trd>p&ocirc;r peso em</trd>
  </trdgrp>
</drv>

<drv mrk="sxargx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Peza&jcirc;o portata de iu:
      <ekz>
        <ind><tld/>obesto</ind>
        <klr>(besto ordinare utiligata por porti peza&jcirc;ojn)</klr>.
        <trd lng="hu">igavon&oacute; barom</trd>
        <trd lng="ru">&c_v;&c_mol;&c_ju;&c_ch;&c_n;&c_o;&c_je; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;</trd>        
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Peza&jcirc;o portata de io.
    </dif>
  </snc>
  <snc>
    <dif>
      Varoj transportataj per ia veturilo, vagono a&ubreve; &scirc;ipo.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Peno a&ubreve; prirespondeco trudata al iu:
      <ekz>
        &ccirc;iu havas sian <tld/>on
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honor.0iga">honoriga</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      &Scirc;argo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;</trd>
  <trd lng="fr">charger</trd>
  <trdgrp lng="hu">
    <trd>teher</trd>,
    <trd>terhel&eacute;s</trd>,
    <trd>rakom&aacute;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;</trd>
  <trd lng="nl">lading</trd>
  <trd lng="pt">carga</trd>
</drv>
</art>
