<drv mrk="sxargx.tro0i">
  <kap>tro<tld/>i, <var><kap>super<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.tro0i.KOMUNE">
    <dif>
      Tre <tld/>i kun risko de rompo, renverso, frakaso...:
      <ekz>
        super<tld/>ita per laboro, per &scirc;uldoj, per impostoj;
      </ekz>
      <ekz>
        kamparanoj, konsumitaj de la malsano, tro<tld/>itaj per la laboro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        tro<tld/>i boaton
        <fnt>
          <bib>Monato</bib>
          <aut>Douglas Draper</aut>
          <vrk><url ref="&Monato;/2012/010860.php">Freneza, ne; celkonscia, jes.</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tro<tld/>i la memoron de infano;
      </ekz>
      <ekz>
         la linioj havas pli ol 20 haltejojn kaj estas super<tld/>itaj
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2007/009247.php">Busaj servoj plibonigendaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne super<tld/>u la servilojn per multaj mendoj samtempe
        <fnt>
        <aut>B. Rankin, trad. M. Weichert</aut>,
        <vrk><url ref="http://www.esperanto.net/veb/retumi-retposxte.txt">
        Internet-aliro per retpo&scirc;to</url></vrk>,
        <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berladen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_v;&c_y;&c_sh; &c_m;&c_je;&c_r;&c_y;)</klr></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_a;&c_b;&c_c;&c_ja;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_t;&c_o;&c_v;&c_a;&c_r;&c_v;&c_a;&c_m;</trd>
  <trd lng="es">sobrecargar</trd>
  <trd lng="fr">surcharger</trd>
  <trd lng="it">sovraccaricare</trd>
  <trd lng="ja">&#x904E;&#21104;&#x306A;&#x8CA0;&#x62C5;&#12434;&#x304B;&#12369;&#x308B; 
  <pr>&#x304B;&#12376;&#12423;&#12358;&#x306A;&#12405;&#x305F;&#12435;&#12434;&#x304B;&#12369;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>prze&lstroke;adowa&cacute;</trd>,
    <trd>przeci&aogonek;&zdot;y&cacute;</trd>,
    <trd>przyt&lstroke;oczy&cacute;</trd>,
    <trd>zarzuci&cacute;</trd>
