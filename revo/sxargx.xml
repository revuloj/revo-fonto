<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ar&gcirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxargx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.0i.KOMUNE">
    <dif>
      Surmeti peza&jcirc;on:
    </dif>
    <subsnc mrk="sxargx.0i.iun">
      <dif>
        Meti pezan portota&jcirc;on sur iun:
        <ekz>
          <tld/>i iun per sako da terpomoj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>beladen</trd>,
        <trd>belasten</trd>,
        <trd>befrachten</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>berak</trd>,
        <trd>felrak</trd>,
        <trd>rakodik</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="sxargx.0i.ion">
      <dif>
        Meti pezan objekton sur ion:
        <ekz>
          <tld/>i tablon per man&gcirc;a&jcirc;oj, bretaron per libroj;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          tiuj elspezoj peze <tld/>is la bud&gcirc;eton;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          bonvolu <tld/>i mian konton per viaj elspezoj;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <tld/>ita <klr>(enhavanta monon a&ubreve; valora&jcirc;on)</klr> 
letero.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>beladen</trd>,
        <trd>belasten</trd>,
        <trd>voll stellen</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>megrak</trd>,
        <trd>megterhel</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="sxargx.0i.FIG_ajxon">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Trudi al iu ion penigan, spirite pezan:
        <ekz>
          pardonu, ke mi denove <tld/>as vin per
          peto<fnt>Z</fnt>;
        </ekz>
        <ekz>
          gravaj devoj <tld/>as viajn &scirc;ultrojn;
        </ekz>
        <ekz>
          la popolo estis <tld/>ita de malfacila laboro<fnt>Z</fnt>;
        </ekz>
        <ekz>
          ili estis nur homoj kaj <tld/>itaj
          (<ref tip="vid" cel="sufer.0i">suferi</ref>)
          per homaj difektoj;
        </ekz>
        <ekz>
          rolo tre flata sed tre <tld/>a<fnt>Z</fnt> <klr>(peza)</klr>;
        </ekz>
        <ekz>
          super<tld/>ita per laboro, per &scirc;uldoj, per impostoj;
        </ekz>
        <ekz>
          vi labor<tld/>is min per viaj pekoj<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trd lng="be">
&c_a;&c_b;&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">bel&auml;stigen</trd>
      <trdgrp lng="hu">
        <trd>terhel <klr tip="ind">(&aacute;tv.)</klr></trd>,
        <trd>megterhel <klr tip="ind">(&aacute;tv.)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="sxargx.0i.FIG_respondecon">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Trudi al iu prirespondota&jcirc;on:
        <ekz>
          du krimoj <tld/>as lin;
        </ekz>
        <ekz>
          lia konscienco estas <tld/>ita
          <klr>(devas respondi pri, estas kulpa pri)</klr>
          de mortigo<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>belasten</trd>,
        <trd>lasten (auf)</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>nyom</trd>,
        <trd>terhel</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="sxargx.0i.sxargi">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="sxarg.0i">&scirc;argi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>laden</trd>,
    <trd>best&uuml;cken</trd>
  </trdgrp>
  <trd lng="en">burden</trd>
  <trd lng="fr">charger</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>laden</trd>,
    <trd>belasten</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>carregar</trd>,
    <trd>p&ocirc;r peso em</trd>
  </trdgrp>
</drv>

<drv mrk="sxargx.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxargx.0o.KOMUNE">
    <dif>
      Peza&jcirc;o portata.
    </dif>
    <subsnc mrk="sxargx.0o.de_iu">
      <dif>
        Peza&jcirc;o portata de iu:
        <ekz>
          <ind><tld/>obesto</ind>
          <klr>(besto ordinare utiligata por porti peza&jcirc;ojn)</klr>.
          <trd lng="hu">igavon&oacute; barom</trd>
          <trd lng="ru">&c_v;&c_mol;&c_ju;&c_ch;&c_n;&c_o;&c_je; 
&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;</trd>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sxargx.0o.de_io">
      <dif>
        Peza&jcirc;o portata de io, ekzemple komerca&jcirc;oj transportataj 
per ia
        veturilo, vagono a&ubreve; &scirc;ipo.
      </dif>
     <trd lng="it">carico</trd>
    </subsnc>
    <subsnc mrk="sxargx.0o.FIG_de_ajxo">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Peno trudata al iu:
        <ekz>
          &ccirc;iu havas sian <tld/>on
          <fnt><bib>PrV</bib></fnt>
        </ekz>
      </dif>
     <trdgrp lng="it">
     <trd>fardello<klr> (fig.)</klr></trd>, 
     <trd>pena<klr> (fig.)</klr></trd>, 
     <trd>croce<klr> (fig.)</klr></trd>
     </trdgrp>
    </subsnc>
    <subsnc mrk="sxargx.0o.FIG_de_respondeco">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Prirespondeco trudata al iu.
      </dif>
      <refgrp tip="vid">
        <ref cel="honor.0iga">honoriga</ref>.
      </refgrp>
     <trd lng="it">carica<klr tip="amb"> (responsabilit&agrave;)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="sxargx.0o.sxargo">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="sxarg.0o">&scirc;argo</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;</trd>
  <trd lng="fr">charge</trd>
  <trdgrp lng="hu">
    <trd>teher</trd>,
    <trd>terhel&eacute;s</trd>,
    <trd>rakom&aacute;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;</trd>
  <trd lng="nl">lading</trd>
  <trd lng="pt">carga</trd>
</drv>

<drv mrk="sxargx.de0i">
  <kap>de<tld/>i, <var><kap>el<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.de0i.KOMUNE">
    <dif>
      Depreni de sur portanto:
      <ekz>
        la vidvino Stecken, kiu havis sanan dormon, veki&gcirc;is 
&ccirc;iutage
        regule pro la bruo de l' de<tld/>ataj laktokru&ccirc;egoj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>;
      </ekz>
      <ekz>
        malsupre, &ccirc;e la rivero, kelkdek homoj el<tld/>is &ccirc;e 
tor&ccirc;oj
        &scirc;tonojn el grandega floso
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;charger</trd>
  <trd lng="it">scaricare<klr> (mettere gi&ugrave;)</klr></trd>
</drv>

<drv mrk="sxargx.mal0i">
  <kap>mal<tld/>i, <var><kap>sen<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.mal0i.KOMUNE">
    <dif>
      Senigi je <tld/>o, je peza&jcirc;o:
      <ekz>
        en via loko, anstata&ubreve; mal<tld/>i &scirc;ipojn, anstata&ubreve; 
fari laboron,
        kiu vin lacegigas, kaj por kiu vi ne ta&ubreve;gas, mi denove reprenus
        mian oficon
        <fnt>
        <aut>H. Vallienne</aut>,
        <vrk>&Ccirc;u li?</vrk>,
        <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la Sa&gcirc;ulo regas, li sen<tld/>igas korojn
        <fnt>
          <aut>-, trad. S. E. Kisa</aut>,
          <vrk><url ref="http://www.kisa.ca/taoismo.html">
          Da&ubreve;de&gcirc;ingo</url></vrk>,
          <lok>2000-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;charger</trd>
  <trdgrp lng="it">
  <trd>scaricare<klr> (liberare dal carico)</klr></trd>
  <trd>alleggerire<klr> (scaricare)</klr></trd>
  <trd>esonerare<klr> (liberare dal incarico/impegno)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxargx.tro0i">
  <kap>tro<tld/>i, <var><kap>super<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.tro0i.KOMUNE">
    <dif>
      Tre <tld/>i kun risko de rompo, renverso, frakaso...:
      <ekz>
        tro<tld/>i la memoron de infano;
      </ekz>
      <ekz>
        ne super<tld/>u la servilojn per multaj mendoj samtempe
        <fnt>
        <aut>Doctor Bob Rankin, trad. M. Weichert</aut>,
        <vrk><url ref="http://www.esperanto.net/veb/retumi-retposxte.txt">
        Internet-aliro per retpo&scirc;to</url></vrk>,
        <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">surcharger</trd>
  <trd lng="it">sovraccaricare</trd>
</drv>
</art>
