<drv mrk="sxargx.mal0i">
  <kap>mal<tld/>i, <var><kap>sen<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxargx.mal0i.KOMUNE">
    <dif>
      Senigi je <tld/>o, je peza&jcirc;o:
      <ekz>
        mal<tld/>i &scirc;ipojn
        <fnt>
        <aut>H. Vallienne</aut>,
        <vrk>&Ccirc;u li?</vrk>,
        <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;barkoj revenas kaj mal<tld/>as fi&scirc;ojn kaj krustacojn
        <fnt>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="&Monato;/2000/006851.php">Quimper</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la projekto celas mal<tld/>i la a&ubreve;tovojon inter Munkeno kaj la flughaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard F&ouml;ssmeier</aut>
          <vrk><url ref="&Monato;/2004/008200.html">&Ccirc;u iam rapidos Transrapid?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiam la Sa&gcirc;ulo regas, li sen<tld/>igas korojn
        <fnt>
          <aut>-, trad. S. E. Kisa</aut>,
          <vrk><url ref="http://www.kisa.ca/taoismo.html">
          Da&ubreve;de&gcirc;ingo</url></vrk>,
          <lok>2000-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        refoja ekskuo <klr>[&#8230;]</klr> sen<tld/>igis lin de &ccirc;iu cetera pripensado
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entladen</trd>,
      <trd>abladen</trd>,
      <trd>erleichtern</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="es">descargar</trd>
  <trd lng="fr">d&eacute;charger</trd>
  <trdgrp lng="it">
  <trd>scaricare<klr> (liberare dal carico)</klr></trd>
  <trd>alleggerire<klr> (scaricare)</klr></trd>
  <trd>esonerare<klr> (liberare dal incarico/impegno)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A4D;&#33655;&#12434;&#x304A;&#x308D;&#12377;  <pr>&#12388;&#x307F;&#x306B;&#12434;&#x304A;&#x308D;&#12377;</pr></trd>,
    <trd>&#x8CA0;&#33655;&#12434;&#x306F;&#x305A;&#12377;  <pr>&#12405;&#x304B;&#12434;&#x306F;&#x305A;&#12377;</pr></trd>,
    <trd>&#x8CA0;&#x62C5;&#12434;&#38500;&#x304F;  <pr>&#12405;&#x305F;&#12435;&#12434;&#x306E;&#x305E;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">roz&lstroke;adowa&cacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&c_v;&c_a;&#x43D;&c_t;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43D;&c_ib;&#x43C;&c_a;&c_t;&c_i; &c_v;&c_a;&#x43D;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_z;&#x43D;&c_ib;&#x43C;&c_a;&c_t;&c_i; &c_t;&#x44F;&c_g;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="sxargx.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxargx.sen0igxi.KOMUNE">
    <dif>
      Seni&gcirc;i je peza&jcirc;o, prema&jcirc;o:
      <ekz>
        la &scirc;ipo devis sen<tld/>i&gcirc;i
        <fnt><bib>NT</bib>, <lok>&Ago; 21:3</lok></fnt>;
      </ekz>
      <ekz>
        sen<tld/>igi sian konsciencon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia koro sen<tld/>i&gcirc;is
