<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spalier.xml,v 1.3 2022/05/06 04:41:15 afido Exp $">
<kap>
  <rad>spalier</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="spalier.0o">
  <kap><tld/>o</kap>
  <snc mrk="spalier.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="gimnas1.0ilo.SPO">Gimnastikilo</ref>
      konsistanta el vico de horizontalaj stangoj,
      iom simila al tre lar&gcirc;a &scirc;tupetaro:
      <ekz>
        <tld/>o retrofleksanta la &scirc;ultrojn por dilatigi la torakon
        <fnt>
          <aut>M. Proust, trad. J.-L. Tortel</aut>
          <vrk>Unu amo de Swann</vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>wall bars (gymnastics)</trd>,
    <trd>gymnastic ladder</trd>,
    <trd>Swedish ladder</trd>,
    <trd>Swedish wall</trd>,
    <trd>stall bars</trd>
  </trdgrp>
  <trd lng="fr">espalier de gymnastique</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_v;&c_je;&c_d;&c_s;&c_mol;&c_k;&c_a; &c_s;&c_t;&c_ib;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_ib;&c_m;&c_n;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_a; &c_d;&c_r;&c_a;&c_b;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>

