<art mrk="$Id: dumpin.xml,v 1.25 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>dumping</rad>/o
</kap>

<drv mrk="dumpin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vend.0o">Vendo</ref> de komerca&jcirc;oj je
      prezo
      pli malalta ol la produkt- a&ubreve; akir-kosto por ruinigi
      konkurajn
      kompaniojn:
      <ekz>
        en la nuna epoko, <tld/>o estas tre disvastigita en multaj
        landoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_p;&c_ib;&c_n;&c_g;</trd>
  <trdgrp lng="cs">
    <trd>damping</trd>,
    <trd>dumping</trd>
  </trdgrp>
  <trd lng="de">Dumping</trd>
  <trd lng="en">dumping</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_z;&a_A;&a_r;&a_sh;&f_k;&a_n;&f_y;</trd>,
    <trd>&a_d;&a_A;&a_m;&f_p;&f_y;&a_n;&f_g;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dumping</trd>,
    <trd>vente &agrave; perte</trd>
  </trdgrp>
  <trd lng="hu">d&ouml;mping</trd>
  <trd lng="ja">&#x30C0;&#x30F3;&#x30D4;&#x30F3;&#x30B0;  <pr>&#x3060;&#x3093;&#x3074;&#x3093;&#x3050;</pr></trd>
  <trd lng="nl">dumping</trd>
  <trd lng="pl">dumping</trd>
  <trd lng="pt">dumping</trd>
  <trd lng="ru">&c_d;&c_je;&c_m;&c_p;&c_i;&c_n;&c_g;</trd>
  <trd lng="sk">damping</trd>
  <trd lng="uk">&#x434;&#x435;&#x43C;&#x43F;&#x456;&#x43D;&#x433;</trd>
  <trd lng="zh">&#x503E;&#x9500; <pr>q&#x12B;ngxi&#x101;o</pr></trd>
</drv>

<drv mrk="dumpin.kontraux0a">
  <kap>kontra&ubreve;<tld/>a</kap>
  <snc>
    <dif>
      Celanta &ccirc;esigi dumpingon a&ubreve; &gcirc;ian malutilan
      efekton:
      <ekz>
        kontra&ubreve;<tld/>aj rimedoj, kontra&ubreve;<tld/>a politiko.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_d;&c_e;&c_m;&c_p;&c_ib;&c_n;&c_g;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">Antidumping</trd>
  <trd lng="hu">d&ouml;mpingellenes</trd>
  <trd lng="nl">antidumping-</trd>
  <trd lng="pl">antydumpingowy</trd>
  <trd lng="pt">antidumping</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_d;&c_je;&c_m;&c_p;&c_i;&c_n;&c_g;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="zh">&#x53CD;&#x503E;&#x9500; <pr>f&#x1CE;nq&#x12B;ngxi&#x101;o</pr></trd>
</drv>

</art>
