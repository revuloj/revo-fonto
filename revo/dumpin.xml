<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dumping</rad>/o
</kap>
<drv mrk="dumpin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vend.0o">Vendo</ref> de komerca&jcirc;oj je prezo
      pli malalta ol la produkt- a&ubreve; akir-kosto por ruinigi konkurajn
      kompaniojn:
      <ekz>
        en la nuna epoko, <tld/>o estas tre disvastigita en multaj landoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_p;&c_ib;&c_n;&c_g;</trd>
  <trd lng="en">dumping</trd>
  <trd lng="de">Dumping</trd>
  <trdgrp lng="fr">
    <trd>dumping</trd>,
    <trd>vente &agrave; perte</trd>
  </trdgrp>
  <trd lng="hu">d&ouml;mping</trd>
  <trd lng="ru">&c_d;&c_je;&c_m;&c_p;&c_i;&c_n;&c_g;</trd>
</drv>
</art>
