<art mrk="$Id: kalist.xml,v 1.19 2023/08/31 19:24:09 revo Exp $">
<kap>
  <rad>kalistef</rad>/o
</kap>
<drv mrk="kalist.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Callistephus</trd>), kun multaj &gcirc;ardene 
      kulturataj varia&jcirc;oj:
      <ekz>
        la t.n. <tld/>a revolucio ricevis la nomon pro tio, ke la
        soldatoj atendantaj
        sur stacioj la envagonigon, rifuzis la obeemon kaj anstata&ubreve; la
        blazona &ccirc;apela
        insigno kaj anstata&ubreve; la butonoj, alfiksis <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=166637">Kalistefa revolucio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/08/Callistephus_or_china_aster_1959.JPG/640px-Callistephus_or_china_aster_1959.JPG" prm="CC BY-SA 3.0"><tld/>oj, Bangaloro, Barato
    
        <fnt>
          <aut>Rameshng, propra verko</aut>
          <lok><url ref="&WCW;/File:Callistephus_or_china_aster_1959.JPG">&WCW;/File:Callistephus_or_china_aster_1959.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a; &c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="ca">octubrera</trd>
  <trdgrp lng="cs">
    <trd>astra</trd>,
    <trd>hv&ecaron;zdnice zahr.</trd>
  </trdgrp>
  <trd lng="de">China-Aster</trd>
  <trd lng="en">china aster</trd>
  <trdgrp lng="fr">
    <trd>Aster de Chine</trd>,
    <trd>Reine-Marguerite</trd>
  </trdgrp>
  <trd lng="hu">&odblac;szir&oacute;zsa</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30BE;&#x30AE;&#x30AF;</trd>,
    <trd>&#x30A2;&#x30B9;&#x30BF;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">Chinese <ind>aster</ind></trd>
  <trdgrp lng="pl">
    <trd>gwiazdosz</trd>,
    <trd>aster chi&nacute;ski</trd>
  </trdgrp>   
  <trd lng="pt">cal&iacute;stefo</trd>
  <trd lng="ro">aster chinez&abreve; </trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_a;
&c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="sk">astra &ccaron;&iacute;nska</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43B;&#x456;&#x441;&#x442;&#x435;&#x444;&#x443;&#x441;</trd>,
    <trd>&#x441;&#x430;&#x434;&#x43E;&#x432;&#x430; &#x430;&#x439;&#x441;&#x442;&#x440;&#x430;</trd>
  </trdgrp>
</drv>
</art>
