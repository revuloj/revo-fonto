<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aprob</rad>/i
</kap>
<drv mrk="aprob.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aprob.0i.pense">
    <dif>
      Opinii ion bona:
      <ekz>
        tia estas ilia vojo, ilia espero, kaj iliaj posteuloj <tld/>as
        ilian opinion
        <fnt><bib>MT</bib> <lok>&Psa; 49:13</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcept.0i">akcepti</ref>,
      <ref cel="amen.0">amen</ref>,
      <ref cel="konsen.0i">konsenti</ref>
    </refgrp>
    <trd lng="de">billigen</trd>
    <trd lng="fr">approuver</trd>
    <trd lng="hu">j&oacute;v&aacute;hagy</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_d;&c_o;&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aprob.0i.parole">
    <dif>
      Deklari ion bona kaj la&ubreve;dinda:
      <ekz>
        <tld/>i agon, ideon.
      </ekz>
      <ekz>
        la a&ubreve;toro estis tiel kontenta, ke li <tld/>is &gcirc;ian publikigon
        <fnt><aut>Sten Johansson</aut>: <vrk>Epokfara metamorfozo (Franz
        Kafka, La metamorfozo)</vrk>, <bib>Monato</bib>, <lok>jaro 1997a,
        numero 6a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplaux.0i">apla&ubreve;di</ref>,
      <ref cel="lauxd.0i">la&ubreve;di</ref>
    </refgrp>
    <trd lng="de">guthei&szlig;en</trd>
    <trd lng="fr">approuver</trd>
    <trd lng="hu">helyesel</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_d;&c_o;&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="aprob.0i.oficiale">
    <dif>
      Deklari iun, post oficiala ekzameno, ta&ubreve;ga por ia posteno:
      <ekz>
        <tld/>ita profesoro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diplom.0o">diplomo</ref>,
      <ref cel="sankci.0i">sankcii</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>approbieren</trd>,
      <trd>best&auml;tigen</trd>,
      <trd>genehmigen</trd>
    </trdgrp>
    <trd lng="fr">agr&eacute;er</trd>
    <trd lng="hu">enged&eacute;lyez</trd>
    <trd lng="ru">&c_u;&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_v; &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
  </snc>
  <trd lng="en">approve</trd>
  <trd lng="es">aprobar</trd>
  <trd lng="it">approvare</trd>
  <trd lng="sk">schv&aacute;li&tcaron;</trd>
</drv>

<drv mrk="aprob.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Subtenanta, jesanta, akceptanta ion kiel bona&jcirc;on:
      <ekz>
        fari <tld/>an kapmovon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="favor.0a">favora</ref>
  </snc>
  <trd lng="fr">approbatif</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_o;&c_b;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr> (&c_o; &c_r;&c_je;&c_z;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_i;, &c_o;&c_t;&c_z;&c_y;&c_v;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="aprob.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Subtena opinio a&ubreve; deklaro:
      <ekz>
        ankora&ubreve; necesas fina <tld/>o de la E&ubreve;ropa Parlamento
        <fnt><aut>Roland Rotsaert</aut>: <vrk>Pagi ene de la E&ubreve;ropa Unio
        kostos malpli</vrk>, <bib>Monato</bib>, <lok>jaro 2002a, numero
        2a, p. 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Billigung</trd>,
      <trd>Zustimmung</trd>,
      <trd>Approbation</trd>
  </trdgrp>
  <trd lng="fr">approbation</trd>
  <trd lng="sk">schv&aacute;lenie</trd>
  <trd lng="ru">&c_o;&c_d;&c_o;&c_b;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
