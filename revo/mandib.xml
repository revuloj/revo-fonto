<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mandib.xml,v 1.25 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>mandibl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mandib.0o">
  <kap><tld/>o</kap>
  <snc mrk="mandib.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La malsupra parto de <ref tip="malprt" cel="makzel.0o">makzelo</ref>.
    </dif>
    <trd lng="be">&c_n;&c_ib;&c_zh;&c_n;&c_ja;&c_ja; <ind>&c_s;&c_k;&c_ib;&c_v;&c_ib;&c_c;&c_a;</ind></trd>
    <trd lng="bg">&c_d;&c_o;&c_l;&c_n;&c_a; &c_ch;&c_je;&c_l;&c_ju;&c_s;&c_t;</trd>
    <trd lng="de">Unterkiefer</trd>
    <trd lng="es">mand&iacute;bula<klr tip="amb"> (maxilar
    inferior)</klr></trd>
    <trd lng="hu">als&oacute; <ind>&aacute;llkapocs</ind></trd>
    <trd lng="nl">onderkaak</trd>
    <trdgrp lng="pl">
      <trd>&zdot;uchwa</trd>,
      <trd>szcz&eogonek;ka dolna <klr>(pot.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja; &c_ch;&c_je;&c_l;&c_ju;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="mandib.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la du rigidaj bu&scirc;partoj de insektoj,
      per kiuj ili kaptas, rompas, eventuale
      la&ubreve; la specioj pin&ccirc;as, prenas, luktas:
      <ekz>
        masklaj lukanoj ofte portas la vundojn de
        la grandegaj <tld/>oj de aliaj maskloj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_n;&c_d;&c_y;&c_b;&c_u;&c_l;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">mand&iacute;bula</trd>
  <trdgrp lng="cs">
    <trd>doln&#xED; &#x10D;elist</trd>,
    <trd>mandibula</trd>,
    <trd>sanice</trd>
  </trdgrp>
  <trd lng="en">mandible</trd>
  <trd lng="fr">mandibule</trd>
  <trd lng="it">mandibola</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0B;&#x3042;&#x3054;  <pr>&#x3057;&#x305F;&#x3042;&#x3054;</pr></trd>,
    <trd>&#x4E0B;&#x984E;&#x9AA8;  <pr>&#x304B;&#x304C;&#x304F;&#x3053;&#x3064;</pr></trd>,
    <trd>&#x5927;&#x984E;  <pr>&#x3060;&#x3044;&#x3042;&#x3054;</pr></trd>
  </trdgrp>
  <trd lng="pl">&zdot;uwaczka</trd>
  <trd lng="pt">mand&iacute;bula</trd>
  <trd lng="sk">spodn&#xE1; &#x10D;e&#x13E;us&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_je;&#x43B;&c_je;&#x43F;&c_a;</trd>,
    <trd>&#x43D;&c_i;&c_zh;&#x43D;&#x44F; &c_shch;&c_je;&#x43B;&c_je;&#x43F;&c_a; (&c_u; &c_h;&c_r;&c_je;&c_b;&c_je;&c_t;&#x43D;&c_i;&c_h;)</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&#x43D;&#x44F; &c_shch;&c_je;&#x43B;&c_je;&#x43F;&c_a; (&c_u; &c_ch;&#x43B;&c_je;&#x43D;&c_i;&c_s;&c_t;&#x43E;&#x43D;&#x43E;&c_g;&c_i;&c_h;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0A;&#x9837; <pr>sh&#xE0;ngh&#xE0;n</pr></trd>,
    <trd>&#x4E0A;&#x9894; <pr>sh&#xE0;ngh&#xE0;n</pr></trd>,
    <trd>&#x4E0B;&#x981C; <pr>xi&#xE0;h&#xE9;</pr></trd>,
    <trd>&#x4E0B;&#x988C; <pr>xi&#xE0;h&#xE9;</pr></trd>,
    <trd>&#x4E0B;&#x981C;&#x9AA8; <pr>xi&#xE0;h&#xE9;g&#x1D4;</pr></trd>,
    <trd>&#x4E0B;&#x988C;&#x9AA8; <pr>xi&#xE0;h&#xE9;g&#x1D4;</pr></trd>,
    <trd>&#x4E0B;&#x984E; <pr>xi&#xE0;&#xE8;</pr></trd>,
    <trd>&#x4E0B;&#x989A; <pr>xi&#xE0;&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
