<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mandib.xml,v 1.26 2025/04/02 18:50:41 revo Exp $">
<kap>
  <rad>mandibl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mandib.0o">
  <kap><tld/>o</kap>
  <snc mrk="mandib.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La malsupra parto de <ref tip="malprt" cel="makzel.0o">makzelo</ref>.
    </dif>
    <trd lng="be">&c_n;&c_ib;&c_zh;&c_n;&c_ja;&c_ja; <ind>&c_s;&c_k;&c_ib;&c_v;&c_ib;&c_c;&c_a;</ind></trd>
    <trd lng="bg">&c_d;&c_o;&c_l;&c_n;&c_a; &c_ch;&c_je;&c_l;&c_ju;&c_s;&c_t;</trd>
    <trd lng="de">Unterkiefer</trd>
    <trd lng="es">mand&iacute;bula<klr tip="amb"> (maxilar
    inferior)</klr></trd>
    <trd lng="hu">als&oacute; <ind>&aacute;llkapocs</ind></trd>
    <trd lng="nl">onderkaak</trd>
    <trdgrp lng="pl">
      <trd>&zdot;uchwa</trd>,
      <trd>szcz&eogonek;ka dolna <klr>(pot.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja; &c_ch;&c_je;&c_l;&c_ju;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="mandib.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la du rigidaj bu&scirc;partoj de insektoj,
      per kiuj ili kaptas, rompas, eventuale
      la&ubreve; la specioj pin&ccirc;as, prenas, luktas:
      <ekz>
        masklaj lukanoj ofte portas la vundojn de
        la grandegaj <tld/>oj de aliaj maskloj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_n;&c_d;&c_y;&c_b;&c_u;&c_l;&c_a;</trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malsupra makzelo</ke>.
  </rim>

  <trd lng="ca">mand&iacute;bula</trd>
  <trdgrp lng="cs">
    <trd>doln&#237; &#269;elist</trd>,
    <trd>mandibula</trd>,
    <trd>sanice</trd>
  </trdgrp>
  <trd lng="en">mandible</trd>
  <trd lng="fr">mandibule</trd>
  <trd lng="it">mandibola</trd>
  <trdgrp lng="ja">
    <trd>&#19979;&#12354;&#12372;  <pr>&#12375;&#12383;&#12354;&#12372;</pr></trd>,
    <trd>&#19979;&#38990;&#39592;  <pr>&#12363;&#12364;&#12367;&#12371;&#12388;</pr></trd>,
    <trd>&#22823;&#38990;  <pr>&#12384;&#12356;&#12354;&#12372;</pr></trd>
  </trdgrp>
  <trd lng="pl">&zdot;uwaczka</trd>
  <trd lng="pt">mand&iacute;bula</trd>
  <trd lng="sk">spodn&#225; &#269;e&#318;us&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_je;&#1083;&c_je;&#1087;&c_a;</trd>,
    <trd>&#1085;&c_i;&c_zh;&#1085;&#1103; &c_shch;&c_je;&#1083;&c_je;&#1087;&c_a; (&c_u; &c_h;&c_r;&c_je;&c_b;&c_je;&c_t;&#1085;&c_i;&c_h;)</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&#1085;&#1103; &c_shch;&c_je;&#1083;&c_je;&#1087;&c_a; (&c_u; &c_ch;&#1083;&c_je;&#1085;&c_i;&c_s;&c_t;&#1086;&#1085;&#1086;&c_g;&c_i;&c_h;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19978;&#38967; <pr>sh&#224;ngh&#224;n</pr></trd>,
    <trd>&#19978;&#39060; <pr>sh&#224;ngh&#224;n</pr></trd>,
    <trd>&#19979;&#38940; <pr>xi&#224;h&#233;</pr></trd>,
    <trd>&#19979;&#39052; <pr>xi&#224;h&#233;</pr></trd>,
    <trd>&#19979;&#38940;&#39592; <pr>xi&#224;h&#233;g&#468;</pr></trd>,
    <trd>&#19979;&#39052;&#39592; <pr>xi&#224;h&#233;g&#468;</pr></trd>,
    <trd>&#19979;&#38990; <pr>xi&#224;&#232;</pr></trd>,
    <trd>&#19979;&#39066; <pr>xi&#224;&#232;</pr></trd>
  </trdgrp>
</drv>
</art>
