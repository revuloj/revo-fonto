<vortaro>
<art mrk="$Id: eksper2.xml,v 1.29 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekspertiz</rad>/i
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eksper2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>  
  <snc mrk="eksper2.0i.JUR">
    <dif>
      Komisie <ref tip="super" cel="esplor.0i">esplori</ref> a&ubreve; 
      <ref tip="super" cel="taks.0i.KOMUNE">taksi</ref> iun aferon, kiel faka spertulo,
      ekz-e kadre de kazo jura, asekura ks:
      <ekz>
        la registaro <klr>[&#x2026;]</klr> nomumis alian komisionon, kies tasko estis <tld/>i,
        kiel stabiliga fiska politiko de la registaro kaj parlamento kompensu la
        perdon de la monpolitiko de la &scirc;tata centra banko
        <fnt>
          <bib>Monato</bib>
          <aut>Bo Sandelin</aut>
          <vrk><url ref="&Monato;/2004/008235.html">Svedoj balotis kontra&ubreve; e&ubreve;ro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni priskribis, kiel okazis &ccirc;iu unuopa el la 77 mortigoj <klr>[&#x2026;]</klr> kaj oni
        <tld/>is pri la dekstrega ideologio, kiu motivis la atencojn
        <fnt>
          <bib>Monato</bib>
          <aut>Douglas Draper</aut>
          <vrk><url ref="&Monato;/2012/010860.php">Freneza, ne; celkonscia, jes.</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        junaj muzeanoj volonte rezignas pri interesa okupo <tld/>i historiajn objektojn
        kaj unu post alia forlasas la muzeojn
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008682.php">Senmonaj muzeoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_e;&c_k;&c_s;&c_p;&c_e;&c_r;&c_t;&c_y;&c_z;&c_u;</ind></trd>
  <trdgrp lng="ca">
    <trd>peritar</trd>,
    <trd>avaluar</trd>,
    <trd>examinar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prov&eacute;st expertizu</trd>,
    <trd>znalecky vy&scaron;et&rcaron;it</trd>,
    <trd>zpracovat expertizu</trd>
  </trdgrp>
  <trd lng="de">begutachten</trd>
  <trdgrp lng="en">
    <trd>appraise</trd>,
    <trd>examine</trd>,
    <trd>survey</trd>
  </trdgrp>
  <trd lng="fr">expertiser</trd>
  <trdgrp lng="hu">
    <trd>szak&eacute;rt&odblac;i vizsg&aacute;latot folytat</trd>,
    <trd>szakv&eacute;lem&eacute;nyez</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9451;&#x5B9A;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x3066;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x67FB;&#x5B9A;&#x3059;&#x308B;  <pr>&#x3055;&#x3066;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przeprowadza&cacute; <ind>ekspertyz&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fazer <ind>vistoria</ind></trd>,
    <trd>examinar <klr tip="amb">(por meio de peritos)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
  <ind>&c_e;&c_k;&c_s;&c_p;&c_je;&c_r;&c_t;&c_i;&c_z;&c_u;</ind></trd>
  <trdgrp lng="sk">
    <trd>odborne presk&uacute;ma&tcaron;</trd>,
    <trd>pos&uacute;di&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_je;&#x43A;&c_s;&#x43F;&c_je;&c_r;&c_t;&c_i;&c_z;&c_u;</trd>
  <trdgrp lng="zh">
    <trd>&#x9274;&#x5B9A;&#x8FC7; <pr>ji&#xE0;nd&#xEC;nggu&#xF2;</pr></trd>,
    <trd>&#x8ADF; <pr>sh&#xEC;</pr></trd>,
    <trd>&#x4F3A; <pr>s&#xEC;</pr></trd>,
    <trd>&#x5BA1; <pr>sh&#x11B;n</pr></trd>,
    <trd>&#x5BA1;&#x67E5; <pr>sh&#x11B;nch&#xE1;</pr></trd>,
    <trd>&#x6537; <pr>k&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eksper2.0o">
  <kap><tld/>o</kap>
  <snc mrk="eksper2.0o.JUR">
    <dif>
      Fakula <ref tip="lst" lst="voko:komunikoj" cel="esplor.0o">esploro</ref> a&ubreve; taksado de iu
      afero la&ubreve; mendo de
      institucio a&ubreve; kompanio:
      <ekz>
        letero de la rektoro de Justo al la Scienc-Esplora Konsilio &ccirc;e Rusia
        Supera Tribunalo kun propono okazigi internacian <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Monato;/2008/009334.php">Esperanto kiel helpilo por lerni fremdajn
          lingvojn</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o por la usona parlamento pri la planata raketprotekto konkludas, ke la
        sistemo ne funkcias
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007090.php">Kanajlo-&scirc;tatoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 05</lok>
          </fnt>;
      </ekz>
      <ekz>
        la germana ministrejo pri trafiko mendis <tld/>on pri la eblo konstrui tian
        trafikilon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009349.php">Superflua rapidego</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_e;&c_r;&c_t;&c_y;&c_z;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>peritatge</trd>,
    <trd>avaluaci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>expertiza</trd>,
    <trd>znaleck&yacute; n&aacute;lez</trd>,
    <trd>znaleck&yacute; posudek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gutachten</trd>,
    <trd>Expertise</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>appraisal</trd>,
    <trd>examination</trd>,
    <trd>survey</trd>
  </trdgrp>
  <trd lng="es">esperticia</trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5DE;&#x5D7;&#x5D9;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>szak&eacute;rt&odblac;i vizsg&aacute;lat</trd>,
    <trd>szakv&eacute;lem&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9451;&#x5B9A;  <pr>&#x304B;&#x3093;&#x3066;&#x3044;</pr></trd>,
    <trd>&#x67FB;&#x5B9A;  <pr>&#x3055;&#x3066;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">ekspertyza</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_je;&c_r;&c_t;&c_i;&c_z;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dobrozdanie</trd>,
    <trd>znaleck&yacute; posudok</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&#x43A;&c_s;&#x43F;&c_je;&c_r;&c_t;&c_i;&c_z;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9274;&#x5B9A;&#x62A5;&#x544A; <pr>ji&#xE0;nd&#xEC;ngb&#xE0;og&#xE0;o</pr></trd>,
    <trd>&#x4E13;&#x5BB6;&#x610F;&#x89C1; <pr>zhu&#x101;nji&#x101;y&#xEC;ji&#xE0;n</pr></trd>,
    <trd>&#x9274;&#x5B9A; <pr>ji&#xE0;nd&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eksper2.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="eksper2.0isto.JUR">
    <dif>
      Profesia komisiito pri pretigado de <tld/>oj:
      <ekz>
        sinjoro <nom>Pro&scirc;ek</nom> &ccirc;irka&ubreve;iradas la belajn
        &ccirc;evalojn <klr>[&#x2026;]</klr> kaj observas per 
        <tld/>ista okulo iliajn korpojn kaj jungilojn
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/VIII.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[oni]</klr> informis la du <tld/>istojn de la diserta&jcirc;o kaj la
        super-regionan &jcirc;urnalon
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard F&ouml;ssmeier</aut>
          <vrk><url ref="&Monato;/2011/010330.php">Fino de ministreco: kroniko</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fak.0ulo">fakulo</ref>,
      <ref cel="specia.0isto">specialisto</ref>,
      <ref cel="eksper1.0o">eksperto</ref>,
      <ref cel="kompet.0ulo">kompetentulo</ref>,
      <ref cel="scienc.0isto">sciencisto</ref>,
      <ref cel="spert">spertulo</ref>    
    </refgrp>
  </snc>
  <rim>
    <tld lit="E"/>isto estas dungita por esplori demandon
    (ofte rilatantan kun justico, asekuroj, arto...),
    dum eksperto estas spertulo, kies konsilon oni
    volonte petas, sed malpli formale kaj en sferoj pli vastaj.
    <aut>MB</aut>
  </rim>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_e;&c_r;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>p&egrave;rit</trd>,
    <trd>avaluador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odborn&iacute;k</trd>,
    <trd>specialista</trd>,
    <trd>znalec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sachverst&auml;ndiger</trd>,
    <trd>Gutachter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>expert</trd>,
    <trd>surveyor</trd>
  </trdgrp>
  <trd lng="es">experto</trd>
  <trd lng="fr">expert</trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5BC;&#x5DE;&#x5D7;&#x5B6;&#x5D4;</trd>
  <trd lng="hu">szak&eacute;rt&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x9451;&#x5B9A;&#x4EBA;  <pr>&#x304B;&#x3093;&#x3066;&#x3044;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x67FB;&#x5B9A;&#x4EBA;  <pr>&#x3055;&#x3066;&#x3044;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bieg&lstroke;y</trd>,
    <trd>ekspert</trd>,
    <trd>znawca</trd>,
    <trd>rzeczoznawca</trd>,
    <trd>specjalista</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_je;&c_r;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>expert</trd>,
    <trd>odborn&iacute;k</trd>,
    <trd>znalec</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43A;&c_s;&#x43F;&c_je;&c_r;&c_t;</trd>,
    <trd>&c_z;&#x43D;&c_a;&c_v;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_f;&c_a;&c_h;&c_ib;&c_v;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E13;&#x4E1A;&#x4EBA;&#x58EB; <pr>zhu&#x101;ny&#xE8;r&#xE9;nsh&#xEC;</pr></trd>,
    <trd>&#x719F;&#x4E60; <pr>sh&#xFA;x&#xED;</pr></trd>
  </trdgrp>
</drv>
