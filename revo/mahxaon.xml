<vortaro>
  <art mrk="$Id: mahxaon.xml,v 1.1 2025/12/26 20:33:13 afido Exp $">
    <kap>
      <rad>ma&hcirc;aon</rad>/o
      <fnt>
          <aut>Wouter F. Pilger</aut>
          <vrk><url ref="https://www.eventoj.hu/steb/vortaroj/insektu8.htm"
              >provizora privata listo de nomoj de bestoj - insektoj</url></vrk>
        </fnt>
    </kap>

<drv mrk="mahxaon.0o">
  <kap><tld/>o</kap>
    <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="papili.0o">papilio</ref> (<trd lng="la">Papilio machaon</trd>)
      kun flugiloj helflavaj, blue-nigre kadritaj,
      kun ostformaj lobetoj kaj ru&gcirc;a okulmakulo en la ena malanta&ubreve;a parto:
      <ekz>
        ho! kia bela <tld/>o, &scirc;i ekkriis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/69/Thomas_Bresson_-_Machaon-1_(by).jpg/640px-Thomas_Bresson_-_Machaon-1_(by).jpg"
      prm="CC BY 2.0"
      ><tld/>o
        <fnt>
          <aut>Thomas Bresson </aut>
          <lok><url ref="&WCW;/File:Thomas_Bresson_-_Machaon-1_(by).jpg"
              >https://commons.wikimedia.org/wiki/File:Thomas_Bresson_-_Machaon-1_(by).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Schwalbenschwanz <klr>(Schmetterling)</klr></trd>
    <trd lng="en">yellow <ind>swallowtail</ind></trd>
    <trd lng="fr">machaon</trd>
    <trd lng="ru">&c_m;&c_a;&c_h;&c_a;&c_o;&c_n;</trd>
  </snc>
</drv>

  </art>

