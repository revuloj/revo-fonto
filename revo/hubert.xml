<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hubert.xml,v 1.7 2022/03/20 17:24:56 revo Exp $">
<kap>
  <rad>Hubert</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hubert.0o">
  <kap><tld/>o</kap>
  <snc mrk="hubert.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        vi a&ubreve;dis sendube pri Sankta <tld/>o, la sanktulo kaj
        protektanto
        de &ccirc;asistoj kaj sportistoj
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hubert</trd>,
    <trd>Hubertus</trd>
  </trdgrp>
  <trd lng="en">Hubert</trd>
  <trd lng="fr">Hubert</trd>
  <trdgrp lng="it">
    <trd>Uberto</trd>,
    <trd>Oberto</trd>
  </trdgrp>
  <trd lng="pl">Hubert</trd>
  <trd lng="pt">Hubert</trd>
  <trd lng="uk">&#x413;&#x443;&#x431;&#x435;&#x440;&#x442;</trd>
</drv>
</art>

