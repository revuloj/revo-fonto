<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hubert.xml,v 1.9 2023/11/15 17:08:02 revo Exp $">
<kap>
  <rad>Hubert</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hubert.0o">
  <kap><tld/>o</kap>
  <snc mrk="hubert.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        vi a&ubreve;dis sendube pri Sankta <tld/>o, la sanktulo kaj
        protektanto
        de &ccirc;asistoj kaj sportistoj
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_u;&c_b;&c_e;&c_r;&c_t;</trd>,
    <trd>&c_H;&c_u;&c_b;&c_e;&c_r;&c_t;</trd>,
    <trd>&c_Ju;&c_b;&c_e;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hubert</trd>,
    <trd>Hubertus</trd>
  </trdgrp>
  <trd lng="en">Hubert</trd>
  <trd lng="fr">Hubert</trd>
  <trdgrp lng="it">
    <trd>Uberto</trd>,
    <trd>Oberto</trd>
  </trdgrp>
  <trd lng="pl">Hubert</trd>
  <trd lng="pt">Hubert</trd>
  <trd lng="uk">&c_G;&c_u;&c_b;&c_je;&c_r;&c_t;</trd>
  <trd lng="zh">&#x80E1;&#x4F2F;&#x7279; <pr>h&#xFA;b&#xF3;t&#xE8;</pr></trd>
</drv>
</art>

