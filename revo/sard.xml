<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sard.xml,v 1.8 2024/03/24 19:59:59 revo Exp $">
    <kap>
      <rad>sard</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="sard.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de la insulo <tld lit="S"/>ujo, kiu situas sude de
      Korsiko:
      <ekz>
        per regiona le&gcirc;o de 1997 kaj itala le&gcirc;o de 1999 la <tld/>a
        oficiale akiris la
        samajn rajtojn kiel la itala lingvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17823">Sarda lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sardon.0a_rido">sardona rido</ref>
    <trd lng="de">Sarde</trd>
    <trd lng="fr">Sarde</trd>
    <trd lng="pl">Sardy&nacute;czyk</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_d;&c_y;&c_n;&c_je;&c_c;</trd>
  <trd lng="ja">&#x30B5;&#x30EB;&#x30C7;&#x30A3;&#x30FC;&#x30CB;&#x30E3;&#x4EBA; 
  <pr>&#x30B5;&#x30EB;&#x30C7;&#x30A3;&#x30FC;&#x30CB;&#x30E3;&#x3058;&#x3093;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_d;&c_i;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_d;&c_je;&c_r; (&c_r;&c_ib;&c_z;&#x43D;&#x43E;&c_v;&c_i;&c_d; &c_h;&c_a;&#x43B;&c_c;&c_je;&c_d;&#x43E;&#x43D;&c_u;)</trd>
  </trdgrp>
</drv>

<drv mrk="sard.S0ujo">
  <kap>
    <tld lit="S"/>ujo
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">Insulo</ref>
      en <ref tip="malprt" cel="mediterane.M0o">Mediteraneo</ref>
      sude de Korsiko:
      <ekz>
        <klr>[la&ubreve;mite]</klr> la re&gcirc;o Thespios havis kvindek filinojn
        <klr>[&#x2026;]</klr> Heraklo amoris kun &ccirc;iuj
        <klr>[kaj]</klr> havis kvindek infanojn <klr>[&#x2026;]</klr>, kvardek
        unu el ili
        fondis kolonion en <tld lit="S"/>ujo, kiam ili estis
        plenkreskuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26241">Heraklo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_r;&c_d;&c_y;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="de">Sardinien</trd>
  <trd lng="fr">Sardaigne</trd>
  <trd lng="pl">Sardynia</trd>
  <trdgrp lng="zh">
    <trd>&#x6492;&#x4E01;&#x5C9B; <pr>s&#x101;d&#x12B;ngd&#x1CE;o</pr></trd>,
    <trd>&#x6492;&#x4E01;&#x5CF6; <pr>s&#x101;d&#x12B;ngd&#x1CE;o</pr></trd>,
    <trd>&#x8428;&#x4E01;&#x5C9B; <pr>s&#xE0;d&#x12B;ngd&#x1CE;o</pr></trd>,
    <trd>&#x85A9;&#x4E01;&#x5CF6; <pr>s&#xE0;d&#x12B;ngd&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

  </art>

