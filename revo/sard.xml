<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sard.xml,v 1.3 2022/03/26 20:43:11 revo Exp $">
    <kap>
      <rad>sard</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="sard.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de la insulo <tld lit="S"/>ujo, kiu situas sude de
      Korsiko:
      <ekz>
        per regiona le&gcirc;o de 1997 kaj itala le&gcirc;o de 1999 la <tld/>a
        oficiale akiris la
        samajn rajtojn kiel la itala lingvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17823">Sarda lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sardon.0a_rido">sardona rido</ref>
    <trd lng="de">Sarde</trd>
    <trd lng="fr">Sarde</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x441;&#x430;&#x440;&#x434;</trd>,
    <trd>&#x441;&#x430;&#x440;&#x434;&#x438;&#x43D;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x441;&#x430;&#x440;&#x434;&#x435;&#x440; (&#x440;&#x456;&#x437;&#x43D;&#x43E;&#x432;&#x438;&#x434; &#x445;&#x430;&#x43B;&#x446;&#x435;&#x434;&#x43E;&#x43D;&#x443;)</trd>
  </trdgrp>
</drv>

<drv mrk="sard.S0ujo">
  <kap>
    <tld lit="S"/>ujo
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">Insulo</ref>
      
      en <ref tip="malprt" cel="mediterane.M0o">Mediteraneo</ref>
      
      sude
      de Korsiko:
      <ekz>
        <klr>[la&ubreve;mite]</klr> la re&gcirc;o Thespios havis kvindek filinojn
        <klr>[&#x2026;]</klr> Heraklo amoris kun &ccirc;iuj
        <klr>[kaj]</klr> havis kvindek infanojn <klr>[&#x2026;]</klr>, kvardek
        unu el ili
        fondis kolonion en <tld lit="S"/>ujo, kiam ili estis
        plenkreskuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26241">Heraklo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sardinien</trd>
  <trd lng="fr">Sardaigne</trd>
</drv>




  </art>

