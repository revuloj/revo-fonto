    </trdgrp>
    <trd lng="fr">bulletin d'<ind>abonnement</ind></trd>
    <trd lng="hu">megrendel&odblac;lap</trd>
    <trd lng="nl">inschrijvingsformulier<klr tip="ind"> (voor
    abonnementen)</klr></trd>
    <trdgrp lng="pl">
      <trd>formularz <ind>prenumeraty</ind></trd>,
      <trd>formularz <ind>subskrybcji</ind></trd>,
      <trd>zam&oacute;wienie</trd>
    </trdgrp>
    <trd lng="pt">formul&aacute;rio de assinatura</trd>
    <trdgrp lng="es">
      <trd>formulario de inscripci&oacute;n</trd>,
      <trd>hoja de suscripci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
      <ind>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;&c_a;</ind>
      </trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>abonentka</trd>,
    <trd>p&rcaron;edplatn&iacute; l&iacute;stek</trd>
  </trdgrp>
  <trd lng="he">&#x5D8;&#x5D5;&#x5E4;&#x5E1; &#x5D4;&#x5E8;&#x5E9;&#x5DE;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x4E88;&#x7D04;&#30003;&#x8FBC;&#x66F8;  <pr>&#12424;&#12420;&#x304F;&#12418;&#12358;&#12375;&#12371;&#x307F;&#12375;&#12423;</pr></trd>,
