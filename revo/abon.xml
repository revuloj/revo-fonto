    <trd>&#x5B9A;&#x671F;&#21033;&#29992;&#22865;&#x7D04;  <pr>&#12390;&#12356;&#x304D;&#x308A;&#12424;&#12358;&#12369;&#12356;&#12420;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">abonnement</trd>
  <trdgrp lng="pl">
    <trd>abonament</trd>,
    <trd>prenumerata</trd>,
    <trd>subskrybcja</trd>
  </trdgrp>
  <trd lng="pt">assinatura</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">uzavretie zmluvy</trd>
  <trdgrp lng="sv">
    <trd>abonnemang</trd>,
    <trd>prenumeration</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
