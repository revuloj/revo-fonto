<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>abon</rad>/i
</kap>
<drv mrk="abon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abon.0i.ricevadi">
    <dif>
      Ricevi, la&ubreve; kontrakto, gazeton a&ubreve; servon:
      <ekz>
        mi de du jaroj <tld/>as la revuon;
      </ekz>
      <ekz>
        la revuo havas multajn <ind><tld/>anto</ind>jn.
        <trd lng="fr">abonn&eacute;</trd>
        <trd lng="hu">el&odblac;fizet&odblac;</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="nl">ge<ind>abonneerd</ind> zijn op</trd>
    <trd lng="fr">&ecirc;tre <ind>abonn&eacute;</ind> (&agrave;)</trd>
    <trd lng="hu">el&odblac;fizet</trd>
    <trd lng="pt">assinar</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_p;&c_i;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">odobera&tcaron;</trd>
    <trdgrp lng="sv">
      <trd>prenumerera</trd>,
      <trd>abonnera <klr>(p&aring;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="abon.0i.kontrakti">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>F</bib></fnt>
    <ref tip="dif" cel="abon.ek0i">ek<tld/>i</ref>
    <ekz>
      la revuo havas multajn <tld/>intojn..
    </ekz>
    <rim>
      Kvankam &ccirc;i tiu senco estas la nura, kiu aperas en
      la Fundamento, &gcirc;i preska&ubreve; plene malaperis.
    </rim>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_a;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>zich <ind>abonneren</ind> op</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>s'<ind>abonner</ind> (&agrave;)</trd>,
      <trd>souscrire un <ind>abonnement</ind> (&agrave;)</trd>.
    </trdgrp>
    <trd lng="hu">megrendel</trd>
    <trd lng="pt">assinar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">predplati&tcaron; (si)</trd>
    <trd lng="sv">teckna ett <ind>abonnemang</ind>
    <klr>(p&aring;)</klr></trd>
  </snc>
  <snc mrk="abon.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ani&gcirc;i al a&ubreve; membri en komputilreta
      <ref tip="malprt" cel="forum.0o.komp">forumo</ref> a&ubreve;
      dissendolisto.
    </dif>
    <trd lng="fr">s'<ind>inscrire</ind> (&agrave;)</trd>
    <trd lng="hu">feliratkozik</trd>
    <trd lng="pt">inscrever-se</trd>
    <trd lng="nl">zich <ind>inschrijven</ind> op</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_n;&c_a; &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ju;
    &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <trd lng="de">abonnieren</trd>
  <trd lng="en">subscribe</trd>
  <trd lng="it">abbonarsi <klr>(a)</klr></trd>
  <trd lng="sk">prihl&aacute;si&tcaron; sa</trd>
</drv>

<drv mrk="abon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tiucela kontrakto:
      <ekz>
        la prezo de la <tld/>o estas multekosta.
      </ekz>
    </dif>
    <trd lng="en">subscription</trd>
  </snc>
  <snc>
    <dif>
      Da&ubreve;ro de tiu kontrakto:
      <ekz>
        via <tld/>o fini&gcirc;as en majo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_k;&c_a;</trd>
  <trd lng="nl">abonnement</trd>
  <trd lng="de">Abonnement</trd>
  <trd lng="en">subscription</trd>
  <trd lng="fr">abonnement</trd>
  <trd lng="hu">el&odblac;fizet&eacute;s</trd>
  <trd lng="it">abbonamento</trd>
  <trd lng="pt">assinatura</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>abonnemang</trd>,
    <trd>prenumeration</trd>
  </trdgrp>
</drv>

<drv mrk="abon.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <ref tip="dif" cel="abon.0i.ricevadi"><tld/>i
      <sncref ref="abon.0i.ricevadi"/></ref>:
      <ekz>
        <tld/>igu al mi lo&gcirc;ion en la Operejo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">jemanden etwas <ind>abonnieren lassen</ind></trd>
  <trd lng="fr">abonner</trd>
  <trd lng="hu">kib&eacute;rel</trd>
  <trd lng="nl">abonneren</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="abon.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="formul.0aro.radika">Formularo</ref> por
      aboni <klr>(plenigita a&ubreve; neplenigita)</klr>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_a;&c_n;&c_e;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k; <ind>&c_a;&c_b;&c_a;&c_n;&c_e;&c_m;&c_e;&c_n;&c_t;&c_u;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bestellformular</trd>,
      <trd>Bestellzettel</trd>
    </trdgrp>
    <trd lng="en">subscription form</trd>
    <trd lng="fr">bulletin d'<ind>abonnement</ind></trd>
    <trd lng="hu">megrendel&odblac;lap</trd>
    <trd lng="nl">inschrijvingsformulier<klr tip="ind"> (voor
    abonnementen)</klr></trd>
    <trd lng="pt">formul&aacute;rio de assinatura</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k; <ind>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;&c_a;</ind></trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k; <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="abon.ek0i">
  <kap>ek<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Kontrakti por ricevi, dum difinita tempo, gazeton
      a&ubreve; servon.
    </dif>
    <ref tip="sin" cel="abon.0i.kontrakti"><tld/>i <sncref
    ref="abon.0i.kontrakti"/>.</ref>
  </snc>
  <trd lng="de">abonnieren</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>abonner</ind> (&agrave;)</trd>,
    <trd>souscrire un <ind>abonnement</ind> (&agrave;)</trd>.
  </trdgrp>
  <trd lng="hu">megrendel</trd>
  <trd lng="nl">een <ind>abonnement</ind> nemen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="abon.mal0i">
  <kap>mal<tld/>i</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      &Ccirc;esigi sian <tld/>on.
    </dif>
    <trd lng="de">abbestellen</trd>
    <trd lng="en">unsubscribe</trd>
    <trd lng="fr">se <ind>d&eacute;sabonner</ind></trd>
    <trd lng="hu">lemond <klr>(megrendel&eacute;st)</klr></trd>
    <trd lng="nl">opzeggen<klr tip="ind"> (abonnement)</klr></trd>
    <trd lng="pt">cancelar a <ind>assinatura</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_o;&c_t;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>,
      <trd>&c_a;&c_n;&c_n;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_o;&c_t;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_v; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_je;)</klr></trd>
    </trdgrp>
    <trd lng="sk">odhl&aacute;si&tcaron;</trd>
  </snc>
</drv>
</art>
