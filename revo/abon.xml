<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>abon</rad>/i
</kap>
<drv mrk="abon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kontrakti por ricevi, anta&ubreve;e paginte, dum difinita tempo,
      gazeton a&ubreve; servon:
      <ekz>
        <tld/>igu al mi lo&gcirc;ion en la Operejo;
      </ekz>
      <ekz>
        la revuo havas multajn <tld/>intojn.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>zich <ind>abonneren</ind></trd>,
      <trd>zich <ind>abonneren</ind> op</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>s'<ind>abonner</ind> <klr>(&agrave;)</klr></trd>,
      <trd>souscrire un <ind>abonnement</ind>
<klr>(&agrave;)</klr></trd>.
    </trdgrp>
    <trd lng="pt">assinar</trd>
    <trdgrp lng="ru">
     
<trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     
<trd>&c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">teckna ett <ind>abonnemang</ind> <klr>(p&aring;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Ricevi, la&ubreve; kontrakto, gazeton a&ubreve; servon:
      <ekz>
        mi de du jaroj <tld/>as la revuon;
      </ekz>
      <ekz>
        la revuo havas multajn <tld/>&c_a;ntojn.
      </ekz>
    </dif>
    <trd lng="nl">geabonneerd zijn op</trd>
    <trd lng="fr">&ecirc;tre <ind>abonn&eacute;</ind>
<klr>(&agrave;)</klr></trd>
    <trd lng="pt">assinar</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_p;&c_i;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
     
<trd>&c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
        <trd>prenumerera</trd>,
        <trd>abonnera <klr>(p&aring;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ani&gcirc;i al a&ubreve; membri en komputilreta
      <ref tip="malprt" cel="forum.0o.komp">forumo</ref> a&ubreve;
      dissendolisto.
    </dif>
    <trd lng="fr">s'<ind>inscrire</ind> <klr>(&agrave;)</klr></trd>
    <trd lng="pt">inscrever-se</trd>
    <trd
lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <klr>(&c_n;&c_a;
&c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ju;
    &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <trd lng="de">abonnieren</trd>
  <trd lng="en">subscribe</trd>
  <trd lng="it">abbonarsi <klr>(a)</klr></trd>
</drv>
<drv mrk="abon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiucela kontrakto:
      <ekz>
        la prezo de la <tld/>o estas multekosta.
      </ekz>
    </dif>
    <trd lng="en">subscription</trd>
    <trd lng="pt">assinatura</trd>
  </snc>
  <snc>
    <dif>
      Da&ubreve;ro de tiu kontrakto:
      <ekz>
        via <tld/>o fini&gcirc;as en majo.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">abonnement</trd>
  <trd lng="de">Abonnement</trd>
  <trd lng="en">subscription</trd>
  <trd lng="fr">abonnement</trd>
  <trd lng="it">abbonamento</trd>
  <trd lng="pt">assinatura</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sv">
        <trd>abonnemang</trd>,
        <trd>prenumeration</trd>
  </trdgrp>
</drv>
<drv mrk="abon.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="formul.0aro.radika">Formularo</ref> por
      aboni <klr>(plenigita a&ubreve; neplenigita)</klr>.
    </dif>
    <trd lng="fr">bulletin d'<ind>abonnement</ind></trd>
    <trd lng="pt">formul&aacute;rio de assinatura</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
     
<ind>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;&c_a;</ind></trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="abon.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      &Ccirc;esigi sian <tld/>on.
    </dif>
    <trd lng="fr">se <ind>d&eacute;sabonner</ind></trd>
    <trd lng="pt">cancelar a assinatura</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;
&c_o;&c_t;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>,
     
<trd>&c_a;&c_n;&c_n;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_u;</ind></trd>
    </trdgrp>
  </snc>
</drv>
</art>
