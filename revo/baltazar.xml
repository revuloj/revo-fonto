<art mrk="$Id: baltazar.xml,v 1.12 2023/10/29 19:11:00 revo Exp $">
<kap>
  <rad>Baltazar</rad>/o <fnt>
  <bib>PIV1</bib></fnt>
</kap>

<drv mrk="baltazar.0o">
  <kap><tld/>o</kap>
  <snc mrk="baltazar.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> akaddevena:
      <ekz>
        Antifolo Efeza estas for&scirc;losita el la propra domo, en
        &ccirc;eesto de la urbaj kolegoj Angelo kaj <tld/>o
        <fnt>
          <aut>H. Tonkin</aut>,
          <vrk><url ref="http://esperanto.org/uk2000/Tonkin.html">
          Shakespeare tutmonda</url></vrk>,
          <lok>UK 2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Tago de la Tri Re&gcirc;oj Melkioro, Gasparo kaj <tld/>o
        <fnt>
          <aut>Rubeno</aut>,
          <vrk>Novjara Artikoleto</vrk>,
          <lok><url ref="http://mobigen.com/~hiongun/homepage/LA">
          La Lanterno Azia</url>, 2005-07-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5DF4;&#x5C14;&#x7279;&#x8428; <pr>b&#x101;&#x11B;rt&#xE8;s&#xE0;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_z;&c_a;&c_r;</trd>,
    <trd>&c_B;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_s;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Balthazar</trd>
  <trd lng="fr">Balthazar</trd>
  <trd lng="hu">Baltaz&aacute;r</trd>
  <trd lng="nl">Balthazar</trd>
  <trd lng="pl">Baltazar</trd>
  <trd lng="pt">Baltazar</trd>
  <trd lng="ru">&c_B;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_z;&c_a;&c_r;</trd>
  <trdgrp lng="uk">
    <trd>&#x411;&#x430;&#x43B;&#x442;&#x430;&#x437;&#x430;&#x440;</trd>,
    <trd>&#x412;&#x430;&#x43B;&#x442;&#x430;&#x441;&#x430;&#x440;&#xA0;(&#x456;&#x43C;&#x2019;&#x44F;)</trd>
  </trdgrp>
</drv>
</art>

