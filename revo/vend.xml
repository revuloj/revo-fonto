  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Granda podetala <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      kiu ofertas tre diversajn viv-utilajn komerca&jcirc;ojn,
      aran&gcirc;itajn la&ubreve; fakoj kaj eta&gcirc;oj:
      <ekz>
        Anjon oni arestis je la momento kiam &scirc;i provis enmanigi
        al kasistino de la &ccirc;io<tld/>ejo &ccirc;e <nom>Arbat</nom> dekdolaran
        bileton
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 27.</lok></fnt>;
      </ekz>
      <ekz>
        la kosto de &scirc;teloj en grandaj &ccirc;io<tld/>ejoj atingis 4,6 miliardojn da e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2008/009473.php">&Scirc;telo i&gcirc;as kutimo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        iuj traser&ccirc;as rubujojn de &ccirc;io<tld/>ejoj kaj vivas de for&jcirc;etitaj sed ankora&ubreve;
        konsumeblaj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2012/010623.php">For&jcirc;etu la dezirojn, ne la pirojn</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bazar.super0o">superbazaro</ref>,
      <ref cel="magaze.0ego">magazenego</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_ib;&c_v;&c_e;&c_r;&c_m;&c_a;&c_g;</trd>
  <trd lng="ca">supermercat</trd>
  <trdgrp lng="de">
    <trd>Supermarkt</trd>,
    <trd>Kaufhaus</trd>
  </trdgrp>
  <trd lng="en">department store</trd>
  <trdgrp lng="fr">
    <trd>grand magasin</trd>,
    <trd>bazar</trd>,
    <trd>&eacute;picerie</trd>,
    <trd>superette</trd>,
    <trd>supermarch&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ABC-&aacute;ruh&aacute;z</trd>,
    <trd>vegyeskeresked&eacute;s</trd>,
    <trd>vegyesbolt</trd>
  </trdgrp>
  <trd lng="nl">supermarkt</trd>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_m;&c_a;&c_g;</trd>
</drv>

<drv mrk="vend.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sin malembarasi je io per <tld/>o,
      eventuale je malalta prezo a&ubreve; por detruo:
      <ekz>
        tiu firmao fine devis rezigni, kaj &gcirc;i for<tld/>is sian PC-procesoran sekcion
        <klr>[&#x2026;]</klr> rabate
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007114.html">Intel alfrontas konkurencon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        markitan tekokomputilon oni ja malpli facile for<tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Rob Ebenau</aut>
          <vrk><url ref="https://www.monato.be/2005/008528.php">Kiel protekti kontra&ubreve; perdo kaj &scirc;telo?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne volas for<tld/>i &gcirc;in, tamen estas pli bone <tld/>i &gcirc;in ol vidi &gcirc;in disfali
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2007/009263.php">Nova mono por malnovaj domoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aluminiaj platoj estis for<tld/>itaj por kunfinanci la rekonstruon
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2008/009517.php">Feli&ccirc;an datrevenon!</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="vend.for0i.tute">
    <dif>
      <tld lit="V"/>i komplete, la tutan stokon:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi estis jam kun mia ha&ubreve;to <klr>[&#x2026;]</klr> for<tld/>ita al la
        anatomio
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>
        <klr>(ekzekutota)</klr>;
      </ekz>
      <ekz>
        tiuj lastaj for<tld/>as sian tutan ri&ccirc;a&jcirc;on, nur por 
        liberigi siajn hejmulojn
        <fnt><aut>Bardhyl Selimi</aut>: <vrk>Voja&gcirc;impresoj,
        somero 2000</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a,
        p.26a</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas nur 16-pa&gcirc;a kaj nigrablanka, sed eldoni&gcirc;is en trioble pli granda kvanto
        kaj fulmrapide for<tld/>i&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2003/008082.html">Kiam arestoj pruvas terorismajn ligojn ...</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>malvendre</trd>,
    <trd>liquidar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odprodat</trd>,
    <trd>zpen&ecaron;&zcaron;it</trd>
  </trdgrp>
  <trd lng="de">verkaufen</trd>
  <trdgrp lng="fr">
    <trd>brader</trd>,
    <trd>bazarder</trd>,
    <trd>liquider</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&aacute;rus&iacute;t</trd>,
    <trd>p&eacute;nzz&eacute; tesz</trd>
  </trdgrp>
  <trd lng="nl">verkopen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odpreda&tcaron;</trd>
  <trd lng="tr">ucuz fiyatla elden &ccedil;&inodot;karmak</trd>
</drv>

<drv mrk="vend.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="V"/>i ion, kion oni a&ccirc;etis:
      <ekz>
        malhaveblas kartoj, &ccirc;ar kelkaj homoj akiras kiel eble plej multajn por poste
        re<tld/>i
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="https://www.monato.be/2015/011562.php">Vifio - ankora&ubreve; defio</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej multaj retservoj simple re<tld/>as sub sia aparta nomo la
        servon de pli granda ret- a&ubreve; telefon-firmao
        <fnt>
        <aut>Edmund Grimley Evans</aut>: <vrk>Interreto senpage</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 23a</lok>
        </fnt>;
      </ekz>
      <ekz>
        re<tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Jacques Tuinder</aut>
          <vrk><url ref="https://www.monato.be/2011/010356.php">Fonda&jcirc;o E-vid-E-nt-E jam 45 jarojn aktivadas</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">revendre</trd>
  <trd lng="cs">kupovat z druh&eacute; ruky</trd>
  <trd lng="de">wiederverkaufen</trd>
  <trd lng="el">&my;&epsilon;&tau;&alfa;&pi;&omikron;&ypsilon;&lambda;&omega_ton;</trd>
  <trd lng="es">revender</trd>
  <trd lng="fr">revendre</trd>
  <trdgrp lng="hu">
    <trd>&uacute;jra elad</trd>,
    <trd>&uacute;jra &eacute;rt&eacute;kes&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">herverkopen</trd>
  <trd lng="pt">revender</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_r;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kupova&tcaron; z druhej ruky</trd>
  <trd lng="tr">geri satmak</trd>
</drv>

<drv mrk="vend.super0ejo">
  <kap>super<tld/>ejo, <var><kap><tld/>ejego</kap></var></kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="bazar.super0o">Superbazaro</ref>:
      <ekz>
        mia edzino butikumis en super<tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007170.html">Ni hontas kaj postulas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la super<tld/>ejoj trovi&gcirc;as precipe en la &ccirc;efurboj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2008/009346.php">Butikumanta Litovio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antaro de la urba distrikto, kie devis elkreski la
        super<tld/>ejo, &gcirc;in ne deziras
        <fnt><aut>Antanas Grincevi&ccaron;ius</aut>:
        <vrk>Interpu&scirc;i&gcirc;ado en eksterlanda merkato</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 7a, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> kantas sub pontoj kaj en <tld/>ejegoj
        <fnt>
          <bib>Monato</bib>
          <aut>R&ouml;ssler Franz-Georg</aut>
          <vrk><url ref="https://www.monato.be/2011/010305.php">Skulpta&jcirc;oj kantataj</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_e;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
  <trd lng="ca">supermercat</trd>
  <trdgrp lng="de">
    <trd>Supermarkt</trd>,
    <trd>Kaufhaus</trd>
  </trdgrp>
  <trd lng="fr">supermarch&eacute;</trd>
  <trd lng="hu">szpermarket</trd>  
  <trd lng="nl">supermarkt</trd>
  <trd lng="ru">&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
</drv>

