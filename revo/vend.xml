<drv mrk="vend.rabat0i">
  <kap>
    rabat<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="V"/>i kun rabato:
      <ekz>
        la magazeno komencos grandan rabat<tld/>adon, kaj oni bezonos &ccirc;iun metron
        <fnt>
          <bib>KrM</bib>
          <lok>28. Tute Libera Kiel Birdo Aera.</lok>
        </fnt>;
      </ekz>
      <ekz>
        koleginoj enoficejaj malkovris rabat<tld/>adon de viraj puloveroj
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malpaco malinstigas turismon kaj ka&ubreve;zas gravajn ekonomiajn perdojn dum la
        novjaraj rabat<tld/>adoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2013/010959.php">Malpaco flagranta</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vend.dis0i">disvendi</ref>,
      <ref cel="vend.for0i.tute">forvendi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_s;&c_a; &c_z;&c_mol;&c_n;&c_ib;&c_zh;&c_k;&c_a;&c_j;</trd>
  <trdgrp lng="de">
    <trd>ausverkaufen</trd>,
    <trd>eine <ind>Rabattaktion</ind> durchf&uuml;hren</trd>
  </trdgrp>
  <trd lng="en">sell off</trd>
</drv>

