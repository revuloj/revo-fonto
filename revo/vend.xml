<drv mrk="vend.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="V"/>i ion, kion oni a&ccirc;etis:
      <ekz>
        malhaveblas kartoj, &ccirc;ar kelkaj homoj akiras kiel eble plej multajn por poste
        re<tld/>i
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="https://www.monato.be/2015/011562.php"
              >Vifio - ankora&ubreve; defio</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej multaj retservoj simple re<tld/>as sub sia aparta nomo la
        servon de pli granda ret- a&ubreve; telefon-firmao
        <fnt>
        <aut>Edmund Grimley Evans</aut>: <vrk>Interreto senpage</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 23a</lok>
        </fnt>;
      </ekz>
      <ekz>
        re<tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Jacques Tuinder</aut>
          <vrk><url ref="https://www.monato.be/2011/010356.php"
              >Fonda&jcirc;o E-vid-E-nt-E jam 45 jarojn aktivadas</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_p;&c_je;&c_r;&c_a;&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">revendre</trd>
  <trd lng="cs">kupovat z druh&eacute; ruky</trd>
  <trd lng="de">wiederverkaufen</trd>
  <trd 
lng="el">&my;&epsilon;&tau;&alfa;&pi;&omikron;&ypsilon;&lambda;&omega_ton;</trd>
  <trd lng="es">revender</trd>
  <trd lng="fr">revendre</trd>
  <trdgrp lng="hu">
    <trd>&uacute;jra elad</trd>,
    <trd>&uacute;jra &eacute;rt&eacute;kes&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">herverkopen</trd>
  <trd lng="pt">revender</trd>
  <trd 
lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_r;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kupova&tcaron; z druhej ruky</trd>
  <trd lng="tr">geri satmak</trd>
</drv>

<drv mrk="vend.super0ejo">
  <kap>super<tld/>ejo, <var><kap><tld/>ejego</kap></var></kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="bazar.super0o">Superbazaro</ref>:
      <ekz>
        mia edzino butikumis en super<tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007170.html"
              >Ni hontas kaj postulas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la super<tld/>ejoj trovi&gcirc;as precipe en la &ccirc;efurboj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2008/009346.php"
              >Butikumanta Litovio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antaro de la urba distrikto, kie devis elkreski la
        super<tld/>ejo, &gcirc;in ne deziras
        <fnt><aut>Antanas Grincevi&ccaron;ius</aut>:
        <vrk>Interpu&scirc;i&gcirc;ado en eksterlanda merkato</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 7a, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> kantas sub pontoj kaj en <tld/>ejegoj
        <fnt>
          <bib>Monato</bib>
          <aut>R&ouml;ssler Franz-Georg</aut>
          <vrk><url ref="https://www.monato.be/2011/010305.php"
              >Skulpta&jcirc;oj kantataj</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_s;&c_u;&c_p;&c_e;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
  <trd lng="ca">supermercat</trd>
  <trdgrp lng="de">
    <trd>Supermarkt</trd>,
    <trd>Kaufhaus</trd>
  </trdgrp>
  <trd lng="fr">supermarch&eacute;</trd>
  <trd lng="hu">szpermarket</trd>  
  <trd lng="nl">supermarkt</trd>
  <trd 
lng="ru">&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
</drv>

