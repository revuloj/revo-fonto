<vortaro>
  <art mrk="$Id: vindhuk.xml,v 1.7 2024/05/19 11:18:32 afido Exp $">
    <kap>
      <rad>Vindhuk</rad>/o
    </kap>

<drv mrk="vindhuk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="namibi.N0o">Namibio</ref>:
      <ekz>
        <tld/>o estis fondita en 1890 <klr>[&#x2026;]</klr>, la originala nomo, kiu signifas
        &leftquot;varmega fonto&rightquot;, indikas ke iam ekzistis varmaj fontoj en la regiono
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Windhoek">Vindhuko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Sud-Afrika Disvolva kaj Kunordiga Konferenco okazigis kunvenon en la namibia
        &ccirc;efurbo <tld/>o
        <fnt>
          <vrk><url ref="&CRI;1/2005/08/01/1@27647.htm">Sud-Afrika Disvolva Komunumo</url></vrk>
          <lok>China Radio International, Jul 6a, 2020</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_V;&c_ib;&c_n;&c_d;&c_g;&c_u;&c_k;</trd>
    <trd lng="de">Windhoek</trd>
    <trd lng="en">Windhoek</trd>
    <trd lng="fr">Windhoek</trd>
    <trd lng="nl">Windhoek</trd>
  </snc>
  <trd lng="ja">&#x30A6;&#x30A3;&#x30F3;&#x30C8;&#x30D5;&#x30C3;&#x30AF;</trd>
  <trd lng="uk">&c_V;&c_ib;&#x43D;&c_d;&c_g;&c_u;&#x43A; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F; &#x41D;&c_a;&#x43C;&c_ib;&c_b;&c_ib;&c_ji;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6E29;&#x5F97;&#x548C;&#x514B; <pr>W&#x113;nd&#xE9;h&#xE9;k&#xE8;</pr></trd>,
    <trd>&#x6EAB;&#x5F97;&#x548C;&#x514B; <pr>W&#x113;nd&#xE9;h&#xE9;k&#xE8;</pr></trd>
  </trdgrp>
</drv>

