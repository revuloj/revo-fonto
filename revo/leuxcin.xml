<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>le&ubreve;cin</rad>/o
    </kap>

<drv mrk="leuxcin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIO</uzo><uzo tip="fak">KEM</uzo>
  <mlg>Leu</mlg>
  <mlg>L</mlg>
  <snc>
    <dif>
      <frm>CH<sub>3</sub>-CH2-CH(CH<sub>3</sub>)-CH(NH<sub>2</sub>)-COOH</frm>,
      en sia L-formo esenca <ref tip="super"
      cel="acid.amin0o">aminacido</ref>,
      kristali&gcirc;anta en blankaj (nomdona) folietoj:
      <ekz>
        uracilo-uracilo-adenino en RNA-mesa&gcirc;ista &ccirc;eno igas la ribosomon
        aldoni <tld/>on
        al la konstruata proteina molekulo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007157.html"
              >VIVO: Riboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alanino, <tld/>o, prolino kaj lizerga acido estas uzataj en &ccirc;elo
        por produkti
        ergozinon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=410663"
              >Ergozino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Leucin</trd>
    <trd lng="pl">leucyna</trd>     
    <trd lng="ro">leucin&abreve;</trd>
  </snc>
</drv>

  </art>

