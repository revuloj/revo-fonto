<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vind.xml,v 1.30 2016/11/21 21:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vind</rad>/i
</kap>
<drv mrk="vind.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vind.0i.volvi">
    <dif>
      &Ccirc;irka&ubreve;volvi iun, precipe su&ccirc;infanon a&ubreve; mortinton,
      per longa tuko:
      <ekz>
        &scirc;i naskis sian unuan filon kaj &scirc;i
        &ccirc;irka&ubreve;<tld/>is
        lin kaj ku&scirc;igis lin en staltrogon
        <fnt><bib>NT</bib>, <lok>&Luk; 2:6</lok></fnt>;
      </ekz>
      <ekz>
        la junuloj levi&gcirc;is kaj lin &ccirc;irka&ubreve;<tld/>is
        kaj forportis kaj enterigis
        <fnt><bib>NT</bib>, <lok>&Ago; 5:6</lok></fnt>;
      </ekz>
      <ekz>
        ili do prenis la korpon de Jesuo kaj &ccirc;irka&ubreve;<tld/>is
        &gcirc;in per tola&jcirc;oj kun aroma&jcirc;oj la&ubreve;
        la kutimo de la Judoj por entombigo
        <fnt><bib>NT</bib>, <lok>&Joh; 19:40</lok></fnt>;
      </ekz>
      <ekz>
        statuo de la dio Ptah <tld/>ita de l' piedoj &gcirc;is la brusto
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 21</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>embolcallar</trd>,
      <trd>cobrir <klr>(amb tela)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>wickeln</trd>,
      <trd>einwickeln</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>empa&ntilde;ar <klr>(poner pa&ntilde;al, o envolver a un beb&eacute;)</klr></trd>,
      <trd>amotarjar</trd>
    </trdgrp>
    <trd lng="fr">langer</trd>
    <trdgrp lng="hu">
      <trd>bep&oacute;ly&aacute;z</trd>,
      <trd>becsavar</trd>,
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zawija&cacute;</trd>,
      <trd>owija&cacute;</trd>,
      <trd>spowija&cacute;</trd>
    </trdgrp>
    <trd lng="pt">envolve</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vind.0i.higieno">
    <dif>
      Vesti la malsupran korpoparton de su&ccirc;infano
      a&ubreve; persono ne kapabla uzi necesejon per
      tuko a&ubreve; kalsonsimila sorba peco por reteni la
      pisa&jcirc;on kaj feka&jcirc;on.
    </dif>
    <trd lng="ca">bolcar</trd>
    <trdgrp lng="de">
      <trd>windeln</trd>,
      <trd>wickeln</trd>
    </trdgrp>
    <trd lng="es">empa&ntilde;ar <klr>(poner pa&ntilde;al)</klr></trd>
    <trd lng="fr">changer les <ind>couches</ind></trd>
    <trd lng="hu">bepelenk&aacute;z</trd>
    <trd lng="pl">przewija&cacute;</trd>
    <trd lng="pt">p&ocirc;r fralda</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vind.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      &Ccirc;irka&ubreve;volvi korpoparton por kuraci, senmovigi,
      senfunkciigi a&ubreve; protekti &gcirc;in:
      <ekz>
        <tld/>i la vunditan brakon;
      </ekz>
      <ekz>
        <tld/>i la okulojn al iu.
      </ekz>
    </dif>
    <trd lng="ca">embenar</trd>
    <trdgrp lng="de">
      <trd>verbinden</trd>,
      <trd>umwickeln</trd>
    </trdgrp>
    <trd lng="en">bandage</trd>
    <trd lng="es">vendar</trd>
    <trdgrp lng="fr">
      <trd>bander</trd>,
      <trd>panser</trd>
    </trdgrp>
    <trd lng="hu">bep&oacute;ly&aacute;z</trd>
    <trdgrp lng="pl">
      <trd>owija&cacute;</trd>,
      <trd>banda&zdot;owa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">enfaixar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <ref tip="super" cel="vest.0i">vesti</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_u;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>swathe</trd>,
    <trd>swaddle</trd>,
    <trd>wrap</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omwikkelen</trd>,
    <trd>inbakeren</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obviaza&tcaron;</trd>,
    <trd>ovin&uacute;&tcaron;</trd>,
    <trd>zavin&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obv&aacute;zat</trd>,
    <trd>pov&iacute;t</trd>,
    <trd>v&iacute;t</trd>,
    <trd>zavinout</trd>,
    <trd>zav&aacute;zat</trd>
  </trdgrp>
</drv>

<drv mrk="vind.0o">
  <kap><tld/>o</kap>
  <snc>
      <dif>
        <ref tip="dif" cel="vind.0ajxo"><tld lit="V"/>a&jcirc;o</ref>:
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <ind>li ne elrampis ankora&ubreve; el la <tld/>oj</ind>
          <klr>(li estas sensperta)</klr>
          <fnt><bib>PrV</bib></fnt>;
          <trd lng="ca">encara hi est&agrave; a les beceroles</trd>
          <trd lng="hu">m&eacute;g ott a toj&aacute;sh&eacute;j a fenek&eacute;n</trd>
        </ekz>
        <ekz>
          Teksa&jcirc;aj vindoj estas elfaritaj el teksa&jcirc;o &#8208; 
          kotono, bambuo a&ubreve; lino, <klr>[...]</klr> laveblaj kaj refoje
          uzeblaj
          <fnt>
            <bib>Viki</bib>
            <lok><url
            ref='https://eo.wikipedia.org/?curid=92776'>Vindotuko</url></lok>
          </fnt>.
        </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>bandage</trd>,
    <trd>dressing</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_l;&c_ju;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_ib;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obvaz</trd>,
    <trd>plena</trd>,
    <trd>plenka</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Windel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>venda</trd>,
    <trd>pa&ntilde;al</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&oacute;lya</trd>,
    <trd>pelenka</trd>
  </trdgrp>
  <trd lng="nl">windsel</trd>
  <trdgrp lng="pl">
    <trd>pielucha</trd>,
    <trd>powijak</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_i;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obv&auml;z</trd>,
    <trd>plienka</trd>
  </trdgrp>
</drv>

<drv mrk="vind.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vind.0ajxo.volvajxo">
    <dif>
      Longa tuko a&ubreve; &scirc;tofa strio, per kiu oni
      <tld/>as <sncref ref="vind.0i.volvi"/>:
      <ekz>
        Sara per propraj manoj lavis la <tld/>a&jcirc;ojn de la filo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi estis naskita oni ne detran&ccirc;is vian umbilikon oni ne
        lavis vin per akvo kaj ne <tld/>is per <tld/>a&jcirc;oj
        <fnt><bib>MT</bib>, <lok>Je&hcirc;ezkel 16:4</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>binding</trd>,
      <trd>wrapping</trd>
    </trdgrp>
    <trd lng="ca">bena</trd>
    <trdgrp lng="es">
      <trd>mortaja</trd>,
      <trd>venda</trd>,
      <trd>sudario</trd>
    </trdgrp>
    <trd lng="fr">lange</trd>
    <trdgrp lng="hu">
      <trd>p&oacute;lya</trd>,
      <trd>k&ouml;t&eacute;s</trd>
    </trdgrp>
    <trd lng="pl">zaw&oacute;j</trd>
    <trd lng="ru">&c_p;&c_je;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="vind.0ajxo.higieno">
    <dif>
      Tuko a&ubreve; alia <ref tip="super" cel="vest.0o">vesto</ref>
      uzata por <tld/>i <sncref ref="vind.0i.higieno"/>.
    </dif>
    <trdgrp lng="en">
      <trd>swaddle</trd>,
      <trd>diaper</trd>
    </trdgrp>
    <trd lng="ca">bolquers</trd>
    <trd lng="es">pa&ntilde;al</trd>
    <trd lng="fr">couches<klr> (hygi&eacute;niques)</klr></trd>
    <trd lng="hu">pelenka</trd>
    <trd lng="pl">pielucha</trd>
    <trd lng="pt">fralda</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_i;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="sv">bl&ouml;ja</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_l;&c_ju;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_ib;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Wickel</trd>,
      <trd>Wickelband</trd>,
      <trd>Windel</trd>
  </trdgrp>
  <trd lng="nl">zwachtel</trd>
</drv>

<drv mrk="vind.0otuko">
  <kap><tld/>otuko</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="vind.0ajxo"><tld lit="V"/>a&jcirc;o</ref>:
      <ekz>
        kiam Reo revenis al Krono, <klr>[&scirc;i]</klr> donis al li anstata&ubreve; novnaskito &scirc;tonon &ccirc;irka&ubreve;volvitan per <tld/>otuko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=134833'>Adrastejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>
  <trd lng="en">swaddling cloth</trd>
  <trd lng="be">&c_p;&c_ja;&c_l;&c_ju;&c_sh;&c_k;&c_a;</trd>
  <trd lng="ca">bolquer</trd>
  <trdgrp lng="cs">
    <trd>obvaz</trd>,
    <trd>plena</trd>,
    <trd>plenka</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Wickeltuch</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mortaja</trd>,
    <trd>pa&ntilde;al</trd>,
    <trd>venda</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&oacute;lya</trd>,
    <trd>k&ouml;t&eacute;s</trd>,
    <trd>pelenka</trd>
  </trdgrp>
  <trd lng="nl">luier</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>obv&auml;z</trd>,
    <trd>plienka</trd>
  </trdgrp>
  <trd lng="sv">bl&ouml;ja</trd>
</drv>

<drv mrk="vind.0otablo">
  <kap><tld/>otablo <fnt><bib>HejmVort</bib></fnt></kap>
  <snc>
    <dif>
      Tablo a&ubreve; simila <ref tip="lst" lst="voko:mebloj" cel="mebl.0o">meblo</ref>, 
      sur kiun oni ku&scirc;igas
      su&ccirc;infanon por purigi kaj vindi &gcirc;in:
      <ekz>
        unika kvaza&ubreve; estas vindotablo <klr>[por]</klr> beboj baptotaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=566822'>Kirko
          Jesuo (Kirschkau)</url></lok>
        </fnt>.
      </ekz>
    </dif> 

  </snc>
  <trd lng="en">swaddling table</trd>
  <trd lng="ca">canviador <klr>(per a nadons)</klr></trd>
  <trdgrp lng="de">
    <trd>Wickeltisch</trd>,
    <trd>Wickelkommode</trd>
  </trdgrp>
  <trd lng="fr">table &agrave; langer</trd>
  <trd lng="hu">pelenk&aacute;z&oacute;asztal</trd>
  <trd lng="nl">wikkeltafel</trd>
  <trd lng="pl">przewijak</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_t;&c_o;&c_l;&c_i;&c_k;</trd>
  <trd lng="sv">sk&ouml;tbord</trd>
</drv>
</art>
