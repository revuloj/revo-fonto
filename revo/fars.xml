<art mrk="$Id: fars.xml,v 1.25 2025/04/28 22:29:12 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fars</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="fars.0o">
  <kap><tld/>o</kap>
  <snc mrk="fars.0o.TEA">
  <uzo tip="fak">TEA</uzo>
    <dif>
      Mallonga, vulgare komika <ref tip="super" cel="teatr.0ajxo">teatra&jcirc;o</ref>:
      <ekz>
        tripa&gcirc;a tre amuza <tld/>eto<fnt>
        <aut>Donald Broadribb</aut>:
        <vrk>Utilaj por distraj klubkunvenoj (Maurice Baring, Arthur
        Hopkins, Kvin etaj humuraj dramoj)</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 30a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burles.0o">burlesko</ref>,
    </refgrp>
    <ref tip="super" cel="komedi.0o.TEA">komedio</ref>
  <trd lng="be">&c_f;&c_a;&c_r;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Posse</trd>,
    <trd>Schwank</trd>
  </trdgrp>
  <trd lng="en">farce <klr>(theater)</klr></trd>
  <trd lng="es">farsa</trd>
  <trd lng="fr">farce<klr tip="ind"> (pi&egrave;ce comique)</klr></trd>
  <trd lng="hu">boh&oacute;zat</trd>
  <trd lng="nl">farce<klr tip="ind"> (dwaas toneelstuk)</klr></trd>
  <trdgrp lng="pl">
    <trd>farsa</trd>,
    <trd>krotochwila</trd>
  </trdgrp>
  <trd lng="pt">farsa</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_s;</trd>
    <trdgrp lng="zh">
    <trd>&#33618;&#35628;&#21487;&#31505;&#30340;&#26481;&#35199; <pr>hu&amacron;ngmi&ugrave;k&ecaron;xi&agrave;od&igrave;d&omacron;ngxi</pr></trd>,
    <trd>&#24618;&#35477;&#30340;&#20107;&#29289; <pr>gu&agrave;id&agrave;ndesh&igrave;w&ugrave;</pr></trd>,
    <trd>&#28369;&#31293;&#21095; <pr>hu&aacute;j&imacron;j&ugrave;</pr></trd>,
    <trd>&#36259;&#21095; <pr>q&ugrave;j&ugrave;</pr></trd>,
    <trd>&#38393;&#21095; <pr>n&agrave;oj&ugrave;</pr></trd>,
    <trd>&#28369;&#31293; <pr>g&#468;j&imacron;</pr></trd>,
    <trd>&#32993;&#38393; <pr>h&uacute;n&agrave;o</pr></trd>
  </trdgrp>
</snc>

  <snc mrk="fars.0o.KOM">
   <dif>
      Ridinda konduto:
      <ekz>
        tri tagojn da&ubreve;ris tiu <tld/>o, tiu hontinda&jcirc;o
        <fnt>
        <aut>Trevor Steele</aut>:
        <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>,
        <lok>p. 92</lok></fnt>;
      </ekz>
      <ekz>
        la brita fervoj-<tld/>o fari&gcirc;as &ccirc;iutage pli fantazia
         <fnt><aut>Paul Gubbins</aut>, <vrk>Fervoja farso</vrk>,
         <bib>Monato</bib>, <lok>1995/06, p. 14</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_f;&c_a;&c_r;&c_s;</trd>,
       <trd>&c_f;&c_ib;&c_g;&c_l;&c_ja;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
     </trdgrp>
     <trd lng="en">farce <klr>(conduct)</klr></trd>
     <trdgrp lng="nl">
       <trd>farce</trd>,
       <trd>schertsvertoning</trd>
     </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>farce</trd>,
    <trd>fars</trd>,
    <trd>fra&scaron;ka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31505;&#21127;  <pr>&#12375;&#12423;&#12358;&#12370;&#12365;</pr></trd>,
    <trd>&#12501;&#12449;&#12523;&#12473;</trd>,
    <trd>&#33590;&#30058;&#21127;  <pr>&#12385;&#12419;&#12400;&#12435;&#12370;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="sk">fra&scaron;ka</trd>
  <trd lng="uk">&c_f;&c_a;&c_r;&c_s;</trd>
</drv>
</art>
