<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fars.xml,v 1.21 2023/08/31 13:11:27 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fars</rad>/o
</kap>
<drv mrk="fars.0o">
  <kap><tld/>o</kap>
  <snc mrk="fars.0o.TEA">
  <uzo tip="fak">TEA</uzo>
    <dif>
      Mallonga, vulgare komika <ref tip="super" cel="teatr.0ajxo">teatra&jcirc;o</ref>:
      <ekz>
        tripa&gcirc;a tre amuza <tld/>eto<fnt>
        <aut>Donald Broadribb</aut>:
        <vrk>Utilaj por distraj klubkunvenoj (Maurice Baring, Arthur
        Hopkins, Kvin etaj humuraj dramoj)</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 30a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burles.0o">burlesko</ref>,
    </refgrp>
    <ref tip="super" cel="komedi.0o.TEA">komedio</ref>
  <trd lng="be">&c_f;&c_a;&c_r;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Posse</trd>,
    <trd>Schwank</trd>
  </trdgrp>
  <trd lng="en">farce</trd>
  <trd lng="es">farsa</trd>
  <trd lng="fr">farce<klr tip="ind"> (pi&egrave;ce comique)</klr></trd>
  <trd lng="hu">boh&oacute;zat</trd>
  <trd lng="nl">farce<klr tip="ind"> (dwaas toneelstuk)</klr></trd>
  <trdgrp lng="pl">
    <trd>farsa</trd>,
    <trd>krotochwila</trd>
  </trdgrp>
  <trd lng="pt">farsa</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_s;</trd>
  </snc>
  
  <snc mrk="fars.0o.KOM">
   <dif>
      Ridinda konduto:
      <ekz>
        tri tagojn da&ubreve;ris tiu <tld/>o, tiu hontinda&jcirc;o 
        <fnt>
        <aut>Trevor Steele</aut>:
        <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>,
        <lok>p. 92</lok></fnt>;
      </ekz>
      <ekz>
        la brita fervoj-<tld/>o fari&gcirc;as &ccirc;iutage pli fantazia
         <fnt><aut>Paul Gubbins</aut>, <vrk>Fervoja farso</vrk>,   
         <bib>Monato</bib>, <lok>1995/06, p. 14</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_f;&c_a;&c_r;&c_s;</trd>,
       <trd>&c_f;&c_ib;&c_g;&c_l;&c_ja;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>farce</trd>,
       <trd>schertsvertoning</trd>
     </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>farce</trd>,
    <trd>fars</trd>,
    <trd>fra&scaron;ka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7B11;&#x5287;  [<ind>&#x3057;&#x3087;&#x3046;&#x3052;&#x304D;</ind>]</trd>,
    <trd>&#x30D5;&#x30A1;&#x30EB;&#x30B9;</trd>,
    <trd>&#x8336;&#x756A;&#x5287;  [<ind>&#x3061;&#x3083;&#x3070;&#x3093;&#x3052;&#x304D;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">fra&scaron;ka</trd>
  <trd lng="uk">&#x444;&#x430;&#x440;&#x441;</trd>
</drv>
</art>
