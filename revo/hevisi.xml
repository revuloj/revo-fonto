<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Hevisid</rad>/o
</kap>
<drv mrk="hevisi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Anglalingve:
      Oliver <trd lng="en">Heaviside</trd>, 1850-1925. Angla matematikisto
      kaj fizikisto.
    </dif>
    <rim>
      Ni spontane esperantigis la nomo al <ctl>Hevisajdo</ctl>, sed &ccirc;ar
      <ctl><tld/>o</ctl> havas a&ubreve;toritatan fonton, ne valoras krei
      duobla&jcirc;on.
    </rim>
  </snc>
  <trd lng="de">Heaviside</trd>
  <trd lng="fr">Heaviside</trd>
  <trd lng="nl">Oliver <ind>Heaviside</ind></trd>
  <trd lng="ru">&c_H;&c_je;&c_v;&c_i;&c_s;&c_a;&c_j;&c_d;</trd>
</drv>

<drv mrk="hevisi.h0a">
  <kap><tld lit="h"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="funkci.hevisijda0o"><tld lit="h"/>a funkcio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Heavisidesche</trd>
  <trd lng="en">Heaviside['s]</trd>
  <trd lng="fr">de <ind>Heaviside</ind></trd>
  <trd lng="nl">van <ind>Heaviside</ind></trd>
  <trd lng="ru">&c_H;&c_je;&c_v;&c_i;&c_s;&c_a;&c_j;&c_d;&c_a;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>