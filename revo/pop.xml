<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>pop</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="pop.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Neformala sinonimo por ortodokskristana
          <ref tip="super" cel="pastr.0o.presbitero">pastro</ref>
          (<ref tip="super" cel="presbi.0o.duagrada">presbitero</ref>).
        </dif>
        <ref tip="vid" cel="ortodo.0a.kristanismo">ortodoksa kristanismo</ref>
      </snc>
      <trd lng="de">Pope</trd>
      <trd lng="en">pope</trd>
      <trd lng="gr">&pi;&alfa;&pi;&alfa_ton;&sigma_fina;</trd>
      <trd lng="nl">pope</trd>
      <trd lng="ru">&c_p;&c_o;&c_p;</trd>
    </drv>

  </art>
