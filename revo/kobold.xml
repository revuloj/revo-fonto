<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kobold</rad>/o
</kap>
<drv mrk="kobold.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fabela esta&jcirc;o gardanta la trezorojn de la montoj. 
    </dif>
    <refgrp tip="sin">
      <ref cel="gnom.0o">gnomo</ref>,
      <ref cel="fe.0o">feo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kobold</trd>
  <trdgrp lng="en">
    <trd>(hob)goblin</trd>,
    <trd>gnome</trd>,
    <trd>imp</trd>,
    <trd>evil sprite</trd>
  </trdgrp>
  <trd lng="nl">kabouter</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_b;&c_o;&c_l;&c_mol;&c_d;</trd>,
    <trd>&c_l;&c_je;&c_sh;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_n;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
</art>
