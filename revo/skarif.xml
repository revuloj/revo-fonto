<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skarif.xml,v 1.21 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>skarif</rad>/i <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="ik">skarifik</rad>/i <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="skarif.0i">
  <kap><tld/>i, <var><kap><tld var="ik"/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skarif.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tratran&ccirc;eti la ha&ubreve;ton por kuraca a&ubreve; ornama celo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        violkolora fulmo <tld/>is la sufoke koaguli&gcirc;intan aeron
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>tk-stafeto, 1982</lok></fnt>;      
      </ekz>
      <ekz>
        li praktikis unu el siaj profesioj &mdash; barbiradon
        <klr>[&#x2026;]</klr> kaj anka&ubreve; li sciis <tld var="ik"/>i, se necesis
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tatu.0i">tatui</ref>
    <bld lok="&WCU;/thumb/3/38/Suri_Scarification.jpg/370px-Suri_Scarification.jpg" prm="CC BY-SA     2.0">tradicia surmaa <tld/>o, Valo Omo, Etiopujo
        <fnt>
          <aut>Rod Waddington el Kergunyah, A&ubreve;stralio</aut>
          <lok><url ref="&WCW;/File:Suri_Scarification.jpg">&WCW;/File:Suri_Scarification.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_r;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pou&scaron;t&ecaron;t &zcaron;ilou</trd>,
    <trd>skarifikovat</trd>
  </trdgrp>
  <trd lng="de">skarifizieren</trd>
  <trd lng="en">to scarify</trd>
  <trd lng="fr">scarifier</trd>
  <trdgrp lng="ja">
    <trd>&#x4E71;&#x5207;&#x3059;&#x308B;  <pr>&#x3089;&#x3093;&#x305B;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x304B;&#x304D;&#x307B;&#x3050;&#x3059;</trd>
  </trdgrp>
  <trd lng="nl">schrapen</trd>
  <trd lng="pl">skaryfikowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_r;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nareza&tcaron; ko&zcaron;u</trd>,
    <trd>skarifikova&tcaron;</trd>,
    <trd>urobi&tcaron; z&aacute;rez</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_d;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_ib;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_a;&c_r;&c_i;&c_f;&c_ib;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_a;&c_r;&c_i;&c_f;&c_ib;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_gu;&c_r;&c_u;&#x43D;&c_t;</trd>
  </trdgrp>
</drv>
</art>
