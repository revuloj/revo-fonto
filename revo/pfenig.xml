<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pfenig</rad>/o
</kap>
<drv mrk="pfenig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Iama germana monereto, centono de marko:
      <ekz>
        la nuna disketo kostas nur <tld/>ojn<fnt><aut>Evgeni
        Georgiev</aut>: <vrk>Zipo minacas la mikrodisketon</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 7a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="centim.0o">centimo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Pfennig</trd>
  <trdgrp lng="fr">
    <trd>pfennig</trd>
  </trdgrp>
  <trd lng="en">pfenig</trd>
  <trd lng="nl">pfennig</trd>
  <trd lng="ru">&c_p;&c_f;&c_je;&c_n;&c_n;&c_i;&c_g;</trd>
</drv>
</art>
