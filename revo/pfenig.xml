<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pfenig</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pfenig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pfenig.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.monasub0o">Mona subunuo</ref>, egala al
      centono de germana
      <ref tip="malprt" cel="mark2.0o.EKON">marko</ref>;
      tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        la nuna disketo kostas nur <tld/>ojn<fnt><aut>E.
        Georgiev</aut>, <vrk>Zipo minacas la mikrodisketon</vrk>,
        <bib>Monato</bib>, <lok>jaro 1998a, numero 7a, p.
        17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_f;&c_e;&c_n;&c_ib;&c_g;</trd>
  <trd lng="ca">pfening</trd>
  <trd lng="de">Pfennig</trd>
  <trd lng="fr">pfennig</trd>
  <trd lng="en">pfenig</trd>
  <trd lng="hu">pfennig</trd>
  <trd lng="nl">pfennig</trd>
  <trd lng="pt">f&ecirc;nigue</trd>
  <trd lng="ru">&c_p;&c_f;&c_je;&c_n;&c_n;&c_i;&c_g;</trd>
</drv>
</art>
