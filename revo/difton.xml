<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
    <ofc>1</ofc>
    <rad>diftong</rad>/o
    </kap>

    <drv mrk="difton.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">FON</uzo>
      <snc mrk="difton.0o.propre">
	<dif>
           Malsimpla <ref tip="super" cel="vokal.0o">vokalo</ref>
           konsistanta el du partoj, unu silaba (la <em>kerno</em> de <tld/>o)
           kaj unu nesilaba;
           &gcirc;ia artikolado komenci&gcirc;as en la pozicio de unu simpla
           vokalo, kaj fini&gcirc;as en la artikolada pozicio de alia simpla
           vokalo; ekz-e <em>a&ubreve;</em> en Esperanto.
        </dif>
	<ref tip="ant" cel="dierez.0o.FON">dierezo</ref>
      </snc>
      <trd lng="en">diphthong</trd>
      <trdgrp lng="de">
       <trd>Zwielaut</trd>,
       <trd>Diphtong</trd>
     </trdgrp>
      <trd lng="es">diptongo</trd>
      <trd lng="fr">diphtongue</trd>
      <trd lng="ru">&c_d;&c_i;&c_f;&c_t;&c_o;&c_n;&c_g;</trd>
    </drv>
  </art>
