<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>livre</rad>/o
</kap>
<drv mrk="livre.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Distingiga vesto, ordinare kolora kaj galonita, kiun portas la
      servistoj de sama domo a&ubreve; mastro:
      <ekz>
        vestita per griza <tld/>a jako
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 3a, p. 16a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_w;&c_r;&c_e;&c_ja;</trd>
  <trd lng="de">Livree</trd>
  <trd lng="el">&lambda;&jota;&beta;&rho;&epsilon_ton;&alfa;</trd>
  <trd lng="en">livery</trd>
  <trdgrp lng="fr">
    <trd>livr&eacute;e<klr> (uniforme)</klr></trd>,
    <trd>uniforme<klr> (livr&eacute;e)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lak&aacute;jruha</trd>,
    <trd>inasruha</trd>,
    <trd>lib&eacute;ria</trd>
  </trdgrp>
  <trd lng="nl">livrei</trd>
  <trd lng="pt">libr&eacute;</trd>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_r;&c_je;&c_ja;</trd>
</drv>
</art>
