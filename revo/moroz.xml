<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: moroz.xml,v 1.14 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>moroz</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="moroz.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Videble <ref tip="super" cel="gaj.mal0a">malgaj</ref>humora:
      <ekz>
        &ccirc;u la vizitinto tro drinkis, &ccirc;u li estis <tld/>a, distrita
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/007079.html">Inter la dek plej bonaj romanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Jakobo estas tie morozulo, da&ubreve;re nubmiena kaj vort&scirc;parema
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="konten.mal0a">malkontenta</ref>,
       <ref cel="grumbl.0ema">grumblema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_a;&c_v;&c_malmol;&c_s;&c_je;&c_n;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mor&oacute;zn&iacute;</trd>,
    <trd>mrzut&yacute;</trd>,
    <trd>nevrl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&uuml;rrisch</trd>,
    <trd>verstimmt</trd>,
    <trd>sauer <klr>(verstimmt)</klr></trd>
  </trdgrp>
  <trd lng="en">morose</trd>
  <trd lng="fr">morose</trd>
  <trd lng="nl">somber</trd>
  <trdgrp lng="pl">
    <trd>kostyczny</trd>,
    <trd>zgry&zacute;liwy</trd>,
    <trd>o kwa&sacute;nym humorze</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>carrancudo</trd>,
    <trd>rabugento</trd>,
    <trd>sorumb&aacute;tico</trd>    
  </trdgrp>
  <trdgrp lng="ro">
    <trd>slab</trd>,
    <trd>acru</trd>,
    <trd>cu o stare acr&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mrzut&yacute;</trd>,
    <trd>nev&lcaron;&uacute;dny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x445;&#x43C;&#x443;&#x440;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x43D;&#x443;&#x440;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x443;&#x43C;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="moroz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Malgaja <ref tip="super" cel="humor.0o.animstato">humoro</ref>:
      <ekz>
        &scirc;i ne tro mal&gcirc;oju, nek falu en malsanon pro da&ubreve;ra <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        384-385a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mor&oacute;znost</trd>,
    <trd>mrzutost</trd>,
    <trd>nevrlost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&uuml;rrischkeit</trd>,
    <trd>Verstimmtheit</trd>
  </trdgrp>
  <trd lng="fr">morosit&eacute;</trd>
  <trdgrp lng="pl">
    <trd>pos&eogonek;pno&sacute;&cacute;</trd>,
    <trd>ponuro&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;ntunecime</trd>,
    <trd>jale</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mor&oacute;znos&tcaron;</trd>,
    <trd>mrzutos&tcaron;</trd>
  </trdgrp>
</drv>
</art>
