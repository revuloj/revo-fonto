<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: moroz.xml,v 1.20 2025/09/03 12:57:09 revo Exp $">
<kap>
  <rad>moroz</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="moroz.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Videble <ref tip="super" cel="gaj.mal0a">malgaj</ref>humora:
      <ekz>
        &ccirc;u la vizitinto tro drinkis, &ccirc;u li estis <tld/>a, distrita
        <fnt>
          <aut>Donald Broadribb</aut>
            <vrk><url ref="&Monato;/2000/007079.php">Inter la dek plej bonaj romanoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        Jakobo estas tie morozulo, da&ubreve;re nubmiena kaj vort&scirc;parema
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="konten.mal0a">malkontenta</ref>,
       <ref cel="grumbl.0ema">grumblema</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malgaja</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_a;&c_v;&c_malmol;&c_s;&c_je;&c_n;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>trist</trd>,
    <trd>malenconi&oacute;s</trd>,
    <trd>melangi&oacute;s</trd>,
    <trd>abatut</trd>,
    <trd>decaigut</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mor&oacute;zn&iacute;</trd>,
    <trd>mrzut&yacute;</trd>,
    <trd>nevrl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&uuml;rrisch</trd>,
    <trd>verstimmt</trd>,
    <trd>sauer <klr>(verstimmt)</klr></trd>
  </trdgrp>
  <trd lng="en">morose</trd>
  <trd lng="fr">morose</trd>
  <trdgrp lng="id">
    <trd>buram</trd>,
    <trd>masygul</trd>,
    <trd>muram</trd>,
    <trd>murung</trd>,
    <trd>sedih</trd>,
    <trd>suram</trd>
  </trdgrp>
  <trd lng="nl">somber</trd>
  <trdgrp lng="pl">
    <trd>kostyczny</trd>,
    <trd>zgry&zacute;liwy</trd>,
    <trd>o kwa&sacute;nym humorze</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>carrancudo</trd>,
    <trd>rabugento</trd>,
    <trd>sorumb&aacute;tico</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>slab</trd>,
    <trd>acru</trd>,
    <trd>cu o stare acr&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mrzut&yacute;</trd>,
    <trd>nev&lcaron;&uacute;dny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_h;&c_m;&c_u;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_u;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36208;&#35519; <pr>z&#466;udi&agrave;o</pr></trd>,
    <trd>&#36208;&#35843; <pr>z&#466;udi&agrave;o</pr></trd>,
    <trd>&#19981;&#39640;&#20852;&#22320; <pr>b&ugrave;g&amacron;ox&igrave;ngde</pr></trd>,
    <trd>&#19981;&#39640;&#33288;&#22320; <pr>b&ugrave;g&amacron;ox&igrave;ngde</pr></trd>,
    <trd>&#21741;&#20007;&#30528;&#33080; <pr>k&umacron;sangzheli&#462;n</pr></trd>,
    <trd>&#21741;&#21930;&#33879;&#33225; <pr>k&umacron;sangzheli&#462;n</pr></trd>,
    <trd>&#33086;&#27668;&#22351; <pr>p&iacute;q&igrave;hu&agrave;i</pr></trd>,
    <trd>&#33086;&#27683;&#22750; <pr>p&iacute;q&igrave;hu&agrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="moroz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Malgaja <ref tip="super" cel="humor.0o.animstato">humoro</ref>:
      <ekz>
        &scirc;i ne tro mal&gcirc;oju, nek falu en malsanon pro da&ubreve;ra <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        384-385a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_a;&c_s;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tristor</trd>,
    <trd>malenconia</trd>,
    <trd>melangia</trd>,
    <trd>abatiment</trd>,
    <trd>deca&iuml;ment</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mor&oacute;znost</trd>,
    <trd>mrzutost</trd>,
    <trd>nevrlost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&uuml;rrischkeit</trd>,
    <trd>Verstimmtheit</trd>
  </trdgrp>
  <trd lng="fr">morosit&eacute;</trd>
  <trdgrp lng="id">
    <trd><baz>buram</baz>keburaman</trd>,
    <trd><baz>masygul</baz>kemasygulan</trd>,
    <trd><baz>muram</baz>kemuraman</trd>,
    <trd><baz>murung</baz>kemurungan</trd>,
    <trd><baz>sedih</baz>kesedihan</trd>,
    <trd><baz>suram</baz>kesuraman</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>morose</trd>,
    <trd>droefgeestigheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pos&eogonek;pno&sacute;&cacute;</trd>,
    <trd>ponuro&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;ntunecime</trd>,
    <trd>jale</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mor&oacute;znos&tcaron;</trd>,
    <trd>mrzutos&tcaron;</trd>
  </trdgrp>
</drv>
</art>
