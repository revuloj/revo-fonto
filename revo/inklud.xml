<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inklud.xml,v 1.27 2025/12/05 22:34:33 revo Exp $">
<kap>
  <rad>inklud</rad>/i
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="inklud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="inklud.0i.inkluzivigi">
    <dif>
      <ref tip="dif" cel="inkluz.0igi">Inkluzivigi</ref>:
      <ekz>
        ne &ccirc;iuj leksikografoj <tld/>is &gcirc;in en vortaroj<klr>[n]</klr>
        <fnt><aut>Bernard Golden</aut>:
          <vrk><url ref="&Monato;/1996/005300.php">Katamito: seksologia faktermino</url></vrk>,
          <bib>Monato</bib>
          <lok>1996/12, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la misiistoj lastatempe volas <tld/>i e&ccirc; la gardantojn en la
        rondon familian &ndash; &ccirc;u pro kristana amo, &ccirc;u pro taktiko
        <fnt>
          <aut>Sten Johansson</aut>: <vrk>Garantiulo</vrk>.
          <lok>En: Denaska kongresano, ISBN 91-7303-070-0, p.
          11a.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="don.al0i">aldoni</ref>,
      <ref cel="al.0igi">aligi</ref>,
      <ref cel="implic.0i.iu">implici<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; &c_u; &c_s;&c_ja;&c_b;&c_je;</trd>,
      <trd>&c_u;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einschlie&szlig;en</trd>,
      <trd>einbeziehen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>inclure<klr tip="ind"> (comporter)</klr></trd>,
      <trd>comporter<klr tip="ind"> (inclure)</klr></trd>,
      <trd>comprendre<klr tip="ind"> (inclure)</klr></trd>,
      <trd>contenir<klr tip="ind"> (inclure)</klr></trd>,
      <trd>renfermer<klr tip="ind"> (inclure)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bevesz <klr tip="amb">(vmi k&ouml;z&eacute;)</klr></trd>,
      <trd>belevesz</trd>,
      <trd>felvesz <klr tip="amb">(vmibe)</klr></trd>
    </trdgrp>
    <trd lng="nl">insluiten</trd>
    <trd lng="pl">obejmowa&cacute;</trd>
    <trdgrp lng="pt">
      <trd>incluir</trd>,
      <trd>abranger</trd>,
      <trd>abarcar</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;<klr> (&c_v;
    &c_s;&c_je;&c_b;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="inklud.0i.inkluzivi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="inkluz.0i">Inkluzivi</ref>:
      <ekz>
        estas &gcirc;ojige, ke &ccirc;i tiu esperanta versio <tld/>as belajn
        ilustra&jcirc;ojn de <nom>John Tenniel</nom>, kiu<klr>[j]</klr> aperis en la
        unua angla originalo
        <fnt>
          <aut>Garbhan MacAoidh</aut>: <vrk>Inversigita mondo</vrk>.
          <bib>Monato</bib>
          <lok>2002/01, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katalogo <tld/>as plurajn vortarojn de la iam malestimata skota lingvo
        <fnt>
	<aut>Garbhan MacAoidh</aut>
	<vrk><url ref="&Monato;/2000/006815.php">&Ccirc;u nova &scirc;anco por malfortaj lingvoj?</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 23</lok>
        </fnt>;
      </ekz><ekz>
        <ctl>mondoparto</ctl> estas geografia regiono kiu <tld/>as
        la apudajn insulojn: E&ubreve;ropo, Azio, Oceanio estas mondopartoj
        <fnt>
          <aut>Sergio Pokrovskij</aut>: <vrk>Internaciismo kaj
          purismo</vrk>.
          <bib>LOdE</bib>. <lok>2000:10</lok>.
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="implic.0i.io">implici<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_u; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
      <trd>&c_d;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einschlie&szlig;en</trd>,
      <trd>beinhalten</trd>,
      <trd>enthalten</trd>
    </trdgrp>
    <trd lng="nl">bevatten</trd>
    <trd lng="pl">zawiera&cacute;</trd>
    <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;<klr> (&c_v;&c_o;
    &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>obsahovat</trd>,
    <trd>zahrnovat</trd>
  </trdgrp>
  <trd lng="en">include</trd>
  <trd lng="es">incluir</trd>
  <trd lng="ja">&#21547;&#12435;&#12391;&#12356;&#12427;  <pr>&#12405;&#12367;&#12435;&#12391;&#12356;&#12427;</pr></trd>
  <trdgrp lng="sk">
    <trd>obsahova&tcaron;</trd>,
    <trd>zahrnova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#20839;&#21547; <pr>n&egrave;ih&aacute;n</pr></trd>,
    <trd>&#21253;&#25324; <pr>b&amacron;oku&ograve;</pr></trd>,
    <trd>&#21253;&#32599; <pr>b&amacron;olu&oacute;</pr></trd>,
    <trd>&#21547;&#25324; <pr>h&aacute;ngu&amacron;</pr></trd>,
    <trd>&#22218;&#25324; <pr>n&aacute;ngku&ograve;</pr></trd>,
    <trd>&#25324; <pr>ku&ograve;</pr></trd>,
    <trd>&#25910;&#21015; <pr>sh&omacron;uli&egrave;</pr></trd>,
    <trd>&#24402;&#20837; <pr>gu&imacron;r&ugrave;</pr></trd>,
    <trd>&#28085; <pr>h&aacute;n</pr></trd>
  </trdgrp>
</drv>
</art>
