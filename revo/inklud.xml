<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inklud</rad>/i
</kap>
<drv mrk="inklud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="inkluz.0i">inkluzivi</ref>
    <ekz>
      ne &ccirc;iuj leksikografoj <tld/>is &gcirc;in en vortaroj<fnt><aut>Bernard
      Golden</aut>: <vrk>Katamito: seksologia faktermino</vrk>,
      <bib>Monato</bib><lok>jaro 1996a, numero 12a, p. 16a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>einschlie&szlig;en</trd>,
    <trd>beinhalten</trd>,
    <trd>enthalten</trd>
  </trdgrp>
  <trd lng="es">incluir</trd>
  <trdgrp lng="fr">
    <trd>comprendre<klr> (inclure)</klr></trd>,
    <trd>inclure</trd>
  </trdgrp>
  <trd lng="nl">insluiten</trd>
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_v; &c_s;&c_je;&c_b;&c_ja;</trd>
</drv>
</art>
