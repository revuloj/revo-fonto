<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inklud</rad>/i
</kap>
<drv mrk="inklud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="inkluz.0i">inkluzivi</ref>
    <ekz>
      ne &ccirc;iuj leksikografoj <tld/>is &gcirc;in en vortaroj
      <fnt><aut>Bernard Golden</aut>: 
      <vrk>Katamito: seksologia faktermino</vrk>,
      <bib>Monato</bib><lok>jaro 1996a, numero 12a, p. 16a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; &c_u; &c_s;&c_ja;&c_b;&c_je;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einschlie&szlig;en</trd>,
    <trd>beinhalten</trd>,
    <trd>enthalten</trd>
  </trdgrp>
  <trd lng="es">incluir</trd>
  <trdgrp lng="fr">
    <trd>inclure<klr tip="ind"> (comporter)</klr></trd>,
    <trd>comporter<klr tip="ind"> (inclure)</klr></trd>,
    <trd>comprendre<klr tip="ind"> (inclure)</klr></trd>,
    <trd>contenir<klr tip="ind"> (inclure)</klr></trd>,
    <trd>renfermer<klr tip="ind"> (inclure)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mag&aacute;ban foglal</trd>,
    <trd>tartalmaz</trd>,
    <trd>bevesz <klr tip="amb">(vmi k&oacute;z&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="nl">insluiten</trd>
  <trdgrp lng="pt">
   <trd>incluir</trd>, 
   <trd>abranger</trd>, 
   <trd>abarcar</trd> 
  </trdgrp>
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_v; &c_s;&c_je;&c_b;&c_ja;</trd>
</drv>
</art>
