<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Lie&gcirc;</rad>/o
</kap>
<drv mrk="liegx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="liegx.0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la samnoma belga provinco
      <ref tip="malprt" cel="liegx.0o.prov">Lie&gcirc;o</ref>, &ccirc;e la bordo de la 
      rivero <ref tip="vid" cel="moz.0o">Mozo</ref> (5&ring;35'E, 50&ring;38'N).
    </dif>
  </snc>
  <snc mrk="liegx.0o.prov">
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en 
      <ref tip="malprt" cel="valon.V0ujo">Valonujo</ref> (suda Belgujo) 
      kies &ccirc;efurbo anka&ubreve; nomi&gcirc;as 
      <ref tip="prt" cel="liegx.0o.urb">Lie&gcirc;o</ref>:
      <ekz>la E&ubreve;regiono Mozo-Rejno konsistas el la belgaj provincoj Limburgo kaj <tld/>o, 
      el la suda parto de nederlanda Limburgo, kaj el la subregiono Akeno
      <fnt><aut>Jean-Marie JACQUES</aut>:
      <vrk>Senlime</vrk>, 
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Lutych</trd>
  <trd lng="de">L&uuml;ttich</trd>
  <trd lng="en">Liege</trd>
  <trd lng="es">Lieja</trd>
  <trd lng="fr">Li&egrave;ge</trd>
  <trd lng="it">Liegi</trd>
  <trd lng="lat">Leodium</trd>
  <trd lng="nl">Luik</trd>
  <trd lng="pl">Leodium</trd>
  <trd lng="ru">&c_L;&c_mol;&c_je;&c_zh;</trd>
  </drv>
</art>

