<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hxinin.xml,v 1.13 2023/11/15 17:08:02 revo Exp $">
<kap>
  <rad>&hcirc;inin</rad>/o
  <fnt><bib>BoER</bib>, <lok>p. 219</lok></fnt>
</kap>

<drv mrk="hxinin.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Senkolora kristala <ref tip="super" cel="substa.0o.FIZ">substanco</ref>
      <frm>C<sub>20</sub>H<sub>24</sub>O<sub>2</sub>N<sub>2</sub></frm>,
      ekstraktita el la &scirc;elo de
      <ref cel="kinkon.0o">kinkono</ref>,
      kaj uzata kontra&ubreve; <ref cel="malari.0o">malario</ref>:
      <ekz>
        la 24-an de novembro 1869 <nom>Louis Moreau Gottschalk</nom> kolapsis pro malario-atako, el
        kiu li ne plu resani&gcirc;is, <klr>[&#x2026;]</klr> la 18-an de decembro 1869, <nom>Gottschalk</nom>
        mortis <klr>[&#x2026;]</klr> &#x2212; supozeble pro trodozo da <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=525395">Louis Moreau Gottschalk</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Gerard van Swieten</nom> <klr>[&#x2026;]</klr> fami&gcirc;is <klr>[&#x2026;]</klr> pro siaj esploroj
        rilate al la vaskula algio de la viza&gcirc;o, anka&ubreve; konata kiel memmortiga
        migreno, kaj ties traktado per la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=503846">Gerard van Swieten</url></lok>
        </fnt>.
      </ekz> 
    </dif>
    <ref tip="vid" cel="kinkon1.0o">kinkonino</ref>
    <rim>
      Por &ccirc;i tiu signifo la Akademio oficialigis (en 4&#x1D43;OA)
      la vorton
      <ref tip="sin" cel="kinin.0o.kontrauxmalaria">kinino<sncref/></ref>,
      sen atenti la ekziston de la homonimo
      <ref tip="sin" cel="kinin.0o.hormono">kinino<sncref/></ref>.
    </rim>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_n;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_h;&c_i;&c_n;&c_i;&c_n;</trd>
  <trd lng="ca">quinina</trd>
  <trd lng="cs">chinin</trd>
  <trd lng="de">Chinin</trd>
  <trd lng="en">quinine</trd>
  <trd lng="es">quinina</trd>
  <trd lng="fa">&f_k;&f_y;&a_n;&f_y;&a_n;</trd>
  <trd lng="hu">kinin</trd>
  <trd lng="la">chininum</trd>
  <trd lng="nl">kinine</trd>
  <trd lng="pl">chinina</trd>
  <trd lng="pt">quinina</trd>     
  <trd lng="ro">chinin&abreve;</trd>
  <trd lng="ru">&c_h;&c_i;&c_n;&c_i;&c_n;</trd>
  <trd lng="it">chinino</trd>
  <trdgrp lng="zh">
    <trd>&#x594E;&#x5B81; <pr>ku&#xED;n&#xED;ng</pr></trd>,
    <trd>&#x91D1;&#x96DE;&#x7D0D; <pr>j&#x12B;nj&#x12B;n&#xE0;</pr></trd>
  </trdgrp>
</drv>
</art>

  
