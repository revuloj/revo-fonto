<vortaro>
<art mrk="$Id$">
<kap>
  <rad>suspensi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suspensi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="sxveb.0o.KOMUNE">&scirc;vebo <sncref/></ref>
    <ekz>
      la poleno <klr>...</klr> kaptas mineralajn molekulojn
      <klr>...</klr>, kiuj estas en <tld/>o en la aero
      <fnt>
      <aut>Robert Roze</aut>,
      <vrk>Vivisekcio de polenero</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 1997a, numero 11a, p. 12a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>flottement<klr> (chimie)</klr></trd>,
    <trd>suspension<klr> (chimie)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szuszpenzi&oacute;</trd>,
    <trd>lebeg&eacute;s<klr> (k&eacute;miai)</klr></trd>
  </trdgrp>
</drv>
</art>

