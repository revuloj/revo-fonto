<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suspensi.xml,v 1.12 2022/03/29 14:19:46 revo Exp $">
<kap>
  <rad>suspensi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suspensi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="sxveb.0o.KOMUNE">&Scirc;vebo<sncref/></ref>:
      <ekz>
        la poleno <klr>[&#x2026;]</klr> kaptas mineralajn molekulojn
        <klr>[&#x2026;]</klr>, kiuj estas en <tld/>o en la aero
        <fnt>
          <aut>Robert Roze</aut>,
          <vrk>Vivisekcio de polenero</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 11a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        partikloj en suspensio: nuboj, nebulo, brumo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7191">Akvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_s;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_p;&c_e;&c_n;&c_z;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">suspensi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>p&eacute;rov&aacute;n&iacute;</trd>,
    <trd>zav&ecaron;&scaron;en&iacute;</trd>,
    <trd>z&aacute;v&ecaron;s</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Suspension</trd>,
    <trd>Schwebstoff</trd>,
    <trd>Feinstaub</trd>,
    <trd>Vernebelung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flottement<klr> (chimie)</klr></trd>,
    <trd>suspension<klr> (chimie)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szuszpenzi&oacute;</trd>,
    <trd>lebeg&eacute;s<klr> (k&eacute;miai)</klr></trd>
  </trdgrp>
  <trd lng="it">sospensione<klr> (ch.)</klr></trd>
  <trd lng="nl">suspensie</trd>
  <trd lng="no">suspensjon</trd>
  <trdgrp lng="pl">
    <trd>zawieszenie</trd>,
    <trd>zwis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_v;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_p;&c_je;&c_n;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&eacute;rovanie</trd>,
    <trd>zavesenie</trd>,
    <trd>z&aacute;ves</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x441;&#x43F;&#x435;&#x43D;&#x437;&#x456;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x432;&#x438;&#x441;&#x43B;&#x438;&#x439; &#x441;&#x442;&#x430;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

