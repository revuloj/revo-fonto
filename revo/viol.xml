<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>viol</rad>/o
</kap>
<drv mrk="viol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="violac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Viola</trd>), malalta vegeta&jcirc;o, kun kvinpetalaj,
      bonodoraj, ofte bluaj floretoj havantaj spronon, kaj kies fruktoj estas 
      trivalvaj kapsuloj.
    </dif>
  </snc>
  <bld lok="../bld/viol.jpg">
    <ind>odoranta <tld/>o</ind>
    <trd lng="la">Viola odorata</trd>
  </bld>
  <trd lng="de">Veilchen</trd>
  <trd lng="fr">violette</trd>
  <trd lng="nl">viooltje</trd>
  <trd lng="ru">&c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="tr">menek&scedil;e</trd>
</drv>

<drv mrk="viol.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Specioj de <tld/>o (<trd lng="la">Viola tricolor</trd>, k.a.),
      kun grandaj, belaj tre diverskoloraj, plataj floroj.
    </dif>
  </snc>
  <bld lok="../bld/viol1.jpg">
    &gcirc;ardenaj <tld/>egoj
  </bld>
  <trd lng="de">Stiefm&uuml;tterchen</trd>
  <trd lng="fr">pens&eacute;e</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_jo;&c_h;&c_c;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
    <ind>&c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</ind></trd>,
    <trd>&c_a;&c_n;&c_ju;&c_t;&c_i;&c_n;&c_y;
    &c_g;&c_l;&c_a;&c_z;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="viol.0kolora">
  <kap><tld/>(kolor)a</kap>
  <snc>
    <dif>
      Havanta koloron mezan inter bluo kaj ru&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">violett</trd>
  <trd lng="fr">violet</trd>
  <trd lng="nl">violet</trd>
  <trd lng="pl">fioletowy</trd>
  <trd lng="ru">&c_f;&c_i;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="tr">mor</trd>
</drv>

<drv mrk="viol.trans0a">
  <kap>trans<tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Kvalifikas nevideblajn rapidege vibrantajn radiojn deviitajn de la
      prismo trans la <tld/>aj.
    </dif>
    <refgrp tip="sin">
      <ref cel="rugx.trans0a">transru&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ultraviolett</trd>
  <trd lng="fr">ultraviolet</trd>
  <trd lng="nl">ultraviolet</trd>
  <trd
  lng="ru">&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_f;&c_i;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="tr">mor&ouml;te</trd>
</drv>
</art>
