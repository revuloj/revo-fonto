<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>viol</rad>/o
</kap>
<drv mrk="viol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="viol.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Viola</trd>), malalta vegeta&jcirc;o, kun kvinpetalaj,
      bonodoraj, ofte bluaj floretoj.
    </dif>
  </snc>
  <trd lng="de">Veilchen</trd>
  <trd lng="fr">violette</trd>
  <trd lng="tr">menek&scedil;e</trd>
</drv>
<drv mrk="viol.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Violaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Veilchengew&auml;chse</trd>
</drv>
<drv mrk="viol.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Specioj de <tld/>o (<trd lng="la">Viola tricolor</trd>, k.a.),
      kun grandaj, belaj tre diverskoloraj, plataj floroj.
    </dif>
  </snc>
<bld lok="../bld/viol1.jpg">&gcirc;ardenaj <tld/>egoj</bld>
<bld lok="../bld/viol2.jpg">&gcirc;ardenaj <tld/>egoj</bld>
  <trd lng="de">Stiefm&uuml;tterchen</trd>
  <trd lng="fr">pens&eacute;e</trd>
</drv>
<drv mrk="viol.0kolora">
  <kap><tld/>(kolor)a</kap>
  <snc>
    <dif>
      Havanta koloron mezan inter bluo kaj ru&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">violett</trd>
  <trd lng="fr">violet</trd>
  <trd lng="tr">mor</trd>
</drv>
<drv mrk="viol.trans0a">
  <kap>trans<tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Kvalifikas nevideblajn rapidege vibrantajn radiojn deviitajn de la
      prismo trans la <tld/>aj.
    </dif>
    <refgrp tip="sin">
      <ref cel="rugx.trans0a">transru&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ultraviolett</trd>
  <trd lng="fr">ultraviolet</trd>
  <trd lng="tr">mor&ouml;te</trd>
</drv>
</art>
