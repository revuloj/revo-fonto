<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: viol.xml,v 1.44 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>viol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="viol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="viol.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta <ref tip="lst" cel="stauxd.0o" lst="voko:malpigioparencoj">sta&ubreve;do</ref>
      a&ubreve; duonarbusto kun kvinpetalaj, bonodoraj,
      ofte bluaj floretoj havantaj spronon,
      kaj kies fruktoj estas trivalvaj
      <ref tip="prt" cel="kapsul.0o.BOT">kapsuloj</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="violac.0oj">
      <tld/>acoj</ref> (<trd lng="la">Viola</trd>):
      <ekz>
        la bonodoro de la <tld/>oj <klr>[&#x2026;]</klr> en printempo abunde kreskas
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bluaj <tld/>oj <klr>[&#x2026;]</klr> dancas kun hiacintoj kaj
        safranoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la aero fluis odoro de rozoj, konvaloj kaj <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/viol.jpg">
    <ind>odoranta <tld/>o</ind>
    <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;&a_h;&a_hamza_sure;
    &a_m;&a_ayn;&a_T;&a_r;</trd>
    <trd lng="la">Viola odorata</trd>
  </bld>
  <trd lng="be">&c_f;&c_ib;&c_ja;&c_l;&c_k;&c_a;</trd>
  <trd lng="de">Veilchen</trd>
  <trd lng="en">violet</trd>
  <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;&a_h;</trd>
  <trd lng="fr">violette</trd>
  <trd lng="hu">ibolya</trd>
  <trd lng="ja">&#x30B9;&#x30DF;&#x30EC;</trd>
  <trd lng="nl">viooltje</trd>
  <trd lng="pl">fio&lstroke;ek</trd>
  <trd lng="pt">violeta</trd>
  <trd lng="ru">&c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="sv">viol</trd>
  <trd lng="tr">menek&scedil;e</trd>
  <trd lng="sk">sir&ocirc;tka</trd>
  <trdgrp lng="cs">
    <trd>mace&scaron;ka</trd>,
    <trd>mace&scaron;ka trojbarevn&aacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_ib;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="viol.0a">
  <kap><tld/>a</kap>
  <snc mrk="viol.0a.KOL">
    <dif>
      <ref tip="dif" cel="viol.0kolora.KOL"><tld lit="V"/>kolora</ref>:
      <ekz>
        matene <klr>[&#x2026;]</klr> la alta&jcirc;oj de la dezerto fari&gcirc;is <tld/>aj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li venis al la maro, la akvo estis <tld/>a kaj blua, griza kaj densa
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu rideto ekfloris sur la <tld/>aj lipoj de <nom>Emma</nom>
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj okuloj rigardis lin per fu&gcirc;ema sed penetrema rigardo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_ib;&c_ja;&c_l;&c_je;&c_t;&c_a;&c_v;&c_y;</trd>
    <trd lng="de">violett</trd>
    <trd lng="en">violet</trd>
    <trd lng="es">violeta</trd>
    <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;</trd>
    <trd lng="fr">violet<klr> (adj.)</klr></trd>
    <trd lng="hu">ibolyasz&iacute;n&udblac;</trd>
    <trdgrp lng="id">
      <trd>ungu</trd>,
      <trd>violet</trd>
    </trdgrp>
    <trd lng="nl">violet<klr tip="ind"> (bn.)</klr></trd>
    <trd lng="pl">fioletowy</trd>
    <trd lng="pt">violeta</trd>
    <trd lng="ru">&c_f;&c_i;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
    <trd lng="sv">violett</trd>
    <trd lng="tr">mor</trd>
  </snc>
  <trd lng="cs">fialov&yacute;</trd>
  <trd lng="ja">&#x30B9;&#x30DF;&#x30EC;&#x306E;</trd>
  <trd lng="sk">fialov&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_ib;&c_a;&c_l;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_ib;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="viol.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="viol.0ego.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Specioj de <ref tip="super" cel="viol.0o.BOT"><tld/>o</ref>
      (<trd lng="la">Viola tricolor</trd>, k. a.),
      kun grandaj, belaj tre diverskoloraj, plataj floroj.
    </dif>
  </snc>
  <bld lok="../bld/viol1.jpg">
    &gcirc;ardenaj <tld/>egoj
  </bld>
  <trd lng="de">Stiefm&uuml;tterchen</trd>
  <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;&a_h;&a_hamza_sure;
  &a_s;&a_h;&zwnj;&a_r;&a_n;&f_g;</trd>
  <trd lng="fr">pens&eacute;e<klr> (botanique)</klr></trd>
  <trd lng="hu">&aacute;rv&aacute;cska</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30F3;&#x30B8;&#x30FC;</trd>,
    <trd>&#x30B5;&#x30F3;&#x30B7;&#x30AD;&#x30B9;&#x30DF;&#x30EC;</trd>
  </trdgrp>
  <trd lng="nl">driekleurig <ind>viooltje</ind></trd>
  <trdgrp lng="pl">
    <trd>bratek</trd>,
    <trd>fio&lstroke;ek tr&oacute;jbarwn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_jo;&c_h;&c_c;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
    <ind>&c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</ind></trd>,
    <trd>&c_a;&c_n;&c_ju;&c_t;&c_i;&c_n;&c_y;
    &c_g;&c_l;&c_a;&c_z;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sv">styvmorsviol</trd>
  <trd lng="sk">sir&ocirc;tka</trd>
  <trd lng="cs">zahradn&iacute; mace&scaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_ib;&c_a;&c_l;&c_k;&c_a; &c_g;&c_ib;&c_b;&c_r;&c_i;&c_d;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_k;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;-&c_ib;-&c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="viol.0kolora">
  <kap><tld/>kolora</kap>
  <snc mrk="viol.0kolora.KOL">
    <dif>
      Havanta koloron mezan inter bluo kaj ru&gcirc;o;
      <ref tip="sin" cel="viol.0a.KOL"><tld/>a</ref>:
      <ekz>
        tiom da violoj ornamas la herbaron, ke <klr>[&#x2026;]</klr> &ccirc;io estas <tld/>kolora,
        kaj la bonodoro atingas e&ccirc; nian dometon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        virino en moda vesto el <tld/>kolora atlaso
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;veli&gcirc;as maturaj <tld/>koloraj prunoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_ja;&c_l;&c_je;&c_t;&c_a;&c_v;&c_y;</trd>
  <trd lng="bo">&t_ma;&t_shabchu;&t_tsheg;&t_ma;&t_na;&t_tsheg;</trd>
  <trd lng="cs">fialov&yacute;</trd>
  <trd lng="de">violett</trd>
  <trd lng="en">violet</trd>
  <trd lng="es">violeta</trd>
  <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;</trd>
  <trd lng="fr">violet<klr> (adj.)</klr></trd>
  <trd lng="hu">ibolyasz&iacute;n&udblac;</trd>
  <trdgrp lng="id">
    <trd>ungu</trd>,
    <trd>violet</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x307F;&#x308C;&#x8272;&#x306E;  <pr>&#x3059;&#x307F;&#x308C;&#x3044;&#x308D;&#x306E;</pr></trd>,
    <trd>&#x7D2B;&#x8272;&#x306E;  <pr>&#x3080;&#x3089;&#x3055;&#x304D;&#x3044;&#x308D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">violet<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">fioletowy</trd>
  <trd lng="pt">violeta</trd>
  <trd lng="ru">&c_f;&c_i;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_j;
  </trd>
  <trd lng="sk">fialov&yacute;</trd>
  <trd lng="sv">violett</trd>
  <trd lng="tr">mor</trd>
  <trd lng="uk">&c_f;&c_ib;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="viol.0koloro">
  <kap><tld/>koloro</kap>
  <snc mrk="viol.0koloro.KOL">
    <dif>
      <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">Koloro</ref> de <tld/>o,
      meza inter bluo kaj ru&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_ja;&c_l;&c_je;&c_t;</trd>
  <trd lng="de">Violett</trd>
  <trd lng="en">violet</trd>
  <trd lng="fa">&a_r;&a_n;&f_g; <ind>&a_b;&a_n;&a_f;&a_sh;</ind></trd>
  <trd lng="fr">violet<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x307F;&#x308C;&#x8272;  <pr>&#x3059;&#x307F;&#x308C;&#x3044;&#x308D;</pr></trd>,
    <trd>&#x7D2B;&#x8272;  <pr>&#x3080;&#x3089;&#x3055;&#x304D;&#x3044;&#x308D;</pr></trd>
  </trdgrp>
  <trd lng="nl">violet<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">fiolet</trd>
  <trd lng="sv">violett<klr> (subst.)</klr></trd>
</drv>

<drv mrk="viol.trans0a">
  <kap>trans<tld/>a</kap>
  <snc mrk="viol.trans0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(pri radiado)</uzo>
    <dif>
      Rapidege vibranta, deviigita de prismo trans
      la <tld/>aj radioj;
      <ref tip="sin" cel="ultraviol.0a.FIZ">ultraviola</ref>:
      <ekz>
        &gcirc;is nun oni konsideris nur du etapojn: la hereda difekto,
        kiu okazas en &ccirc;iu &ccirc;elo, kaj la efiko de
        trans<tld/>aj radioj sur unu &ccirc;elon
        <fnt>
          <aut>S. Norvell</aut>,
          <vrk>&Ccirc;u anka&ubreve; genoj responsas pri
          melanomo?</vrk>,
          <bib>Monato</bib>,
          <lok>1997:7, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rugx.trans0a">transru&gcirc;a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_f;&c_ib;&c_ja;&c_l;&c_je;&c_t;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_z;&c_a;&c_f;&c_ib;&c_ja;&c_l;&c_je;&c_t;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">ultrafialov&yacute;</trd>
  <trd lng="de">ultraviolett</trd>
  <trd lng="en">ultraviolet</trd>
  <trd lng="es">ultravioleta</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_r;&a_A;&a_b;&a_n;&a_f;&a_sh;</trd>,
    <trd>&a_m;&a_A;&a_w;&a_r;&a_A;&a_hamza;&a_b;&a_n;&a_f;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">ultraviolet</trd>
  <trdgrp lng="hu">
    <trd>ultraibolya</trd>,
    <trd>ultraviola</trd>,
    <trd>iboly&aacute;nt&uacute;li</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ultraungu</trd>,
    <trd>ultraviolet</trd>
  </trdgrp>
  <trd lng="ja">&#x7D2B;&#x5916;&#x7DDA;&#x306E;  <pr>&#x3057;&#x304C;&#x3044;&#x305B;&#x3093;&#x306E;</pr></trd>
  <trdgrp lng="nl">
    <trd>ultraviolet</trd>,
    <trd>uv</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ultrafioletowy</trd>,
    <trd>nadfioletowy</trd>
  </trdgrp>
  <trd lng="pt">ultravioleta</trd>
  <trd lng="ru">&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_f;&c_i;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">ultrafialov&yacute;</trd>
  <trd lng="sv">ultraviolett</trd>
  <trd lng="tr">mor&ouml;te</trd>
  <trd lng="uk">&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_f;&c_ib;&c_o;&c_l;&c_je;&c_t;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>
</art>
