<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Hele malva, kiel iuj lilakfloroj:
      <ekz>
        la&ubreve; ordono de Feniksa oni alportis <tld/>an kulvualon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 3a, p. 47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_l;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_b;&c_e;&c_z;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="de">lila</trd>
  <trdgrp lng="fr">
    <trd>lilas<klr> (adj. couleur)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="L"/>a <ref tip="super" cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trd lng="de">Lila</trd>
  <trdgrp lng="fr">
    <trd>lilas<klr> (subst. couleur)</klr></trd>
  </trdgrp>
</drv>
</art>

