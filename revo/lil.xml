<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lil.xml,v 1.15 2024/01/15 19:02:14 revo Exp $">
<kap>
  <rad>lil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Inter rozkolora kaj violkolora, kiel iuj 
      <ref cel="lilak.0o">lilak</ref>floroj:
      <ekz>
        la&ubreve; ordono de Feniksa oni alportis <tld/>an kulvualon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 3a, p. 47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_l;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_b;&c_e;&c_z;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lila</trd>,
    <trd>lilov&yacute;</trd>,
    <trd>&scaron;e&rcaron;&iacute;kov&ecaron; fialov&eacute; barvy</trd>
  </trdgrp>
  <trd lng="de">lila</trd>
  <trdgrp lng="fr">
    <trd>lilas<klr> (adj. couleur)</klr></trd>
  </trdgrp>
  <trd lng="hu">lila</trd>
  <trd lng="nl">lila<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>lila</trd>,
    <trd>liliowy</trd>
  </trdgrp>
  <trd lng="ro">liliac</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_o;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>lila</trd>,
    <trd>orgov&aacute;novej farby</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_ib;&c_l;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_z;&c_k;&c_o;&c_v;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_k;&c_o;&c_l;&c_ib;&c_r;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F50;&#x9751; <pr>j&#xEC;q&#x12B;ng</pr></trd>,
    <trd>&#x5F51;&#x9751; <pr>j&#xEC;q&#x12B;ng</pr></trd>,
    <trd>&#x96EA;&#x9751; <pr>xu&#x11B;q&#x12B;ng</pr></trd>,
    <trd>&#x828B;&#x5934;&#x8272; <pr>y&#xF9;tous&#xE8;</pr></trd>,
    <trd>&#x828B;&#x982D;&#x8272; <pr>y&#xF9;tous&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="L"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_l;&c_o;&c_v;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  <trd lng="de">Lila</trd>
  <trdgrp lng="fr">
    <trd>lilas<klr> (subst. couleur)</klr></trd>
  </trdgrp>
  <trd lng="hu">lila sz&iacute;n</trd>
  <trd lng="nl">lila<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">kolor <ind>liliowy</ind></trd>
  <trd lng="pt">lil&aacute;s</trd>
  <trd lng="ro">liliac de culoare</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_o;&c_v;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x828B;&#x5934;&#x8272; <pr>y&#xF9;tous&#xE8;</pr></trd>,
    <trd>&#x828B;&#x982D;&#x8272; <pr>y&#xF9;tous&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>

