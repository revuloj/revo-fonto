<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mehar.xml,v 1.6 2025/10/05 18:35:48 revo Exp $">
<kap>
  <rad>mehar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="mehar.0o">
  <kap><tld/>o</kap>
  <snc mrk="mehar.0o.RAJ">
    <dif>
      <ke>Kur<ref tip="super" cel="dromed.0o">dromedaro</ref></ke>:
      <ekz>
        trinkinte la tri ritajn tasojn da teo
        la vagemulo sur<tld/>i&gcirc;is
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mehari</trd>
  <trd lng="en">mehari</trd>
  <trd lng="fr">m&eacute;hari</trd>
  <trdgrp lng="pl">
    <trd>dromader afryka&nacute;ski</trd>,
    <trd>mehari</trd>
  </trdgrp>
  <trd lng="ro">dromedarul african</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_je;&c_g;&c_a;&c_r;&c_ib;</trd>,
    <trd>&c_m;&c_je;&c_h;&c_a;&c_r;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_i;&c_j; &c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="mehar.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="mehar.0isto.RAJ">
    <dif>
      Rajdanto de <tld/>o,
      precipe tia soldato:
      <ekz>
        la <tld/>isto zonpremas siajn
        interna&jcirc;ojn de la subventro
        &gcirc;is la stomako
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kamelreiter</trd>
  <trd lng="en">meharist</trd>
  <trd lng="fr">m&eacute;hariste</trd>
  <trd lng="pl">je&zacute;dziec na <ind>mehari</ind></trd>
  <trd lng="ro">c&abreve;l&abreve;re&tcomma; pe <ind/>mehari</trd>
</drv>
</art>

