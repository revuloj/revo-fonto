<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mehar.xml,v 1.1 2019/06/20 11:10:14 revo Exp $">
<kap>
  <rad>mehar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="mehar.0o">
  <kap><tld/>o</kap>
  <snc mrk="mehar.0o.RAJ">
    <dif>
      Kur<ref tip="super" cel="dromed.0o">dromedaro</ref>:
      <ekz>
        trinkinte la tri ritajn tasojn da teo
        la vagemulo sur<tld/>i&gcirc;is  
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">m&eacute;hari</trd>
</drv>

<drv mrk="mehar.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="mehar.0isto.RAJ">
    <dif>
      Rajdanto de <tld/>o,
      precipe tia soldato:
      <ekz>
        la <tld/>isto zonpremas siajn 
        interna&jcirc;ojn de la subventro
        &gcirc;is la stomako
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">m&eacute;hariste</trd>
</drv>
</art>

