<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mehar.xml,v 1.3 2022/03/23 14:01:06 revo Exp $">
<kap>
  <rad>mehar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="mehar.0o">
  <kap><tld/>o</kap>
  <snc mrk="mehar.0o.RAJ">
    <dif>
      Kur<ref tip="super" cel="dromed.0o">dromedaro</ref>:
      <ekz>
        trinkinte la tri ritajn tasojn da teo
        la vagemulo sur<tld/>i&gcirc;is  
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">m&eacute;hari</trd>
  <trdgrp lng="pl">
    <trd>dromader afryka&nacute;ski</trd>,
    <trd>mehari</trd>
  </trdgrp> 
  <trd lng="ro">dromedarul african</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x433;&#x430;&#x440;&#x456;</trd>,
    <trd>&#x43C;&#x435;&#x445;&#x430;&#x440;&#x456;</trd>,
    <trd>&#x432;&#x435;&#x440;&#x445;&#x43E;&#x432;&#x438;&#x439; &#x432;&#x435;&#x440;&#x431;&#x43B;&#x44E;&#x434;</trd>
  </trdgrp>
</drv>

<drv mrk="mehar.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="mehar.0isto.RAJ">
    <dif>
      Rajdanto de <tld/>o,
      precipe tia soldato:
      <ekz>
        la <tld/>isto zonpremas siajn 
        interna&jcirc;ojn de la subventro
        &gcirc;is la stomako
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">m&eacute;hariste</trd>
  <trd lng="pl">je&zacute;dziec na <ind>mehari</ind></trd>     
  <trd lng="ro">c&abreve;l&abreve;re&#x21B; pe <ind/>mehari</trd>
</drv>
</art>

