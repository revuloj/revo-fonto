<art mrk="$Id: disoci.xml,v 1.17 2023/10/14 11:26:54 revo Exp $">
  <kap>
    <rad>disoci</rad>/i
    <fnt><bib>PV</bib></fnt>
  </kap>
<drv mrk="disoci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      Parte a&ubreve; progrese <ref cel="kompon.mal0i">malkomponi</ref>:
      <ekz>
        <nom>Ostwald</nom> komune kun <nom>Svanthe Arrhenius</nom> en Upsalo <klr>[&#x2026;]</klr> ellaboris la
        teorion de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26704">Wilhelm Ostwald</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja; &c_d;&c_o; &c_d;&c_i;&c_s;&c_o;&c_c;&c_i;&c_a;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">dissociar</trd>
  <trdgrp lng="cs">
    <trd>disociovat</trd>,
    <trd>rozlu&ccaron;ovat</trd>,
    <trd>roz&scaron;t&ecaron;pit</trd>
  </trdgrp>
  <trd lng="de">dissoziieren</trd>
  <trd lng="en">dissociate</trd>
  <trd lng="fr">dissocier</trd>
  <trd lng="ja">&#x89E3;&#x96E2;&#x3055;&#x305B;&#x308B;  <pr>&#x304B;&#x3044;&#x308A;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">scheiden</trd>
  <trdgrp lng="pl">
    <trd>dysocjowa&cacute;</trd>,
    <trd>rozk&lstroke;ada&cacute;<klr> (chem.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dissociar</trd>,
    <trd>desagregar</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_o;&c_c;&c_i;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rozl&uacute;&ccaron;i&tcaron;</trd>,
    <trd>roz&scaron;tiepi&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_i;&c_s;&c_o;&c_c;&c_ib;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
