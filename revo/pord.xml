<drv mrk="pord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pord.0o.spaco">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="ferm.mal0ajxo.KOMUNE">Malferma&jcirc;o</ref>
      en <ref tip="malprt" cel="mur.0o">muro</ref> de konstrua&jcirc;o,
      en <ref tip="malprt" cel="bar.0ilo">barilo</ref>, en flanko de
      <ref tip="malprt" cel="vetur.0ilo">veturilo</ref> a&ubreve; ma&scirc;ino por trairo:
      <ekz>
        li ne povas trairi tra nia mallar&gcirc;a <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        la kovriloj de l' fenestroj kaj la <tld/>oj restis fermitaj la tutan tagon
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>okurteno ek&jcirc;eti&gcirc;is flanken, kaj sur la sojlo de la <tld/>o sin montris
        alta, gracia virina figuro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kale&scirc;<tld/>o, &ccirc;ar<tld/>o
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sidis &ccirc;e la <tld/>o de la tendo
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 18:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        sie&gcirc;i ies <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se vi agos malbone, la peko ku&scirc;os &ccirc;e la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ora &scirc;losilo &ccirc;iun <tld/>on malfermas
        <klr>(<ref cel="acxet.sub0i">suba&ccirc;etas</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en bona ordo tra la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se &ccirc;iu balaos anta&ubreve; sia <tld/>o, tiam en la tuta urbo estos ordo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pelu mizeron tra l' <tld/>o, &gcirc;i revenos tra l' fenestro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        diablo ne &ccirc;iam unu <tld/>on sie&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="ir.en0ejo">enirejo</ref>
    <refgrp tip="sub">
       <ref cel="luk.0o">luko</ref>,
       <ref cel="potern.0o">poterno</ref>
    </refgrp>
    <refgrp tip="prt">
      <ref cel="ans.0o">anso</ref>,
      <ref cel="klink.0o.pordanso">klinko<sncref/></ref>,
      <ref cel="klap.0o.pordo">klapo<sncref/></ref>,
      <ref cel="hok.pord0o.TEK">pordhoko</ref>,
      <ref cel="serur.0o">seruro</ref>
    </refgrp>
    <trd lng="de">T&uuml;r&ouml;ffnung</trd>
    <trd lng="es">portal</trd>
    <trd lng="nl">doorgang</trd>
  </snc>
  <snc mrk="pord.0o.fermajxo">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="plat.0o">Plato</ref>, kiu fermas <tld/>an spacon, kaj klape a&ubreve; glite
      formovi&gcirc;as por ebligi trairon:
      <ekz>
        &scirc;losi la <tld/>on
        <fnt><bib>F</bib> <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        li fermis kolere la <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili aliris, por elrompi la <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 19:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>o estis malgranda tabuleto, sur kiu per grandaj literoj estis
        skribita: &leftquot;tiu &ccirc;i domo estas vendata&rightquot;
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        subite oni frapas tre malla&ubreve;te sur la <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>krad<tld/>o</ind>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
        <trd lng="de">Gatter</trd>
      </ekz>
      <ekz>
        du soldatoj portis la knabeton eksteren al la baril<tld/>o de la kampo
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fajro<tld/>oj salte malfermi&gcirc;is kaj la ardanta karbo lante voras je niaj
        piedoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        dezirante sin sciigi, <klr>[ili]</klr> iris <ind>frapi sur la <tld/>on</ind>
        <klr>(peti <ref cel="help.0o">helpo</ref>n a&ubreve; konsilon)</klr>
        de diversaj amikoj, kaj fine alvenis en la hejmon de Klozelo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
        <trd lng="de">an die <ind>T&uuml;r</ind> klopfen <klr>(mit einer Bitte)</klr></trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>lasi la <tld/>on malfermita</ind>
        <klr>(rezervi la eblon)</klr>
        al pliaj sendependigaj provoj
        <fnt>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2013/010900.php">Angla hundo, skota vosto</url></vrk>
          <bib>Monato</bib>
          <lok>2013/01, p. 5</lok>
        </fnt>.
        <trd lng="de">die <ind>T&uuml;r</ind> offen lassen</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flug.0ilo.svingigxi">flugilo<sncref/></ref>,
      <ref cel="klap.0o.pordo">klapo<sncref/></ref>
    </refgrp>
    <trd lng="de">T&uuml;r</trd>
    <trd lng="es">puerta</trd>
    <trd lng="nl">deur</trd>
  </snc>
