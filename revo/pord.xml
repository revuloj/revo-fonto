<drv mrk="pord.0ego">
  <kap><ofc>*</ofc><tld/>ego
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -eg'</lok></fnt></kap>
  <snc mrk="pord.0ego.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Ccirc;efa <ref tip="super" cel="pord.0o"><tld/>o</ref> de domo, de
      fortika urbo, por paso de veturiloj:
      <ekz>
        kun bruo oni malfermis la <tld/>egon, kaj la kale&scirc;o enveturis
        en la korton
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        kiam li venis al la <tld/>ego de la urbo, la gardistoj demandis,
        &ccirc;u ili devas anonci lin al la re&gcirc;ino
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu templo kaj palaco havis grandegan <tld/>egon kun pilonoj, kaj tial oni nomis
        Tebojn urbo &leftquot;cent<tld/>ega&rightquot;
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> elportas la paka&jcirc;on ekster la <tld/>egon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        via idaro posedos la <tld/>egojn de siaj malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Gibraltara <tld/>ego <klr>(<ref cel="kol.mar0o">markolo</ref>)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la propraj poseda&jcirc;oj, kun kiuj <klr>[vi]</klr> venas al 
        la <tld/>ego de la socia vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_gata;&t_naro;</trd>
  <trdgrp lng="cs">
    <trd>br&aacute;na</trd>,
    <trd>vrata</trd>
  </trdgrp>
  <trd lng="de">Tor</trd>
  <trdgrp lng="en">
    <trd>garage <ind>door</ind></trd>,
    <trd>gate</trd>
  </trdgrp>
  <trd lng="es">port&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>portail</trd>,
    <trd>portes<klr tip="amb"> (d'une r&eacute;sidence, d'une
    ville)</klr></trd>
  </trdgrp>
  <trd lng="hu">kapu</trd>
  <trd lng="nl">poort</trd>
  <trdgrp lng="pl">
    <trd>brama</trd>,
    <trd>wrota</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;na</trd>,
    <trd>vr&aacute;ta</trd>
  </trdgrp>
  <trd lng="sv">port</trd>
</drv>

<drv mrk="pord.0egejo">
  <kap><tld/>egejo</kap>
  <snc mrk="pord.0egejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Granda <ref tip="super" cel="ir.en0ejo">enirejo</ref> en
      konstrua&jcirc;o, kiun fermas <tld/>ego:
      <ekz>
        la malluma figuro de la virino kuris <klr>[...]</klr> tra la
        arka&jcirc;an <tld/>egejon de la domo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la kastelo <klr>[&#x2026;]</klr>, tri turoj, la <tld/>egejo kaj la domo de la
        administranto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=676966">Kastelo Landsberg (Meiningen)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Toreinfahrt</trd>,
    <trd>Torweg</trd>,
    <trd>Torhaus</trd>
  </trdgrp>
  <trd lng="en">gateway</trd>
  <trd lng="fr">porche</trd>
  <trd lng="hu">kapualj</trd> 
  <trdgrp lng="nl">
    <trd>portaal</trd>,
    <trd>portiek</trd>
  </trdgrp> 
  <trd lng="pl">portal</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_malmol;&c_je;&c_z;&c_d;</trd>
</drv>

<drv mrk="pord.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pord.0isto.KOMUNE">
    <dif>
      <ref tip="super" cel="gard.0isto">Gardisto</ref> de domo, kiu kontrolas 
      la enirantojn, ricevas po&scirc;ta&jcirc;ojn...:
      <ekz>
        homo forvoja&gcirc;inta <klr>[...]</klr> ordonis al la <tld/>isto, ke
        li viglu
        <fnt><bib>NT</bib>, <lok>&Mar; 13:34</lok></fnt>;
      </ekz>
      <ekz>
        majeste staris la <tld/>isto en veluro kaj silko, kun granda ar&gcirc;entita bastono
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto kaj du servistinoj rekonis lin, &ccirc;ar la nokto estis sufi&ccirc;e luma
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto kuras post mi sur la &scirc;tuparo kun broso: permesu <klr>[ke]</klr> mi
        al vi purigos la botojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="grum.0o">grumo</ref>,
       <ref cel="lake.0o.servisto">lakeo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>port&yacute;r</trd>,
    <trd>vr&aacute;tn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>T&uuml;rsteher</trd>,
    <trd>Torw&auml;chter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>doorman</trd>,
    <trd>gatekeeper</trd>
  </trdgrp>
  <trd lng="es">portero</trd>
  <trdgrp lng="fr">
    <trd>concierge</trd>,
    <trd>gardien d'immeuble</trd>,
    <trd>portier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapus</trd>,
    <trd>port&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">portier</trd>
  <trdgrp lng="pl">
    <trd>od&zdot;wierny</trd>,
    <trd>porter</trd>,
    <trd>furtian <klr>(w klasztorze)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_r;&c_a;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">vr&aacute;tnik</trd>
</drv>

<drv mrk="pord.dom0o">
  <kap>dom<tld/>o</kap>
  <snc mrk="pord.dom0o.KOMUNE">
    <dif>
      <tld lit="P"/>o tra kiu oni eniras a&ubreve; eliras domon:
      <ekz>
        kiam ili iris el la dom<tld/>o,
        Rejnhardo donis al &scirc;i la brakon
        <fnt>
          <aut>T. Storm, trad. A. Bader</aut>,
          <vrk>Imenlago</vrk>,
          <lok>en: Du Amosentaj Noveloj, 1947</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni staris anta&ubreve; la dom<tld/>o por kapti iom da fre&scirc;a aero
        <fnt>
          <bib>Monato</bib>
          <aut>Czar</aut>
          <vrk><url ref="https://www.monato.be/2003/008152.html">La fino de Vincent van Gogh</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ka&scirc;is &scirc;losilon de la malanta&ubreve;a dom<tld/>o sub la tegmenton de &gcirc;ardena budo
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2009/009779.php">&Scirc;losila teorio</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">domovn&iacute; dve&rcaron;e</trd>
  <trd lng="de">Haust&uuml;r</trd>
  <trdgrp lng="en">
    <trd>entry door</trd>,
    <trd>front door</trd>, <trd>exterior door</trd>
  </trdgrp>
  <trd lng="fr">porte d'entr&eacute;e<klr> (de maison)</klr></trd>
  <trd lng="pl">drzwi <klr>domu</klr></trd>
  <trd lng="sk">domov&eacute; dvere</trd>
</drv>

<drv mrk="pord.fald0o">
  <kap>fald<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.fald0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      <ref tip="prt" cel="balg.0o">balge</ref> faldebla ferma&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Faltt&uuml;r</trd>
  <trdgrp lng="en">
    <trd>folding door</trd>,
    <trd>accordion door</trd>
  </trdgrp>
  <trd lng="fr">porte pliante</trd>
  <trd lng="hu">harmonikaajt&oacute;</trd> 
  <trd lng="nl">vouwdeur</trd>
  <trdgrp lng="pl">
    <trd>drzwi sk&lstroke;adana</trd>,
    <trd>drzwi harmonijkowe</trd>
  </trdgrp>
</drv>

<drv mrk="pord.klap0o">
  <kap>klap<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.klap0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      klape movebla ferma&jcirc;o:
      <ekz>
        post kelkaj minutoj en la muro per si mem deklini&gcirc;is malgranda tablo, kaj post
        tio tra la klap<tld/>o oni &scirc;ovis sur &gcirc;in pladon da man&gcirc;a&jcirc;o, kaj poste la
        pordo denove fermi&gcirc;is
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi konstruis klap<tld/>on, per kiu mi povis faligi miajn
        <klr>[...]</klr> amatinojn en profundakvan puton
        <fnt><bib>SatirRak</bib>, <lok>perfekta civitano</lok></fnt>;
      </ekz>
      <ekz>
        la klap<tld/>o de la kripto <klr>[&#x2026;]</klr> abomene fa&ubreve;kis tuj anta&ubreve; la katedro,
        kaj nun senbrue tordi&gcirc;is suben
        <fnt>
          <aut>Sonja Lang</aut>
          <vrk>La Soleno</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;erkisto jam estis proksime de sia domo, kiam <klr>[&#x2026;]</klr> iu venis al lia
        <tld/>ego, malfermis la klap<tld/>eto&#x202F; kaj malaperis post &gcirc;i
        <fnt>
          <bib>LOdE</bib>
          <aut>Aleksandr Pu&scirc;kin </aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">&Ccirc;erkisto </url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim mrk="pord.klap0o.RIM_1">
      Vortaroj kaj verkoj prezentas ordinare &leftquot;klap<tld/>on&rightquot; nur kun
      la senco de klapa <ref tip="vid" cel="pord.plank0o.ARKI">plank<tld/>o</ref>. 
      Tamen la vorto ta&ubreve;gas anka&ubreve; por vertikala <tld/>o kun klapego, kontraste al fald-,
      &scirc;ov-<tld/>oj kaj aliaj.
      <aut>&jcirc;v</aut>
    </rim>
  </snc>
  <trdgrp lng="cs">
    <trd>poklop</trd>,
    <trd>skl&aacute;p&ecaron;c&iacute; dve&rcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Klappt&uuml;r</trd>,
    <trd>Fallt&uuml;r</trd>
  </trdgrp>
  <trd lng="en">trapdoor</trd>
  <trd lng="fr">porte &agrave; battants</trd>
  <trd lng="hu">csap&oacute;ajt&oacute;</trd>
  <trd lng="nl">klapdeur</trd>
  <trd lng="pl">drzwi skrzyd&lstroke;owe</trd>
  <trd lng="sk">poklop</trd>
</drv>

<drv mrk="pord.kluz0o">
  <kap>kluz<tld/>o</kap>
  <snc mrk="pord.kluz0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Movebla parto de <ref tip="malprt" cel="kluz.0o.MAR">kluzo</ref>,
      per kiu oni &gcirc;in fermas kaj malfermas:
      <ekz>
        doko <klr>[estas]</klr> havena baseno, kun kluz<tld/>oj kaj kajoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        li iradis inspektadi la kluz<tld/>on
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/III.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schleusentor</trd>
  <trd lng="en">lock gate</trd>
  <trd lng="fr">porte d'&eacute;cluse</trd>
  <trd lng="pl">wrota <klr>&sacute;luzy</klr></trd>
</drv>

<drv mrk="pord.lev0o">
  <kap>lev<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.lev0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      vertikale levebla ferma&jcirc;o, ekzemple en <ref tip="malprt" cel="kluz.0o.MAR">kluzon</ref>.
    </dif>
    <ref tip="sin" cel="hers.0o">herso</ref>
  </snc>
  <trdgrp lng="en">
    <trd>drop gate</trd>,
    <trd>guillotine gate</trd>, <trd>vertical door</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>emel&odblac;kapu</trd>,
    <trd>zsilipkapu</trd>
  </trdgrp>
  <trd lng="es">puente levadizo</trd>
</drv>

<drv mrk="pord.plank0o">
  <kap>plank<tld/>o</kap>
  <snc mrk="pord.plank0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ku&scirc;a <ref tip="super" cel="pord.0o.spaco"><tld/>o</ref> en planko
      super &scirc;tuparo a&ubreve; alirebla per &scirc;tupetaro:
      <ekz>
        &gcirc;i eltiris la feran bolton el la fermita plank<tld/>o kaj
        &scirc;telece
        malsupreniris al la malliberulo
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">propadli&scaron;t&ecaron;</trd>
  <trd lng="de">Fallt&uuml;r</trd>
  <trdgrp lng="en">
    <trd>access hatch</trd>, 
    <trd>access door</trd>, <trd>trapdoor</trd>
  </trdgrp>
  <trd lng="fr">trappe</trd>
  <trd lng="hu">csap&oacute;ajt&oacute; <klr tip="amb">(v&iacute;zszintes)</klr></trd>
  <trd lng="pl">klapa</trd>
  <trd lng="ru">&c_l;&c_ju;&c_k;</trd>
  <trd lng="sk">priepadlisko</trd>
</drv>

<drv mrk="pord.sving0o">
  <kap>sving<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.sving0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      ferma&jcirc;o egale pu&scirc;ebla a&ubreve; tirebla, kiun risorto a&ubreve;tomate
      relokas:
      <ekz>
        la sving<tld/>o de la banko malfermi&gcirc;is malpeze
        <fnt>
          <vrk><url ref="http://www.elerno.cn/penseo/pen155.pdf">Penseo N-ro 155</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>"l&iacute;ta&ccaron;ky"</trd>,
    <trd>k&yacute;vac&iacute; dve&rcaron;e</trd>
  </trdgrp>
  <trd lng="de">Schwingt&uuml;r</trd>
  <trd lng="en">swinging door</trd>
  <trd lng="hu">leng&odblac;ajt&oacute;</trd>
  <trd lng="pl">drzwi wahad&lstroke;owe</trd>
  <trd lng="sk">k&yacute;vacie dvere</trd>
</drv>

<drv mrk="pord.sxov0o">
  <kap>&scirc;ov<tld/>o <fnt><bib>EBV</bib></fnt>,
  <var><kap>glit<tld/>o <fnt><bib>EBV</bib></fnt></kap></var></kap>
  <snc mrk="pord.sxov0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      flanken &scirc;ovebla ferma&jcirc;o:
      <ekz>
        &scirc;ova <tld/>o kondukas en grandegan &ccirc;ambron najbaran, kiu tuta estas plenigita de
        paketoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        paniki&gcirc;inte li malaperis en la tute hazarde jam prete atendantan lifton, la
        a&ubreve;tomataj &scirc;ov<tld/>oj helike malrapide fermi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Tagmeze en Tunizo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        rjokanaj &ccirc;ambroj anka&ubreve; prezentas aleron a&ubreve; balkonon, same ekipitan de glit<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=714772">Rjokano (gastejo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schiebet&uuml;r</trd>
  <trd lng="en">sliding door</trd>
  <trd lng="fr">porte coulissante</trd>
  <trd lng="es">puerta deslizable</trd>
  <trd lng="hu">tol&oacute;ajt&oacute;</trd>
  <trd lng="nl">schuifdeur</trd>
  <trd lng="pl">drzwi przesuwne</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_v;&c_i;&c_zh;&c_n;&c_a;&c_ja; &c_d;&c_v;&c_je;&c_r;&c_mol;</trd>
</drv>

