<drv mrk="pord.0ego">
  <kap><ofc>*</ofc><tld/>ego
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -eg'</lok></fnt></kap>
  <snc mrk="pord.0ego.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Ccirc;efa <ref tip="super" cel="pord.0o"><tld/>o</ref> de domo, de
      fortika urbo, por paso de veturiloj:
      <ekz>
        kun bruo oni malfermis la <tld/>egon, kaj la kale&scirc;o enveturis
        en la korton
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        kiam li venis al la <tld/>ego de la urbo, la gardistoj demandis,
        &ccirc;u ili devas anonci lin al la re&gcirc;ino
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu templo kaj palaco havis grandegan <tld/>egon kun pilonoj, kaj tial oni nomis
        Tebojn urbo &leftquot;cent<tld/>ega&rightquot;
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> elportas la paka&jcirc;on ekster la <tld/>egon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        via idaro posedos la <tld/>egojn de siaj malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Gibraltara <tld/>ego <klr>(<ref cel="kol.mar0o">markolo</ref>)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la propraj poseda&jcirc;oj, kun kiuj <klr>[vi]</klr> venas al
        la <tld/>ego de la socia vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_gata;&t_naro;</trd>
  <trdgrp lng="cs">
    <trd>br&aacute;na</trd>,
    <trd>vrata</trd>
  </trdgrp>
  <trd lng="de">Tor</trd>
  <trdgrp lng="en">
    <trd>garage <ind>door</ind></trd>,
    <trd>gate</trd>
  </trdgrp>
  <trd lng="es">port&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>portail</trd>,
    <trd>portes<klr tip="amb"> (d'une r&eacute;sidence, d'une
    ville)</klr></trd>
  </trdgrp>
  <trd lng="hu">kapu</trd>
  <trd lng="id">gerbang</trd>
  <trdgrp lng="ja">
    <trd>&#38272;  <pr>&#12418;&#12435;</pr></trd>,
    <trd>&#22823;&#25144;  <pr>&#x304A;&#x304A;&#12393;</pr></trd>
  </trdgrp>
  <trd lng="nl">poort</trd>
  <trdgrp lng="pl">
    <trd>brama</trd>,
    <trd>wrota</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;na</trd>,
    <trd>vr&aacute;ta</trd>
  </trdgrp>
  <trd lng="sv">port</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_m;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pord.0egejo">
  <kap><tld/>egejo</kap>
  <snc mrk="pord.0egejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Granda <ref tip="super" cel="ir.en0ejo">enirejo</ref> en
      konstrua&jcirc;o, kiun fermas <tld/>ego:
      <ekz>
        la malluma figuro de la virino kuris <klr>[...]</klr> tra la
        arka&jcirc;an <tld/>egejon de la domo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la kastelo <klr>[&#8230;]</klr>, tri turoj, la <tld/>egejo kaj la domo de la
        administranto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=676966">Kastelo Landsberg
          (Meiningen)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&#8217;&c_je;&c_z;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Toreinfahrt</trd>,
    <trd>Torweg</trd>,
    <trd>Torhaus</trd>
  </trdgrp>
  <trd lng="en">gateway</trd>
  <trd lng="fr">porche</trd>
  <trd lng="hu">kapualj</trd>
  <trdgrp lng="nl">
    <trd>portaal</trd>,
    <trd>portiek</trd>
  </trdgrp>
  <trd lng="pl">portal</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_malmol;&c_je;&c_z;&c_d;</trd>
</drv>

<drv mrk="pord.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pord.0isto.KOMUNE">
    <dif>
      <ref tip="super" cel="gard.0isto">Gardisto</ref> de domo, kiu kontrolas
      la enirantojn, ricevas po&scirc;ta&jcirc;ojn...:
      <ekz>
        homo forvoja&gcirc;inta <klr>[...]</klr> ordonis al la <tld/>isto, ke
        li viglu
        <fnt><bib>NT</bib> <lok>&Mar; 13:34</lok></fnt>;
      </ekz>
      <ekz>
        majeste staris la <tld/>isto en veluro kaj silko, kun granda ar&gcirc;entita bastono
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto kaj du servistinoj rekonis lin, &ccirc;ar la nokto estis sufi&ccirc;e luma
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 16a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto kuras post mi sur la &scirc;tuparo kun broso: permesu <klr>[ke]</klr> mi
        al vi purigos la botojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="grum.0o">grumo</ref>,
       <ref cel="lake.0o.servisto">lakeo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_b;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_c;&c_mol;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_mol;&c_s;&c_mol;&c_je;&c_r;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>port&yacute;r</trd>,
    <trd>vr&aacute;tn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>T&uuml;rsteher</trd>,
    <trd>Torw&auml;chter</trd>
  </trdgrp>
