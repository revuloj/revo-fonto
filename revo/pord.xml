  <rad>pord</rad>/o <fnt><bib>PV</bib></fnt>
</kap>


<drv mrk="pord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pord.0o.spaco">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="ferm.mal0ajxo.KOMUNE">Malferma&jcirc;o</ref>
      en <ref tip="malprt" cel="mur.0o">muro</ref> de konstrua&jcirc;o, 
      en <ref tip="malprt" cel="bar.0ilo">barilo</ref>, en flanko de
      <ref tip="malprt" cel="vetur.0ilo">veturilo</ref> a&ubreve; ma&scirc;ino por trairo:
      <ekz>
        li ne povas trairi tra nia mallar&gcirc;a <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        la kovriloj de l' fenestroj kaj la <tld/>oj restis fermitaj la tutan tagon
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>okurteno ek&jcirc;eti&gcirc;is flanken, kaj sur la sojlo de la <tld/>o sin montris
        alta, gracia virina figuro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sidis &ccirc;e la <tld/>o de la tendo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        sie&gcirc;i ies <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se vi agos malbone, la peko ku&scirc;os &ccirc;e la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ora &scirc;losilo &ccirc;iun <tld/>on malfermas
        <klr>(<ref cel="acxet.sub0i">suba&ccirc;etas</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en bona ordo tra la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se &ccirc;iu balaos anta&ubreve; sia <tld/>o, tiam en la tuta urbo estos ordo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pelu mizeron tra l' <tld/>o, &gcirc;i revenos tra l' fenestro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        diablo ne &ccirc;iam unu <tld/>on sie&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="ir.en0ejo">enirejo</ref>
    <refgrp tip="sub">
       <ref cel="luk.0o">luko</ref>,
       <ref cel="potern.0o">poterno</ref>
    </refgrp>
    <refgrp tip="prt">
      <ref cel="ans.0o">anso</ref>,
      <ref cel="klink.0o.pordanso">klinko<sncref/></ref>,
      <ref cel="klap.0o.pordo">klapo<sncref/></ref>,
      <ref cel="serur.0o">seruro</ref>
    </refgrp>
    <trd lng="de">T&uuml;r&ouml;ffnung</trd>
    <trd lng="es">portal</trd>
    <trd lng="nl">doorgang</trd>
  </snc>
  <snc mrk="pord.0o.fermajxo">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="plat.0o">Plato</ref>, kiu fermas <tld/>an spacon, kaj klape a&ubreve; glite formovi&gcirc;as
      por ebligi trairon:
      <ekz>
        &scirc;losi la <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        li fermis kolere la <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili aliris, por elrompi la <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>o estis malgranda tabuleto, sur kiu per grandaj literoj estis
        skribita: &leftquot;tiu &ccirc;i domo estas vendata&rightquot;
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        subite oni frapas tre malla&ubreve;te sur la <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        dezirante sin sciigi, <klr>[ili]</klr> iris <ind>frapi sur la <tld/>on</ind> 
        <klr>(peti <ref cel="help.0o">helpo</ref>n a&ubreve; konsilon)</klr>
        de diversaj amikoj, kaj fine alvenis en la hejmon de Klozelo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
        <trd lng="de">an die <ind>T&uuml;r</ind> klopfen <klr>(mit einer Bitte)</klr></trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>lasi la <tld/>on malfermita</ind> 
        <klr>(rezervi la eblon)</klr>
        al pliaj sendependigaj provoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2013/010900.php">Angla hundo, skota vosto</url></vrk>
          <lok>2013</lok>
        </fnt>.
        <trd lng="de">die <ind>T&uuml;r</ind> offen lassen</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flug.0ilo.svingigxi">flugilo<sncref/></ref>,
      <ref cel="klap.0o.pordo">klapo<sncref/></ref>
    </refgrp>
    <trd lng="de">T&uuml;r</trd>
    <trd lng="es">puerta</trd>
    <trd lng="nl">deur</trd>
  </snc>
  <trd lng="af">deur</trd>
  <trd lng="am">&#x1260;&#x122D;</trd>
  <trd lng="ar">&#x628;&#x627;&#x628;</trd>
  <trd lng="be">&c_d;&c_z;&c_mol;&c_v;&c_je;&c_r;&c_y;</trd>
  <trd lng="bg">&c_v;&c_r;&c_a;&c_t;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_gata;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>branka</trd>,
    <trd>dve&rcaron;e</trd>,
    <trd>pr&uring;chod</trd>
  </trdgrp>
  <trd lng="de">T&uuml;r</trd>
  <trdgrp lng="en">
    <trd>door</trd>,
    <trd>gate</trd>
  </trdgrp>
  <trd lng="fr">porte</trd>
  <trd lng="he">&dalet;&lamed;&tav;</trd>
  <trd lng="hu">ajt&oacute;</trd>
  <trd lng="id">pintu</trd>
  <trd lng="ne">&#x922;&#x94B;&#x915;&#x93E;</trd>
  <trd lng="pl">drzwi</trd>
  <trd lng="pt">porta</trd>
  <trd lng="ru">&c_d;&c_v;&c_je;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;nka</trd>,
    <trd>prechod</trd>
  </trdgrp>
  <trd lng="sq">dyer</trd>
  <trd lng="sv">d&ouml;rr</trd>
  <trd lng="tp">lupa</trd>
  <trd lng="zh">&#x95E8; [<ind>m&eacute;n</ind>]</trd>
</drv>

<drv mrk="pord.0ego">
  <kap><ofc>*</ofc><tld/>ego
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -eg'</lok></fnt></kap>
  <snc mrk="pord.0ego.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Ccirc;efa <ref tip="super" cel="pord.0o"><tld/>o</ref> de domo, de
      fortika urbo, por paso de veturiloj:
      <ekz>
        kun bruo oni malfermis la <tld/>egon, kaj la kale&scirc;o enveturis
        en la korton
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        kiam li venis al la <tld/>ego de la urbo, la gardistoj demandis,
        &ccirc;u ili devas anonci lin al la re&gcirc;ino
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> elportas la paka&jcirc;on ekster la <tld/>egon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        via idaro posedos la <tld/>egojn de siaj malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Gibraltara <tld/>ego <klr>(<ref cel="kol.mar0o">markolo</ref>)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la propraj poseda&jcirc;oj, kun kiuj <klr>[vi]</klr> venas al 
        la <tld/>ego de la socia vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_gata;&t_naro;</trd>
  <trdgrp lng="cs">
    <trd>br&aacute;na</trd>,
    <trd>vrata</trd>
  </trdgrp>
  <trd lng="de">Tor</trd>
  <trdgrp lng="en">
    <trd>garage <ind>door</ind></trd>,
    <trd>gate</trd>
  </trdgrp>
  <trd lng="es">port&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>portail</trd>,
    <trd>portes<klr tip="amb"> (d'une r&eacute;sidence, d'une
    ville)</klr></trd>
  </trdgrp>
  <trd lng="hu">kapu</trd>
  <trd lng="nl">poort</trd>
  <trdgrp lng="pl">
    <trd>brama</trd>,
    <trd>wrota</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;na</trd>,
    <trd>vr&aacute;ta</trd>
  </trdgrp>
  <trd lng="sv">port</trd>
</drv>

<drv mrk="pord.0egejo">
  <kap><tld/>egejo</kap>
  <snc mrk="pord.0egejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Granda <ref tip="super" cel="ir.en0ejo">enirejo</ref> en
      konstrua&jcirc;o, kiun fermas <tld/>ego:
      <ekz>
        la malluma figuro de la virino kuris <klr>[...]</klr> tra la
        arka&jcirc;an <tld/>egejon de la domo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la kastelo <klr>[&#x2026;]</klr>, tri turoj, la <tld/>egejo kaj la domo de la
        administranto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=676966">Kastelo Landsberg (Meiningen)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Toreinfahrt</trd>,
    <trd>Torweg</trd>,
    <trd>Torhaus</trd>
  </trdgrp>
  <trd lng="en">gateway</trd>
  <trd lng="fr">porche</trd>
  <trd lng="hu">kapualj</trd> 
  <trdgrp lng="nl">
    <trd>portaal</trd>,
    <trd>portiek</trd>
  </trdgrp> 
  <trd lng="pl">portal</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_malmol;&c_je;&c_z;&c_d;</trd>
</drv>

<drv mrk="pord.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pord.0isto.KOMUNE">
    <dif>
      <ref tip="super" cel="gard.0isto">Gardisto</ref> de domo, kiu kontrolas 
      la enirantojn, ricevas po&scirc;ta&jcirc;ojn...:
      <ekz>
        homo forvoja&gcirc;inta <klr>[...]</klr> ordonis al la <tld/>isto, ke
        li viglu
        <fnt><bib>NT</bib>, <lok>&Mar; 13:34</lok></fnt>;
      </ekz>
      <ekz>
        majeste staris la <tld/>isto en veluro kaj silko, kun granda ar&gcirc;entita bastono
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto kaj du servistinoj rekonis lin, &ccirc;ar la nokto estis sufi&ccirc;e luma
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto kuras post mi sur la &scirc;tuparo kun broso: permesu <klr>[ke]</klr> mi
        al vi purigos la botojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="grum.0o">grumo</ref>,
       <ref cel="lake.0o.servisto">lakeo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>port&yacute;r</trd>,
    <trd>vr&aacute;tn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>T&uuml;rsteher</trd>,
    <trd>Torw&auml;chter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>doorman</trd>,
    <trd>gatekeeper</trd>
  </trdgrp>
  <trd lng="es">portero</trd>
  <trdgrp lng="fr">
    <trd>concierge</trd>,
    <trd>gardien d'immeuble</trd>,
    <trd>portier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapus</trd>,
    <trd>port&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">portier</trd>
  <trdgrp lng="pl">
    <trd>od&zdot;wierny</trd>,
    <trd>porter</trd>,
    <trd>furtian <klr>(w klasztorze)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_r;&c_a;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">vr&aacute;tnik</trd>
</drv>

<drv mrk="pord.dom0o">
  <kap>dom<tld/>o</kap>
  <snc mrk="pord.dom0o.KOMUNE">
    <dif>
      <tld lit="P"/>o tra kiu oni eniras a&ubreve; eliras domon:
      <ekz>
        kiam ili iris el la dom<tld/>o,
        Rejnhardo donis al &scirc;i la brakon
        <fnt>
          <aut>T. Storm, trad. A. Bader</aut>,
          <vrk>Imenlago</vrk>,
          <lok>en: Du Amosentaj Noveloj, 1947</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">domovn&iacute; dve&rcaron;e</trd>
  <trdgrp lng="en">
    <trd>entry door</trd>,
    <trd>front door</trd>, <trd>exterior door</trd>
  </trdgrp>
  <trd lng="fr">porte d'entr&eacute;e<klr> (de maison)</klr></trd>
  <trd lng="pl">drzwi <klr>domu</klr></trd>
  <trd lng="sk">domov&eacute; dvere</trd>
</drv>

<drv mrk="pord.fald0o">
  <kap>fald<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.fald0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      balge
      faldebla ferma&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>folding door</trd>,
    <trd>accordion door</trd>
  </trdgrp>
  <trd lng="fr">porte pliante</trd>
  <trd lng="hu">harmonikaajt&oacute;</trd> 
  <trd lng="nl">vouwdeur</trd>
  <trdgrp lng="pl">
    <trd>drzwi sk&lstroke;adana</trd>,
    <trd>drzwi harmonijkowe</trd>
  </trdgrp>
</drv>

<drv mrk="pord.klap0o">
  <kap>klap<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.klap0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      klape
      movebla ferma&jcirc;o:
      <ekz>
        vi konstruis klap<tld/>on, per kiu mi povis faligi miajn
        <klr>[...]</klr> amatinojn en profundakvan puton
        <fnt><bib>SatirRak</bib>, <lok>perfekta civitano</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="pord.klap0o.RIM_1">
      Vortaroj kaj verkoj prezentas ordinare &leftquot;klap<tld/>on&rightquot; nur kun
      la senco de klapa <ref tip="vid" cel="pord.plank0o.ARKI">plank<tld/>o</ref>. Tamen la vorto ta&ubreve;gas
      anka&ubreve; por vertikala <tld/>o kun klapego, kontraste al fald-,
      &scirc;ov-<tld/>oj kaj aliaj.
      <aut>&jcirc;v</aut>
    </rim>
  </snc>
  <trdgrp lng="cs">
    <trd>poklop</trd>,
    <trd>skl&aacute;p&ecaron;c&iacute; dve&rcaron;e</trd>
  </trdgrp>
  <trd lng="en">trapdoor</trd>
  <trd lng="fr">porte &agrave; battants</trd>
  <trd lng="hu">csap&oacute;ajt&oacute;</trd>
  <trd lng="nl">klapdeur</trd>
  <trd lng="pl">drzwi skrzyd&lstroke;owe</trd>
  <trd lng="sk">poklop</trd>
</drv>

<drv mrk="pord.kluz0o">
  <kap>kluz<tld/>o</kap>
  <snc mrk="pord.kluz0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Movebla parto de
      <ref tip="malprt" cel="kluz.0o.MAR">kluzo</ref>,
      per kiu oni &gcirc;in fermas kaj malfermas:
      <ekz>
        li iradis inspektadi la kluz<tld/>on
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/III.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">lock gate</trd>
  <trd lng="fr">porte d'&eacute;cluse</trd>
  <trd lng="pl">wrota <klr>&sacute;luzy</klr></trd>
</drv>

<drv mrk="pord.lev0o">
  <kap>lev<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="pord.lev0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="pord.0o.spaco"><tld lit="P"/>o</ref> kun
      vertikale levebla ferma&jcirc;o, ekzemple en <ref tip="malprt" cel="kluz.0o.MAR">kluzon</ref>.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>drop gate</trd>,
    <trd>guillotine gate</trd>, <trd>vertical door</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>emel&odblac;kapu</trd>,
    <trd>zsilipkapu</trd>
  </trdgrp>
  <trd lng="es">puente levadizo</trd>
</drv>

<drv mrk="pord.plank0o">
  <kap>plank<tld/>o</kap>
  <snc mrk="pord.plank0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ku&scirc;a <ref tip="super" cel="pord.0o.spaco"><tld/>o</ref> en planko
      super &scirc;tuparo a&ubreve; alirebla per &scirc;tupetaro:
      <ekz>
        &gcirc;i eltiris la feran bolton el la fermita plank<tld/>o kaj
        &scirc;telece
        malsupreniris al la malliberulo
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
