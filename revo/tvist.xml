<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tvist</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="tvist.0o">
  <kap><tld/>o</kap>
  <snc mrk="tvist.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Rapida para <ref tip="super" cel="danc.0o">danco</ref>
      populari&gcirc;inta en la 1960aj jaroj.
    </dif>
  </snc>
  <snc mrk="tvist.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>odanco:
      <ekz>
        <ctl>tendara <tld/>o</ctl>
        <fnt>
        <aut>trad. Jefim Zajdman</aut>,
        <vrk>Tendara Tvisto</vrk>
        <lok><url ref="http://www.ikso.net/kantaro/Kantaro-Vikio">
        Kantaro-Vikio</url>, Oraj Kantoj-4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Twist</trd>
  <trd lng="fr">twist<klr> (danse)</klr></trd>
  <trd lng="hu">twist</trd>
  <trd lng="it">twist</trd>
  <trd lng="nl">twist</trd>
  <trd lng="no">twist</trd>
  <trd lng="pl">twist</trd>
  <trdgrp lng="pt">
   <trd>tu&iacute;ste</trd>,
   <trd>twist</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_v;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">twist</trd>
</drv>
</art>

