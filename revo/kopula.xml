<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kopulaci</rad>/o
</kap>
<drv mrk="kopula.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Seksa kuni&gcirc;o de masklo kun ino, per kio ebli&gcirc;as la reproduktado
      de la specio:
      <ekz>la emo al <tld/>o estas kvaza&ubreve; la malsato de la specio.</ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="koit.0o">koito</ref>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_p;&c_l;&c_o;&c_d;&c_o;&c_t;&c_v;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>    
  </trdgrp>
</drv>
<drv mrk="kopula.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>