<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: proceso.xml,v 1.17 2024/02/17 07:52:36 revo Exp $">
<kap>
  <rad>procesor</rad>/o
</kap>
<drv mrk="proceso.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="malprt" cel="komput.0ilo">Komputila</ref> organo
      plenumanta komandojn:
      <ekz>
        &ccirc;iuj lastatempaj <tld/>oj estas unu-blataj, do kun enblata
        staplo<fnt><aut>Frank Arnaud</aut>: <vrk>Intel alfrontas
        konkurencon</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 10a,
        p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="trakt.0ilo">traktilo</ref>
    <ref tip="malprt" cel="aparat.0aro.KOMP">aparataro</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;&c_a;&c_r;</trd>
  <trd lng="ca">processador</trd>
  <trd lng="cs">procesor</trd>
  <trd lng="de">Prozessor</trd>
  <trd lng="en">processor</trd>
  <trd lng="es">procesador</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_r;&a_d;&a_A;&a_z;&a_n;&a_d;&a_h;</trd>,
    <trd>&f_p;&a_r;&a_d;&a_A;&a_z;&a_sh;&f_g;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>processeur</trd>
  </trdgrp>
  <trd lng="hu">processzor</trd>
  <trdgrp lng="ja">
    <trd>&#x30D7;&#x30ED;&#x30BB;&#x30C3;&#x30B5;</trd>,
    <trd>&#x51E6;&#x7406;&#x88C5;&#x7F6E;  <pr>&#x3057;&#x3087;&#x308A;&#x305D;&#x3046;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="nl">processor</trd>
  <trd lng="pl">procesor</trd>
  <trd lng="pt">processador</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>
  <trd lng="sk">procesor</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_c;&c_je;&c_s;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x5904;&#x7406;&#x5668; <pr>ch&#x1D4;l&#x1D0;q&#xEC;</pr></trd>,
    <trd>&#x8655;&#x7406;&#x5668; <pr>ch&#x1D4;l&#x1D0;q&#xEC;</pr></trd>,
    <trd>&#x5FAE;&#x5904;&#x7406;&#x5668; <pr>w&#x113;ich&#x1D4;l&#x1D0;q&#xEC;</pr></trd>,
    <trd>&#x5FAE;&#x8655;&#x7406;&#x5668; <pr>w&#x113;ich&#x1D4;l&#x1D0;q&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="proceso.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Tiu organo de
      <ref tip="malprt" cel="komput.0ilo">komputilo</ref>,
      kiu plenumas la plejan parton da laboro, kaj rekte a&ubreve; pere regas
      la aliajn aran&gcirc;a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;&c_a;&c_r;</ind></trd>
  <trdgrp lng="en">
    <trd>central processor</trd>,
    <trd>central processing unit</trd>, <trd>CPU</trd>
  </trdgrp>
  <trd lng="es">procesador principal</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_A;&a_H;&a_d; &f_p;&a_r;&a_d;&a_A;&a_z;&a_sh; &a_m;&a_r;&f_k;&a_z;&f_y;</trd>,
    <trd>&f_p;&a_r;&a_d;&a_A;&a_z;&a_n;&a_d;&a_h;&a_hamza_sure; &a_m;&a_r;&f_k;&a_z;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>CPU</trd>,
    <trd>unit&eacute; centrale</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>CPU</trd>,
    <trd>k&ouml;zponti egys&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">hoofdprocessor</trd>
  <trd lng="pl">procesor g&lstroke;&oacute;wny</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>,
    <trd>&c_C;&c_P;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E3B;&#x673A; <pr>zh&#x1D4;j&#x12B;</pr></trd>,
    <trd>&#x4E3B;&#x6A5F; <pr>zh&#x1D4;j&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="proceso.krom0o">
  <kap>krom<tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aparta cirkvito en
      <ref tip="malprt" cel="komput.0ilo">komputilo</ref>,
      kiu kapabligas la &ccirc;efprocesoron plenumi kromajn funkciojn
      a&ubreve; mem plenumas kroman laboron dum la &ccirc;efprocesoro
      faras ion alian.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>dopl&ncaron;kov&yacute; procesor</trd>,
    <trd>vedlej&scaron;&iacute;</trd>
  </trdgrp>
  <trd lng="en">coprocessor</trd>
  <trd lng="es">coprocesador</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&#x200C;&f_p;&a_r;&a_d;&a_A;&a_z;&a_n;&a_d;&a_h;</trd>,
    <trd>&f_k;&a_m;&f_k;&#x200C;&f_p;&a_r;&a_d;&a_A;&a_z;&a_n;&a_d;&a_h;</trd>,
    <trd>&f_p;&a_r;&a_d;&a_A;&a_z;&a_n;&a_d;&a_h;&a_hamza_sure; &a_f;&a_r;&a_ayn;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>seg&eacute;dprocesszor</trd>,
    <trd>koprocesszor</trd>
  </trdgrp>
  <trd lng="nl">coprocessor</trd>
  <trd lng="pl">procesor dodatkowy</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>
  <trd lng="sk">ved&lcaron;aj&scaron;&iacute; procesor</trd>
  <trdgrp lng="zh">
    <trd>&#x534F;&#x5904;&#x7406;&#x5668; <pr>xi&#xE9;ch&#x1D4;l&#x1D0;q&#xEC;</pr></trd>,
    <trd>&#x5354;&#x8655;&#x7406;&#x5668; <pr>xi&#xE9;ch&#x1D4;l&#x1D0;q&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
