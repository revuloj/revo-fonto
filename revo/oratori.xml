<kap><rad>oratori</rad>/o <fnt><bib>MuzTerm</bib></fnt></kap>
<drv mrk="oratori.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Ampleksa <ref tip="super" cel="kompon.0ajxo">kompona&jcirc;o</ref>
      por sol-kantistoj, &hcirc;oro kaj orkestro prezentanta rakonton sen
      aktorado:
      <ekz>
        <nom>Liszt</nom> <klr>[&#x2026;]</klr> komponis multajn pecojn kun hungarmuzika 
        fono, ekzemple la Hungarajn rapsodiojn kaj la <tld/>on Sankta Elizabeto
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010588.php">Geniulo mondcivitana</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="oper.0o">opero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="cs">oratorium</trd>
  <trd lng="de">Oratorium</trd>
  <trd lng="es">oratorio</trd>
  <trd lng="fr">oratorio</trd>
  <trd lng="hu">orat&oacute;rium</trd>
  <trd lng="it">oratorio<klr> (mus.)</klr></trd>
  <trd lng="nl">oratorium</trd>
  <trd lng="pl">oratorium</trd>
  <trd lng="pt">orat&oacute;rio</trd>
  <trd lng="ro">oratoriu</trd>
  <trd lng="ru">&c_o;&c_r;&c_a;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">orat&oacute;rium</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x440;&#x430;&#x442;&#x43E;&#x440;&#x456;&#x44F;</trd>,
    <trd>&#x43C;&#x43E;&#x43B;&#x438;&#x442;&#x43E;&#x432;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
