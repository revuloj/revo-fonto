<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>3</ofc>
<rad>bilet</rad>/o
</kap>
<drv mrk="bilet.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Oficiala paperfolieto, kiu rajtigas por eniro a&ubreve; partopreno:
<ekz>fervoja, teatra, loteria <tld/>o;</ekz>
<ekz>flug<tld/>o.</ekz>
</dif>
<trdgrp lng="de">
   <trd>Eintrittskarte</trd>,
   <trd>Fahrkarte</trd>,
   <trd>Fahrschein</trd>,
   <trd>Flugschein</trd>,
   <trd>Billett [CH]</trd></trdgrp>
<trd lng="en">ticket</trd>
<trdgrp lng="fr">
  <trd>billet <klr>(d'acc&egrave;s)</klr></trd>,
  <trd>ticket</trd></trdgrp> 
<trdgrp lng="nl">
    <trd>ticket</trd>,
    <trd>toegangsbewijs</trd>,
    <trd>kaartje</trd>
  </trdgrp>
</snc>

      <snc mrk="bilet.0o.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          Paperfolieto, emisiita de <ref cel="bank.0o">banko</ref>, kun 
          le&gcirc;e garantiata mona valoro.
        </dif>
        <trdgrp lng="de">
           <trd>Banknote</trd>,
           <trd>Geldschein</trd></trdgrp>
        <trdgrp lng="en">
           <trd>bank note</trd>,
           <trd>bankbill</trd>,
           <trd>bill of money</trd></trdgrp>
        <trdgrp lng="fr">
          <trd>billet<klr tip="ind"> (monnaie)</klr></trd>,
          <trd>billet de banque</trd></trdgrp> 
        <trdgrp lng="nl">
          <trd>biljet<klr tip="ind"> (geld)</klr></trd>,
          <trd>bankbiljet</trd>,
          <trd>bankbriefje</trd>,
          <trd>briefje<klr tip="ind"> (geld)</klr></trd>
        </trdgrp>
<trdgrp lng="ru">
           <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j; 
        &c_b;&c_i;&c_l;&c_je;&c_t;</trd>,
           <trd>&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;</trd>
        </trdgrp>
      </snc>

<trdgrp lng="cs">
  <trd>l&iacute;stek</trd>,
  <trd>j&iacute;zdenka</trd>
</trdgrp>
<trd lng="ru">&c_b;&c_i;&c_l;&c_je;&c_t;</trd>
<adm>
  Kontroli, &ccirc;u la &ccirc;e&hcirc;aj tradukoj plu
  validas. <aut>MB</aut>
</adm>
</drv>

<drv mrk="bilet.bank0o">
<kap>bank<tld/>o</kap>
<snc>
<ref tip="dif" cel="bilet.0o.EKON"><tld/>o <sncref ref="bilet.0o.EKON"/>.</ref>
</snc>
</drv>

</art>
