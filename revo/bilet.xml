<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>bilet</rad>/o
</kap>
<drv mrk="bilet.0o">
  <kap><tld/>o</kap>
  <snc mrk="bilet.0o.eniro">
    <dif>
      Oficiala paperfolieto, kiu rajtigas por eniro a&ubreve; partopreno:
      <ekz>
        fervoja, teatra, loteria <tld/>o;
      </ekz>
      <ekz>
        flug<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_ib;&c_l;&c_je;&c_t;</trd>,
      <trd>&c_k;&c_v;&c_ib;&c_t;&c_o;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>l&iacute;stek</trd>,
      <trd>j&iacute;zdenka</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Eintrittskarte</trd>,
      <trd>Fahrkarte</trd>,
      <trd>Fahrschein</trd>,
      <trd>Flugschein</trd>,
      <trd>Billett [CH]</trd>
    </trdgrp>
    <trd lng="en">ticket</trd>
    <trd lng="es">boleto</trd>
    <trdgrp lng="fr">
      <trd>billet <klr>(d'acc&egrave;s)</klr></trd>,
      <trd>ticket</trd>
    </trdgrp>
    <trd lng="hu">jegy<klr tip="ind"> (pl. bel&eacute;pti)</klr></trd>
    <trd lng="he">&kaf;&resh;&tet;&yod;&samekh;</trd>
    <trdgrp lng="nl">
      <trd>ticket</trd>,
      <trd>toegangsbewijs</trd>,
      <trd>kaartje</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_i;&c_l;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="bilet.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="bilet.mon0o">mon<tld/>o</ref>
    <ekz>
      plenigu &ccirc;i tiun sakon per spesoj, grandaj <tld/>oj, kaj faru tuj
      <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 1a</lok></fnt>!
    </ekz>
  </snc>
  <trd lng="es">billete</trd>
</drv>

<drv mrk="bilet.bank0o">
  <kap>bank<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <ref tip="dif" cel="bilet.mon0o">mon<tld/>o</ref>
  </snc>
</drv>

<drv mrk="bilet.mon0o">
  <kap>mon<tld/>o<fnt>Z</fnt></kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Paperfolieto, emisiita de <ref cel="bank.0o">banko</ref>,
      kun le&gcirc;e garantiita mona valoro:
      <ekz>
        en tiuj fruaj tagoj oni devis flankenlasi provizore
        aliajn pa&scirc;ojn por starigo de aparta banko de la UL
        <klr>(...)</klr> kaj por eldono de moneroj kaj
        mon<tld/>oj
        <fnt>
        <aut>Lawrence MEE</aut>, <vrk>La stelo</vrk>.
        <bib>Monato</bib>.
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Banknote</trd>,
      <trd>Geldschein</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>bank note</trd>,
      <trd>bankbill</trd>,
      <trd>bill of money</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>billet<klr tip="ind"> (monnaie)</klr></trd>,
      <trd>billet de banque</trd>
    </trdgrp>
    <trd lng="hu">bankjegy</trd>
    <trd lng="he">&shin;&tet;&resh;</trd>
    <trdgrp lng="nl">
      <trd>biljet<klr tip="ind"> (geld)</klr></trd>,
      <trd>bankbiljet</trd>,
      <trd>bankbriefje</trd>,
      <trd>briefje<klr tip="ind"> (geld)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j; &c_b;&c_i;&c_l;&c_je;&c_t;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;</trd>,
      <trd>&c_k;&c_u;&c_p;&c_ju;&c_r;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="bilet.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="bilet.0ujo.mona">
    <ref tip="dif" cel="paper.mon0ujo">monpaperujo</ref>
  </snc>
</drv>
</art>
