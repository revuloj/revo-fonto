<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bilet.xml,v 1.37 2021/06/17 06:38:32 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>bilet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bilet.0o">
  <kap><tld/>o</kap>
  <snc mrk="bilet.0o.KOMUNE">
    <dif>
      Oficiala paperfolieto, kiu rajtigas por eniro a&ubreve;
      partopreno:
      <ekz>
        fervoja, teatra, loteria <tld/>o;
      </ekz>
      <ekz>
        li venis al sia vagono, montris <tld/>on, diris al la pola
        vagonservisto <ctl>dankon</ctl>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://www.esperanto.be/fel/mon/006805.html">Kota Akvo:
          Konstanten Krysakov</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        prenu samtempe anka&ubreve; po&scirc;toveturan <tld/>on
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ccirc;eti al si <tld/>on por la teatro
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas esplori kiom familio racie povas elspezi por
        loteri<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/006920.html">Loterioj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aviz.0o">avizo</ref>,
      <ref cel="kart.0o">karto</ref>,
      <ref cel="kontra6.0o">kontramarko</ref>,
      <ref cel="kupon.0o">kupono</ref>,
      <ref cel="slip.0o">slipo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_ib;&c_l;&c_je;&c_t;</trd>,
      <trd>&c_k;&c_v;&c_ib;&c_t;&c_o;&c_k;</trd>
    </trdgrp>
    <trd lng="bg">&c_b;&c_i;&c_l;&c_je;&c_t;</trd>
    <trdgrp lng="ca">
      <trd>bitllet</trd>,
      <trd>entrada</trd>
    </trdgrp>
    <trd lng="ms">tiket kapal terbang</trd>
    <trdgrp lng="cs">
      <trd>l&iacute;stek</trd>,
      <trd>j&iacute;zdenka</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Eintrittskarte</trd>,
      <trd>Fahrkarte</trd>,
      <trd>Fahrschein</trd>,
      <trd>Flugschein</trd>,
      <trd>Billett [CH]</trd>
    </trdgrp>
    <trd lng="en">ticket</trd>
    <trd lng="es">boleto</trd>
    <trd lng="hu">jegy<klr tip="ind"> (pl. bel&eacute;pti)</klr></trd>
    <trd lng="he">&kaf;&resh;&tet;&yod;&samekh;</trd>
    <trdgrp lng="nl">
      <trd>ticket</trd>,
      <trd>toegangsbewijs</trd>,
      <trd>kaartje</trd>
    </trdgrp>
    <trd lng="oc">bilhet</trd>
    <trdgrp lng="pl">
      <trd>bilet</trd>,
      <trd>wej&sacute;ci&oacute;wka</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_i;&c_l;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="bilet.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="bilet.mon0o">Mon<tld/>o</ref>:
      <ekz>
        tri kvinspesmilaj <tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne &ccirc;iu papereto estas banka <tld/>o
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        plenigu &ccirc;i tiun sakon per spesoj, grandaj <tld/>oj, kaj faru
        tuj
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 1a</lok></fnt>!
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;&c_a;</trd>,
      <trd>&c_b;&c_ib;&c_l;&c_je;&c_t; <klr>(&c_b;&c_a;&c_n;&c_k;&c_a;&c_w;&c_s;&c_k;&c_ib;, &c_s;&c_k;&c_a;&c_r;&c_b;&c_u;)</klr></trd>
    </trdgrp>
    <trd lng="pl">banknot</trd>
  </snc>
  <trd lng="bo">&#xF4A;&#xF72;&#xF0B;&#xF40;&#xF0B;&#xF66;&#xF72;&#xF0B;</trd>
  <trd lng="cs">l&iacute;stek <klr>(vstupenka| j&iacute;zdenka)</klr></trd>
  <trd lng="es">billete</trd>
  <trdgrp lng="fr">
    <trd>billet</trd>,
    <trd>ticket</trd>
  </trdgrp>
  <trd lng="sk">cestovn&yacute; l&iacute;stok</trd>
  <trd lng="tp">lipu</trd>
</drv>

<drv mrk="bilet.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="bilet.0isto.KOMUNE">
    <dif>
      Homo kiu vendas a&ubreve; kontrolas <tld/>ojn,
      ekzemple en publika veturilo, &ccirc;e la enirejo de spekta&jcirc;ejo...:
      <ekz>
        la olda <tld/>istino helpas
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>poin&ccedil;onneur</trd>,
    <trd>receveur<klr> (de tickets)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bileter</trd>,
    <trd>kontroler</trd>
  </trdgrp>
</drv>

<drv mrk="bilet.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="bilet.0ujo.mona">
    <dif>
      <ref tip="dif" cel="paper.mon0ujo">Monpaperujo</ref>:
      <ekz>
        la librotenisto, premante subbrake la valoregan tekon, tiris el
        sia <tld/>ujo &ccirc;ervoncon kaj montris &gcirc;in al la &scirc;oforo
        <fnt>
          <vrk><url ref="http://www.miresperanto.com/biblioteko/margarita/17.htm">Chapitro
          17 - miresperanto.com</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">portefeuille</trd>
    <trd lng="pl">portfel</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>,
    <trd>&c_g;&c_a;&c_m;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="cs">schr&aacute;nka na l&iacute;stky</trd>
  <trd lng="de">Geldb&ouml;rse</trd>
  <trd lng="sk">schr&aacute;nka na l&iacute;stky</trd>
</drv>

<drv mrk="bilet.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="bilet.flug0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <tld lit="B"/>o rajtiga por
      aeroplana flugo:
      <ekz>
        de 1994 &gcirc;is 1997 la prezo de enlanda flug<tld/>o levi&gcirc;is je 114
        %
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007047.html">&Ccirc;inio:
          Enlanda aviado multekostas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        La eta fluglinio <klr>...</klr>
        kalkulas la prezon de la 
        flug<tld/>o la&ubreve; la pezo de la pasa&gcirc;eroj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="esperantaretradio.blogspot.com/2013/04/pagi-flugbileton-lau-pezo.html&acirc;??">
            Pagi Flugbileton la&ubreve; Pezo</url></vrk>,
          <lok>Esperanta Retradio, 2013-04-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_v;&c_ib;&c_ja;&c_k;&c_v;&c_ib;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_a;&c_v;&c_ib;&c_ja;&c_b;&c_ib;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">letenka</trd>
  <trd lng="en">air ticket</trd>
  <trd lng="fr">billet d'avion</trd>
  <trd lng="pl">bilet lotniczy</trd>
  <trd lng="sk">letenka</trd>
</drv>

<drv mrk="bilet.mon0o">
  <kap>mon<tld/>o,
  <var><kap>bank<tld/>o</kap></var></kap>
  <snc mrk="bilet.mon0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Paperfolieto, emisiita de <ref cel="bank.0o">banko</ref>,
      kun le&gcirc;e garantiita mona valoro:
      <ekz>
        eleganta koverto, en kiu oni povis vidi kelke da mon<tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        Jo&ccirc;jo <klr>(...)</klr> estis devigita preni 
        de la kolonelo unu centrublan bank<tld/>on
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen 
        titolo</vrk>, <lok>[1898?]</lok></fnt>;
      </ekz>
      <ekz>
        1000-&scirc;illinga bank<tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=56">A&ubreve;strio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj fruaj tagoj oni devis flankenlasi provizore
        aliajn pa&scirc;ojn <klr>(...)</klr> por eldono de moneroj
        kaj mon<tld/>oj
        <fnt>
        <aut>L. Mee</aut>, <vrk>La stelo</vrk>,
        <bib>Monato</bib>.
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;&c_a;</trd>,
      <trd>&c_b;&c_ib;&c_l;&c_je;&c_t; <klr>(&c_b;&c_a;&c_n;&c_k;&c_a;&c_w;&c_s;&c_k;&c_ib;, &c_s;&c_k;&c_a;&c_r;&c_b;&c_u;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Banknote</trd>,
      <trd>Geldschein</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>bank note</trd>,
      <trd>bankbill</trd>,
      <trd>bill of money</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>billet<klr tip="ind"> (monnaie)</klr></trd>,
      <trd>billet de banque</trd>
    </trdgrp>
    <trd lng="hu">bankjegy</trd>
    <trd lng="he">&shin;&tet;&resh;</trd>
    <trdgrp lng="nl">
      <trd>biljet<klr tip="ind"> (geld)</klr></trd>,
      <trd>bankbiljet</trd>,
      <trd>bankbriefje</trd>,
      <trd>briefje<klr tip="ind"> (geld)</klr></trd>
    </trdgrp>
    <trd lng="pl">banknot</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;
      &c_b;&c_i;&c_l;&c_je;&c_t;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;</trd>,
      <trd>&c_k;&c_u;&c_p;&c_ju;&c_r;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">bankovka</trd>
  <trdgrp lng="sk">
    <trd>bankovka</trd>,
    <trd>papierov&yacute; peniaz</trd>
  </trdgrp>
</drv>

<drv mrk="bilet.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc mrk="bilet.sen0ulo.KOMUNE">
    <dif>
      Enirinto en lokon sen rajtiga <tld/>o, kontra&ubreve;regula:
      <ekz>
        kun iom da helpo la sen<tld/>ulo sukcesas
        atingi la lar&gcirc;an baga&gcirc;breton plej supre
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_mol;&c_b;&c_ib;&c_l;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_ja;&c_c; <klr>(&c_r;&c_a;&c_z;&c_m;. &c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
  </trdgrp>
  <trd lng="fr">fraudeur<klr> (sans billet)</klr></trd>
</drv>
</art>
