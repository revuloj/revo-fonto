<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>arkimandrit</rad>/o
    </kap>

    <drv mrk="arkimandrit.0o">
      <kap><tld/>o <fnt><bib>PIV1</bib></fnt></kap>
      <uzo tip="fak">KRI</uzo>
      <snc mrk="arkimandrit.0o.orto">
    	<dif>
          En ortodoksismo,
          <ref tip="super" cel="monahx.0ejestro">mona&hcirc;ejestro</ref>
    	  de grava mona&hcirc;ejo a&ubreve; estro de pluraj mona&hcirc;ejoj; rango pli
          alta ol <ref cel="higumen.0o">higumeno</ref>.
    	</dif>
	<rim>
          Kp <ref cel="provincial.0o">provincialo</ref> en katolikismo.
        </rim>
      </snc>
      <trd lng="en">archimandrite</trd>
      <trd lng="grc">&alfa_psili;&rho;&chi;&jota;&my;&alfa;&ny;&delta;&rho;&jota_acute;&tau;&eta;&sigma_fina;</trd>
      <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_m;&c_a;&c_n;&c_d;&c_r;&c_i;&c_t;</trd>
    </drv>
  </art>

