<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ultra</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ultra.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="ultra.0.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Super la ordinaraj valoroj, super difinita limo:
      <ekz>
        <ref tip="vid" cel="ultrason.0o.FIZ">ultrasono</ref>;
      </ekz>
      <ekz>
        <ref tip="vid" cel="ultraviol.0a.FIZ">ultraviola</ref>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="infra.0.SCI">infra-</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="super.0.pref_SPAC">super-<sncref/></ref>,
      <ref cel="trans.0.pref_SPAC">trans-<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fr">ultra-</trd>
</drv>
</art>

