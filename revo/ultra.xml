<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ultra.xml,v 1.5 2023/04/12 08:32:02 revo Exp $">
<kap>
  <rad>ultra</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ultra.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="ultra.0.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Super la ordinaraj valoroj, super difinita limo:
      <ekz>
        <ref tip="vid" cel="ultrason.0o.FIZ">ultrasono</ref>;
      </ekz>
      <ekz>
        <ref tip="vid" cel="ultraviol.0a.FIZ">ultraviola</ref>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="infra.0.SCI">infra-</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="super.0_.space">super-<sncref/></ref>,
      <ref cel="trans.0_.space">trans-<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;-</trd>,
    <trd>&c_n;&c_a;&c_d;-</trd>,
    <trd>&c_z;&c_v;&c_y;&c_sh;-</trd>
  </trdgrp>
  <trd lng="fr">ultra-</trd>
  <trdgrp lng="pl">
    <trd>ultra-</trd>,
    <trd>nad-</trd>
  </trdgrp>
  <trd lng="uk">&c_u;&#x43B;&#x44C;&c_t;&c_r;&c_a;&#8230;</trd>
</drv>
</art>

