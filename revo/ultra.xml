<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ultra.xml,v 1.4 2022/03/28 11:08:53 revo Exp $">
<kap>
  <rad>ultra</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ultra.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="ultra.0.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Super la ordinaraj valoroj, super difinita limo:
      <ekz>
        <ref tip="vid" cel="ultrason.0o.FIZ">ultrasono</ref>;
      </ekz>
      <ekz>
        <ref tip="vid" cel="ultraviol.0a.FIZ">ultraviola</ref>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="infra.0.SCI">infra-</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="super.0_.space">super-<sncref/></ref>,
      <ref cel="trans.0_.space">trans-<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fr">ultra-</trd>
  <trdgrp lng="pl">
    <trd>ultra-</trd>,
    <trd>nad-</trd>
  </trdgrp>
  <trd lng="uk">&#x443;&#x43B;&#x44C;&#x442;&#x440;&#x430;&#x2026;</trd>
</drv>
</art>

