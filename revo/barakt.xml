<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barakt.xml,v 1.25 2023/10/14 08:39:48 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>barakt</rad>/i
</kap>
<drv mrk="barakt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Forte skui, movi la membrojn, luktante por sin liberigi:
      <ekz>
        <tld/>i en &scirc;limo, en akvo;
      </ekz>
      <ekz>
        la kaptito furioze <tld/>is<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i kontra&ubreve; la morto;
      </ekz>
      <ekz>
        <tld/>i kiel fi&scirc;o ekster la akvo<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="tord">tordi&gcirc;i</ref>,
      <ref cel="star">kontra&ubreve;stari</ref>,
      <ref cel="ced.mal0i">malcedi</ref>,
      <ref cel="rezist.0i">rezisti</ref>,
      <ref cel="lukt.0i">lukti</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd><ind>difreta&ntilde;</ind>, en em zifreta&ntilde;</trd>,
      <trd>diskrapa&ntilde;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>verg&odblac;dik</trd>,
      <trd>kap&aacute;l&oacute;dzik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>worstelen</trd>,
      <trd>vechten</trd>
    </trdgrp>
    <trd lng="pl">szamota&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>debater-se</trd>,
      <trd>estrebuchar</trd>,
      <trd>espernear</trd>,
      <trd>sacudir-se</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_r;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte penadi:
      <ekz>
        <tld/>i kontra&ubreve; malfacila&jcirc;oj,
        kontra&ubreve; la cirkonstancoj;
      </ekz>
      <ekz>
        li <tld/>as en kontra&ubreve;diroj;
      </ekz>
      <ekz>
        multaj projektoj <tld/>is en lia kapo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nun en &scirc;ia kapo <tld/>is nova demando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la internacia vivo <tld/>as por sin organizi.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&uuml;szk&ouml;dik</trd>,
      <trd>tusakodik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kampen</trd>,
      <trd>zwoegen</trd>,
      <trd>zich <ind>uitsloven</ind></trd>
    </trdgrp>
    <trd lng="pl">boryka&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>debater-se</trd>,
      <trd>estrebuchar</trd>,
      <trd>espernear</trd>,
      <trd>sacudir-se</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_t;&c_u;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>bojovat</trd>,
    <trd>m&#xE1;vat</trd>,
    <trd>oh&#xE1;n&#x11B;t se</trd>,
    <trd>t&rcaron;epetat se</trd>,
    <trd>zm&#xED;tat se</trd>,
    <trd>z&#xE1;pasit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zappeln</trd>,
    <trd>strampeln</trd>,
    <trd>k&auml;mpfen</trd>
  </trdgrp>
  <trd lng="en">struggle</trd>
  <trdgrp lng="es">
    <trd>Debatirse</trd>,
    <trd>sacudirse</trd>,
    <trd>luchar por liberarse</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_q;&a_l;&a_A; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_d;&a_s;&a_t; &a_w; &f_p;&a_A; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;battre</ind></trd>,
    <trd>se <ind>d&eacute;mener</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D9;&#x5D0;&#x5D1;&#x5E7;</trd>
  <trdgrp lng="ja">
    <trd>&#12418;&#x304C;&#x304F;</trd>,
    <trd>&#12376;&#x305F;&#12400;&#x305F;&#12377;&#x308B;</trd>,
    <trd>&#x82E6;&#x95D8;&#12377;&#x308B;  <pr>&#x304F;&#12392;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bori&tcaron; sa</trd>,
    <trd>zmieta&tcaron; sa</trd>,
    <trd>z&#xE1;pasi&tcaron;<klr>(s nie&#x10D;&#xED;m)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43E;&c_r;&c_s;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_t;&c_r;&c_ib;&#x43F;&#x43E;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_je;&c_t;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_t;&c_r;&c_ib;&#x43F;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
