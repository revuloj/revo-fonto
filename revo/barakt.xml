<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barakt.xml,v 1.27 2023/10/29 19:11:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>barakt</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="barakt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="barakt.0i.skui">
    <dif>
      Forte <ref tip="super" cel="sku.0i">skui</ref>,
      movi la membrojn, luktante por sin liberigi:
      <ekz>
        kiam li &scirc;in subakvigis, <klr>[&#x2026;]</klr> &scirc;i <tld/>is sub liaj manoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam oni sidigis lin sur la herbo inter la flavaj floroj, li la&ubreve;te ekridis de
        &gcirc;ojo, <tld/>is per la piedetoj, rulis sin sur la tero
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        momente ili tiel <tld/>is en ekstazo kontra&ubreve; <klr>[&#x2026;]</klr> la amo, kiu pu&scirc;is
        unu al la alia
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kiel fi&scirc;o ekster la akvo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="tord">tordi&gcirc;i</ref>,
      <ref cel="star">kontra&ubreve;stari</ref>,
      <ref cel="ced.mal0i">malcedi</ref>,
      <ref cel="rezist.0i">rezisti</ref>,
      <ref cel="lukt.0i">lukti</ref>
    </refgrp>
    <trdgrp lng="br">
      <trd><ind>difreta&ntilde;</ind>, en em zifreta&ntilde;</trd>,
      <trd>diskrapa&ntilde;</trd>
    </trdgrp>
    <trd lng="de">zappeln</trd>
    <trdgrp lng="hu">
      <trd>verg&odblac;dik</trd>,
      <trd>kap&aacute;l&oacute;dzik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>worstelen</trd>,
      <trd>vechten</trd>
    </trdgrp>
    <trd lng="pl">szamota&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>debater-se</trd>,
      <trd>estrebuchar</trd>,
      <trd>espernear</trd>,
      <trd>sacudir-se</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_r;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6251;&#x817E; <pr>p&#x16B;teng</pr></trd>,
    <trd>&#x640F; <pr>b&#xF3;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="barakt.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte <ref tip="super" cel="pen.0i">penadi</ref>:
      <ekz>
        <tld/>i kontra&ubreve; malfacila&jcirc;oj,
        kontra&ubreve; la cirkonstancoj;
      </ekz>
      <ekz>
        en &scirc;ia kapo <tld/>is nova demando, kiu komence konsistis nur el la mallonga
        vorto: kial
        <fnt>
          <bib>Marta</bib>
        </fnt>?
      </ekz>
      <ekz>
        ni devas ofte <tld/>i pene por ricevi, kvaza&ubreve; hundoj sub la tablo, kelkajn
        paneretojn de oficiala subteno
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &ccirc;iam <tld/>is por perlabori sufi&ccirc;on
        <fnt>
          <bib>KrM</bib>
          <lok>29. Sorto Flatas, Sorto Batas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        UEA <tld/>is pro manko de financaj rimedoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro <tld/>is dum la tuta nokto, ser&ccirc;adis, dubis, ribelis, matene li
        konsoli&gcirc;is
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&uuml;szk&ouml;dik</trd>,
      <trd>tusakodik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kampen</trd>,
      <trd>zwoegen</trd>,
      <trd>zich <ind>uitsloven</ind></trd>
    </trdgrp>
    <trd lng="pl">boryka&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>debater-se</trd>,
      <trd>estrebuchar</trd>,
      <trd>espernear</trd>,
      <trd>sacudir-se</trd>
    </trdgrp>
    <trd lng="zh">&#x594B;&#x6597; <pr>f&#xE8;nd&#xF2;u</pr></trd>
</snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_t;&c_u;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>bojovat</trd>,
    <trd>m&aacute;vat</trd>,
    <trd>oh&aacute;n&ecaron;t se</trd>,
    <trd>t&rcaron;epetat se</trd>,
    <trd>zm&iacute;tat se</trd>,
    <trd>z&aacute;pasit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>strampeln</trd>,
    <trd>ringen</trd>,
    <trd>k&auml;mpfen</trd>
  </trdgrp>
  <trd lng="en">struggle</trd>
  <trdgrp lng="es">
    <trd>Debatirse</trd>,
    <trd>sacudirse</trd>,
    <trd>luchar por liberarse</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_q;&a_l;&a_A; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_d;&a_s;&a_t; &a_w; &f_p;&a_A; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;battre</ind></trd>,
    <trd>se <ind>d&eacute;mener</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&yod;&alef;&bet;&qof;</trd>
  <trdgrp lng="ja">
    <trd>&#x3082;&#x304C;&#x304F;</trd>,
    <trd>&#x3058;&#x305F;&#x3070;&#x305F;&#x3059;&#x308B;</trd>,
    <trd>&#x82E6;&#x95D8;&#x3059;&#x308B;  <pr>&#x304F;&#x3068;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bori&tcaron; sa</trd>,
    <trd>zmieta&tcaron; sa</trd>,
    <trd>z&aacute;pasi&tcaron;<klr>(s nie&ccaron;&iacute;m)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_r;&c_s;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_p;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_i;&c_d;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
