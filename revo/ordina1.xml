<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ordinat</rad>/o
</kap>
<drv mrk="ordina1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      La dua el la du
      <ref tip="super" cel="koordi.kartezia0o">karteziaj koordinatoj</ref>,
      kiuj difinas la situon de punkto sur
      <ref cel="eben.0o.MAT">ebeno</ref>:
      <ekz>
        la <tld/>on de punkto oni kutime signas per la litero y;
      </ekz>
      <ekz>
        &ccirc;e grafika&jcirc;o de funkcio de unu argumento, la <tld/>o
        reprezentas la valoron de la funkcio.
      </ekz>
    </dif>
    <ref tip="vid" cel="abscis.0o">absciso.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Ordinate</trd>,
    <trd>y-Koordinate</trd>
  </trdgrp>
  <trd lng="el">&tau;&alfa;&tau;&alfa;&gamma;&my;&epsilon_ton;&ny;&eta;</trd>
  <trdgrp lng="en">
    <trd>ordinate</trd>,
    <trd>y-coordinate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ordonn&eacute;e</trd>,
    <trd>coordonn&eacute;e y</trd>
  </trdgrp>
  <trd lng="hu">ordin&aacute;ta</trd>
  <trdgrp lng="nl">
    <trd>ordinaat</trd>,
    <trd>y-as</trd>
  </trdgrp>
  <trd lng="pl">rz&eogonek;dna</trd>
  <trdgrp lng="pt">
    <trd>ordenada</trd>,
    <trd>coordenada y</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_r;&c_d;&c_i;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>y-&c_k;&c_o;&c_o;&c_r;&c_d;&c_i;&c_n;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>ordinata</trd>,
    <trd>y-koordinat</trd>
  </trdgrp>
</drv>
</art>
