<art mrk="$Id: picx.xml,v 1.16 2024/02/17 06:43:07 revo Exp $">
<kap>
<rad>pi&ccirc;</rad>/o
</kap>
<drv mrk="picx.0o">
  <kap><tld/>o</kap>
  <snc mrk="picx.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="stl">VULG</uzo>
    <dif>
      <ref tip="dif" cel="vulv.0o">Vulvo</ref>:
      <ekz>
        ne mirinde, se doloras vin la <tld/>o, fra&ubreve;lino
        <fnt>
          <aut>Louis Beaucaire</aut>
          <vrk>Kruko kaj Baniko el Bervalo</vrk>
          <lok>jaro 1974a</lok>
        </fnt>;
      </ekz>
      <ekz>
        timante, ke tiu interna ideo de lia romano povas soni tro fremde kaj forpele, li
        volvas &gcirc;in en jupojn, pugojn, mamojn kaj <tld/>ojn, ama&jcirc;ojn
        <fnt>
          <aut>Raita Pyh&auml;l&auml;</aut>
          <vrk><url ref="&Monato;/2005/008685.php">Kesto de viveroj</url></vrk>
          <bib>Monato</bib>
          <lok>2005/07, p. 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        trafita de fulma sort' mi lekas pi&ccirc;ojn ka&scirc;ite, samseksemas ne &ccirc;iu virin' kun kiu
        mi ku&scirc;as enlite
        <fnt>
          <aut>Alejandro Cossavella</aut>
          <vrk><url ref="&Monato;/2006/008766.php">Poemoj de Alejandro Cossavella</url></vrk>
          <bib>Monato</bib>
          <lok>2006/04, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">cony</trd>
    <trdgrp lng="de">
      <trd>Muschi</trd>,
      <trd>Fotze</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cunt</trd>,
      <trd>pussy</trd>
    </trdgrp>
    <trd lng="es">co&ntilde;o</trd>
    <trd lng="fa">&f_k;&a_s;</trd>
    <trdgrp lng="fr">
      <trd>con</trd>,
      <trd>chatte</trd>
    </trdgrp>
    <trd lng="it">figa</trd>
    <trd lng="nl">kut</trd>
    <trd lng="pl">piczka</trd>
    <trd lng="pt">boceta</trd>
    <trdgrp lng="zh">
    <trd>&#x6DEB;&#x7A74; <pr>y&#xED;nxu&#xE9;</pr></trd>,
    <trd>&#x871C;&#x7A74; <pr>m&#xEC;xu&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>p&iacute;&ccaron;a</trd>,
    <trd>pi&ccaron;a</trd>
  </trdgrp>
  <trd lng="sk">pi&ccaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_ib;&#x43D;&#x43E;&c_ch;&c_ib; &c_z;&#x43E;&c_v;&#x43D;&c_ib;&c_sh;&#x43D;&c_ib; &c_s;&c_t;&c_a;&c_t;&c_je;&c_v;&c_ib; &#x43E;&c_r;&c_g;&c_a;&#x43D;&c_i;</trd>,
    <trd>&#x43F;&c_i;&c_z;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>

