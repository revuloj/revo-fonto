    <trd>een aantal</trd>,
    <trd>een zekere hoeveelheid</trd>,
    <trd>een zekere mate</trd>
  </trdgrp>
<trd lng="th">&#xE08;&#xE33;&#xE19;&#xE27;&#xE19;&#xE2B;&#xE19;&#xE36;&#xE48;&#xE07;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_o;&c_h;&c_i;</trd>,
    <trd>&c_d;&c_je;&c_ja;&c_k;&c_a; &c_k;&c_ib;&c_l;&c_mol;&c_k;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ja;&c_k;&c_o;&c_ju;&c_s;&c_mol; &c_m;&c_ib;&c_r;&c_o;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C11;&#x91CF; <pr>sh&#x1CE;oli&#xE0;ng</pr></trd>,
    <trd>&#x7A0D; <pr>sh&amacron;o</pr></trd>,
    <trd>&#x4E00;&#x70B9; <pr>y&#x12B;di&#x1CE;n</pr></trd>,
