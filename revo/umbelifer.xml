<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: umbelifer.xml,v 1.18 2023/10/21 16:50:27 revo Exp $">
<kap>
<rad>umbelifer</rad>/oj
</kap>
<drv mrk="umbelifer.0oj">
   <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Tradicia nomo de
      <ref tip="dif" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Umbelliferae</trd>):
      <ekz>
        apiacoj havas umbelojn, ombrelosimilajn florarojn, tial la alternativa nomo estas <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18064">Apiacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la kampa klik-skarabo <klr>[&#x2026;]</klr> vivas en <tld/> infloresko de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=327254">Elateredoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_o;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
   <trd lng="ca">umbel&middot;l&iacute;feres</trd>
   <trdgrp lng="de">
     <trd>Doldengew&auml;chse</trd>,
     <trd>Doldenbl&uuml;tler</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>Umbelliferae</trd>,
     <trd>celery</trd>,
     <trd>carrot and parsley family</trd>
   </trdgrp>
   <trd lng="es">umbel&iacute;feras</trd>
   <trd lng="fi"><klr tip="amb">(vanh.) </klr>sarjakukkaiset</trd>
   <trd lng="fr">Ombellif&egrave;res</trd>
   <trd lng="hu">erny&odblac;s&ouml;k</trd>
   <trdgrp lng="it">
   <trd>ombrellifere</trd>,
   <trd>apiacee</trd>,
   <trd>umbellifere</trd>
   </trdgrp>
   <trd lng="ja">&#x6563;&#x5F62;&#x79D1;  <pr>&#x3061;&#x304B;&#x305F;&#x304B;</pr></trd>
  <trd lng="nl">schermbloemigen</trd>
   <trd lng="pl">selerowate</trd>
   <trd lng="ru">&c_z;&c_o;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_je;</trd>
  <trd lng="uk">&c_z;&c_o;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_ib;</trd>
</drv>
</art>
