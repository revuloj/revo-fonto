<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fanfar.xml,v 1.18 2017/08/31 22:10:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fanfar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fanfar.0o">
  <kap><tld/>o</kap>
  <snc mrk="fanfar.0o.MUZ_muzikistaro">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref cel="muzik.0istaro.MUZ">Muzikistaro</ref>,
      kiu uzas nur blovinstrumentojn kuprajn kaj latunajn. 
    </dif>
    <trd lng="de">Blaskapelle</trd>
    <trdgrp lng="hu">
     <trd>f&uacute;v&oacute;szenekar</trd>,
     <trd>rezesbanda</trd></trdgrp>
  </snc>
  <snc mrk="fanfar.0o.MUZ_muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Gaja ario por kupraj kaj latunaj muzikiloj:
      <ekz>
        &ccirc;asa, militista <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Fanfare</trd>,
     <trd>Trompetengeschmetter</trd>,
     <trd>Tusch</trd></trdgrp>
    <trd lng="en">fanfare</trd>
    <trdgrp lng="hu">
      <trd>fanf&aacute;r</trd>,
      <trd>darab <ind>r&eacute;zf&uacute;v&oacute;sokra</ind></trd>,
    </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>fanf&aacute;ra</trd>,
    <trd>intr&aacute;da</trd>,
    <trd>tu&scaron;</trd>
  </trdgrp>
  <trd lng="es">fanfarria</trd>
  <trd lng="fr">fanfare</trd>
  <trd lng="nl">fanfare</trd>
  <trdgrp lng="pt">
    <trd>fanfarra</trd>,
    <trd>charanga</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>fanf&aacute;ry</trd>,
    <trd>tu&scaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fanfar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="fanfar.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Soni kiel <tld/>o:
      <ekz>
        la orkestro <tld/>is, kaj la amasoj da homoj
        &ccirc;irka&ubreve; Donald degelis
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">retentir<klr> (comme une fanfare)</klr></trd>
</drv>
</art>

