<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>fanfar</rad>/o
</kap>
<drv mrk="fanfar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc num="1">
    <dif>
      Muzikistaro, kiu uzas nur kuprajn kaj latunajn muzikilojn. 
    </dif>
    <trd lng="de">Blaskapelle</trd>
    <trdgrp lng="hu">
     <trd>f&uacute;v&oacute;szenekar</trd>,
     <trd>rezesbanda</trd></trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Gaja ario por kupraj kaj latunaj muzikiloj: <ekz>&ccirc;asa, militista
      <tld/>o.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Fanfare</trd>,
     <trd>Trompetengeschmetter</trd>,
     <trd>Tusch</trd></trdgrp>
    <trd lng="en">fanfare</trd>
    <trdgrp lng="hu">
      <trd>fanf&aacute;r</trd>,
      <trd>darab <ind>r&eacute;zf&uacute;v&oacute;sokra</ind></trd>,
    </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
  <trd lng="fr">fanfare</trd>
  <trd lng="es">fanfarria</trd>
  <trd lng="nl">fanfare</trd>
  <trdgrp lng="pt">
    <trd>fanfarra</trd>,
    <trd>charanga</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
</drv>
</art>

