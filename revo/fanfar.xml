<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>fanfar</rad>/o
</kap>
<drv mrk="fanfar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc num="1">
    <dif>
      Muzikistaro, kiu uzas nur kuprajn kaj latunajn muzikilojn. 
    </dif>
    <trd lng="de">Blaskapelle</trd>
  </snc>
  <snc num="2">
    <dif>
      Gaja ario por kupraj kaj latunaj muzikiloj: <ekz>&ccirc;asa, militista <tld/>o.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Fanfare</trd>,
     <trd>Trompetengeschmetter</trd>,
     <trd>Tusch</trd></trdgrp>
  </snc>
  <trd lng="fr">fanfare</trd>
  <trd lng="ru">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
</drv>
</art>

