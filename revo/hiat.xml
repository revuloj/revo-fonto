<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hiat</rad>/o
</kap>
<drv mrk="hiat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Senpera sinsekvo de vokaloj ordinare pli malpli malagrabla por
      la orelo, ekz. "la arbo; amata infano; apelacii".  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hiat</trd>,
    <trd>Hiatus</trd>
  </trdgrp>
  <trd lng="en">hiatus</trd>
  <trd lng="es">hiato</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_a;&c_t;&c_u;&c_s;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_k; &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
