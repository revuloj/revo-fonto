<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hiat</rad>/o
</kap>
<drv mrk="hiat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Senpera sinsekvo de vokaloj, konsiderata en iuj lingvoj malagrabla por
      la orelo:
      <ekz>
        la arbo;
      </ekz>
      <ekz>
        amata infano;
      </ekz>
      <ekz>
        apelacii.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hiat</trd>,
    <trd>Hiatus</trd>
  </trdgrp>
  <trd lng="en">hiatus</trd>
  <trd lng="es">hiato</trd>
  <trd lng="fr">hiatus<klr tip="ind"> (phon.)</klr></trd>
  <trd lng="nl">hiaat</trd>
  <trd lng="pl">rozziew</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_a;&c_t;&c_u;&c_s;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_k; &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_h;</trd>,
    <trd>&c_z;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
