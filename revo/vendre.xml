<drv mrk="vendre.sankta_0o">
  <kap>
    Sankta <tld lit="V"/>o
  </kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      <klr>(en <ref tip="malprt" cel="krist.0anismo">kristanismo</ref>)</klr>
      La <tld/>o anta&ubreve; <ref cel="pask.0o.KRI">Pasko</ref>:
      <ekz>
        en la liturgia pre&gcirc;o de la sankta <tld/>o (kiam oni memoras la
        morto-tagon de Jesuo)
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2008/009452.php">Pluiras, sed la&ubreve;
          rifoj,
          la dialogo inter judoj kaj la katolika eklezio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja; <ind>&c_p;&c_ja;&c_t;&c_n;&c_ib;&c_c;&c_a;</ind></trd>
