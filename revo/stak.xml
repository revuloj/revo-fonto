<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stak.xml,v 1.27 2026/01/08 06:53:15 revo Exp $">
<kap>
  <rad>stak</rad>/o
  <fnt><aut>F. Fulcher, B. Long</aut>,
    <vrk>English-Esperanto dictionary</vrk>,
    <lok>1921-1925</lok>
  </fnt>
</kap>
<drv mrk="stak.0o">
  <kap><tld/>o</kap>
  <snc mrk="stak.0o.fojna">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="amas.0o">Amaso</ref> da pajlo a&ubreve; da kunrastita fojno a&ubreve; da
      grengarboj ktp:
      <ekz>
        <ind>fojno<tld/>o</ind>;
        <trd lng="be">&c_s;&c_t;&c_o;&c_g; &c_s;&c_a;&c_l;&c_o;&c_m;&c_y;</trd>
        <trd lng="de">Heuhaufen</trd>
        <trd lng="en">haystack</trd>
        <trd lng="fr">meule de foin</trd>
        <trd lng="hu">szalmakazal</trd>
        <trd lng="ru">&c_s;&c_t;&c_o;&c_g; &c_s;&c_je;&c_n;&c_a;</trd>
      </ekz>
      <ekz>
        sur la alia flanko de la grenejo kreskis la pajlo je <tld/>oj, similantaj
        flaveblankajn montarojn
        <fnt>
          <bib>VRA</bib>
        </fnt>;
        <trd lng="fr">meule de paille</trd>
        <trd lng="hu">szalmakazal</trd>
        <trd lng="ru">&c_o;&c_m;&c_jo;&c_t; &c_s;&c_o;&c_l;&c_o;&c_m;&c_y;</trd>
      </ekz>
      <ekz>
        li haltigis la a&ubreve;ton malanta&ubreve; <klr>[&#8230;]</klr> la <ind>pajlo<tld/>o</ind>j
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        en malfrua a&ubreve;tuno la fumo de la <ind>karbig<tld/>o</ind>j
        ku&scirc;is kiel nebulamasoj super la
        arbaro
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
        <trd lng="de">Meiler</trd>
      </ekz>
      <ekz>
        la formikoj fervore kuradis sur siaj <tld/>oj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
<!--
      <ekz>
        <ind>gren<tld/>o</ind>;
        <trd lng="be">&c_s;&c_k;&c_ib;&c_r;&c_d;&c_a; &c_h;&c_l;&c_je;&c_b;&c_a;</trd>
        <trd lng="hu">gabonaasztag</trd>
        <trd lng="ru">&c_s;&c_k;&c_i;&c_r;&c_d;&c_a; &c_h;&c_l;&c_je;&c_b;&c_a;</trd>
      </ekz>
-->
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_o;&c_g;</trd>,
      <trd>&c_s;&c_k;&c_ib;&c_r;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Haufen</trd>
    <trd lng="fr">meule<klr tip="ind"> (foin, etc.)</klr></trd>
    <trd lng="hu">kazal</trd>
    <trd lng="nl">hooiopper</trd>
    <trdgrp lng="pl">
      <trd>st&oacute;g</trd>,
      <trd>sterta</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_o;&c_g;</trd>,
      <trd>&c_s;&c_k;&c_i;&c_r;&c_d;&c_a;</trd></trdgrp>
  </snc>
  <snc mrk="stak.0o.vico">
    <dif>
      Aro da samformaj objektoj, metitaj unuj sur la aliajn,
      <ref tip="sin" cel="stapl.0o">staplo</ref>,
      <ref tip="sin" cel="kolon.0o.staplo">kolono<sncref/></ref>:
<!--
      <ekz>
        <ind><tld/>o da libroj</ind>,
        <trd lng="be">
        &c_s;&c_t;&c_o;&c_p;&c_k;&c_a; &c_k;&c_n;&c_ib;&c_g;</trd>
        <trd lng="hu">egy <ind>halom</ind> k&ouml;nyv</trd>
        <trd lng="ru">
        &c_s;&c_t;&c_o;&c_p;&c_k;&c_a; &c_k;&c_n;&c_i;&c_g;</trd>
      </ekz>
-->
      <ekz>
        dika <tld/>o de manuskriptoj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[videblis]</klr> bariloj kun gardobudoj kaj ligno<tld/>oj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        Jano forprenis de sur se&gcirc;o <tld/>on da libroj
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> eltiris el <tld/>o da paperoj la jidan gazeton
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi jam &scirc;vitante kaj blasfemante ellernis la senfinan <tld/>on da esperantaj
        tabelvortoj &ndash; kiu, kio, kies, tiu, tio kaj tiel plu
        <fnt>
          <bib>DKo</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_p;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_p;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_s;</trd>,
      <trd>&c_p;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Stapel</trd>
    <trd lng="en">pile</trd>
    <trd lng="fr">pile<klr tip="ind">
    (assiettes, livres, etc.)</klr></trd>
    <trd lng="hu">halom</trd>
    <trd lng="nl">stapel</trd>
    <trdgrp lng="pl">
      <trd>stos</trd>,
      <trd>sterta</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_p;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_p;&c_k;&c_a;</trd></trdgrp>
    <trdgrp lng="zh">
    <trd>&#26729; <pr>zhu&amacron;ng</pr></trd>,
    <trd>&#x6A01; <pr>zhu&amacron;ng</pr></trd>,
    <trd>&#x4ECE;&#x7EB8;&#22534;&#21040;&#x7EB8;&#22534;&#30340;&#x52A0;&#21360;&#x673A;
    <pr>c&oacute;ngzh&#x1D0;du&imacron;d&agrave;ozh&#x1D0;du&imacron;deji&amacron;y&igrave;nj&imacron;</pr></trd>,
    <trd>&#x5F9E;&#x7D19;&#22534;&#21040;&#x7D19;&#22534;&#30340;&#x52A0;&#21360;&#x6A5F;
    <pr>c&oacute;ngzh&#x1D0;du&imacron;d&agrave;ozh&#x1D0;du&imacron;deji&amacron;y&igrave;nj&imacron;</pr></trd>,
    <trd>&#26729;&#x57FA;&#30784; <pr>zhu&amacron;ngj&imacron;ch&#x1D4;</pr></trd>,
    <trd>&#x6A01;&#x57FA;&#x790E; <pr>zhu&amacron;ngj&imacron;ch&#x1D4;</pr></trd>,
    <trd>&#x7D19;&#x579B; <pr>zh&#x1D0;du&#x1D2;</pr></trd>,
    <trd>&#x7EB8;&#x579B; <pr>zh&#x1D0;du&#x1D2;</pr></trd>,
    <trd>&#x4E00;&#25209; <pr>y&imacron;p&imacron;</pr></trd>,
    <trd>&#x4ED3;&#x50A8;&#x8D85;&#x5E02; <pr>c&amacron;ngch&#x1D4;ch&amacron;osh&igrave;</pr></trd>,
    <trd>&#20489;&#20786;&#x8D85;&#x5E02; <pr>c&amacron;ngch&#x1D4;ch&amacron;osh&igrave;</pr></trd>,
    <trd>&#22823;&#x767E;&#x8CA8;&#21830;&#x5E97; <pr>d&agrave;ib&#x1CE;ihu&ograve;sh&amacron;ngdi&agrave;n</pr></trd>,
    <trd>&#22823;&#x767E;&#x8D27;&#21830;&#x5E97; <pr>d&agrave;ib&#x1CE;ihu&ograve;sh&amacron;ngdi&agrave;n</pr></trd>,
    <trd>&#x5B58;&#x50A8;&#22788; <pr>c&uacute;nch&#x1D4;ch&ugrave;</pr></trd>,
    <trd>&#x5B58;&#20786;&#34389; <pr>c&uacute;nch&#x1D4;ch&ugrave;</pr></trd>,
    <trd>&#25209;&#x91CF; <pr>p&imacron;li&agrave;ng</pr></trd>,
    <trd>&#x70DF;&#x6C14;&#33073;&#x786B; <pr>y&amacron;nq&igrave;tu&omacron;li&uacute;</pr></trd>,
    <trd>&#29017;&#x6C23;&#x812B;&#x786B; <pr>y&amacron;nq&igrave;tu&omacron;li&uacute;</pr></trd>,
    <trd>&#x6BDB;&#x88AB; <pr>m&aacute;ob&egrave;i</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="stak.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Inversvica memoro,
      <ref tip="super" cel="datum.0strukturo">datumstrukturo</ref>
      ebliganta aldoni a&ubreve; depreni anojn, kondi&ccirc;e ke la
      ordo de deprenoj estas inversa al tiu de la aldonoj (t.e. el
      &ccirc;iuj elementoj &ccirc;iam atingeblas nur la plej
      &jcirc;use aldonita el la ankora&ubreve; ne deprenitaj):
      <ekz>
        <ind>sur<tld/>igi datumon</ind>
        <klr>(la&ubreve; la metaforo, la aldonata datumo trafas
          <ctl>sur la pinton</ctl> de la <tld/>o)</klr>;
        <trd lng="en">push</trd>
        <trd lng="fr">empiler une donn&eacute;e</trd>
        <trd lng="hu">adatot <ind>verembe</ind> &iacute;r</trd>
        <trd lng="ru">
        &c_p;&c_o;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;
        &c_v; &c_s;&c_t;&c_je;&c_k;
        </trd>
      </ekz><ekz>
        <ind>el<tld/>igi datumon</ind>
        <klr>(legi el <tld/>o)</klr>.
        <trd lng="en">pop</trd>
        <trd lng="fr">d&eacute;piler une donn&eacute;e</trd>
        <trd lng="hu">adatot <ind>veremb&odblac;l</ind>
        kiolvas</trd>
        <trd lng="ru">
        &c_s;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;
        &c_s;&c_o; &c_s;&c_t;&c_je;&c_k;&c_a;
        </trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="stapl.0o.KOMP">staplo<sncref/></ref>
    <trd lng="be">&c_s;&c_t;&c_e;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Stapelspeicher</trd>,
      <trd>Kellerspeicher</trd>,
      <trd>Stack</trd>
    </trdgrp>
    <trd lng="fr">pile<klr tip="ind"> (structure de donn&eacute;es)
    </klr></trd>
    <trd lng="hu">verem<klr tip="ind"> (adat&eacute;)</klr></trd>
    <trd lng="nl">stapelgeheugen</trd>
    <trd lng="pl">stos</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_je;&c_k;</trd>,
      <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd></trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>z&aacute;sobn&iacute;k</trd>,
    <trd>z&aacute;sobn&iacute;kov&aacute; pam&ecaron;&tcaron;</trd>
  </trdgrp>
  <trd lng="en">stack</trd>
  <trdgrp lng="ja">
    <trd>&#12356;&#x306A;&#12416;&#12425;</trd>,
    <trd>&#x5C71;  <pr>&#12416;&#x308C;</pr></trd>,
    <trd>&#x30B9;&#x30BF;&#x30C3;&#x30AF;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hranica</trd>,
    <trd>kopa</trd>,
    <trd>stoh</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_ib;&c_g;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_p;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_ib;&c_s;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_p;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_je;&c_m;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="stak.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Meti en stako:
      <ekz>
        ilin <tld/>igas &ccirc;i tie
        <fnt>
          <bib>ChB</bib>
          <aut>Joh&aacute;n Valano</aut>
          <lok>17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal&scirc;ar&gcirc;is amforojn kaj <tld/>igis ilin sur la trotuaro ekster vinvendejo
        <fnt>
          <bib>MJA</bib>
          <aut>Anna L&ouml;wenstein</aut>
          <lok>Pontio, 3</lok>
        </fnt>;
      </ekz>
      <ekz>
        Palaso prenis el la &scirc;ranko grandan paka&jcirc;on, kaj <tld/>igis la enhavon, plurajn dekojn
        da tabuletoj, sur la tablo
        <fnt>
          <bib>MJA</bib>
          <aut>Anna L&ouml;wenstein</aut>
          <lok>Efezo, 2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_u; &c_s;&c_t;&c_o;&c_s;, &c_s;&c_t;&c_o;&c_p;&c_k;&c_u;)</klr></trd>
  <trd lng="en">stack</trd>
</drv>

</art>
