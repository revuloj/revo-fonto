<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mosul.xml,v 1.5 2024/01/26 19:05:53 afido Exp $">
    <kap>
      <rad>Mosul</rad>/o
    </kap>

<drv mrk="mosul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="mosul.0o.urbo">
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref>
      en <ref tip="malprt" cel="irak.I0o.GEOG">Irako</ref> &ccirc;e <ref cel="tigris.T0o">Tigriso</ref>, 
      &ccirc;. 350&#x202F;km nord-nordokcidente de <ref cel="bagdad.B0o">Bagdado</ref>:
      <ekz>
        en <tld/>o kaj precipe en Kirkuko, turkmenoj, araboj kaj asirianoj formas
        grandan parton de la lo&gcirc;antaro
        <fnt>
          <bib>MD02</bib>
          <vrk>Postmilita milito en Irako</vrk>
        </fnt>;
      </ekz>
      <ekz>
        post la konkero de <tld/>o fare de la Organizo de la Islama &Scirc;tato <klr>[&#x2026;]</klr>
        la regiona registaro de Kurdujo ricevis novan internacian apogon
        <fnt>
          <bib>MD14</bib>
          <vrk>Inter influo de Turkujo kaj Irano &dash; La kurdoj, kiom da divizioj?</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la liberigo de <tld/>o estis unue farita de la ora divizio de la Iraka armeo,
        lasinte nur etan rolon al la militfortoj regataj de Teherano
        <fnt>
          <bib>MD17</bib>
          <vrk>Militaj sukcesoj kaj sociaj &scirc;an&gcirc;i&gcirc;oj &dash; Irano reinventas sin, kiel regionan
          potencon</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="niniv.0o">Ninivo, Nineveo</ref>
    <trd lng="ar">&a_A;&a_l;&a_m;&a_w;&a_S;&a_l;</trd>
    <trd lng="de">Mossul</trd>
    <trd lng="en">Mosul</trd>
    <trd lng="fr">Mossoul</trd>
    <trd lng="he">&mem;&vav;&samekh;&vav;&lamed;</trd>
    <trd lng="ku">&a_m;&a_w;&a_w;&a_s;&#x6B5;</trd>
    <trdgrp lng="zh">
    <trd>&#x6469;&#x82CF;&#x5C14; <pr>M&#xF3;s&#x16B;&#x11B;r</pr></trd>,
    <trd>&#x6469;&#x8607;&#x723E; <pr>M&#xF3;s&#x16B;&#x11B;r</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_M;&c_a;&c_s;&c_u;&c_l;</trd>
  <trd lng="ja">&#x30E2;&#x30B9;&#x30EB;</trd>
  <trd lng="uk">&#x41C;&#x43E;&c_s;&c_u;&#x43B;</trd>
</drv>

  </art>

