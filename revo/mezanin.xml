<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mezanin</rad>/o
</kap>
<drv mrk="mezanin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="mezanin.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Interna
      <ref tip="vid" cel="balkon.0o.ARKI">balkona</ref>
      planko mezalte pendanta en &ccirc;ambro:
      <ekz>
        Davenant &scirc;ovis ilin sub la &scirc;tuparon, kondukantan
        el la halo en la <tld/>on, kie li lo&gcirc;is
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la stacio Peel <klr>...</klr> <tld/>o &scirc;vebanta en la
        stacivolumeno, <klr>...</klr> faras bonan uzon de la
        disponebla volumeno
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Metroo_de_Montrealo">
          Metroo de Montrealo</url>, 2011-06-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mezzanine</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>