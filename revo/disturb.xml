<art mrk="$Id: disturb.xml,v 1.6 2025/03/31 16:37:45 revo Exp $">
<kap>
  <rad>disturb</rad>/i
</kap>
<drv mrk="disturb.0i">
  <kap><tld/>i</kap>
  <snc mrk="disturb.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="pertur.0i.KOMUNE">Perturbi</ref>:
      <ekz>
        la apudesto de mia &ccirc;evalino povas <tld/>i vian &ccirc;evalon
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 16a, p.
        476a</lok></fnt>;
      </ekz>
      <ekz>
        iu sor&ccirc;a dia&jcirc;o komprenis miajn planojn kaj plene ilin
        <tld/>is
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url ref="http://www.esperanto.gr/lit/els/lukio.pdf">Lukio a&ubreve;
          azeno</url></vrk>,
          <lok>fonto, 1988</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ina&ubreve;guran ceremonion <tld/>is &gcirc;ustamomente &scirc;ar&gcirc;aviadila bruo
        <fnt>
          <aut>B. Ragnarsson</aut>,
          <vrk>la&ubreve; neplanitaj padoj</vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">st&ouml;ren</trd>
  <trdgrp lng="en">
    <trd>disturb</trd>,
    <trd>perturb</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;ner</trd>,
    <trd>perturber</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niepokoi&cacute;</trd>,
    <trd>zak&lstroke;&oacute;ca&cacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x788D;&#x811A; <pr>&#xE0;iji&#x1CE;o</pr></trd>,
    <trd>&#25443; <pr>d&#x1CE;o</pr></trd>,
    <trd>&#x9A37;&#x64FE; <pr>s&amacron;or&#x1CE;o</pr></trd>,
    <trd>&#25171;&#25200; <pr>d&#x1CE;r&#x1CE;o</pr></trd>,
    <trd>&#x5E72;&#25200; <pr>g&amacron;nr&#x1CE;o</pr></trd>,
    <trd>&#25200; <pr>r&#x1CE;o</pr></trd>,
    <trd>&#x5A06; <pr>r&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
</art>

