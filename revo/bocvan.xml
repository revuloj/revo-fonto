<art mrk="$Id: bocvan.xml,v 1.14 2022/04/26 15:00:33 revo Exp $">
<kap>
  <rad>Bocvan</rad>/o
</kap>

<drv mrk="bocvan.B0o">
  <kap>
    <tld/>o
    <fnt><bib>RLN</bib></fnt>
</kap>
  <mlg kod="ISO-3166">BW</mlg>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="bocvan.B0o.GEOG">
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la suda 
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo 
      <ref tip="prt" cel="gaberon.G0o">Gaberono</ref>,
      teritorio de Respubliko <tld/>o,
      &gcirc;is 1966 nomata Be&ccirc;uanalando,
      limanta kun
      <ref tip="vid" cel="namibi.N0o.GEOG">Namibio</ref>,
      <ref tip="vid" cel="zambi.Z0o.GEOG">Zambio</ref>,
      <ref tip="vid" cel="zimbabv.Z0o.GEOG">Zimbabvo</ref>
      kaj Sud-Afriko:
      <ekz>
        inter plena&gcirc;uloj estas HIV-infektitoj: 36&nbsp;% en <tld/>o, 25&nbsp;% en Svazilando kaj Zimbabvo, 24
% en Lesoto, 20&nbsp;% en Namibio kaj Zambio
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la diplomatia reto de la Sankta Se&gcirc;o rimarkinde pliampleksi&gcirc;is <klr>[&#8230;]</klr>
        dum la papado de Benedikto la 16a aldoni&gcirc;is aliaj tri <klr>[landoj]</klr>: la &jcirc;us
        sendependi&gcirc;inta
        Montenegro en 2006, la Unui&gcirc;intaj Arabaj Emirlandoj en 2007 kaj <tld/>o en 2008
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2009/009739.php">Vatikano diplomatie rilatas kun 177
          landoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Botswana</trd>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_t;&c_s;&c_v;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="en">
    <trd>Botswana</trd>,
    <trd>Republic of Botswana</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Botswana</trd>,
    <trd>R&eacute;publique du Botswana</trd>
  </trdgrp>
  <trd lng="id">Botswana</trd>
  <trd lng="nl">Botswana</trd>
  <trd lng="no">Botswana</trd>
  <trdgrp lng="pl">
    <trd>Botswana</trd>,
    <trd>Republika <ind>Botswany</ind></trd>
  </trdgrp>
  <trd lng="pt">Botswana</trd>
  <trd lng="uk">&c_B;&#x43E;&c_t;&c_s;&c_v;&c_a;&#x43D;&c_a;</trd>
</drv>

</art>

