<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bocvan</rad>/o
</kap>

<drv mrk="bocvan.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bocvan.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda Afriko kun &ccirc;efurbo Gaberono.
      Teritorio de Respubliko Bocvano.
      &Gcirc;is 1966 nomata Be&ccirc;uanalando.
      ISO-3166 kodo: BW.
      &Gcirc;i limas kun
      <ref tip="vid" cel="namibi.0o.GEOG">Namibio</ref>,
      <ref tip="vid" cel="zambi.0o.GEOG">Zambio</ref>,
      <ref tip="vid" cel="zimbabv.0o.GEOG">Zimbabvo</ref>
      kaj Sud-Afriko.
    </dif>
    <trdgrp lng="pl">
      <trd>Botswana</trd>,
      <trd>Republika <ind>Botswany</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

