<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>progres</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="progre.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="progre.0i.perfektigxi">
    <dif>
      Anta&ubreve;eniri al supera grado de disvolvi&gcirc;o, 
      perfekti&gcirc;i:
      <ekz>
        tie li povis pli bone esti instruata kaj pli bone <tld/>i;
      </ekz>
      <ekz>
        <tld/>i en la studado, en la civilizo;
      </ekz>
      <ekz>
        la afero rapide <tld/>as;
      </ekz>
      <ekz>
        la preparoj por la ekspozicio kontentige <tld/>as;
      </ekz>
      <ekz>
        la scienco, moralo &ccirc;iutage <tld/>as;
      </ekz>
      <ekz>
        malgra&ubreve; <tld/>inta le&gcirc;aro la indianoj vivas mizere;
      </ekz>
      <ekz>
        nuntempe muzika kulturo de Esperantujo iom malpli
        <tld/>as kompare al poezi-kulturo
        <fnt>
        <aut>J. Zajdman, P. Mo&jcirc;ajev</aut>,
        <vrk>Ege ri&ccirc;aj harmonie</vrk>,
        <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas, ke la plej populara libreto pri gramatiko
        por <tld/>antoj, plene en Esperanto anta&ubreve;e estis
        <ctl>La tuta esperanto</ctl> de Henrik Seppik
        <fnt>
        <aut>C. Keysers</aut>,
        <vrk>Gramatiko facile asimilebla</vrk>,
        <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">d&ecaron;lat pokroky</trd>
  <trdgrp lng="de">
    <trd>Fortschritte machen</trd>,
    <trd>voranschreiten</trd>,
    <trd>sich <ind>weiterentwickeln</ind></trd>
  </trdgrp>
  <trd lng="en">progress</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_r;&a_f;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_r;&a_q;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">progresser</trd>
  <trdgrp lng="hu">
    <trd>halad</trd>,
    <trd>el&odblac;remegy</trd>,
    <trd>fejl&odblac;dik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vooruitgaan<klr tip="ind"> (vorderen)</klr></trd>,
    <trd>vorderingen maken</trd>,
    <trd>vorderen</trd>
  </trdgrp>
  <trd lng="pt">progredir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="progre.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iompostioma disvolvi&gcirc;o al pli alta grado:
      <ekz>
        la <tld/>o de la scienco, malsano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pasx.0o">pa&scirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;</trd>
  <trd lng="cs">pokrok</trd>
  <trdgrp lng="de">
    <trd>Fortschritt</trd>,
    <trd>Weiterentwicklung</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_r;&a_f;&a_t;</trd>,
    <trd>&a_t;&a_r;&a_q;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">progr&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>halad&aacute;s</trd>,
    <trd>fejl&odblac;d&eacute;s</trd>,
    <trd>el&odblac;remenetel</trd>
  </trdgrp>
  <trd lng="nl">vooruitgang</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;</trd>
</drv>

<drv mrk="progre.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="progre.0ema.POL">
    <dif>
      Faciliganta la <tld/>on de la civilizo:
      <ekz>
        <tld/>ema ministro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">fortschrittlich</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_r;&a_w;</trd>,
    <trd>&a_m;&a_t;&a_r;&a_q;&f_y;</trd>,
    <trd>&a_t;&a_r;&a_q;&f_y;&zwnj;&a_kh;&a_w;&a_A;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">progressiste</trd>
  <trdgrp lng="nl">
    <trd>progressief</trd>,
    <trd>vooruitstrevend</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>halad&oacute; szellem&udblac;</trd>,
    <trd>progressz&iacute;v</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_o;&c_v;&c_o;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="progre.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <refgrp tip="dif">
      <ref cel="pusx">anta&ubreve;enpu&scirc;i</ref>,
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="volv.dis0i">disvolvi</ref>,
      <ref cel="bon.pli0igi">plibonigi</ref>,
      <ref cel="perfek.0igi">perfektigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_z;&c_je;&c_j;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">voranbringen</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&a_b;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">promouvoir</trd>
  <trdgrp lng="hu">
    <trd>el&odblac;revisz</trd>,
    <trd>fejleszt</trd>
  </trdgrp>
  <trd lng="nl">bevorderen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="progre.0into">
  <kap><tld/>into</kap>
  <snc mrk="progre.0into.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Lernanto, kiu jam <tld/>is, havas sciojn
      en la koncerna temo:
      <ekz>
        legolibreto por komencantoj kaj <tld/>intoj
        <fnt>
          <aut>-</aut>,
          <vrk>Mozaiko</vrk>,
          <lok>1935</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;l&egrave;ve <ind>avanc&eacute;</ind></trd>
</drv>

<drv mrk="progre.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="progre.0i.degeneri">
    <dif>
      Perdi atingitan nivelon de disvolvi&gcirc;o, kaduki&gcirc;i, degeneri,
      malkreski:
      <ekz>
        en Bervala &scirc;pinejo la produktado mal<tld/>as
        <fnt>
        <vrk>L. Beaucaire</vrk>,
        <vrk>Kruko kaj Baniko el Bervalo</vrk>.
        <lok>TK Kobenhavn, 1974</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi supre konstatis, ke Esperanto ne estas <tld/>inta,
        kaj eble e&ccirc; mal<tld/>inta, en la hinda kontinento
        <fnt>
        <aut>V. Sadler</aut>, <vrk>Azia Strategio</vrk>.
        <lok>Esperanto, 1963-01 (6)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_g;&c_r;&c_e;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_e;&c_g;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_s;&zwnj;&a_r;&a_f;&a_t;&a_n;</trd>
  <trd lng="fr">r&eacute;gresser</trd>
  <trd lng="nl">achteruitgaan</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="progre.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="progre.0i.degenero">
    <dif>
      Perdo de atingita nivelo de disvolvi&gcirc;o, kaduki&gcirc;o,
      <ref tip="vid" cel="degene.0o">degenero</ref>,
      malkresko,
      <ref tip="vid" cel="dekade.0o">dekadenco</ref>:
      <ekz>
        nun en la simfonia &gcirc;enro ni observas gravan mal<tld/>on
        nur iomete diluitan per eksperimentoj de I. Stravinskij
        kaj S. Prokofjev
        <fnt><aut>J. Mura&scirc;kovskij, I. Mura&scirc;kovska</aut>, <vrk>Kien 
        fluas roj&#8217; Castalie?</vrk>, <lok>1984-1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_g;&c_r;&c_e;&c_s;</trd>,
    <trd>&c_d;&c_e;&c_g;&c_r;&c_a;&c_d;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>  <trd lng="fa">&f_p;&a_s;&a_r;&a_f;&a_t;</trd>
  <trd lng="fr">r&eacute;gression</trd>
  <trd lng="nl">achteruitgang</trd>
  <trd lng="ru">&c_r;&c_je;&c_g;&c_r;&c_je;&c_s;&c_s;</trd>
</drv>
</art>
