  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_r;&a_f;&a_t;</trd>,
    <trd>&a_t;&a_r;&a_q;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">progr&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>halad&aacute;s</trd>,
    <trd>fejl&odblac;d&eacute;s</trd>,
    <trd>el&odblac;remenetel</trd>
  </trdgrp>
  <trdgrp lng="ja">
