<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>selen</rad>/o
</kap>
<drv mrk="selen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 34, atompezo 78,96, simbolo Se.
      <ref tip="super" cel="metalo.0o">Metaloido</ref>:
      <ekz>
        oni el&ccirc;erpas anka&ubreve; rarajn metalojn, interalie: indion, germanion,
        galion, <tld/>on
        <fnt><aut>Bardhyl Mezini</aut>: <vrk>La vera ka&ubreve;zo de la
        milito</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 5a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Selen</trd>
  <trdgrp lng="fr">
    <trd>s&eacute;l&eacute;nium</trd>
  </trdgrp>
  <trd lng="hu">szel&eacute;n</trd>
  <trd lng="nl">selenium</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_je;&c_n;</trd>
</drv>
</art>
