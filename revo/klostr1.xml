<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>klostrofobi</rad>/o
    </kap>

    <drv mrk="klostr1.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">MED</uzo>
      <fnt><bib>PIV1</bib><lok>klostro</lok></fnt>
      <snc>
        <dif>
          Mensa malsano, karakterizata per timo esti en fermita spaco.
        </dif>
      </snc>
      <ref tip="sin" cel="klauxs1.0o">kla&ubreve;strofobio</ref>
      <ref tip="ant" cel="agoraf.0o">agorafobio</ref>
      <trd
lng="be">&c_k;&c_l;&c_ja;&c_w;&c_s;&c_t;&c_r;&c_a;&c_f;&c_o;&c_b;&c_ib;&c_ja;</trd>
      <trd lng="ca">claustrof&ograve;bia</trd>
      <trdgrp lng="de">
        <trd>Klaustrofobie</trd>,
        <trd>Platzangst</trd>
      </trdgrp>
      <trd lng="en">claustrophobia</trd>
      <trd lng="es">claustrofobia</trd>
      <trd lng="fr">claustrophobie</trd>
      <trdgrp lng="hu">
        <trd>z&aacute;rthelyiszony</trd>,
        <trd>klausztrof&oacute;bia</trd>
      </trdgrp>
      <trd lng="nl">claustrofobie</trd>
      <trd
lng="ru">&c_k;&c_l;&c_a;&c_u;&c_s;&c_t;&c_r;&c_o;&c_f;&c_o;&c_b;&c_i;&c_ja;</trd>
    </drv>
  </art>

