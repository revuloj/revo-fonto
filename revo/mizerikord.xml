<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mizerikord.xml,v 1.13 2024/01/26 21:19:07 revo Exp $">
<kap>
  <rad>mizerikord</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mizerikord.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="fak">REL</uzo>
  <snc mrk="mizerikord.0o.REL">
    <dif>
      <ke><ref tip="dif" cel="kor.favor0eco">Favorkoreco</ref></ke>:
      <ekz>
        se li sin korektos, eble li trovos la plenan <tld/>on, kiun oni
        rifuzas nun al li
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 6a, p.
        59a</lok></fnt>;
      </ekz>
      <ekz>
        kompatu min, <tld/>a Patrino, murmuretis &scirc;i petege, <klr>[&#8230;]</klr> sin premante
        konfide al la sanktaj piedoj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam oni konfidas nur al la <tld/>o de Dio
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/010116.php">Eklezia ludo kondamnita</url></vrk>
          <bib>Monato</bib>
          <lok>2010/10, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        bildo de la <tld/>a Maria
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2007/009331.php">Mirakla pre&gcirc;eja incendio</url></vrk>
          <bib>Monato</bib>
          <lok>2007/12, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la korano spiras <tld/>on por &ccirc;iaj homoj
        <fnt>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010808.php">Rajtoj pri ritoj</url></vrk>
          <bib>Monato</bib>
          <lok>2012/10, p. 29</lok>
        </fnt>;
      </ekz>
      <ekz>
        Francisko restis en Kubo kiel &leftquot;misiisto de <tld/>o&#8221; kaj gvidis diservojn en tri
        urboj
        <fnt>
          <aut>Juan Carlos Montero Medina</aut>
          <vrk><url ref="&Monato;/2015/011589.php">Vizito benita</url></vrk>
          <bib>Monato</bib>
          <lok>2015/11, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_e;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>milosrdenstv&iacute;</trd>,
    <trd>slitov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Barmherzigkeit</trd>
  <trd lng="fr">mis&eacute;ricorde</trd>
  <trd lng="nl">barmhartigheid</trd>
  <trdgrp lng="pl">
    <trd>lito&sacute;&cacute;</trd>,
    <trd>mi&lstroke;osierdzie</trd>
  </trdgrp>
  <trd lng="pt">miseric&oacute;rdia</trd>
  <trdgrp lng="ro">
    <trd>mil&abreve;</trd>,
    <trd>mizericordie</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>milosrdenstvo</trd>,
    <trd>zmilovanie</trd>,
    <trd>z&lcaron;utovanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_ja;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20161;&#24904; <pr>r&eacute;nc&iacute;</pr></trd>,
    <trd>&#20840;&#24515;&#20840;&#24847; <pr>qu&aacute;nx&imacron;nqu&aacute;ny&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>

