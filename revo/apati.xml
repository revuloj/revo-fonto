<art mrk="$Id: apati.xml,v 1.18 2021/02/28 18:02:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>apati</rad>/o
</kap>
<drv mrk="apati.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malvigleco, indiferenteco:
      <ekz>
        kun malespero kaj <tld/>o lasi fali la
        manojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="marasm.0o">marasmo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_y;&c_ja;&c_k;&c_a;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>apatie</trd>,
    <trd>lhostejnost</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Teilnahmslosigkeit</trd>,
      <trd>Apathie</trd>
  </trdgrp>
  <trd lng="en">apathy</trd>
  <trd lng="es">apat&iacute;a</trd>
  <trd lng="fr">apathie</trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5D3;&#x5B4;&#x5D9;&#x5E9;&#x5C1;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>ap&aacute;tia</trd>,
    <trd>egykedv&udblac;s&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>apatia</trd>
  </trdgrp>
  <trd lng="nl">apathie</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_v;&c_n;&c_o;&c_d;&c_u;&c_sh;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apatia</trd>,
    <trd>nev&#x161;&#xED;mavos&#x165;</trd>,
    <trd>otupenos&#x165;</trd>,
    <trd>&#x13E;ahostajnos&#x165; vo&#x10D;i okoliu</trd>
  </trdgrp>
</drv>
</art>
