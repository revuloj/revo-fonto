<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kompromis</rad>/o
</kap>
<drv mrk="kompro.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Interkonsento aran&gcirc;ita pro reciprokaj cedoj de du kontra&ubreve;uloj
      pri la diskutita&jcirc;o:
      <ekz>finveni al <tld/>o;</ekz>
      <ekz>fari<fnt>Z</fnt>,</ekz>
      <ekz>aran&gcirc;i, kontrakti <tld/>on;</ekz>
      <ekz>
        ne ser&ccirc;u vanajn <tld/>ojn, kiuj flikas unu lokon
        krevigante alian<fnt>Z</fnt>.
      </ekz>  
    </dif>
    <refgrp tip="sin">
      <ref cel="akord.0o">akordo</ref>,
      <ref cel="ced">intercedo</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Kompromiss</trd>,
     <trd>Vergleich</trd>,
     <trd>Ausgleich</trd>
    </trdgrp>
    <trd lng="en">compromise</trd>
    <trd lng="nl">compromis</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_r;&c_o;&c_m;&c_i;&c_s;&c_s;</trd>
  </snc>
</drv>
</art>
