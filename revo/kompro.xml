<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kompromis</rad>/o
</kap>
<drv mrk="kompro.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="konsen.inter0o">Interkonsento</ref>,
      aran&gcirc;ita pro reciprokaj cedoj de du kontra&ubreve;uloj
      pri la diskutita&jcirc;o:
      <ekz>
        <ind>finveni al <tld/>o</ind>;
        <trd lng="fr">atteindre un <ind>compromis</ind></trd>
      </ekz>
      <ekz>
        aran&gcirc;i,
      </ekz>
      <ekz>
        <ind>fari <tld/>on</ind><fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>faire un <ind>compromis</ind></trd>,
          <trd>composer<klr tip="ind"> (transiger)</klr></trd>,
          <trd>transiger</trd>
        </trdgrp>
      </ekz>
      <ekz>
        ne ser&ccirc;u vanajn <tld/>ojn, kiuj flikas unu lokon
        krevigante alian<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0o">akordo</ref>,
      <ref cel="ced.0o">cedo</ref>,
      <ref cel="konced.0o">koncedo</ref>.
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_r;&c_a;&c_m;&c_ib;&c_s;</trd>
    <trdgrp lng="de">
      <trd>Kompromiss</trd>,
      <trd>Vergleich</trd>,
      <trd>Ausgleich</trd>
    </trdgrp>
    <trd lng="en">compromise</trd>
    <trdgrp lng="fr">
      <trd>compromis<klr tip="ind"> (subst.)</klr></trd>,
      <trd>accommodement</trd>,
      <trd>arrangement<klr tip="ind"> (accord avec concessions)</klr></trd>,
      <trd>transaction<klr tip="ind"> (accord avec concessions)</klr></trd>
    </trdgrp>
    <trd lng="nl">compromis</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_r;&c_o;&c_m;&c_i;&c_s;&c_s;</trd>
  </snc>
</drv>

<drv mrk="kompro.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        la bur&gcirc;aro <tld/>is kaj cedadis, kio donis
        al Yuan Shikai, &ccirc;efo de la nordaj militaristoj,
        &scirc;ancon uzurpi la frukton de la revolucio
        <fnt>
        <vrk>Anta≈≠moderna historio</vrk>
        <lok><url ref="http://www.chinareport.com.cn/18s/01/index-02-2.htm">El Popola &Ccirc;inio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>faire un <ind>compromis</ind></trd>,
    <trd>composer<klr tip="ind"> (transiger)</klr></trd>,
    <trd>transiger</trd>
  </trdgrp>
</drv>
</art>
