<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pav</rad>/o
</kap>
<drv mrk="pav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fazan.0edoj">fazanedoj</ref>
      (<trd lng="la">Pavo</trd>), kun granda belkolora ventumilforme
      etendebla vosto kovrita per brilaj okulformaj makuloj.
    </dif>
    <trd lng="de">Pfau</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodestulo, parademulo.
    </dif>
  </snc>
</drv>
<drv mrk="pav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="klr">(pp <tld/>o)</uzo>
    <dif>
      Eletendi sian voston.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodeste paradi
    </dif>
    <refgrp tip="vid">
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="fier.0i">fieri</ref>,
      <ref cel="sxvel">brusto&scirc;veli</ref>,
      <ref cel="vant.0a">vanta</ref>.
    </refgrp>
  </snc>
</drv>
</art>
