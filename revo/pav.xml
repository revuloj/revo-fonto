<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pav.xml,v 1.36 2022/10/12 21:36:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pav</rad>/o
</kap>
<drv mrk="pav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pav.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Pavo</trd>), kun granda belkolora ventumilforme
      etendebla vosto kovrita per brilaj okulformaj makuloj:
      <ekz>
        &scirc;iparo, alportante oron, ar&gcirc;enton, eburon, simiojn, kaj
        <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        apude staris amaso da <tld/>oj kun etenditaj radiantaj vostoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kolorri&ccirc;a kiel vosto de <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Malbona princo (Legendo)</lok>
        </fnt>;
      </ekz>
      <ekz>
        pelis <tld/>on, falis kavon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la instruistino &ccirc;irka&ubreve;pa&scirc;etis kiel <tld/>o inter la
        laboristoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>unua parto, &ccirc;apitro 2a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">paun</trd>
    <trd lng="de">Pfau</trd>
    <trd lng="en">peacock</trd>
    <trd lng="fa">&a_T;&a_A;&a_w;&a_w;&a_s;</trd>
    <trd lng="fr">paon</trd>
    <trd lng="es">pavo real</trd>
    <trd lng="hu">p&aacute;va</trd>
    <trd lng="id">merak</trd>
    <trd lng="nl">pauw</trd>
    <trd lng="pl">paw</trd>
    <trd lng="ro">p&abreve;un</trd>
    <trd lng="sv">p&aring;f&aring;gel</trd>
  </snc>
  <snc mrk="pav.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodestulo, parademulo.
    </dif>
    <trd lng="br">brager</trd>
    <trdgrp lng="de">
      <trd>Angeber</trd>,
      <trd>Snob</trd>,
      <trd>Schn&ouml;sel</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_l;&c_ib;&c_n;</trd>
  <trd lng="cs">p&aacute;v</trd>
  <trd lng="hu">p&aacute;v&aacute;skod&oacute;</trd>
  <trd lng="pt">pav&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_v;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">p&aacute;v</trd>
</drv>

<drv mrk="pav.P0o">
  <kap><tld lit="P"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Pav</mlg>
  <snc mrk="pav.P0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Pavo</trd>):
      <ekz>
        NGC 6398 estas galaksio en konstelacio <tld lit="P"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=119609">NGC
          6398</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Pfau <klr>(Sternbild)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_P;&c_a;&c_w;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_P;&c_a;&c_v;&c_a; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xFA8;&#xF0B;&#xF56;&#xFB1;&#xF0B;</trd>
  <trd lng="en">Pavo</trd>
  <trd lng="fr">Paon<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="pl">Paw  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">P&abreve;unul  <klr>(constela&#539;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_v;&c_a;</trd>,
    <trd>&#x43F;&c_a;&c_v;&c_i;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="pav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pav.0i.ZOO">
    <uzo tip="klr">(pri <tld/>o)</uzo>
    <dif>
      Mar&scirc;i digne, eletendante sian voston.
    </dif>
    <trd lng="de">ein <ind>Rad</ind> schlagen <klr>(Pfau)</klr></trd>
    <trd lng="fa">&f_ch;&a_t;&a_r; &a_z;&a_d;&a_n;<klr tip="ind"> (&a_T;&a_A;&a_w;&a_w;&a_s;)</klr></trd>
    <trd lng="fr">faire la roue</trd>
    <trd lng="pl">rozk&lstroke;ada&cacute; ogon</trd>
    <trd lng="ro">&icirc;mpu&scedil;c&abreve; coada</trd>
  </snc>
  <snc mrk="pav.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodeste paradi,
      <ref tip="vid" cel="sxvel">brusto&scirc;veli</ref>,
      <ref tip="vid" cel="fanfar1.0i">fanfaroni</ref>:
      <ekz>
        en kortega vest' li <tld/>is, ledajn &scirc;uojn li surhavis
        <fnt>
          <aut>Anonimulo el la 14a jarcento, trad. M. Luk&aacute;&scaron;</aut>
          <vrk>&Ccirc;e&hcirc;a kaj slovaka antologio, Unua Parto</vrk>
          <lok>Stalisto kaj Studento</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu opinias sin neerarema, tiu <tld/>as, kvaza&ubreve; li estus la
        egalulo de Dio
        <fnt>
          <aut>La Libro de la Spiritoj</aut>
          <vrk>Allan Kardec, trad. L. C. Porto Carreiro Neto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        se l' viro duan amatinon havas <klr>[&#8230;]</klr> &scirc;i en
        ekster' &scirc;ajnigas tre angori, sed en la koro pluas bonhumori, 
        kvankam la viro pri l' alia <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2008/009455.php">Mezepokaj
           sekret(igit)aj sonetoj</url></vrk>
        </fnt>.
      </ekz>
      <ekz>
        ofte dum kongresa teatro-vespero oni <tld/>as per tradukita,
        nacia dramo anstata&ubreve; per indi&gcirc;ena, vere internacia kaj esperanta
        originala&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011436.php">Eternaj 
           demandoj el rubkamiono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pav.0umi.KOMUNE"><tld lit="p"/>umi</ref>
    <trd lng="br">bragal</trd>
    <trd lng="de">sich <ind>spreizen</ind> <klr>(wie ein
    Pfau)</klr></trd>
    <trdgrp lng="en">
      <trd>strut</trd>,
      <trd>swagger</trd>
    </trdgrp>
    <trd lng="fa">&a_kh;&a_r;&a_A;&a_m;&f_y;&a_d;&a_n;</trd>
    <trd lng="fr">se <ind>pavaner</ind></trd>
    <trd lng="id"><baz>sombong</baz>menyombongkan<klr tip="amb">
    (diri)</klr></trd>
    <trdgrp lng="pl">
      <trd>paradowa&cacute;</trd>,
      <trd>puszy&cacute; si&eogonek;</trd>,
      <trd>chodzi&cacute; jak <ind>paw</ind></trd>,
      <trd>popisywa&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>demonstra&tcedil;ie</trd>,
      <trd>parada</trd>,
      <trd>merg&acirc;nd ca un <ind>p&abreve;un</ind></trd>,
      <trd>se d&abreve; mare</trd>
    </trdgrp>
  </snc>
  <trd lng="br">rodal</trd>
  <trdgrp lng="cs">
    <trd>bl&yacute;skat se</trd>,
    <trd>b&yacute;t hrd&yacute; p&rcaron;ehnan&ecaron;</trd>,
    <trd>chlubit se</trd>,
    <trd>honosit se</trd>
  </trdgrp>
  <trd lng="es">pavonear(se)</trd>
  <trd lng="hu">p&aacute;v&aacute;skodik</trd>
  <trd lng="nl">pronken</trd>
  <trd lng="sk">rozprestrie&tcaron; p&aacute;v&iacute; chvost</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x44F;&#x43A; &#x43F;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_z; &#x43F;&#x43E;&c_v;&c_a;&c_zh;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&#x43F;&c_i;&c_h;&c_a;&c_t;&c_i;&#x43C; &c_v;&c_i;&c_g;&#x43B;&#x44F;&c_d;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="pav.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pav.0umi.KOMUNE">
    <dif>
      <ref tip="sin" cel="pav.0i.FIG">Pavi<sncref/></ref>:
      <ekz>
        &scirc;i klopodas <tld/>umi sinjorinece
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">bragal</trd>
  <trdgrp lng="en">
    <trd>strut</trd>,
    <trd>swagger</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&a_r;&a_A;&a_m;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">se <ind>pavaner</ind></trd>
  <trd lng="id"><baz>sombong</baz>menyombongkan<klr tip="amb">
  (diri)</klr></trd>
  <trdgrp lng="pl">
    <trd>paradowa&cacute;</trd>,
    <trd>puszy&cacute; si&eogonek;</trd>,
    <trd>chodzi&cacute; jak <ind>paw</ind></trd>,
    <trd>popisywa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>demonstra&tcedil;ie</trd>,
    <trd>parada</trd>,
    <trd>merg&acirc;nd ca un <ind>p&abreve;un</ind></trd>,
    <trd>se d&abreve; mare</trd>
  </trdgrp>
</drv>
</art>
