<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pav</rad>/o
</kap>
<drv mrk="pav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pav.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Pavo</trd>), kun granda belkolora ventumilforme
      etendebla vosto kovrita per brilaj okulformaj makuloj.
    </dif>
    <trd lng="br">paun</trd>
    <trd lng="de">Pfau</trd>
    <trd lng="en">peacock</trd>
    <trd lng="fr">paon</trd>
    <trd lng="es">pavo real</trd>
    <trd lng="hu">páva</trd>
    <trd lng="nl">pauw</trd>
  </snc>
  <snc mrk="pav.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodestulo, parademulo.
    </dif>
    <trd lng="br">brager</trd>
  </snc>
  <trd lng="be">паўлін</trd>
  <trd lng="hu">páváskodó</trd>
  <trd lng="pt">pavão</trd>
  <trdgrp lng="ru">
    <trd>павлин</trd>,
    <trd>пава</trd>
  </trdgrp>
</drv>

<drv mrk="pav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pav.0i.ZOO">
    <uzo tip="klr">(pri <tld/>o)</uzo>
    <dif>
      Eletendi sian voston.
    </dif>
    <trd lng="fr">faire la roue</trd>
  </snc>
  <snc mrk="pav.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodeste paradi
    </dif>
    <refgrp tip="vid">
      <ref cel="sxvel">brustoŝveli</ref>,
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="fier.0i">fieri</ref>,
      <ref cel="vant.0a">vanta</ref>
    </refgrp>
    <trd lng="br">bragal</trd>
    <trdgrp lng="en">
      <trd>strut</trd>,
      <trd>swagger</trd>
    </trdgrp>
    <trd lng="fr">se <ind>pavaner</ind></trd>
  </snc>
  <trd lng="br">rodal</trd>
  <trd lng="hu">páváskodik</trd>
  <trd lng="es">pavonear(se)</trd>
  <trd lng="nl">pronken</trd>
</drv>
</art>
