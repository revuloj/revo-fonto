<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pav</rad>/o
</kap>
<drv mrk="pav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Pavo</trd>), kun granda belkolora ventumilforme
      etendebla vosto kovrita per brilaj okulformaj makuloj.
    </dif>
    <trd lng="de">Pfau</trd>
    <trd lng="hu">p&aacute;va</trd>
    <trd lng="nl">pauw</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodestulo, parademulo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_l;&c_ib;&c_n;</trd>
  <trd lng="hu">p&aacute;v&aacute;skod&oacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_v;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="klr">(pp <tld/>o)</uzo>
    <dif>
      Eletendi sian voston.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmodeste paradi
    </dif>
    <refgrp tip="vid">
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="fier.0i">fieri</ref>,
      <ref cel="sxvel">brusto&scirc;veli</ref>,
      <ref cel="vant.0a">vanta</ref>.
    </refgrp>
  </snc>
  <trd lng="hu">p&aacute;v&aacute;skodik</trd>
  <trd lng="nl">pronken</trd>
</drv>
</art>
