<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kapilar</rad>/o
</kap>
<drv mrk="kapila.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Harforma angio, organo, kies interna diametro estas treege
      malgranda.
    </dif>
  </snc>
  <trd lng="de">Kapillare</trd>
  <trd lng="en">capillary</trd>
  <trd lng="nl">haarvat</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;</trd>
</drv>

<drv mrk="kapila.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o:
      <ekz>
        <tld/>a aktiveco, anulizo, &hcirc;emio.
      </ekz>
    </dif>
    <trd lng="en">capillary</trd>
  </snc>
  <snc>
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        <tld/>aj tuboj
      </ekz>
    </dif>
    <trd lng="en">capillary</trd>
  </snc>
  <trd lng="nl">capillair</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kapila.0eco">
  <kap>
    <tld/>eco
  </kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Propreco de <tld/>aj tuboj rilate al likva&jcirc;oj, kiuj ilin
      trafluas:
      <ekz>
        la suprenfluo de la suko en la plantoj estas fenomeno de
        <tld/>eco.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>capillarity</trd>,
      <trd>capillary action</trd>
    </trdgrp>
    <trd lng="nl">capillariteit</trd>
  </snc>
</drv>
</art>
