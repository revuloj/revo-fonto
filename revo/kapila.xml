<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kapilar</rad>/o
</kap>
<drv mrk="kapila.0o">
  <kap><tld/>o</kap>
  <snc mrk="kapila.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Harforma <ref tip="super" cel="angi.0o">angio</ref>, organo, kies
      interna diametro estas treege malgranda.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_l;&c_ja;&c_r;</trd>
  <trd lng="de">Kapillare</trd>
  <trd lng="en">capillary</trd>
  <trd lng="es">capilar</trd>
  <trdgrp lng="fr">
    <trd>capillaire<klr tip="ind"> (subst.)</klr></trd>,
    <trd>vaisseau <ind>capillaire</ind></trd>,
    <trd>art&eacute;riole</trd>,
    <trd>veinule</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajsz&aacute;lcs&odblac;</trd>,
    <trd>hajsz&aacute;l&eacute;r</trd>,
    <trd>kapill&aacute;ris</trd>
  </trdgrp>
  <trd lng="nl">haarvat</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;</trd>
</drv>

<drv mrk="kapila.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o:
      <ekz>
        <tld/>a aktiveco, analizo, &hcirc;emio.
      </ekz>
    </dif>
    <trd lng="en">capillary</trd>
  </snc>
  <snc>
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        <tld/>aj tuboj
      </ekz>
    </dif>
    <trd lng="en">capillary</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  <trd lng="es">capilar</trd>
  <trd lng="fr">capillaire<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>hajsz&aacute;lcs&ouml;es</trd>,
    <trd>hajsz&aacute;leres</trd>,
    <trd>kapill&aacute;ris</trd>
  </trdgrp>
  <trd lng="nl">capillair</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kapila.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kapila.0eco.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Propreco de <tld/>aj tuboj rilate al likva&jcirc;oj, kiuj ilin
      trafluas:
      <ekz>
        la suprenfluo de la suko en la plantoj estas fenomeno de
        <tld/>eco.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>capillarity</trd>,
      <trd>capillary action</trd>
    </trdgrp>
    <trd lng="es">capilaridad</trd>
    <trd lng="fr">capillarit&eacute;</trd>
    <trdgrp lng="hu">
      <trd>hajsz&aacute;lcs&ouml;vess&eacute;g</trd>,
      <trd>hajsz&aacute;leress&eacute;g</trd>,
      <trd>kapillarit&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">capillariteit</trd>
  </snc>
</drv>
</art>
