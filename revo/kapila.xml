<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapila.xml,v 1.22 2023/12/03 08:00:22 revo Exp $">
<kap>
  <rad>kapilar</rad>/o
</kap>
<drv mrk="kapila.0o">
  <kap><tld/>o</kap>
  <snc mrk="kapila.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Harforma <ref tip="super" cel="angi.0o">angio</ref>, organo, kies
      interna diametro estas treege malgranda.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_l;&c_ja;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>kapil&#xE1;ra</trd>,
    <trd>kapil&#xE1;rka</trd>,
    <trd>vl&#xE1;se&#x10D;nice</trd>
  </trdgrp>
  <trd lng="de">Kapillare</trd>
  <trd lng="en">capillary</trd>
  <trd lng="es">capilar</trd>
  <trd lng="fa">&a_m;&a_w;&f_y;&a_r;&f_g;</trd>
  <trdgrp lng="fr">
    <trd>capillaire<klr tip="ind"> (subst.)</klr></trd>,
    <trd>vaisseau <ind>capillaire</ind></trd>,
    <trd>art&eacute;riole</trd>,
    <trd>veinule</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajsz&aacute;lcs&odblac;</trd>,
    <trd>hajsz&aacute;l&eacute;r</trd>,
    <trd>kapill&aacute;ris</trd>
  </trdgrp>
  <trd lng="ja">&#x6BDB;&#x7D30;&#x8840;&#x7BA1;  <pr>&#x3082;&#x3046;&#x3055;&#x3044;&#x3051;&#x3063;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">haarvat</trd>
  <trdgrp lng="pl">
    <trd>kapilara</trd>,
    <trd>naczynie w&lstroke;oskowe</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kapil&#xE1;ra</trd>,
    <trd>vl&#xE1;so&#x10D;nica</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&c_a;&#x43F;&c_ib;&#x43B;&#x44F;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x6BDB;&#x7D30;&#x7BA1; <pr>m&#xE1;ox&#xEC;gu&#x1CE;n</pr></trd>,
    <trd>&#x5FAE;&#x8840;&#x7BA1; <pr>w&#x113;ixi&#x11B;gu&#x1CE;n</pr></trd>,
    <trd>&#x5FAE;&#x8840;&#x7BA1; <pr>w&#x113;ixu&#xE8;gu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kapila.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o:
      <ekz>
        <tld/>a aktiveco, analizo, &hcirc;emio.
      </ekz>
    </dif>
    <trd lng="en">capillary</trd>
  </snc>
  <snc>
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        <tld/>aj tuboj
      </ekz>
    </dif>
    <trd lng="en">capillary</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>kapil&#xE1;rn&#xED;</trd>,
    <trd>vlasov&#x11B; tenk&#xFD;</trd>
  </trdgrp>
  <trd lng="es">capilar</trd>
  <trd lng="fr">capillaire<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>hajsz&aacute;lcs&ouml;es</trd>,
    <trd>hajsz&aacute;leres</trd>,
    <trd>kapill&aacute;ris</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6BDB;&#x7BA1;&#x306E;  <pr>&#x3051;&#x304B;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x6BDB;&#x7D30;&#x306E;  <pr>&#x3082;&#x3046;&#x3055;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x6BDB;&#x7D30;&#x8840;&#x7BA1;&#x306E;  <pr>&#x3082;&#x3046;&#x3055;&#x3044;&#x3051;&#x3063;&#x304B;&#x3093;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">capillair</trd>
  <trd lng="pl">kapilarny</trd>
  <trd lng="ru">
  &c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kapil&#xE1;rny</trd>,
    <trd>vlasovo tenk&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43F;&c_ib;&#x43B;&#x44F;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&#x43E;&#x43B;&#x43E;&c_s;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x6BDB;&#x7D30; <pr>m&#xE1;ox&#xEC;</pr></trd>
</drv>

<drv mrk="kapila.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kapila.0eco.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Propreco de <tld/>aj tuboj rilate al likva&jcirc;oj, kiuj ilin
      trafluas:
      <ekz>
        la suprenfluo de la suko en la plantoj estas fenomeno de
        <tld/>eco.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>capillarity</trd>,
      <trd>capillary action</trd>
    </trdgrp>
    <trd lng="es">capilaridad</trd>
    <trd lng="fa">&a_m;&a_w;&f_y;&f_y;&a_n;&f_g;&f_y;</trd>
    <trd lng="fr">capillarit&eacute;</trd>
    <trdgrp lng="hu">
      <trd>hajsz&aacute;lcs&ouml;vess&eacute;g</trd>,
      <trd>hajsz&aacute;leress&eacute;g</trd>,
      <trd>kapillarit&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">capillariteit</trd>
    <trd lng="pl">efekt <ind>kapilarny</ind></trd>
    <trd lng="ru">
    &c_k;&c_a;&c_p;&c_i;&c_l;&c_l;&c_ja;&c_r;&c_n;&c_o;&c_s;&c_t;&c_mol;
    </trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_l;&c_ja;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">kapilarita</trd>
  <trd lng="ja">&#x6BDB;&#x7BA1;&#x73FE;&#x8C61;  <pr>&#x3082;&#x3046;&#x304B;&#x3093;&#x3052;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>
  <trd lng="sk">kapilarita</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43F;&c_ib;&#x43B;&#x44F;&c_r;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_v;&#x43E;&#x43B;&#x43E;&c_s;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
