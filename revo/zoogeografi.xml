  <trdgrp lng="uk">
    <trd>&#x433;&#x435;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F; &#x442;&#x432;&#x430;&#x440;&#x438;&#x43D;</trd>,
    <trd>&#x437;&#x43E;&#x43E;&#x433;&#x435;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F;</trd>
