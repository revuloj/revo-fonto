<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: padus.xml,v 1.13 2024/02/01 09:24:08 revo Exp $">
<kap>
  <rad>padus</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="padus.0o">
  <kap><tld/>o</kap>
  <snc mrk="padus.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo a&ubreve; arbeto el E&ubreve;ropo kaj Nordazio kun densa
      foliaro, longaj, duonpendaj grapoloj da blankaj floroj, nigraj
      amaraj fruktetoj (<trd lng="la">Prunus padus</trd>):
      <ekz>
        la <tld/>aj densejoj sur la deklivetoj apud la rivero brilas kiel amasoj da
        blanka ne&gcirc;o
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vasta densejo el <tld/>oj, alnoj kaj aliaj arboj <klr>[&#8230;]</klr> kreskis sur
        dikaj tavoloj de malnova, altforna skorio
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="P"/>o, re&gcirc;ino de l' nordo &#11005;
        Kun blanka floraro, &#11005;
        Unua printempa heroldo, &#11005;
        Ornam' de l' kamparo
        <fnt>
          <aut>H. Dresen</aut>,
          <vrk>Paduso</vrk>,
          <lok>Dekdu poetoj, 1934</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meriz.0arbo">merizarbo</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/1/13/Ab_plant_28.jpg/640px-Ab_plant_28.jpg"
      prm="CC BY-SA 3.0"
      >floraroj de <tld/>o
        <fnt>
          <aut>Andrew Butko, propra verko</aut>
          <lok><url ref="&WCW;/File:Ab_plant_28.jpg"
              >https://commons.wikimedia.org/wiki/File:Ab_plant_28.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_m;&c_h;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_e;&c_m;&c_h;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_k;&c_a;&c_l;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_c;&c_ja;&c_r;&c_o;&c_m;&c_h;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_v;&c_a; <ind>&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_a;</ind></trd>,
    <trd>&c_g;&c_u;&c_r;&c_u;&c_p;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">st&rcaron;emcha</trd>
  <trd lng="de">Traubenkirsche</trd>
  <trdgrp lng="fr">
    <trd>cerisier &agrave; grappes</trd>,
    <trd>merisier &agrave; grappes</trd>
  </trdgrp>
  <trd lng="hu">zselnicemeggy</trd>
  <trdgrp lng="it">
    <trd>pado</trd>,
    <trd>ciliegio a grappoli</trd>
  </trdgrp>
  <trd lng="ja">&#12456;&#12478;&#12494;&#12454;&#12527;&#12511;&#12474;&#12470;&#12463;&#12521;</trd>
  <trd lng="nl">vogelkers</trd>
  <trd lng="pl">czeremcha</trd>
  <trd lng="sk">&ccaron;remcha</trd>
  <trd lng="sv">h&auml;gg</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_je;&c_m;&c_h;&c_a;</trd>
</drv>
</art>

