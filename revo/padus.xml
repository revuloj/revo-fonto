<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>padus</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="padus.0o">
  <kap><tld/>o</kap>
  <snc mrk="padus.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo a&ubreve; arbeto el E&ubreve;ropo kaj Nordazio kun densa
      foliaro, longaj, duonpendaj grapoloj da blankaj folioj, nigraj
      amaraj fruktetoj (<trd lng="la">Prunus padus</trd>):
      <ekz>
        <tld lit="P"/>o, re&gcirc;ino de l' nordo /
        Kun blanka floraro, /
        Unua printempa heroldo, /
        Ornam' de l' kamparo
        <fnt>
          <aut>H. Dresen</aut>,
          <vrk>Paduso</vrk>,
          <lok>Dekdu poetoj, 1934</lok>
        </fnt>;
      </ekz>
      <ekz>
        printempe kiam ekfloras <tld/>o la temperaturo forte
        malalti&gcirc;as 
        <fnt>
          <aut>dmych</aut>,
          <vrk><url ref="http://oramezo.wordpress.com/">Ora Mezo</url>
          </vrk>,
          <lok>2008-05-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cerisier &agrave; grappes</trd>,
    <trd>merisier &agrave; grappes</trd>
  </trdgrp>
  <trd lng="hu">zselnicemeggy</trd>
  <trd lng="pl">czeremcha</trd>
</drv>
</art>

