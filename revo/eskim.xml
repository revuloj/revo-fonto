<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eskim.xml,v 1.19 2025/10/20 16:38:37 revo Exp $">
<kap><rad>eskim</rad>/o <fnt><bib>JED</bib></fnt></kap>
<drv mrk="eskim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de popolo lo&gcirc;anta en Arkto, precipe en Gronlando, Norda
      Kanado,
      Alasko kaj orienta Siberio:
      <ekz>
        man&gcirc;kutimo alta je graso <klr>...</klr> ligi&gcirc;as kun
        plia
        ofteco de
        arteriosklerozo kaj korinfarkto, kvankam <tld/>oj estas esceptoj
        al tiu regulo
        <fnt><aut>Krys Ungar</aut>: <vrk>Vivu vigle kaj vegetare!</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="inuit.0o">inuito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_ib;&c_m;&c_o;&c_s;</trd>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;</trd>
  <trd lng="cs">Eskym&aacute;k</trd>
  <trd lng="de">Eskimo</trd>
  <trdgrp lng="en">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trd lng="es">esquimal</trd>
  <trdgrp lng="fr">
    <trd>Esquimau<klr> (subst.)</klr></trd>,
    <trd>Eskimo<klr> (subst.)</klr></trd>,
    <trd>Inuit<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">eszkim&oacute;</trd>
  <trdgrp lng="id">
    <trd>Eskimo</trd>,
    <trd>Inuit</trd>
  </trdgrp>
  <trd lng="ja">&#x30A8;&#x30B9;&#x30AD;&#x30E2;&#x30FC;</trd>
  <trd lng="nl">eskimo</trd>
  <trd lng="pl">Eskimos</trd>
  <trdgrp lng="pt">
    <trd>esquim&oacute;</trd>,
    <trd>esquimau</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;</trd>
  <trd lng="sk">Eskim&aacute;k</trd>
  <trd lng="uk">&c_je;&c_s;&#x43A;&c_ib;&#x43C;&#x43E;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x611B;&#x65AF;&#x57FA;&#25705; <pr>&#xE0;is&#x12B;j&#x12B;m&#xF3;</pr></trd>,
    <trd>&#x611B;&#x65AF;&#x57FA;&#25705;&#x4EBA; <pr>&#xE0;is&#x12B;j&#x12B;m&#xF3;r&#xE9;n</pr></trd>,
    <trd>&#x4F0A;&#22900;&#x4F9D; <pr>y&#x12B;n&#xFA;y&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eskim.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="inuit.0a">inuita</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_ib;&c_m;&c_o;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;&c_k;&c_i;</trd>
  <trd lng="es">esquimal</trd>
  <trd lng="de">Eskimo...</trd>
  <trdgrp lng="en">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>esquimau<klr> (adj.)</klr></trd>,
    <trd>eskimo<klr> (adj.)</klr></trd>,
    <trd>inuit<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">eszkim&oacute;</trd>
  <trdgrp lng="id">
    <trd>... <ind>Eskimo</ind></trd>,
    <trd>... <ind>Inuit</ind></trd>
  </trdgrp>
  <trd lng="nl">eskimo-</trd>
  <trd lng="pl">eskimoski</trd>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&c_je;&c_s;&#x43A;&c_ib;&#x43C;&#x43E;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>
</art>
