<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lumbag</rad>/o
</kap>
<drv mrk="lumbag.0o">
  <kap><tld/>o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad>lumbalgi</rad>/o</kap></var></kap>
  <snc mrk="lumbag.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Akuta <ref tip="malprt" cel="dolor.lumbo0o">lumbodoloro</ref>,
      plej ofte pro fortostre&ccirc;o:
      <ekz>
        li eksuferis de serioza gastrospasmo kaj lumbalgio
        <fnt>
          <vrk><url
          ref="http://www.espero.com.cn/old/epch/08h/08h-02/ziran2-5.htm">Hejmo
          de cignoj en &Ccirc;inio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pro akuta lumbago mi fari&gcirc;is ne povi sidi&gcirc;i longe
        <fnt>
          <vrk><url ref="http://www.ipernity.com/blog/48093/81480">Akuta
          lumbago</url>, blogero</vrk>
        </fnt>.
      </ekz>
    </dif>
    
  </snc>
  <trdgrp lng="ca">
    <trd>lumbago</trd>,
    <trd>lumb&agrave;lgia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bolest v z&aacute;dech</trd>,
    <trd>houser</trd>,
    <trd>lumbago</trd>,
    <trd>&uacute;st&rcaron;el</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Hexenschuss</trd>,
     <trd>Lumbalgie</trd>
  </trdgrp>
  <trd lng="en">lumbago <klr>(old popular term)</klr></trd>
  <trdgrp lng="fr">
    <trd>lumbago</trd>,
    <trd>tour de rein</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>lumbago</trd>,
    <trd><ind>nyeri</ind> pinggang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lumbago</trd>,
    <trd>heksenszus</trd>,
    <trd>korzonki</trd>
  </trdgrp>
  <trd lng="sk">&uacute;sad</trd>
</drv>
</art>

