<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lumbag</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lumbag.0o">
  <kap><tld/>o</kap>
  <snc mrk="lumbag.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tre forta lumbo<ref tip="lst" cel="dolor.0o"
      lst="voko:malsanoj">doloro</ref>:
      <ekz>
        mia fratino suferas de <klr>...</klr> kronika <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 78a, volumo 2a, p.
        635a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>lumbago</trd>,
    <trd>lumb&agrave;lgia</trd>
  </trdgrp>
  <trd lng="de">Hexenschuss</trd>
  <trdgrp lng="fr">
    <trd>lumbago</trd>,
    <trd>tour de rein</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>lumbago</trd>,
    <trd><ind>nyeri</ind> pinggang</trd>
  </trdgrp>
</drv>
</art>

