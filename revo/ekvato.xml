<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>ekvator</rad>/o
</kap>
<drv mrk="ekvato.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="cirkl.cxef0o">&Ccirc;efcirklo</ref> de la tera sfero,
      orta al ties rotacia <ref cel="aks.0o.rotacia">akso</ref>:
      <ekz>
        &ccirc;e la <tld/>o tago kaj nokto estas proksimume samda&ubreve;raj;
      </ekz>
      <ekz>
        la <tld/>a ebeno dividas la terglobon en du duonsferojn;
      </ekz>
      <ekz>
        la <ref cel="latitu.0o">latitudo</ref> de la <tld/>aj punktoj estas
        0&ring;.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;quateur</trd>
  <trd lng="ru">&c_e;&c_k;&c_v;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sv">ekvator</trd>
</drv>
</art>
