<kap>
<ofc>1</ofc>
<rad>ekvator</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="ekvato.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="cirkl.cxef0o">&Ccirc;efcirklo</ref> de la
      tera
      sfero,
      orta al ties rotacia <ref cel="aks.0o.rotacia">akso</ref>:
      <ekz>
        &ccirc;e la <tld/>o tago kaj nokto estas proksimume
        samda&ubreve;raj;
      </ekz>
      <ekz>
        la <tld/>a ebeno dividas la terglobon en du duonsferojn;
      </ekz>
      <ekz>
        la <ref cel="latitu.0o">latitudo</ref> de la <tld/>aj punktoj
        estas
        0&#730;.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_v;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">equador</trd>
  <trdgrp lng="cs">
    <trd>ekv&aacute;tor</trd>,
    <trd>rovn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">&Auml;quator</trd>
  <trd lng="en">equator</trd>
  <trd lng="es">ecuador (la l&iacute;nea)</trd>
  <trd lng="fr">&eacute;quateur</trd>
  <trd lng="hu">egyenl&iacute;t&odblac;</trd>
  <trd lng="ja">&#36196;&#36947;  <pr>&#12354;&#12363;&#12415;&#12385;</pr></trd>
  <trd lng="nl">evenaar</trd>
  <trd lng="pl">r&oacute;wnik</trd>
  <trd lng="pt">equador</trd>
  <trd lng="ru">&c_e;&c_k;&c_v;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">rovn&iacute;k</trd>
  <trd lng="sv">ekvator</trd>
  <trd lng="uk">&c_je;&c_k;&c_v;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="zh">&#36196;&#36947; <pr>ch&igrave;d&agrave;o</pr></trd>
</drv>
