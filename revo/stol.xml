<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stol.xml,v 1.21 2024/04/03 06:26:34 revo Exp $">
<kap>
  <rad>stol</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="stol.0o">
  <kap><tld/>o</kap>
  <snc mrk="stol.0o.TEKS">
    <uzo tip="fak">KRI</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Longa <ref tip="super" cel="tuk.kol0o">koltuko</ref>
      pendanta de &ccirc;irka&ubreve; la kolo &gcirc;is la genuoj,
      portata de kristanaj pastroj dum diservo:
      <ekz>
        en la mezo de la procesio <klr>[&#8230;]</klr> oni vidis pastrojn en blankaj surplisoj
        el multekostaj puntoj a&ubreve; en helsilkaj <tld/>oj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas ornato <klr>[&#8230;]</klr> aran&gcirc;ita per stofa strio ofte brodita,
        kies liturgiaj koloroj varias la&ubreve; la liturgia tempo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=406733">Stolo (liturgio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_p;&c_ib;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_l;&c_mol;
  </trd>
  <trd lng="cs">&scaron;t&oacute;la</trd>
  <trdgrp lng="de">
    <trd>Stola</trd>,
    <trd>Epitrachelion</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stole <klr>(vestment)</klr></trd>,
    <trd>epitrachelion</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;tole</trd>,
    <trd>&eacute;pitrachelion</trd>
  </trdgrp>
  <trd lng="grc">&epsilon_psili;&pi;&jota;&tau;&rho;&alfa;&chi;&eta_ton;&lambda;&jota;&omikron;&ny;</trd>
  <trd lng="hu">st&oacute;la</trd>
  <trd lng="it">stola<klr> (paramento sacro)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12473;&#12488;&#12521;</trd>,
    <trd>&#38968;&#22402;&#24111; <pr>&#12369;&#12356;&#12377;&#12356;&#12383;&#12356;</pr></trd>,
    <trd>&#12473;&#12488;&#12540;&#12523;</trd>
  </trdgrp>
  <trd lng="nl">stool</trd>
  <trd lng="pl">stu&lstroke;a</trd>
  <trd lng="ru">&c_je;&c_p;&c_i;&c_t;&c_r;&c_a;&c_h;&c_i;&c_l;&c_mol;
  </trd>
  <trd lng="sk">&scaron;t&oacute;la <klr>(&ccaron;as&tcaron; r&uacute;cha)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_jeu;&c_p;&c_i;&c_t;&c_r;&c_a;&c_h;&c_i;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
