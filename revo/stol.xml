<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="stol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Longa <ref tip="super" cel="tuk.kol0o">koltuko</ref>
      pendanta de &ccirc;irka&ubreve; la kolo &gcirc;is la genuoj,
      portata de kristanaj pastroj dum diservo. 
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_p;&c_ib;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_l;&c_mol;
  </trd>
  <trd lng="de">Stola</trd>
  <trd lng="en">stole <klr>(vestment)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;tole</trd>
  </trdgrp>
  <trd lng="hu">st&oacute;la</trd>
  <trd lng="it">stola<klr> (paramento sacro)</klr></trd>
  <trd lng="nl">stool</trd>
  <trd lng="pl">stu&lstroke;a</trd>
  <trd lng="ru">&c_je;&c_p;&c_i;&c_t;&c_r;&c_a;&c_h;&c_i;&c_l;&c_mol;
  </trd>
</drv>
</art>
