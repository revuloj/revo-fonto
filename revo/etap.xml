<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>etap</rad>/o
</kap>
<drv mrk="etap.0o">
<kap><tld/>o</kap>

 <snc>
  <dif>
   <refgrp tip="dif">
     <ref cel="stadi.0o.haltejo">Stadio (haltejo).</ref>
   </refgrp>
  </dif>
  <trdgrp lng="de">
     <trd>Rastplatz</trd>,
     <trd>Etappenort</trd>,
     <trd>Etappenziel</trd>,
     <trd>Etappe</trd>
  </trdgrp>
  <trd lng="hu">pihen&odblac;hely</trd>
 </snc>

 <snc>
  <dif>
   <refgrp tip="dif">
     <ref cel="stadi.0o.interhalteja_distanco">Stadio (interhalteja distanco)</ref>
   </refgrp>:
   <ekz>la regimento faris hodia&ubreve; longan <tld/>on..</ekz>
  </dif>
  <trd lng="de">Tagesmarsch</trd>
  <trdgrp lng="hu">
    <trd>menetszakasz</trd>,
    <trd>&uacute;tszakasz</trd>,
    <trd>etap</trd>
  </trdgrp>
 </snc>

 <snc>
  <dif>
   <refgrp tip="dif">
    <ref cel="stadi.0o.fazo">Stadio (fazo)</ref>.
   </refgrp>
  </dif>
  <trd lng="de">Etappe</trd>
  <trd lng="hu">szakasz</trd>
 </snc>

<trdgrp lng="cs">
  <trd>etapa</trd>,
  <trd>b&ecaron;h</trd>
</trdgrp>
<trd lng="fr">&eacute;tape</trd>
<trdgrp lng="ru">
  <trd>&c_e;&c_t;&c_a;&c_p;</trd>,
  <trd>&c_s;&c_t;&c_a;&c_d;&c_i;&c_ja;</trd>
</trdgrp>
</drv>
</art>
