<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kurkum</rad>/o
</kap>
<drv mrk="kurkum.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="zingib1.0oj">zingibracoj</ref>
      (<trd lng="la">Curcuma</trd>), kies rizomo enhavas flavan, tre
      kolorigan substancon uzatan kiel spica&jcirc;o kaj kuracilo.
    </dif>
  </snc>
  <trd lng="de">Gelbwurzel</trd>
  <trd lng="en">turmeric</trd>
  <trd lng="fr">curcuma</trd>
  <trdgrp lng="nl">
    <trd>geelwortel</trd>,
    <trd>curcuma</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_k;&c_u;&c_m;&c_a;</trd>
</drv>
</art>
