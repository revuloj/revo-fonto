<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kurkum.xml,v 1.29 2023/12/03 09:59:04 revo Exp $">
<kap>
<rad>kurkum</rad>/o
</kap>
<drv mrk="kurkum.0o">
  <kap>
  <tld/>o
  </kap>
  <snc mrk="kurkum.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="zingib1.0oj">zingibracoj</ref>
      (<trd lng="la">Curcuma</trd>), 
      plurjara <ref tip="lst" cel="herb.0o.BOT" lst="voko:zingibroparencoj">herbo</ref>
      kun disbran&ccirc;i&gcirc;anta karna aroma rizomo ofte kun tuberoj,   
      de kelkaj specioj uzata kiel spico, kolorilo kaj kuracilo;
      ordinare havanta elbaze kreskantajn pli malpli lancetformajn
      foliojn, kaj spikecajn florarojn sur apartaj tigoj provizitaj kun brakteoj
      portantaj kvaza&ubreve; en po&scirc;o du &gcirc;is sep trinombrajn florojn kun
      funeloforma korolo; la elipsoida tri&ccirc;ela 
      <ref tip="prt" cel="kapsul.0o">kapsulo</ref>
      malfermi&gcirc;as maturi&gcirc;inte:      
      <ekz>
        kelkaj fre&scirc;aj legomoj kaj herboj, ekzemple brokolo, petroselo, <tld/>o
        <klr>[&#x2026;]</klr> havas kancer-preventajn ecojn
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/7e/C&uacute;rcuma_(Curcuma_alismatifolia)_-_Flickr_-_Alejandro_Bayer.jpg/320px-C&uacute;rcuma_(Curcuma_alismatifolia)_-_Flickr_-_Alejandro_Bayer.jpg" prm="CC BY-SA 2.0"><tld/>o (Curcuma alismatifolia)
        <fnt>
          <aut>Alejandro  Bayer Tamayo el Armenia, Colombia</aut>
          <lok><url ref="&WCW;/File:C%C3%BArcuma_(Curcuma_alismatifolia)_-_Flickr_-_Alejandro_Bayer.jpg">&WCW;/File:C&uacute;rcuma_(Curcuma_alismatifolia)_-_Flickr_-_Alejandro_Bayer.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="pl">
      <trd>ostry&zdot;</trd>,
      <trd>kurkuma</trd>,
      <trd>szafranica</trd>,
      <trd>&zdot;&oacute;&lstroke;cie&nacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kurkum.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">Spico</ref> farita
      el la rizomo de <ref tip="malprt" cel="kurkum.0o.genro"><tld/>o<sncref/></ref>:
      <ekz>
        kareo <klr>[&#x2026;]</klr> konsistas el almena&ubreve; ses diversaj muelitaj spicoj, ekzemple:
        kajena pipro, <tld/>o, kariofilo, karvio, kardamomo, koriandro, zingibro,
        cinamo kaj pipro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2383">Karea&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/5b/Curcuma_longa_roots.jpg/320px-Curcuma_longa_roots.jpg" prm="CC BY-SA 3.0">rizomo kaj pulvoro de <tld/>o (Curcuma longa)
        <fnt>
          <aut>Simon A. Eugster, propra verko</aut>
          <lok><url ref="&WCW;/File:Curcuma_longa_roots.jpg">&WCW;/File:Curcuma_longa_roots.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="pl">kurkuma</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_k;&c_u;&c_m;&c_a;</trd>
  <trd lng="bg">&c_k;&c_u;&c_r;&c_k;&c_u;&c_m;&c_a;</trd>
  <trd lng="ca">c&uacute;rcuma</trd>
  <trd lng="de">Gelbwurz</trd>
  <trd lng="en">turmeric</trd>
  <trd lng="es">c&uacute;rcuma</trd>
  <trd lng="fa">&a_z;&a_r;&a_d;&f_ch;&a_w;&a_b;&a_h;</trd>
  <trd lng="fr">curcuma</trd>
  <trd lng="hu">kurkuma</trd>
  <trd lng="id">kunyit</trd>
  <trd lng="ja">&#x30A6;&#x30B3;&#x30F3;</trd>
  <trdgrp lng="nl">
    <trd>geelwortel</trd>,
    <trd>curcuma</trd>
  </trdgrp>
  <trd lng="pt">c&uacute;rcuma</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_k;&c_u;&c_m;&c_a;</trd>
  <trd lng="sv">gurkmeja</trd>
  <trd lng="uk">&#x43A;&#x443;&#x440;&#x43A;&#x443;&#x43C;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x59DC;&#x9EC4; <pr>ji&#x101;nghu&#xE1;ng</pr></trd>,
    <trd>&#x8591;&#x9EC3; <pr>ji&#x101;nghu&#xE1;ng</pr></trd>,
    <trd>&#x9EC3;&#x8591; <pr>hu&#xE1;ngji&#x101;ng</pr></trd>,
    <trd>&#x9EC4;&#x59DC; <pr>hu&#xE1;ngji&#x101;ng</pr></trd>,
    <trd>&#x6C89;&#x91D1; <pr>ch&#xE9;nj&#x12B;n</pr></trd>,
    <trd>&#x59DC;&#x9EC4;&#x6839; <pr>ji&#x101;nghu&#xE1;ngg&#x113;n</pr></trd>,
    <trd>&#x8591;&#x9EC3;&#x6839; <pr>ji&#x101;nghu&#xE1;ngg&#x113;n</pr></trd>,
    <trd>&#x59DC;&#x9EC4;&#x6839;&#x7684;&#x7C89;&#x672B; <pr>ji&#x101;nghu&#xE1;ngg&#x113;ndef&#x11B;nm&#xF2;</pr></trd>,
    <trd>&#x8591;&#x9EC3;&#x6839;&#x7684;&#x7C89;&#x672B; <pr>ji&#x101;nghu&#xE1;ngg&#x113;ndef&#x11B;nm&#xF2;</pr></trd>,
    <trd>&#x6C89;&#x91D1;&#x6839;&#x4E4B;&#x7C89;&#x672B; <pr>ch&#xE9;nj&#x12B;ng&#x113;nzh&#x12B;f&#x11B;nm&#xF2;</pr></trd>
  </trdgrp>
</drv>
</art>
