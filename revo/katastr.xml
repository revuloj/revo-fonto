<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
 <kap>
  <rad>katastr</rad>/o
 </kap>
 <drv mrk="katastr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
     Mezura planaro de la bienoj kaj terposedoj, starigita por difini la
terimpostojn kaj eviti koliziojn inter terposedantoj. Terenregistro.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Kataster</trd>,
     <trd>Grundbuch</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>land register</trd>,
     <trd>register of title deeds</trd>,
     <trd>official real estate register</trd>
  </trdgrp>
 </drv>
 <drv mrk="katastr.0ejo">
   <kap><tld/>ejo</kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       La oficejo, kie oni flegas la katastrojn. Terenregistrejo.
     </dif>
     <trdgrp lng="de">
        <trd>Katasteramt</trd>,
        <trd>Grundbuchamt</trd>
     </trdgrp>
   </snc>
 </drv>
</art>

