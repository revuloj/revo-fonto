<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brand.xml,v 1.28 2022/04/01 07:29:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brand</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brand.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="alkoho.0ajxo">Alkohola&jcirc;o</ref>, 
      farita per <ref tip="vid" cel="distil.0ado">distilado</ref>
      de fruktoj, grenoj, vino:
      <ekz>
        la &scirc;ipisto trinkis glaseton da <tld/>o por varmi&gcirc;i
        interne
        <fnt><bib>Fab4</bib>, <lok>Anneto</lok></fnt>;
      </ekz>
      <ekz>
        plej belaj grasaj angiloj, post kiuj oni nepre devas trinki 
        glaseton da <tld/>o
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
      </ekz> 
      <ekz>
        li komencis dronigadi siajn pensojn kaj sin mem en <tld/>o
        <fnt><bib>Fab3</bib>, <lok>Muta libro</lok></fnt>;
      </ekz>
      <ekz>
        kredeble li kanajlo anka&ubreve; drinkas <tld/>on
        <fnt><bib>Revizoro</bib></fnt>;
      </ekz>
      <ekz>
        li [...] ver&scirc;os en etan glason iom de la hejma forta
        <tld/>o<fnt><aut>J. Modest</aut>, <vrk>La ikono</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 12a, p. 
        23a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vodk.0o.KUI">vodko</ref>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwin-ardant</trd>,
    <trd>lambig</trd>,
    <trd>odivi</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnaps</trd>,
    <trd>Branntwein</trd>
  </trdgrp>
  <trd lng="en">brandy</trd>
  <trd lng="es">brandy</trd>
  <trdgrp lng="fr">
    <trd>brandy</trd>,
    <trd>eau-de-vie</trd>
  </trdgrp>
  <trd lng="hu">p&aacute;linka</trd>
  <trdgrp lng="pl">
    <trd>w&oacute;dka<klr> kolorowa</klr></trd>,
    <trd>brandy</trd>
  </trdgrp>
  <trd lng="pt">aguardente</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>brandy</trd>,
    <trd>ko&rcaron;alka</trd>,
    <trd>p&aacute;lenka</trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5E8;&#x5E0;&#x5D3;&#x5D9;</trd>
  <trdgrp lng="sk">
    <trd>brandy</trd>,
    <trd>p&aacute;lenka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&c_r;&c_ib;&#x43B;&#x43A;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_je;&#x43D;&c_d;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="brand.juniper0o">
  <kap>juniper<tld/>o</kap>
  <snc mrk="brand.juniper0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Greno<ref tip="super" cel="brand.0o.KUI"><tld/>o</ref>
      aromigita per juniperaj beroj:
      <ekz>
        mankis e&ccirc; guto da juniper<tld/>o, kaj anka&ubreve; ne plu
        eblis 
        kredite a&ccirc;eti iom da
        <fnt>
          <aut>J. H. F. Gr&ouml;nloh, trad. M. Meeuwissen</aut>,
          <vrk><url ref="http://www.purl.org/NET/mihxil/profitulo.html">
          La profitulo</url></vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n; <klr tip="amb">
  (&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="ca">ginebra</trd>
  <trdgrp lng="de">
    <trd>Wacholderbranntwein</trd>,
    <trd>Wacholderschnaps</trd>,
    <trd>Gin</trd>
  </trdgrp>
  <trd lng="en">gin</trd>
  <trd lng="es">ginebra</trd>
  <trd lng="fr">gin</trd>
  <trdgrp lng="hu">
    <trd>gin</trd>,
    <trd>bor&oacute;kap&aacute;linka</trd>
  </trdgrp>
  <trd lng="nl">gin</trd>
  <trdgrp lng="pl">
    <trd>gin</trd>,
    <trd>d&zdot;in</trd>,
    <trd>ja&lstroke;owc&oacute;wka</trd>
  </trdgrp>
  <trd lng="pt">gim</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>borovi&ccaron;ka</trd>,
    <trd>jalovcov&aacute;</trd>
  </trdgrp>
  <trd lng="sk">borovi&ccaron;ka</trd>
  <trd lng="uk">&#x44F;&#x43B;&c_ib;&c_v;&c_c;&c_ib;&c_v;&#x43A;&c_a;</trd>
</drv>

<drv mrk="brand.prun0o">
  <kap>prun<tld/>o</kap>
  <snc mrk="brand.prun0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="brand.0o.KUI"><tld lit="B"/>o</ref>
      farita el <ref tip="vid" cel="prun.0o">prunoj</ref>:
      <ekz>
        la urbo <klr>[Vizovice]</klr> estas sidejo de mondfama prun<tld/>ofarejo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=218086">Vizovice</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zwetschgenwasser</trd>
    <trdgrp lng="fr">
      <trd><ind>eau-de-vie</ind> de prunes</trd>,
      <trd>quetsch</trd>
    </trdgrp>
    <trd lng="pl">&sacute;liwowica</trd>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_ja;&c_n;&c_k;&c_a; <klr>(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">slivovice</trd>
  <trd lng="sk">slivovica</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43B;&c_i;&c_v;&#8217;&#x44F;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_s;&#x43B;&c_i;&c_v;&#x43E;&c_v;&c_i;&c_c;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="brand.cxeriz0o">
  <kap>&ccirc;eriz<tld/>o</kap>
  <snc mrk="brand.cxeriz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="brand.0o.KUI"><tld lit="B"/>o</ref>
      farita el <ref tip="vid" cel="cxeriz.0o">&ccirc;erizoj</ref>:
      <ekz>
        al la krono de &ccirc;iuj spiritoj decas la <klr>[...]</klr> &ccirc;erizbrando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=497876">Waldulm</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kirschwasser</trd>
    <trd lng="fr"><ind>eau-de-vie</ind> de cerises</trd>
    <trd lng="pl">wi&sacute;ni&oacute;wka</trd>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_sh;&c_n;&c_jo;&c_w;&c_k;&c_a; <klr>(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5E0;&#x5D3;&#x5D9;
  &#x5D3;&#x5D5;&#x5D1;&#x5D3;&#x5D1;&#x5E0;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_sh;&#x43D;&c_ib;&c_v;&#x43A;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_sh;&#x43D;&#x44F;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
