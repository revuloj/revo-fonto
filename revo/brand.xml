<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>brand</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brand.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="alkoho.0ajxo">Alkohola&jcirc;o</ref>, 
      farita per <ref tip="vid" cel="distil.0ado">distilado</ref>
      de fruktoj, grenoj, vino:
      <ekz>
        la &scirc;ipisto trinkis glaseton da <tld/>o por varmi&gcirc;i
        interne
        <fnt><bib>Fab4</bib>, <lok>Anneto</lok></fnt>;
      </ekz>
      <ekz>
        plej belaj grasaj angiloj, post kiuj oni nepre devas trinki 
        glaseton da <tld/>o
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
      </ekz> 
      <ekz>
        li komencis dronigadi siajn pensojn kaj sin mem en <tld/>o
        <fnt><bib>Fab3</bib>, <lok>Muta libro</lok></fnt>;
      </ekz>
      <ekz>
        kredeble li kanajlo anka&ubreve; drinkas <tld/>on
        <fnt><bib>Revizoro</bib></fnt>;
      </ekz>
      <ekz>
        li [...] ver&scirc;os en etan glason iom de la hejma forta
        <tld/>o<fnt><aut>J. Modest</aut>, <vrk>La ikono</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 12a, p. 
        23a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vodk.0o.KUI">vodko</ref>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwin-ardant</trd>,
    <trd>lambig</trd>,
    <trd>odivi</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnaps</trd>,
    <trd>Branntwein</trd>
  </trdgrp>
  <trd lng="en">brandy</trd>
  <trd lng="es">brandy</trd>
  <trdgrp lng="fr">
    <trd>brandy</trd>,
    <trd>eau-de-vie</trd>
  </trdgrp>
  <trd lng="hu">p&aacute;linka</trd>
  <trdgrp lng="pl">
    <trd>w&oacute;dka<klr> kolorowa</klr></trd>,
    <trd>brandy</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_k;&c_a;</trd>
  </snc>
</drv>

<drv mrk="brand.juniper0o">
  <kap>juniper<tld/>o</kap>
  <snc mrk="brand.juniper0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Greno<ref tip="super" cel="brand.0o.KUI"><tld/>o</ref>
      aromigita per juniperaj beroj:
      <ekz>
        mankis e&ccirc; guto da juniper<tld/>o, kaj anka&ubreve; ne plu
        eblis 
        kredite a&ccirc;eti iom da
        <fnt>
          <aut>J. H. F. Gr&ouml;nloh, trad. M. Meeuwissen</aut>,
          <vrk><url ref="http://www.purl.org/NET/mihxil/profitulo.html">
          La profitulo</url></vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n; <klr tip="amb">
  (&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="ca">ginebra</trd>
  <trdgrp lng="de">
    <trd>Wacholderbranntwein</trd>,
    <trd>Wacholderschnaps</trd>,
    <trd>Gin</trd>
  </trdgrp>
  <trd lng="en">gin</trd>
  <trd lng="es">ginebra</trd>
  <trd lng="fr">gin</trd>
  <trdgrp lng="hu">
    <trd>gin</trd>,
    <trd>bor&oacute;kap&aacute;linka</trd>
  </trdgrp>
  <trd lng="nl">gin</trd>
  <trdgrp lng="pl">
    <trd>gin</trd>,
    <trd>d&zdot;in</trd>,
    <trd>ja&lstroke;owc&oacute;wka</trd>
  </trdgrp>
  <trd lng="pt">gim</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;</trd>
  </snc>
</drv>

<drv mrk="brand.prun0o">
  <kap>prun<tld/>o</kap>
  <snc mrk="brand.prun0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="brand.0o.KUI"><tld lit="B"/>o</ref>
      farita el <ref tip="vid" cel="prun.0o">prunoj</ref>.
    </dif>
    <trd lng="pl">&sacute;liwowica</trd>
  </snc>
</drv>

<drv mrk="brand.cxeriz0o">
  <kap>&ccirc;eriz<tld/>o</kap>
  <snc mrk="brand.cxeriz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="brand.0o.KUI"><tld lit="B"/>o</ref>
      farita el <ref tip="vid" cel="cxeriz.0o">&ccirc;erizoj</ref>.
    </dif>
    <trd lng="pl">wi&sacute;ni&oacute;wka</trd>
  </snc>
</drv>
</art>
