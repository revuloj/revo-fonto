<vortaro>
<art mrk="$Id$">
<kap>
  <rad>duenj</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="duenj.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ina <ref tip="super" cel="serv.0isto">servisto</ref>, pli a&gcirc;a,
      kiu gardas altrangan fra&ubreve;linon eksterhejme:
      <ekz>
        la <tld/>o, duonmorta de envio kaj plena de timo, staras
        garde<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 32a, p.
        238a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>chaperon</trd>,
    <trd>du&egrave;gne</trd>
  </trdgrp>
</drv>
</art>

