<vortaro>
<art mrk="$Id$">
<kap>
  <rad>duenj</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="duenj.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ina <ref tip="super" cel="serv.0isto">servisto</ref>, pli
      a&gcirc;a,
      kiu gardas altrangan fra&ubreve;linon eksterhejme:
      <ekz>
        la <tld/>o, duonmorta de envio kaj plena de timo, staras
        garde
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 32a, p.
        238a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_e;&c_n;&c_mol;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_mol;&c_jo;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_a;&c_p;&c_je;&c_r;&c_o;&c_n;&c_a;</trd>
  <trd lng="de">Anstandsdame</trd>
  <trdgrp lng="en">
    <trd>chaperon</trd>,
    <trd>chaperone</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chaperon</trd>,
    <trd>du&egrave;gne</trd>
  </trdgrp>
  <trd lng="nl">chaperonne</trd>
  <trd lng="pl">przyzwoitka</trd>
  <trdgrp lng="pt">
    <trd>duenha</trd>,
    <trd>dama de companhia</trd>
  </trdgrp>
  <trd lng="sk">due≈àa</trd>
</drv>
</art>
