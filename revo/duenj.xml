<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: duenj.xml,v 1.14 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>duenj</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="duenj.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ina <ref tip="super" cel="serv.0isto">servisto</ref>, pli
      a&gcirc;a,
      kiu gardas altrangan fra&ubreve;linon eksterhejme:
      <ekz>
        la <tld/>o, duonmorta de envio kaj plena de timo, staras
        garde
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 32a, p.
        238a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_e;&c_n;&c_mol;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_mol;&c_jo;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_a;&c_p;&c_je;&c_r;&c_o;&c_n;&c_a;</trd>
  <trd lng="cs">due&ncaron;a</trd>
  <trd lng="de">Anstandsdame</trd>
  <trdgrp lng="en">
    <trd>chaperon</trd>,
    <trd>chaperone</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>due&ntilde;a</trd>,
    <trd>do&ntilde;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chaperon</trd>,
    <trd>du&egrave;gne</trd>
  </trdgrp>
  <trd lng="nl">chaperonne</trd>
  <trd lng="pl">przyzwoitka</trd>
  <trdgrp lng="pt">
    <trd>duenha</trd>,
    <trd>dama de companhia</trd>
  </trdgrp>
  <trd lng="sk">due&ncaron;a</trd>
  <trd lng="uk">&#x434;&#x443;&#x435;&#x43D;&#x44C;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F34;&#x62A4; <pr>b&#xE0;nh&#xF9;</pr></trd>,
    <trd>&#x5973;&#x4F34; <pr>n&#x1DA;b&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
