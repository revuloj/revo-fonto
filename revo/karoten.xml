<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karoten.xml,v 1.4 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>karoten</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="karoten.0o">
  <kap><tld/>o</kap>
  <snc mrk="karoten.0o.DIET">
    <dif>
      Oran&gcirc;kolora
      <ref tip="super" cel="pigment.0o.BIO">pigmento</ref>
      trovebla en iuj formoj en karotoj, spinakoj, kapsikoj,
      kukurboj kaj aliaj:
      <ekz>
        multaj vegeta&jcirc;aj nutra&jcirc;oj enhavas <tld/>on,
        substancon tre parencan al vitamino A
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">carotene</trd>
  <trd lng="es">caroteno</trd>
  <trd lng="fr">carot&egrave;ne</trd>
  <trd lng="it">carotene</trd>
  <trd lng="pl">karoten</trd>     
  <trd lng="ro">caroten</trd>
  <trd lng="uk">&#x43A;&#x430;&#x440;&#x43E;&#x442;&#x438;&#x43D;</trd>
</drv>
</art>

