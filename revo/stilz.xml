<art mrk="$Id: stilz.xml,v 1.17 2024/03/25 07:28:36 revo Exp $">
<kap>
  <rad>stilz</rad>/o <fnt><aut>K. Bennemann</aut>, <vrk>Esperanto
  Hand-W&ouml;rterbuch</vrk>, <lok>1923-1926</lok></fnt>
</kap>
<drv mrk="stilz.0o">
  <kap><tld/>o</kap>
  <snc mrk="stilz.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="ir.0ilo">Irilo</ref>,
      <ref tip="sin" cel="pasx.0stango">pa&scirc;stango</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tie staris malgrandaj grupoj el arboj, kies trunkoj malsupre
        parti&gcirc;is je kvar, kvin a&ubreve; pli multaj kurbi&gcirc;intaj <tld/>oj,
        tiel ke ili aspektis kiel grandaj kankroj, starantaj sur multaj kruroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Pramaljuna Morlino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_d;&c_u;&c_l;&c_ja;</trd>
  <trd lng="de">Stelze</trd>
  <trd lng="fr">&eacute;chasse</trd>
  <trd lng="hu">g&oacute;lyal&aacute;b</trd>
  <trd lng="ja">&#x7AF9;&#x99AC;  <pr>&#x305F;&#x3051;&#x3046;&#x307E;</pr></trd>
  <trd lng="nl">stelt</trd>
  <trd lng="pl">szczud&lstroke;o</trd>
  <trd lng="ru">&c_h;&c_o;&c_d;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&#x43E;&#x44F;&#x43A; (&c_r;&c_i;&c_sh;&c_t;&c_u;&c_v;&c_a;&#x43D;&#x44C;)</trd>,
    <trd>&c_d;&c_i;&c_b;&c_i;</trd>,
    <trd>&c_h;&#x43E;&c_d;&c_u;&#x43B;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8DF7; <pr>qi&#x101;o</pr></trd>,
    <trd>&#x8E7A; <pr>qi&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
