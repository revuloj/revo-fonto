<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>recept</rad>/o
</kap>
<drv mrk="recept.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Skriba klarigo pri preparado de man&gcirc;a&jcirc;o, trinka&jcirc;o
      a&ubreve; ia teknika&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili postulas, ke ni rebaku la tutan lingvon
        la&ubreve; ilia teoria <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="he">&mem;&tav;&kaf;&vav;&fnun;</trd>
    <trd lng="hu">&eacute;telrecept</trd>    
  </snc>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Skriba formulo pri preparado kaj uzado de kuracilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="preskr.0i">preskribi</ref>.
    </refgrp>
    <trd lng="tr">re&ccedil;ete</trd>
    <trd lng="he">&mem;&resh;&shin;&fmem;</trd>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_c;&c_e;&c_p;&c_t;</trd>
  <trd lng="de">Rezept</trd>
  <trd lng="fr">recette</trd>
  <trd lng="he">&resh;&tsadi;&pe;&tet;</trd>
  <trdgrp lng="hu">
    <trd>recept</trd>,
    <trd>v&eacute;ny</trd>,
    <trd>rendelv&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">recept</trd>
  <trd lng="es">receta</trd>
  <trd lng="ru">&c_r;&c_je;&c_c;&c_je;&c_p;&c_t;</trd>
</drv>
</art>
