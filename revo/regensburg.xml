<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Regensburg</rad>/o<fnt>Z</fnt>
    </kap>

    <drv mrk="regensburg.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>

      <snc>
	<fnt><bib>ZR</bib></fnt>
	<fnt><bib>PIV1</bib></fnt>
	<dif>
          <ref tip="super" cel="urb.0o">Urbo</ref> en suda
	  <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta &ccirc;e la
	  rivero Danubo en la federacia lando
	  <ref tip="vid" cel="bavar.B0ujo">Bavarujo</ref>:
	  <ekz>
	    <klr>(la bavaraj episkopoj)</klr> kaptis Metodion kaj
	    ju&gcirc;is lin en <tld/>o anta&ubreve; Ludoviko la Germana
	    <fnt>
	      <aut>Sergio Pokrovskij</aut>:
	      <vrk>Slavonaj skriboj (1)</vrk>,
	      <bib>LOdE</bib><lok>1998, n-ro 12(50).</lok>
	    </fnt>
	  </ekz>
	</dif>
	<trd lng="be">&c_R;&c_e;&c_g;&c_je;&c_n;&c_s;&c_b;&c_u;&c_r;&c_g;</trd>
	<trd lng="de">Regensburg</trd>
	<trd lng="hu">Regensburg</trd>
	<trd lng="lat">Regina Castra</trd>
	<trd lng="la">Ratisbona</trd>
      </snc>
      <trd lng="nl">Regensburg</trd>
      <trd lng="ru">&c_R;&c_je;&c_g;&c_je;&c_n;&c_s;&c_b;&c_u;&c_r;&c_g;</trd>
      
    </drv>
  </art>

