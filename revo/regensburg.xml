<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
    <rad>Regensburg</rad>/o<fnt>Z</fnt>
   </kap>
   <drv mrk="regensburg.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>

    <snc>
       <fnt><bib>ZR</bib></fnt>
       <fnt><bib>PIV1</bib></fnt>
       <dif>
          <ref tip="super" cel="urb.0o">Urbo</ref> en suda <ref
tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta &ccirc;e la
rivero Danubo en la federacia lando <ref tip="vid"
cel="bavar.B0ujo">Bavarujo</ref>.
       </dif>
       <trd lng="de">Regensburg</trd>
       <trd lng="lat">Regina Castra</trd>
       <trd lng="la">Ratisbona</trd>
    </snc>
<trd lng="nl">Regensburg</trd>
       
   </drv>
  </art>

