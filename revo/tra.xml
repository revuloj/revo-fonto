  <trd lng="hmn">los ntawm</trd>
  <trd lng="hr">kroz</trd>
  <trd lng="ht">atrav&egrave;</trd>
  <trd lng="hy">&#1396;&#1387;&#1403;&#1400;&#1409;&#1400;&#1406;</trd>
  <trd lng="ig">site na</trd>
