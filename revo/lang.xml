<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lang</rad>/o
</kap>
<drv mrk="lang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lang.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Movebla mola karna organo en la <ref tip="malprt"
      cel="busx.0o.ANA">bu&scirc;o</ref>, servanta por gustumi kaj paroli:
      <ekz>
        montri la <tld/>on al la kuracisto;
      </ekz>
      <ekz>
        man&gcirc;i bov<tld/>on.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lang.0o.parolkapablo">
    <dif>
      Tiu organo, &ccirc;e la homo, konsiderata kiel sidejo de parolkapablo:
      <ekz>
        ili parolas kun mi per mensogema <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 109:2</lok></fnt>;
      </ekz>
      <ekz>
        milda<fnt>Z</fnt>,
      </ekz>
      <ekz>
        malpia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mia <tld/>o parolos veron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        gardi sian <tld/>on<fnt>Z</fnt>
        <klr>(silenti)</klr>;
      </ekz>
      <ekz>
        <tld/>a vundo plej profunda
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        plej bona gvidilo estas <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia nomo estas sur &ccirc;ies <tld/>o
        <klr>(li estas tre fama)</klr>;
      </ekz>
      <ekz>
        <ctl>Fajro sur mia <tld/>o</ctl>
        <klr>(poemaro de Edwin de Kock)</klr>
        <fnt><aut>de Kock Edwin</aut>, <vrk>Fajro sur mia lango. - La
        Laguna: R&eacute;gulo, 1967</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lang.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io simila al <tld/>o:
      <ekz>
        longaj <tld/>oj de flamo;
      </ekz>
      <ekz>
        ter<tld/>o <klr>(terkapo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_z;&c_y;&c_k;</trd>
  <trd lng="cs">jazyk (v &uacute;stech)</trd>
  <trd lng="de">Zunge</trd>
  <trd lng="el">&gamma;&lambda;&omega_ton;&sigma;&sigma;&alfa;</trd>
  <trd lng="en">tongue</trd>
  <trd lng="es">lengua</trd>
  <trd lng="fr">langue<klr> (organe)</klr></trd>
  <trd lng="it">lingua</trd>
  <trd lng="nl">tong</trd>
  <trd lng="pt">l&iacute;ngua</trd>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_k; <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;)</klr></trd>
  <trd lng="sv">tunga</trd>
  <trd lng="tr">dil</trd>
</drv>

<drv mrk="lang.0trinki">
  <kap><tld/>trinki</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trinki elprenante flua&jcirc;on per la <tld/>o, kiel hundo.
    </dif>
    <refgrp tip="vid">
      <ref cel="lek.0i">leki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_ja;&c_b;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>lecken</trd>,
    <trd>auflecken</trd>,
    <trd>schlecken</trd>
  </trdgrp>
  <trd lng="en">lap</trd>
  <trd lng="fr">laper</trd>
  <trdgrp lng="nl">
    <trd>oplikken</trd>,
    <trd>slobberen</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">lapa</trd>
</drv>
</art>
