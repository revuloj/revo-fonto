<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bukl.xml,v 1.23 2021/03/05 18:27:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bukl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bukl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bukl.0o.hara">
    <dif>
      Spirale volvi&gcirc;anta fasketo da haroj:
      <ekz>
        blondaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tuf.0o">tufo</ref>.
    </refgrp>
    <trd lng="bg">&c_k;&c_malmol;&c_d;&c_r;&c_i;&c_c;&c_a;</trd>
    <trd lng="de">Locke</trd>
    <trdgrp lng="en">
      <trd>curl</trd>,
      <trd>lock</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hajf&uuml;rt</trd>,
      <trd>tincs</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>lok</trd>,
      <trd>k&eogonek;dzior</trd>
    </trdgrp>
    <trd lng="pt">caracol<klr> (nos cabelos)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_k;&c_o;&c_n;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_i;&c_t;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bukl.0o.benda">
    <dif>
      Ringforme kurbigita fadeno, rubando, &scirc;nuro.
    </dif>
    <refgrp tip="vid">
      <ref cel="bant.0o">banto</ref>,
      <ref cel="masx.0o">ma&scirc;o</ref>,
      <ref cel="nod.0o">nodo</ref>.
    </refgrp>
    <trd lng="bg">&c_p;&c_r;&c_i;&c_m;&c_k;&c_a;</trd>
    <trd lng="de">Schleife</trd>
    <trd lng="en">loop</trd>
    <trd lng="hu">hurok</trd>
    <trd lng="pl">p&eogonek;tla</trd>
    <trd lng="ru">&c_v;&c_i;&c_t;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="bukl.0o.grafeo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      Tia <ref tip="super" cel="egx.0o.grafeo">e&gcirc;o</ref>, ke la du
      verticoj &gcirc;in difinantaj estas identaj:
      <ekz>
        <tld/>on de neoritentita grafeo eblas difini kiel unuelementan
        e&gcirc;on.
      </ekz>
    </dif>
    <trd lng="bg">&c_p;&c_r;&c_i;&c_m;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Schlinge</trd>,
      <trd>singul&auml;re <ind>Kante</ind></trd>,
      <trd>Einkreis</trd>,
      <trd>Schleife</trd></trdgrp> 
    <trd lng="en">loop<klr tip="ind"> (in a graph)</klr></trd>
    <trd lng="fr">boucle<klr tip="ind"> (d'un graphe)</klr></trd>
    <trd lng="hu">hurok&eacute;l<klr tip="ind">
    (gr&aacute;f&eacute;)</klr></trd>    
    <trd lng="pl">p&eogonek;tla<klr tip="ind"> (grafu)</klr></trd>
    <trd lng="ru">&c_p;&c_je;&c_t;&c_l;&c_ja;<klr tip="ind">
    (&c_g;&c_r;&c_a;&c_f;&c_a;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kade&rcaron;</trd>,
    <trd>kudrna</trd>,
    <trd>ku&ccaron;era</trd>
  </trdgrp>
  <trd lng="es">bucle</trd>
  <trd lng="fr">boucle<klr tip="ind"> (de cheveux, fil,
  etc.)</klr></trd>
  <trd lng="he">&#x5EA;&#x5DC;&#x5EA;&#x5DC;</trd>
  <trdgrp lng="sk">
    <trd>kulma</trd>,
    <trd>nat&aacute;&ccaron;ka</trd>
  </trdgrp>
</drv>

<drv mrk="bukl.0a">
  <kap><tld/>a, <var><kap><tld/>i&gcirc;a</kap></var></kap>
  <snc mrk="bukl.0a.KOMUNE">
    <dif>
      Spirale volvi&gcirc;anta:
      <ekz>
        jes, jes! &ndash; kun fiereco respondis grafino E&ubreve;dokio, 
        fermante la romanon kaj ordigante per la manoj siajn <tld/>ajn 
        harojn
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>[1898?]</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_malmol;&c_d;&c_r;&c_a;&c_v;</trd>
  <trd lng="de">lockig</trd>
  <trdgrp lng="en">
    <trd>curly</trd>,
    <trd>loopy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rizado</trd>,
    <trd>ondulado</trd>
  </trdgrp>
  <trd lng="fr">boucl&eacute;</trd>
  <trd lng="he">&#x5DE;&#x5EA;&#x5D5;&#x5DC;&#x5EA;&#x5DC;</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;nd&ouml;r</trd>,
    <trd>g&ouml;nd&ouml;r&ouml;d&odblac;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kr&eogonek;cone<klr> (o w&lstroke;osach)</klr></trd>,
    <trd>zakr&eogonek;cone w <ind>loki</ind></trd>,
    <trd>k&eogonek;dzierzawe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_v;&c_mol;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_r;&c_ja;&c_v;&c_y;&c_j; <klr>(&c_o;
    &c_v;&c_o;&c_l;&c_o;&c_s;&c_a;&c_h;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bukl.0i">
  <kap><tld/>i, <var><kap><tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bukl.0i.bukleformi">
    <dif>
      <tld lit="B"/>eformi:
      <ekz>
        liaj haroj estas <tld/>itaj, nigraj kiel korvo
        <fnt><bib>MT</bib> <lok>&Alt; 5:11</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="friz.0i">frizi</ref>,
      <ref cel="krisp">krispi</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_k;&c_malmol;&c_d;&c_r;&c_ja;</trd>
  <trd lng="cs">nat&aacute;&ccaron;et vlasy</trd>
  <trd lng="de">locken</trd>
  <trd lng="en">curl</trd>
  <trd lng="es">rizar</trd>
  <trd lng="fr">boucler <klr tip="ind">(donner une forme de
  boucle)</klr></trd>
  <trd lng="he">&#x5DC;&#x5EA;&#x5DC;&#x5EA;&#x5DC;</trd>
  <trd lng="hu">g&ouml;nd&ouml;r&ouml;dik</trd>
  <trdgrp lng="pl">
    <trd>kr&eogonek;ci&cacute;<klr> w&lstroke;osy</klr></trd>,
    <trd>nawija&cacute;<klr> w&lstroke;osy (np. na
    lok&oacute;wk&eogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nakaderi&tcaron;</trd>,
    <trd>naku&ccaron;eri&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="bukl.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="bukl.0ilo.KOS">
    <dif>
       Cilindreto, &ccirc;irka&ubreve; kiu oni volvas varmigitajn harojn por formi <tld/>on:
       <ekz>
         pro hasto &scirc;i forgesis demeti du <tld/>ilojn &ccirc;e la tempioj
         <fnt>
           <aut>I. Silone, trad. D. Mistretta</aut>
           <vrk><url ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">La sekreto de Luka</url></vrk>
           <lok>[vidita en 2018]</lok>
         </fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="fr">bigoudi</trd>
  <trd lng="he">&#x5DE;&#x5E1;&#x5DC;&#x5E1;&#x5DC;</trd>
</drv>



<drv mrk="bukl.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="bukl.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on.
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_j;&c_t;&c_o; &c_n;&c_ja;&c_m;&c_a; &c_k;&c_malmol;&c_d;&c_r;&c_i;&c_c;&c_i;</trd>
    <trd lng="hu">sima <klr tip="ind">(haj)</klr></trd>        
    <trd lng="pl">prosty<klr> (bez lok&oacute;w)</klr></trd>
  </snc>
  <snc mrk="bukl.sen0a.grafeo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      Tia, ke neniu e&gcirc;o de &gcirc;i estas 
      <ref cel="bukl.0o.grafeo"><tld/>o <sncref/></ref>.
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_j;&c_t;&c_o; &c_n;&c_ja;&c_m;&c_a; &c_p;&c_r;&c_i;&c_m;&c_k;&c_i;</trd>
    <trd lng="de"><klr tip="ind">(Graph) </klr>ohne
    <ind>Schleife</ind></trd>
    <trd lng="en">loop-free<klr tip="ind"> (graph)</klr></trd>
    <trd lng="es">sin <ind>rizos</ind></trd>
    <trd lng="fr"><klr tip="ind">(graphe) </klr>sans
    <ind>boucle</ind></trd>
    <trd lng="hu">hurok&eacute;lmentes <klr tip="ind">(gr&aacute;f)</klr></trd>    
    <trd lng="ru"><klr tip="ind">(&c_g;&c_r;&c_a;&c_f;)
    </klr>&c_b;&c_je;&c_z; &c_p;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trd lng="cs">beze smy&ccaron;ky</trd>
  <trd lng="sk">bez slu&ccaron;ky</trd>
</drv>

</art>
