<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bukl</rad>/o
</kap>
<drv mrk="bukl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Spirale volvi&gcirc;anta fasketo da haroj:
      <ekz>
        blondaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tuf.0o">tufo</ref>.
    </refgrp>
    <trd lng="de">Locke</trd>
  </snc>
  <snc num="2">
    <dif>
      Ringforme kurbigita fadeno, rubando, &scirc;nuro. 
    </dif>
    <refgrp tip="sin">
      <ref cel="bant.0o">banto</ref>,
      <ref cel="masx.0o">ma&scirc;o</ref>,
      <ref cel="nod.0o">nodo</ref>.
    </refgrp>
    <trd lng="de">Schleife</trd>
  </snc>
</drv>
<drv mrk="bukl.0a0igxa">
  <kap><tld/>a, <tld/>i&gcirc;a</kap>
  <snc>
    <dif>
      Spirale volvi&gcirc;anta.
    </dif>
  </snc>
  <trd lng="de">lockig</trd>
</drv>
<drv mrk="bukl.0igi">
  <kap><tld/>(ig)i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld/>eformi:
      <ekz>
        Liaj haroj estas <tld/>itaj nigraj kiel korvo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Alta kanto 5, 11</lok>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="friz.0i">frizi</ref>,
      <ref cel="krisp">krispi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">locken</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>