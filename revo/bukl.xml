<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bukl.xml,v 1.29 2023/10/29 19:11:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bukl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bukl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bukl.0o.hara">
    <dif>
      Spirale volvi&gcirc;anta <ref tip="super" cel="fask.0o.KOMUNE">fasketo</ref> da haroj:
      <ekz>
        <klr>[li]</klr> kaptis min je la <tld/>oj de mia kapo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 8:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        abundaj <tld/>oj ondi&gcirc;adis &ccirc;irka&ubreve; la rondaj &scirc;ultroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rabeno <klr>[&#x2026;]</klr> pin&ccirc;adis kaj krispigadis la <tld/>ojn de sia barbo
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda falsa <tld/>o pendis super unu el &scirc;iaj okuloj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluvo gluis la <tld/>ojn de liaj longaj haroj en &scirc;nurojn
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &jcirc;uras al vi ne je la <tld/>oj de Bak&hcirc;o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XL</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rabota&jcirc;o fluis kiel belaj, flavaj <tld/>oj el la rabotilo
        <fnt>
          <bib>ViV</bib>
          <lok>VI.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frangx.0o">fran&gcirc;o</ref>,
      <ref cel="tuf.0o">tufo</ref>,
      <ref cel="vil.0o">vilo</ref>
    </refgrp>
    <trd lng="bg">&c_k;&c_malmol;&c_d;&c_r;&c_i;&c_c;&c_a;</trd>
    <trd lng="de">Locke</trd>
    <trdgrp lng="en">
      <trd>curl</trd>,
      <trd>lock</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hajf&uuml;rt</trd>,
      <trd>tincs</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>lok</trd>,
      <trd>k&eogonek;dzior</trd>
    </trdgrp>
    <trd lng="pt">caracol<klr> (nos cabelos)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_k;&c_o;&c_n;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_i;&c_t;&c_o;&c_k;</trd>
    </trdgrp>
    <trd lng="zh">&#x9B08;&#x53D1; <pr>qu&#xE1;nf&#x1CE;</pr></trd>
</snc>
  <snc mrk="bukl.0o.benda">
    <dif>
      Ronde kurbigita fadeno, rubando, &scirc;nuro:
      <ekz>
        <ref tip="malprt" cel="bant.0o">banto</ref> <klr>[&#x2026;]</klr> havas du <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=459936">Banto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        lapfermilo <klr>[konsistas el]</klr> du kuni&gcirc;antaj partoj: kro&ccirc;iloj unuflanke
        kaj harsimilaj <tld/>oj aliflanke
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=203074">Lapfermilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Race Across The Alps</nom> <klr>[la&ubreve;ire de]</klr> <tld/>o de 530&#x202F;km 
        traveturante la plej imponajn trapasejojn de la Alpoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=566290">Ultrabiciklado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="masx.0o">ma&scirc;o</ref>,
      <ref cel="nod.0o">nodo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="bander.0o">banderolo</ref>,
      <ref cel="kordon.0o.rubando">kordono<sncref/></ref>,
      <ref cel="zon.0o">zono</ref>
    </refgrp>
    <trd lng="bg">&c_p;&c_r;&c_i;&c_m;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Schleife</trd>,
      <trd>Schlaufe</trd>
    </trdgrp>
    <trd lng="en">loop</trd>
    <trd lng="hu">hurok</trd>
    <trd lng="pl">p&eogonek;tla</trd>
    <trd lng="ru">&c_v;&c_i;&c_t;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="bukl.0o.grafeo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      Tia <ref tip="super" cel="egx.0o.grafeo">e&gcirc;o</ref>, ke la du
      verticoj &gcirc;in difinantaj estas identaj:
      <ekz>
        <tld/>on de neoritentita grafeo eblas difini kiel unuelementan
        e&gcirc;on;
      </ekz>
      <ekz>
        oni povas konsideri <tld/>on kiel specialan kazon de ciklo <klr>[kiu]</klr>
        havas unusolan e&gcirc;on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=582763">Buklo (grafeo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_p;&c_r;&c_i;&c_m;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Schlinge</trd>,
      <trd>singul&auml;re <ind>Kante</ind></trd>,
      <trd>Einkreis</trd>,
      <trd>Schleife</trd></trdgrp> 
    <trd lng="en">loop<klr tip="ind"> (in a graph)</klr></trd>
    <trd lng="fr">boucle<klr tip="ind"> (d'un graphe)</klr></trd>
    <trd lng="hu">hurok&eacute;l<klr tip="ind">
    (gr&aacute;f&eacute;)</klr></trd>    
    <trd lng="pl">p&eogonek;tla<klr tip="ind"> (grafu)</klr></trd>
    <trd lng="ru">&c_p;&c_je;&c_t;&c_l;&c_ja;<klr tip="ind">
    (&c_g;&c_r;&c_a;&c_f;&c_a;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kade&rcaron;</trd>,
    <trd>kudrna</trd>,
    <trd>ku&ccaron;era</trd>
  </trdgrp>
  <trd lng="es">bucle</trd>
  <trd lng="fr">boucle<klr tip="ind"> (de cheveux, fil,
  etc.)</klr></trd>
  <trd lng="he">&tav;&lamed;&tav;&lamed;</trd>
  <trdgrp lng="ja">
    <trd>&#x5DFB;&#x304D;&#x6BDB;  <pr>&#x307E;&#x304D;&#x3052;</pr></trd>,
    <trd>&#x30AB;&#x30FC;&#x30EB;</trd>,
    <trd>&#x8F2A;  <pr>&#x308F;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kulma</trd>,
    <trd>nat&aacute;&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_i;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_o;&c_k;&nbsp;(&c_d;&c_r;&c_o;&c_t;&c_u;)</trd>,
    <trd>&c_p;&c_je;&c_t;&c_l;&c_ja;&nbsp;(&c_s;&c_t;&c_r;&c_ib;&c_ch;&c_k;&c_i; &c_ib; &c_t;. &c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="bukl.0a">
  <kap><tld/>a, <var><kap><tld/>i&gcirc;a</kap></var></kap>
  <snc mrk="bukl.0a.volvigxa">
    <dif>
      Spirale volvi&gcirc;anta:
      <ekz>
        <tld/>a hartufo
        <fnt>
          <bib>MkM</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        jes, jes! &ndash; kun fiereco respondis grafino E&ubreve;dokio, 
        fermante la romanon kaj ordigante per la manoj siajn <tld/>ajn 
        harojn
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>[1898?]</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <snc mrk="bukl.0a.havanta">
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        blond<tld/>a knabineto
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda <tld/>a kapeto
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grilo &ccirc;irpis sub long<tld/>aj arboj
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_malmol;&c_d;&c_r;&c_a;&c_v;</trd>
  <trd lng="de">lockig</trd>
  <trdgrp lng="en">
    <trd>curly</trd>,
    <trd>loopy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rizado</trd>,
    <trd>ondulado</trd>
  </trdgrp>
  <trd lng="fr">boucl&eacute;</trd>
  <trd lng="he">&mem;&tav;&vav;&lamed;&tav;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;nd&ouml;r</trd>,
    <trd>g&ouml;nd&ouml;r&ouml;d&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5DFB;&#x304D;&#x6BDB;&#x306E;  <pr>&#x307E;&#x304D;&#x3051;&#x306E;</pr></trd>,
    <trd>&#x30AB;&#x30FC;&#x30EB;&#x3057;&#x305F;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kr&eogonek;cone<klr> (o w&lstroke;osach)</klr></trd>,
    <trd>zakr&eogonek;cone w <ind>loki</ind></trd>,
    <trd>k&eogonek;dzierzawe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_v;&c_mol;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_r;&c_ja;&c_v;&c_y;&c_j; <klr>(&c_o;
    &c_v;&c_o;&c_l;&c_o;&c_s;&c_a;&c_h;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bukl.0i">
  <kap><tld/>i, <var><kap><tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bukl.0i.bukleformi">
    <dif>
      Formi <tld/>ojn:
      <ekz>
        liaj haroj estas <tld/>itaj, nigraj kiel korvo
        <fnt><bib>MT</bib> <lok>&Alt; 5:11</lok></fnt>;
      </ekz>
      <ekz>
        nigraj haroj <tld/>is sur la granda lar&gcirc;a frunto
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="friz.0i">frizi</ref>,
      <ref cel="krisp">krispi</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_k;&c_malmol;&c_d;&c_r;&c_ja;</trd>
  <trd lng="cs">nat&aacute;&ccaron;et vlasy</trd>
  <trd lng="de">locken</trd>
  <trd lng="en">curl</trd>
  <trd lng="es">rizar</trd>
  <trd lng="fr">boucler <klr tip="ind">(donner une forme de
  boucle)</klr></trd>
  <trd lng="he">&lamed;&tav;&lamed;&tav;&lamed;</trd>
  <trd lng="hu">g&ouml;nd&ouml;r&ouml;dik</trd>
  <trdgrp lng="ja">
    <trd>&#x5DFB;&#x304D;&#x6BDB;&#x306B;&#x3059;&#x308B;  <pr>&#x307E;&#x304D;&#x3051;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8F2A;&#x306B;&#x5DFB;&#x304F;  <pr>&#x308A;&#x3093;&#x306B;&#x307E;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kr&eogonek;ci&cacute;<klr> w&lstroke;osy</klr></trd>,
    <trd>nawija&cacute;<klr> w&lstroke;osy (np. na
    lok&oacute;wk&eogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nakaderi&tcaron;</trd>,
    <trd>naku&ccaron;eri&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_a;&c_k;&c_r;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v;&c_o;&c_l;&c_o;&c_s;&c_s;&c_ja; &c_n;&c_a; &c_b;&c_ib;&c_g;&c_u;&c_d;&c_ib;</trd>
</drv>

<drv mrk="bukl.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="bukl.0ilo.KOS">
    <dif>
       Cilindreto, &ccirc;irka&ubreve; kiu oni volvas varmigitajn harojn por formi <tld/>on:
       <ekz>
         pro hasto &scirc;i forgesis demeti du <tld/>ilojn &ccirc;e la tempioj
         <fnt>
           <aut>I. Silone, trad. D. Mistretta</aut>
           <vrk><url ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">La
           sekreto de Luka</url></vrk>
           <lok>[vidita en 2018]</lok>
         </fnt>;
       </ekz>
      <ekz>
        &scirc;i eligis siajn <tld/>ilojn, fajrigis la gason kaj eklaboris por ripari la
        dama&gcirc;on, kiun faris malavareco lige kun amo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5377;&#x53D1;&#x5939;&#x5B50; <pr>ju&#x1CE;nf&#xE0;ji&#x101;zi</pr></trd>
</snc>
  <trd lng="de">Lockenwickler</trd>
  <trd lng="fr">bigoudi</trd>
  <trd lng="he">&mem;&samekh;&lamed;&samekh;&lamed;</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30FC;&#x30EB;&#x30D4;&#x30F3;</trd>,
    <trd>&#x30AB;&#x30FC;&#x30E9;&#x30FC;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d; &c_d;&c_l;&c_ja; &c_z;&c_a;&c_v;&c_i;&c_v;&c_k;&c_i;&nbsp;(&c_shch;&c_i;&c_p;&c_c;&c_ib;</trd>,
    <trd>&c_b;&c_ib;&c_g;&c_u;&c_d;&c_ib; &c_ib; &c_t;. &c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="bukl.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="bukl.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on.
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_j;&c_t;&c_o; &c_n;&c_ja;&c_m;&c_a; &c_k;&c_malmol;&c_d;&c_r;&c_i;&c_c;&c_i;</trd>
    <trd lng="hu">sima <klr tip="ind">(haj)</klr></trd>        
    <trd lng="pl">prosty<klr> (bez lok&oacute;w)</klr></trd>
  </snc>
  <snc mrk="bukl.sen0a.grafeo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      Tia, ke neniu e&gcirc;o de &gcirc;i estas 
      <ref cel="bukl.0o.grafeo"><tld/>o <sncref/></ref>.
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_j;&c_t;&c_o; &c_n;&c_ja;&c_m;&c_a; &c_p;&c_r;&c_i;&c_m;&c_k;&c_i;</trd>
    <trd lng="de"><klr tip="ind">(Graph) </klr>ohne
    <ind>Schleife</ind></trd>
    <trd lng="en">loop-free<klr tip="ind"> (graph)</klr></trd>
    <trd lng="es">sin <ind>rizos</ind></trd>
    <trd lng="fr"><klr tip="ind">(graphe) </klr>sans
    <ind>boucle</ind></trd>
    <trd lng="hu">hurok&eacute;lmentes <klr tip="ind">(gr&aacute;f)</klr></trd>    
    <trd lng="ru"><klr tip="ind">(&c_g;&c_r;&c_a;&c_f;)
    </klr>&c_b;&c_je;&c_z; &c_p;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trd lng="cs">beze smy&ccaron;ky</trd>
  <trd lng="sk">bez slu&ccaron;ky</trd>
</drv>

</art>
