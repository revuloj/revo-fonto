<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxtohxton.xml,v 1.14 2023/10/28 08:33:10 revo Exp $">
<kap>
  <rad>a&ubreve;to&hcirc;ton</rad>/o,
  <var><kap><rad var="k">a&ubreve;tokton</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="auxtohxton.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtohxton.0o.ETN">
    <dif>
      <ref tip="dif" cel="indigx.0o.ETN">Indi&gcirc;eno</ref>:
      <ekz>
        distingi <tld/>on de fremdulo
        <fnt><bib>Far1</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        ekzistas kvartaloj, lo&gcirc;ataj precipe de ne<tld var="k"/>oj, kie agreso
        kaj perforto
        furoras
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010808.php">Rajtoj pri ritoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5F53;&#x5730;&#x4EBA; <pr>d&#x101;ngd&#xEC;r&#xE9;n</pr></trd>,
    <trd>&#x7576;&#x5730;&#x4EBA; <pr>d&#x101;ngd&#xEC;r&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_h;&c_t;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Eingeborener</trd>,
    <trd>Einheimischer</trd>
  </trdgrp>
  <trd lng="fr">autochtone</trd>
  <trdgrp lng="hu">
    <trd>&odblac;slakos</trd>,
    <trd>bennsz&uuml;l&ouml;tt</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>autoctono<klr> (sost.)</klr></trd>,
    <trd>nativo<klr> (sost.)</klr></trd>,
    <trd>indigeno<klr> (sost.)</klr></trd>
  </trdgrp>
  <trd lng="nl">autochtoon<klr tip="ind"> (zn.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_v;&c_t;&#x43E;&c_h;&c_t;&#x43E;&#x43D;</trd>,
    <trd>&#x43A;&#x43E;&c_r;&c_ib;&#x43D;&#x43D;&c_i;&c_j; &c_zh;&c_i;&c_t;&c_je;&#x43B;&#x44C;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_ib;&#x43B;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="auxtohxton.0a">
  <kap><tld/>a, <var><kap><tld var="k"/>a</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtohxton.0a.ETN">
    <dif>
      <ref tip="dif" cel="indigx.0a.ETN">Indi&gcirc;ena</ref>:
      <ekz>
        la <tld/>aj Egiptanoj havis kupran koloron de l' ha&ubreve;to
        <fnt><bib>Far1</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        la internacia komunumo decidis sendependigi Kosovon, kie pli ol
        90&#x202F;% de la lo&gcirc;antaro konsistas el albanetnuloj <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009385.php">&Ccirc;u fine
          solvo?</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        albanoj estas <tld var="k"/>aj en okcidenta Makedonio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/008198.html">Neniu paco sen
          interkonsento</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la esperanta prononco estas tre malsimila al la landnomo
        <tld var="k"/>a
        <fnt>
          <aut>Remush</aut>,
          <vrk>Asia esperanto</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          2004-07-10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x571F;&#x4EA7; <pr>t&#xFA;ch&#x1CE;n</pr></trd>,
    <trd>&#x571F;&#x7522; <pr>t&#xFA;ch&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_h;&c_t;&c_o;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>autochthon</trd>,
    <trd>einheimisch</trd>,
    <trd>eingeboren</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>&odblac;shonos</trd>,
     <trd>autochton</trd>
   </trdgrp>
   <trdgrp lng="it">
    <trd>autoctono<klr> (agg.)</klr></trd>,
    <trd>nativo<klr> (agg.)</klr></trd>,
    <trd>indigeno<klr> (agg.)</klr></trd>
  </trdgrp>
   <trd lng="nl">autochtoon<klr tip="ind"> (bn.)</klr></trd>
</drv>
</art>

