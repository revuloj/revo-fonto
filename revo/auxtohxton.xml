<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;to&hcirc;ton</rad>/o,
  <var><kap><rad>a&ubreve;tokton</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="auxtohxton.0o">
  <kap><tld/>o, <var><kap>a&ubreve;toktono</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtohxton.0o.ETN">
    <dif>
      <ref tip="dif" cel="indigx.0o.ETN">Indi&gcirc;eno</ref>:
      <ekz>
        distingi <tld/>on de fremdulo
        <fnt><bib>Far1</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        ekzistas kvartaloj, lo&gcirc;ataj precipe de nea&ubreve;toktonoj, kie agreso kaj perforto
        furoras
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2012/010808.php"
              >Rajtoj pri ritoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Eingeborener</trd>,
    <trd>Einheimischer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&odblac;slakos</trd>,
    <trd>bennsz&uuml;l&ouml;tt</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>autoctono<klr> (sost.)</klr></trd>,
    <trd>nativo<klr> (sost.)</klr></trd>,
    <trd>indigeno<klr> (sost.)</klr></trd>
  </trdgrp>
  <trd lng="nl">autochtoon<klr tip="ind"> (zn.)</klr></trd>
</drv>

<drv mrk="auxtohxton.0a">
  <kap><tld/>a, <var><kap>a&ubreve;toktona</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtohxton.0a.ETN">
    <dif>
      <ref tip="dif" cel="indigx.0a.ETN">Indi&gcirc;ena</ref>:
      <ekz>
        la <tld/>aj Egiptanoj havis kupran koloron de l' ha&ubreve;to
        <fnt><bib>Far1</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        la internacia komunumo decidis sendependigi Kosovon, kie pli ol 90&#8239;% de la
        lo&gcirc;antaro konsistas el albanetnuloj <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2008/009385.php">&Ccirc;u fine solvo?</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        albanoj estas a&ubreve;toktonaj en okcidenta Makedonio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/008198.html"
              >Neniu paco sen interkonsento</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la esperanta prononco estas tre malsimila al la landnomo a&ubreve;toktona
        <fnt>
          <aut>Remush</aut>,
          <vrk>Asia esperanto</vrk>,
          <lok><url
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
          2004-07-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">autochthon</trd>
  <trdgrp lng="hu">
     <trd>&odblac;shonos</trd>,
     <trd>autochton</trd>
   </trdgrp>
   <trdgrp lng="it">
    <trd>autoctono<klr> (agg.)</klr></trd>,
    <trd>nativo<klr> (agg.)</klr></trd>,
    <trd>indigeno<klr> (agg.)</klr></trd>
  </trdgrp>
   <trd lng="nl">autochtoon<klr tip="ind"> (bn.)</klr></trd>
</drv>
</art>

