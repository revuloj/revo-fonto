<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>distik</rad>/o
  </kap>
  <drv mrk="distik.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">POE</uzo>
    <snc>
      <dif>
        Versduo:
        <ekz>elegia <tld/>o <klr>(konsistanta el heksametro kun
        pentametro)</klr>.
        </ekz>  
      </dif>
    </snc>
    <trd
    lng="be">&c_d;&c_v;&c_u;&c_h;&c_r;&c_a;&c_d;&c_k;&c_o;&c_w;&c_je;</trd>
    <trd lng="de">Distichon</trd>
    <trdgrp lng="en">
      <trd>distich</trd>,
      <trd>couplet</trd>
    </trdgrp>
    <trd lng="es">d&iacute;stico</trd>
    <trd lng="fr">distique</trd>
    <trd lng="hu">disztichon</trd>
    <trd lng="pl">dystych</trd>
    <trdgrp lng="pt">
      <trd>d&iacute;stico</trd>,
      <trd>parelha <klr tip="amb">(po&eacute;t.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_u;&c_s;&c_t;&c_i;&c_sh;&c_i;&c_je;</trd>,
      <trd>&c_d;&c_i;&c_s;&c_t;&c_i;&c_h;</trd>
    </trdgrp>
  </drv>
</art>
