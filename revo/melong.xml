<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>melongen</rad>/o
</kap>
<drv mrk="melong.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="solan.0o">solano</ref>,
      (<trd lng="la">Solanum melongena</trd>),
      kun man&gcirc;ebla, pirforma frukto de blanka, flava a&ubreve; viola
koloro.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_l;&c_a;&c_zh;&c_a;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Aubergine</trd>, <trd>Eierfrucht</trd>
  </trdgrp>
  <trd lng="en">eggplant</trd>
  <trd lng="fr">aubergine</trd>
  <trdgrp lng="hu">
    <trd>padlizs&aacute;n</trd>,
    <trd>toj&aacute;sgy&uuml;m&ouml;lcs</trd>,
    <trd>toj&aacute;scsucsor</trd>,
    <trd>t&ouml;r&ouml;kparadicsom</trd>
  </trdgrp>
  <trd lng="nl">aubergine</trd>
  <trdgrp lng="pt">
    <trd>beringela</trd>,
    <trd>berinjela</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_zh;&c_a;&c_n;</trd>
  <trd lng="tr">patl&inodot;can</trd>
</drv>
</art>
