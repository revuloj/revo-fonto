<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: melong.xml,v 1.29 2024/01/25 20:10:36 revo Exp $">
<kap>
  <ofc>9</ofc><rad>melongen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="melong.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <snc mrk="melong.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      de <ref tip="super" cel="solan.0o">solano</ref>,
      (<trd lng="la">Solanum melongena</trd>),
      legoma <ref tip="lst" cel="veget.0ajxo" lst="voko:solanoparencoj">vegeta&jcirc;o</ref>
      kun man&gcirc;ebla, ovo- a&ubreve; pirforma, frukto blanka, flava a&ubreve;
      violkolora:
      <ekz>
        <tld/>o estas unujara planto, kiu atingas altecon de maksimume 1
m 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20032">Melongeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>lilek vejcoplod&yacute;</trd>,
      <trd>bakla&zcaron;&aacute;n</trd>
    </trdgrp>
  </snc>
  <snc mrk="melong.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
       La frukto (<ref tip="super" cel="ber.0o.BOT">bero<sncref/></ref>)
       de <ref tip="malprt" cel="melong.0o.BOT">
       <tld/>o<sncref ref="melong.0o.BOT"/></ref>:
      <ekz>
        <klr>[por]</klr> pakoro <klr>[&#x2026;]</klr> oni trempas ingrediencon
        (ekzemple cepo, 
        <tld/>o, spinaco, terpomoj, florbrasiko, tomato, koka&jcirc;o) en
        pasto el faruno de 
        kikeroj kaj fritas &gcirc;in en oleo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=551419">Pakoro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">lilek</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_l;&c_a;&c_zh;&c_a;&c_n;</trd>
  <trd lng="bo">&#xF60;&#xF56;&#xF7A;&#xF0B;&#xF62;&#xF92;&#xF53;&#xF0B;</trd>
  <trd lng="ca">alberg&iacute;nia</trd>
  <trdgrp lng="de">
    <trd>Aubergine</trd>, <trd>Eierfrucht</trd>
  </trdgrp>
  <trd lng="en">eggplant</trd>
  <trd lng="es">berenjena</trd>
  <trd lng="fa">&a_b;&a_A;&a_d;&a_n;&a_j;&a_A;&a_n;</trd>
  <trd lng="fr">aubergine</trd>
  <trd lng="hi">&#x92C;&#x948;&#x902;&#x917;&#x928; [<ind>bai&#x1E41;gan</ind>]</trd>
  <trdgrp lng="hu">
    <trd>padlizs&aacute;n</trd>,
    <trd>toj&aacute;sgy&uuml;m&ouml;lcs</trd>,
    <trd>toj&aacute;scsucsor</trd>,
    <trd>t&ouml;r&ouml;kparadicsom</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>terong</trd>,
    <trd>terung</trd>
  </trdgrp>
  <trd lng="ja">&#x30CA;&#x30B9;  <pr>&#x306A;&#x3059;</pr></trd>
  <trd lng="nl">aubergine</trd>
  <trd lng="pl">bak&lstroke;a&zdot;an</trd>
  <trdgrp lng="pt">
    <trd>beringela</trd>,
    <trd>berinjela</trd>
  </trdgrp>
  <trd lng="ro">v&acirc;n&abreve;t&abreve;</trd>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_zh;&c_a;&c_n;</trd>
  <trd lng="tr">patl&inodot;can</trd>
  <trd lng="uk">&#x431;&#x430;&#x43A;&#x43B;&#x430;&#x436;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x8304;&#x5B50; <pr>qi&#xE9;zi</pr></trd>,
    <trd>&#x8304; <pr>qi&#xE9;</pr></trd>,
    <trd>&#x5706;&#x8304;&#x5B50; <pr>yu&#xE1;nqi&#xE9;zi</pr></trd>,
    <trd>&#x5713;&#x8304;&#x5B50; <pr>yu&#xE1;nqi&#xE9;zi</pr></trd>
  </trdgrp>
</drv>
</art>
