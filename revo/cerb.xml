<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cerb.xml,v 1.58 2024/12/22 12:44:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cerb</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="cerb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cerb.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="organ.0o.BIO">Organo</ref> konsistanta el maso da mola substanco, entenata en la
      kranio kaj formanta la centron de nerva sistemo:
      <ekz>
        la para&scirc;ito prenis kurban metalfadenon metis &gcirc;in en la nazon de l' mortinto kaj
        komencis eltiri la <tld/>on
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u li preferas esti vergata tridek ses fojojn de la tuta regimento, a&ubreve; ricevi
        dek-du kuglojn en la <tld/>on
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>?
      </ekz>
      <ekz>
        kapo kun herbo, sen guto da <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la lango laboru la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_m;&c_o;&c_z;&c_malmol;&c_k;</trd>
    <trd lng="br">empenn</trd>
    <trd lng="ca">cervell</trd>
    <trdgrp lng="de">
      <trd>Gehirn</trd>,
      <trd>Hirn</trd>
    </trdgrp>
    <trd lng="el">&epsilon;&gamma;&kappa;&epsilon_ton;&phi;&alfa;&lambda;&omikron;&sigma_fina;</trd>
    <trd lng="es">cerebro</trd>
    <trd lng="en">brain</trd>
    <trd lng="fr">cerveau</trd>
    <trd lng="he">&mem;&vav;&het;</trd>
    <trd lng="pl">m&oacute;zg</trd>
    <trd lng="pt">c&eacute;rebro</trd>
    <trd lng="sv">hj&auml;rna</trd>
    <trdgrp lng="zh">
      <trd>&#33041;&#34955; <pr>n&#462;odai</pr></trd>,
      <trd>&#33041;&#23376; <pr>n&#462;ozi</pr></trd>,
      <trd>&#27927;&#33041; <pr>x&#464;n&#462;o</pr></trd>,
      <trd>&#33041;&#39635; <pr>n&#462;osu&#464;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="cerb.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="spirit.0o.menso">Spirito</ref>,
      <ref tip="sin" cel="mens.0o">menso</ref>, <ref cel="pens.0o.pensado">penso</ref>:
      <ekz>
        venis al mi en la <tld/>on, ke...
        <fnt>PV</fnt>;
      </ekz>
      <ekz>
        <nom>Kay</nom> <klr>[&#8230;]</klr> kaj meditis tiel, ke lia <tld/>o estis forte stre&ccirc;ita
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        venas proverbo el popola la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld/>o iris promeni
        <klr>(li estis tute distrita)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kap.0o">kapo</ref>.
    </refgrp>
    <trdgrp lng="ca">
    <trd>ment</trd>,
    <trd>cap</trd>
  </trdgrp>
    <trd lng="de">Kopf <klr>(Geist)</klr></trd>
    <trd lng="he">&shin;&kaf;&lamed;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_z;&c_g;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_g;&c_ib;</trd>
  </trdgrp>
  <trd lng="bo">&t_ka;&t_lata;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="cs">mozek</trd>
  <trdgrp lng="hu">
    <trd>agy</trd>,
    <trd>agyvel&odblac;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>otak</trd>,
    <trd>benak</trd>
  </trdgrp>
  <trd lng="it">cervello</trd>
  <trdgrp lng="ja">
    <trd>&#38957;&#33075;  <pr>&#12378;&#12398;&#12358;</pr></trd>,
    <trd>&#33075;  <pr>&#12394;&#12378;&#12365;</pr></trd>,
    <trd>&#22823;&#33075;  <pr>&#12384;&#12356;&#12398;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">hersenen</trd>
  <trd lng="ro">creier</trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_g;</trd>
  <trd lng="sk">mozog</trd>
  <trd lng="uk">&c_m;&c_o;&c_z;&c_o;&c_k;</trd>
</drv>

<drv mrk="cerb.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a skuego
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in pereigis <tld/>a sangalfluo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la baptofilo neniam montris signojn de <tld/>a malsano
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        fama klinikisto <klr>[rimarkis]</klr> &scirc;an&gcirc;ojn okazintajn en la <tld/>a stato
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_g;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">Hirn-</trd>
  <trd lng="es">cerebral</trd>
  <trd lng="id">serebral</trd>
  <trdgrp lng="ja">
    <trd>&#38957;&#33075;&#30340;&#12394;  <pr>&#12378;&#12398;&#12358;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#33075;&#12398;  <pr>&#12398;&#12358;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_o;&c_z;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="cerb.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cerb.0eto.KOMUNE">
    <dif>
      Malgranda <tld/>o:
      <ekz>
        birda <tld/>eto
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 10 (60)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infaneta <tld/>eto bezonas abundan, intensan kaj varian bombardon de lingvouzo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2013/011010.php"
              >Kiel, kiam, kial lerni fremdan lingvon?</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">kleines <ind>Gehirn</ind></trd>
  </snc>
  <snc mrk="cerb.0eto.stulto">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom simpla, stulta menso:
      <ekz>
        via <tld/>eto renversi&gcirc;os en via kranio, kiam mia naskema sprito aku&scirc;i&gcirc;os
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili superu je mucideco kaj mensa limigiteco la <klr>[&#8230;]</klr> oficistan
        <tld/>eton
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 38</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="cerb.0eto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Maso da nerva substanco, kiu estas sub la posta parto de la
      <tld/>o.
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_z;&c_zh;&c_a;&c_ch;&c_o;&c_k;</trd>,
      <trd>&c_m;&c_o;&c_z;&c_a;&c_ch;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="bg">&c_m;&c_a;&c_l;&c_k;&c_i;&c_ja;&c_t; <ind>&c_m;&c_o;&c_z;&c_malmol;&c_k;</ind></trd>
    <trd lng="br">ilpenn</trd>
    <trd lng="ca">cerebel</trd>
    <trdgrp lng="cs">
      <trd>cerebellum</trd>,
      <trd>mal&yacute; mozek</trd>,
      <trd>moze&ccaron;ek</trd>
    </trdgrp>
    <trd lng="de">Kleinhirn</trd>
    <trd lng="el">&pi;&alfa;&rho;&epsilon;&gamma;&kappa;&epsilon;&phi;&alfa;&lambda;&jota_ton;&delta;&alfa;</trd>
    <trd lng="en">cerebellum</trd>
    <trd lng="es">cerebelo</trd>
    <trd lng="fr">cervelet</trd>
    <trd lng="he">&he;&mem;&vav;&het; &he;&qof;&tet;&fnun;</trd>
    <trd lng="hu">kisagy</trd>
    <trd lng="id"><ind>otak</ind> kecil</trd>
    <trdgrp lng="it">
    <trd>cervelletto</trd>,
    <trd>cerebello</trd>
    </trdgrp>
    <trd lng="ja">&#23567;&#33075;  <pr>&#12375;&#12423;&#12358;&#12398;&#12358;</pr></trd>
    <trdgrp lng="nl">
      <trd>kleine <ind>hersenen</ind></trd>,
      <trd>cerebellum</trd>
    </trdgrp>
    <trd lng="pl">m&oacute;&zdot;d&zdot;ek</trd>
    <trd lng="pt">cerebelo</trd>
    <trd lng="ru">&c_m;&c_o;&c_z;&c_zh;&c_je;&c_ch;&c_o;&c_k;</trd>
    <trdgrp lng="sk">
      <trd>mal&yacute; mozog</trd>,
      <trd>mozo&ccaron;ek</trd>
    </trdgrp>
    <trd lng="zh">&#23567;&#33041; <pr>xi&#462;on&#462;o</pr></trd>
  </snc>
</drv>

<drv mrk="cerb.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      La hista maso de <tld/>o:
      <ekz>
        <tld/>a&jcirc;oj estis dis&scirc;utitaj sur la tero apud tratran&ccirc;itaj kruroj kaj brakoj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o <klr>(prefere <ref cel="medol.0o.ANA">medolo</ref>)</klr> de miaj ostoj frosti&gcirc;is en mi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_z;&c_g;&c_ib; <klr tip="amb">(&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;)</klr></trd>
    <trd lng="br">boued <ind>penn</ind></trd>
    <trd lng="ca">cervell <klr>(menjar)</klr></trd>
    <trdgrp lng="de">
      <trd>Gehirnmasse</trd>,
      <trd>Hirnmasse</trd>
    </trdgrp>
    <trd lng="en">brains</trd>
    <trd lng="es">sesos</trd>
    <trd lng="fr">cervelle</trd>
    <trd lng="he">&mem;&vav;&het;<klr tip="amb">
    (&kaf;&mem;&alef;&kaf;&lamed;)</klr></trd>
    <trd lng="hu">vel&odblac; <klr>(&eacute;tel)</klr></trd>
    <trd lng="id">otak<klr> (makanan)</klr></trd>
    <trd lng="it">cervella</trd>
    <trdgrp lng="ja">
      <trd>&#33075;&#28479;  <pr>&#12398;&#12358;&#12375;&#12423;&#12358;</pr></trd>,
      <trd>&#12475;&#12523;&#12505;&#12540;&#12523;</trd>
    </trdgrp>
    <trd lng="nl">hersenen<klr tip="amb"> (als voedsel)</klr></trd>
    <trd lng="pl">m&oacute;&zdot;d&zdot;ek</trd>
    <trd lng="ru">&c_m;&c_o;&c_z;&c_g;&c_i;
    <klr>(&c_je;&c_d;&c_a;)</klr></trd>
  </snc>
  <adm>
    Necesas kontroli la tradukojn post forigo de KUI kaj pli&gcirc;eneraligo de la difino.
  </adm>
</drv>

<drv mrk="cerb.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="krani.0o">Kranio</ref>:
      <ekz>
        kio misordi&gcirc;is en via <tld/>ujo
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Potifar, la bonkorulo</lok>
        </fnt>?
      </ekz>
      <ekz>
        kial li nun permesas al tiu &ccirc;i maldelikata senta&ubreve;gulo bati lin per malpura
        fosilo sur la <tld/>ujon
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_a;&c_p;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_p;</trd>
  <trd lng="br">klopenn</trd>
  <trd lng="ca">crani</trd>
  <trd lng="cs">mozkovna</trd>
  <trd lng="de">Sch&auml;del</trd>
  <trd lng="el">&kappa;&rho;&alfa;&ny;&jota_ton;&omikron;</trd>
  <trdgrp lng="en">
    <trd>brainpan</trd>,
    <trd>braincase</trd>,
    <trd>cranium</trd>
  </trdgrp>
  <trd lng="es">cr&aacute;neo</trd>
  <trd lng="fr">bo&icirc;te <ind>cr&acirc;nienne</ind></trd>
  <trd lng="he">&gimel;&vav;&lamed;&gimel;&vav;&lamed;&tav;</trd>
  <trdgrp lng="hu">
    <trd>koponya</trd>,
    <trd>kobak<klr tip="ind"> (koponya)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>tengkorak</trd>,
    <trd>kranium</trd>,
    <trd><ind>tulang</ind> kepala</trd>
  </trdgrp>
  <trd lng="it">cranio</trd>
  <trd lng="ja">&#38957;&#33995;  <pr>&#12378;&#12364;&#12356;</pr></trd>
  <trd lng="nl">schedel</trd>
  <trd lng="pl">czaszka</trd>
  <trd lng="pt">cr&acirc;nio</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;</trd>
  <trd lng="sk">lebka</trd>
  <trdgrp lng="zh">
    <trd>&#39025; <pr>l&uacute;</pr></trd>,
    <trd>&#39025;&#39592; <pr>l&uacute;g&#468;</pr></trd>,
    <trd>&#22836;&#39592; <pr>t&oacute;ug&#468;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cerb.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Penege stre&ccirc;i sian penson, spiriton pri io:
      <ekz>
        li <tld/>umis nur pri malgajigaj ideoj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        post iom da vana kaj senrezulta <tld/>umado li &gcirc;enerale inciti&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lin terure &ccirc;agrenis la milito kaj li tiam <tld/>umis sian faman
        Leteron al la Diplomatoj
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 28a, p.
        69a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontempl.0i">kontempli</ref>,
      <ref cel="medit.0i">mediti</ref>,
      <ref cel="pens.pri0i">pripensi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_a;&c_v;&c_u;</ind></trd>,
    <trd>&c_a;&c_b;&c_m;&c_a;&c_z;&c_g;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_m;&c_i;&c_sh;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>en em <ind>so&ntilde;jal</ind></trd>,
    <trd>prederia&ntilde;<klr tip="ind"> (en em so&ntilde;jal)</klr></trd>
  </trdgrp>
  <trd lng="ca">escalfar-se el cap</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;dat</trd>,
    <trd>hloubat</trd>,
    <trd>nechat proj&iacute;t hlavou</trd>,
    <trd>p&rcaron;em&yacute;&scaron;let</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gr&uuml;beln</trd>,
    <trd>sinnen</trd>,
    <trd>br&uuml;ten <klr>(sinnen)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rack one's brains</trd>,
    <trd>brood</trd>,
    <trd>cerebrate</trd>
  </trdgrp>
  <trd lng="es">reflexionar</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;fl&eacute;chir</trd>,
    <trd>cogiter</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&resh;&he;&resh;</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;ri a <ind>fej&eacute;t</ind></trd>,
    <trd>elgondolkodik</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>peras</baz>memeras <klr>(otak)</klr></trd>,
    <trd><baz>putar</baz>memutar <klr>(otak)</klr></trd>,
    <trd><baz>renung</baz>merenung</trd>
  </trdgrp>
  <trd lng="it">scervellarsi</trd>
  <trd lng="ja">&#38957;&#12434;&#12402;&#12397;&#12427;  <pr>&#12354;&#12383;&#12414;&#12434;&#12402;&#12397;&#12427;</pr></trd>
  <trd lng="nl">nadenken</trd>
  <trd lng="pl">g&lstroke;owi&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_m;&c_a;&c_t;&c_mol; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_z;&c_g;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&aacute;ma&tcaron; si <ind>hlavu</ind></trd>,
    <trd>l&aacute;ma&tcaron; si hlavu</trd>,
    <trd>rozm&yacute;&scaron;&lcaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>grubbla</trd>,
    <trd>fundera</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_m;&c_a;&c_t;&c_i; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u; &c_n;&c_a;&c_d; &c_ch;&c_i;&c_m;-&c_n;.</trd>,
    <trd>&c_m;&c_ib;&c_z;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_d;&c_u;&c_m;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="zh">&#32004;&#25720; <pr>yu&emacron;m&omacron;</pr></trd>
</drv>

<drv mrk="cerb.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Profunde firmigi en la menson:
