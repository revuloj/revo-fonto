<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cerb</rad>/o
</kap>
<drv mrk="cerb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Organo konsistanta el maso da mola substanco, entenata en la kranio k
      formanta la centron de nerva sistemo.
    </dif>
    <trd lng="de">Gehirn</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Spirito, menso, penso:
      <ekz>
        venis al mi en la <tld/>on, ke...<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia <tld/>o iris promeni
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt> 
        <klr>(li estis tute distrita)</klr>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="kap.0o">kapo</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_g;</trd>
</drv>
<drv mrk="cerb.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Maso da nerva substanco, kiu estas sub la posta parto de la <tld/>o. 
    </dif>
  </snc>
  <trd lng="de">Kleinhirn</trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_zh;&c_je;&c_ch;&c_o;&c_k;</trd>
</drv>
<drv mrk="cerb.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Besta <tld/>o, konsiderata kiel man&gcirc;a&jcirc;o. 
    </dif>
  </snc>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_g;&c_i; <klr>(&c_je;&c_d;&c_a;)</klr></trd>
</drv>
<drv mrk="cerb.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      <ref tip="dif"> cel="krani.0o">Kranio</ref>. 
    </dif>
  </snc>
  <trd lng="de">Sch&auml;del</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;</trd>
</drv>
<drv mrk="cerb.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Penege stre&ccirc;i sian penson, spiriton pri io:
      <ekz>
        ne valoras <tld/>umi pri tia bagatelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gr&uuml;beln</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_m;&c_a;&c_t;&c_mol; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_z;&c_g;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="cerb.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Profunde firmigi en la menson. 
    </dif>
  </snc>
  <trdgrp lng="de"><trd>einh&auml;mmern</trd>, <trd>eintrichtern</trd></trdgrp>
  <trd lng="ru">&c_v;&c_b;&c_i;&c_t;&c_mol; &c_v; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;</trd>
</drv>
<drv mrk="cerb.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc>
    <dif>
      Senpripensa homo. 
    </dif>
  </snc>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_m;&c_o;&c_z;&c_g;&c_l;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
</drv>
<drv mrk="cerb.osto0o">
  <kap>osto<tld/>o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Mola substanco, similanta la <tld/>an substancon k entenata en iaj
      ostoj
    </dif>
    <refgrp tip="sin">
      <ref cel="medol.0o">medolo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Knochenmark</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j; &c_m;&c_o;&c_z;&c_g;</trd>
</drv>
</art>
