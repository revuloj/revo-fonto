<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pram</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pram.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pram.0o.KOMUNE">
    <dif>
      Granda plata boato por transriverigi homojn, bestojn a&ubreve;
      veturilojn:
      <ekz>
        la <tld/>o veturas preska&ubreve; du horojn
        <fnt><aut>Werner Schad</aut>: <vrk>Tra la Andoj
        boate</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 12a, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_m;</trd>
  <trd lng="bg">&c_f;&c_je;&c_r;&c_i;&c_b;&c_o;&c_t;</trd>
  <trd lng="ca">transbordador</trd>
  <trd lng="de">F&auml;hre</trd>
  <trdgrp lng="en">
    <trd>ferry</trd>,
    <trd>pontoon</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ferry</trd>,
    <trd>transbordador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bac<klr> (bateau)</klr></trd>
  </trdgrp>
  <trd lng="hu">komp</trd>
  <trdgrp lng="it">
    <trd>traghetto</trd>,
    <trd>ferryboat</trd>
  </trdgrp>
  <trd lng="nl">veerpont</trd>
  <trdgrp lng="pt">
    <trd>barca</trd>,
    <trd>ferryboat</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>kompa</trd>,
    <trd>prievoz</trd>
  </trdgrp>
  <trd lng="sv">f&auml;rja</trd>
</drv>

<drv mrk="pram.kosmo0o">
  <kap>kosmo<tld/>o</kap>
  <snc mrk="pram.kosmo0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="sxip.kosmo0o.AVI">kosmo&scirc;ipo</ref> povanta 
      elflugi el planedo kaj reveni al &gcirc;i:
      <ekz>
        la katastrofo de la kosmo<tld/>o Kolombio en 2003, en kiu mortis
        
        sep kosmona&ubreve;toj kaj kiu ka&ubreve;zis 29-monatan pa&ubreve;zon de 
        kosmo<tld/>aj flugoj, ka&ubreve;zis gravan rekonsideron de la 
        prioritatoj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Nasa&rightquot;, 
        2008-04-17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">transbordador espacial</trd>
  <trd lng="de">Raumf&auml;hre</trd>
  <trdgrp lng="es">
    <trd>nave <ind>espacial</ind></trd>,
    <trd>transbordador <ind>espacial</ind></trd>
  </trdgrp>
  <trd lng="fr">navette spatiale</trd>
  <trdgrp lng="it">
    <trd>navetta <ind>spaziale</ind></trd>,
    <trd>shuttle</trd>,
    <trd>spaceshuttle</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ruimteveer</trd>,
    <trd>ruimtependel</trd>,
    <trd>shuttle</trd>,
    <trd>spaceshuttle</trd>
  </trdgrp>
</drv>
</art>
