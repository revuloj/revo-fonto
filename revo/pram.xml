<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pram.xml,v 1.28 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pram</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pram.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pram.0o.KOMUNE">
    <dif>
      Granda plata boato por transriverigi homojn, bestojn a&ubreve;
      veturilojn:
      <ekz>
        la <tld/>o veturas preska&ubreve; du horojn
        <fnt>
        <aut>W. Schad</aut>
        <vrk>Tra la Andoj boate</vrk>
        <bib>Monato</bib>
        <lok>2001:12a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_m;</trd>
  <trd lng="bg">&c_f;&c_je;&c_r;&c_i;&c_b;&c_o;&c_t;</trd>
  <trd lng="bo">&#xF42;&#xFB2;&#xF74;&#xF0B;&#xF53;&#xF44;&#xF0B;&#xF60;&#xF42;&#xFB2;&#xF7C;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="ca">transbordador</trd>
  <trdgrp lng="cs">
    <trd>pramice</trd>,
    <trd>pr&aacute;m</trd>
  </trdgrp>
  <trd lng="de">F&auml;hre</trd>
  <trdgrp lng="en">
    <trd>ferry</trd>,
    <trd>pontoon</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ferry</trd>,
    <trd>transbordador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bac<klr> (bateau)</klr></trd>
  </trdgrp>
  <trd lng="hu">komp</trd>
  <trdgrp lng="it">
    <trd>traghetto</trd>,
    <trd>ferryboat</trd>
  </trdgrp>
  <trd lng="ja">&#x6E21;&#x3057;&#x8239;  <pr>&#x308F;&#x305F;&#x3057;&#x3076;&#x306D;</pr></trd>
  <trd lng="nl">veerpont</trd>
  <trd lng="pl">prom</trd>
  <trdgrp lng="pt">
    <trd>barca</trd>,
    <trd>ferryboat</trd>
  </trdgrp>
  <trd lng="ro">bac</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>kompa</trd>,
    <trd>prievoz</trd>
  </trdgrp>
  <trd lng="sv">f&auml;rja</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_r;&#x43E;&#x43C;</trd>,
    <trd>&#x43F;&c_a;&c_r;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="pram.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pram.0isto.KOMUNE">
    <dif>
      Tiu, kiu veturigas <tld/>on:
      <ekz>
        &gcirc;i estis nigra, malnovmoda <tld/>o, kaj sur &gcirc;i <klr>[estis]</klr> malbonhumora,
        malvigla <tld/>isto
        <fnt>
          <aut>I. Andri&cacute;, trad. A. Sekelj</aut>
          <vrk>La ponto super Drino</vrk>
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_m;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="fr">nocher</trd>
  <trd lng="ja">&#x8239;&#x982D;  <pr>&#x305B;&#x3093;&#x3069;&#x3046;</pr></trd>
  <trd lng="pl">sternik</trd>
  <trd lng="ro">timonier</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&#x43E;&#x43C;&#x43D;&c_i;&#x43A;</trd>
</drv>

<drv mrk="pram.kosmo0o">
  <kap>kosmo<tld/>o</kap>
  <snc mrk="pram.kosmo0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="sxip.kosmo0o.AVI">kosmo&scirc;ipo</ref> povanta
      elflugi el planedo kaj reveni al &gcirc;i:
      <ekz>
        la katastrofo de la kosmo<tld/>o Kolombio en 2003, en kiu mortis
        sep kosmona&ubreve;toj kaj kiu ka&ubreve;zis 29-monatan pa&ubreve;zon de
        kosmo<tld/>aj flugoj, ka&ubreve;zis gravan rekonsideron de la
        prioritatoj
        <fnt><bib>Viki</bib> <lok>artikolo &leftquot;Nasa&rightquot;,
        2008-04-17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_t;&c_l; <klr>(&c_k;&c_a;&c_s;&c_mol;&c_m;&c_ib;&c_ch;&c_n;&c_y; &c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
  <trd lng="ca">transbordador espacial</trd>
  <trd lng="de">Raumf&auml;hre</trd>
  <trdgrp lng="es">
    <trd>nave <ind>espacial</ind></trd>,
    <trd>transbordador <ind>espacial</ind></trd>
  </trdgrp>
  <trd lng="fr">navette spatiale</trd>
  <trdgrp lng="it">
    <trd>navetta <ind>spaziale</ind></trd>,
    <trd>shuttle</trd>,
    <trd>spaceshuttle</trd>
  </trdgrp>
  <trd lng="ja">&#x30B9;&#x30DA;&#x30FC;&#x30B9;&#x30B7;&#x30E3;&#x30C8;&#x30EB;</trd>
  <trdgrp lng="nl">
    <trd>ruimteveer</trd>,
    <trd>ruimtependel</trd>,
    <trd>shuttle</trd>,
    <trd>spaceshuttle</trd>
  </trdgrp>
  <trd lng="pl">prom kosmiczny</trd>
  <trd lng="ro">nava spa&#x21B;ial&abreve;</trd>
</drv>
</art>
