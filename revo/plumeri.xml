<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: plumeri.xml,v 1.2 2024/01/22 06:32:01 afido Exp $">
    <kap>
      <rad>plumeri</rad>/o
      <fnt><bib>Viki</bib></fnt>
    </kap>

<drv mrk="plumeri.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref> de sudamerikaj
      arboj kaj arbustoj el familio <ref tip="malprt" cel="apocin.0oj">apocinacoj</ref> (<trd
      lng="la">Plumeria</trd>)
      kun karnecaj folioj kaj lakteca suko:
      <ekz>
        diversaj tropikaj floroj floras kun pompa beleco &ccirc;ie en la insulo, inter ili
        trovi&gcirc;as <tld/>o, ru&gcirc;a zingibro kaj bugenvilo
        <fnt>
          <vrk><url ref="https://dlibra.kul.pl/Content/50555/55503_OOMOTO_1981_jan-jun.pdf"
              >Oomoto inauguris monumenton en Ponapo</url></vrk>
          <lok>Oomoto jan-jun 1981, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzotaj plantoj kiel jakarandoj kaj <tld/>oj estis enkondukitaj en parkoj kaj
        &gcirc;ardenoj <klr>[de Sidnejo]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37662"
              >Sidnejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/4a/Plumeria-0006-Zachi-Evenor.jpg/501px-Plumeria-0006-Zachi-Evenor.jpg"
      prm="CC BY 2.0"
      >floroj kaj folioj de ru&gcirc;a <tld/>o (Plumeria rubra), Israelo
        <fnt>
          <aut>Zachi Evenor, propra foto</aut>
          <lok><url ref="&WCW;/File:Plumeria-0006-Zachi-Evenor.jpg"
              >https://commons.wikimedia.org/wiki/File:Plumeria-0006-Zachi-Evenor.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Plumeria</trd>
    <trd lng="en">Plumeria</trd>
    <trd lng="fr">Plumeria</trd>
    <trd lng="ru">&c_p;&c_l;&c_ju;&c_m;&c_je;&c_r;&c_i;&c_ja;</trd>
  </snc>
</drv>

  </art>

