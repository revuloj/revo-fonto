<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc><rad>Peru</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="peru.P0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Peruo">Peruo</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>,
      limanta al 
      <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>, 
      <ref tip="vid" cel="cxili.CX0o">&Ccirc;ilio</ref>,
      <ref tip="vid" cel="ekvador.E0o">Ekvadoro</ref> kaj
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>, 
      kaj anka&ubreve; al
      <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>. 
    </dif>
    <trd lng="be">&c_P;&c_e;&c_r;&c_u;</trd>
    <trd lng="bg">&c_P;&c_je;&c_r;&c_u;</trd>
    <trd lng="de">Peru</trd>
    <trd lng="en">Peru</trd>
    <trd lng="fa">&f_p;&a_r;&a_w;</trd>
    <trdgrp lng="fr">
      <trd>P&eacute;rou</trd>,
      <trd>R&eacute;publique du P&eacute;rou</trd>
    </trdgrp>
    <trd lng="id">Peru</trd>
    <trd lng="nl">Peru</trd>
    <trd lng="pl">Peru</trd>
    <trd lng="pt">Peru</trd>
  </snc>
  <trd lng="cs">Peru</trd>
  <trd lng="es">Per&uacute;</trd>
  <trd lng="sk">Peru</trd>
</drv>

<drv mrk="peru.0ano">
  <kap><tld lit="p"/>ano <fnt><bib>Viki</bib>, <lok><url 
  ref="http://eo.wikipedia.org/wiki/Blankaj peruanoj">Blankaj
  peruanoj</url></lok></fnt>,
  <var><kap><tld lit="p"/>anino</kap></var>
  </kap>
  <snc>
    <dif>
    Civitano, civitanino de Peruo.
    </dif>
  </snc>
  <trd lng="en">Peruvian</trd>
  <trdgrp lng="fr"> 
    <trd>P&eacute;ruvien</trd>
    <trd>P&eacute;ruvienne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="peru.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Peruo.
    </dif>
  </snc>
  <trd lng="en">Peruvian</trd>
  <trdgrp lng="fr"> 
    <trd>p&eacute;ruvien</trd>
    <trd>p&eacute;ruvienne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>


