<vortaro>
<art mrk="$Id$">
<kap><rad>Peru</rad>/o</kap>
<drv mrk="peru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>,
limanta
      al 
      <ref tip="vid" cel="brazil.0o">Brazilo</ref>,
      <ref tip="vid" cel="bolivi.0o">Bolivio</ref>, 
      <ref tip="vid" cel="cxili.0o">&Ccirc;ilio</ref>,
      <ref tip="vid" cel="ekvador.0o">Ekvadoro</ref> kaj
      <ref tip="vid" cel="kolombi.0o">Kolombio</ref>, 
      kaj anka&ubreve; al
      <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>. 
    </dif>
    <trd lng="nl">Peru</trd>
    <trd lng="pl">Peru</trd>
    <trd lng="pt">Peru</trd>
  </snc>
</drv>
</art>


