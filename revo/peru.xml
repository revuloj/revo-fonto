<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: peru.xml,v 1.26 2024/02/17 06:43:06 revo Exp $">
<kap>
  <ofc>8</ofc><rad>Peru</rad>/o <fnt><bib>Homaranismo</bib></fnt>
</kap>

<drv mrk="peru.P0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">PE</mlg>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&scirc;tato</ref>,
      limanta kun
      <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>,
      <ref tip="vid" cel="cxili.CX0o">&Ccirc;ilio</ref>,
      <ref tip="vid" cel="ekvador.E0o">Ekvadoro</ref> kaj
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>,
      kaj anka&ubreve; al
      <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>.
      <ekz>
        Ekzemploj: Svisujo, Belgujo, A&ubreve;strujo, Kanado, Meksiko, <tld/>o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;rinde la hispanoj intertempe malkovris gemojn, oron en
        Meksikio kaj <tld/>o, sekve la portugaloj ekimitis la aliajn en
        koloniigado 20 jarojn poste
        <fnt>
          <aut>Gilbert Ledon</aut>
          <vrk>El mia vidpunkto</vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o prezidanto <nom>Fujimori</nom> sukcesis savi sian
        landon el &hcirc;aoso
        <fnt>
          <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2001/007311.html">Putrantaj
          demokratioj</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p.7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_e;&c_r;&c_u;</trd>
  <trd lng="bg">&c_P;&c_je;&c_r;&c_u;</trd>
  <trd lng="cs">Peru</trd>
  <trd lng="de">Peru</trd>
  <trd lng="en">Peru</trd>
  <trd lng="es">Per&uacute;</trd>
  <trd lng="fa">&f_p;&a_r;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>P&eacute;rou</trd>,
    <trd>R&eacute;publique du P&eacute;rou</trd>
  </trdgrp>
  <trd lng="id">Peru</trd>
  <trd lng="ja">&#x30DA;&#x30EB;&#x30FC;</trd>
  <trd lng="nl">Peru</trd>
  <trd lng="pl">Peru</trd>
  <trd lng="pt">Peru</trd>
  <trd lng="sk">Peru</trd>
  <trd lng="tok">ma Pelu</trd>
  <trd lng="ru">&c_P;&c_je;&c_r;&c_u;</trd>
  <trd lng="uk">&#x41F;&c_je;&c_r;&c_u;</trd>
  <trd lng="vo">Peruv&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x79D8;&#x9B6F; <pr>B&#xEC;l&#x1D4;</pr></trd>,
    <trd>&#x79D8;&#x9C81; <pr>B&#xEC;l&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="peru.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato Peruo:
      <ekz>
        la <tld lit="p"/>a nova suno estas la nacia valuto de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=340102">Perua
          nova suno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_u;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">peruanisch</trd>
  <trd lng="en">Peruvian</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;ruvien</trd>
    <trd>p&eacute;ruvienne<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Peruviaans</trd>,
    <trd>Peruaans</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_u;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
 <trd lng="tok">pi ma Pelu</trd>
  <trd lng="vo">Peruv&auml;nik</trd>
</drv>

<drv mrk="peru.0ano">
  <kap><tld lit="p"/>ano</kap>
  <snc>
    <dif>
      Civitano de Peruo:
      <ekz>
        <nom>Tony Halik</nom> <klr>[&#x2026;]</klr> en 1976 kune kun
        <nom>El&zdot;bieta Dzikowska</nom> kaj <tld lit="p"/>ano Edmundo
        <nom>Guillen</nom> atingis legendan &ccirc;efurbon de inkaoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=361648">Tony
          Halik</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_u;&c_a;&c_n;&c_je;&c_c;</trd>
  <trdgrp lng="de">
    <trd>Peruaner</trd>
    <trd>Peruanerin</trd>
  </trdgrp>
  <trd lng="en">Peruvian</trd>
  <trdgrp lng="es">
    <trd>peruano</trd>
    <trd>peruana</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>P&eacute;ruvien</trd>
    <trd>P&eacute;ruvienne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Peruviaan</trd>,
    <trd>Peruaan</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>peruano</trd>
    <trd>peruana</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_u;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="tok">jan pi ma Pelu</trd>
  <trd lng="vo">Peruv&auml;nan</trd>
  <trdgrp lng="zh">
    <trd>&#x79D8;&#x9B6F;&#x4EBA; <pr>b&#xEC;l&#x1D4;r&#xE9;n</pr></trd>,
    <trd>&#x79D8;&#x9C81;&#x4EBA; <pr>b&#xEC;l&#x1D4;r&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="peru.0anino">
  <kap><tld lit="p"/>anino</kap>
  <snc>
    <dif>
      Civitanino de Peruo; inseksa <ref tip="super" cel="peru.0ano"><tld lit="p"/>ano</ref>:
      <ekz>
        la lastan tagon de la jaro 1953, <nom>Guevara</nom> alvenis al
        Gvatemalo <klr>[kaj]</klr> konati&gcirc;is kun la <tld lit="p"/>anino
        <nom>Hilda Gadea</nom>, sia onta edzino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10951">Che
          Guevara</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_u;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="de">Peruanerin</trd>
  <trd lng="en">Peruvian woman</trd>
  <trd lng="es">peruana</trd>
  <trd lng="fr">P&eacute;ruvienne<klr> (subst.)</klr></trd>
   <trdgrp lng="nl">
    <trd>Peruviaanse</trd>,
    <trd>Peruaanse</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_u;&c_a;&c_n;&c_k;&c_a;</trd>
   <trd lng="pt">peruana</trd>
  <trd lng="tok">meli pi ma Pelu</trd>
  <trd lng="vo">ji-Peruv&auml;nan</trd>
</drv>
</art>

