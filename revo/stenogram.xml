<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stenogram.xml,v 1.7 2023/12/04 23:26:11 revo Exp $">
<kap>
  <rad>stenogram</rad>/o
</kap>
<drv mrk="stenogram.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="stenogram.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenog.0ajxo.TIP">Stenografa&jcirc;o</ref>:
      <ekz>
        &ccirc;iuj <tld/>oj estas ekstreme zorge desegnitaj
        <fnt>
          <aut>J. Vilkki</aut>,
          <vrk>Intersteno a&ubreve; unesteno?</vrk>,
          <lok><url ref="http://laine.pp.fi/ef/ef682.html">
          Esperanta Finnlando, 1968:2</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povos &scirc;an&gcirc;i la sencon de viaj vortoj,
        estas necese postuli <tld/>on
        <fnt>
          <aut>R. Rolland, trad. A. kaj C. Gacond</aut>,
          <vrk><url ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego_0268.pdf">
          Mahatma Gandhi en Svislando (4)</url></vrk>,
          <lok>1968-11-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="cs">stenogram</trd>
  <trd lng="de">Stenogramm</trd>
  <trdgrp lng="en">
    <trd>stenogram</trd>,
    <trd>transcript</trd>,
    <trd>shorthand</trd>
  </trdgrp>
  <trd lng="fr">st&eacute;nogramme</trd>
  <trd lng="pl">stenogram</trd>
  <trd lng="sk">r&yacute;chlopisn&yacute; z&aacute;znam</trd>
</drv>
</art>

