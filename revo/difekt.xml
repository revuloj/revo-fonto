<drv mrk="difekt.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <snc mrk="difekt.0igxi.KOMUNE">
     <dif>
       Fari&gcirc;i <tld/>ita:
      <ekz>
        kiam mi <tld/>i&gcirc;is, ili &gcirc;ojis
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 35:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la greno <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Joel 1:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blovilo brul<tld/>i&gcirc;is, la plumbo malaperis en la fajro
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        naivuloj anta&ubreve;enpa&scirc;as, kaj <tld/>i&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 22:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj pio&ccirc;oj kaj &scirc;oveliloj estas lignaj, kaj tiaj iloj facile <tld/>i&gcirc;as
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la elektraj piloj de sonoriloj, ne estante plu observitaj, <tld/>i&gcirc;is kaj ne
        funkciis plu
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la randoj de la pentra&jcirc;o <tld/>i&gcirc;is
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi zorgos, ke la reputacio de amba&ubreve; sufi&ccirc;e <tld/>i&gcirc;u
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kulturis grenon, <klr>[sed]</klr> &gcirc;i <tld/>i&gcirc;is pro la senpluveco
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenie rompi&gcirc;as a&ubreve; <tld/>i&gcirc;as la kontinueco
        inter la lingvo malnova kaj nova
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista 
          en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>.
      </ekz>
    </dif>
