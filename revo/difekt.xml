<drv mrk="difekt.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="difekt.sen0a.KOMUNE">
    <dif>
      Ne<tld/>ita:
      <ekz>
        li alportu kiel sian oferon virkapron sen<tld/>an
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 4:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        li falis sen<tld/>a sur molan muskon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam vi estis tiel feli&ccirc;a, tiel sen<tld/>a, tiel sennube gaja
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi alportos lin sen<tld/>an kaj vivan, vi ricevos milionon rekompence
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas tie &scirc;mira&jcirc;on, kiu povus tuj fari &scirc;ian piedon sen<tld/>a kaj sana
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> montris sen<tld/>ajn vicojn da fortaj, flaveblankaj dentoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
